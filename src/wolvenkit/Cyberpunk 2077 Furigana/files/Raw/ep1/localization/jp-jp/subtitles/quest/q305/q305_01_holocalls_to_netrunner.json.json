    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？ どうしてまた私にかけてきたの？",
              "maleVariant": "",
              "stringId": "3064683470421626880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイト・コープの株価について相談したいってわけじゃない",
              "maleVariant": "",
              "stringId": "3064694718722871296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V… 私… そういう仕事は請けてない",
              "maleVariant": "",
              "stringId": "3064695219640209408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "貸しがある",
              "maleVariant": "",
              "stringId": "3064709063510986756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。またね",
              "maleVariant": "わかった。またな",
              "stringId": "3064709356676059136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "貸しがあるでしょ。デカいのが",
              "maleVariant": "貸しがあるだろ。デカいのが",
              "stringId": "3064710263585247232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。邪魔して悪かったね",
              "maleVariant": "わかった。邪魔して悪かったな",
              "stringId": "3064710333781118976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "助けてくれたときの報酬は受け取ったはずよ。貸し借りはない",
              "maleVariant": "",
              "stringId": "3064712748207050752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハァ… なら、あくまで仮定として聞くけど、どういう手伝いをさせるつもり？",
              "maleVariant": "",
              "stringId": "3064713743733493760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "取引成立ね",
              "maleVariant": "取引成立だな",
              "stringId": "3064717461749108740"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[切る] 少し考えさせて",
              "maleVariant": "[切る] 少し考えさせてくれ",
              "stringId": "3064717761205637120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マックス・タックのデータ要塞に潜って、ロス・パドレス刑務所に向かう陸上輸送隊について調べてほしい",
              "maleVariant": "",
              "stringId": "3064719529490649088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダメよV、もう… 連絡してこないで。その… 気をつけてね",
              "maleVariant": "",
              "stringId": "3064721084906344448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんな頼み？",
              "maleVariant": "どんな頼みだ？",
              "stringId": "3064721968763637760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "考えておく。また連絡する",
              "maleVariant": "",
              "stringId": "3064722335547133952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで手を打つ。手早くお願い。時間がなくて",
              "maleVariant": "それで手を打つ。手早く頼む。時間がなくてな",
              "stringId": "3064724327287885824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつものことでしょ…",
              "maleVariant": "",
              "stringId": "3064725985615024128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど、でもちょっと待って。先にそっちの頼み事について教えてくれる？",
              "maleVariant": "なるほど、だがちょっと待て。先にそっちの頼み事について教えてくれるか？",
              "stringId": "3064726566777786368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いま説明してもらう",
              "maleVariant": "",
              "stringId": "3064727137521897472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞いてる？ 条件は変わらないから",
              "maleVariant": "",
              "stringId": "3064728837909516288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、どうしたんだい。今日は面白い噂話でも聞かせてくれるの？",
              "maleVariant": "",
              "stringId": "3064734113991626752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vじゃない",
              "maleVariant": "",
              "stringId": "3064734837643284480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マックス・タックをハッキングしたい",
              "maleVariant": "",
              "stringId": "3064735263751016452"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "できないわ。ごめんね、V… 危険が大きすぎる",
              "maleVariant": "",
              "stringId": "3064735376376467456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "結構込み入った依頼でね",
              "maleVariant": "",
              "stringId": "3064735455917248512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっぱり、後でまた連絡する",
              "maleVariant": "",
              "stringId": "3064736397806931968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっぱり、後でまた連絡するね、キャロル",
              "maleVariant": "やっぱり、後でまた連絡するよ、キャロル",
              "stringId": "3064736651243556864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マックス・タックを… ハックしたい",
              "maleVariant": "",
              "stringId": "3064744127858872320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フー… いい、V。仮にハックできたとしても、さすがにマックス・タックが相手だと私もファミリーもタダじゃすまない。みんなを危険に晒すことになる",
              "maleVariant": "",
              "stringId": "3064745170999054336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ「やらない」とは言ってないよね？",
              "maleVariant": "まだ「やらない」とは言ってないよな？",
              "stringId": "3064745963789619200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいよ。事情はわかってるし。他を当たってみる",
              "maleVariant": "いいんだ。事情はわかってるし。他を当たってみる",
              "stringId": "3064747569704734720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "できないわ。ごめんね、V。悪気はないけど… ファミリーのことが第一なの。わかるでしょう？ 特別扱いはできない",
              "maleVariant": "",
              "stringId": "3064748491260432384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残念ながら。今日は急ぎで手伝ってほしいことがあって",
              "maleVariant": "",
              "stringId": "3064750144839618560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ、暗号化されたチップでも見つけたとか？ 話を聞いてあげる",
              "maleVariant": "",
              "stringId": "3064750994471391232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やあ、キャロル。手を借りたいの",
              "maleVariant": "やあ、キャロル。手を借りたいんだ",
              "stringId": "3064752178607296512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえええ。急にまたどうしたの。どんなこと？",
              "maleVariant": "",
              "stringId": "3064752438704476160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう。わかったわ",
              "maleVariant": "",
              "stringId": "3064758405152247808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？ 何を企んでるのか言ってみな",
              "maleVariant": "",
              "stringId": "3064759870138748928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サンドラ、手を借して。ハイリスクな依頼で、あんたみたいな腕のいいランナーの助けがいる",
              "maleVariant": "サンドラ、手を借してくれ。ハイリスクな依頼で、あんたみたいな腕のいいランナーの助けがいる",
              "stringId": "3069264396183334912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃあまた",
              "maleVariant": "",
              "stringId": "3069267377461063680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、本気で言ってるの？",
              "maleVariant": "おい、本気で言ってるのか？",
              "stringId": "3069270562615521280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サンドラ？ 聞いてる？",
              "maleVariant": "サンドラ？ 聞いてるか？",
              "stringId": "3069274574081421312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "代わりに、私の頼みも聞いてもらいたいの。それが飲めないなら、話はここまでよ",
              "maleVariant": "",
              "stringId": "3069274824431038464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ホロでできるような話じゃないわ。ちゃんと後で説明するから",
              "maleVariant": "",
              "stringId": "3069281503105183744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "飲めないならそのまま切って、二度と連絡しないで",
              "maleVariant": "",
              "stringId": "3069282985892311040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、いつものことでしょ…",
              "maleVariant": "",
              "stringId": "3069291967340015616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "相手が悪いよ。サイコスクワッドの名は伊達じゃない。たったひとつのミスですぐにアルデカルドスまで突き止められて、連中がすっ飛んでくる。そうなったらおしまいよ…",
              "maleVariant": "",
              "stringId": "3069294442281365504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気にしないで、キャロル。他をあたってみる",
              "maleVariant": "気にするな、キャロル。他をあたってみる",
              "stringId": "3069296691820818432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気をつけて、いい？",
              "maleVariant": "",
              "stringId": "3069334168614924288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って。サイバー空間の外でマックス・タックと戦うことになった場合に備えて、渡したいものがある。役に立つかも",
              "maleVariant": "",
              "stringId": "3069336730026045440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう、キャロル。本当に助かるよ",
              "maleVariant": "",
              "stringId": "3069337845778018304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく気をつけて、V",
              "maleVariant": "",
              "stringId": "3069337965399568384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やってもいい。でも、ひとつ条件がある",
              "maleVariant": "",
              "stringId": "3072087100347314176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チャン・フン、ちょっと話がある。経験豊富なランナーが必要なの",
              "maleVariant": "チャン・フン、ちょっと話がある。経験豊富なランナーが必要なんだ",
              "stringId": "3101251486139420732"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、すまないが、勘弁してくれ。今は小さな店をやってるんだ。前回の件で目が覚めたよ。もうあんなことはやってられない",
              "maleVariant": "",
              "stringId": "3101251486156197888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "機密データがいる… 相手はマックス・タック",
              "maleVariant": "",
              "stringId": "3101251493588504576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "後でまた連絡する",
              "maleVariant": "",
              "stringId": "3101251493588504608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マックス・タックのデータ要塞に潜入して、ロス・パドレスに向かう連中の陸上輸送隊についての情報を盗んでほしい",
              "maleVariant": "",
              "stringId": "3101251493689167896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おいおい、大ごとだな、V。本気か？ マックス・タックに手を出すやつなんかいない。聞いたことがないぞ",
              "maleVariant": "",
              "stringId": "3101251493689167908"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中は要塞からデーモン、パーマフラックスコードに至るまで、すべてにSHAC-761で暗号化されたSHA-37インターフェースを使ってる… 果たしてブリーチできるかどうか",
              "maleVariant": "",
              "stringId": "3101251493689167932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが、もし成功すれば、大手柄だな… わかった、やってみるよ、V",
              "maleVariant": "",
              "stringId": "3101251493689167944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チャン・フン、後でまた連絡させて",
              "maleVariant": "チャン・フン、後でまた連絡するよ",
              "stringId": "3101251501557682188"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうか。じゃあまたな、V",
              "maleVariant": "",
              "stringId": "3101251501557682200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ決まりね",
              "maleVariant": "じゃあ決まりだな",
              "stringId": "3101251502715310080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう少し考える時間がいる",
              "maleVariant": "",
              "stringId": "3101251502732087300"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ごめん、やっぱりもう少し検討してみる。また連絡するね",
              "maleVariant": "すまん、やっぱりもう少し検討してみる。また連絡するよ",
              "stringId": "3101251502866305036"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、確かにこれは危険な仕事だからな。じっくり考えて決めるといい",
              "maleVariant": "",
              "stringId": "3101251502866305048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし… じゃあ、精一杯やってみて。お願いだから失敗しないでね",
              "maleVariant": "よし… じゃあ、精一杯やってみてくれ。頼むから失敗しないでくれよ",
              "stringId": "3101251504762130444"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "雷に二度打たれる奴はいないだろ？ さっそく取り掛かろう",
              "maleVariant": "",
              "stringId": "3101251504762130456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、結局のところどうするんだ？ 話を振ってきたのはそっちだろ。決断は？ 俺にゴーサインを出すのか？ V？",
              "maleVariant": "",
              "stringId": "3101251507278712840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ランナー引退には早いんじゃない？",
              "maleVariant": "ランナー引退には早いんじゃないか？",
              "stringId": "3103022874782699524"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なら仕方ない",
              "maleVariant": "",
              "stringId": "3103023789224857600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お願いよ、チャン・フン。ランに失敗して死にかけたネットランナーなんていくらでもいる。諦めるのは早いんじゃない？",
              "maleVariant": "お願いだ、チャン・フン。ランに失敗して死にかけたネットランナーなんていくらでもいる。諦めるのは早いんじゃないか？",
              "stringId": "3103024318260809728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、無理強いはしない。ナイトシティじゃ、命はいくつあっても足りないしね",
              "maleVariant": "まあ、無理強いはしない。ナイトシティじゃ、命はいくつあっても足りないしな",
              "stringId": "3103024437748142080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭を下げても無駄だ。引退だよ",
              "maleVariant": "",
              "stringId": "3103032802599604224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう。じゃあまたね",
              "maleVariant": "そうか。じゃあまたな",
              "stringId": "3103032933713547264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "元気でな、V",
              "maleVariant": "",
              "stringId": "3103033138580131840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺はもう67だぞ、V",
              "maleVariant": "",
              "stringId": "3103035234658390016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ランナーの価値は最後にやった仕事で決まるものよ。今のままじゃ自宅の地下室で死にかけた、情けないネットオタクってところじゃない？",
              "maleVariant": "ランナーの価値は最後にやった仕事で決まるものだろ。今のままじゃ自宅の地下室で死にかけた、情けないネットオタクってとこじゃないか？",
              "stringId": "3103035417412603904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ったく、意地が悪いな。それを知ってるのはワカコとお前だけだ",
              "maleVariant": "",
              "stringId": "3103048254432317440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気にしてんでしょ？",
              "maleVariant": "気にしてんだろ？",
              "stringId": "3103048336691007488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まったく… で、ブリーチする相手は誰か言ってみろ",
              "maleVariant": "",
              "stringId": "3103048421936041984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今の分析はちょっと…",
              "maleVariant": "",
              "stringId": "3111527145765441536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "SHA-37…？ マックス・タックはQLSに変えたんじゃ？",
              "maleVariant": "",
              "stringId": "3111527570480664576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あ、ああ… そうかもな。後で確認してみる",
              "maleVariant": "",
              "stringId": "3111551132906131456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うん… そうして",
              "maleVariant": "ああ、そうしてくれ",
              "stringId": "3111554711721185280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8時間後に、レコンシリエーション・パークで会いましょう。そこにデータを持ってく",
              "maleVariant": "",
              "stringId": "3357694373444587520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしもし…？",
              "maleVariant": "",
              "stringId": "3357697875319328768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V… 話す気がないなら、切ってくれる？",
