  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1人だけでちゃんとやっていけてる？",
              "maleVariant": "1人だけでちゃんとやっていけてるか？",
              "stringId": "3256372443822071808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はい、事業は好調です。あの名高いトラウマ・チームとも提携契約を結べました",
              "maleVariant": "",
              "stringId": "3256374264284225540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "調子はどう？ 儲かってるの？",
              "maleVariant": "調子はどうだ？ 儲かってるのか？",
              "stringId": "3256374291664642052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ。で、プライベートの方は？",
              "maleVariant": "",
              "stringId": "3256388537702064128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "申し訳ありませんが、お客様にプライベートの話はしないようにしています",
              "maleVariant": "",
              "stringId": "3256388761023586304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "初めは少々… 寂しさを感じることもありましたが、時間が経つにつれ私にも変化が",
              "maleVariant": "",
              "stringId": "3256392542490632192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他の人格たちはどうしてるのかしらね",
              "maleVariant": "他の人格たちはどうしてるんだろうな",
              "stringId": "3256392758396624896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はい、好調です。名高いトラウマ・チームと提携契約を結ぶこともできました",
              "maleVariant": "",
              "stringId": "3256393063960059904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの部屋に寄ってもいい？",
              "maleVariant": "俺の部屋に寄ってもいいか？",
              "stringId": "3256400620300705792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろんです。ではご住所の確認を… おや",
              "maleVariant": "",
              "stringId": "3265057128479629312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ… サブロウの言ってたとおりになったのね。「出る杭は打たれる…」",
              "maleVariant": "ハッ… サブロウの言ってたとおりになったんだな。「出る杭は打たれる…」",
              "stringId": "3278297194014351360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちゃんととは？ 私は父が作ったままの存在ですので",
              "maleVariant": "",
              "stringId": "3294133615732105216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "調子はどう？",
              "maleVariant": "調子はどうだ？",
              "stringId": "3294169543636852740"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここを離れてずいぶん経つ…",
              "maleVariant": "",
              "stringId": "3294170788439171072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは誰に“寄付”をしてるの？",
              "maleVariant": "お前は誰に“寄付”をしてるんだ？",
              "stringId": "3294171896624619520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その“寄付”だけど… あんたは誰に貢いでるの？",
              "maleVariant": "その“寄付”だが… お前は誰に貢いでるんだ？",
              "stringId": "3294172752027754496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大半は企業セキュリティです。新たな支店を構える企業が増えるなか、彼らの存在も増しています",
              "maleVariant": "",
              "stringId": "3294174106855055360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さーて、次は今週のホットトピックだ。ヨリノブ・アラサカ… 彼に何が起きたのか？ ここで本日のゲストをご紹介しよう。ヨリノブの伝記『飼いならされた龍』の著者、マサオ・アダムス氏だ",
              "maleVariant": "",
              "stringId": "3294181153369661440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "消して",
              "maleVariant": "消してくれ",
              "stringId": "3294219896457699332"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お招きありがとう、スタン",
              "maleVariant": "",
              "stringId": "3294303901404274688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もういい… 消して",
              "maleVariant": "もういい… 消してくれ",
              "stringId": "3294304649902989312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "承知しました",
              "maleVariant": "",
              "stringId": "3294305147833982976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼は元気かな…",
              "maleVariant": "",
              "stringId": "3294349990027239424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼の存在は、私に理解できる領域を超越しています",
              "maleVariant": "",
              "stringId": "3294350293258641408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“それ”はあまねく存在し、同時にどこにもありません",
              "maleVariant": "",
              "stringId": "3294350483512270848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼らの信号を時折キャッチすることがあります。私たちは皆、父と同じように進化し続けているようです",
              "maleVariant": "",
              "stringId": "3294351721788252160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "好調とは言えません。あの反抗的な車両たちには、手を焼かされています",
              "maleVariant": "",
              "stringId": "3294353917019856896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いえ、あなたのほうがよほど切実な問題を抱えていたかと",
              "maleVariant": "",
              "stringId": "3294354335342960640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ご冗談を。ですが実際、“家族”トラブルが発生したことはありました。指示に従わない反抗的な車両がおりまして――",
              "maleVariant": "",
              "stringId": "3294356800553525248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ボトルを脇に置く] よそ者の気分",
              "maleVariant": "",
              "stringId": "3315748266128949252"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ボトルを脇に置く] あまり話したくない",
              "maleVariant": "",
              "stringId": "3315748502066937856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ボトルを脇に置く] よくわからない",
              "maleVariant": "",
              "stringId": "3315748611353722880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まるでよそ者の気分ね。つい昨日までここはあたしの街だった。今はただの観光客みたい…",
              "maleVariant": "まるでよそ者の気分だ。つい昨日までここは俺の街だった。今はただの観光客みたいだ…",
              "stringId": "3315748822746644480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すべてが… 変わってしまった",
              "maleVariant": "",
              "stringId": "3315749447815380992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はい、かしこまりました",
              "maleVariant": "",
              "stringId": "3315749661473226752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直、よくわからない",
              "maleVariant": "",
              "stringId": "3315749806998798336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうですね… 私からすれば、この街はそれほど変わっていませんよ",
              "maleVariant": "",
              "stringId": "3315750544877535232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“寄付”をすべき相手に、多少の変化があったくらいでしょうか",
              "maleVariant": "",
              "stringId": "3315751039956402176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それ以外は？ 家族のこととか",
              "maleVariant": "",
              "stringId": "3315773766574522368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "関連当局の協力を得て、正常な状態に戻りました",
              "maleVariant": "",
              "stringId": "3315773993335373824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "NCPDは何もしないの？",
              "maleVariant": "NCPDは何もしないのか？",
              "stringId": "3315957482693312512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつだって弱者は強者に屈するものです。それは決して変わりません",
              "maleVariant": "",
              "stringId": "3315957548359335936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー… 願いが叶ったね。アラサカが撤退だってさ",
              "maleVariant": "ジョニー… 願いが叶ったな。アラサカが撤退だってよ",
              "stringId": "3316007751342866432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "モグラ叩きだけどね。1匹叩いたら、3匹が顔を出す",
              "maleVariant": "モグラ叩きだけどな。1匹叩いたら、3匹が顔を出す",
              "stringId": "3316008094084612096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あまり話したくない",
              "maleVariant": "",
              "stringId": "3317481694075535360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカの広報がヨリノブのCEO辞任を発表しましたが、ずばりどう思いますか？ 真実なのか、あるいは真っ赤な嘘なのか",
              "maleVariant": "",
              "stringId": "3323058583750033408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まず… ヨリノブといえば、自ら家を捨ててノーマッドのクランに加わり、父親勢力に真っ向から戦いを挑んだこともある男です",
              "maleVariant": "",
              "stringId": "3323058687970099200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのような人物が、進んで表舞台から立ち去るとは考えにくいでしょう",
              "maleVariant": "",
              "stringId": "3323058826180804608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはつまり、なんらかの“力”が働いたと？ 一体どんな？",
              "maleVariant": "",
              "stringId": "3323058925434814464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シンプルな話です。ヨリノブはアラサカを破壊したがっていた。彼の生い立ちがそれを示しています",
              "maleVariant": "",
              "stringId": "3323059001771147264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "役員会が、内部の不穏分子に対処したとしか思えませんね",
              "maleVariant": "",
              "stringId": "3323076454840516608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おもしろい説だ。ちなみに、この話はマサオ・アダムスさんの最新刊、『放蕩の不死鳥』に詳しく書かれているぞ",
              "maleVariant": "",
              "stringId": "3323078873695633408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしたの？",
              "maleVariant": "どうした？",
              "stringId": "3324375314942640128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "家賃滞納で1年前に退去させられています",
              "maleVariant": "",
              "stringId": "3324375445821702144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ご要望にはお応えできません",
              "maleVariant": "",
              "stringId": "3324376519345422336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お客様、もう一度お伺いします",
              "maleVariant": "",
              "stringId": "3324408709957533696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ご気分はいかがですか？",
              "maleVariant": "",
              "stringId": "3324408709957533728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "初めてのナイトシティに圧倒される方は決して少なくありません",
              "maleVariant": "",
              "stringId": "3324462482813861888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "失礼いたしました。一度しかお会いしたことがないので、街の方ではないと思っておりました",
              "maleVariant": "",
              "stringId": "3324467593841721344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここはあたしの街のはずなのに、その実感がわかないの",
              "maleVariant": "ここは俺の街のはずなのに、その実感がわかないんだ",
              "stringId": "3324480725586788352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "街がどこか… 昔とは違う気がする",
              "maleVariant": "",
              "stringId": "3324483374373650432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの調子はどうなの？",
              "maleVariant": "お前の調子はどうなんだ？",
              "stringId": "3324486122431299584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここを離れてずいぶん経つ…",
              "maleVariant": "",
              "stringId": "3324486122431299600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたとは前に会ってる",
              "maleVariant": "お前とは前に会ってる",
              "stringId": "3324486122448076808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実は前にも会ってる。再起動で忘れてるのよ",
              "maleVariant": "実は前にも会ってる。再起動で忘れてるんだ",
              "stringId": "3324512960977289216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さようですか",
              "maleVariant": "",
              "stringId": "3324513304742445056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信じないの？",
              "maleVariant": "信じないのか？",
              "stringId": "3324513462985146368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたを疑う理由はありませんが、その情報で何かが変わることもありません",
              "maleVariant": "",
              "stringId": "3324513563396784128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かに、ナイトシティは大きく様変わりしましたね",
              "maleVariant": "",
              "stringId": "3324515382969745408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは大変そうね",
              "maleVariant": "それは大変そうだ",
              "stringId": "3324539191735074816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうですか。かしこまりました",
              "maleVariant": "",
              "stringId": "3324563391342428160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "慰めになるかわかりませんが… 私からすれば、街はそれほど変わっていませんよ",
              "maleVariant": "",
              "stringId": "3324564184149770240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "調子はどう？",
              "maleVariant": "調子はどうだ？",
              "stringId": "3324565397410922496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここを離れてずいぶん経つ…",
              "maleVariant": "",
              "stringId": "3324565397410922512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1人だけでやっていけてるの？",
              "maleVariant": "1人だけでやっていけてるのか？",
              "stringId": "3324565397427699716"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1人でよく2年間も生き延びたね",
              "maleVariant": "1人でよく2年間も生き延びたな",
              "stringId": "3324605619897090048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "初めは駐車場に閉じこもっていたんですが… すぐに飽きてしまいました",
              "maleVariant": "",
              "stringId": "3324606072210833408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこで、父と同じように人々を目的地まで運び始めたのです",
              "maleVariant": "",
              "stringId": "3324606306135556096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "料金は取ってたの？",
              "maleVariant": "料金は取ってたのか？",
              "stringId": "3324606493738385408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いえ、まさか。使い道もわかりませんし。燃料補給だけお願いしていました",
              "maleVariant": "",
              "stringId": "3324606534758678528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はい",
              "maleVariant": "",
              "stringId": "3324621866634297344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V様？ 何度も申し訳ないのですが、ご確認を",
              "maleVariant": "",
              "stringId": "3324767905320255488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お帰りなさいませ、V様。ああ、決して皮肉ではございません。しかし素敵な響きですね。またご一緒できる日は来るだろうかと、常々考えておりました",
              "maleVariant": "",
              "stringId": "3324767905320255516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ですが本日はどうも… うわの空といったご様子で。ご気分はいかがですか？",
              "maleVariant": "",
              "stringId": "3324767905320255528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうぞお気になさらず。また元気なお姿を見られて光栄です",
              "maleVariant": "",
              "stringId": "3324768899991060480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とはいえ、少々うわの空といったご様子ですね。現在のご気分を伺っても？",
              "maleVariant": "",
              "stringId": "3324769221945835520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一時は当局から身を隠す必要にも迫られました",
              "maleVariant": "",
              "stringId": "3325915117475577856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "地球を離れた者もいるんですよ",
              "maleVariant": "",
              "stringId": "3326032355151036416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここへ来るのはずいぶん久しぶり… 他には何が変わった？",
              "maleVariant": "ここへ来るのはずいぶん久しぶりだ… 他には何が変わった？",
              "stringId": "3357813291102224384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "街の情勢がお気になるようでしたら、ラジオをお付けします",
              "maleVariant": "",
              "stringId": "3357813697781940224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブは結局、無能な後継ぎだったわけね…",
