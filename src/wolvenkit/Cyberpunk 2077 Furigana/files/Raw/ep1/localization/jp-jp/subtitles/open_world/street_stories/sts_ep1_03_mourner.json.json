  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[待つ]",
              "maleVariant": "",
              "stringId": "2074958653177454592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ドッグタグを渡す] ラインダーは死んだ",
              "maleVariant": "",
              "stringId": "3346023036627152900"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パシフィカでラインダーを見つけて、始末した",
              "maleVariant": "",
              "stringId": "3346023263018905600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ドッグタグを渡す] 仕事はこなした",
              "maleVariant": "",
              "stringId": "3346030007258411008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう。当然の報いよ",
              "maleVariant": "",
              "stringId": "3346033242039873536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "終わったの？ ラインダーは？ 約束は守ってくれたんでしょうね",
              "maleVariant": "",
              "stringId": "3348880108362764288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。もう奴を気にする必要はない",
              "maleVariant": "ああ。もう奴を気にする必要はない",
              "stringId": "3348884288808783872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スタックスのみんなを代表して… ありがとう",
              "maleVariant": "",
              "stringId": "3348884639133831168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "恐怖に震えながら死んだことを祈るわ",
              "maleVariant": "",
              "stringId": "3349031107131969536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大したことじゃない",
              "maleVariant": "",
              "stringId": "3349033835660939264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "歓喜に震えてなかったことは確かね…",
              "maleVariant": "歓喜に震えてなかったことは確かだな…",
              "stringId": "3349041852620050432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[もたれかかる] 「正義」の解釈はいろいろある",
              "maleVariant": "",
              "stringId": "3397142537085227008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "金をかき集めたってどういうこと？",
              "maleVariant": "金をかき集めたってどういうことだ？",
              "stringId": "3397142537085227024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「正義」ね… 人によって意味が異なるけど… あんたにとっての正義って何？",
              "maleVariant": "「正義」ね… 人によって意味が異なるが… あんたにとっての正義って何だ？",
              "stringId": "3397142537219444772"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レオン・ラインダーは生きるに値しないクズ野郎よ。奴はハンセンの殺し屋だった",
              "maleVariant": "",
              "stringId": "3397142537236221952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やれやれ、聞くまでもなかったか…",
              "maleVariant": "",
              "stringId": "3397142537236221964"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何とでも言って。奴がマーサの息子ジェイソンとその友達、さらに4人の住民を殺したことに変わりはない",
              "maleVariant": "",
              "stringId": "3397142537236221976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジェイソン、ケイレブ、ディオンテ、オマール、ファレル、マティアス、アリッサ、それにタミカ…",
              "maleVariant": "",
              "stringId": "3397142537236221992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインダーは何のためにジェイソンと会ってたの？",
              "maleVariant": "ラインダーは何のためにジェイソンと会ってたんだ？",
              "stringId": "3397142550859321348"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインダーをスカベンジャーに紹介するつもりだったみたいよ",
              "maleVariant": "",
              "stringId": "3397142550876098560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スカベンジャー…？ ジェイソンはあんな連中と付き合いがあったの？",
              "maleVariant": "スカベンジャー…？ ジェイソンはあんな連中と付き合いがあったのか？",
              "stringId": "3397142550892875776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドッグタウンで生き抜こうとする若者だったのよ。聖人ではなかったけど、性根は腐ってなかった。マーサのおかげでね",
              "maleVariant": "",
              "stringId": "3397142550892875788"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "金をかき集めたって？",
              "maleVariant": "",
              "stringId": "3397142569649803276"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スタックスの住人のみんなで少しずつ出し合ったのよ",
              "maleVariant": "",
              "stringId": "3397142569666580480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリアナ？",
              "maleVariant": "",
              "stringId": "3397142573558894592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "失礼――あたしはV。ハンズから依頼を受けた者よ。ブリアナね？",
              "maleVariant": "失礼――俺はV。ハンズから依頼を受けた者だ。ブリアナだな？",
              "stringId": "3397142574078988304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。マーサ、この人は私たちを助けに来てくれたの。少し話してくるから… ね？",
              "maleVariant": "",
              "stringId": "3397142574095765516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ… わかった",
              "maleVariant": "",
              "stringId": "3397142574112542720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこのあんた… あいつを地獄にぶち込んでやって",
              "maleVariant": "",
              "stringId": "3397142574112542732"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ごめんなさい、みんな我慢の限界なのよ",
              "maleVariant": "",
              "stringId": "3397142585739153408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話を始める前に、あなたのこと… あなたの素性を教えて。ドッグタウンのことはどれくらい知ってる？",
              "maleVariant": "",
              "stringId": "3397142585755930624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカで何年も働いてた",
              "maleVariant": "",
              "stringId": "3397142589648244736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "元ノーマッドよ",
              "maleVariant": "元ノーマッドだ",
              "stringId": "3397142589665021968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティのストリートで育った",
              "maleVariant": "",
              "stringId": "3397142589681799172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティ出身よ。それ以上言う必要はない",
              "maleVariant": "ナイトシティ出身だ。それ以上言う必要はない",
              "stringId": "3397142589681799192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それより殺人犯の話を聞きたい",
              "maleVariant": "",
              "stringId": "3397142589698576384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何年もアラサカで働いて、時間以上のものを奪われた。でも、ドッグタウンは――",
              "maleVariant": "何年もアラサカで働いて、時間以上のものを奪われた。だが、ドッグタウンは――",
              "stringId": "3397142589832794112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一部の人間を除いて、搾取されるだけの街よ。家族を育てるような場所じゃない",
              "maleVariant": "一部の人間を除いて、搾取されるだけの街だ。家族を育てるような場所じゃない",
              "stringId": "3397142589832794128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここに住んでる人は、他に行き場がないのよ",
              "maleVariant": "",
              "stringId": "3397142589832794140"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは旅をしながら育った。星空の下で眠り、ファミリーのみんなと焚き火を囲んでね",
              "maleVariant": "俺は旅をしながら育った。星空の下で眠り、ファミリーのみんなと焚き火を囲んでな",
              "stringId": "3397142599228035080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "厳しい暮らしをする者たちには絆が生まれる。ここの人たちもそうでしょ",
              "maleVariant": "厳しい暮らしをする者たちには絆が生まれる。ここの人たちもそうだろ",
              "stringId": "3397142599244812288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたとは気が合いそうね",
              "maleVariant": "",
              "stringId": "3397142599244812300"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんどは私の番ね。名前はブリアナ。6年前、クロウズに追われてドッグタウン… スタックスにたどり着いた",
              "maleVariant": "",
              "stringId": "3397142607230767112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "住民の大半も似たような過去を持ってる。みんな必死に生きてるだけ。だからこそ…",
              "maleVariant": "",
              "stringId": "3397142607230767124"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなことより、殺人犯のことを聞かせてくれる？ 仕事に取り掛かりたい",
              "maleVariant": "そんなことより、殺人犯のことを聞かせてくれるか？ 仕事に取り掛かりたい",
              "stringId": "3397142612280709132"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お願いよ。あなたを信用するためにも",
              "maleVariant": "",
              "stringId": "3397142612280709144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはヘイウッド出身よ。生活は楽じゃなかったけど、諦めずに生きてきた",
              "maleVariant": "俺はヘイウッド出身だ。生活は楽じゃなかったが、諦めずに生きてきた",
              "stringId": "3397142615317385232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここの暮らしも似たようなものだと思う。警察から逃げ回る必要がないだけでね",
              "maleVariant": "ここの暮らしも似たようなものだと思う。警察から逃げ回る必要がないだけでな",
              "stringId": "3397142615334162436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ… たしかに警察からはね",
              "maleVariant": "",
              "stringId": "3397142615350939652"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "犯人の動機は？",
              "maleVariant": "",
              "stringId": "3397142633688436736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バーゲストはラインダーを野放しにしてるの？",
              "maleVariant": "バーゲストはラインダーを野放しにしてるのか？",
              "stringId": "3397142633688436752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインダーはなぜバーゲストを抜けたの？",
              "maleVariant": "ラインダーはなぜバーゲストを抜けたんだ？",
              "stringId": "3397142633688436768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインダーはバーゲストを抜けたんだっけ？ なんで？",
              "maleVariant": "ラインダーはバーゲストを抜けたんだっけ？ なんでだ？",
              "stringId": "3397142633822654488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知らないわ。知ってるのは、奴が凶暴で残忍だってことだけ",
              "maleVariant": "",
              "stringId": "3397142633822654500"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、ハンセンとの関係は？",
              "maleVariant": "",
              "stringId": "3397142633839431688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセンがここに定着したときからの付き合いよ。他の人がやりたがらないような仕事を請け負ってたらしい",
              "maleVariant": "",
              "stringId": "3397142633839431700"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バーゲストも、殺人を犯した奴には制裁を加えるでしょ",
              "maleVariant": "バーゲストも、殺人を犯した奴には制裁を加えるだろ",
              "stringId": "3397142649157029900"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "捕まえればね。連中にも事件のことは話した。でも動く気配すら見せない",
              "maleVariant": "",
              "stringId": "3397142649157029912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "殺害の動機は？ 何か心当たりは？",
              "maleVariant": "",
              "stringId": "3397142652864794636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心当たり？ 悪に理由なんか必要ないでしょ",
              "maleVariant": "",
              "stringId": "3397142652864794648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わからない。でも、ジェイソンがラインダーのためにやっていたことが記録されてるはず",
              "maleVariant": "",
              "stringId": "3397142673165225988"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを受け取る] わかった、じゃあね",
              "maleVariant": "[チップを受け取る] わかった、じゃあな",
              "stringId": "3397142682761793536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを受け取る] ジェイソンの母親に、望みは叶えると伝えて",
              "maleVariant": "[チップを受け取る] ジェイソンの母親に、望みは叶えると伝えてくれ",
              "stringId": "3397142682778570760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "任せておいて",
              "maleVariant": "任せておけ",
              "stringId": "3397142682979897364"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい？ 私たちは正義のためにお金を払ってる。17人が汗水たらしてかき集めたお金よ――決して軽くはない",
              "maleVariant": "",
              "stringId": "3397142682996674564"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "覚えておく",
              "maleVariant": "",
              "stringId": "3397142682996674576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインダーには必ず報いを受けさせる。ジェイソンのお母さんにもそう伝えておいて",
              "maleVariant": "ラインダーには必ず報いを受けさせる。ジェイソンの母親にもそう伝えておいてくれ",
              "stringId": "3397142689640452104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、伝えておく。V、お願いよ。私たちをがっかりさせないでね",
              "maleVariant": "",
              "stringId": "3397142689657229312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、任せて",
              "maleVariant": "ああ、任せろ",
              "stringId": "3397142689657229324"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインダーのドッグタグを持ってきて。見せしめにしてやるの。ゴロツキどもに、私たちもやられっぱなしではないと証明する",
              "maleVariant": "",
              "stringId": "3397142695613141000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティ出身。それで十分でしょ。あとドッグタウンに関しては、他と比べて別に良くも悪くもない",
              "maleVariant": "ナイトシティ出身。それで十分だろ。あとドッグタウンに関しては、他の場所と比べて別に良くも悪くもない",
              "stringId": "3397142698482044940"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうやって無関心を装っても、それはそれで自己紹介してるようなものよ",
              "maleVariant": "",
              "stringId": "3397142698482044952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一度でいいから、必死にかき集めたお金で正義をもたらしたいの。本物の正義をね",
              "maleVariant": "",
              "stringId": "3406004572904591360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マーサの息子、ジェイソンはフィクサーになりたがってた。仕事のためにラインダーをスタックスに呼び寄せたの",
              "maleVariant": "",
              "stringId": "3407481313590349824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも何かしら問題が起きて、ラインダーが発砲した… その結果… 遺体の身元はインプラントで特定したと言えばわかるかしら",
              "maleVariant": "",
              "stringId": "3407486695922569216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど",
              "maleVariant": "",
              "stringId": "3410210002514055168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジェイソンの遺体からこれが見つかった。暗号化されてるけど、あなたなら解読できるでしょ。この辺りは自由に調べていい。“痕跡”… はたくさん残ってるわ",
              "maleVariant": "",
              "stringId": "3410211755900899328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チップの中身は？",
              "maleVariant": "",
              "stringId": "3414338913489489920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チップの中身は？",
              "maleVariant": "",
              "stringId": "3414353134075875328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やってみる",
              "maleVariant": "",
              "stringId": "3414644246355218432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話す",
              "maleVariant": "",
              "stringId": "3429038275498971140"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインダーは私からすべてを奪った… あいつが同じ目に遭うまで、死んでも死にきれない",
              "maleVariant": "",
              "stringId": "3429317283167965184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立つ] やってみる",
              "maleVariant": "",
              "stringId": "3515940200084385796"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインダーはジェイソンに何の用が？",
              "maleVariant": "",
              "stringId": "3569505197557649408"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}