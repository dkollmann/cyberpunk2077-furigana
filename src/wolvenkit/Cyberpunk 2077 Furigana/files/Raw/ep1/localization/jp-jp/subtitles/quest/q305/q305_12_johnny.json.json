  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミはようやく自由になった",
              "maleVariant": "",
              "stringId": "3115707157979205636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミをマイヤーズのモルモットにするわけにはいかなかった",
              "maleVariant": "",
              "stringId": "3115708640783110144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リードを失望させたくなかった",
              "maleVariant": "",
              "stringId": "3115711565471289344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミを殺せなかった",
              "maleVariant": "",
              "stringId": "3115711565471289356"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミの望みをかなえてあげただけ。間違ってたって言いたいの？",
              "maleVariant": "ソミの望みをかなえてあげただけだ。間違ってたって言いたいのか？",
              "stringId": "3115719512838508544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミをマイヤーズに渡す気はなかった。モルモットとしていじくり回されるのがオチよ",
              "maleVariant": "ソミをマイヤーズに渡す気はなかった。モルモットとしていじくり回されるのがオチだ",
              "stringId": "3115719581943861248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これまでたくさん手にかけてきたけど、ソミだけは無理だった",
              "maleVariant": "これまでたくさん手にかけてきたが、ソミだけは無理だったよ",
              "stringId": "3115719803302449152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ま、勲章はもらえた",
              "maleVariant": "",
              "stringId": "3115728336093843456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だがソングバードやリードのことを考えてみると… この俺がなぜあんな人生を送ったのかもわかってきた",
              "maleVariant": "",
              "stringId": "3115728440011919360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "間違ってたとは思わないが…",
              "maleVariant": "",
              "stringId": "3115798679940632576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だよな。州境で見たマイヤーズ… あいつは人間の皮とスーツをかぶった、人間じゃない何かだよ",
              "maleVariant": "",
              "stringId": "3115800939798073344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミが死んだほうが皆のためよ",
              "maleVariant": "ソミが死んだほうが皆のためだ",
              "stringId": "3115802781751840768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "選択肢はなかった。正しかったと思いたい",
              "maleVariant": "",
              "stringId": "3115803176586842112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正しいかどうかはわかりやしない。マイヤーズがあの女の頭から何を引き出すか、誰にもわからないようにな。やりたい放題さ",
              "maleVariant": "",
              "stringId": "3115806553303552000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いくつもある真実のうち、どれかひとつを選ぶしかなかった。リードのをね",
              "maleVariant": "いくつもある真実のうち、どれかひとつを選ぶしかなかった。リードのをな",
              "stringId": "3115810161109635072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "戦時中にコールってチューマがいた。M-9でズタズタにされ、俺の膝の上を血まみれにしたんだ",
              "maleVariant": "",
              "stringId": "3115811828160925696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すると奴は、もういいから煙草をくれと言ってきた",
              "maleVariant": "",
              "stringId": "3115812195783282688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、死なせたの？",
              "maleVariant": "で、死なせたのか？",
              "stringId": "3115814864551809024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ま、他に救う方法があったんじゃないかって悩まなきゃいけないのは、俺じゃない",
              "maleVariant": "",
              "stringId": "3120178310433402880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "違うな。お前が選んだのは自分に嘘をつくことだ",
              "maleVariant": "",
              "stringId": "3120208213254008832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミはもう一度生きるチャンスを得られるはず",
              "maleVariant": "",
              "stringId": "3120212899616976896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミが死ぬべき理由はなかった。正しいことをしただけよ",
              "maleVariant": "ソミが死ぬべき理由はなかった。正しいことをしただけだ",
              "stringId": "3120213548056375296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は奴の内臓が出てこないよう腹を押さえてた。大丈夫だ、家に帰してやるぞって言い聞かせながらな…",
              "maleVariant": "",
              "stringId": "3120217386331074560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "煙草を吸わせてやったのさ",
              "maleVariant": "",
              "stringId": "3120223201934897152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前自身にとって？ あの女にとって？ それとも俺か？",
              "maleVariant": "",
              "stringId": "3120226945418326016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミはきっと回復する。それが求めてる結末よ",
              "maleVariant": "ソミはきっと回復する。それが求めてる結末だ",
              "stringId": "3120227151610310656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ、何の根拠があるんだ？ お前の言葉は、がらんどう、空っぽだよ",
              "maleVariant": "",
              "stringId": "3120227523796070400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたたちは全然似てない",
              "maleVariant": "お前たちは全然似てない",
              "stringId": "3292636477372166148"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リードはソミを救うことに全力だった。でもあんたは…",
              "maleVariant": "リードはソミを救うことに全力だった。でもお前は…",
              "stringId": "3292636748139655172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたたちは全然似てない",
              "maleVariant": "お前たちは全然似てない",
              "stringId": "3292637256740958208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "たしかに共通点は多いけど…",
              "maleVariant": "たしかに共通点は多いが…",
              "stringId": "3292639883935543296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分で見つけるしかない",
              "maleVariant": "",
              "stringId": "3292675651668209668"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知らないほうがいい",
              "maleVariant": "",
              "stringId": "3292679914725240832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "理由は自分で見つけるしかない",
              "maleVariant": "",
              "stringId": "3292689397593600000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんというか、理由を知らないほうがいい時もあるんじゃない？",
              "maleVariant": "なんというか、理由を知らないほうがいい時もあるんじゃないか？",
              "stringId": "3292689490237386752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずは鏡に映った自分でも見てみるか",
              "maleVariant": "",
              "stringId": "3292690348308099072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真実を語ることで自由になれるとは限らない。知らぬが仏、というだろ",
              "maleVariant": "",
              "stringId": "3292692546358276096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし… 話はこれくらいで充分だろ",
              "maleVariant": "",
              "stringId": "3292694081121218560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最初はサブロウ。それからヨリノブ、今度はマイヤーズにソングバード… お前はホントに危険に飛び込むのが好きな奴だ",
              "maleVariant": "",
              "stringId": "3296952321967321088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、すべてを終えて、今回の結果についてはどう思ってるんだ？",
              "maleVariant": "",
              "stringId": "3296953035183550464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一度戦争から逃げた俺が、どうしてアラサカに戦争を仕掛けたのか。なぜケリーのことを腰抜けと罵り続けたのか",
              "maleVariant": "",
              "stringId": "3296975010098507776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] あんたと話をするだけ無駄ね",
              "maleVariant": "[座る] お前と話をするだけ無駄だな",
              "stringId": "3296979310333849604"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] もういい",
              "maleVariant": "",
              "stringId": "3296979489296412672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] 何もせずに文句を言うのは簡単よ",
              "maleVariant": "[座る] 何もせずに文句を言うのは簡単だろ",
              "stringId": "3296979600747458560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話すだけ無駄ね",
              "maleVariant": "話すだけ無駄だな",
              "stringId": "3296984509391761408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分は何もしてないくせに、偉そうに文句だけ言うのね。あんたはいつもそう",
              "maleVariant": "自分は何もしてないくせに、偉そうに文句だけ言うんだな。お前はいつもそうだ",
              "stringId": "3296984598864654336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もういい、やめて",
              "maleVariant": "もういい、やめろ",
              "stringId": "3296984709191626752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "慰めのひとつも言えなくて、悪かったな…",
              "maleVariant": "",
              "stringId": "3296986142922174464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "理想は捨てたの？",
              "maleVariant": "理想は捨てたのか？",
              "stringId": "3296999999661076480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "理想は捨てたわけ？ ここにきて、今さら？",
              "maleVariant": "理想は捨てたのかよ？ ここにきて、今さら？",
              "stringId": "3297006538262245376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただ理解しただけだ。俺たちが信じるすべての裏側には、向き合いたくない醜い真実がある",
              "maleVariant": "",
              "stringId": "3297008017928810496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気に食わない顔が映ってるんだろうな",
              "maleVariant": "",
              "stringId": "3297010271729676288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あるいはいい加減大人になれってことかもな",
              "maleVariant": "",
              "stringId": "3297010751272841216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "預言者ギャリーは正しかったのかも。マイヤーズが爬虫類人だって話",
              "maleVariant": "預言者ギャリーは正しかったのかもな。マイヤーズが爬虫類人だって話",
              "stringId": "3356350935960784896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ、それか科学魔術師だな。とにかく… お前はあのイカれた問題にケリをつけた。やり方は二流だがな",
              "maleVariant": "",
              "stringId": "3356353107335188480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "隠そうとしたって隠せないもんさ。つまり最悪の事態も考えられた。だが、どうにか事を収められたな。おめでとさん",
              "maleVariant": "",
              "stringId": "3356357971872190464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なあ…",
              "maleVariant": "",
              "stringId": "3363312004000575488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FIAのゴタゴタに巻き込まれて以来、上手く言葉にできない妙な感覚があったんだ…",
              "maleVariant": "",
              "stringId": "3363320317010440192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺の内面をむしばむ恐怖が、どんどん声を大きくして叫んでやがる。今すぐ、できる限り遠くまで逃げろとな。その正体がやっとわかった気がする",
              "maleVariant": "",
              "stringId": "3363321665529507840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは事態が複雑になるといつも逃げる",
              "maleVariant": "お前は事態が複雑になるといつも逃げる",
              "stringId": "3363324541261770752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは、過去からの亡霊？",
              "maleVariant": "それは、過去からの亡霊か？",
              "stringId": "3363325775142121472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分には優れた直感があるって言いたいの？",
              "maleVariant": "自分には優れた直感があるって言いたいのか？",
              "stringId": "3363326000913117184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今更？ 事態が複雑になったり居心地が悪くなると、すぐいなくなるじゃない",
              "maleVariant": "今更？ 事態が複雑になったり居心地が悪くなると、すぐいなくなるだろうが",
              "stringId": "3363326654721224704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ？ この俺様が、いつそんなことをした",
              "maleVariant": "",
              "stringId": "3363329174608736256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "過去からの亡霊がまとわりついて来るとか？",
              "maleVariant": "",
              "stringId": "3363330707492630528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "諦めの悪いクズどもがな",
              "maleVariant": "",
              "stringId": "3363331143247261696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "当ててあげる。俺は初めから全部わかってた、並外れたロッカーボーイの直感だ、とか言いたいんでしょ",
              "maleVariant": "当ててやる。俺は初めから全部わかってた、並外れたロッカーボーイの直感だ、とか言いたいんだろ",
              "stringId": "3363331579689758720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなのは、当たり前すぎて今更言うまでもないだろ",
              "maleVariant": "",
              "stringId": "3363332153336328192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "物語ってのは、常にハッピーエンドばかりとは限らない。永久に答えの見つからない問いが残ることだってある",
              "maleVariant": "",
              "stringId": "3363336770711384064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうみたいね",
              "maleVariant": "そうみたいだな",
              "stringId": "3363338531782193152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "結末を迎えてよかった",
              "maleVariant": "",
              "stringId": "3363340068994609152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "忘れればいい",
              "maleVariant": "",
              "stringId": "3363340859268591616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "結末を迎えたのよ。気にしても仕方ない",
              "maleVariant": "結末を迎えたんだ。気にしても仕方ない",
              "stringId": "3363341797836386304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人生では色んな人に出会う。時にそいつらは、糞みたいに疑問を垂れ流していく。そんなもんでしょ",
              "maleVariant": "人生では色んな人に出会う。時にそいつらは、糞みたいに疑問を垂れ流していく。そんなもんだろ",
              "stringId": "3363342043219947520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真実が含まれることもあれば、そうじゃないこともある。忘れるしかないのよ。他に方法はない",
              "maleVariant": "真実が含まれることもあれば、そうじゃないこともある。忘れるしかないんだ。他に方法はない",
              "stringId": "3363349253027196928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "過去は過去。過ぎたことは、そっとしておけばいい",
              "maleVariant": "",
              "stringId": "3363351560229941248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実はまだ続いていたとしたら？",
              "maleVariant": "",
              "stringId": "3363352151039602688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ… 何にしたって、いつもロクなことにならねえな",
              "maleVariant": "",
              "stringId": "3363354759393374208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "犯した間違いは一生つきまとう",
              "maleVariant": "",
              "stringId": "3363357665559506944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "過去の過ちにこだわってると、もう一度それに足をすくわれるよ",
              "maleVariant": "過去の過ちにこだわってると、もう一度それに足をすくわれるぞ",
              "stringId": "3363357923307876352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういう意味だ？",
              "maleVariant": "",
              "stringId": "3363360130216423424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "必死に逃げてたつもりなのに、いつの間にかきれいな円を描いて、元居た場所に一周してるのよ",
              "maleVariant": "必死に逃げてたつもりなのに、いつの間にかきれいな円を描いて、元居た場所に一周してるんだ",
              "stringId": "3363360334546137088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そして衝突か",
              "maleVariant": "",
              "stringId": "3363365986991534080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういうこと",
              "maleVariant": "",
              "stringId": "3363366474990415872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人間関係を振り返ってみたら？",
              "maleVariant": "人間関係を振り返ってみろよ？",
              "stringId": "3366409245728894976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、もういい",
              "maleVariant": "",
              "stringId": "3366413875133214720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真実だ",
              "maleVariant": "",
              "stringId": "3366415016738877440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういう疑問は、死ぬまで一生つきまとうんだ",
              "maleVariant": "",
              "stringId": "3366416413257560064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺がなぜリードの奴を信用できなかったか、ようやくわかったよ",
              "maleVariant": "",
              "stringId": "3486555175345385472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつは俺とそっくりなんだ。理想主義者。そしてその信念が、周りの人間を食い散らかすってことに気づいちゃいねえ",
              "maleVariant": "",
              "stringId": "3486555252721905664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが、ひとつ違うとこがある",
              "maleVariant": "",
              "stringId": "3486555952566693888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺と違って奴は最後までやり遂げた。代償もクソもねえ。そこは認めてやる",
              "maleVariant": "",
              "stringId": "3486556008904585216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺がまだリードの野郎に同情できる理由が、ようやくわかった気がする",
              "maleVariant": "",
              "stringId": "3486556087455510528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちはそっくりなのさ。大義を掲げて戦う理想主義者。そのせいで周りの人間が犠牲になることに気づいちゃいねえ",
              "maleVariant": "",
              "stringId": "3486556140219854848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "唯一の違いは奴が体制側の人間で、その原理を心の底から信じてるってことだ。理想を追い求める者と、単に夢を見てる世間知らずの差は…",
              "maleVariant": "",
              "stringId": "3486556198017363968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "紙一重だがな",
              "maleVariant": "",
              "stringId": "3486556249103986688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そっくり？ 全然違う。リードは過ちを認め過去を償おうとして、彼自身とソミのために戦った。でもあんたは…？",
              "maleVariant": "そっくり？ 全然違う。リードは過ちを認め過去を償おうとして、あいつ自身とソミのために戦った。でもお前は…？",
              "stringId": "3486556746380668928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かにあたしもあんたとリードには共通点が多いと思ってた。でも理想と向き合うにしても、いろんな方法があるはず",
              "maleVariant": "確かに俺もお前とリードには共通点が多いと思ってた。だが理想と向き合うにしても、いろんな方法があるはずだ",
              "stringId": "3486556843000655872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミを救うために手を尽くしたリードは、いずれ彼女の死も受け入れられる。でもあんたは…",
              "maleVariant": "ソミを救うために手を尽くしたリードは、いずれ彼女の死も受け入れられる。だがお前は…",
              "stringId": "3486556913381076992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたたちは全然似てない。リードはソミを救うために手を尽くした。自分自身さえ犠牲にしてね。でもあんたは…",
              "maleVariant": "お前たちは全然似てない。リードはソミを救うために手を尽くした。自分自身さえ犠牲にしてな。だがお前は…",
              "stringId": "3486556998894546944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は結局、理想を追ってたわけじゃなかった。俺は… 生きることから逃げてたんだ",
              "maleVariant": "",
              "stringId": "3486557102342860800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、今も逃げてるの？",
              "maleVariant": "で、今も逃げてるのか？",
              "stringId": "3486557167958552576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今も息はしてる… 一応な。だがなんのためかはわからん",
              "maleVariant": "",
              "stringId": "3486557230956998656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今そういう話はしたくない",
              "maleVariant": "",
              "stringId": "3508321837089951744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー… 今そういう話はしたくない",
              "maleVariant": "",
              "stringId": "3508322632128663552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああそうか、そうだよな。じっくり振り返って結論を出す… 考えなしのお前には難しかったか",
              "maleVariant": "",
              "stringId": "3508324793268965376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "脱走兵からテロリストに鞍替えした奴がよく言えたもんね",
              "maleVariant": "脱走兵からテロリストに鞍替えした奴がよく言えたもんだ",
              "stringId": "3508327608687136768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立つ] 話せてよかった。あんたも変わったね",
              "maleVariant": "[立つ] 話せてよかった。お前も変わったな",
              "stringId": "3541773454095347712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立つ] あんたからそんな話が聞けるとはね",
              "maleVariant": "[立つ] お前からそんな話が聞けるとはな",
              "stringId": "3541774639388880896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、話せてよかった、ジョニー。あんたも変わったね",
              "maleVariant": "なあ、話せてよかった、ジョニー。お前も変わったな",
              "stringId": "3541778055766929408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの口から、そういう考えや感情が出てくるとはね",
              "maleVariant": "お前の口から、そういう考えや感情が出てくるとはな",
              "stringId": "3541784900183191552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい意味でか？",
              "maleVariant": "",
              "stringId": "3541785557279633408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはいずれわかる。行きましょ",
              "maleVariant": "それはいずれわかる。もう行こう",
              "stringId": "3541786688902852608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺のことを知ったような口を利くじゃねえか",
              "maleVariant": "",
              "stringId": "3541787964709466112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別にそういうわけじゃない… ただ、いけ好かないロッカーボーイの中に、繊細な男がいたんだなって",
              "maleVariant": "",
              "stringId": "3541789064892182528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハ、ふざけやがって",
              "maleVariant": "",
              "stringId": "3556389753987461120"
            }
          ]
        }
      }
