  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "許せない。私のルックスには著作権があるのよ！？ 一生使用料を払ってもらうから、覚悟しなさい",
              "maleVariant": "",
              "stringId": "3237458293385302016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたのルックス？ ふざけないで！",
              "maleVariant": "",
              "stringId": "3237458539305734144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バッグに靴にサングラス、全部特注品よ。リナ・マリーナは登録商標なの。このモノマネビッチ",
              "maleVariant": "",
              "stringId": "3237458834131750912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、何なのこれ… 私に何を演じろっていうのよ",
              "maleVariant": "",
              "stringId": "3237459745822445568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたはそこにいる彼の姉、それでいきましょ",
              "maleVariant": "あんたはそこにいる彼の姉、それでいこう",
              "stringId": "3237460227395653636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ツールの手紙を送る] これを読んで",
              "maleVariant": "[ツールの手紙を送る] これを読んでくれ",
              "stringId": "3237460459810426880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分は生き別れの姉だと伝えて",
              "maleVariant": "自分は生き別れの姉だと伝えるんだ",
              "stringId": "3237460787335237632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "セリフを送るから、それを読み上げて",
              "maleVariant": "セリフを送るから、それを読み上げてくれ",
              "stringId": "3237461156484321280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お姉ちゃんよ…",
              "maleVariant": "",
              "stringId": "3237468952202285056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "二度と会えないと思ってたのに… まさか、こうして会えるなんて！",
              "maleVariant": "",
              "stringId": "3237469395556995072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "嘘… 嘘よ。こんなのあり得ない…！",
              "maleVariant": "",
              "stringId": "3237469470349824000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私たち姉妹は昔からまさに瓜二つだった",
              "maleVariant": "",
              "stringId": "3237469725044740096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マジかよ。リナの奴、めっちゃ乗り気じゃねえか",
              "maleVariant": "",
              "stringId": "3237469993681522688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK… 来たわ",
              "maleVariant": "",
              "stringId": "3237470820496281600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私のフェイスプレートをつけて、靴まで真似するなんて！ この恥知らず！",
              "maleVariant": "",
              "stringId": "3275454853125476352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこの彼はFIAが作ったクローン、ってことで",
              "maleVariant": "",
              "stringId": "3275460794457755648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼はクローンだと伝えて。FIAに作られたの",
              "maleVariant": "彼はクローンだと伝えるんだ。FIAに作られたと",
              "stringId": "3275462457264410624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ… 私たち、そっくり過ぎると思わない？ まるで… 生き写しでしょ",
              "maleVariant": "",
              "stringId": "3275463820832641024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはあんたが私を真似してるからよ",
              "maleVariant": "",
              "stringId": "3275464689674326016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのさ、こういうのは私のスタイルじゃないかも。今後のことは一旦考えさせて。とにかく… 今日はありがと。チャオ！",
              "maleVariant": "",
              "stringId": "3275479073335250944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "上手くはまらなかったか… まあ政治陰謀モノはリナのキャラじゃないからな",
              "maleVariant": "",
              "stringId": "3275479567290044416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ？ まあ、そういう設定なら…",
              "maleVariant": "",
              "stringId": "3281644754151690240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すべて計画のうちよ。さあ、しっかりやって",
              "maleVariant": "すべて計画のうちだ。さあ、しっかり頼んだぞ",
              "stringId": "3281644813173936128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘイウッドで一緒に暮らしてた最初のアパートを覚えてる？ オールフーズとカン・タオの倉庫に挟まれてた…",
              "maleVariant": "",
              "stringId": "3281655857397125120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソートンのタイヤでできたブランコ… ほら、あんたが作ってくれたでしょう？",
              "maleVariant": "",
              "stringId": "3281657702337236992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お姉ちゃん… お姉ちゃんなのね…！",
              "maleVariant": "",
              "stringId": "3281657908730548224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お願いよ。思い出して",
              "maleVariant": "",
              "stringId": "3281662815613243392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よくやってくれた。期待をはるかに超える仕事ぶりだ",
              "maleVariant": "",
              "stringId": "3281671803922829312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え？ 本気で言ってる…？ デタラメにもほどがある",
              "maleVariant": "",
              "stringId": "3281682990936903680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… 自分が本物のリナだと思ってるわけね",
              "maleVariant": "",
              "stringId": "3281746183713476608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "唯一無二のね",
              "maleVariant": "",
              "stringId": "3281747599072972800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "両親のことを覚えてる？ 初めての生理日は？ なんでもいい。これより… 前の記憶はある？",
              "maleVariant": "",
              "stringId": "3281748395235758080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え… いや… わ、私を混乱させる気ね！",
              "maleVariant": "",
              "stringId": "3281751433371471872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなんじゃない！ でも思い出せないのは… あんたがクローンだからよ！",
              "maleVariant": "",
              "stringId": "3281753929754103808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バイオテクニカのラボで作られたの。FIAの命令で… 新合衆国のスパイである私の影武者としてね",
              "maleVariant": "",
              "stringId": "3281754436828680192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私の人生は… 嘘だった？",
              "maleVariant": "",
              "stringId": "3281755997126545408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って。あんたがそれを知ってるってことは、私の正体がバレてるってことなんじゃないの？ それじゃ影武者の意味が！",
              "maleVariant": "",
              "stringId": "3281757250049036288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずいわ。証拠隠滅のために消されちゃう！",
              "maleVariant": "",
              "stringId": "3281758371505262592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼、真に迫る演技だったわ。でも… なんていうか不気味で、背筋が凍った…",
              "maleVariant": "",
              "stringId": "3281763029397295104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前を褒める日が来るとは思わなかったぜ、V。やるときはやるじゃねえか",
              "maleVariant": "",
              "stringId": "3281810287710982144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「リナ様。僕は昔からのあなたのファンです。バッドランズのバイカーたちと撮影していたBDから見ています」",
              "maleVariant": "",
              "stringId": "3281814847389192192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「僕は今、辛い時期を過ごしています。散々な毎日ですが、あなたになっている間だけはすべてを忘れることができるんです」",
              "maleVariant": "",
              "stringId": "3281815867091603456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うう、ぐあああ！",
              "maleVariant": "",
              "stringId": "3281816665972297728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「全身全霊を込めてBDを作るあなたを応援しています。あなたの大ファン、エドガー・ツール」",
              "maleVariant": "",
              "stringId": "3281819708017102848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カット！",
              "maleVariant": "",
              "stringId": "3293315579598622724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、カット！",
              "maleVariant": "",
              "stringId": "3293317877322240000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪くなかったわね。初めはどうなるかと思ったけど… いいケミストリーを感じた",
              "maleVariant": "",
              "stringId": "3293323498494906368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他に撮るものはあるの？",
              "maleVariant": "",
              "stringId": "3293324704827068416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今日は以上だ。見事な演技だったよ",
              "maleVariant": "",
              "stringId": "3293326474487812096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ならもう行くわ。撮影スケジュールを送っておいてね。それじゃ、チャオ！",
              "maleVariant": "",
              "stringId": "3293327102811328512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当にきれいになった… 三つ編みを結ってあげたのが昨日のことみたい",
              "maleVariant": "",
              "stringId": "3293329851221299200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話したいことがたくさんあるわ！",
              "maleVariant": "",
              "stringId": "3293330735665795072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昔、ママの服でファッションショーをしたの",
              "maleVariant": "",
              "stringId": "3293335746852442112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "テッテレー！",
              "maleVariant": "",
              "stringId": "3293338456758702084"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これは芸術作品よ",
              "maleVariant": "これは芸術作品だ",
              "stringId": "3293338897294839808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すっかり騙されたでしょ？ いいリアクションだったよ",
              "maleVariant": "すっかり騙されただろ？ いいリアクションだったぞ",
              "stringId": "3293342209754812416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "素人に付き合ってる暇ないから。じゃあね",
              "maleVariant": "",
              "stringId": "3293347761889234944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シャンク。今の子にはもうBDを売るなよ",
              "maleVariant": "",
              "stringId": "3293350402371375104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー… 助かったよ。いま報酬を支払う",
              "maleVariant": "",
              "stringId": "3293350892450631680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい… シャンク！ なんだこの服は？",
              "maleVariant": "",
              "stringId": "3293351459285651456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "才能があるのかも",
              "maleVariant": "才能があるのかもな",
              "stringId": "3294829894084349956"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "満足したなら報酬を",
              "maleVariant": "",
              "stringId": "3294830489860067328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いまエディーを送る",
              "maleVariant": "",
              "stringId": "3294832149781041152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BDショービズの才能があるみたいね",
              "maleVariant": "BDショービズの才能があるみたいだな",
              "stringId": "3294832988255965184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お世辞はいいから、報酬を",
              "maleVariant": "",
              "stringId": "3294833122490470400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ったく、奇妙な一日だったぜ",
              "maleVariant": "",
              "stringId": "3294834521542512640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕事は済んだ",
              "maleVariant": "",
              "stringId": "3294841256890093572"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪い結果ではなかったはず",
              "maleVariant": "",
              "stringId": "3294841420266622976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだな。いま報酬を払う",
              "maleVariant": "",
              "stringId": "3294847112323026944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チューニングでなんとかまともな作品に仕上げられるといいが",
              "maleVariant": "",
              "stringId": "3294847507527127040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あとツールを見つけないとな。幸い、奴のトラッカーにはペアレンタルコントロールを設定してある",
              "maleVariant": "",
              "stringId": "3294848109208424448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さーて、どこ行った…？",
              "maleVariant": "",
              "stringId": "3294849257105543168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは言われたことをやったはずよ",
              "maleVariant": "俺は言われたことをやったはずだぞ",
              "stringId": "3294851524680503296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BDは撮れたんだし、上出来でしょ",
              "maleVariant": "BDは撮れたんだし、上出来だろ",
              "stringId": "3294852292489793536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うわ、あんたリナ・マリーナそっくりだな",
              "maleVariant": "",
              "stringId": "3294855259590774784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リナ・マリーナ本人だからよ",
              "maleVariant": "",
              "stringId": "3294855838404726784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人生しんどいと現実逃避したくなるのはわかる。別の誰かになろうとしてみたりな",
              "maleVariant": "",
              "stringId": "3294856242030989312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもどこかで自分自身を受け入れなきゃダメなんだ。誰もがな",
              "maleVariant": "",
              "stringId": "3294856517965860864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何これ、ドッキリ？",
              "maleVariant": "",
              "stringId": "3294857012038094848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん… それはどうも",
              "maleVariant": "ふん… そりゃどうも",
              "stringId": "3294872499136516096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "型にはまったお決まりのストーリーはもう古い。これはアートなの。人間の魂の迷宮に迫る芸術",
              "maleVariant": "型にはまったお決まりのストーリーはもう古い。これはアートなんだ。人間の魂の迷宮に迫る芸術さ",
              "stringId": "3298442019864334336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー… よう、ツール。元に戻って嬉しいよ",
              "maleVariant": "",
              "stringId": "3369536194302726144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "元に戻って？ いったい何のことだ？",
              "maleVariant": "",
              "stringId": "3369537866101968896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あとで全部説明してやる、な？ その… お客様の前で話すことじゃない",
              "maleVariant": "",
              "stringId": "3369538458337693696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫？",
              "maleVariant": "大丈夫か？",
              "stringId": "3375499390554398724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ツール？",
              "maleVariant": "",
              "stringId": "3375502813123239936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "調子はどう？",
              "maleVariant": "調子はどうだ？",
              "stringId": "3375514849936859136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[リナのバイクを手に入れる]",
              "maleVariant": "",
              "stringId": "3437685784382726148"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[リナのタンクトップを取る]",
              "maleVariant": "",
              "stringId": "3447503020542844928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "二日酔い… というか頭の中にクソをされたみたいな感じだな。ここ数日間の記憶もあやふやだ",
              "maleVariant": "",
              "stringId": "3520152486973034496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて、本番を始めるよ",
              "maleVariant": "さて、本番を始めるぞ",
              "stringId": "3612931596719247360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はいはい、お喋りはそこまでにして。監督の登場よ",
              "maleVariant": "はいはい、お喋りはそこまでにしろ。監督の登場だ",
              "stringId": "3612989858101612544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫？",
              "maleVariant": "大丈夫か？",
              "stringId": "3866736782724583424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "調子はどう？",
              "maleVariant": "調子はどうだ？",
              "stringId": "3866737113286070272"
