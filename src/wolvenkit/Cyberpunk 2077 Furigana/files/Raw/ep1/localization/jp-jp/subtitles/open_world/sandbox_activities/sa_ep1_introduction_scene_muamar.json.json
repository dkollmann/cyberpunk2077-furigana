  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よう。調子はどうだ？",
              "maleVariant": "",
              "stringId": "1933089478186401796"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうかしたか？",
              "maleVariant": "",
              "stringId": "1933089478219956228"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その「独創的な計画」について聞かせて",
              "maleVariant": "その「独創的な計画」について聞かせてくれ",
              "stringId": "3359361371935498244"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このミズタニは電気系統に問題がある",
              "maleVariant": "",
              "stringId": "3359369691924684800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミズタニ シオン MZ2の電気系トラブルは、大抵搭載コンピューターの故障が原因よ。見てあげようか？",
              "maleVariant": "ミズタニ シオン MZ2の電気系トラブルは、大抵搭載コンピューターの故障が原因だ。見てやろうか？",
              "stringId": "3359370428008259584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待て待て、V！ あまりでしゃばってくれるな。俺には譲れない領域がある。ファッション、髪型、そして車だ。こいつは自分でどうにかする",
              "maleVariant": "",
              "stringId": "3359370946038358016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "途中でちょっとした“障害”に出くわしたけど",
              "maleVariant": "途中でちょっとした“障害”に出くわしたが",
              "stringId": "3359371803001774080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シックス・ストリートか？ 少々行き違いがあってな。俺と仕事をするなら慣れておいた方がいいぞ",
              "maleVariant": "",
              "stringId": "3359372184415002624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前はスピードに飢えてるか、V？ まあ、順を追って説明しよう。まず1つ目… お前が運んだ車は傷ひとつない。完璧なものほど怖いもんはねえ",
              "maleVariant": "",
              "stringId": "3365086995320823808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前はスピードに飢えてるか、V？ まあ、順を追って説明しよう。まず1つ目… お前が持ってきた車は少々状態が悪い。傷の数だけ価値は下がる",
              "maleVariant": "",
              "stringId": "3365087165357907968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ！ どこのどいつだ、こんなイグニションを入れたバカは！ クソポンコツめ…！",
              "maleVariant": "",
              "stringId": "3367870303832879104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハハ！ 冗談だよ、冗談。ほら、いいから乗れよ。雨んなか突っ立ててもしょうがないだろ",
              "maleVariant": "",
              "stringId": "3368077586655326208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[乗る] これも誰かが“配達”した車？",
              "maleVariant": "[乗る] これも誰かが“配達”した車か？",
              "stringId": "3368081336866418692"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これも盗んだ車？",
              "maleVariant": "これも盗んだ車か？",
              "stringId": "3368082489662484480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そして2つ目… 俺は車を売りたい。3つ目… ドッグタウンはチャンスの宝庫だ。4つ目… 「企み」ではなく「独創的な計画」と呼んでくれ",
              "maleVariant": "",
              "stringId": "3394176178864885760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V！ 見てくれよ、またこのザマだ！ 見てくれはいいが役立たず。まあ、お前の真逆ってことだな",
              "maleVariant": "",
              "stringId": "3394178013302460416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「企み」は「企み」でしょ",
              "maleVariant": "「企み」は「企み」だろ",
              "stringId": "3394291427416838144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、その“独創的な計画”とやらについて聞かせて",
              "maleVariant": "じゃあ、その“独創的な計画”とやらについて聞かせろ",
              "stringId": "3394324404444172288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どう考えても「企み」の方が適切でしょ。口車には乗せられない",
              "maleVariant": "どう考えても「企み」の方が適切だろ。口車には乗せられない",
              "stringId": "3394324483582300160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺はなんつうか… 第六感みたいなもんを持ってるんだ。退屈して持ち主を変えたがってる車がわかるのさ。奴らの声はドッグタウンから聞こえることもあれば、ナイトシティから聞こえることもある",
              "maleVariant": "",
              "stringId": "3394324643049738240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なに、簡単に金儲けできるってだけさ",
              "maleVariant": "",
              "stringId": "3394334107043729408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はいはい、わかったよ。別に仲良しになる必要はねえ。ひと儲けできればいい",
              "maleVariant": "",
              "stringId": "3394335263077797888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その第六感が、この違法ショップまで車を運べって言ってるの？",
              "maleVariant": "その第六感が、この違法ショップまで車を運べって言ってるのか？",
              "stringId": "3394337169976168448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「違法」ね… その人聞きの悪い呼び方はともかく、そうだ。車を届ければ金を払う。だが、これはもっと大きな計画の一環に過ぎない…",
              "maleVariant": "",
              "stringId": "3394341780522897408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなはずはない",
              "maleVariant": "",
              "stringId": "3395603725884428292"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "排除すればいい",
              "maleVariant": "",
              "stringId": "3395603814166138880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなはずはない",
              "maleVariant": "",
              "stringId": "3395631242716483584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "排除すればいい。相手はパトカー1台よ",
              "maleVariant": "排除すればいい。相手はパトカー1台だ",
              "stringId": "3395631404297850880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着け、ここは俺のシマだ。話を聞いてみよう…",
              "maleVariant": "",
              "stringId": "3395631640168730624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "NCPDだ！ 免許と登録証明書はあるんだろうな？",
              "maleVariant": "",
              "stringId": "3395675282438721536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "来る場所を間違えたね、チューマ",
              "maleVariant": "来る場所を間違えたな、チューマ",
              "stringId": "3395680985014439940"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見て見ぬふりは警官の得意技でしょ。今が使い時よ",
              "maleVariant": "見て見ぬふりは警官の得意技だろ。今が使い時だ",
              "stringId": "3395683945874296832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんだこいつは？ ちゃんとしつけとけよ",
              "maleVariant": "",
              "stringId": "3395686835548426240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダニエルズ、何のつもりだ？ パトカーで盗難車の受け取りか？ しかも制服姿で",
              "maleVariant": "",
              "stringId": "3395689111746875392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "管轄内だから当然だろ。それと――お偉方のためにアルヴァラードも頼む。計画に必要な装備は任せとけ",
              "maleVariant": "",
              "stringId": "3395690300009975808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "勘弁してくれ…",
              "maleVariant": "",
              "stringId": "3395691271142346752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと待って。賄賂ってこと？ まったく… この街に誠実な警官はもう残ってないの？",
              "maleVariant": "ちょっと待て。賄賂ってことか？ まったく… この街に誠実な警官はもう残ってないのかよ？",
              "stringId": "3395694284212899840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残ってるさ。ただ誠実な奴は金もなけりゃ、目に光もない",
              "maleVariant": "",
              "stringId": "3395694374843420672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "金は受け取った。すぐに車の詳細と書類を送る",
              "maleVariant": "",
              "stringId": "3395709885597716480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの見返りは？",
              "maleVariant": "俺の見返りは？",
              "stringId": "3395713936523628548"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "車泥棒ね… 考えてみる",
              "maleVariant": "車泥棒か… 考えてみる",
              "stringId": "3395715952037695488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あれが顧客の1人だ。で、お前も一枚噛むか？",
              "maleVariant": "",
              "stringId": "3395716518184849408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「計画」って結局何なの？",
              "maleVariant": "「計画」って結局何なんだ？",
              "stringId": "3395717840430796800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「泥棒」って呼び方も好きじゃなさそうね",
              "maleVariant": "「泥棒」って呼び方も好きじゃなさそうだな",
              "stringId": "3395727513569562624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前もわかってきたじゃねえか、V。そうだな… 「特別配達」とでも呼んでくれ。そしてお前が配達人ってわけだ",
              "maleVariant": "",
              "stringId": "3395727882198552576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[降りる]",
              "maleVariant": "",
              "stringId": "3395741436008042500"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこかで聞いたようなセリフね… ま、考えておく",
              "maleVariant": "どこかで聞いたようなセリフだな… ま、考えておく",
              "stringId": "3395747398932598784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、任せとけ。きっと後悔はしないぜ。じゃあ、また連絡するからよ",
              "maleVariant": "",
              "stringId": "3395747566637649920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうせあちこち走り回るなら、ついでに稼ぐのも悪くないか。報酬はもらえるんでしょ？",
              "maleVariant": "どうせあちこち走り回るなら、ついでに稼ぐのも悪くないか。報酬はもらえるんだろ？",
              "stringId": "3395747945332969472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "当然だ。エル・キャピタンと組めば未来は明るいぞ！ 仕事が貰えて、懐は潤う。いいこと尽くめだ",
              "maleVariant": "",
              "stringId": "3395749146682298368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "期待してる",
              "maleVariant": "",
              "stringId": "3395750968805396480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おいおい！ つけられたな、V",
              "maleVariant": "",
              "stringId": "3397068990182690816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダニエルズも言ってたけど、「計画」って？",
              "maleVariant": "ダニエルズも言ってたが、「計画」って？",
              "stringId": "3507287391210934272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フッ… 引き受けてくれればわかるさ… いずれな。で、どうするんだ？ やるのか？",
              "maleVariant": "",
              "stringId": "3507346475951951872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[乗る] 誰にでもそんな口をきくの？",
              "maleVariant": "[乗る] 誰にでもそんな口をきくのか？",
              "stringId": "3516138465703333888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつもそうやって仕事相手に毒を吐いてるわけ？",
              "maleVariant": "いつもそうやって仕事相手に毒を吐いてるのか？",
              "stringId": "3518414824911769600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "用件を言って。車を集めて何を企んでるの？",
              "maleVariant": "用件を言え。車を集めて何を企んでるんだ？",
              "stringId": "3518415043703443456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「盗んだ」？ おいおい、人聞き悪いな",
              "maleVariant": "",
              "stringId": "3518485864862027776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっとした確認さ。エゴの塊とは仕事したくねえからな",
              "maleVariant": "",
              "stringId": "3518488126950838272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜあたしは追われたの？",
              "maleVariant": "なぜ俺は追われたんだ？",
              "stringId": "3518493032793346048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "な？ こういう商売だ。よし、雨はやんだぞ",
              "maleVariant": "",
              "stringId": "3518527142601527296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今なんて？",
              "maleVariant": "",
              "stringId": "3527523994692849664"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}