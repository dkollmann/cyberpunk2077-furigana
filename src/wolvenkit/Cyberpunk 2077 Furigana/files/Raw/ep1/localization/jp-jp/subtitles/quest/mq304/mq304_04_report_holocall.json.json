    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V… 進捗はどうだ？ ヤゴは… 考え直したか？",
              "maleVariant": "",
              "stringId": "3237362592739790848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何よりだ。他には何かあるか？",
              "maleVariant": "",
              "stringId": "3237366576858828800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ベネットとアラサカは繋がってる",
              "maleVariant": "",
              "stringId": "3237367012345024516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何もない",
              "maleVariant": "",
              "stringId": "3237367451538984960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実は… ヤゴいわく、ベネットはアラサカと繋がってる",
              "maleVariant": "",
              "stringId": "3237367700210880512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一応調べておく。たわ言と真実を切り分けるのは、傭兵ではなくフィクサーである私の仕事だ。教えてくれて感謝する",
              "maleVariant": "",
              "stringId": "3237368607438835712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "特にない",
              "maleVariant": "",
              "stringId": "3237373560911859712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "葬儀と通夜はブラック・サファイアで執り行われる。アギラルなら問題なく参列できるはずだ",
              "maleVariant": "",
              "stringId": "3237374037133135872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ベネットとも会った",
              "maleVariant": "",
              "stringId": "3276545680598142976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ベネットとも会った。あいつは完全にアラサカの操り人形よ",
              "maleVariant": "ベネットとも会った。あいつは完全にアラサカの操り人形だぞ",
              "stringId": "3276546133348093952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼んだ覚えはないが… 感謝する。それでもなお、ヤゴよりベネットの方がリーダーにふさわしい",
              "maleVariant": "",
              "stringId": "3276547527937396736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヤゴとは話した。ヴードゥーもこれ以上邪魔はしないはずよ",
              "maleVariant": "ヤゴとは話した。ヴードゥーもこれ以上邪魔はしないはずだ",
              "stringId": "3327407514928390144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言いづらいことでもあるのか？",
              "maleVariant": "",
              "stringId": "3327410702029004800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人間は締め上げられると、おかしな音を漏らすものだ",
              "maleVariant": "",
              "stringId": "3327414089852231680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、次は？",
              "maleVariant": "",
              "stringId": "3327419306727546880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死者に敬意を表すのさ。アギラルとしてハンセンの葬儀に参列し、代替わりが滞りなく済むよう見届けてくれ",
              "maleVariant": "",
              "stringId": "3327419491360808960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…トラブルが起きると思ってるの？",
              "maleVariant": "…トラブルが起きると思ってるのか？",
              "stringId": "3327421985243635712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こうした場では、感情が噴出しがちだ。悲しみの形は実に多様で、理性的であることは少ない",
              "maleVariant": "",
              "stringId": "3327422116038811648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうか",
              "maleVariant": "",
              "stringId": "3327433358652579840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": "3327471567285624832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "国王崩御、国王万歳――頼んだぞ、V",
              "maleVariant": "",
              "stringId": "3327471709791297536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも、糸は切っておいた。結び直されないといいけどね",
              "maleVariant": "だが、糸は切っておいた。結び直されないといいがな",
              "stringId": "3327477753649143808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ベネットはリーダーに適してない。アラサカとの取引から手を引かせ、ヤゴをサポートするよう説得した",
              "maleVariant": "",
              "stringId": "3387147594756366336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうことだ？ ベネットがバーゲスト、そしてドッグタウンの新たなリーダーになるべきだと、はっきり伝えたはずだぞ。アラサカの関与はお前が気にすることではない",
              "maleVariant": "",
              "stringId": "3387150799473057792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正義感か何か知らないが、ただちに過ちを正せ。さもなければ、この依頼はなかったことにする",
              "maleVariant": "",
              "stringId": "3387155415220723712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ベネットはリーダーに適してない",
              "maleVariant": "",
              "stringId": "3466610371773964288"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
