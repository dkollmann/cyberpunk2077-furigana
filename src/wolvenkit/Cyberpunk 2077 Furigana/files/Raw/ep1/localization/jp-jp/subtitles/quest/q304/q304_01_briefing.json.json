  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あ、来た。V",
              "maleVariant": "",
              "stringId": "2889744952298852352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待ってたぞ。2人で作戦を詰めていた",
              "maleVariant": "",
              "stringId": "2889746351870988288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カート・ハンセン…",
              "maleVariant": "",
              "stringId": "2889750446753206276"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "元はフットボールスタジアムなの？",
              "maleVariant": "元はフットボールスタジアムなのか？",
              "stringId": "2889751332472770560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バーゲストの指揮系統について教えて",
              "maleVariant": "バーゲストの指揮系統について教えてくれ",
              "stringId": "2889751422398648320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カート・ハンセン… 少し若い頃ね。憎らしいツラは変わらないけど",
              "maleVariant": "カート・ハンセン… 少し若い頃だな。憎らしいツラは変わらないが",
              "stringId": "2889752759341436928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトホークスって？",
              "maleVariant": "",
              "stringId": "2889753310187769856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで撤退を？ なのにハンセンはナイトシティが好き過ぎて戻って来たわけ？",
              "maleVariant": "それで撤退を？ なのにハンセンはナイトシティが好き過ぎて戻って来たわけか？",
              "stringId": "2889753466685640704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "プロのアメフトチームだ。パシフィカの大物投資家たちが出資し、負け知らずだった",
              "maleVariant": "",
              "stringId": "2889753876754354176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バーゲストの中枢にいるのは… 元ミリテクのコマンドー？",
              "maleVariant": "バーゲストの中枢にいるのは… 元ミリテクのコマンドーか？",
              "stringId": "2889753963391897612"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2人がナイトシティで使うレンタカーを利用する",
              "maleVariant": "",
              "stringId": "2889799740076085248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リード、あんたの役目は？",
              "maleVariant": "リード、お前の役目は？",
              "stringId": "2889804446521380864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて、そろそろ話をナイトシティに戻してもいいか",
              "maleVariant": "",
              "stringId": "3028682092087472128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ… だが、似た計画だとしても、状況が異なる",
              "maleVariant": "",
              "stringId": "3030390136841093120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから、メデジンもそうだったでしょ。数年後のナイトシティでの作戦もね",
              "maleVariant": "",
              "stringId": "3030392084004782080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ID、生体認証、行動様態… すべてコピーした。昔ながらの方法だが確実だ",
              "maleVariant": "",
              "stringId": "3030392609030979584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "比較してみましょうか。グローバルな武器取引に、成りすまし、ああ、あと自信過剰っぷりも変わってない",
              "maleVariant": "",
              "stringId": "3030393253645172736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "統一戦争時代の写真だ。「真夜中の嵐」作戦… 奴が指揮を執っていた",
              "maleVariant": "",
              "stringId": "3030453411121381376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴の部隊は排水路に身を潜め、増援を待っていたが… 湾に現れたのはアラサカの超大型空母だったのさ",
              "maleVariant": "",
              "stringId": "3030454563397353472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや。奴らは新合衆国とミリテクに見捨てられたんだ。作戦は闇に葬られた。ハンセンは結局ゾーンを掌握し… 自分のものにした",
              "maleVariant": "",
              "stringId": "3030460843428069376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今の奴があるのも現在の状況も… すべては我々の愚かな判断の結果だ",
              "maleVariant": "",
              "stringId": "3030464136594243584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのときと一緒よ、リード。そっくりじゃない",
              "maleVariant": "",
              "stringId": "3031537465396740096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とある作戦でナイトシティに来たハンセンの部下たちだ。十数人ほどしかいない",
              "maleVariant": "",
              "stringId": "3031760876928679936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "傍目には盤石に見えるが、実情はそれとは程遠い。ハンセンを失えば指揮系統は一気に崩壊する",
              "maleVariant": "",
              "stringId": "3031761226985291776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メデジンでの作戦について聞かせて",
              "maleVariant": "メデジンでの作戦について聞かせてくれ",
              "stringId": "3031860569864060928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、何があったの？",
              "maleVariant": "で、何があった？",
              "stringId": "3031870453271674880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コロンビア大統領の後ろ盾を持つ男に、密輸した企業のテックを売っていたんだ",
              "maleVariant": "",
              "stringId": "3031891841319616512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FIAは海外でも暗躍してるの？",
              "maleVariant": "FIAは海外でも暗躍してるのか？",
              "stringId": "3031893629569515520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちの活動の多くは公にされない",
              "maleVariant": "",
              "stringId": "3031893834469654528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく、その作戦でも成りすましの手を使い、確かに問題が生じたが… 話せば長くなる",
              "maleVariant": "",
              "stringId": "3031895580323528704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ルイスの件はソミが資料をまとめたんだが…",
              "maleVariant": "",
              "stringId": "3033287155951779840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "重要な情報が抜けてたんだ。そのエンジニアは犬が大の苦手だったのさ。それも犬を見ただけで冷や汗を垂らし、手が震え、小便をチビるレベルだ",
              "maleVariant": "",
              "stringId": "3033287807595626496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だがドーベルマンが脚にすり寄ってきた時… アレックスは頭を撫でてしまった。「いい子、いい子」と可愛がってから、会議の席に着くや否や…",
              "maleVariant": "",
              "stringId": "3033288381107978240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2つの銃口が頭に… 3つ目が首元に突き付けられた",
              "maleVariant": "",
              "stringId": "3033289160980086784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうなったの？",
              "maleVariant": "どうなったんだ？",
              "stringId": "3033295375948759044"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうやって助け出したの？",
              "maleVariant": "どうやって助け出したんだ？",
              "stringId": "3033295383431397376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも無事だったんでしょ？ だから今こうしてここにいる",
              "maleVariant": "でも無事だったんだろ？ だから今こうしてここにいる",
              "stringId": "3033295714865299456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうなったの？",
              "maleVariant": "どうなったんだ？",
              "stringId": "3033295728270295040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手を縛られて頭に袋を被せられ… 警棒で文字通り袋叩きにされるなか、唯一できることをした",
              "maleVariant": "",
              "stringId": "3033309668945608704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "必死で嘘をついたの。あれ以来、背筋を伸ばして座れない",
              "maleVariant": "",
              "stringId": "3033310513225449472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ… 代わりにヘルナンデスたちに、綺麗さっぱり消えてもらう必要があった",
              "maleVariant": "",
              "stringId": "3033313486819213312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "潜入作戦はすべて無となり、俺たちは現場に踏み込んだ。“カルテル同士のいざこざ”… というのが表向きの顛末だ",
              "maleVariant": "",
              "stringId": "3033313756445851648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4試合しかしてないけどね",
              "maleVariant": "",
              "stringId": "3033351197823254528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すぐにチームは解散。数年経ってハンセンが現れたってわけ",
              "maleVariant": "",
              "stringId": "3033353395705659392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は移動手段を手配し、ルートの安全を確保しておく",
              "maleVariant": "",
              "stringId": "3033397535889866752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "目標はルイス・ヘルナンデス。武器商人だ。奴の組織に工作員を潜入させる作戦だった",
              "maleVariant": "",
              "stringId": "3033466660754472960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アレックスが奴のエンジニアに成りすまし、すべて順調に進んでいたように思えた",
              "maleVariant": "",
              "stringId": "3033466987524308992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だがある日、アレックスが会議に参加した時のことだ。ルイスが囲ってる派手な女の1人が、奴のドーベルマンを連れて通りかかった。その手の贅沢に金を注ぎ込む男でな",
              "maleVariant": "",
              "stringId": "3033467256412749824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2人で、ね",
              "maleVariant": "",
              "stringId": "3043485779304460288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コスプレするだけが、能じゃないんでね",
              "maleVariant": "",
              "stringId": "3052282951904706560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それに相手の意表をつく形になるから。心配してくれてどうも",
              "maleVariant": "",
              "stringId": "3052283151134146560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "前にもまったく同じ話をしたでしょ？",
              "maleVariant": "",
              "stringId": "3239020710227787776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今回の結果も想像できるってわけ",
              "maleVariant": "",
              "stringId": "3239023502745059328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アレックス…",
              "maleVariant": "",
              "stringId": "3239025243582545920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの時も彼女に自由にやらせたよね",
              "maleVariant": "",
              "stringId": "3239048104300953600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今回は手綱を放り出してるわけじゃない。比べるのは筋違いだ…",
              "maleVariant": "",
              "stringId": "3239048501333770240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってるくせに",
              "maleVariant": "",
              "stringId": "3239048816980312064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふうん… 二人で、ね",
              "maleVariant": "",
              "stringId": "3239061860695326720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アレックスは気に入らないようね",
              "maleVariant": "アレックスは気に入らないようだな",
              "stringId": "3239061871348858880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティの前にコロンビアのメデジンで作戦があった。ソミの初陣よ",
              "maleVariant": "",
              "stringId": "3239061950084333568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時間を無駄にすべきじゃない",
              "maleVariant": "",
              "stringId": "3239062000650862596"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コロンビアの話を聞きたい",
              "maleVariant": "",
              "stringId": "3239062146310651904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっとした意見の相違はあるがな",
              "maleVariant": "",
              "stringId": "3239083130581336064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "任務が気に入らないようね、アレックス",
              "maleVariant": "任務が気に入らないようだな、アレックス",
              "stringId": "3239084490727014404"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "過去の作戦を思い出してね。特に似てるのがひとつある",
              "maleVariant": "",
              "stringId": "3239096059338866688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よせ、アレックス。昔のことを掘り返してる場合じゃない",
              "maleVariant": "",
              "stringId": "3239101555487719424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞かせて",
              "maleVariant": "聞かせてくれ",
              "stringId": "3239106614455877632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ別の機会に",
              "maleVariant": "",
              "stringId": "3239107512204705792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1分ぐらいの余裕はあるんじゃない？ ソミについて役に立つ話を聞けるかもしれないし",
              "maleVariant": "1分ぐらいの余裕はあるんじゃないか？ ソミについて役に立つ話を聞けるかもしれないしな",
              "stringId": "3239108438458023936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは別の時に聞く。今はそれどころじゃないから",
              "maleVariant": "それは別の時に聞く。今はそれどころじゃないからな",
              "stringId": "3239108458506797056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずは情報を共有しよう",
              "maleVariant": "",
              "stringId": "3239111529794289664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミから貰ったチップの情報を解析した",
              "maleVariant": "",
              "stringId": "3239195679578570752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これから行う任務の基盤となるものだ",
              "maleVariant": "",
              "stringId": "3239204982981050368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "監視網の確認から、作戦立案まで、一通り完了している",
              "maleVariant": "",
              "stringId": "3239205533407952896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこでお前の出番だ。レンタカー会社の追跡システムに侵入してくれ。詳細は後でアレックスが話す",
              "maleVariant": "",
              "stringId": "3240538351266349056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "流れはわかったか？ 質問があれば今のうちに言ってくれ",
              "maleVariant": "",
              "stringId": "3240550573401096192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よくわかった。仕事にかかりましょ",
              "maleVariant": "よくわかった。仕事にかかるとしよう",
              "stringId": "3240550832911073280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、どうやってランナーを捕まえるの？",
              "maleVariant": "で、どうやってランナーを捕まえるんだ？",
              "stringId": "3240550835947749376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ待って",
              "maleVariant": "",
              "stringId": "3240561601333608448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バーゲストの下級構成員について知りたい",
              "maleVariant": "",
              "stringId": "3243307593623425024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもそのチューマ、一人でナイトシティで一から軍隊を築いたって言うの？ 信じ難い話ね",
              "maleVariant": "だがそのチューマ、一人でナイトシティで一から軍隊を築いたって言うのか？ 信じ難い話だ",
              "stringId": "3243308217366761472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大部分は地元のゴロツキどもだ。ハンセンの側近たちが武器を与え、最低限の訓練をしている",
              "maleVariant": "",
              "stringId": "3243309004083974144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "普通の装備じゃない",
              "maleVariant": "",
              "stringId": "3243311682633617408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴の得物… 特殊部隊が使う標準装備とは違う",
              "maleVariant": "",
              "stringId": "3243312445812396032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "鋭いな。それはヤクートナイフだ。動物の皮はぎから戦闘まで、あらゆる用途に使用できる。シベリア土産さ。奴の拳銃も同様だ",
              "maleVariant": "",
              "stringId": "3243313753478942720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スタジアムの今の用途は？",
              "maleVariant": "",
              "stringId": "3243319880535003136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スタジアム… 現在の用途は？",
              "maleVariant": "",
              "stringId": "3243320910941278208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "世界中の闇商人が利用するワンストップショップだ",
              "maleVariant": "",
              "stringId": "3243321447258542080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいだろう…",
              "maleVariant": "",
              "stringId": "3259212252842213376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "調子にのったガキどもさ。街で目に留まることをすれば銃を与えられる。後はただ生き残るだけで、防弾ベストやら禁制品やらが手に入る。そういう仕組みだ",
              "maleVariant": "",
              "stringId": "3259290382944616448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんな“荷台から落っこちた試作型の銃”を持ってるわけね。最高",
              "maleVariant": "みんな“荷台から落っこちた試作型の銃”を持ってるわけか。最高だ",
              "stringId": "3259290748134277120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "世界の端でやってる戦争のために戦車を調達したいなら、ここにくればいい",
              "maleVariant": "",
              "stringId": "3259309908352483328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセンに金を渡し、奴が指示を出せば、一瞬で公海のどこかに戦車が届けられるのさ。必要なら軌道から投下もしてくれる",
              "maleVariant": "",
              "stringId": "3259310494246420480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大したもんね… ハンセンはやり手よ",
              "maleVariant": "大したもんだな… ハンセンはやり手だ",
              "stringId": "3282369769965113344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それがいい…",
              "maleVariant": "",
              "stringId": "3286897474563035188"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しょせん、お山の大将を気取ってるだけよ",
              "maleVariant": "",
              "stringId": "3289594179986046976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティは氷山の一角。高需要商品の山を抱え、大規模な販売網を築き、政治家を買収…",
              "maleVariant": "",
              "stringId": "3289597329321148416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大した事業ね",
              "maleVariant": "大した事業だな",
              "stringId": "3289598981960810496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセンはそこらにいる軍隊経験のあるチンピラとは違う",
              "maleVariant": "",
              "stringId": "3289600703202508800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "旅好きなのね",
              "maleVariant": "旅好きなんだな",
              "stringId": "3289663702739107840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "作戦で海外に行っては、気に入ったものを持ち帰っていたらしい。相当なナイフマニアで、戦闘での扱いにも長けている",
              "maleVariant": "",
              "stringId": "3289666240930562048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし。取り掛かるぞ",
              "maleVariant": "",
              "stringId": "3295660302962872324"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その話はまた今度",
              "maleVariant": "その話はまた今度だ",
              "stringId": "3296891517934063616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞いてるのか？",
              "maleVariant": "",
              "stringId": "3297003551568777216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうした、V",
              "maleVariant": "",
              "stringId": "3297004331977756680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここまではいいか？",
              "maleVariant": "",
              "stringId": "3297004544880627720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すべて確認したようだな",
              "maleVariant": "",
              "stringId": "3353579753921925120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、こっちだ",
              "maleVariant": "",
              "stringId": "3353663271372935168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待たせるのか？",
              "maleVariant": "",
              "stringId": "3353663571416666112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、我々の目的だが、ソミをハンセンのもとから助け出す。しかし通常の救出任務のようにはいかない。なぜなら…",
              "maleVariant": "",
              "stringId": "3480913353885532160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミがブラックウォールを越えた何かの影響を受けているからだ。結果的にそんなことになってしまった",
              "maleVariant": "",
              "stringId": "3480920171189813248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺もテックに詳しいわけじゃない。だが重要なのは、治療を受けなければソミは死ぬということ",
              "maleVariant": "",
              "stringId": "3481044354716581888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを取る] アレックスの言う通りよ",
              "maleVariant": "[チップを取る] アレックスの言う通りだ",
              "stringId": "3481047780506648580"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アレックスの言う通りよ。責任があるとすればマイヤーズでしょ",
              "maleVariant": "アレックスの言う通りだ。責任があるとすればマイヤーズだろ",
              "stringId": "3481047835821129728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "安心して、リード。ソミは必ず助ける",
              "maleVariant": "安心しろ、リード。ソミは必ず助ける",
              "stringId": "3481047844142628864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを取る] ソミを救い出しましょう",
              "maleVariant": "[チップを取る] ソミを救い出してやろう",
              "stringId": "3481055170266316800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ようやく… 過ちを正す時が来た",
              "maleVariant": "",
              "stringId": "3481071947515871232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい加減にしなよ、リード。こうなったのは別にあんたのせいじゃない",
              "maleVariant": "",
              "stringId": "3481072531715309568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの子は大人よ。自らが招いた結果ってだけ",
              "maleVariant": "",
              "stringId": "3481072701836279808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、ソミは操られたんだ。頼れる味方もいない中な",
              "maleVariant": "",
              "stringId": "3481072886251438080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、それを。作戦の詳細が入っている",
              "maleVariant": "",
              "stringId": "3481073055734874112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、だが俺のせいでもある",
              "maleVariant": "",
              "stringId": "3481073905702191104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。そっちが片付いたら… お前もな",
              "maleVariant": "",
              "stringId": "3481074352546562048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "治療の要となるのは、神経マトリックス内に“生息”するAIよ。それをハンセンから奪う",
              "maleVariant": "",
              "stringId": "3481075701149515776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "作戦は？",
              "maleVariant": "",
              "stringId": "3481078872496324612"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "結局、神経マトリックスって何なの？",
              "maleVariant": "結局、神経マトリックスって何なんだ？",
              "stringId": "3481078905111232512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。作戦を聞かせて",
              "maleVariant": "わかった。作戦を聞かせろ",
              "stringId": "3481078916502962176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "神経マトリックスについて知りたい",
              "maleVariant": "",
              "stringId": "3481079191112433664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実験的テック。ドッグタウンの地下深くで見つかったメインフレームに組み込まれてた",
              "maleVariant": "",
              "stringId": "3481090096084729856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正確にはバンカーにあったの。アラサカのソウルキラーに対抗するための旧合衆国とミリテクの共同研究、サイノシュア・プロジェクトの施設よ",
              "maleVariant": "",
              "stringId": "3481092195015114752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マトリックス自体は入れ物に過ぎない。中には壁の向こう側のAIが収納されていて、それを“展開”する機能が備わってる。あんたとソングバードの治療のためとかにね",
              "maleVariant": "",
              "stringId": "3481093418795581440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセンは例のネットランナーたちと会う。その会合の場が、ソミとマトリックスに接近するチャンスだ",
              "maleVariant": "",
              "stringId": "3481106761849683968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ランナーたちはフランス人の双子。以前ミリテクで働いていたため、セキュリティプロトコルを熟知している",
              "maleVariant": "",
              "stringId": "3481107512378441728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2人はアクセスコードを売るつもりなのよ。ソングバードは、それを使ってメインフレームからマトリックスを取り出す",
              "maleVariant": "",
              "stringId": "3481108338522112000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スキャンと行動様態は入手済み。あとは双子たちに退場してもらうだけ",
              "maleVariant": "",
              "stringId": "3481111485609066496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまり、誘拐ね",
              "maleVariant": "つまり、誘拐だな",
              "stringId": "3481111845094473732"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その双子について知りたい",
              "maleVariant": "",
              "stringId": "3481112010534600704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アイメリクとオロールだけど… ホテルで得た情報以外に何かある？",
              "maleVariant": "アイメリクとオロールだが… ホテルで得た情報以外に何かあるか？",
              "stringId": "3481113101322076160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。ソングバードが情報をまとめてくれた",
              "maleVariant": "",
              "stringId": "3481117451335417856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手短に言うと… 2人は当初ミリテクで働いていたんだけど、ある時、裏稼業のほうが金になると悟った…",
              "maleVariant": "",
              "stringId": "3481117884070150144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど。確かにエディーが好きそうだった",
              "maleVariant": "",
              "stringId": "3481118348815810560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。でも次の報酬は受け取れない。私たちにさらわれることになるからね",
              "maleVariant": "",
              "stringId": "3481118606178304000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誘拐するのね。具体的には？",
              "maleVariant": "誘拐するんだな。具体的には？",
              "stringId": "3481119280504946688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その車を追跡してジャックするんだ。スタジアムへ着く前にな",
              "maleVariant": "",
              "stringId": "3481123833556156416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確保したら2人に成り代わってもらう。行動インプリントは調整済みだ",
              "maleVariant": "",
              "stringId": "3481171304621719552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最終ステージはハンセンとご対面？",
              "maleVariant": "最終ステージはハンセンとご対面か？",
              "stringId": "3481172118266363908"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの役割は？",
              "maleVariant": "お前の役割は？",
              "stringId": "3481172444784541696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前たちの脱出ルートを確保する。目的を果たしたら急いで逃げろ",
              "maleVariant": "",
              "stringId": "3481183100279635968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そして最終ステージでは、ハンセンとご対面？",
              "maleVariant": "",
              "stringId": "3481187678899322880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。お前は奴にメインフレームへのアクセスコードを売る。そのコードがお前たちの治療法への鍵にもなる",
              "maleVariant": "",
              "stringId": "3481188001290305536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どちらかがラボでソミと合流し、コードの入力を手伝う。もう1人はハンセンに残れと言われるだろう",
              "maleVariant": "",
              "stringId": "3481188754922848256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私が残る。あんたはラボ担当よ",
              "maleVariant": "",
              "stringId": "3481189565010722816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "データへのアクセスが可能になったら合図して。私がハンセンと側近たちを排除する",
              "maleVariant": "",
              "stringId": "3481191722225156096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセンを殺すのね",
              "maleVariant": "ハンセンを殺すんだな",
              "stringId": "3481192007202947072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセンと手下たちを、1人で相手する気…？",
              "maleVariant": "ハンセンと手下たちを、1人で相手する気か…？",
              "stringId": "3481193625197658112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセンを殺すのは確定事項ね…",
              "maleVariant": "ハンセンを殺すのは確定事項か…",
              "stringId": "3481196182078930944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズ直々の命令だ。新合衆国大統領の命を脅かす行動を取った以上、当然だろう",
              "maleVariant": "",
              "stringId": "3481196632127750144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソングバードは…？",
              "maleVariant": "",
              "stringId": "3481197044360724480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "救出する。大統領からも言われただろう",
              "maleVariant": "",
              "stringId": "3481198036061962240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "問題ない",
              "maleVariant": "",
              "stringId": "3481199616341491716"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずは何をするんだっけ？",
              "maleVariant": "",
              "stringId": "3481199823456223232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最終目的についてリマインドして",
              "maleVariant": "最終目的についてリマインドしてくれ",
              "stringId": "3481199923062554624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズはこの作戦をどれだけ把握してるの？",
              "maleVariant": "マイヤーズはこの作戦をどれだけ把握してるんだ？",
              "stringId": "3481200124624027648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最初は何をするんだっけ？",
              "maleVariant": "",
              "stringId": "3481201265256611840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最終目的を確認したい",
              "maleVariant": "",
              "stringId": "3481201278493835264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズは作戦をどれだけ把握してるの？",
              "maleVariant": "マイヤーズは作戦をどれだけ把握してるんだ？",
              "stringId": "3481201310420877312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ネットランナーの車を追跡する。これはお前の担当だ。詳細はアレックスが",
              "maleVariant": "",
              "stringId": "3481223085468860416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その間にこちらで人物データとインプリントを仕上げておく",
              "maleVariant": "",
              "stringId": "3481223485118922752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセンに囚われたソミの救出だ。同時にお前たちの命を救うテックも奪う",
              "maleVariant": "",
              "stringId": "3481225624817942528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前とアレックスはハンセンが雇ったネットランナーに成りすまして潜入することになる",
              "maleVariant": "",
              "stringId": "3481226676598390784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スペースフォース・ワンを撃墜した男にソミが捕まっている、と伝えてある",
              "maleVariant": "",
              "stringId": "3481227844863049728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それだけ？",
              "maleVariant": "",
              "stringId": "3481228471743725568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はな",
              "maleVariant": "",
              "stringId": "3481229082350501888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1人で大丈夫？",
              "maleVariant": "1人で大丈夫か？",
              "stringId": "3481243971928375296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセンは一人で軍隊を築いたの？",
              "maleVariant": "ハンセンは一人で軍隊を築いたのか？",
              "stringId": "3515714787713503236"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スタジアム、ね",
              "maleVariant": "スタジアム、か",
              "stringId": "3515743596307304448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スタジアムね",
              "maleVariant": "スタジアムか",
              "stringId": "3515752895314153476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、来たか",
              "maleVariant": "",
              "stringId": "3556454590481518592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バーゲストの中枢は元ミリテク兵なのね",
              "maleVariant": "バーゲストの中枢は元ミリテク兵なんだな",
              "stringId": "3589504037386473472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
