  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[開く]",
              "maleVariant": "",
              "stringId": "3231661292709961728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[起動する]",
              "maleVariant": "",
              "stringId": "3231661292793847808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、みんな！ どうしてわかったの？",
              "maleVariant": "",
              "stringId": "3231661293683040300"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウィリーのパーティーで星占いをしたでしょ。あれでわかったのよ",
              "maleVariant": "",
              "stringId": "3231661293699817472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう！ 完全に騙された！",
              "maleVariant": "",
              "stringId": "3231661293699817488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミはやぎ座でしょ？ さあ、願いごとをして",
              "maleVariant": "",
              "stringId": "3231661293699817504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの星占いより、叶いますように…",
              "maleVariant": "",
              "stringId": "3231661293699817520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なあ、カネもらって潜ったのは初めてだろ？ …すげえ。一発でこんなに稼げるとはな。どんな気分だ？",
              "maleVariant": "",
              "stringId": "3231661305175433252"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんなとこ出てくか？",
              "maleVariant": "",
              "stringId": "3231661305175433268"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさか…",
              "maleVariant": "",
              "stringId": "3231661305192210444"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんだけあればどこにだって行けるって。ここは退屈、みんな夢がないからつまらないとずっと言ってたろ",
              "maleVariant": "",
              "stringId": "3231661305192210460"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "冗談でしょ。私はどこへも行かない",
              "maleVariant": "",
              "stringId": "3231661331800875052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "家や友達を捨ててあんたと行けって？ 大事なものを全部捨てて？",
              "maleVariant": "",
              "stringId": "3231661331817652232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その先にあるもののほうがよほど重要だ…",
              "maleVariant": "",
              "stringId": "3231661331817652248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、そう。お断りよ！",
              "maleVariant": "",
              "stringId": "3231661331817652264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よく考えろ。ネットウォッチがここに来たら、お前を信じ、大事にしてくれた人たちはどうなる？",
              "maleVariant": "",
              "stringId": "3231661331817652280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "機材を買ったのは誰か。カフェのサブネットを使わせたのは誰か… すべて明るみに出る",
              "maleVariant": "",
              "stringId": "3231661331817652296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいか… お前自身や友人を守りたければ他に方法はない。彼らが傷つくのは嫌だろう？",
              "maleVariant": "",
              "stringId": "3231661331834429440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここはどこ？ 助けてくれたの？",
              "maleVariant": "ここはどこだ？ 助けてくれたのか？",
              "stringId": "3231661346212503572"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また〈Relic〉をハックしたのね？",
              "maleVariant": "また〈Relic〉をハックしたんだな？",
              "stringId": "3231661346229280772"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "普段は… よく窓を開けて、そこに腰かけて…",
              "maleVariant": "",
              "stringId": "3231661346413830180"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただ… 通りの音に耳を澄ましてた。街の、営みの音。わかるでしょ… 心が落ち着くの",
              "maleVariant": "",
              "stringId": "3231661346413830196"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はもう… 何も、聞こえないし、感じない。何もよ。あなたは？",
              "maleVariant": "",
              "stringId": "3231661346413830212"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの過去を見た。リードと、マイヤーズもいたよ… 覚えてないの？",
              "maleVariant": "お前の過去を見た。リードと、マイヤーズもいたぞ… 覚えてないのか？",
              "stringId": "3231661351161782288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いえ… ブラックウォールが… 私を壊すから",
              "maleVariant": "",
              "stringId": "3231661351161782304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無が広がっていく… 記憶が次々と… 失われていく… 私には何も残らない",
              "maleVariant": "",
              "stringId": "3231661351161782320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分のしたことや、払った代償は理解してる…",
              "maleVariant": "",
              "stringId": "3231661357453238340"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ？ ソミ！",
              "maleVariant": "",
              "stringId": "3231661357470015488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やったこと… 人を傷つけたこと… 全部無意味だった。まったく、何の意味もなかったの…",
              "maleVariant": "",
              "stringId": "3231661374364672012"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私は… 負けた、そうでしょ？",
              "maleVariant": "",
              "stringId": "3231661374381449224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死ぬ時に記憶に慰められるって、本当なのね…？",
              "maleVariant": "",
              "stringId": "3231661377451679748"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死なせはしないよ、ソミ。ここを出るの… 一緒にね",
              "maleVariant": "死なせはしないさ、ソミ。ここを出るんだ… 一緒にな",
              "stringId": "3231661385068535816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう",
              "maleVariant": "",
              "stringId": "3231661385068535848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダメよ。待って。ほかにも…！",
              "maleVariant": "ダメだ。待て。ほかにも…！",
              "stringId": "3231661385068535864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ごめん。力になりたいの",
              "maleVariant": "すまない。力になりたいんだ",
              "stringId": "3231661388876963860"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはみんなを騙した。あたしのことも",
              "maleVariant": "お前はみんなを騙した。俺のことも",
              "stringId": "3231661388876963880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブルックリンの私の部屋… 前に話したの覚えてる？",
              "maleVariant": "",
              "stringId": "3231661397282349076"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また〈Relic〉をハックしたのね。これは現実じゃない",
              "maleVariant": "また〈Relic〉をハックしたんだな。これは現実じゃない",
              "stringId": "3231661416056053764"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ… あたしは… あの時は正しい判断をしたつもりだった…",
              "maleVariant": "ソミ… 俺は… あの時は正しい判断をしたつもりだった…",
              "stringId": "3231661417767329800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正しい？ 誰にとって？ あなた？ リード？",
              "maleVariant": "",
              "stringId": "3231661417767329816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全員にとってよ",
              "maleVariant": "全員にとってだ",
              "stringId": "3231661417767329832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウィンウィンは存在しない。まして3者の間では",
              "maleVariant": "",
              "stringId": "3231661417784107012"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時に… 正しく、勇敢な選択は… あなたにとって、利己的な選択になりうる",
              "maleVariant": "",
              "stringId": "3231661417784107028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ごめんなさい、ソミ。ほんとよ。どうしたら… 力になれる？",
              "maleVariant": "すまない、ソミ。ほんとだ。どうしたら… 力になれる？",
              "stringId": "3231661417784107044"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ望みはある",
              "maleVariant": "",
              "stringId": "3231661436088049664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直、あたしにはわからない",
              "maleVariant": "正直、俺にはわからない",
              "stringId": "3231661436104826900"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "負けを認めて",
              "maleVariant": "負けを認めろ",
              "stringId": "3231661436104826920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ、あたしも必死に生きようとしてる。誰だって道を誤るし、迷うことだってある",
              "maleVariant": "ソミ、俺も必死に生きようとしてる。誰だって道を誤るし、迷うことだってある",
              "stringId": "3231661436171935772"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "砕け散った命のかけらを拾い集めて、つなぎ合わせるの。意味を求めてね",
              "maleVariant": "",
              "stringId": "3231661436188712960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ避けられる。最悪の、結末だけは",
              "maleVariant": "",
              "stringId": "3231661436188712976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直、あたしにはわからない… リードが間違ってるのか… それとも、あんたがやり過ぎたのか…",
              "maleVariant": "正直、俺にはわからない… リードが間違ってるのか… それとも、お前がやり過ぎたのか…",
              "stringId": "3231661439997141048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リードはずっと間違ってばかりよ。今なら、はっきりわかる",
              "maleVariant": "",
              "stringId": "3231661439997141064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう、あんたの負けよ、ソミ。何をどこでどう間違ったのかはわからないけど…",
              "maleVariant": "ああ、お前の負けだ、ソミ。何をどこでどう間違ったのかはわからないが…",
              "stringId": "3231661442312396884"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私にも… わからない…",
              "maleVariant": "",
              "stringId": "3231661442329174020"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは嘘をつき、身近な人たちを裏切った",
              "maleVariant": "お前は嘘をつき、身近な人たちを裏切った",
              "stringId": "3231661442329174036"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "身近な…？",
              "maleVariant": "",
              "stringId": "3231661442329174052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真実と向き合う時よ、ソミ",
              "maleVariant": "真実と向き合う時だ、ソミ",
              "stringId": "3231661442329174068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私… 前に進み続けることが、安心に繋がると思ってた",
              "maleVariant": "",
              "stringId": "3231661451976073272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも過去を見るべきだったのよ。何度も味わった、何かを失う経験を…",
              "maleVariant": "",
              "stringId": "3231661451976073288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V… ご、ごめんなさい… 私、その…",
              "maleVariant": "",
              "stringId": "3231661465112633424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ？",
              "maleVariant": "",
              "stringId": "3231905930825916420"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[抱きしめる] あたしは味方よ",
              "maleVariant": "[抱きしめる] 俺は味方だ",
              "stringId": "3233460312390459396"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[揺さぶる] しっかりして",
              "maleVariant": "[揺さぶる] しっかりしろ",
              "stringId": "3233460989804109824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "周りを見て、ソミ！ わかる？ これを止めて！",
              "maleVariant": "周りを見ろ、ソミ！ わかるか？ これを止めてくれ！",
              "stringId": "3233465645179559976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいえ… もう手遅れよ",
              "maleVariant": "",
              "stringId": "3233465645196337156"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これが私に残った最後の記憶… 昔は帰る場所があって… 大事な… 人もいた…",
              "maleVariant": "",
              "stringId": "3233465645196337172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お願いよ。目を覚まして！ でないと…！",
              "maleVariant": "お願いだ。目を覚ましてくれ！ でないと…！",
              "stringId": "3233465645196337188"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一体何が？",
              "maleVariant": "",
              "stringId": "3233501612342992896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ？ ここはどこ…？",
              "maleVariant": "ソミ？ ここはどこだ…？",
              "stringId": "3241597999692050432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ？ あんたなの？",
              "maleVariant": "ソミ？ お前なのか？",
              "stringId": "3241600467335311360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ… ついさっきラボで、確かにあの機械が… あたしはてっきり… あんたが助けてくれたの？",
              "maleVariant": "ソミ… ついさっきラボで、確かにあの機械が… 俺はてっきり… お前が助けてくれたのか？",
              "stringId": "3241611276945911808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここには、時空を超えてあなたを助けた私と、あなたと話してる私が同時に存在している",
              "maleVariant": "",
              "stringId": "3241611943890579456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここはどこなの？",
              "maleVariant": "ここはどこなんだ？",
              "stringId": "3241612628233220096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "好きな現実を選んで、V。時間ならある。この場所には、時間があるの",
              "maleVariant": "",
              "stringId": "3241613706421006336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここはまだバンカーの中よ",
              "maleVariant": "ここはまだバンカーの中だ",
              "stringId": "3241614959846813700"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここは危険よ",
              "maleVariant": "ここは危険だ",
              "stringId": "3241615189258465280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ… ここはまだバンカーの中よ。これは… 現実じゃない",
              "maleVariant": "ソミ… ここはまだバンカーの中だ。これは… 現実じゃない",
              "stringId": "3241616127306166272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここは危険よ。この場所がどこであれ、急いで脱出しないと",
              "maleVariant": "ここは危険だ。この場所がどこであれ、急いで脱出しないと",
              "stringId": "3241616235552763904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私はたくさん… ひどい間違いを犯した。でもここでは… そんなことはすべて、関係なかった…",
              "maleVariant": "",
              "stringId": "3241616423189147648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここが私にとっての最後の希望だったのに… それを奪うのね… どうして？",
              "maleVariant": "",
              "stringId": "3241620388987465728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは嘘をついた。みんなを裏切ったのよ",
              "maleVariant": "お前は嘘をついた。みんなを裏切ったんだ",
              "stringId": "3241623609457963008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見たでしょ… 彼らのせいで私がどうなり… 何を失ったか…",
              "maleVariant": "",
              "stringId": "3241626856050585600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕方なかったの。彼らに加わりたくはなかった… 私は…",
              "maleVariant": "",
              "stringId": "3241627600740872192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私を脅そうっていうの？ フン、見くびらないで",
              "maleVariant": "",
              "stringId": "3241640670460571648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何が正解かは理解してるはず。あとは本人が受け入れるだけだ",
              "maleVariant": "",
              "stringId": "3241641515914817536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブラックウォールよ。時間はない…",
              "maleVariant": "",
              "stringId": "3241669519772676096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ、どうすれば…？",
              "maleVariant": "",
              "stringId": "3241670254128197632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コアにいる。私を探して… 助け出して。もう一度だけ",
              "maleVariant": "",
              "stringId": "3241670437754826752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この瞬間だけは覚えておく…  これだけは… この部屋、大切な友達のこと",
              "maleVariant": "",
              "stringId": "3241678807320686592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうなってるの！？",
              "maleVariant": "どうなってるんだ！？",
              "stringId": "3241686202935603200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたも私も、崖っぷち",
              "maleVariant": "",
              "stringId": "3241692874697535488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すべてが闇に包まれる前の… 最後の一瞬に、一人じゃなかったと思える",
              "maleVariant": "",
              "stringId": "3241708644458491904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうせ映像でしょ、ほかのもすべてそう",
              "maleVariant": "どうせ映像だろ、ほかのもすべてそうだ",
              "stringId": "3253574311952445444"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何も聞こえない",
              "maleVariant": "",
              "stringId": "3253574563946229760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうだろう。なんというか、ビジョンのような幻影を見た",
              "maleVariant": "どうだろうな。なんというか、ビジョンのような幻影を見た",
              "stringId": "3253575728989663232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ、これは全部… 映像か何かなんでしょ。何もかもが",
              "maleVariant": "ソミ、これは全部… 映像か何かなんだろ。何もかもが",
              "stringId": "3253575835910860800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何もかも？",
              "maleVariant": "",
              "stringId": "3253577882747990016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "幻影…？",
              "maleVariant": "",
              "stringId": "3253581852354736128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "現実って何？ あなたを救ってあげる私？ ここで話してる私？ 死にかけてる私？",
              "maleVariant": "",
              "stringId": "3253595189318709248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この場所って…？",
              "maleVariant": "",
              "stringId": "3253598098286637056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここでは安心を感じたのね",
              "maleVariant": "ここでは安心を感じたんだな",
              "stringId": "3253602580839985152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、覚えてる。あんたが唯一安心できた、“本当の居場所”。でも… ソミ、これは現実じゃない",
              "maleVariant": "ああ、覚えてる。お前が唯一安心できた、“本当の居場所”。だが… ソミ、これは現実じゃない",
              "stringId": "3253603378462388224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いったい何？ この場所は？",
              "maleVariant": "いったい何だ？ この場所は？",
              "stringId": "3256079013634043904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもあなたにはわからないでしょ… 私がどれほど悔やみ… 苦しんでるか…",
              "maleVariant": "",
              "stringId": "3256095676781752320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気になる？",
              "maleVariant": "",
              "stringId": "3256100848794791936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまらないのはあなただけよ。なんてね。まだどこにも行かないわ",
              "maleVariant": "",
              "stringId": "3256110473145413632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "たぶん… わかってる… わかってるの…",
              "maleVariant": "",
              "stringId": "3256145436813225984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しっかりして、ソミ！",
              "maleVariant": "しっかりしろ、ソミ！",
              "stringId": "3256148310800658432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カネは何に使うんだ？",
              "maleVariant": "",
              "stringId": "3292394208568209408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね",
              "maleVariant": "",
              "stringId": "3292424045370470400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰だって間違いは犯す",
              "maleVariant": "",
              "stringId": "3292443163461087236"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私は間違いをたくさん犯した。数えきれないほど…",
              "maleVariant": "",
              "stringId": "3292443517108023296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その間違いが生んだ結果と向き合うのよ",
              "maleVariant": "その間違いが生んだ結果と向き合うんだ",
              "stringId": "3292445822196838400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰だって間違いは犯す。あたしだってそう… 数えきれないくらいよ",
              "maleVariant": "誰だって間違いは犯す。俺だってそう… 数えきれないくらいだ",
              "stringId": "3292453697556246528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "状況はよくない。それは確かよ。でも… 諦めないで",
              "maleVariant": "状況はよくない。それは確かだ。でも… 諦めるなよ",
              "stringId": "3292453797716226048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なに、それでもう諦めて、戦うのをやめるっていうの？",
              "maleVariant": "なんだ、それでもう諦めて、戦うのをやめるっていうのか？",
              "stringId": "3292456198569390080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "過去は変えられないのよ、ソミ。背を向けて、置いて行くべき記憶もある",
              "maleVariant": "過去は変えられないんだ、ソミ。背を向けて、置いて行くべき記憶もある",
              "stringId": "3292457540897665024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やり直したかったんでしょ。今がそのチャンスよ。落ち込むのも結構だけど、大事なのは今ここで何をするかよ",
              "maleVariant": "やり直したかったんだろ。今がそのチャンスだ。落ち込むのも結構、だが大事なのは今ここで何をするかだろ",
              "stringId": "3292462341731577856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "取り返しのつかないものもある…",
              "maleVariant": "",
              "stringId": "3292473555438202880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "諦めないって… どうすれば… 見てよ… 気がつけばこのザマ…",
              "maleVariant": "",
              "stringId": "3292476006102921216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの神経リンクに侵入し、〈Relic〉のリソースと演算力を利用したようね",
              "maleVariant": "俺の神経リンクに侵入し、〈Relic〉のリソースと演算力を利用したようだな",
              "stringId": "3297299731390009344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたにとってはね。私にとっては… あなたを救う私と、あなたと話す私、死にかけの私が同時に存在してる",
              "maleVariant": "",
              "stringId": "3297300630833336320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここはサイバー空間ね",
              "maleVariant": "ここはサイバー空間だな",
              "stringId": "3297302384169848832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたのデジタル精神世界へ連れてきたんでしょ？ この場所は、安全なの？",
              "maleVariant": "お前のデジタル精神世界へ連れてきたってわけだろ？ この場所は、安全なのか？",
              "stringId": "3344602899346026496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもこれは精神世界じゃない",
              "maleVariant": "",
              "stringId": "3344610284055752704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ何なの？",
              "maleVariant": "じゃあ何なんだ？",
              "stringId": "3344611045053493248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、答えてよ、お願い",
              "maleVariant": "",
              "stringId": "3359099863455981568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜなの？",
              "maleVariant": "",
              "stringId": "3359100536406892548"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソーッ！",
              "maleVariant": "",
              "stringId": "3359233768624447488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、うるせえぞ！ こっちは寝ようとしてるんだ！",
              "maleVariant": "",
              "stringId": "3359246912398778368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私の邪魔をしないで！ 人のことに口出ししないでよ！ ",
              "maleVariant": "",
              "stringId": "3360479616480505856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、ソミ！ こっちは3日も連絡してたんだぞ！",
              "maleVariant": "",
              "stringId": "3489604124159438848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心配しないで。順調だから",
              "maleVariant": "",
              "stringId": "3489612874719096832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ？ どこがだよ？ 先週だって椅子に座ったまま気ぃ失ってたろ！",
              "maleVariant": "",
              "stringId": "3489613279653982208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰も頼んでないけどね！",
              "maleVariant": "",
              "stringId": "3489614030434398208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、本気で言ってんのか？",
              "maleVariant": "",
              "stringId": "3489614682933882880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞いて。新しい仕事が入ったの。デカい仕事よ。上手くいけば… 歴史に名が残る",
              "maleVariant": "",
              "stringId": "3489617520162435072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最近はすぐそれだ。俺たちはどうなるんだよ？",
              "maleVariant": "",
              "stringId": "3489620420003557376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "開いてるよ！",
              "maleVariant": "",
              "stringId": "3489705750635409408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またICEか… でもこうしたら…？",
              "maleVariant": "",
              "stringId": "3538765422478422016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ…",
              "maleVariant": "",
              "stringId": "3538766297276018688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あともうちょっとだったのに…",
              "maleVariant": "",
              "stringId": "3538767157561323520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "熱暴走で倒れないよう、付きっ切りだったんだぞ！",
              "maleVariant": "",
              "stringId": "3540445625663549440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから今は集中させて。考えるための時間と場所がいる",
              "maleVariant": "",
              "stringId": "3540449240516063232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、もういい。好きにしろよ",
              "maleVariant": "",
              "stringId": "3540452067426627584"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}