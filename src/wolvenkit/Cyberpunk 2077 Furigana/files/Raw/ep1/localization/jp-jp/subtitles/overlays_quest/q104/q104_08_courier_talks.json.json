      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさかコーポ野郎の世話になるとはな",
              "maleVariant": "",
              "stringId": "1460312072358776832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウソだろ",
              "maleVariant": "",
              "stringId": "1642055975806193696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほう… 驚いたな。これはまるで…",
              "maleVariant": "",
              "stringId": "1642058267523567616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すごいでしょ？",
              "maleVariant": "すごいだろ？",
              "stringId": "1642061177011589124"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしたの？",
              "maleVariant": "どうしたんだ？",
              "stringId": "1642061649541877760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なかなかすごいでしょ？",
              "maleVariant": "なかなかすごいだろ？",
              "stringId": "1642062121854062592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何？ どうしたの？",
              "maleVariant": "なんだ？ どうしたんだ？",
              "stringId": "1642062192637136896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんだ？ どこだここは？",
              "maleVariant": "",
              "stringId": "1683551801916592128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チップについて話がある",
              "maleVariant": "",
              "stringId": "1683607082339995648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ド田舎のモーテル",
              "maleVariant": "",
              "stringId": "1683613099740172288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ド田舎のモーテルよ",
              "maleVariant": "ド田舎のモーテルだ",
              "stringId": "1683615130823495680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "説明になっていない",
              "maleVariant": "",
              "stringId": "1683617115836583936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうでもいいでしょ。あんたが一人でここを出ることはない",
              "maleVariant": "どうでもいいだろ。あんたが一人でここを出ることはない",
              "stringId": "1683620963204534272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたには何が何でも協力してもらう",
              "maleVariant": "",
              "stringId": "1683768331451305988"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "取引する気はない",
              "maleVariant": "",
              "stringId": "1689393372272594944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰の刺客でもない。ただし、あんたには協力してもらう",
              "maleVariant": "",
              "stringId": "1689394056816562176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "勘違いしてるみたいね。取引をする気はない",
              "maleVariant": "勘違いしてるようだな。取引をする気はない",
              "stringId": "1689395463519014912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "では何の用だ？",
              "maleVariant": "",
              "stringId": "1689410043691470848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きちんと分析できないのが非常に悔やまれる",
              "maleVariant": "",
              "stringId": "1689685299370987520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残念だったな、V。モルモットになり損ねたようだぞ",
              "maleVariant": "",
              "stringId": "1689688758145060864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、取り除き方は？",
              "maleVariant": "",
              "stringId": "1689702666624897024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残念だが… 君の神経回路は完全に損傷している。もはやこのチップなしでは機能できない",
              "maleVariant": "",
              "stringId": "1689703210559016960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私にできることがあったとしても…",
              "maleVariant": "",
              "stringId": "1689705621277184000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "出たな！ 「金が必要」か？",
              "maleVariant": "",
              "stringId": "1689706066812932096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…スウェーデンの診療所を紹介することぐらいだ。苦痛を緩和して、楽な最期を迎えられるようにしてくれる",
              "maleVariant": "",
              "stringId": "1689706442102476800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ヘルマンにチップを見せる]",
              "maleVariant": "",
              "stringId": "1700893544206897156"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信用できない",
              "maleVariant": "",
              "stringId": "1700894472725139456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": "1700896721660612608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ会ったばかりでしょ。信用できない相手には見せられない",
              "maleVariant": "まだ会ったばかりだろ。信用できない相手には見せられない",
              "stringId": "1700903883350806528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なら、なぜ君は私をさらったんだ？",
              "maleVariant": "",
              "stringId": "1700906791463096320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チップを見ないことには、協力のしようがない",
              "maleVariant": "",
              "stringId": "1700918965380124672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何が目的だ？",
              "maleVariant": "",
              "stringId": "1730272597896400896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの発明品――アラサカのために作った生体チップについて話がしたい",
              "maleVariant": "",
              "stringId": "1735733740165206016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待て… その前にひとつ確認させてくれ",
              "maleVariant": "",
              "stringId": "1740103786777874432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "君はヨリノブの刺客じゃないんだな？",
              "maleVariant": "",
              "stringId": "1740118559754096640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ということは、私と取引したいのか",
              "maleVariant": "",
              "stringId": "1740129553259761692"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "君のボスがカン・タオよりも金を出すなら、話をしよう",
              "maleVariant": "",
              "stringId": "1740151950708903936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたのチップのせいであたしは死にかけてる。とんだ不良品ね",
              "maleVariant": "あんたのチップのせいで俺は死にかけてる。とんだ不良品だな",
              "stringId": "1740163245684363264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "苦情の申し立てなら、もう少し詳細を聞かせてくれ",
              "maleVariant": "",
              "stringId": "1740168020295487488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭の中に、シルヴァーハンドのコンストラクトが入ってるのよ。それを取り除きたい",
              "maleVariant": "頭の中に、シルヴァーハンドのコンストラクトが入ってるんだ。それを取り除きたい",
              "stringId": "1740171092488949760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたのチップのせいで死にかけてるの",
              "maleVariant": "あんたのチップのせいで死にかけてる",
              "stringId": "1740172869028352004"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "協力？ どういうことだ？",
              "maleVariant": "",
              "stringId": "1740173612175134720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの頭にシルヴァーハンドのコンストラクトが入ってる",
              "maleVariant": "俺の頭にシルヴァーハンドのコンストラクトが入ってる",
              "stringId": "1740174131731959808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭の中にシルヴァーハンドのコンストラクトが入ってる。このままじゃあたしは死ぬしかない。チップを取り除かない限りね",
              "maleVariant": "頭の中にシルヴァーハンドのコンストラクトが入ってる。このままじゃ俺は死ぬしかない。チップを取り除かない限りな",
              "stringId": "1740175208711786496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブ・アラサカから盗んだ。頼まれてやったんだけど、クライアントが問題ありでね",
              "maleVariant": "ヨリノブ・アラサカから盗んだ。頼まれてやったんだが、クライアントが問題ありでな",
              "stringId": "1740177672647282688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんなことになるなんて、注意書きに書いてなかったんだけど",
              "maleVariant": "こんなことになるなんて、注意書きに書いてなかったんだが",
              "stringId": "1740179051449225216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チップを取り除いて",
              "maleVariant": "チップを取り除いてくれ",
              "stringId": "1740187473477439492"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シルヴァーハンド？ コンストラクト…？ そんなバカな！",
              "maleVariant": "",
              "stringId": "1740206501289857024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブ・アラサカから盗んだ",
              "maleVariant": "",
              "stringId": "1740232258124845056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "長い話よ",
              "maleVariant": "長い話だ",
              "stringId": "1740245364586647552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいから、早くチップを取り除いて",
              "maleVariant": "いいから、早くチップを取り除いてくれ",
              "stringId": "1740246115014742016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "長い話よ",
              "maleVariant": "長い話だ",
              "stringId": "1740246444435378176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカに見つかれば、私も君も殺される。もったいぶっている場合じゃないと思うがね",
              "maleVariant": "",
              "stringId": "1740247144884146176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう単純な話じゃない",
              "maleVariant": "",
              "stringId": "1740249017926094848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたが作ったチップでしょ？ 何が問題なの？",
              "maleVariant": "あんたが作ったチップだろ？ 何が問題なんだ？",
              "stringId": "1740249280338530304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さっきの話が本当なら、君が差したのは新型のプロトタイプだ",
              "maleVariant": "",
              "stringId": "1740251679245537280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの頭に何が起きてるの？",
              "maleVariant": "俺の頭に何が起きてるんだ？",
              "stringId": "1740454646414381060"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "プロジェクトはまだテスト段階なんでしょ？",
              "maleVariant": "プロジェクトはまだテスト段階なんだろ？",
              "stringId": "1740467631644020736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って。プロジェクトはまだテスト段階なんでしょ？",
              "maleVariant": "待て。プロジェクトはまだテスト段階なんだろ？",
              "stringId": "1740469365284081664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この先どうなるかなんてわからないはず",
              "maleVariant": "この先どうなるかなんてわからないはずだ",
              "stringId": "1740471930637848576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの頭に何が起きてるの？",
              "maleVariant": "俺の頭に何が起きてるんだ？",
              "stringId": "1740482451361566720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちが聞きたい。2つの人格を持つのはどんな感じなんだ？",
              "maleVariant": "",
              "stringId": "1740493006579240960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "先ほど、脳がコンストラクトに食われている様を見た",
              "maleVariant": "",
              "stringId": "1740494585248157696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何があっても宿主を乗っ取るよう、プログラムされているんだ",
              "maleVariant": "",
              "stringId": "1740496364018929664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "君の哀れな脳みそに、なす術はない",
              "maleVariant": "",
              "stringId": "1740497718829453312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何をしたって、いずれは記憶痕跡に支配されるってこと？",
              "maleVariant": "何をしても、いずれは記憶痕跡に支配されるってことか？",
              "stringId": "1740499338015346688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。はた迷惑な奴だな。肉体のあった頃と何も変わってない",
              "maleVariant": "",
              "stringId": "1740499665909256192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "幻聴とはわけが違う。君は自分であると同時に、シルヴァーハンドでもあるんだ",
              "maleVariant": "",
              "stringId": "1740501499575078912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "哲学を語りに来たんじゃない",
              "maleVariant": "",
              "stringId": "1740502637825298436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "姿が見え、話しかけることもできる",
              "maleVariant": "",
              "stringId": "1740504463521943552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "哲学の授業を受けにきた覚えはない",
              "maleVariant": "",
              "stringId": "1741419733283311616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "姿が見えるし、話すこともできる",
              "maleVariant": "",
              "stringId": "1741420140197908480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "哲学ではない。神経生物学だ。そして君は今、私の仮説を証明してくれた",
              "maleVariant": "",
              "stringId": "1741428005155442688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“話してる”わけじゃないがね。言いたいことはわかる",
              "maleVariant": "",
              "stringId": "1741431021430444032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コンストラクトが君の意思決定に影響を与えたことは？",
              "maleVariant": "",
              "stringId": "1741433263185268736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーがあたしを操ることはできない",
              "maleVariant": "ジョニーが俺を操ることはできない",
              "stringId": "1741435253667729412"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういう意味？",
              "maleVariant": "どういう意味だ？",
              "stringId": "1741436864246910976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーは口を挟むだけ。物事を決めるのはあくまであたし自身よ",
              "maleVariant": "ジョニーは口を挟むだけ。物事を決めるのはあくまで俺自身だ",
              "stringId": "1741441168106131456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういう意味？",
              "maleVariant": "どういう意味だ？",
              "stringId": "1741442398949806080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いくらあいつでも、あたしを操るようなことはできない",
              "maleVariant": "いくらあいつでも、俺を操るようなことはできない",
              "stringId": "1741443128993583104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まったくそのとおり。お前の頑固さは筋金入りだ",
              "maleVariant": "",
              "stringId": "1741444155088113664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "君はいずれ、以前では考えられなかったような行動を取り始める",
              "maleVariant": "",
              "stringId": "1741445720284925952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かにそう感じるだろう。ただし天秤はいずれ傾く。ゆっくりと、だが確実に…",
              "maleVariant": "",
              "stringId": "1741453170039918592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そしてシルヴァーハンドのことはよく知っているだろう。破滅願望持ちの凶悪なテロリストだ",
              "maleVariant": "",
              "stringId": "1741455759586430976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言いたい放題言いやがって。洗脳されるなよ？",
              "maleVariant": "",
              "stringId": "1741469077055279104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少なくともジョニーは、あんたみたいなコウモリ野郎じゃない",
              "maleVariant": "",
              "stringId": "1741470079426826244"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もういい",
              "maleVariant": "",
              "stringId": "1741471177562722304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少なくともジョニーは、あんたみたいなコウモリ野郎じゃない",
              "maleVariant": "",
              "stringId": "1741474130470178816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんな話はもういい。もしかして、時間稼ぎでもしてるの？",
              "maleVariant": "そんな話はもういい。もしかして、時間稼ぎでもしてるのか？",
              "stringId": "1741476464130592768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼をかばうのか？ それとも今のはジョニーの言葉か？",
              "maleVariant": "",
              "stringId": "1741479817493417984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "賭けてもいい――彼は、過去に君の体を乗っ取ろうとしたことがあるはずだ",
              "maleVariant": "",
              "stringId": "1741480413269135360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それとも、自分が何も知らない役立たずだって言いたいわけ？",
              "maleVariant": "それとも、自分が何も知らない役立たずだって言いたいのか？",
              "stringId": "1741606084330983424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしそうなら、この場で頭を撃ち抜いてあげる。時間の無駄だから",
              "maleVariant": "もしそうなら、この場で頭を撃ち抜いてやるよ。時間の無駄だからな",
              "stringId": "1741607239979175936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "したけど、止めた",
              "maleVariant": "したが、止めた",
              "stringId": "1741617877069664256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいえ",
              "maleVariant": "いいや",
              "stringId": "1741618266099748864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "次はどうだろうな、喜んで彼に体を預けるかもしれないぞ",
              "maleVariant": "",
              "stringId": "1741622654163816448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。でも何も起きなかった",
              "maleVariant": "ああ。だが何も起きなかった",
              "stringId": "1741634756693790720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まるで恋に落ちた乙女だな。何を言っても私の言葉は届かない",
              "maleVariant": "",
              "stringId": "1741635660046209024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーは君を、君の体から追い出そうとしたんだぞ。なぜそれを問題視しない？",
              "maleVariant": "",
              "stringId": "1741638258467868672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "試してみるか？ 楽しそうだぞ…",
              "maleVariant": "",
              "stringId": "1741642078589620224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー…",
              "maleVariant": "",
              "stringId": "1741642380713725952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "冗談さ。こいつの口車に乗せられるんじゃねえぞ",
              "maleVariant": "",
              "stringId": "1741642791168315392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっちまえ",
              "maleVariant": "",
              "stringId": "1741646296633380864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "力になろう… 私と一緒にカン・タオへ来てくれるなら",
              "maleVariant": "",
              "stringId": "1741646538946711552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だからこそ私は、隔離された医療機関での治療を検討すべきだと思う",
              "maleVariant": "",
              "stringId": "1741660091783122944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今、手元にあるの？",
              "maleVariant": "今、手元にあるのか？",
              "stringId": "1741671447357886464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何もないよりマシか。こいつを読み取れるテックオタクを見つけないとな",
              "maleVariant": "",
              "stringId": "1741688433617997824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "持ってなかったら、いよいよ役立たずだな",
              "maleVariant": "",
              "stringId": "1741691437209423872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブの計画は絶対に失敗すると思っていたが… こんな展開は想定外だ",
              "maleVariant": "",
              "stringId": "1741703675819397120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実際、悪い考えじゃなかったのよ",
              "maleVariant": "実際、悪い考えじゃなかったんだ",
              "stringId": "1741706535596527616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それでチップを自分の頭に入れたのか？ 正気とは思えない",
              "maleVariant": "",
              "stringId": "1741707120634826752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭を撃たれたけど、チップのおかげで生き返ることができた",
              "maleVariant": "頭を撃たれたが、チップのおかげで生き返ることができた",
              "stringId": "1741709244143833088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカの最新技術が民間人の手に渡るとは",
              "maleVariant": "",
              "stringId": "1741711329149128704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "違う",
              "maleVariant": "",
              "stringId": "1753070742304788480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それに、純粋に興味があるんだ",
              "maleVariant": "",
              "stringId": "1753093466456657920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チップはどこで手に入れたんだ？",
              "maleVariant": "",
              "stringId": "1753100126306766848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "普通のと何が違うの？",
              "maleVariant": "普通のと何が違うんだ？",
              "stringId": "1760266424849600516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "抜けばいいってことじゃないのは理解してる",
              "maleVariant": "",
              "stringId": "1760268953041833984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "従来のチップと何が違うの？ 記憶痕跡のタイプが… なんというか、より…",
              "maleVariant": "従来のチップと何が違うんだ？ 記憶痕跡のタイプが… なんというか、より…",
              "stringId": "1760282850834583552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もういい",
              "maleVariant": "",
              "stringId": "1760287834573705216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もういい",
              "maleVariant": "",
              "stringId": "1760290635446030336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "難しいのはわかってる",
              "maleVariant": "",
              "stringId": "1760293835062448128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…主体的？",
              "maleVariant": "",
              "stringId": "1760298829639651328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…攻撃的とか？",
              "maleVariant": "",
              "stringId": "1760300219883655168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "特別なのはそのチップであり、シルヴァーハンドの記憶痕跡ではない",
              "maleVariant": "",
              "stringId": "1760302462544449536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "以前のバージョンでは、あらかじめ保存された記憶痕跡とやりとりするだけだった",
              "maleVariant": "",
              "stringId": "1760307569696772096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。希少価値も価格もバカ高かったんでしょ",
              "maleVariant": "ああ。希少価値も価格もバカ高かったんだろ",
              "stringId": "1760341795737505792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "プロトタイプはそれ以上だ。記憶痕跡を別の生体内で活動させられる",
              "maleVariant": "",
              "stringId": "1760345936874508288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "モルモットになれて光栄ね",
              "maleVariant": "モルモットになれて光栄だな",
              "stringId": "1760347764601196548"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私がアラサカを離れた時、プロジェクトはまだテスト段階だった",
              "maleVariant": "",
              "stringId": "1760375424190582784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "能書きはいいから、さっさとチップを取り出して",
              "maleVariant": "能書きはいいから、さっさとチップを取り出してくれ",
              "stringId": "1760378576797241344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんな高額なチップを買える人なんて存在するの？",
              "maleVariant": "そんな高額なチップを買える人なんて存在するのか？",
              "stringId": "1760383619910930432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなすごい技術のモルモットになれて光栄ね",
              "maleVariant": "そんなすごい技術のモルモットになれて光栄だな",
              "stringId": "1760400910291636224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "要は永遠の命じゃない… そんな代物を買える人なんているの？",
              "maleVariant": "要は永遠の命だろ… そんな代物を買える人なんているのか？",
              "stringId": "1760401041455910912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新型チップの本来の目的は何なの？",
              "maleVariant": "新型チップの本来の目的は何なんだ？",
              "stringId": "1760425684216647680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新型チップはそもそもどういう目的で作られたの？",
              "maleVariant": "新型チップはそもそもどういう目的で作られたんだ？",
              "stringId": "1760430031059095552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなことはどうでもいい。さっさとチップを取りだして",
              "maleVariant": "そんなことはどうでもいい。さっさとチップを取りだしてくれ",
              "stringId": "1760494239243112448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずは見させてもらってからだ",
              "maleVariant": "",
              "stringId": "1760497478420205568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "記憶痕跡が元の人格を追い出し、体を乗っ取るっていうのは、バグじゃなくて仕様なわけ？",
              "maleVariant": "記憶痕跡が元の人格を追い出し、体を乗っ取るっていうのは、バグじゃなくて仕様なのか？",
              "stringId": "1760513541950214144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "体の方は移植工程で神経活動を停止する――試験段階ではそう考えられていた",
              "maleVariant": "",
              "stringId": "1760528180876480512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死ぬってこと？",
              "maleVariant": "死ぬってことか？",
              "stringId": "1760534612724224000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その通り。だからこそ、君の身に起きたことは興味深い",
              "maleVariant": "",
              "stringId": "1760534904765222912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いないさ。そもそも社外に出す予定はなかった",
              "maleVariant": "",
              "stringId": "1760537649635201024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "プロトタイプはサブロウ・アラサカのために作られたんだ。プロジェクト全体の指揮も彼が執っていた",
              "maleVariant": "",
              "stringId": "1760538035393728512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブがいなければ、誰にも知られることはなかったのね",
              "maleVariant": "ヨリノブがいなければ、誰にも知られることはなかったわけか",
              "stringId": "1760546282402025472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そして君がいなければ、プロジェクトは成功していなかっただろう。君は歴史を作ったんだ",
              "maleVariant": "",
              "stringId": "1760547566261702656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、協力はするの？",
              "maleVariant": "で、協力はするのか？",
              "stringId": "1760557486059098112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カン・タオはダメよ",
              "maleVariant": "カン・タオはダメだ",
              "stringId": "1760605837039308800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんな言葉じゃ信用できない",
              "maleVariant": "",
              "stringId": "1760606745827545088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カン・タオはダメよ。他に考えはないの？",
              "maleVariant": "カン・タオはダメだ。他に考えはないのか？",
              "stringId": "1760607440270069760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まったく信用できない。むしろあたしをダシに、カン・タオに取り入ろうとしてるとしか思えない",
              "maleVariant": "まったく信用できないな。むしろ俺をダシに、カン・タオに取り入ろうとしてるとしか思えない",
              "stringId": "1760609138342432768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その頭にあるデータの価値を、わかっているのか？",
              "maleVariant": "",
              "stringId": "1760610143968759808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ、この野郎。ついに本音が出やがったな。こいつはお前を利用したいだけだ",
              "maleVariant": "",
              "stringId": "1760611506446471168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "君を助けられないとしても、そのデータだけは救い出さなくては",
              "maleVariant": "",
              "stringId": "1760617145553981440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私以外に頼れる人物がいるというなら、チップの設計図を渡してやってもいい",
              "maleVariant": "",
              "stringId": "1760636790969274368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "プロジェクトの全貌が記載されている。カン・タオは大層な額を提示してくれたよ",
              "maleVariant": "",
              "stringId": "1760637546363424768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V",
              "maleVariant": "",
              "stringId": "1760656716396302336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと待ってて",
              "maleVariant": "ちょっと待ってくれ",
              "stringId": "1760657355926999040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "タケムラ？ どうしてこいつが？",
              "maleVariant": "",
              "stringId": "1760658297044930560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼からもいくつか質問があるそうよ。正直に答えれば、助けてくれるかもね",
              "maleVariant": "彼からもいくつか質問があるそうだ。正直に答えれば、助けてくれるかもな",
              "stringId": "1760659035544424448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "設計図を",
              "maleVariant": "",
              "stringId": "1760660024158654464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはもう行くから。こいつは好きにして",
              "maleVariant": "俺はもう行く。こいつは好きにしてくれ",
              "stringId": "1760663529086849024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話をすれば、きっとわかり合えるはずだ",
              "maleVariant": "",
              "stringId": "1760663529086849048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん… だといいがな",
              "maleVariant": "",
              "stringId": "1760663529103626248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V。このことは忘れない",
              "maleVariant": "",
              "stringId": "1760663529103626260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リパーが言うには…",
              "maleVariant": "",
              "stringId": "1764872465671327744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん、アラサカ内でも俺様は伝説的存在になってるのか",
              "maleVariant": "",
              "stringId": "1771984362950430720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこらのリパードクに見せたのか？ 他に誰が知ってる！？",
              "maleVariant": "",
              "stringId": "1772012712536379392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "助けが必要だったのよ",
              "maleVariant": "助けが必要だったんだ",
              "stringId": "1772018444673552384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "医者に行く前に、カン・タオのAVを墜とそうとするほどバカじゃない",
              "maleVariant": "",
              "stringId": "1772022590290071552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつはどうなるの？",
              "maleVariant": "こいつはどうなるんだ？",
              "stringId": "1793692332974825476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつはどうなるの？",
              "maleVariant": "こいつはどうなるんだ？",
              "stringId": "1793708328305451008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ決めかねている",
              "maleVariant": "",
              "stringId": "1793709156814708736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "起きて",
              "maleVariant": "起きろ",
              "stringId": "1795207584801951744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "冗談でしょ？",
              "maleVariant": "冗談だろ？",
              "stringId": "1802416175686422528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "流れに身を任せてみようかと思ってな",
              "maleVariant": "",
              "stringId": "1802418009637457920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] ゴロウ、あとは任せた",
              "maleVariant": "",
              "stringId": "2057822433011068932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] 起きて",
              "maleVariant": "[座る] 起きろ",
              "stringId": "2059154438717992960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しかし、暑いな。喉がカラカラだよ",
              "maleVariant": "",
              "stringId": "2243749229481246720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんだ、Vは水も出さなかったのか？",
              "maleVariant": "",
              "stringId": "2243753091210493952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やれやれ、非情な女だ",
              "maleVariant": "やれやれ、非情な男だ",
              "stringId": "2243758465439318016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが心配するな。俺は違う。今すぐバケツ一杯持ってきてやろう",
              "maleVariant": "",
              "stringId": "2243758750534549504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "い、いや… 結構だ",
              "maleVariant": "",
              "stringId": "2243759873047740416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、私はなぜここに？",
              "maleVariant": "",
              "stringId": "2258210404868636672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうした、何とか言ったらどうだ？",
              "maleVariant": "",
              "stringId": "2258222213545672704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "君の状態について、もっと知りたくはないかね？",
              "maleVariant": "",
              "stringId": "2258229581511737344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ずいぶんと静かだな",
              "maleVariant": "",
              "stringId": "2258235725345013760"
            },
            {
