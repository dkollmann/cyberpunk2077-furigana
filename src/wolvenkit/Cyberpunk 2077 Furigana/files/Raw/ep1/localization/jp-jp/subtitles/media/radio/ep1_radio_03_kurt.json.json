  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドッグタウンの全住人に告ぐ！ こちらはカート・ハンセンだ",
              "maleVariant": "",
              "stringId": "2584133210136862720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "所属不明の航空機がこの地区に墜落した",
              "maleVariant": "",
              "stringId": "3065195960630448128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "墜落現場は当面立入禁止とさせてもらう",
              "maleVariant": "",
              "stringId": "3065202296579624960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "安全確認が済むまで、住民たちは自宅で待機するように",
              "maleVariant": "",
              "stringId": "3065203029962067968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "追って通告があるまで、ドッグタウンの非常事態を宣言する",
              "maleVariant": "",
              "stringId": "3065215056591917056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "状況に進展があり次第、続報を伝える",
              "maleVariant": "",
              "stringId": "3336323385528786944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "流れ弾を食らいたくなければ従うことだ",
              "maleVariant": "",
              "stringId": "3336327947639693312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドッグタウンに“不法入国”したサイバーサイコの件について続報だ。懸賞金を2倍に引き上げることにした",
              "maleVariant": "",
              "stringId": "3365151643775197184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "皆の者に朗報だ。先日、病院の改修が完了した",
              "maleVariant": "",
              "stringId": "3365265242204069892"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "火曜日に、スカベンジャーの代表者らがドッグタウンを訪問する",
              "maleVariant": "",
              "stringId": "3365265768320786440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "皆の者に朗報だ。とある大統領が、無事ワシントンにいることが判明した",
              "maleVariant": "",
              "stringId": "3365266578106671112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もうすぐ、我々がここに新たな故郷を築いてから7年が経つ",
              "maleVariant": "",
              "stringId": "3365266578157002760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドッグタウンの住民たち。こちらはカート・ハンセンだ",
              "maleVariant": "",
              "stringId": "3365301507028869128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドッグタウンの全住民に告ぐ。こちらはカート・ハンセンだ",
              "maleVariant": "",
              "stringId": "3365301866212286464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞け！ こちらはカート・ハンセンだ",
              "maleVariant": "",
              "stringId": "3365301866229063688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "通信終わり",
              "maleVariant": "",
              "stringId": "3365305042927804424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "以上だ",
              "maleVariant": "",
              "stringId": "3365305042944581640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新しい情報が入り次第、追って通告する",
              "maleVariant": "",
              "stringId": "3365305042978136072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドッグタウンにお客様がやってくる。私個人が招いたゲストだ。無礼を働かないよう、注意せよ",
              "maleVariant": "",
              "stringId": "3366315694819565576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドッグタウンの全住民に告ぐ！ こちらはベネットだ",
              "maleVariant": "",
              "stringId": "3366340273239678984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドッグタウンの全住民に告ぐ！ こちらはベネット中… 大佐だ！",
              "maleVariant": "",
              "stringId": "3366340830276804616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すでに聞いている者も多いと思うが、先日ドッグタウンに特別なお客様がいらっしゃった。マイヤーズ大統領だ",
              "maleVariant": "",
              "stringId": "3366340957263552520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドッグタウンの住民たちへ。こちらはヤゴだ",
              "maleVariant": "",
              "stringId": "3366393203862986760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カート・ハンセン大佐が死亡した。繰り返す、カート・ハンセン大佐が死亡した",
              "maleVariant": "",
              "stringId": "3394251262425145344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まことに残念な報せである",
              "maleVariant": "",
              "stringId": "3394251843554353152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセン大佐がいなくとも、バーゲストは健在だからだ！",
              "maleVariant": "",
              "stringId": "3394252636495912960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ、これどうやってオフにするんだ…？",
              "maleVariant": "",
              "stringId": "3394255686862217216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "標的はでかい図体をしており、フル・フェイスプレートを装着",
              "maleVariant": "",
              "stringId": "3395367408528367616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "武装しており、まともに会話できる状態ではない。見つけた場合は躊躇せずに撃て",
              "maleVariant": "",
              "stringId": "3395367608764440576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新合衆国は、彼女が搭乗するスペースフォース・ワンが、ドッグタウンに不時着したと発表している",
              "maleVariant": "",
              "stringId": "3395370272298475520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしそれが本当ならば、我々には彼女の無事を確認し、保護する義務がある",
              "maleVariant": "",
              "stringId": "3395371142046130176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大統領に関する情報を提供した者には、私個人から報酬を与えよう。神経スティムだ",
              "maleVariant": "",
              "stringId": "3395372270968217600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "現在、表通りや検問所の警備を強化している。許可証がなければ出ることも、入ることもできないので注意するように",
              "maleVariant": "",
              "stringId": "3395372672514105344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大佐は、戦いの中で命を落とした。軍人らしい立派な最期だった。そして、彼を殺したクソ野郎どもには、必ず報いを受けてもらう。なぜかわかるか？",
              "maleVariant": "",
              "stringId": "3395377432948482048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2か月に及ぶ重労働がようやく実を結んだ。本日より、病院およびその関係者は、我々の庇護のもと働くことになる",
              "maleVariant": "",
              "stringId": "3395476523682471936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "切り傷の処置から新しい義足まで、必要なものがあればドクター・ロバートソンに報告するがいい",
              "maleVariant": "",
              "stringId": "3395477468424282112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "遠方からの出張だ。時差ボケで深夜の徘徊に出るかもしれない。そのため、6時以降の外出を禁止する",
              "maleVariant": "",
              "stringId": "3395479029074468864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "戸締りをしっかりと行うように。またBDによる記録も禁止とする",
              "maleVariant": "",
              "stringId": "3395479345962524672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "暴力、抑圧、空っぽの約束――我々は長年に渡り試され続けてきた。しかし、どうだ。今もなお、こうやってここに立っている。むしろ、思い出せば懐かしさすら感じるくらいだ",
              "maleVariant": "",
              "stringId": "3395481131528069120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今は、タバコに火をつけ、吸い殻を向こう側に吐き捨てることができる。だが7年前、あの運河はもっと広く、“向こう側”はもっと遠かった。そして、宙を舞っていたのは吸い殻ではなく、鉛玉だ",
              "maleVariant": "",
              "stringId": "3395481407664267264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私は、ナイトシティ襲撃の際に3つの命令を受けた。1つ目は、街の最南端の制圧。我々は成功した。2つ目は、そこに拠点を築き、維持すること。我々は成功した",
              "maleVariant": "",
              "stringId": "3395481667140689920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そして3つ目は… 撤退。それまでの命令をなかったことにすることだった。今、我々の拠点があったあの運河を見たらどうだ。昼間は住民たちがビールを飲み、夜になると届けられた物資を回収する。じつにのどかな光景だと思わないか？",
              "maleVariant": "",
              "stringId": "3395481855599157248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スタンという男の話をしてやろう。スタンは南部出身の青年だった。ダンサーになるのが夢だったが、どういうわけか私の部隊に配属された。人生とは、得てしてそういうものだ",
              "maleVariant": "",
              "stringId": "3395482398040104960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの夜… スタンは私の目の前を走っていた。そしたら、急に綺麗なピルエットを披露したんだ。こんな時に何をやってるんだと説教しようとした瞬間――敵のスナイパー部隊が目に入った",
              "maleVariant": "",
              "stringId": "3395482621512622080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョーイ… アトランタ出身のメカニックで、2人の子供がいた。心優しいエンジニアのエミリー。衛生兵のカール――冷酷で血を見るのが好きだったが、なぜか嫌いになれなかった",
              "maleVariant": "",
              "stringId": "3395483304009768960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼らの階級はあえて言うまい。立派な名を持つ一人の人間として、記憶に刻むべきだ。そして、あまりにも多くの名前をこれまで刻んできた…",
              "maleVariant": "",
              "stringId": "3395483381117853696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "我々は与えられた任務をすべて、完璧にこなした。なのに上は、我々をゴミのように運河に捨てることを選んだんだ。「お前たちは存在しない。我々はそもそもここにいなかった」… 奴らの言葉、裏切り者の言葉だ",
              "maleVariant": "",
              "stringId": "3395483935504179200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スタンと他の殉職者たちに、黙とうを捧げるべきか？ いや、もっといい考えがある。銃を手にしろ。そして表に出て、21発どころか、1000発の祝砲をあげるんだ",
              "maleVariant": "",
              "stringId": "3395484184578727936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼らは、それに値する働きをしてくれた。我々はまだここにいると。そして、どこにも行かないと――ドッグタウンの咆哮を天国にまで響かせてやれ！",
              "maleVariant": "",
              "stringId": "3395484920645525504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズ大統領だが、依然として行方不明のままだ",
              "maleVariant": "",
              "stringId": "3395522225674493956"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私のことを知らない者もいると思うが… この度、カート・ハンセン大佐の後を継ぐことになった。大佐、どうか安らかに…",
              "maleVariant": "",
              "stringId": "3395578197990285312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まず、これより組織改革を進めていく。しかし、住民たちの生活やビジネスに影響が出ることはないので安心してくれ",
              "maleVariant": "",
              "stringId": "3395579945001787392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大佐のような偉大な男の代わりになれるとは思えないが、できる限りのことはしていくつもりだ",
              "maleVariant": "",
              "stringId": "3395581467500584960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なお、バーゲストによるラジオ通達は、これで最後となる。今後はもう少し… 近代的な通信手段を確立するつもりだ",
              "maleVariant": "",
              "stringId": "3395583349920997376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "通信終わり",
              "maleVariant": "",
              "stringId": "3395592548902084608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本日より、この私がバーゲストを率いることになった",
              "maleVariant": "",
              "stringId": "3395595323232854016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドッグタウンの未来は明るい。皆の期待に添えられるよう努力… 尽力するつもりだ",
              "maleVariant": "",
              "stringId": "3395608639023980544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろん、命令に従う住民に限った話だがな",
              "maleVariant": "",
              "stringId": "3395611115072626688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "報告は以上だ。通信終わり",
              "maleVariant": "",
              "stringId": "3395612340312711168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直… 君たちには少し失望した",
              "maleVariant": "",
              "stringId": "3395620918973014016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズ大統領の居場所に関する情報を引き続き求める。報酬は2倍だ。通信終わり",
              "maleVariant": "",
              "stringId": "3395621197927784448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私に言わせれば、そもそも彼女はずっとD.C.にいらっしゃったと思うがな… いずれにせよ、新合衆国の見え透いた挑発に乗るような愚かな真似はしない",
              "maleVariant": "",
              "stringId": "3395634287276826624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私はスケープゴートにされているんだ。新合衆国は長年、私の首を狙い続けている",
              "maleVariant": "",
              "stringId": "3470880000792961024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とはいえ、これは決して笑いごとではない。これを聞いている者たちも、裏に隠された真実について考える必要がある",
              "maleVariant": "",
              "stringId": "3470882295999995904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私は決して聖人ではないが、自殺願望があるわけでもない。大統領の暗殺を企てるなど… バカバカしい",
              "maleVariant": "",
              "stringId": "3470883160731262976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこにきてこれだ。そして連中がドッグタウンを制圧して満足すると思うか？ いいや――次の標的はナイトシティだ",
              "maleVariant": "",
              "stringId": "3470887239725457408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新合衆国は、ドッグタウンに軍隊を送り込む口実を探り続けていた",
              "maleVariant": "",
              "stringId": "3470888729793896448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいか… 新たな戦争…",
              "maleVariant": "",
              "stringId": "3470890842313826304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こちらはカート・ハンセン。そして、ここはドッグタウンだ",
              "maleVariant": "",
              "stringId": "3489790704098844672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ルールその2。コーポも立入禁止だ！ お前らの居場所はここにない！",
              "maleVariant": "",
              "stringId": "3489791848271421440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ルールその1。NCPDは立入禁止だ。発見した場合、即刻頭を撃ち抜いてやる！",
              "maleVariant": "",
              "stringId": "3489793122047991808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ルールその3。この地区の治安は私の部下が守る。ここでは彼らが法だ",
              "maleVariant": "",
              "stringId": "3489794109202604032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "従わない者には相応の罰を与える",
              "maleVariant": "",
              "stringId": "3489809289412190208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全住民に告ぐ！ こちらはカート・ハンセンだ",
              "maleVariant": "",
              "stringId": "3489829697553276928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "当地区に、テロリストが潜伏していることが発覚した。これに伴い、警備の強化を実施する",
              "maleVariant": "",
              "stringId": "3489830921618956288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "住民たちは外出を控えるように",
              "maleVariant": "",
              "stringId": "3489834730416087040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "不審な行動を見かけた際は、ただちに報告せよ",
              "maleVariant": "",
              "stringId": "3489835740589039616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こちらはカート・ハンセンだ",
              "maleVariant": "",
              "stringId": "3499852020599595008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "我らがドッグタウンへようこそ。歓迎する",
              "maleVariant": "",
              "stringId": "3499853481475678208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここにはNCPDもいなければ、ギャングの抗争もない",
              "maleVariant": "",
              "stringId": "3499854249570181120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "税金？ そんなものは不要だ",
