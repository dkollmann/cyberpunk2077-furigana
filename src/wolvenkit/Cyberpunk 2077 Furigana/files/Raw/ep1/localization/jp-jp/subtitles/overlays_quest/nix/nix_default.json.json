      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、お互いネットランナーでしょ",
              "maleVariant": "なあ、お互いネットランナーだろ",
              "stringId": "1777673526209060864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "教えてほしいことがある",
              "maleVariant": "",
              "stringId": "1777673849103360004"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこかで会ったような…",
              "maleVariant": "",
              "stringId": "1777674139936399360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこかで会ったような…",
              "maleVariant": "",
              "stringId": "1777675953134018564"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪いが、人違いだ",
              "maleVariant": "",
              "stringId": "1777675953134018572"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグと働くのはどんな感じなの？",
              "maleVariant": "ローグと働くのはどんな感じなんだ？",
              "stringId": "1941427920394907648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "プロとしてのコツを教えてくれない？",
              "maleVariant": "プロとしてのコツを教えてくれないか？",
              "stringId": "1941429259451625472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別にいいぜ。ローグの仲間なら、俺の仲間だからな",
              "maleVariant": "",
              "stringId": "1941431684312985600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バートモス時代のレアものを、あんたに売ってやってもいい",
              "maleVariant": "",
              "stringId": "1941432754581925888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バートモス？ あのレイシィ・バートモス？ 冗談でしょ",
              "maleVariant": "バートモス？ あのレイシィ・バートモス？ 冗談だろ",
              "stringId": "1941433570608934912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "姿を消す前、あいつはネットの片隅にICEを突破するプログラムや試作品をばらまいたんだ",
              "maleVariant": "",
              "stringId": "1941435270073806848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "妄想癖がひどかったらしいね。結局彼はどうなったの？",
              "maleVariant": "妄想癖がひどかったらしいな。結局彼はどうなったんだ？",
              "stringId": "1941438056182181888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあな。でもバートモスが残したテックをいくつか見つけることができた。ほら、見せてやるよ",
              "maleVariant": "",
              "stringId": "1941438867645788160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグと働くのはどんな感じ？",
              "maleVariant": "ローグと働くのはどんな感じだ？",
              "stringId": "1941440636333453312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "思い出した… アラサカとN54との交渉で… あの防衛部のサイバーサイコシス騒動よ",
              "maleVariant": "思い出した… アラサカとN54との交渉で… あの防衛部のサイバーサイコシス騒動だ",
              "stringId": "1941442575007870976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカの元従業員ね。あの漏えいに関わってたんでしょ？ もう死んだと思ってた。なのにこうしてアフターライフにいるなんて！",
              "maleVariant": "アラサカの元従業員だな。あの漏えいに関わってたんだろ？ もう死んだと思ってた。なのにこうしてアフターライフにいるなんて！",
              "stringId": "1941445151837253632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お互い元モノクローマーか。きれいな空気だけは恋しいよ",
              "maleVariant": "",
              "stringId": "1941446732200669184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "普通さ",
              "maleVariant": "",
              "stringId": "1941449279518928896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「普通」って、それだけ？",
              "maleVariant": "",
              "stringId": "1941456656360140800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグについて知りたいなら、自分で聞けよ。喜んで答えてくれるって",
              "maleVariant": "",
              "stringId": "1941456983029313536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またあとで",
              "maleVariant": "",
              "stringId": "2534350856845660160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞いて、ニックス… あんたにぴったりの仕事があるんだけど",
              "maleVariant": "聞いてくれ、ニックス… あんたにぴったりの仕事があるんだが",
              "stringId": "3224447274096390172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょうど暇してたとこさ。どんな話だ？",
              "maleVariant": "",
              "stringId": "3224447274096390184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マックス・タックのシステムに侵入してほしい",
              "maleVariant": "",
              "stringId": "3224447276277428224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "超がつくくらい危険だけど、大丈夫？",
              "maleVariant": "超がつくくらい危険だが、大丈夫か？",
              "stringId": "3224447276277428240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マックス・タックのシステムに侵入してほしい",
              "maleVariant": "",
              "stringId": "3224447276327759872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっぱり、また後で連絡する",
              "maleVariant": "",
              "stringId": "3224447276327759888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼んだよ、ニックス。ありがとう",
              "maleVariant": "頼んだぞ、ニックス。ありがとう",
              "stringId": "3224447276378091520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっぱりもう少し考えてみる",
              "maleVariant": "",
              "stringId": "3224447276378091536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "かなり危険なヤマよ。あんたがそこまでエッジを攻める気があるかどうか…",
              "maleVariant": "かなり危険なヤマだ。あんたがそこまでエッジを攻める気があるかどうか…",
              "stringId": "3224447276428423180"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあヒントをくれ…",
              "maleVariant": "",
              "stringId": "3224447276428423192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグなら、その仕事を扱うか？",
              "maleVariant": "",
              "stringId": "3224447276428423204"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや… 正直、なんとも言えない",
              "maleVariant": "正直、なんとも言えない",
              "stringId": "3224447276428423216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっとろくな仕事じゃないな… でも俄然気になってきた",
              "maleVariant": "",
              "stringId": "3224447276428423228"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マックス・タックのデータ要塞に侵入して、ナイトシティからロス・パドレスに向かう輸送隊に関する情報を手に入れてほしい",
              "maleVariant": "",
              "stringId": "3224447278827565068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マックス・タックに侵入？ ハッ、またまた… って、おい… まさか本気か、V？",
              "maleVariant": "",
              "stringId": "3224447278827565080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたじゃ無理？",
              "maleVariant": "あんたじゃ無理か？",
              "stringId": "3224447278827565092"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのな、マックス・タックには数秒おきにアクセスポートを再暗号化する2つの自律型AIがいるんだ",
              "maleVariant": "",
              "stringId": "3224447278827565104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バックドアから見つからないように侵入するのは簡単だ。だが、痕跡を残さずに仕事を終えるとなると… フン",
              "maleVariant": "",
              "stringId": "3224447278827565116"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "できないってことね",
              "maleVariant": "できないってことだな",
              "stringId": "3224447278827565128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何言ってんだ、V。お前には借りがある。やってやるさ",
              "maleVariant": "",
              "stringId": "3224447278827565140"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっぱり… もう少し考えてみる。また後で連絡するよ",
              "maleVariant": "",
              "stringId": "3224447283458076676"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、好きにしな。じゃあな、V",
              "maleVariant": "",
              "stringId": "3224447283458076688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。なんだ、やっと心を決めたのか？",
              "maleVariant": "",
              "stringId": "3224447284364046344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃ頼んだよ、ニックス",
              "maleVariant": "それじゃ頼んだぞ、ニックス",
              "stringId": "3224447284414378000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう",
              "maleVariant": "",
              "stringId": "3224447284414378012"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "礼はデータを渡した時でいい。俺が脳ミソを焼かれるか、ロス・パドレスにぶちこまれてなきゃだが",
              "maleVariant": "",
              "stringId": "3224447284414378024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたにぴったりの仕事があるかも…",
              "maleVariant": "",
              "stringId": "3224449067564003328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ニックス、例の仕事の件だけど…",
              "maleVariant": "ニックス、例の仕事の件だが…",
              "stringId": "3224456195146117120"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
