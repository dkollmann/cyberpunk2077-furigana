    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンズの傭兵だな？ 仕事についてどこまで把握してる？",
              "maleVariant": "",
              "stringId": "3217550595964899332"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あまり。ハンズはアスリートと… あと脱税がどうとか言ってたけど",
              "maleVariant": "あまり。ハンズはアスリートと… あと脱税がどうとか言ってたが",
              "stringId": "3217550597172858892"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あまりわかってない",
              "maleVariant": "",
              "stringId": "3217550599957876736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "挨拶もなし？",
              "maleVariant": "挨拶もなしか？",
              "stringId": "3217550599957876748"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "せめて自己紹介くらいしたらどうなの？",
              "maleVariant": "せめて自己紹介くらいしたらどうなんだ？",
              "stringId": "3217550602709340172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この“アカデミー”は、若いアスリートの育成施設だ。世界中からスカウトが来て、彼らと契約を結ぶ",
              "maleVariant": "",
              "stringId": "3217704968598687744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "問題ない",
              "maleVariant": "",
              "stringId": "3217705298036101124"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業のICEブレーカーを使ってるのね",
              "maleVariant": "企業のICEブレーカーを使ってるんだな",
              "stringId": "3217711142563844096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜドッグタウンにスポーツアカデミーが？",
              "maleVariant": "",
              "stringId": "3217711946511257600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "偽の素性についてもっと詳しく教えて",
              "maleVariant": "偽の素性についてもっと詳しく教えてくれ",
              "stringId": "3217712354717700096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人物設定が心細い。もっと肉付けが必要じゃない？",
              "maleVariant": "人物設定が心細い。もっと肉付けが必要じゃないか？",
              "stringId": "3217713301992869888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや。中へ入るのに使うだけだ。その先は余計なことを言わなきゃいい。上手く紛れ込み、データを盗んで脱出しろ",
              "maleVariant": "",
              "stringId": "3217715863454322688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドッグタウンにスポーツアカデミーが？",
              "maleVariant": "",
              "stringId": "3217718637969641472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "優秀なアスリートを育成する施設は世界中に山ほどある",
              "maleVariant": "",
              "stringId": "3217720641303449600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰も気に留めない場所",
              "maleVariant": "",
              "stringId": "3217722216986021888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その通り。すなわちドッグタウンだ。ハンセンが敷いたルールは絶対だが、それ以外は無法地帯だからな",
              "maleVariant": "",
              "stringId": "3217725088926633984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうした？",
              "maleVariant": "",
              "stringId": "3220434318897025024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "珍しい――それ、アラサカのデーモンでしょ？",
              "maleVariant": "珍しいな――それ、アラサカのデーモンだろ？",
              "stringId": "3220434732371513344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてアラサカとわかった？",
              "maleVariant": "",
              "stringId": "3220454901118504960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "元防諜部よ。第3支部",
              "maleVariant": "元防諜部だ。第3支部",
              "stringId": "3220455217251586048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ。俺はサイバーセキュリティの第1だ。お互い道を誤ったらしいな。失礼… 俺はベアードだ",
              "maleVariant": "",
              "stringId": "3220455392690933760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "問題ない",
              "maleVariant": "",
              "stringId": "3233493918211440640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし。あとはクイックスキャンを… できた。お先にどうぞ、ビクトリア・ウィルソン",
              "maleVariant": "よし。あとはクイックスキャンを… できた。お先にどうぞ、ビクター・ウィルソン",
              "stringId": "3233494790224019456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ、今さら紳士ぶられてもね。それじゃ…",
              "maleVariant": "ハッ、今さら紳士ぶられてもな。それじゃ…",
              "stringId": "3233495172308336640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vよ",
              "maleVariant": "Vだ",
              "stringId": "3234695797197045760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫",
              "maleVariant": "大丈夫だ",
              "stringId": "3258088963907620868"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え？ 聞いてなかった",
              "maleVariant": "",
              "stringId": "3258091632441266176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。サンディエゴ・シュレッダーズのビクトリア・ウィルソン、28歳、目当てはワイドレシーバー",
              "maleVariant": "ああ。サンディエゴ・シュレッダーズのビクター・ウィルソン、28歳、目当てはワイドレシーバー",
              "stringId": "3258092726819065856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前はサンディエゴ・シュレッダーズのスカウトとして潜入する。名はビクトリア・ウィルソン、28歳。お目当てはワイドレシーバーだ",
              "maleVariant": "お前はサンディエゴ・シュレッダーズのスカウトとして潜入する。名はビクター・ウィルソン、28歳。お目当てはワイドレシーバーだ",
              "stringId": "3258093931288956928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "施設のシステムに予約を入れる。ここまではいいか？",
              "maleVariant": "",
              "stringId": "3258094169877745664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜハンズは税金を回収しようとしてるの？",
              "maleVariant": "なぜハンズは税金を回収しようとしてるんだ？",
              "stringId": "3258100093795274752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "えー、ごめん、ぼーっとしてた",
              "maleVariant": "あー、すまん、ぼーっとしてた",
              "stringId": "3258101705498529792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "上出来だ。余計なことを言ってボロが出ないよう気をつけろよ",
              "maleVariant": "",
              "stringId": "3258112155674054656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本気か？",
              "maleVariant": "",
              "stringId": "3258113399083864064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "潜入したらフィオナ・バーガス所長の部屋を探し、過去に契約を結んだスカウトの情報を盗むんだ。何か質問は？",
              "maleVariant": "",
              "stringId": "3258113896326991872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウソよ。サンディエゴ・シュレッダーズのビクトリア・ウィルソン、28歳、目当てはワイドレシーバー、そしてあんたの態度が気に入らない",
              "maleVariant": "ウソだよ。サンディエゴ・シュレッダーズのビクター・ウィルソン、28歳、目当てはワイドレシーバー、そしてあんたの態度が気に入らない",
              "stringId": "3258114808923656192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜハンズが税金の取り立てを？ どうも怪しい",
              "maleVariant": "",
              "stringId": "3258129854395420672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "税金だろうがなんだろうが、エディーが回収できれば充分だろう。地味な仕事だが、儲けはいい",
              "maleVariant": "",
              "stringId": "3258130552294051840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが未成年に“強化”を施したい場合… あんたならどこへ行く？",
              "maleVariant": "",
              "stringId": "3261164732795949056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vよ",
              "maleVariant": "Vだ",
              "stringId": "3262387333941792768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あとはクイックスキャンをすれば… よし。システムに認識された。これで中に入れるはずだ。幸運を祈る",
              "maleVariant": "",
              "stringId": "3265315576894296064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "準備はできた？",
              "maleVariant": "準備はできたか？",
              "stringId": "3265334799255310336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もういける？",
              "maleVariant": "もういけるか？",
              "stringId": "3265335709687713792"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}