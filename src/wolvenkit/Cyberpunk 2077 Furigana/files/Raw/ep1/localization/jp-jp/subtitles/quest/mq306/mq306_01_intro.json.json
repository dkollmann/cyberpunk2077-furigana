  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこまで！ 試合終了だー！",
              "maleVariant": "",
              "stringId": "2918411694085398528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい試合だった",
              "maleVariant": "",
              "stringId": "3226105583450849280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アーロン・ウェインズ？",
              "maleVariant": "",
              "stringId": "3226105716309622784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… ああ。なんだ、サインか？",
              "maleVariant": "",
              "stringId": "3226154055663222784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "次の対戦相手は？",
              "maleVariant": "",
              "stringId": "3226161980414754816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜダミールじゃなきゃダメなの？",
              "maleVariant": "なぜダミールじゃなきゃダメなんだ？",
              "stringId": "3226180595524435972"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アニマルズのことだけど… ボクサーになるために抜けたの？",
              "maleVariant": "アニマルズのことだが… ボクサーになるために抜けたのか？",
              "stringId": "3226182641690476544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なければここに来てない",
              "maleVariant": "",
              "stringId": "3226190283074830340"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今は他のことで忙しい",
              "maleVariant": "",
              "stringId": "3226190606589886464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今は少し忙しくて――他の用事が終わったら、付き合ってもいい",
              "maleVariant": "今は少し忙しくてな――他の用事が終わったら、付き合ってもいい",
              "stringId": "3226195079815102464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早めに頼むぞ。次の試合の前に片づけておかないと、ボクシングに集中できない",
              "maleVariant": "",
              "stringId": "3226196428569051136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴィンス・マリーノだ。知ってるか？",
              "maleVariant": "",
              "stringId": "3226251462216384512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "金をもらってるとはいえ、負けるのは嫌か？",
              "maleVariant": "",
              "stringId": "3226417196666060800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "黙ってろ、ウィル",
              "maleVariant": "",
              "stringId": "3233435627854057472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アーロンね？",
              "maleVariant": "アーロンだな？",
              "stringId": "3234539921273606144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいフックだった",
              "maleVariant": "",
              "stringId": "3234541152587042816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも",
              "maleVariant": "",
              "stringId": "3234545901713575936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "次の試合の対戦相手は？",
              "maleVariant": "",
              "stringId": "3234685707714121728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おいおい、マジかよ。一緒にトレーニングしたことがある。他にあいつとスパーリングしたがる奴がいなくてな",
              "maleVariant": "",
              "stringId": "3234863183346094080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レイザー・ヒューズ",
              "maleVariant": "",
              "stringId": "3234865128110641152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ま、そのうち大物とやるつもりよ",
              "maleVariant": "ま、そのうち大物とやるつもりさ",
              "stringId": "3234872316745043968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。ダミールっていうスカベンジャー御用達のリパードクがいるんだが――",
              "maleVariant": "",
              "stringId": "3253598041815732224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "試合前に、クロームを強化してもらうのね",
              "maleVariant": "試合前に、クロームを強化してもらうのか",
              "stringId": "3254021308577828864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、フレッドの副業に付き合わされたのか。誰か有名な奴を倒したことは？",
              "maleVariant": "",
              "stringId": "3254021915124518912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウェインズのKO勝ち！ 強い、強すぎるー！",
              "maleVariant": "",
              "stringId": "3262228476993032192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK、手を貸す。まずは詳細を教えて",
              "maleVariant": "OK、手を貸す。まずは詳細を教えてくれ",
              "stringId": "3262340114182131712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあいい… 護衛を探してるんだ。危険な連中も関わってくる",
              "maleVariant": "",
              "stringId": "3262340644895805440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく肝の据わった人間が必要なんだが… 興味はあるか？",
              "maleVariant": "",
              "stringId": "3262418142195175424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で？ どうだ、V？ 手を貸してくれる気になったか？",
              "maleVariant": "",
              "stringId": "3262422884879257600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ",
              "maleVariant": "ああ",
              "stringId": "3262433439929159684"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ無理",
              "maleVariant": "まだ無理だ",
              "stringId": "3262433953966280704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ忙しくて。時間ができたら戻ってくる",
              "maleVariant": "まだ忙しくてな。時間ができたら戻ってくる",
              "stringId": "3262435021030772736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ、そうか。ま、俺も同じだ",
              "maleVariant": "",
              "stringId": "3263664424012304384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[支払う] わかった、アーロンに賭ける",
              "maleVariant": "",
              "stringId": "3266867896541642756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[支払う] 賭け金3倍でアーロンに賭ける",
              "maleVariant": "",
              "stringId": "3266868850947129344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おままごとね。胴元に100エディー支払って、勝ったときのあたしの儲けは？ 10？ それとも20？",
              "maleVariant": "おままごとだな。胴元に100エディー支払って、勝ったときの俺の儲けは？ 10？ それとも20か？",
              "stringId": "3266870368429539328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マジか… 誰か見つけたと言ってはいたが、てっきりウソだと思ってた",
              "maleVariant": "",
              "stringId": "3277974702182977536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはV。フレッドの使いよ",
              "maleVariant": "俺はV。フレッドの使いだ",
              "stringId": "3277987514020089856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "色々とめんどくさいんだ。向こうに着いたら詳細を話す",
              "maleVariant": "",
              "stringId": "3282721087620993024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やる気になったら、教えてくれ",
              "maleVariant": "",
              "stringId": "3283970410107195392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、問題ない。任せて",
              "maleVariant": "ああ、問題ない。任せろ",
              "stringId": "3283977771177824256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがたい。だがその前に、ちょっと片づけておきたいことがあってな。後でテラ・コグニータで落ち合おう",
              "maleVariant": "",
              "stringId": "3283978444783046656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんでそこまでそのダミールって奴にこだわるの？",
              "maleVariant": "なんでそこまでそのダミールって奴にこだわるんだ？",
              "stringId": "3284165837024743424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アニマルズ時代に出会った。当時スカベンジャーと手を組んでたんだ",
              "maleVariant": "",
              "stringId": "3284188618873458688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "次の試合は重要でな。リングに上がる前に、ダミールのクリニックに行きたい。そこでお前の出番だ",
              "maleVariant": "",
              "stringId": "3284194699188195328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スカベンジャーから守ってほしいってこと？",
              "maleVariant": "スカベンジャーから守ってほしいってことか？",
              "stringId": "3284195159504670720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "腕のいいリパーなら何人か知ってる。アポだってすぐに取れると思うけど",
              "maleVariant": "腕のいいリパーなら何人か知ってる。アポだってすぐに取れると思うが",
              "stringId": "3284197785491296256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スカベンジャーから守ってほしいってこと？ 一緒に仕事してたんでしょ？",
              "maleVariant": "スカベンジャーから守ってほしいってことか？ 一緒に仕事してたんだろ？",
              "stringId": "3284220554689990656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもダミールは信用できる。きっと手を貸してくれるはずだ。会えさえすればな",
              "maleVariant": "",
              "stringId": "3284221581589827584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最初はギャングの連中がコーチやインプラント代を払ってくれたりしてたんだ…",
              "maleVariant": "",
              "stringId": "3284223936590860288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが二足のわらじを履くのがしんどくなって… 俺はボクシングを選んだ。連中はそれが気に食わなくてな",
              "maleVariant": "",
              "stringId": "3284226296658612224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アニマルズがな。それに俺はもうとっくにギャングを抜けた",
              "maleVariant": "",
              "stringId": "3285270035502026752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、任せて",
              "maleVariant": "ああ、任せろ",
              "stringId": "3285272407733260292"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボクサーになるためにアニマルズを抜けたの？",
              "maleVariant": "ボクサーになるためにアニマルズを抜けたのか？",
              "stringId": "3285273305616306176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってるさ… でもダミールじゃなきゃダメなんだよ",
              "maleVariant": "",
              "stringId": "3288129727085498368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で… 協力してくれるのか？",
              "maleVariant": "",
              "stringId": "3288513566148481024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どっちがアーロン・ウェインズ？",
              "maleVariant": "どっちがアーロン・ウェインズだ？",
              "stringId": "3302656760779984900"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どっちがアーロン・ウェインズ？",
              "maleVariant": "どっちがアーロン・ウェインズだ？",
              "stringId": "3302657856785174528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ん？ アーロン？ オレンジのパンツの方だ",
              "maleVariant": "",
              "stringId": "3302659090682302464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "遠慮しとく",
              "maleVariant": "",
              "stringId": "3302662730734194688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。アーロンに賭ける",
              "maleVariant": "",
              "stringId": "3302662908455243776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いえ… 遠慮しとく",
              "maleVariant": "いや… 遠慮しとく",
              "stringId": "3302662938503237632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話にならない。アーロンに300賭けるから、勝ったらあんたはその倍支払うってのはどう？",
              "maleVariant": "話にならないな。アーロンに300賭けるから、勝ったらあんたはその倍支払うってのはどうだ？",
              "stringId": "3302664398775341056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チッ… まあいいだろう",
              "maleVariant": "",
              "stringId": "3302666801524330496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "試合前にクロームをアップグレードするのね",
              "maleVariant": "試合前にクロームをアップグレードするのか",
              "stringId": "3302702364893728768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞いたことない",
              "maleVariant": "",
              "stringId": "3302715715329806340"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴィクターから聞いたかもしれない",
              "maleVariant": "",
              "stringId": "3302716509798092800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "鉄拳ヴィンスでしょ？ 有名よ",
              "maleVariant": "鉄拳ヴィンスだろ？ 有名だ",
              "stringId": "3302716622574538752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いえ、聞いたことない",
              "maleVariant": "いや、聞いたことない",
              "stringId": "3302717539919155200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "名前くらいは聞いたことがある。友人に熱心なボクシングファンがいて、その人から",
              "maleVariant": "名前くらいは聞いたことがある。友人に熱心なボクシングファンがいて、そいつから",
              "stringId": "3302717965926223872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "鉄拳ヴィンスでしょ？ 誰でも知ってる",
              "maleVariant": "鉄拳ヴィンスだろ？ 誰でも知ってる",
              "stringId": "3302718383360135168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうか。ヴィンスは元ヘビー級のチャンプだ。テクニックもあるし、間合いの取り方が抜群にうまい",
              "maleVariant": "",
              "stringId": "3302720294905794560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全盛期は過ぎたが、パンチ力は健在――俺みたいなスタイルのボクサーにとっては相性が悪すぎる",
              "maleVariant": "",
              "stringId": "3302721000823934976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘイウッドのギャングからヘビー級のチャンピオンにまで成り上がった努力の男",
              "maleVariant": "",
              "stringId": "3302722580583370752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴァレンティーノズの英雄… 全部デタラメだけどね",
              "maleVariant": "ヴァレンティーノズの英雄… 全部デタラメだけどな",
              "stringId": "3302723195367673856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴は死体を踏み台に上り詰めた――ギャングが邪魔になる相手を殺してたのよ",
              "maleVariant": "奴は死体を踏み台に上り詰めた――ギャングが邪魔になる相手を殺してたのさ",
              "stringId": "3302723960912039936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ… なんだ、俺より詳しいんだな",
              "maleVariant": "",
              "stringId": "3302725048578953216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え？",
              "maleVariant": "",
              "stringId": "3302727654298701824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何度かボクサーを相手にしたことがあるから。正規の試合じゃないけど",
              "maleVariant": "何度かボクサーを相手にしたことがあるからな。正規の試合じゃないが",
              "stringId": "3302730121790967808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "結局、わだかまりはずっと残ったままだ",
              "maleVariant": "",
              "stringId": "3302739817679638528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウィル・コリーさ。生まれも育ちもドッグタウンだ",
              "maleVariant": "",
              "stringId": "3302880664035348480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おーい、賭けてない奴はいるか？ そろそろ締め切るぞー！",
              "maleVariant": "",
              "stringId": "3302881394498555904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "対戦相手は…？",
              "maleVariant": "",
              "stringId": "3327601033572864000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "賭け金は100。参加するなら最後のチャンスだぜ",
              "maleVariant": "",
              "stringId": "3327601547895197696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[支払う] ウィルに賭ける",
              "maleVariant": "",
              "stringId": "3327601956453961728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウィルに賭けてみる",
              "maleVariant": "",
              "stringId": "3327605836168384580"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、25だ",
              "maleVariant": "",
              "stringId": "3327615772340342784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜそう思う？",
              "maleVariant": "",
              "stringId": "3357914926975111168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座って待つ]",
              "maleVariant": "",
              "stringId": "3834945801016033284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "3872625318380777484"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}