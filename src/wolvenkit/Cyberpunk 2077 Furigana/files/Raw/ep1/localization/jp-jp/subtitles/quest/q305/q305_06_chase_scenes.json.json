  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "深淵を覗く時、深淵もまたこちらを覗いている、か",
              "maleVariant": "",
              "stringId": "3052356606504079360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だがその視線がいけ好かねえ。まるで、後戻りはできないぞって言ってるようだ",
              "maleVariant": "",
              "stringId": "3052361247165911040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "含蓄に富んだ忠告はそれだけ？",
              "maleVariant": "含蓄に富んだ忠告はそれだけか？",
              "stringId": "3052364382492037120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここまで来たらやるしかない",
              "maleVariant": "",
              "stringId": "3052364577007079424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは悲観的なことしか言えないの？ 「試練が人を強くする」とか言ったらどう？",
              "maleVariant": "お前は悲観的なことしか言えないのか？ 「試練が人を強くする」とか言ったらどうだ？",
              "stringId": "3052370327548858368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、他に選択肢はない",
              "maleVariant": "ああ、他に選択肢はない",
              "stringId": "3052370796404936704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、選択肢はいつだってある。ここで全部捨てて逃げ出したっていいんだぜ",
              "maleVariant": "",
              "stringId": "3052371533176377344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ならこの格言はどうだ？ 「怪物に挑む者は、自身が怪物にならぬよう気をつけよ」",
              "maleVariant": "",
              "stringId": "3052372509560016896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "壁に大穴があいてる。きっとトラックで突き破ったのね",
              "maleVariant": "壁に大穴があいてる。きっとトラックで突き破ったんだ",
              "stringId": "3090958016942313472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "慎重にな",
              "maleVariant": "",
              "stringId": "3090959520516411392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミの状態は？",
              "maleVariant": "",
              "stringId": "3090969070074204160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "中にはいない。トラックは空よ",
              "maleVariant": "中にはいない。トラックは空だ",
              "stringId": "3090972327957893120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マックス・タックの隊員がいる… 死体だけどね。丸焦げよ",
              "maleVariant": "マックス・タックの隊員がいる… 死体だけどな。丸焦げだ",
              "stringId": "3090977517989662720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっとソミの仕業だろう",
              "maleVariant": "",
              "stringId": "3090978017648709632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミが通ったゲートの前にいるけど、閉じられてる",
              "maleVariant": "ソミが通ったゲートの前にいるが、閉じられてる",
              "stringId": "3090979529410088960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他の入口があるはずだ。辺りを探せ",
              "maleVariant": "",
              "stringId": "3090980365368430592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミの姿は？ 無事なのか？",
              "maleVariant": "",
              "stringId": "3094183806299729920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ見当たらない。辺りを調べてみる",
              "maleVariant": "",
              "stringId": "3094184012558823424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この有り様で無事だったとしたら、奇跡ね…",
              "maleVariant": "この有り様で無事だったとしたら、奇跡だな…",
              "stringId": "3094187498209996800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ… 助け出せ、早く",
              "maleVariant": "",
              "stringId": "3094188442163277824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "完全に暴走してるってこと？ どう思う？",
              "maleVariant": "完全に暴走してるってことか？ どう思う？",
              "stringId": "3094196179211317248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあな、わからん",
              "maleVariant": "",
              "stringId": "3094196456219930624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "痕跡を見失うな。俺もできるだけ早くそっちに行く",
              "maleVariant": "",
              "stringId": "3094197378715152384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本気でこの先に行くのか？",
              "maleVariant": "",
              "stringId": "3101095452372824064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、それってあたしのこと？",
              "maleVariant": "なあ、それって俺のことか？",
              "stringId": "3101101049956724736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソングバードに決まってんだろ。一から百まで全部解説してやらないとダメか？",
              "maleVariant": "",
              "stringId": "3101101128239214592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブラックウォールの悪魔に魂を売っちまった女が、この先お前を待ち構えてる。問題は、あの女があの女のままなのかってことだ",
              "maleVariant": "",
              "stringId": "3101101540019204096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "答えはすぐにわかる",
              "maleVariant": "",
              "stringId": "3101102449075875840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今さら無理よ。ソミを助けないと、リードから助けを得られない。このチャンスは逃せないの",
              "maleVariant": "今さら無理だ。ソミを助けないと、リードから助けを得られない。このチャンスは逃せない",
              "stringId": "3101105613812461568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってるさ。だが問題は、この先にいるソングバードが、俺たちの知るあの女なのかってことだ",
              "maleVariant": "",
              "stringId": "3101106655476248576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはすぐにわかる",
              "maleVariant": "",
              "stringId": "3101107713665593344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って。トラックよ。まずい状況ね",
              "maleVariant": "待て。トラックだ。まずい状況だな",
              "stringId": "3121326649065213976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "トラックを見つけた。まずい状況ね",
              "maleVariant": "トラックを見つけた。まずい状況だ",
              "stringId": "3121331346148823052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ショートしてる。まだ時間は経ってないみたいね",
              "maleVariant": "ショートしてる。まだ時間は経ってないみたいだな",
              "stringId": "3153640544756699136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゲートが開かない。ついさっきコンソールが破壊されたみたい。ソミでしょうね…",
              "maleVariant": "ゲートが開かない。ついさっきコンソールが破壊されたみたいだ。ソミだろうな…",
              "stringId": "3153640629213204480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フム… ウィンチは動きそうにないね。ってことは、ソミは… 跳び下りたの？",
              "maleVariant": "フム… ウィンチは動きそうにないな。ってことは、ソミは… 跳び下りたのか？",
              "stringId": "3153640701288124416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このオーラは… デジタルの… 残留物？ なんだか… 不気味な痕跡ね…",
              "maleVariant": "このオーラは… デジタルの… 残留物か？ なんだか… 不気味な痕跡だ…",
              "stringId": "3153640846058721280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いろんな機材がハックされてる。アクセスルートを探したのね。でも、何のため？",
              "maleVariant": "いろんな機材がハックされてる。アクセスルートを探したんだな。だが、何のために？",
              "stringId": "3153641174590164992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ひとつ残らず、回路がやられてる",
              "maleVariant": "",
              "stringId": "3153645663015538688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "辺りの機材を破壊しながら進んでる。嫌な感じね… もう自分を… 制御できていないのかも…",
              "maleVariant": "辺りの機材を破壊しながら進んでる。嫌な感じだな… もう自分を… 制御できていないのかも…",
              "stringId": "3153647283342282752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミだ。まだ近くにいるぞ",
              "maleVariant": "",
              "stringId": "3154664084734144512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうことだ？",
              "maleVariant": "",
              "stringId": "3154666589136285696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "恐らく破壊工作でしょうね。ソミがシステムをハックしたのよ",
              "maleVariant": "恐らく破壊工作だろうな。ソミがシステムをハックしたんだ",
              "stringId": "3154666778098069504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこにいったんだ… なんとか探してくれ。手遅れになる前に",
              "maleVariant": "",
              "stringId": "3154671422316670976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあな… ただ状況が悪化してるのは間違いない",
              "maleVariant": "",
              "stringId": "3154673801309122560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミではなくなっているということだな。すぐに助けてやらないと。頼む、急いでくれ、V",
              "maleVariant": "",
              "stringId": "3154675513524678656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "様子がおかしい。ゲートが壊れてる…",
              "maleVariant": "",
              "stringId": "3192836691094016004"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[中を調べる]",
              "maleVariant": "",
              "stringId": "3300255479715729408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コンソールが壊れてる…",
              "maleVariant": "",
              "stringId": "3302826505490272260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "回路がやられてる。ブラックウォール？",
              "maleVariant": "回路がやられてる。ブラックウォールか？",
              "stringId": "3302826914468466688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またゲートがふさがれてる",
              "maleVariant": "",
              "stringId": "3302826943157506048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウィンチ…？",
              "maleVariant": "",
              "stringId": "3302826981090791424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいか、V。ソミが生きてる限り、事態を解決できる可能性はある",
              "maleVariant": "",
              "stringId": "3305866194103177224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつは危険な状態だ",
              "maleVariant": "",
              "stringId": "3305872791525265408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オーバーロードさせたのね…",
              "maleVariant": "オーバーロードさせたんだな…",
              "stringId": "3305874520165715968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今もブラックウォールに接続してる。乗っ取られるのも時間の問題だ",
              "maleVariant": "",
              "stringId": "3305881413605003264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気をつけろよ",
              "maleVariant": "",
              "stringId": "3305881846641725440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": "3305882725398745092"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そっちもね",
              "maleVariant": "そっちもな",
              "stringId": "3305882995495145472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": "3305883505673506816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、そっちも",
              "maleVariant": "ああ、そっちも",
              "stringId": "3305885714712760320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は平気だ。手当てが済んだら合流する。先に行け",
              "maleVariant": "",
              "stringId": "3305888478373220352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手当てをしたら、すぐにそちらに向かう。報告は怠るなよ",
              "maleVariant": "",
              "stringId": "3305889063176638464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "生きたまま連れ戻してくれ。でなければ、あいつも、お前も助けられない",
              "maleVariant": "",
              "stringId": "3306968481051578380"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってる",
              "maleVariant": "",
              "stringId": "3306968481051578396"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミとのリンクのおかげで可視化されてるんだろう。その痕跡を辿ればよさそうだな",
              "maleVariant": "",
              "stringId": "3306970427946831896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リード？",
              "maleVariant": "",
              "stringId": "3306972417741426708"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "面倒ね。こうなったら…",
              "maleVariant": "面倒だな。こうなったら…",
              "stringId": "3306999023369326592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "急げ！ ソミを見失うぞ！",
              "maleVariant": "",
              "stringId": "3317132002523475968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いったい何をしてる、V？ ここまで来て俺を見捨てる気か！？",
              "maleVariant": "",
              "stringId": "3317349648414142464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっと、ブラックウォール絡みよ。ソミは、扉を開けてしまったみたい",
              "maleVariant": "きっと、ブラックウォール絡みだな。ソミは、扉を開けてしまったみたいだ",
              "stringId": "3327149370163523584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブラックウォールの魔術を使ったか",
              "maleVariant": "",
              "stringId": "3327153760710619136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケガをしてるのか、怯えてるのかもわからない。V、とにかく見つけてくれ",
              "maleVariant": "",
              "stringId": "3344480953077788672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこに目をつけてんだ！",
              "maleVariant": "",
              "stringId": "3349170526520995840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ。今日はこの後始末に追われそうだ",
              "maleVariant": "",
              "stringId": "3349170829416853504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、なんだか… フラフラする…",
              "maleVariant": "",
              "stringId": "3349175191778226176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこで運転、覚えたんだ？ 道、2台しかいなかった。2台だ！ なのに突っ込んでくる奴あるか！？",
              "maleVariant": "",
              "stringId": "3349175507760312320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見てなかったの？ トラックが突っ込んできたんだよ！ あたしにどうしろってのさ！？",
              "maleVariant": "",
              "stringId": "3349177547249020928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マックス・タックめ。サイコスクワッドってのは自分たちがサイコ野郎って意味かよ",
              "maleVariant": "",
              "stringId": "3349177755353608192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別にいいじゃねえか。時給さえもらえりゃ、俺はなんでもいい。それよりマックス・タックを見たか？ すごかったな",
              "maleVariant": "",
              "stringId": "3349178423405572096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信じられない、市民を守るのがあいつらの仕事なんじゃないの！？",
              "maleVariant": "",
              "stringId": "3349179930855206912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本気で言ってんのか？ こっちは停まってたのに、マックス・タックのトラックに押されたんだ！",
              "maleVariant": "",
              "stringId": "3349180432158420992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなこと俺、知るわけないだろ！ だったらお前追いかけて、あいつから保険の情報をもらってこい。クソッ！",
              "maleVariant": "",
              "stringId": "3349180595182628864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうだ？",
              "maleVariant": "",
              "stringId": "3359155322590289920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うう、吐きそう…",
              "maleVariant": "",
              "stringId": "3360431592672784384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うう… 脇腹が… すごく痛い…",
              "maleVariant": "",
              "stringId": "3360432794340880384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここがドッグタウンだからって、人をゴミみたいに跳ね飛ばしてもいいと思ってる！",
              "maleVariant": "",
              "stringId": "3360432894198870016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "壁に突っ込んでったぞ。まるで段ボールみたいに突き破ったな",
              "maleVariant": "",
              "stringId": "3404283816204693504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのトラック、乗っ取られてたのか…？",
              "maleVariant": "",
              "stringId": "3404296128063991808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブラックウォールの痕跡がある。トラックにソミはいない。後を追ってみる",
              "maleVariant": "",
