    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パトカーはその先にある…",
              "maleVariant": "",
              "stringId": "3213064884257333248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドジャーだ",
              "maleVariant": "",
              "stringId": "3213376550824091648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マジかよ…",
              "maleVariant": "",
              "stringId": "3213385687880474624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしたんだ？ 慌ててるようだが、タイミングが悪かったか？",
              "maleVariant": "",
              "stringId": "3213404385131405312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゴホン。実は… あんたのヤクを盗んだ奴を捕まえて、ここに連れてきたんだ。ただ、困ったことにそいつが… ブツを食っちまって…",
              "maleVariant": "",
              "stringId": "3214262496125820928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2人はあんたに殺されると思ってたのよ",
              "maleVariant": "2人はあんたに殺されると思ってたんだ",
              "stringId": "3214288691483680768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしも驚いてる",
              "maleVariant": "俺も驚いてる",
              "stringId": "3214289984184950784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたに消されると思ったのよ",
              "maleVariant": "あんたに消されると思ったんだよ",
              "stringId": "3214291830165250048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "驚いてるのはあたしも同じ。この2人に泣きつかれて、話を聞いたらこれだもの",
              "maleVariant": "驚いてるのは俺も同じさ。この2人に泣きつかれて、話を聞いたらこれだからな",
              "stringId": "3214292035501596672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "反応ありません…  ボス",
              "maleVariant": "",
              "stringId": "3221492082622373888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいだろう、俺の気が変わる前にそいつらを連れて出てけ。だが、これで終わったと思うなよ",
              "maleVariant": "",
              "stringId": "3221569799854268416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドジャーは死んだ",
              "maleVariant": "",
              "stringId": "3242016770867896320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしもし？ お巡りさん？",
              "maleVariant": "",
              "stringId": "3242018047328821248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "終わったのか？",
              "maleVariant": "",
              "stringId": "3242018402066276352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう",
              "maleVariant": "",
              "stringId": "3242021254931193856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おーい、お巡りさんはいますか？",
              "maleVariant": "",
              "stringId": "3242075503153160192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "出てきても平気よ。ドジャーは死んだ",
              "maleVariant": "出てきてもいいぞ。ドジャーは死んだ",
              "stringId": "3242076178184445952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "異常なし。全員無事です",
              "maleVariant": "",
              "stringId": "3245182669410050048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまりこういうことか… お前らの目の前で盗人野郎が過剰摂取し、慌てて掻っ捌いてみたものの、俺の魔法の粉はきれいに溶けた後だった…",
              "maleVariant": "",
              "stringId": "3245184925794607104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "前言撤回だ。帰すわけにはいかなくなった",
              "maleVariant": "",
              "stringId": "3245195064870215680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "は… えっと… 話を聞いてくれ",
              "maleVariant": "",
              "stringId": "3281406365718122496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "黙れ。知らん顔だな… いったい何者だ？",
              "maleVariant": "",
              "stringId": "3281408225069862912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2人に雇われた用心棒よ",
              "maleVariant": "2人に雇われた用心棒だ",
              "stringId": "3281409636486713348"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "説明しようにも、言葉が見つからない",
              "maleVariant": "",
              "stringId": "3281409795635384320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この2人に雇われた者よ",
              "maleVariant": "この2人に雇われた者だ",
              "stringId": "3281410429461827584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "説明しようにも、言葉が見つからない",
              "maleVariant": "",
              "stringId": "3281410575004176384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただ、ひとつわからんのは…",
              "maleVariant": "",
              "stringId": "3281411147409231872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前らの顔についてるのは何だ？ ケチャップ？ それともあの日か？",
              "maleVariant": "",
              "stringId": "3281412367632928768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー… まあ、その… 俺たちのではないというか… なんというか",
              "maleVariant": "",
              "stringId": "3281412554900213760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かに、自分たちのケツも拭けなさそうではあるな",
              "maleVariant": "",
              "stringId": "3281442307044642816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直だな。気に入った",
              "maleVariant": "",
              "stringId": "3281442918121181184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まともに喋れる奴はいないのか？",
              "maleVariant": "",
              "stringId": "3281449208134340608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当の話よ",
              "maleVariant": "本当の話だ",
              "stringId": "3281449821576466436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でまかせよ",
              "maleVariant": "でまかせだ",
              "stringId": "3281450064527331328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "答えるまでもないでしょ",
              "maleVariant": "答えるまでもないだろ",
              "stringId": "3281450314105196544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全部本当よ。ビルが言ったとおり",
              "maleVariant": "全部本当さ。ビルが言ったとおりだ",
              "stringId": "3281450588177797120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その… 銃撃戦があって… そこで撃たれた奴の血がついて… その後、あんたのブツを届けに来て… 上の階に置いてきた… 何も問題ない…",
              "maleVariant": "",
              "stringId": "3281464832906596352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー、もういい。今度はお前の番だ… 今のは作り話か？",
              "maleVariant": "",
              "stringId": "3281465633901219840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "動くな、じっとしてろ",
              "maleVariant": "",
              "stringId": "3281467233155780608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[武器を抜く] 茶番は終わりよ",
              "maleVariant": "[武器を抜く] 茶番は終わりだ",
              "stringId": "3281474125068341248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは… 残念だな",
              "maleVariant": "",
              "stringId": "3281475582857416704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだな、その通りだ",
              "maleVariant": "",
              "stringId": "3281477120673812480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チャーリー、ビル。正直に話して",
              "maleVariant": "チャーリー、ビル。正直に話せ",
              "stringId": "3281477194661335040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺はバカにされるのが何よりも嫌いでね。なあ、お前ら？",
              "maleVariant": "",
              "stringId": "3281480519402229760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "茶番ね。話し合いは終わりにして、さっさと殺し合わない？",
              "maleVariant": "茶番だな。話し合いは終わりにして、さっさと殺し合わないか？",
              "stringId": "3281481610357477376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その言葉を待ってたぜ",
              "maleVariant": "",
              "stringId": "3281486903065808896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "食っただと…？",
              "maleVariant": "",
              "stringId": "3281494366779445248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "丸飲みだ",
              "maleVariant": "",
              "stringId": "3281494769080307712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7オンス分を？ お前らの前で？",
              "maleVariant": "",
              "stringId": "3281494994834526208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一瞬のことで止められなかった。取り出そうとしてはみたが…",
              "maleVariant": "",
              "stringId": "3281495299693318144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ？ 取り出す？",
              "maleVariant": "",
              "stringId": "3281495829534576640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなところだ…",
              "maleVariant": "",
              "stringId": "3282280347321339904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハハハ！ ぶっ飛びすぎだろ！ とんでもないサツだな… さすがは市民の味方！",
              "maleVariant": "",
              "stringId": "3282280567757180928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハハハ！",
              "maleVariant": "",
              "stringId": "3282283058234232832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんな間抜けどもに銃を持たせて、ナイトシティは何がしたいんだ？ ハハ、こいつはいい土産話ができたぜ…",
              "maleVariant": "",
              "stringId": "3282284976926986240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カメラを回しとくべきでしたね",
              "maleVariant": "",
              "stringId": "3282285222880972800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もういい？",
              "maleVariant": "もういいか？",
              "stringId": "3282288562402594820"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2人は連れて帰る",
              "maleVariant": "",
              "stringId": "3282288819731533824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ… もういい？",
              "maleVariant": "じゃあ… もういいか？",
              "stringId": "3282300601397252096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2人を連れて帰る。手出し無用よ",
              "maleVariant": "2人を連れて帰る。手出し無用だ",
              "stringId": "3282300719206862848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それに、こんな話が聞けるなら、7オンスぐらいいつでもくれてやる。2人とも帰っていいぞ",
              "maleVariant": "",
              "stringId": "3282300816313389056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボス、上にいる仲間たちは？",
              "maleVariant": "",
              "stringId": "3282301439989616640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、そうだな。確認しておくか。生体モニターをチェックしろ",
              "maleVariant": "",
              "stringId": "3282302154363473920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ビル、チャーリー… 車に乗って",
              "maleVariant": "ビル、チャーリー… 車に乗るんだ",
              "stringId": "3282313784094724096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行け！",
              "maleVariant": "",
              "stringId": "3282313910511046656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おいおい、俺が警官を襲うとでも？ しかも相手は2人だ。残念ながら、そんな度胸はないさ",
              "maleVariant": "",
              "stringId": "3282346336893374464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。とりあえずはな",
              "maleVariant": "",
              "stringId": "3282350273901989888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー… 後ろにいるよ！",
              "maleVariant": "",
              "stringId": "3298466195733581824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。さっさと車に乗って逃げて。それくらいはできるでしょ",
              "maleVariant": "ああ。さっさと車に乗って逃げるんだな。それくらいはできるだろ",
              "stringId": "3298466671837417472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、早く行くぞ",
              "maleVariant": "",
              "stringId": "3298468922467389440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってるって。ありがとな、ホントに",
              "maleVariant": "",
              "stringId": "3298469043733106688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "と、取り出そうとはしたんだ。そいつの… その、中から… 体内からな",
              "maleVariant": "",
              "stringId": "3308627796851789824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "体内…？ 一体どういう… その血はそれで…？",
              "maleVariant": "",
              "stringId": "3308628411535429632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頑張ってはみたんだが、全部、その、体内で溶けちまってて…",
              "maleVariant": "",
              "stringId": "3308628647037210624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、どう思うよ？",
              "maleVariant": "",
              "stringId": "3308629074134798336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "野蛮ですね",
              "maleVariant": "",
              "stringId": "3308629267844534272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど… で、お前はこれにどう関わってるんだ？",
              "maleVariant": "",
              "stringId": "3308629350120001536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしを見ないで",
              "maleVariant": "俺を見ないでくれ",
              "stringId": "3308634436602793988"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー…",
              "maleVariant": "",
              "stringId": "3308637805987860480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはその場にいなかったから",
              "maleVariant": "俺はその場にいなかったんでね",
              "stringId": "3308639702568243200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺はむしろいたかったぞ。まあいい。それで？ 何が起きたんだ？ 包み隠さず、すべて話せ",
              "maleVariant": "",
              "stringId": "3308654239556145152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "挙げ句の果てに、ドタマをぶち抜かれるのが怖くて、傭兵にボディガードを頼んだと？",
              "maleVariant": "",
              "stringId": "3308707960856088576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと待って…",
              "maleVariant": "ちょっと待ってくれ…",
              "stringId": "3312967797165101060"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着いて… 仲間に最高の笑い話を聞かせてやるんでしょ… あたしたちも、これ以上の面倒ごとを起こすつもりはない",
              "maleVariant": "落ち着いてくれ… 仲間に最高の笑い話を聞かせてやるんだろ… 俺たちも、これ以上の面倒ごとを起こすつもりはない",
              "stringId": "3312972843969777664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前は俺の仲間を殺した",
              "maleVariant": "",
              "stringId": "3312984628588285952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはプロよ。しかも手練れのね。武器とクロームを見ればわかるでしょ。あたしがあんたなら、うかつなことはしないけど…",
              "maleVariant": "俺はプロだ。しかも手練れのな。武器とクロームを見ればわかるだろ。俺があんたなら、うかつなことはしないが…",
              "stringId": "3312984851842699264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい… それは脅迫のつもりか？ 俺がお前にビビるとでも？",
              "maleVariant": "",
              "stringId": "3312994896965898240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく… あたしたちはこれから、ゆっくり、静かにここを去る。誰もバカな真似をする必要はない、そうでしょ？",
              "maleVariant": "とにかく… 俺たちはこれから、ゆっくり、静かにここを去る。誰もバカな真似をする必要はない、そうだろ？",
              "stringId": "3312995001034969088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボス…？",
              "maleVariant": "",
              "stringId": "3313001171107811328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前、持ってるよな？",
              "maleVariant": "",
              "stringId": "3315951007780147200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "キーか？ クソ、待てよ…",
              "maleVariant": "",
              "stringId": "3315953739110912000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、まさか落としてきたとか言うなよ！？",
              "maleVariant": "",
              "stringId": "3315954595269021696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着け、チューマ。あったよ",
              "maleVariant": "",
              "stringId": "3315959996643381248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これはこれは… わざわざお出迎えとは、気が利くこった",
              "maleVariant": "",
              "stringId": "3315978127008960512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行きます、すぐ行きます…",
              "maleVariant": "",
