      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "包帯の様子は？ 血は止まったかね？ 縫合はほどけてないか？",
              "maleVariant": "",
              "stringId": "3263570906553819136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死ぬほど痛え… 立つのもしんどいんだ",
              "maleVariant": "",
              "stringId": "3263575704233615360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふむ… モルヒネの量を増やすことはできない。他の患者の分がなくなってしまう",
              "maleVariant": "",
              "stringId": "3263576863925116928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうせアンダーソンがいなければ、俺らは全員終わりだ",
              "maleVariant": "",
              "stringId": "3263579228338167808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フィクサーに依頼されて来た",
              "maleVariant": "",
              "stringId": "3263580475606736896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オデル？ Vよ、手助けが必要だって聞いたんだけど",
              "maleVariant": "オデルか？ Vだ、手助けが必要だって聞いたんだが",
              "stringId": "3263702013668532224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "必要なのは奇跡だ。スカベンジャーの連中をどうにかしないと、患者たちが死んでしまう",
              "maleVariant": "",
              "stringId": "3263702476837134336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンダーソンはオフィスにいるの？",
              "maleVariant": "アンダーソンはオフィスにいるのか？",
              "stringId": "3263711031489241088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜバーゲストを呼ばないの？",
              "maleVariant": "なぜバーゲストを呼ばないんだ？",
              "stringId": "3263711158090113028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。唯一セキュリティがある場所だ",
              "maleVariant": "",
              "stringId": "3263716102469885952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンダーソンはどこ？",
              "maleVariant": "アンダーソンはどこだ？",
              "stringId": "3263720045216309252"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正面を封鎖する様子は聞こえたが――連中は窓を割って入ってきた… まだそこから入れるかもしれない",
              "maleVariant": "",
              "stringId": "3263721482939834368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "脚の調子は？ よくなったかい？",
              "maleVariant": "",
              "stringId": "3263724214572589056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。膝も曲げられるようになった！",
              "maleVariant": "",
              "stringId": "3263727396774256640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンダーソンは死んだ",
              "maleVariant": "",
              "stringId": "3273815406479974404"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、そうか。この者たちにどう… 何を伝えればいい？",
              "maleVariant": "",
              "stringId": "3273815770797219840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンダーソンは生きてる",
              "maleVariant": "",
              "stringId": "3273817995288289284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おお… 神に、そして君に感謝する",
              "maleVariant": "",
              "stringId": "3273818783280570368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう中に戻っても？",
              "maleVariant": "",
              "stringId": "3273820301031415808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最新のクロームは無理だが、それなりのもので良ければアンソニーが良心的な価格で売ってくれる",
              "maleVariant": "",
              "stringId": "3275504911691849728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここで何をしてるの？",
              "maleVariant": "ここで何をしてるんだ？",
              "stringId": "3276586928374325248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それ、女性用よ。あんたの体重に合ってない",
              "maleVariant": "それ、女性用だぞ。あんたの体重に合ってない",
              "stringId": "3276604940141764608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "荷重設定を調整したほうがいい",
              "maleVariant": "",
              "stringId": "3276605308183552004"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "荷重設定を調整してやればいい。まずは膝のジョイントから様子を見てみるのね",
              "maleVariant": "荷重設定を調整してやればいい。まずは膝のジョイントから様子を見てみるんだな",
              "stringId": "3276612610584256512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほう… それは試していなかった",
              "maleVariant": "",
              "stringId": "3276613595356504064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "外に追い出されたの？",
              "maleVariant": "外に追い出されたのか？",
              "stringId": "3276635499521941504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ピクニックをしているように見えるかね？",
              "maleVariant": "",
              "stringId": "3276635862681559040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここは教会だったの？",
              "maleVariant": "ここは教会だったのか？",
              "stringId": "3276638652715802624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはここで何をしてるの？",
              "maleVariant": "あんたはここで何をしてるんだ？",
              "stringId": "3276643337635930112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スカベンジャーとアンダーソンの間に何があったの？",
              "maleVariant": "スカベンジャーとアンダーソンの間に何があったんだ？",
              "stringId": "3276643935391358976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スカベンジャーとアンダーソンの関係は？",
              "maleVariant": "",
              "stringId": "3276647894898221056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クリニックの患者なの？",
              "maleVariant": "クリニックの患者なのか？",
              "stringId": "3276648519866294272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあな。とにかく、ニカはアンソニーに… 怒っているようだった",
              "maleVariant": "",
              "stringId": "3276648858615062528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは、アンダーソンの右腕ってところ？",
              "maleVariant": "あんたは、アンダーソンの右腕ってところか？",
              "stringId": "3276650646294536192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "医者ではない。私は… 精神的な安らぎを提供している",
              "maleVariant": "",
              "stringId": "3276651379643424768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうやって中に入ればいい？",
              "maleVariant": "",
              "stringId": "3276654866989096960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スカベンジャーたちがインプラントを奪ってないとは…",
              "maleVariant": "",
              "stringId": "3278178828759900160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "金やインプラントが目的じゃないみたいね",
              "maleVariant": "金やインプラントが目的じゃないみたいだな",
              "stringId": "3278182225575714816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあどうやって経営してるの？",
              "maleVariant": "じゃあどうやって経営してるんだ？",
              "stringId": "3278182543000641536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここにはそのどちらもない",
              "maleVariant": "",
              "stringId": "3278184048084688896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クリニックにいるはずだと聞いてたんだけど",
              "maleVariant": "クリニックにいるはずだと聞いてたんだが",
              "stringId": "3308576032478896128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、ハンズの使いか――確かVだったな",
              "maleVariant": "",
              "stringId": "3308576560759873536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "教会の中にクリニックが？",
              "maleVariant": "",
              "stringId": "3308585170709684224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "好きでこれをつけてるわけじゃねえよ",
              "maleVariant": "",
              "stringId": "3308633072194723840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スカベンジャーたちが、ベネットに賄賂を渡したんだろう。ここら辺を担当している、ハンセンの副官だ",
              "maleVariant": "",
              "stringId": "3308650280316739584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バーゲストの給料はそんな低いの？",
              "maleVariant": "バーゲストの給料はそんな低いのか？",
              "stringId": "3308652858639294464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「賄賂は賢人の目をくらます」――ベネットは賢人ですらないが",
              "maleVariant": "",
              "stringId": "3308653334827016192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ… 連中の一人… ニカ・ヤンコビッチという女が、弟を探しているようだ",
              "maleVariant": "",
              "stringId": "3308671470410309632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "安らぎね…",
              "maleVariant": "",
              "stringId": "3308673244265357312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。しかし、今は赦しを求める者より、輸血やインプラントを求める者の方が多い",
              "maleVariant": "",
              "stringId": "3308676206517723136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも人は助けてる",
              "maleVariant": "だが人は助けてる",
              "stringId": "3308678697833635840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "懺悔する場所がなければ、罪も語れまい",
              "maleVariant": "",
              "stringId": "3308679673093201920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "使える出入口は？",
              "maleVariant": "",
              "stringId": "3308696190446903296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“寄付”のおかげさ",
              "maleVariant": "",
              "stringId": "3308730135284719616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしアンソニーに何かあったら… ここの人たちに未来はない",
              "maleVariant": "",
              "stringId": "3308803609994895360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここは大丈夫そう？",
              "maleVariant": "ここは大丈夫そうか？",
              "stringId": "3308806279635836928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やるだけやってみる",
              "maleVariant": "",
              "stringId": "3308832120860221444"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "任せて",
              "maleVariant": "任せとけ",
              "stringId": "3308832296349900800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ったく… 悲観的な奴ね",
              "maleVariant": "ったく… 悲観的な奴だな",
              "stringId": "3308832343560986628"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着いて",
              "maleVariant": "落ち着け",
              "stringId": "3308832520225071104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪い知らせがある",
              "maleVariant": "",
              "stringId": "3308849537925685248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真実を伝えればいい",
              "maleVariant": "",
              "stringId": "3308856185293545476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたがいると伝えればいい",
              "maleVariant": "",
              "stringId": "3308856381754744832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウソをついてもしょうがない。彼らには真実を知る権利がある",
              "maleVariant": "",
              "stringId": "3308857395048259584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "絶望するなと伝えるのよ。だって、あんたがいるでしょ",
              "maleVariant": "絶望するなと伝えるんだ。だって、あんたがいるだろ",
              "stringId": "3308857588607000580"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で… 中に戻ってもいいのか？",
              "maleVariant": "",
              "stringId": "3308879054601887744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "薬も注射器も酸素も… 色々と足りてない。すべて中に残ったままだ",
              "maleVariant": "",
              "stringId": "3308890205410729984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここは大丈夫そう？",
              "maleVariant": "ここは大丈夫そうか？",
              "stringId": "3308891636926685184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンダーソンは無事よ。少し… 酔ってるけど",
              "maleVariant": "アンダーソンは無事だ。少し… 酔ってるがな",
              "stringId": "3308895919864717312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンダーソンがどうやってインプラントを入手してるか知ってたの？",
              "maleVariant": "アンダーソンがどうやってインプラントを入手してるか知ってたのか？",
              "stringId": "3314415323582996480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンダーソンはいつから神の真似事をしてたわけ？",
              "maleVariant": "アンダーソンはいつから神の真似事をしてたんだ？",
              "stringId": "3314416017941635072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え… ど、どういう意味だ？",
              "maleVariant": "",
              "stringId": "3314416830294433792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "患者の死体からクロームをはぎ取ってたんでしょ",
              "maleVariant": "患者の死体からクロームをはぎ取ってたんだろ",
              "stringId": "3314417260982345728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "な… アンソニーはどこだ？",
              "maleVariant": "",
              "stringId": "3314417459574251520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。ニカはもうあんたらの邪魔をしない",
              "maleVariant": "ああ。ニカはもうあんたらの邪魔をしない",
              "stringId": "3314420394278428672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたなら、アンダーソンよりうまくやってけるかもしれない",
              "maleVariant": "あんたなら、アンダーソンよりうまくやっていけるかもしれない",
              "stringId": "3325870296019673088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スカベンジャーはあんたを見逃したの？",
              "maleVariant": "スカベンジャーはあんたを見逃したのか？",
              "stringId": "3334627529234751488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "患者たちは、恐れている。アンダーソンに何かあれば、居場所を失う",
              "maleVariant": "",
              "stringId": "3334648968721637376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜ連中はあんたを見逃してるの？",
              "maleVariant": "なぜ連中はあんたを見逃してるんだ？",
              "stringId": "3334681074927575040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見逃されたというか、ニカの命令で、全員外に追い出されたんだ",
              "maleVariant": "",
              "stringId": "3334682012371296256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンダーソンがどうやってインプラントを入手してるか知ってたの？",
              "maleVariant": "アンダーソンがどうやってインプラントを入手してるか知ってたのか？",
              "stringId": "3334805280633823232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すべて片付いた",
              "maleVariant": "",
              "stringId": "3334809807042813972"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンソニー… くそっ",
              "maleVariant": "",
              "stringId": "3334823311409848320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "祭壇の右手に階段がある。そこから地下に降りるんだ",
              "maleVariant": "",
              "stringId": "3336060188137791488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "侵入したら、オフィスを目指すといい",
              "maleVariant": "",
              "stringId": "3336149821890666496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。ニカはもうあんたらの邪魔をしない",
              "maleVariant": "ああ。ニカはもうあんたらの邪魔をしない",
              "stringId": "3336155832949055488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V――すべて元通りになったら、また戻ってくるといい",
              "maleVariant": "",
              "stringId": "3336156141196845056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはよかった。でもあまり羽目を外さないように。まだバスケットボールは控えるんだよ",
              "maleVariant": "",
              "stringId": "3379763292179701760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたがオデル・ブランコ？ スカベンジャーの件で来たんだけど",
              "maleVariant": "あんたがオデル・ブランコか？ スカベンジャーの件で来たんだが",
              "stringId": "3389657606932963328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おお、待っていたぞ！ 中には入ったか？ アンソニーは？",
              "maleVariant": "",
              "stringId": "3389669217085202432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オデル・ブランコ？",
              "maleVariant": "",
              "stringId": "3389671638389792768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセンがまだいた頃はな。いま、バーゲストにとってスカベンジャーは二の次だ",
