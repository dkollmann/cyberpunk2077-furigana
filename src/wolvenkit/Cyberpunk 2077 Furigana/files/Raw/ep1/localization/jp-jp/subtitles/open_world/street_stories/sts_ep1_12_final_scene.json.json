  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待て、待て。落ち着け。このランナーと繋がってるんだ、ほら",
              "maleVariant": "",
              "stringId": "2759157992353554432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ずいぶんと自信があるのね",
              "maleVariant": "ずいぶんと自信があるんだな",
              "stringId": "2760678434448990212"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "止めたいなら、さっさと話して",
              "maleVariant": "止めたいなら、さっさと話せ",
              "stringId": "2760678885034680320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で？ あんたがあたしを止める気？",
              "maleVariant": "で？ あんたが俺を止める気か？",
              "stringId": "2760680387971244032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたに協力すれば、クライアントが死ぬことになる",
              "maleVariant": "",
              "stringId": "2760681460085678080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜそこまで自信が持てるの？",
              "maleVariant": "なぜそこまで自信が持てるんだ？",
              "stringId": "2760683271706238976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "プロだからさ。君もそうだろう",
              "maleVariant": "",
              "stringId": "2957978391965982720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おいおい、こいつが本気でヴードゥーの連中の被害者を気にかけてると思うか？ んなわけねえだろ",
              "maleVariant": "",
              "stringId": "2957980222242807808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、あんたの任務を進めればいい",
              "maleVariant": "",
              "stringId": "3013028737008070660"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは依頼人を守るために来た",
              "maleVariant": "俺は依頼人を守るために来た",
              "stringId": "3013029409338224640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… わかった、ここはあんたに任せる。ただ失敗したら、あたしがあんたを殺すから",
              "maleVariant": "はあ… わかった、ここはあんたに任せる。ただ失敗したら、俺があんたを殺すからな",
              "stringId": "3013031294291996672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミルコ抜きで何とかするのね",
              "maleVariant": "ミルコ抜きで何とかするんだな",
              "stringId": "3013034019079647232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつを巻き添えにするのはごめんだろ",
              "maleVariant": "",
              "stringId": "3093856028172283904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただミルコを殺しても、今度は別の者がインディラを狙うだけだ",
              "maleVariant": "",
              "stringId": "3211644635745677312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業の連中はどいつも同じだ。笑顔の作り方と自分の成績を上げることしか考えてねえ",
              "maleVariant": "",
              "stringId": "3211665348191907840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう行け。いつ連中が来てもおかしくない",
              "maleVariant": "",
              "stringId": "3223172884820717568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行け。私は仕事に戻る",
              "maleVariant": "",
              "stringId": "3223173338141093888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そいつはもう死んだも同然よ",
              "maleVariant": "そいつはもう死んだも同然だ",
              "stringId": "3275165879385890820"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何者か知らないけど、そこから離れて",
              "maleVariant": "何者か知らないが、そこから離れろ",
              "stringId": "3275253731985629184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのランナーは殺人鬼――そしてあたしの標的よ",
              "maleVariant": "そのランナーは殺人鬼――そして俺の標的だ",
              "stringId": "3275253775858049028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "理由を話して",
              "maleVariant": "理由を話せ",
              "stringId": "3275256283296866308"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしを止める気？",
              "maleVariant": "俺を止める気か？",
              "stringId": "3275256545944182784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ならあたしはリズィー・ウィズィーよ",
              "maleVariant": "なら俺はリズィー・ウィズィーだ",
              "stringId": "3275292842561806340"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ。ブライス・モズリーは知ってる？",
              "maleVariant": "へえ。ブライス・モズリーは知ってるか？",
              "stringId": "3275293019980865536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私はネットウォッチのアラン・ノエル、組織犯罪スペシャリストだ",
              "maleVariant": "",
              "stringId": "3275293485884153856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ネットウォッチの覆面捜査官？",
              "maleVariant": "",
              "stringId": "3275296540612534272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "初めまして、アラン。あたしはリズィー・ウィズィーよ",
              "maleVariant": "初めまして、アラン。俺はリズィー・ウィズィーだ",
              "stringId": "3275299254327222272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ネットウォッチのエージェントが綺麗なネクタイを捨てて、敵の中に潜入してるってわけ？",
              "maleVariant": "ネットウォッチのエージェントが綺麗なネクタイを捨てて、敵の中に潜入してるってわけか？",
              "stringId": "3275299294005338116"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの同僚を知ってる。同じようにブードゥーのことを嗅ぎまわってた",
              "maleVariant": "",
              "stringId": "3275299331502415872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼にも銃を突きつけたのかね？",
              "maleVariant": "",
              "stringId": "3275311653042167808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "君のことは知っている。パシフィカでの“活躍”についてはイグルーでも話題になっていた",
              "maleVariant": "",
              "stringId": "3275314001399422976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、せいぜい頑張って。でもあたしは協力するつもりはない。依頼人の安全が第一よ",
              "maleVariant": "まあ、せいぜい頑張れよ。だが俺は協力するつもりはない。依頼人の安全が第一だ",
              "stringId": "3275344991433957376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その依頼人は… もしかしてコーポか？ ヴードゥー・ボーイズの標的傾向からみると、おそらくエンジニア系かな",
              "maleVariant": "",
              "stringId": "3275347898606723072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最初は引き入れも試したさ。だが、どんな条件を提示しても、寝返る者はいなかった。連中の結束力を見くびっていたんだ",
              "maleVariant": "",
              "stringId": "3275354118642114560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ。でも、アラン――ネットウォッチだか何だか知らないけど、邪魔をすれば排除する",
              "maleVariant": "へえ。だが、アラン――ネットウォッチだか何だか知らないが、邪魔をすれば排除する",
              "stringId": "3275362438195093504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しかし、君と違って私の目的は混乱ではなく、秩序と平穏をもたらすことだ",
              "maleVariant": "",
              "stringId": "3275378356736401408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなことはわかっている",
              "maleVariant": "",
              "stringId": "3275382207644344320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブードゥー・ボーイズはそのテックで何をしようとしてるの？",
              "maleVariant": "ブードゥー・ボーイズはそのテックで何をしようとしてるんだ？",
              "stringId": "3275386631276109828"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "テックを入手してどうするの？ 博物館に展示でもするつもり？",
              "maleVariant": "テックを入手してどうするんだ？ 博物館に展示でもするつもりか？",
              "stringId": "3275387541406523392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ストリートで手に入るようなものじゃないでしょ",
              "maleVariant": "ストリートで手に入るようなものじゃないだろ",
              "stringId": "3275387595412381700"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ… 地上ではな。ドッグタウンは古いミリテクのバンカー施設の上に造られた。大げさな壁で隔離されているのもそれが理由だ",
              "maleVariant": "",
              "stringId": "3275389087259205632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フッ… 古いハードウェアに金を出す連中は大勢いる",
              "maleVariant": "",
              "stringId": "3275390362193403904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "下がって",
              "maleVariant": "下がれ",
              "stringId": "3282521182276329472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミルコと違って、あんたには選択肢がまだある",
              "maleVariant": "",
              "stringId": "3282541443432226816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミルコを殺しても弾の無駄になるだけだぞ",
              "maleVariant": "",
              "stringId": "3282623505543634944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かにミルコの手は汚れている。だが、しょせん小物だ。お山の大将に過ぎない",
              "maleVariant": "",
              "stringId": "3282623933060653056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいか。私はここで何か月も、この社会のダニたちと過ごしてきた。ミルコのような恐喝犯や殺人鬼を、組織ごと一網打尽にするためにな",
              "maleVariant": "",
              "stringId": "3282784554183991296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "せいぜい頑張って",
              "maleVariant": "せいぜい頑張れ",
              "stringId": "3282797171606724612"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミルコはあんたの情報源？",
              "maleVariant": "ミルコはあんたの情報源か？",
              "stringId": "3282800801105747968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミルコはあんたの何なの？ 情報源？",
              "maleVariant": "ミルコはあんたの何なんだ？ 情報源か？",
              "stringId": "3282809297893236764"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミルコがネットに入るたびに、私も便乗してる。彼のボスを特定するためにな",
              "maleVariant": "",
              "stringId": "3282809297893236776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうでしょうね",
              "maleVariant": "どうだろうな",
              "stringId": "3284025214845231104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ネットウォッチのアラン・ノエル、組織犯罪スペシャリストさん――そこをどいて",
              "maleVariant": "ネットウォッチのアラン・ノエル、組織犯罪スペシャリストさん――そこをどけ",
              "stringId": "3284025754853482496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "せめてミルコから切断させてくれ",
              "maleVariant": "",
              "stringId": "3284026601314361344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "モズリーのことか。君のことは聞いている",
              "maleVariant": "",
              "stringId": "3284031100007391232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウソはついてない",
              "maleVariant": "",
              "stringId": "3284031265397186564"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コーポの言うことに耳を貸すつもりは一切ない",
              "maleVariant": "",
              "stringId": "3284054871929954304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "当時も、ネットウォッチと手を組んだのだろう",
              "maleVariant": "",
              "stringId": "3284060286256324608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。こっそり追跡プログラムを体内に入れられるまではね。ずいぶんとなめた真似をしてくれた",
              "maleVariant": "ああ。こっそり追跡プログラムを体内に入れられるまではな。ずいぶんとなめた真似をしてくれた",
              "stringId": "3284060741086650368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが、そのためにはまず黒幕を見つけ出す必要がある",
              "maleVariant": "",
              "stringId": "3284108144674250752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "向こうが先にあたしの脳を焼こうとしたのよ",
              "maleVariant": "向こうが先に俺の脳を焼こうとしたんだ",
              "stringId": "3284115599579848704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仮に本物だとしても、ギャングの方がまだマシよ",
              "maleVariant": "仮に本物だとしても、ギャングの方がまだマシだ",
              "stringId": "3284124630654558208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴードゥーは古い軍用テックを入手したが、再度使用可能にするには、エキスパートにリバースエンジニアリングをしてもらう必要があるのさ",
              "maleVariant": "",
              "stringId": "3284164396716564480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのテックはどこで手に入れたの？",
              "maleVariant": "そのテックはどこで手に入れたんだ？",
              "stringId": "3284170541573251072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "海岸沿いのあらゆる地域で犠牲者が出ている。そしてミルコはあくまでも下っ端ネットランナーの1人だ",
              "maleVariant": "",
              "stringId": "3284172126181302272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここに来るまでに戦闘ボットを見かけた",
              "maleVariant": "",
              "stringId": "3284177346428837888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "道中でボットをいくつか破壊した。古い軍用モデルのヤツよ",
              "maleVariant": "道中でボットをいくつか破壊した。古い軍用モデルのヤツだ",
              "stringId": "3284181972930699264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほう… ヴードゥーの連中は快く思わないだろうな。だがまだストックはある",
              "maleVariant": "",
              "stringId": "3284182955706458112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭を特定さえできれば、組織犯罪そのものに終止符を打つことができる",
              "maleVariant": "",
              "stringId": "3284185441569775616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ！ 自分が何をしたかわかってるのか？！",
              "maleVariant": "",
              "stringId": "3285527314930507776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "後悔はさせない。約束する",
              "maleVariant": "",
              "stringId": "3285529160625594368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "開発途上国では、企業テックのコピー品くらいしか手に入らない。下手したら、買い手はヴードゥーの商品が骨董品だと気づきもしないだろう",
              "maleVariant": "",
              "stringId": "3285594490567827456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "発端は20年代の極秘軍事プロジェクト。しかし大失敗に終わった結果、ミリテクは損切りをし、バンカーを埋めた。しかし、“遺跡荒らし”たちが研究所のなかにあったものを持ち出して捌いてるのさ",
              "maleVariant": "",
              "stringId": "3285611178512261120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話をすればわかり合える",
              "maleVariant": "",
              "stringId": "3286725081048719360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何万エディーもの大金をつぎ込んだネットウォッチの作戦を潰したんだぞ",
              "maleVariant": "",
              "stringId": "3286732930202116096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたにとって、“犠牲者”はファイルにある番号に過ぎない。でもあたしの依頼人には名がある――インディラ・バラサよ。そしてミルコを見逃せば、彼女は死ぬ",
              "maleVariant": "あんたにとって、“犠牲者”はファイルにある番号に過ぎない。でも俺の依頼人には名がある――インディラ・バラサだ。そしてミルコを見逃せば、彼女は死ぬ",
              "stringId": "3286745408021635072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。ハックを手伝うか、ハックされるかってとこね。連中の目的はわかってるの？",
              "maleVariant": "ああ。ハックを手伝うか、ハックされるかってとこさ。連中の目的はわかってるのか？",
              "stringId": "3286935812864122880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私にチャンスをくれ。協力してくれれば、より大勢の人を助けることができる",
              "maleVariant": "",
              "stringId": "3286942743766933504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのためにはミルコが必要なんだ。彼が死んでも、何も変わらない。わかるだろう",
              "maleVariant": "",
              "stringId": "3286979207519182848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やめるんだ！",
              "maleVariant": "",
              "stringId": "3298371804179374080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当に殺す気なのか？",
              "maleVariant": "",
              "stringId": "3298372176230916100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミルコを殺させるわけにはいかないんだ",
              "maleVariant": "",
              "stringId": "3326224723717038156"
            },
            {
