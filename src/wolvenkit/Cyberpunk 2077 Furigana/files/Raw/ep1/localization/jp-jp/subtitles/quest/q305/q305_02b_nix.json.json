  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてここで待ち合わせを？",
              "maleVariant": "",
              "stringId": "2980826641530773504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼んだデータは？",
              "maleVariant": "",
              "stringId": "3064897853747761156"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ツアーガイドにでもなった？",
              "maleVariant": "ツアーガイドにでもなったのか？",
              "stringId": "3064898618973360128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ニックス、スピーチはもういい。データは？",
              "maleVariant": "",
              "stringId": "3064902004665880576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ニックス、一流ランナーからツアーガイドに転職？",
              "maleVariant": "ニックス、一流ランナーからツアーガイドに転職か？",
              "stringId": "3064902139504365568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直、あそこまでの地雷原に挑んだのは久しぶりだったぞ。だがなんとかなった",
              "maleVariant": "",
              "stringId": "3064905295634239488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最高。さすがね、ニックス",
              "maleVariant": "最高。さすがだな、ニックス",
              "stringId": "3064911359406641152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "情報の使い道は聞かないが… V…",
              "maleVariant": "",
              "stringId": "3064916021308428288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを受け取る] 平気よ。仲間がいる",
              "maleVariant": "[チップを受け取る] 平気だ。仲間がいる",
              "stringId": "3064917587193106436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを受け取る] やれるだけやってみる",
              "maleVariant": "",
              "stringId": "3064917801018724352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心配してくれてありがとう。危険な仕事なのはわかってる。でも他に選択肢がないの",
              "maleVariant": "心配してくれてありがとう。危険な仕事なのはわかってる。だが他に選択肢がないんだ",
              "stringId": "3064918804396904448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "データはどうも。あとは任せて",
              "maleVariant": "データはどうも。あとは任せろ",
              "stringId": "3064918896386379776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ならいいが…",
              "maleVariant": "",
              "stringId": "3064920231215239168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだ、渡したいもんがある。完全テック武装のマッポどもを出し抜けるかもな",
              "maleVariant": "",
              "stringId": "3064921729068306432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "というと？",
              "maleVariant": "",
              "stringId": "3064922530935345152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺がアラサカにいたころの逸品さ。軍事用サイバーウェアをカスタムした、名付けてニックス・スペシャル",
              "maleVariant": "",
              "stringId": "3064922637973983232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "恩に着るよ、ニックス",
              "maleVariant": "",
              "stringId": "3064924942341378048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気にするな。一度助けてもらった。その礼だと思ってくれ",
              "maleVariant": "",
              "stringId": "3064925332898189312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頑張れよ、V",
              "maleVariant": "",
              "stringId": "3064927617049038848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "情報の使い道は聞かないが… ロクなことにならないぞ。とにかく… 俺の出番は終わりだ。言いたいことも言ったしな",
              "maleVariant": "",
              "stringId": "3064930126232686592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いろんな話を読んで、慎重なのが一番だということに気づいた",
              "maleVariant": "",
              "stringId": "3069366261264871424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "余計なお世話だって",
              "maleVariant": "",
              "stringId": "3069368811015827456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういうだろうと思ったよ",
              "maleVariant": "",
              "stringId": "3069374180547031040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全部入ってる… 日程、車列の並び順、警護プラン、ロス・パドレスに移送される拘留者の暗号解読済みデータ",
              "maleVariant": "",
              "stringId": "3069384542021414912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は自分が関わった痕跡を消して、最高級の外骨格を着けたサイコスクワッドがうちに押しかけてこないよう祈っとく",
              "maleVariant": "",
              "stringId": "3069387489744711680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ロス・パドレスにブチこまれる立場にはなるな。一度入ったら出られないからな",
              "maleVariant": "",
              "stringId": "3069389123057020928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やあ、ニックス。なぜあえてここなの？",
              "maleVariant": "やあ、ニックス。なぜあえてここなんだ？",
              "stringId": "3069392119316803600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気に入らない場所か？ それでいいんだよ",
              "maleVariant": "",
              "stringId": "3069392119316803612"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺の知り合いに時々いるんだ。自分の身の程を知らずに無茶する奴ってのがな",
              "maleVariant": "",
              "stringId": "3069392119333580800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここに来るとそいつらを思い出す。もう死んじまってるが",
              "maleVariant": "",
              "stringId": "3069392119333580812"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "できるだけ気をつけるよ",
              "maleVariant": "",
              "stringId": "3069397941530849280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今回はソロの仕事じゃない。なんとかやり切ってみせるよ",
              "maleVariant": "今回はソロの仕事じゃない。なんとかやり切ってみせるさ",
              "stringId": "3069401957090390016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "間違ってもロス・パドレスは叩くな。入ったら帰って来れないぞ",
              "maleVariant": "",
              "stringId": "3069403502506864640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰かがあんたの脳をショートさせようとすれば、動的ICEを放出する",
              "maleVariant": "",
              "stringId": "3069408897623322624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だからよ、V、俺はお前に警告してやる。バカどもと同じ轍を踏むな",
              "maleVariant": "",
              "stringId": "3069412447548010496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたも十分気をつけな",
              "maleVariant": "",
              "stringId": "3072078075379511296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、調子はどうだ？",
              "maleVariant": "",
              "stringId": "3121658390692716544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんとかやってる",
              "maleVariant": "",
              "stringId": "3121660188203638784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんとかやってる。そっちはどう？",
              "maleVariant": "なんとかやってる。そっちはどうだ？",
              "stringId": "3121661703941218304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なあ、V… 俺はここでこうやって慰霊碑を見つめるたびに、ある種の人間のことを考える",
              "maleVariant": "",
              "stringId": "3121663143040475136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう",
              "maleVariant": "",
              "stringId": "3121844796383780864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分の身の程を知らずに無茶をして、結局は犬死にしてった奴らをな",
              "maleVariant": "",
              "stringId": "3125722738610860032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺が関わった痕跡も消したことだし、あとは最高級の外骨格を着けたサイコスクワッドがうちに押しかけてこないよう祈っとくよ",
              "maleVariant": "",
              "stringId": "3125731565959835648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのデータを持ってるだけで死ぬほど危険だってのはわかってるよな？",
              "maleVariant": "",
              "stringId": "3357947991062089744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "狙われるのは慣れてる",
              "maleVariant": "",
              "stringId": "3357947991078866944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かにな。よし、我らがスーパースパイのリードに見せて、そいつの活用法を探ってもらうとするか",
              "maleVariant": "",
              "stringId": "3357947991078866956"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺のことを忘れてないか？",
              "maleVariant": "",
              "stringId": "3357948484513566720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話聞いてるのか？",
              "maleVariant": "",
              "stringId": "3357949946127847424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[待つ]",
              "maleVariant": "",
              "stringId": "3374096485791428608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ来てないなら、待つしかないか",
              "maleVariant": "",
              "stringId": "3376897079178555392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを受け取る] ありがとう",
              "maleVariant": "",
              "stringId": "3784197999825739780"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}