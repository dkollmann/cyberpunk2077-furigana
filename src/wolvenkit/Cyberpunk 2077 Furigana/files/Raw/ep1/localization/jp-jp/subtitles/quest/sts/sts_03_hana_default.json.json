        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジェイソンの母親はどう？",
              "maleVariant": "ジェイソンの母親はどうだ？",
              "stringId": "3307274924383498240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "えーと… 何？",
              "maleVariant": "えーと… 何だ？",
              "stringId": "3307275225417084928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "調子はどう？",
              "maleVariant": "調子はどうだ？",
              "stringId": "3307275817971576836"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最近のスタックスはどう？ 少しは… 落ち着いた？",
              "maleVariant": "最近のスタックスはどうだ？ 少しは… 落ち着いたか？",
              "stringId": "3314289765669367808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジェイソンの母親は？",
              "maleVariant": "",
              "stringId": "3314290167785680896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "前に進むしかないからね。現場の後片付けは終わったけど… 決して忘れることはない。絶対にね… ありがとう、V",
              "maleVariant": "",
              "stringId": "3314291039429160960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最近はこもりきりだけど… みんなでちゃんと面倒を見てるわ",
              "maleVariant": "",
              "stringId": "3314292152882655232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何？",
              "maleVariant": "何だ？",
              "stringId": "3314294305466576900"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインダーを見逃したでしょ。あれだけ言ったのに、一体どういうつもりよ！ 二度とここに顔を出さないで",
              "maleVariant": "",
              "stringId": "3314294995094040576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V！ 来て。ちょっと話したいことがあるんだけど",
              "maleVariant": "",
              "stringId": "3434876485110173696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やり直すチャンスを与えるべきよ",
              "maleVariant": "やり直すチャンスを与えるべきだ",
              "stringId": "3434876637061419012"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインダーは病気だったのよ。それに心から後悔してた。やり直すチャンスを与えるべきよ。スタックスの人たちと同じようにね",
              "maleVariant": "ラインダーは病気だったんだ。それに心から後悔してた。やり直すチャンスを与えるべきだ。スタックスの人たちと同じようにな",
              "stringId": "3434876749334548480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ごめんなさい",
              "maleVariant": "すまない",
              "stringId": "3434876889592074240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね… あたしが悪かった",
              "maleVariant": "そうだな… 俺が悪かった",
              "stringId": "3434876976447721472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "謝って済む問題じゃないわ。さっさと消えて",
              "maleVariant": "",
              "stringId": "3434901714821812224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "勝手なことを。あいつのカウンセリングを頼んだ覚えはない",
              "maleVariant": "",
              "stringId": "3434907440164212736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインダーが生きてることは誰にも言わないで。私たちだけの秘密よ。どれだけの人が傷つくか… わかったら、もう消えて",
              "maleVariant": "",
              "stringId": "3434913643321724928"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
