  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ディープダイブを受け取る] わかったからたっぷり聞かせて",
              "maleVariant": "[ディープダイブを受け取る] わかったからたっぷり聞かせてくれ",
              "stringId": "2959966727095881732"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立つ] やるわけないでしょ",
              "maleVariant": "[立つ] やるわけないだろ",
              "stringId": "2959967513880203264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "怪しげなクスリはパス。それがないと退屈な話なの？",
              "maleVariant": "怪しげなクスリはパス。それがないと退屈な話なのか？",
              "stringId": "2959967773356625920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死にゃしないって。キメずに話を聞く方が死にたくなるよ",
              "maleVariant": "",
              "stringId": "2959969261848006656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あれは人生最高の日だった。メジャーへの切符を手にしたんだぜ？ さあ思いっきり吸って、聞いてくれ",
              "maleVariant": "",
              "stringId": "3014410663369191424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺があいつにビビってると思ってんのか？",
              "maleVariant": "",
              "stringId": "3229458209454854144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バカ、ビビッて当然なのよ！",
              "maleVariant": "",
              "stringId": "3229458645645692928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もういい！ お前といるとマトモに考えられないよ！",
              "maleVariant": "",
              "stringId": "3233290827677904896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "似た話を知ってる。そんなふうに始まり、最悪の結末で友達を1人失った",
              "maleVariant": "",
              "stringId": "3242173896153444352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何、あたし？",
              "maleVariant": "何だ、俺か？",
              "stringId": "3272436703285293056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、あんた！ ちょっと来てくれ！",
              "maleVariant": "",
              "stringId": "3272437050590441472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしに言ってる？",
              "maleVariant": "俺に言ってるのか？",
              "stringId": "3272446316294516736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他に誰がいる？ 来いよ、ちょっと困ってるんだ。ウォッカもあるぞ、飲んでけよ？ どうせ俺はハンセンにぶっ殺されるんだから、飲むしかねえ…",
              "maleVariant": "",
              "stringId": "3272447272394502144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座って飲む] 何があったの？",
              "maleVariant": "[座って飲む] 何があったんだ？",
              "stringId": "3272449173957718020"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その最悪な状況ってのは？ 生きるか死ぬかの問題みたいだけど",
              "maleVariant": "その最悪な状況ってのは？ 生きるか死ぬかの問題みたいだが",
              "stringId": "3272451725151514624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、聞いてくれるか？",
              "maleVariant": "",
              "stringId": "3272501876008235008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む] ぜひ聞かせて",
              "maleVariant": "[飲む] 聞かせてくれ",
              "stringId": "3272502807194054660"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立つ] いや、身の上話を聞いてる暇はない",
              "maleVariant": "",
              "stringId": "3272503304890167296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話して、何があったの",
              "maleVariant": "話せ、何があった",
              "stringId": "3276663551779221504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話したいのは山々だけど、長話を聞いてるヒマはないの。きっと何とかなるよ",
              "maleVariant": "話したいのは山々だが、長話を聞いてるヒマはないんだ。きっと何とかなるって",
              "stringId": "3276669321228922880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、なんで俺たちが新人だと？",
              "maleVariant": "",
              "stringId": "3276778076226531328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スペースフォース・ワンが落ちて、みんな物漁りに夢中だってのに、こんな所で呑気に酒を飲んでるからよ。装備も貧弱だし。まだ続ける？",
              "maleVariant": "スペースフォース・ワンが落ちて、みんな物漁りに夢中だってのに、こんな所で呑気に酒を飲んでるからだ。装備も貧弱だしな。まだ続けるか？",
              "stringId": "3276780324792905728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんた頭が切れるんだな。確かにヤバい話で、ペラペラしゃべるような内容じゃないんだが、もう遅い",
              "maleVariant": "",
              "stringId": "3276781688445022208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これは… ミリテクのスマートドラッグに何か混ぜたもの？ 吸入器も似てる。相当効きそうね…",
              "maleVariant": "これは… ミリテクのスマートドラッグに何か混ぜたものか？ 吸入器も似てる。相当効きそうだな…",
              "stringId": "3283987312849268736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんでそんなに詳しいわけ？",
              "maleVariant": "",
              "stringId": "3283992433087819776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "訓練の時に吸ったから…",
              "maleVariant": "訓練の時に吸ったからな…",
              "stringId": "3283993970333790208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すげえな！",
              "maleVariant": "",
              "stringId": "3283995137021390848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チンガ・ス・マドレ！ 来いってば！",
              "maleVariant": "",
              "stringId": "3294047870231326720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー待った待った！ せっかくだ、こいつをやってちゃんと感じてくれ。ディープダイブってブツだ。話の世界にどっぷり入り込める！",
              "maleVariant": "",
              "stringId": "3294079601449709568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、V。この最悪な状況に乾杯だ。あの野郎が消えることを祈って",
              "maleVariant": "",
              "stringId": "3295553640953360384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バーゲストの機密情報を漏らす気？",
              "maleVariant": "バーゲストの機密情報を漏らす気か？",
              "stringId": "3295560365865512960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうかい。酒はまだある。また聞く気になったら寄ってくれ",
              "maleVariant": "",
              "stringId": "3298577240283230208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたたち、バーゲストの新人でしょ？",
              "maleVariant": "お前たち、バーゲストの新人だろ？",
              "stringId": "3298599084872773632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何だよ、助けてくれないのか？ まあ、頭の片隅に置いといてくれ。聞く気になったら、俺たちはここにいる",
              "maleVariant": "",
              "stringId": "3305985315822710784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] ウォッカを",
              "maleVariant": "",
              "stringId": "3306291020173840388"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウォッカをお願いできる",
              "maleVariant": "ウォッカをお願いできるか",
              "stringId": "3306291661583585280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] じゃあ、やってみる",
              "maleVariant": "",
              "stringId": "3307997901061787648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、よこして",
              "maleVariant": "わかった、よこせ",
              "stringId": "3307997901095342088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しっかりしてよ、パコ！ これからどうすんの？",
              "maleVariant": "",
              "stringId": "3312793542237433856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座って飲む] 問題の解決を祈って乾杯",
              "maleVariant": "",
              "stringId": "3323057461724000256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悩みのない人生に",
              "maleVariant": "",
              "stringId": "3323057831862939648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんな人生を送れたら、嬉しいんだけど",
              "maleVariant": "",
              "stringId": "3323062009809154048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞いたことのあるクスリね…",
              "maleVariant": "聞いたことのあるクスリだな…",
              "stringId": "3338864416208007168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "似た話を知ってる。そんなふうに始まって、数時間後には大事な親友が亡くなった",
              "maleVariant": "",
              "stringId": "3339752746261057536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そりゃ悲しいな。安心しろ、この話で俺は死なない",
              "maleVariant": "",
              "stringId": "3339753222448779264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて、そろそろ話を聞かせてよ",
              "maleVariant": "さて、そろそろ話を聞かせてくれ",
              "stringId": "3339757605731340288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "想像してくれ。ハンセンの兵隊になった初日、俺はボスを待つ間、両足がゼリーみたいにブルブル震えてた",
              "maleVariant": "",
              "stringId": "3339759193107632128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、違うな。自分がハンセンだと想像してみてくれ。それで、めちゃめちゃキレてる！",
              "maleVariant": "",
              "stringId": "3339759667114315776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺はパコ。こいつはバブスだ",
              "maleVariant": "",
              "stringId": "3347578879551549440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vよ",
              "maleVariant": "Vだ",
              "stringId": "3347579208821190656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと… パコ、頼むから… 大声で話さないで。誰が聞いてるかわかんないよ",
              "maleVariant": "",
              "stringId": "3347581934514810880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、飲むの？",
              "maleVariant": "",
              "stringId": "3357251285509640192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？ …おい",
              "maleVariant": "",
              "stringId": "3357263239997460488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "続きを聞くか？",
              "maleVariant": "",
              "stringId": "3357265830265393152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "常に成功か失敗の二択ってわけでもないんじゃない？",
              "maleVariant": "常に成功か失敗の二択ってわけでもないんじゃないか？",
              "stringId": "3359407798617452548"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バブスは俺はもうおしまいだ、みたいに言うけどさ、取り返せない失敗はない、だろ？",
              "maleVariant": "",
              "stringId": "3359408236284686336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヤバい状況なら何度かかいくぐってきた",
              "maleVariant": "",
              "stringId": "3359409442465853440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人生の… 一般論として？",
              "maleVariant": "人生の… 一般論としてか？",
              "stringId": "3359413098758090752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヤバい状況なら何度もかいくぐってきた",
              "maleVariant": "",
              "stringId": "3359413249853698048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、そうだな、うん、人生で",
              "maleVariant": "",
              "stringId": "3359414295074254848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "常に成功と失敗の二択ってわけじゃない。時には… 逃げないと",
              "maleVariant": "",
              "stringId": "3359414409259986944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうかな。今回はどこまでも追っかけてきそうなんだ",
              "maleVariant": "",
              "stringId": "3359414467091050496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハハ！ バブス！ 希望の光だ！",
              "maleVariant": "",
              "stringId": "3359416871282880512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "適当なことを言ってるだけよ…",
              "maleVariant": "",
              "stringId": "3359417004376535040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ごちゃごちゃ言うな。とにかく今は、藁でも掴まないと",
              "maleVariant": "",
              "stringId": "3359417271587254272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] 酒はいらない。で、その最悪な状況って？",
              "maleVariant": "",
              "stringId": "3446272175789912064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "酒は遠慮しとくけど、その… 最悪な状況ってのは？",
              "maleVariant": "酒は遠慮しとくが、その… 最悪な状況ってのは？",
              "stringId": "3446340258151870464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話を聞きましょ",
              "maleVariant": "話を聞こう",
              "stringId": "3478070440172531712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やれやれ",
              "maleVariant": "",
              "stringId": "3823203872248508416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、嫌な予感がする",
              "maleVariant": "",
              "stringId": "3823230315087081472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る]",
              "maleVariant": "",
              "stringId": "3853430822161170432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む] その最悪な状況ってのは？",
              "maleVariant": "",
              "stringId": "3855032122603900940"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む] 悩みのない人生に",
              "maleVariant": "",
              "stringId": "3855032122603900964"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "酒は遠慮しとくけど、何があったわけ？",
              "maleVariant": "酒は遠慮しとくが、何があったんだ？",
              "stringId": "3855032122620678160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブチギレ・カート・ハンセンだ",
              "maleVariant": "",
              "stringId": "3895511492891848704"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}