      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "来ない選択肢はなかった",
              "maleVariant": "",
              "stringId": "3008580590255976448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "来ないことも考えた",
              "maleVariant": "",
              "stringId": "3099976227534385152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、色々あったから、じっくり話し合える相手が必要でね",
              "maleVariant": "まあ、色々あったから、じっくり話し合える相手が必要でな",
              "stringId": "3100007499308707840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実を言うと、来る気はなかった。まったくね",
              "maleVariant": "実を言うと、来る気はなかった。まったくな",
              "stringId": "3100007523333681156"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、やっと会えた",
              "maleVariant": "",
              "stringId": "3103159204963659776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしが殺した。そうするしかなかったの",
              "maleVariant": "俺が殺した。そうするしかなかったんだ",
              "stringId": "3103169813750984708"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頑固さが命取りになった",
              "maleVariant": "",
              "stringId": "3103170225329647616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズのせいで死んだ",
              "maleVariant": "",
              "stringId": "3103171098415968256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしが殺した。リードが… 他に選択肢を与えてくれなかったから",
              "maleVariant": "俺が殺した。リードが… 他に選択肢を与えてくれなかったから",
              "stringId": "3103171564369588224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リードは堅物だった。そのせいで死んだの。負けてもなお諦めなかった",
              "maleVariant": "リードは堅物だった。そのせいで死んだんだ。負けてもなお諦めなかった",
              "stringId": "3103171797858103296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつは命令を盲信し、最後まで立ちはだかった",
              "maleVariant": "",
              "stringId": "3103171983028236288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他に… やり方はなかったの？",
              "maleVariant": "",
              "stringId": "3103175314480017408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "懐かしいぜ、この感じ。戦争の臭いがプンプンしやがる",
              "maleVariant": "",
              "stringId": "3103177227317522432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズのババアは、事を荒立てるのがお上手だ",
              "maleVariant": "",
              "stringId": "3103178370751901696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おそらく、マイヤーズとあたしが現れた時から、死は覚悟してたと思う",
              "maleVariant": "おそらく、マイヤーズと俺が現れた時から、死は覚悟してたと思う",
              "stringId": "3103181651922366464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この世を去った、かつての仲間たちに",
              "maleVariant": "",
              "stringId": "3103182673101172736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む] ジャッキーに",
              "maleVariant": "",
              "stringId": "3103185571801444352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む] ジョニーに",
              "maleVariant": "",
              "stringId": "3103185792623161344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む] T-バグに",
              "maleVariant": "",
              "stringId": "3103186017739845632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む] スコーピオンに",
              "maleVariant": "",
              "stringId": "3103186166553751552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む] リードに",
              "maleVariant": "",
              "stringId": "3103186411618545664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "T-バグに",
              "maleVariant": "",
              "stringId": "3103187187464122372"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー・シルヴァーハンドに",
              "maleVariant": "",
              "stringId": "3103187287942868996"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジャッキーに",
              "maleVariant": "",
              "stringId": "3103187431002189828"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "両者がぶつかり合えば、ミリテクがアラサカの喉を引き裂くことになるのは間違いない",
              "maleVariant": "",
              "stringId": "3103202455804424192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "肉体と、魂を失った者たちに",
              "maleVariant": "",
              "stringId": "3103206235073654784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リードに",
              "maleVariant": "",
              "stringId": "3103210434427596800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む] タケムラに",
              "maleVariant": "",
              "stringId": "3103221059857920000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゴロウ・タケムラに",
              "maleVariant": "",
              "stringId": "3103221307841949696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジャッキーに",
              "maleVariant": "",
              "stringId": "3103222013005115392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ… シルヴァーハンドに",
              "maleVariant": "",
              "stringId": "3103223502821896192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "T-バグに",
              "maleVariant": "",
              "stringId": "3103224312473563136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "タケムラさんに、乾杯！",
              "maleVariant": "",
              "stringId": "3103224802183720960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誇り高き愛国者にして… 同僚に",
              "maleVariant": "",
              "stringId": "3103229861235765248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信念を曲げず死んだ",
              "maleVariant": "",
              "stringId": "3103230041372733440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何があったの？ 彼の最期を知りたい",
              "maleVariant": "",
              "stringId": "3103230395120332800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの話よ。すごい偶然ね",
              "maleVariant": "",
              "stringId": "3103335635609956376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかるよ",
              "maleVariant": "",
              "stringId": "3109877073033207808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あえて言わせて。再会できて嬉しい",
              "maleVariant": "あえて言わせてくれ。再会できて嬉しい",
              "stringId": "3109877580711763968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む] エヴリンに",
              "maleVariant": "",
              "stringId": "3110000306550120448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エヴリン・パーカーに",
              "maleVariant": "",
              "stringId": "3110000834294226944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいや。お前にだ、V",
              "maleVariant": "",
              "stringId": "3110027023897501700"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー、パーカーに",
              "maleVariant": "",
              "stringId": "3110115782366240768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、なかった",
              "maleVariant": "ああ、なかった",
              "stringId": "3110139737529556992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リードはFIA内で好かれてた？",
              "maleVariant": "リードはFIA内で好かれてたのか？",
              "stringId": "3110265099219542016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "寂しくなるね",
              "maleVariant": "寂しくなるな",
              "stringId": "3110266167240335360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "職場ではどうだった？",
              "maleVariant": "",
              "stringId": "3110267558558081024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "寂しい？",
              "maleVariant": "寂しいか？",
              "stringId": "3110267623452352512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね。まあ、人によって評判は違ったかな",
              "maleVariant": "",
              "stringId": "3110269607827906560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私の中では7年間死んでたのよ。それを思い出して元の感覚に戻ればいいだけ",
              "maleVariant": "",
              "stringId": "3110275766257360896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "好きだった",
              "maleVariant": "",
              "stringId": "3110326713058762756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "好きではなかった",
              "maleVariant": "",
              "stringId": "3110327236205912064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろん",
              "maleVariant": "",
              "stringId": "3110331172442775552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいえ",
              "maleVariant": "いいや",
              "stringId": "3110331217690927108"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまらん大義に死んだ男に",
              "maleVariant": "",
              "stringId": "3111456220050268160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パーカーに",
              "maleVariant": "",
              "stringId": "3111462063504269312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "T-バグに",
              "maleVariant": "",
              "stringId": "3111462150611574788"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真のレジェンドだ…",
              "maleVariant": "",
              "stringId": "3111462205993164804"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FIAエージェントの運命か",
              "maleVariant": "",
              "stringId": "3233164485864173568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この世界は、複雑な問題で溢れかえってる",
              "maleVariant": "",
              "stringId": "3233168127442792448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新合衆国はすでに撤収の準備中。私も荷造りは済んだ",
              "maleVariant": "",
              "stringId": "3233214067251851264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残された最後の任務が… あんたの始末よ",
              "maleVariant": "",
              "stringId": "3233214434572218368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう簡単には殺されない",
              "maleVariant": "",
              "stringId": "3233214669654568964"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしを殺すつもり？",
              "maleVariant": "俺を殺すつもりか？",
              "stringId": "3233215246992125952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "試してみる？",
              "maleVariant": "試してみるか？",
              "stringId": "3233215711419019264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で？ あたしを殺すの？",
              "maleVariant": "で？ 俺を殺すのか？",
              "stringId": "3233215760039391232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはもう死にかけてる",
              "maleVariant": "",
              "stringId": "3233215802435416064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だったら待つまでよ。仲間内でちょっとした賭けをしててね。「事故」や「自然死」のオッズはかなり低いからさ",
              "maleVariant": "",
              "stringId": "3233216847135879168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう、アレックス",
              "maleVariant": "",
              "stringId": "3233217399223726080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話はそれだけじゃなさそうね…",
              "maleVariant": "話はそれだけじゃなさそうだな…",
              "stringId": "3233217839977967616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リスクはないの？",
              "maleVariant": "リスクはないのか？",
              "stringId": "3233218126583148544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの殺し方について話すために、呼び出したわけじゃないんでしょ？",
              "maleVariant": "俺の殺し方について話すために、呼び出したってわけでもないんだろ？",
              "stringId": "3233218195772387328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "教えてよ。リードはどうなったの？",
              "maleVariant": "",
              "stringId": "3233218491923804160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕事をやめてここを離れるべき",
              "maleVariant": "仕事をやめてここを離れるべきだ",
              "stringId": "3233222184035397632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしが死ぬまではどうするの？",
              "maleVariant": "俺が死ぬまではどうするんだ？",
              "stringId": "3233222589121277952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この仕事はもうやめたほうがいい",
              "maleVariant": "",
              "stringId": "3233222637724872704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "陳腐な台詞はアフターライフの連中用に取っときな",
              "maleVariant": "",
              "stringId": "3233251556544983040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "直球ね",
              "maleVariant": "",
              "stringId": "3233252681658642432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ぶっちゃけ、あんたはそこまで重要視されてない",
              "maleVariant": "",
              "stringId": "3233254388237053952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、そのとおり",
              "maleVariant": "",
              "stringId": "3233257106615808000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのつもりかもしれないよ？",
              "maleVariant": "",
              "stringId": "3233263679224393728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "のんびり過ごすよ。ようやくね",
              "maleVariant": "",
              "stringId": "3233265053563596800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしが死ぬのを待つ間はどうするの？",
              "maleVariant": "俺が死ぬのを待つ間はどうするんだ？",
              "stringId": "3236237199269146624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そっちもね。私が殺すまで、他の奴に殺されないでよ",
              "maleVariant": "",
              "stringId": "3236237726526713896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつは気に入った。敵側なのが残念だ",
              "maleVariant": "",
              "stringId": "3236237726526713904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、次はモナコだっけ？",
              "maleVariant": "",
              "stringId": "3236238673835438080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "留守ね。待つしかないか",
              "maleVariant": "留守だな。待つしかないか",
              "stringId": "3237622636232019968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "世界情勢について話したいわけじゃないでしょ？",
              "maleVariant": "世界情勢について話したいわけじゃないだろ？",
              "stringId": "3278214218938413056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさか本気じゃ…",
              "maleVariant": "",
              "stringId": "3278217198068658176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさか本気じゃ…",
              "maleVariant": "",
              "stringId": "3278219417526562816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから甘いのよ",
              "maleVariant": "",
              "stringId": "3278228915863515136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう",
              "maleVariant": "",
              "stringId": "3278262943161348096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいのよ",
              "maleVariant": "",
              "stringId": "3278263154537492480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたにとってリスクが高いんじゃ？",
              "maleVariant": "",
              "stringId": "3278263945985880064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FIAは今大混乱に陥ってる。あんたの件を気にしてる暇はない",
              "maleVariant": "",
              "stringId": "3278267604727590912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何を知ってる？",
              "maleVariant": "",
              "stringId": "3278292195999051776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スコーピオンに",
              "maleVariant": "",
              "stringId": "3278304787282882560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スコーピオンに",
              "maleVariant": "",
              "stringId": "3278307834394406912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スコーピオンに",
              "maleVariant": "",
              "stringId": "3278308037381943296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[酒を断る] ありがとう、でもやめとく",
              "maleVariant": "[酒を断る] ありがとう、でもやめとくよ",
              "stringId": "3278311197219581952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう、でもやめとく",
              "maleVariant": "",
              "stringId": "3278311770480275456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ私はリードに。誇り高き愛国者。そして絶滅危惧種レベルの愚か者に",
              "maleVariant": "",
              "stringId": "3278312555083558912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信念を曲げずに死んだんだ",
              "maleVariant": "",
              "stringId": "3278313284758237184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうこと？",
              "maleVariant": "",
              "stringId": "3278319459344150528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リードはマイヤーズの冷酷な面をよく知っていた。それを知ってながら、利用され続けたのよ",
              "maleVariant": "リードはマイヤーズの冷酷な面をよく知っていた。それを知ってながら、利用され続けたんだ",
              "stringId": "3278319561332846592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソルは決して意志を曲げない男だった。マイヤーズとソミ、どちらのために死んだと思う？",
              "maleVariant": "",
              "stringId": "3278321362400538624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズに仕えて死んだ",
              "maleVariant": "",
              "stringId": "3278323389121785860"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リードはソミを救おうとした",
              "maleVariant": "",
              "stringId": "3278323870325895168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "両方のために死んだ",
              "maleVariant": "",
              "stringId": "3278324328561995776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミね。取りつかれてたといってもいい",
              "maleVariant": "ソミだな。取りつかれてたといってもいい",
              "stringId": "3278324926820741120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "両方よ。1人を救おうとしながら、もう1人にも忠実であろうとした",
              "maleVariant": "両方だ。1人を救おうとしながら、もう1人にも忠実であろうとした",
              "stringId": "3278324977118834692"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズね。ソミの件すら、彼女ありきだった",
              "maleVariant": "マイヤーズだ。ソミの件すら、彼女ありきだった",
              "stringId": "3278324990070845444"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… リードらしいね…",
              "maleVariant": "",
              "stringId": "3278352480847884288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "友人が言ってた。リードは自分自身を救えないから全力で他者を助けるんだって",
              "maleVariant": "",
              "stringId": "3278353622252220416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "かわいそうな奴",
              "maleVariant": "",
              "stringId": "3278354829574238208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エゴが肥大化し過ぎたのね。心も同じくらい広ければ、今も生きてただろうに",
              "maleVariant": "",
              "stringId": "3278356598580670464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼は好きだった？",
              "maleVariant": "",
              "stringId": "3278421826081222656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わからない",
              "maleVariant": "",
              "stringId": "3278422999026724864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わからない",
              "maleVariant": "",
              "stringId": "3278423837619089408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む] 信念を持つ者に",
              "maleVariant": "",
              "stringId": "3278438751456817156"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信念を持つすべての者に",
              "maleVariant": "",
              "stringId": "3278438914581688320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信念を持つ誰もに",
              "maleVariant": "",
              "stringId": "3278439079250063364"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲まない] 信念を持つ者に",
              "maleVariant": "",
              "stringId": "3278439411506049024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「長く過酷な闘病の末の死」を達成したら、組織の連中は悔しがるだろうね",
              "maleVariant": "",
              "stringId": "3278446124976140288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼の最期を聞かせてよ",
              "maleVariant": "",
              "stringId": "3278450976930230272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて、こっちも新しいお友達を探そうぜ。たっぷり仲良くなって、殺すんだ",
              "maleVariant": "",
              "stringId": "3278455466664226816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの国が新たな企業戦争をおっぱじめたわけね",
              "maleVariant": "あんたの国が新たな企業戦争をおっぱじめたわけだ",
              "stringId": "3308230067915042816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "第五次企業戦争の幕開けを見ることになるとは",
              "maleVariant": "",
              "stringId": "3308230473084809216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気がとがめないの？",
              "maleVariant": "気がとがめないのか？",
              "stringId": "3308244673370763264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、そうね",
              "maleVariant": "ああ、そうだな",
              "stringId": "3339265322967801856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "惜しい。陽射しの強い場所ではある",
              "maleVariant": "",
              "stringId": "3343157118462500864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "幸運を。気をつけてね",
              "maleVariant": "幸運を。気をつけてな",
              "stringId": "3343157665617846272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オービタル・エアーは、ずっとライバルを排除したがってた。遅かれ早かれ彼らは衝突してたわ",
              "maleVariant": "",
              "stringId": "3343207727102337024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新合衆国は、ナイトシティ国際宇宙空港での出来事に一切関与しておりません",
              "maleVariant": "",
              "stringId": "3343218041348857856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイト・コープもね",
              "maleVariant": "ナイト・コープもな",
              "stringId": "3343221247340617728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソングバードはどうだった？",
              "maleVariant": "",
              "stringId": "3343296752362364928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲まない] 信念も言い訳に過ぎない",
              "maleVariant": "",
              "stringId": "3343298128966492160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すべてを犠牲にして立場を貫くなんて、自分で判断することから逃げてるだけよ。責任を取らないための言い訳ね",
              "maleVariant": "すべてを犠牲にして立場を貫くなんて、自分で判断することから逃げてるだけだ。責任を取らないための言い訳だよ",
              "stringId": "3343299887839485952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信念を持った愚か者たちに、乾杯",
              "maleVariant": "",
              "stringId": "3343302066293874688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "かもね。でもリードは違う",
              "maleVariant": "",
              "stringId": "3343356117232558080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[待つ]",
              "maleVariant": "",
              "stringId": "3357756279271350276"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "3368051553663533056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る]",
              "maleVariant": "",
              "stringId": "3377853994936385540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "座って",
              "maleVariant": "",
              "stringId": "3378088757160222720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "来ないかと思ってた",
              "maleVariant": "",
              "stringId": "3378096294693609472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふうん、それは奇遇ね",
              "maleVariant": "",
              "stringId": "3378102239532326912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[コインを返す] 証拠としてこれを",
              "maleVariant": "",
              "stringId": "3556541737126502400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、前にもらったコインよ。証拠を求められた時に使って",
              "maleVariant": "ほら、前にもらったコインだ。証拠を求められた時に使ってくれ",
              "stringId": "3556555606163103744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふーん、自分で持っておかなくていいの？",
              "maleVariant": "",
              "stringId": "3556569275584389120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ",
              "maleVariant": "ああ",
              "stringId": "3557699738591993856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アフターライフのレジェンドにして、あたしの一番の親友",
              "maleVariant": "アフターライフのレジェンドにして、俺の一番の親友",
              "stringId": "3560769449697386496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "史上最高のロッカーボーイ。己の理想に生き、己の理想に死んだ",
              "maleVariant": "",
              "stringId": "3560776418030166016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "情熱にあふれた女性。ギリシャで日がな一日アリストテレスについて学ぶはずだったのに",
              "maleVariant": "",
              "stringId": "3560777947273404416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "朴念仁にして、命の恩人",
              "maleVariant": "",
              "stringId": "3560782988206055424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ファミリーに命を捧げた、恐れ知らずの男",
              "maleVariant": "",
              "stringId": "3560808287929016320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無謀にも夢を掴もうとした、謎多き女",
              "maleVariant": "",
              "stringId": "3560824361340805120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今日は静かね",
              "maleVariant": "今日は静かだな",
              "stringId": "3598516962168578048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今日はどうしたの？ 気分に合ったお気に入りの音楽はなし？",
              "maleVariant": "今日はどうした？ 気分に合ったお気に入りの音楽はなしか？",
              "stringId": "3598544642897256448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悲しい歌は好きじゃない。でもずっとこんな気分でいるわけじゃないよ。また踊れるようになる",
              "maleVariant": "",
              "stringId": "3598549906849439744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そして、リードにも乾杯",
              "maleVariant": "",
              "stringId": "3731945686780665856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む] リードに。残念な結果になった",
              "maleVariant": "",
              "stringId": "3731946748728107012"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む] リードに。安らかに眠って",
              "maleVariant": "[飲む] リードに。安らかに眠ってくれ",
              "stringId": "3731949979281711104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうか安らかに…",
              "maleVariant": "",
              "stringId": "3731953314860687360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、まずは乾杯しましょ",
              "maleVariant": "",
              "stringId": "3731961702931816448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残念な結果になった…",
              "maleVariant": "",
              "stringId": "3735074454385369088"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}