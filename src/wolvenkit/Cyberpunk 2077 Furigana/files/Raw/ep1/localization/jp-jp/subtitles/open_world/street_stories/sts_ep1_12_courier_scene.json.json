  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何様のつもりだ？ バーゲストの連中をカモにしてるからって、俺たちもバカにするつもりか？",
              "maleVariant": "",
              "stringId": "2759242581298241536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いやいや、テックは一級品だ。おたくの研究にもぴったりさ。ただ、少し修理が必要なだけで",
              "maleVariant": "",
              "stringId": "2759266431218081792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その前にお前の頭を修理できないくらい粉々にしてやるよ！",
              "maleVariant": "",
              "stringId": "2759268695857029120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "な、なにを…？！ ヤバい、ヤバい、俺が疑われちまう！",
              "maleVariant": "",
              "stringId": "2759282503874895872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミルコ・アレクシスを探してる",
              "maleVariant": "",
              "stringId": "2759288998083551236"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはハエも殺せないでしょ",
              "maleVariant": "あんたはハエも殺せないだろ",
              "stringId": "2759348652662710272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何が望みだ？",
              "maleVariant": "",
              "stringId": "2759349897498583040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちのセリフよ",
              "maleVariant": "こっちのセリフだ",
              "stringId": "2759351585705943040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブードゥーのミルコ・アレクシスを探してるんだけど、知ってる？",
              "maleVariant": "ブードゥーのミルコ・アレクシスを探してるんだが、知ってるか？",
              "stringId": "2759355718185902080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ… だが知ってるのは名前くらいだ。ほぼネットの住人みたいなもんだからな。周りの連中が気を使ってるところを見ると、かなりの大物らしい",
              "maleVariant": "",
              "stringId": "2759356889571123200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それが恩人にとる態度？ 質問するのはこっちよ",
              "maleVariant": "それが恩人にとる態度か？ 質問するのはこっちだ",
              "stringId": "2759363804669796352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「恩人」だと？ 逆だ！ お前のせいで俺の大事なビジネスは台無しだよ！ わかってるのか？",
              "maleVariant": "",
              "stringId": "2759364617173590016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バーゲストがこれを知ったら大事になるでしょうね…",
              "maleVariant": "バーゲストがこれを知ったら大事になるだろうな…",
              "stringId": "3091299006558040068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[殴る] さっさと教えて",
              "maleVariant": "[殴る] さっさと教えろ",
              "stringId": "3091300690386546688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "有用な情報には金を払う",
              "maleVariant": "",
              "stringId": "3091301541394051072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この恩知らず…",
              "maleVariant": "この恩知らずが…",
              "stringId": "3091343989747019776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知りたいことを教えてくれれば、あんたがバーゲストから盗みを働いてることを言わないでおいてあげる",
              "maleVariant": "知りたいことを教えてくれれば、あんたがバーゲストから盗みを働いてることを言わないでおいてやるよ",
              "stringId": "3091344737725640704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何のことだ？ 俺は何も盗んでないぞ",
              "maleVariant": "",
              "stringId": "3091346125050408960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭を修理できないくらい粉々にされたいの？",
              "maleVariant": "頭を修理できないくらい粉々にされたいのか？",
              "stringId": "3091347964772806656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ぐあっ！ このサイコ女が！",
              "maleVariant": "ぐあっ！ このサイコ野郎が！",
              "stringId": "3091348777545035776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここには金を稼ぎに来たんでしょ？",
              "maleVariant": "ここには金を稼ぎに来たんだろ？",
              "stringId": "3091349941397286912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "情報をくれれば、対価は払う",
              "maleVariant": "",
              "stringId": "3091350443791020032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "インプラントの研究とか言ってたけど、何のこと？",
              "maleVariant": "インプラントの研究とか言ってたけど、何のことだ？",
              "stringId": "3215719156295462912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこにいるかわかる？",
              "maleVariant": "どこにいるかわかるか？",
              "stringId": "3215728238591012864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "勘弁してくれ。これ以上のトラブルはごめんだ",
              "maleVariant": "",
              "stringId": "3215728505633959936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "インプラントの研究とか言ってたけど、何のこと？",
              "maleVariant": "インプラントの研究とか言ってたが、何のことだ？",
              "stringId": "3215732202527059968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあな。知らないし、知りたいとも思わない",
              "maleVariant": "",
              "stringId": "3215732933812350976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "礼くらい言えないの？",
              "maleVariant": "礼くらい言えないのか？",
              "stringId": "3215749507638161408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "交渉の仕方は心得てる。何も問題なかったんだ",
              "maleVariant": "",
              "stringId": "3215750481001902080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… ブードゥー・ボーイズの奴らは…インプラントを入手しては、コードの弱点や新しいハックを探すために解体してる。で、使わないパーツは古いボットに入れてるんだ",
              "maleVariant": "",
              "stringId": "3215766699855376384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほとんどはポンコツだが、一体だけとんでもないのができた",
              "maleVariant": "",
              "stringId": "3215768453141557248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボット？ 何のために？",
              "maleVariant": "",
              "stringId": "3215774735487528960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ま、待て、撃つな！ 俺は仕事でここにいるだけだ！",
              "maleVariant": "",
              "stringId": "3221551434758156288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕事？",
              "maleVariant": "",
              "stringId": "3221554281700716548"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしもよ。さっさと消えて",
              "maleVariant": "俺もだ。さっさと消えろ",
              "stringId": "3221554790218133504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブードゥー・ボーイズと仕事を？",
              "maleVariant": "",
              "stringId": "3221556991841857536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ… だがもう終わりだ。お前のおかげで、連中は新しいサプライヤーを探すだろう",
              "maleVariant": "",
              "stringId": "3221559648312684544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちもよ。さっさと消えて",
              "maleVariant": "こっちもだ。さっさと消えろ",
              "stringId": "3221578212738396160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、わかった。もう行くよ",
              "maleVariant": "",
              "stringId": "3221578870153605120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブードゥー・ボーイズはなんでインプラントを欲しがってるの？",
              "maleVariant": "ブードゥー・ボーイズはなぜインプラントを欲しがってるんだ？",
              "stringId": "3275476898318557184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "インプラントが欲しければ、リパーに行けばいいでしょ",
              "maleVariant": "インプラントが欲しければ、リパーに行けばいいだろ",
              "stringId": "3275479034813128704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "インストールするためのサイバーウェアってわけじゃない。何かの実験に使うらしい",
              "maleVariant": "",
              "stringId": "3275479480147550208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "相手はかなり怒ってたみたいだけど？",
              "maleVariant": "相手はかなり怒ってたようだったがな",
              "stringId": "3275559066143203328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "文句をつけてはまた取引をもちかける――それが連中のやり方なのさ",
              "maleVariant": "",
              "stringId": "3275559913677824000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ。さっき「バーゲストの連中をカモにしてる」って言ってなかったっけ？",
              "maleVariant": "へえ。さっき「バーゲストの連中をカモにしてる」って言ってなかったか？",
              "stringId": "3275596370148761600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クッ… クソ、わかったよ",
              "maleVariant": "",
              "stringId": "3275596891718852608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少し考えりゃわかるだろ。そのミルコって奴が重要なら、常時警護する必要があるってことさ",
              "maleVariant": "",
              "stringId": "3275623439297585152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう行っていいよ。あたしは仕事があるから",
              "maleVariant": "もう行っていいぞ。俺は仕事があるんでな",
              "stringId": "3275627019538702336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前が首を突っ込んでくるまではな",
              "maleVariant": "",
              "stringId": "3275629339861229568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "礼を言う筋合いはない。俺はもう消える。こんな状態の客と一緒にいるとこを見られたらおしまいだ",
              "maleVariant": "",
              "stringId": "3275631228405325824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここから離れた方がいい",
              "maleVariant": "",
              "stringId": "3283836512370073600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話ができるじゃないか",
              "maleVariant": "",
              "stringId": "3283843005572096000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人が来る前に逃げた方がいい",
              "maleVariant": "",
              "stringId": "3283943293058457600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ… 俺もお前もな",
              "maleVariant": "",
              "stringId": "3283986257545699328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ… 羨ましいね",
              "maleVariant": "",
              "stringId": "3286820589930979328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "脅しのつもりか？",
              "maleVariant": "",
              "stringId": "3292565347244613632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[殴る]",
              "maleVariant": "",
              "stringId": "3292566331479990276"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ、俺の鼻が！ めちゃくちゃいてえ！",
              "maleVariant": "",
              "stringId": "3292569012428775424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さっさと話して",
              "maleVariant": "さっさと話せ",
              "stringId": "3292569267811557376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった！ わかったからもうやめてくれ！",
              "maleVariant": "",
              "stringId": "3292569431573962752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたがパーツの供給元でしょ。ボットの倒し方も知ってるはず",
              "maleVariant": "あんたがパーツの供給元だろ。ボットの倒し方も知ってるはずだ",
              "stringId": "3373662260821553152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボットはどうやったら倒せる？",
              "maleVariant": "",
              "stringId": "3375321733942124548"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボットを回避する方法はある？",
              "maleVariant": "ボットを回避する方法はあるか？",
              "stringId": "3375324046815567872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボットを相手にしてる暇はない。回避する方法はないの？",
              "maleVariant": "ボットを相手にしてる暇はない。回避する方法はないのか？",
              "stringId": "3375324630343917588"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "銃の腕前は？",
              "maleVariant": "",
              "stringId": "3375325137150058520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フム… ヴードゥー・ボーイズの連中は、ボットをサーバー室に保管してる。俺がディスクアレイをオーバーロードし、お前がサーバーを撃ちまくれば…",
              "maleVariant": "",
              "stringId": "3375325137166835716"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そいつは最後だ。背後を取って、プロセッサーのソケットを撃てばなんとかなる",
              "maleVariant": "",
              "stringId": "3375325137166835728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "モーションセンサーがあるからな。気づかれずにすり抜けるのは無理だ",
              "maleVariant": "",
              "stringId": "3375657218417795072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこそこよ",
              "maleVariant": "そこそこだ",
              "stringId": "3376621327007260672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さっき言ってた、とんでもない奴は？",
              "maleVariant": "",
              "stringId": "3376683159990476800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "侵入者、侵入者だ！ 殺されちまう！",
              "maleVariant": "",
              "stringId": "3460642223315021824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ、さすが俺",
              "maleVariant": "",
              "stringId": "3460694754070163456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "爆発のパルスで、まとめて回路を焼き切れるかもしれない",
              "maleVariant": "",
              "stringId": "3466666273944281088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕事？ ここで？",
              "maleVariant": "",
              "stringId": "3892902710317113348"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここで何を？",
              "maleVariant": "",
              "stringId": "3894284885235032076"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここで何を？",
              "maleVariant": "ここで何してる？",
              "stringId": "3946246837355876352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんた、いったい何者なの？",
              "maleVariant": "あんた、いったい何者だ？",
              "stringId": "4026041065506390016"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}