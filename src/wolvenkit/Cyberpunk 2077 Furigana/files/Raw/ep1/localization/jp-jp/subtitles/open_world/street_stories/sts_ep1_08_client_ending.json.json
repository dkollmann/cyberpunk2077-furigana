    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アナ… 生体モニターは処分するんだ。わかるだろ",
              "maleVariant": "",
              "stringId": "3267913182460227584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしたの？",
              "maleVariant": "どうしたんだ？",
              "stringId": "3267920321786392576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[生体モニターを渡す] 好きにして",
              "maleVariant": "[生体モニターを渡す] 好きにしろ",
              "stringId": "3268061345779265540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[生体モニターを壊す] 悪いけど渡せない",
              "maleVariant": "[生体モニターを壊す] 悪いが渡せない",
              "stringId": "3268062212372803584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[生体モニターを置く] 勝手にして",
              "maleVariant": "[生体モニターを置く] 勝手にしろ",
              "stringId": "3268064358531051520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言いにくいけど… スティーブンが正しい。リスクが大きすぎる",
              "maleVariant": "言いにくいが… スティーブンが正しい。リスクが大きすぎる",
              "stringId": "3268065993168760832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら",
              "maleVariant": "ほらよ",
              "stringId": "3268066072642433024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも。データは情報局の全エージェントとメディアに送られる。でかい花火が打ちあがるわよ",
              "maleVariant": "",
              "stringId": "3268073379270995968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何が花火だ… クソ、爆発するのはこっちの方だぞ…",
              "maleVariant": "",
              "stringId": "3268082031180623872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私は諦めないから。まだ終わってない…",
              "maleVariant": "",
              "stringId": "3268087772494934016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう、V。報酬は送っておく",
              "maleVariant": "",
              "stringId": "3268088801592586240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダメよ、スティーブン！ なかったことにはさせない。絶対に",
              "maleVariant": "",
              "stringId": "3272319867895394304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼む。大人になるんだ",
              "maleVariant": "",
              "stringId": "3295384923230539776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バカなこと言わないで",
              "maleVariant": "",
              "stringId": "3295385134891896832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしたの？",
              "maleVariant": "どうしたんだ？",
              "stringId": "3295386304431624196"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[生体モニターを置く] あたしの役目は終わった",
              "maleVariant": "[生体モニターを置く] 俺の役目は終わった",
              "stringId": "3295387538496524288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今、ブラジル政府がリオで開いてるサミットに、ソヴオイルが出席してる。この意味がわかるか？",
              "maleVariant": "",
              "stringId": "3295389030712446976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼は無視して生体モニターを渡して。私たちには関係ないことよ",
              "maleVariant": "",
              "stringId": "3295389415481118720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スティーブン、説明して",
              "maleVariant": "スティーブン、説明しろ",
              "stringId": "3295390548312297476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "情報局は信号を無視した。ソヴオイルがバナを殺したと知りながら、確認は取りたくなかったんだ。あまりに“不都合”な事実だからな",
              "maleVariant": "",
              "stringId": "3295404174079266816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜそんなにムキになるの？",
              "maleVariant": "なぜそんなにムキになるんだ？",
              "stringId": "3295414459217317892"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何か考えがあるの？",
              "maleVariant": "何か考えがあるのか？",
              "stringId": "3295414864756183040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真相を明かしたところで、相手は企業や政府機関よ。諦めたほうがいい",
              "maleVariant": "真相を明かしたところで、相手は企業や政府機関だ。諦めたほうがいい",
              "stringId": "3295436511844339712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソヴオイルのしたことを公にすれば、あらゆるビジネスに支障をきたす。大物連中の機嫌も損ねるだろう",
              "maleVariant": "",
              "stringId": "3295437032273580032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、あたしはこれで",
              "maleVariant": "じゃあ、俺はこれで",
              "stringId": "3295437845045809152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "情報局なんてクソよ。バナが殺されたって言うのに、上の連中は揉み消すことしか考えてない… でも何のために？ はした金を懐に入れるため？",
              "maleVariant": "",
              "stringId": "3295444321806159872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アナ――生体データをどうするつもりなの？",
              "maleVariant": "アナ――生体データをどうするつもりなんだ？",
              "stringId": "3295448096344215552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メディアと情報局全体に真実を暴露する。このまま連中の好きにさせちゃ、バナが報われない",
              "maleVariant": "",
              "stringId": "3295448470660681728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アナ――生体モニターは破壊するんだ。バナはもういない。お前まで死ぬつもりか？",
              "maleVariant": "",
              "stringId": "3295453349458317312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とんだ腰抜けね、スティーブン。でも安心して、あなたは元の“生活”に戻ればいい。なんなら昇進できるかもね",
              "maleVariant": "",
              "stringId": "3295454560353882112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アナ、お願いだ。無茶はよせ",
              "maleVariant": "",
              "stringId": "3295455393007112192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、生体モニターを渡して。スーツどもに思い知らせてやる",
              "maleVariant": "",
              "stringId": "3295456224183947264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "諦める！？ 彼から剥ぎ取られた… この生体モニターを前にして、諦めろって言うの？",
              "maleVariant": "",
              "stringId": "3295468210212933632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "情報局に裏切られ、企業に殺されたのよ。私の大切な友人が！",
              "maleVariant": "",
              "stringId": "3295469541585686528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう、V",
              "maleVariant": "",
              "stringId": "3295476864018386944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "後悔してももう遅い",
              "maleVariant": "",
              "stringId": "3295478049009930240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… 予想通り、ブラジル政府は見て見ぬふりをする気ってことさ",
              "maleVariant": "",
              "stringId": "3317437797531750400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと待って。スティーブン、ブラジル政府やソヴオイルがバナの死とどう関係してるの？",
              "maleVariant": "ちょっと待て。スティーブン、ブラジル政府やソヴオイルがバナの死とどう関係してるんだ？",
              "stringId": "3317441855520870400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダメだ… 早く逃げないと。俺はここにいなかった――いいな？",
              "maleVariant": "",
              "stringId": "3317464357307518976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "好きにしなさいよ",
              "maleVariant": "",
              "stringId": "3317464898859274240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふう… ありがとう、V。報酬はすぐに届くはずよ",
              "maleVariant": "",
              "stringId": "3317465197627936768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんな！",
              "maleVariant": "",
              "stringId": "3317466485312819200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、じゃあね",
              "maleVariant": "ああ、じゃあな",
              "stringId": "3317493370633687040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見損なったわ",
              "maleVariant": "",
              "stringId": "3363644390077992960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何を見てるのよ？ もうさっさと出ていって！",
              "maleVariant": "",
              "stringId": "3458158220790829056"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}