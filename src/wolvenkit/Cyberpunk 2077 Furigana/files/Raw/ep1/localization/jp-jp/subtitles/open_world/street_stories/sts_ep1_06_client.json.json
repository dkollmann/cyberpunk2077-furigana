  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい！ ちょっと遊んでかないか？",
              "maleVariant": "",
              "stringId": "3153572377098899456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また今度",
              "maleVariant": "",
              "stringId": "3153577460914114564"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また今度ね。あたしはV。依頼があるって聞いて来たんだけど",
              "maleVariant": "また今度な。俺はV。依頼があるって聞いて来たんだが",
              "stringId": "3153634052594388992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エリックの詳細は？ バイク屋って言ったっけ？",
              "maleVariant": "",
              "stringId": "3153690529401065472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分の息子を陥れたの？ ろくでもない父親ね",
              "maleVariant": "自分の息子を陥れたのか？ ろくでもない父親だな",
              "stringId": "3153690864358182912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "修理屋だ。頑張ってヘイウッドに自分の店を開いた",
              "maleVariant": "",
              "stringId": "3153902301437415424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V！",
              "maleVariant": "",
              "stringId": "3153912016535998464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "証言は削除した",
              "maleVariant": "",
              "stringId": "3153912166306205700"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふう。これでひと安心だな！",
              "maleVariant": "",
              "stringId": "3153919402655010816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "検事長に、あんたの名前とデータは削除してもらった",
              "maleVariant": "",
              "stringId": "3153919493520412676"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃあ支払いを",
              "maleVariant": "",
              "stringId": "3153920184339058688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "払って",
              "maleVariant": "払え",
              "stringId": "3153920184389390336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "簡単な仕事よ。それじゃあ支払いを",
              "maleVariant": "簡単な仕事だ。それじゃあ支払いを",
              "stringId": "3153920184439721988"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "済んだよ。証言は削除された",
              "maleVariant": "",
              "stringId": "3153926824274612224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョルジーナの金で支払うつもり？",
              "maleVariant": "ジョルジーナの金で支払うつもりか？",
              "stringId": "3153928268994236416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ報酬を支払って。息子を裏切って手に入れた金があるでしょ？",
              "maleVariant": "じゃあ報酬を支払え。息子を裏切って手に入れた金があるだろ？",
              "stringId": "3153929111059484676"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは… クソ",
              "maleVariant": "",
              "stringId": "3153935414460862464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "検事長があんたの名前は消してくれた",
              "maleVariant": "",
              "stringId": "3153937132313563136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんだって？ 証言自体を削除するはずだろ！",
              "maleVariant": "",
              "stringId": "3153941667178270720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョルジーナの金があるでしょ",
              "maleVariant": "ジョルジーナの金があるだろ",
              "stringId": "3153943897105158144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知ってるのよ。払ったところであんたの金じゃないでしょ",
              "maleVariant": "知ってるぞ。払ったところであんたの金じゃないだろ",
              "stringId": "3153944217281548288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さっさとよこして。もう顔も見たくないのよ",
              "maleVariant": "さっさとよこせ。もう顔も見たくないんだ",
              "stringId": "3154966676944375808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンズに頼まれて来た",
              "maleVariant": "",
              "stringId": "3275648391047467008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイケル・マルドナド？ あたしはV。ハンズに言われて来たんだけど、手を… 貸してほしいとか",
              "maleVariant": "マイケル・マルドナドか？ 俺はV。ハンズに言われて来たんだが、手を… 貸してほしいとか",
              "stringId": "3275649799175983104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "息子のエリックが、ヘイウッドでバイク修理店を経営していてな。認可も税金もきちんとやってる真っ当な店だ",
              "maleVariant": "",
              "stringId": "3294292714439659520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その検事長は何者？",
              "maleVariant": "その検事長は何者だ？",
              "stringId": "3294297029069852672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まず聞いておきたいんだけど、その検事長は何者？",
              "maleVariant": "まず聞いておきたいんだが、その検事長は何者だ？",
              "stringId": "3294300910143229952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺はカメラの前に座らされ… 連中の指示どおりに話した。怖くて逆らえなかったんだよ",
              "maleVariant": "",
              "stringId": "3294301256106201088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分の息子を陥れる証言をしたの？",
              "maleVariant": "自分の息子を陥れる証言をしたのか？",
              "stringId": "3294323757389533184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "強要って、どんなふうに？",
              "maleVariant": "",
              "stringId": "3294324550482087936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あくどい女さ。ナイトシティの連中はみんなそうだ",
              "maleVariant": "",
              "stringId": "3295478827237855232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誤解しないでくれ。俺だって本当は… 断りたかったんだ",
              "maleVariant": "",
              "stringId": "3295516168035848192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "証言を強要されたって、具体的にどうやって？",
              "maleVariant": "",
              "stringId": "3295561605367287808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。やってみる",
              "maleVariant": "",
              "stringId": "3295598785070452740"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなことはあたしがさせない。検事長を見つけて話をしてみる",
              "maleVariant": "そんなことは俺がさせない。検事長を見つけて話をしてみる",
              "stringId": "3295599208242171904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… よかった。ハンズなら一流を寄こしてくれると思ってたよ",
              "maleVariant": "",
              "stringId": "3295599796736577536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの女と話をつけてくれ。あんた流のやり方でな。エリックはいい子だ… 刑務所に入れたくない",
              "maleVariant": "",
              "stringId": "3295600646620008448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの女はエリックについての“証拠”とやらを見せつけて、その裏付けを求めてきた。もちろんでっちあげさ",
              "maleVariant": "",
              "stringId": "3295602915319042048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時間ができたら、話しにいってみる",
              "maleVariant": "",
              "stringId": "3295610515230117888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時間のある時にやってみる",
              "maleVariant": "",
              "stringId": "3295635255550365696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "急いでくれよ。エリックの裁判はもうすぐだ。あの女と話をつけないと息子は牢屋行きになる",
              "maleVariant": "",
              "stringId": "3295643765860290560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうだった？ 俺が気持ちよく眠れる日はやって来たか？",
              "maleVariant": "",
              "stringId": "3305600426473508864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "相手があたしだけで済んで幸運だったね",
              "maleVariant": "相手が俺だけで済んで幸運だったな",
              "stringId": "3305620921117921280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、ああ、もちろんだとも",
              "maleVariant": "",
              "stringId": "3305637524236750848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "約束と違うぞ！ 報酬はなしだ！",
              "maleVariant": "",
              "stringId": "3307015151606386688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイキー！ このポンコツにまたエディーを取られたんだけど！",
              "maleVariant": "",
              "stringId": "3323111921338822656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よく来てくれた。どんな難題もこなしてくれるとフィクサーから聞いてるぞ",
              "maleVariant": "",
              "stringId": "3323207358281502720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どれくらいの難題？",
              "maleVariant": "どれくらいの難題だ？",
              "stringId": "3323212333581684736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何年も前に母親と一緒にドッグタウンを離れてな… それ以来滅多に会ってない",
              "maleVariant": "",
              "stringId": "3323242673163554816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "検事長はヘビー・ハーツにいる。腐った連中が集まるクラブだ",
              "maleVariant": "",
              "stringId": "3323247108119941120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜ居場所を？",
              "maleVariant": "",
              "stringId": "3323270454924627968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺もそこで… 心温まるもてなしをうけた",
              "maleVariant": "",
              "stringId": "3323270575737360384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その2日後、今度は警官を連れて再びやってきた。タコ殴りにされたよ",
              "maleVariant": "",
              "stringId": "3323271580994588672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ところが検事長のジョルジーナ・ゼンビンスキーという奴が息子に言いがかりをつけてきた。見せしめにしようとしてるんだ",
              "maleVariant": "",
              "stringId": "3324270150774247424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "理由もなくそんなことをする？",
              "maleVariant": "理由もなくそんなことをするか？",
              "stringId": "3324274428561674240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何かしたの？ 理由があるはずよ",
              "maleVariant": "何かしたのか？ 理由があるはずだ",
              "stringId": "3324275490576224256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "相手は検事長だぞ。理由なんてどうとでもなる",
              "maleVariant": "",
              "stringId": "3324277324191715328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたにはその証言を… 消してもらいたい",
              "maleVariant": "",
              "stringId": "3324284677242834944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エリックは純粋すぎるんだ。だから今も標的にされてる",
              "maleVariant": "",
              "stringId": "3324285618931191808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エリックのことをもっと知りたい",
              "maleVariant": "",
              "stringId": "3324291273020755968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで誠実かどうかわかるの？",
              "maleVariant": "それで誠実かどうかわかるのか？",
              "stringId": "3324296194751287296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… 初めはまともな人間に見えた。腰を下ろして普通に話してたんだが…",
              "maleVariant": "",
              "stringId": "3324300387746664448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだったか。こっちで話をしよう",
              "maleVariant": "",
              "stringId": "3347603068774862848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これじゃ遊べないよ。いつになったら修理すんの？",
              "maleVariant": "",
              "stringId": "3347606033023717376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昨日直したんだが… すまないな。次のゲームはタダでいいぞ",
              "maleVariant": "",
              "stringId": "3347611867283804160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "検事長は、ヴァレンティーノズの幹部、ヘクター・サクリスタンって奴を追ってるらしい",
              "maleVariant": "",
              "stringId": "3347854424152559616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エリックじゃなくて？",
              "maleVariant": "エリックじゃなくてか？",
              "stringId": "3347861732307849216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エリックはサクリスタンを知っていて、重要な情報を握ってるそうだ。そこで検事長は息子をムショ送りにしようと、俺に証言を強要した",
              "maleVariant": "",
              "stringId": "3347861908133072896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら… もう行けよ",
              "maleVariant": "",
              "stringId": "3368225478566543360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも",
              "maleVariant": "",
              "stringId": "3369592540785627136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "礼を言うのはこっちだ。俺の窮地を… 何より息子を救ってくれて",
              "maleVariant": "",
              "stringId": "3369592968302645248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もういいから…",
              "maleVariant": "",
              "stringId": "3369593606407278592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいえ、払ってもらう。エリックは密告者の名を知らずに済むんだから",
              "maleVariant": "いいや、払ってもらう。エリックは密告者の名を知らずに済むんだからな",
              "stringId": "3369606609437544448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、あの女は何を話した？",
              "maleVariant": "",
              "stringId": "3369607447241379840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全部よ。でも心配しなくていい。あんたの秘密は守るから",
              "maleVariant": "全部だ。だが心配しなくていい。あんたの秘密は守るからな",
              "stringId": "3369610129096134656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただし、それなりの対価は要求する",
              "maleVariant": "",
              "stringId": "3369610504100466688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "な…",
              "maleVariant": "",
              "stringId": "3369610888550371328"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}