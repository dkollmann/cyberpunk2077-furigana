        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分でやる… 力が残ってない… お願い…",
              "maleVariant": "",
              "stringId": "3047646404924018688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ソミを殺す] わかった",
              "maleVariant": "",
              "stringId": "3047646843195232256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなことはできない",
              "maleVariant": "",
              "stringId": "3047647297371246592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…わかった、ソミ",
              "maleVariant": "",
              "stringId": "3047647858585899008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ…",
              "maleVariant": "",
              "stringId": "3047652406083850240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これで… やっと… 静かに… 眠れるのね…",
              "maleVariant": "",
              "stringId": "3047653543277105152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[リードに手を貸す]",
              "maleVariant": "",
              "stringId": "3047744267347693572"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう大丈夫だ。安心しろ。これで休めるぞ",
              "maleVariant": "",
              "stringId": "3050219583248928768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手を貸せ",
              "maleVariant": "",
              "stringId": "3050228785468018688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女を、運び出そう",
              "maleVariant": "",
              "stringId": "3050236743941754880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残念ね、リード",
              "maleVariant": "残念だ、リード",
              "stringId": "3050247854434947072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残念ね、リード",
              "maleVariant": "残念だ、リード",
              "stringId": "3050250318924091396"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ自身の選択よ。あたしはそれを尊重した",
              "maleVariant": "ソミ自身の選択だ。俺はそれを尊重した",
              "stringId": "3050250494531211264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミは… 自分自身の権限で死を選んだ。あたしは最後の望みを… 尊重しただけ",
              "maleVariant": "ソミは… 自分自身の権限で死を選んだ。俺は最後の望みを… 尊重しただけだ",
              "stringId": "3050254310810980352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…おい、応答しろ！",
              "maleVariant": "",
              "stringId": "3050419802024579072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、目を離すな。俺もすぐに着く。ソミを助けるぞ",
              "maleVariant": "",
              "stringId": "3050420726415626240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞こえてる",
              "maleVariant": "",
              "stringId": "3050421489124642816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V…",
              "maleVariant": "",
              "stringId": "3050426066351820800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ終わりじゃない",
              "maleVariant": "",
              "stringId": "3050427284377702400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こうなる以外に道はなかった",
              "maleVariant": "",
              "stringId": "3050427450807685120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ終わりじゃない。もう少しの辛抱よ",
              "maleVariant": "まだ終わりじゃない。もう少しの辛抱だ",
              "stringId": "3050429972658130944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ… こうなることは予想できたでしょ…",
              "maleVariant": "ソミ… こうなることは予想できただろ…",
              "stringId": "3050430647286763520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは嘘… 顔を見れば… わかる…",
              "maleVariant": "",
              "stringId": "3050432783227686912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あきらめ… られなかった",
              "maleVariant": "",
              "stringId": "3050433906864951296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "意識が… 薄れてく…",
              "maleVariant": "",
              "stringId": "3050437243903545344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すぐに済む… それで終わる…",
              "maleVariant": "",
              "stringId": "3050448879657816064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やり直すチャンスなんて… なかった… でしょ？",
              "maleVariant": "",
              "stringId": "3050450499078590464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？ 聞こえるか？",
              "maleVariant": "",
              "stringId": "3050542911973421056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。生きてるよ",
              "maleVariant": "ああ。生きてるよ",
              "stringId": "3050545924624543744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ…",
              "maleVariant": "",
              "stringId": "3050549515368198144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今ならわかる… 失ったものは… もう戻らない…",
              "maleVariant": "",
              "stringId": "3050552778905808896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ…",
              "maleVariant": "",
              "stringId": "3050553120506703872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "間に合わなかった…",
              "maleVariant": "",
              "stringId": "3050591158414798848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ…",
              "maleVariant": "",
              "stringId": "3050595202646601728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってやって。こうするしかなかった",
              "maleVariant": "わかってやれ。こうするしかなかった",
              "stringId": "3050600536123568128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ、でも…",
              "maleVariant": "",
              "stringId": "3050652046303883264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ソミを助ける]",
              "maleVariant": "",
              "stringId": "3052716431597330436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もし断ったら？",
              "maleVariant": "",
              "stringId": "3092532043867942912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "断ったら、どうなる？",
              "maleVariant": "",
              "stringId": "3092532383573012480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブラックウォールの向こうに… 取り込まれて… 自分を探し続ける… 永遠に… 自分が誰かも忘れて…",
              "maleVariant": "",
              "stringId": "3092532665329577984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなの耐えられない。お願いよ。お願いだから、私を死なせて",
              "maleVariant": "",
              "stringId": "3092536242517241856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして！ 死ぬよりも… 残酷な… 運命… そんなのは… イヤ…",
              "maleVariant": "",
              "stringId": "3102875450334908416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[抜く]",
              "maleVariant": "",
              "stringId": "3234757700543537156"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リードが向かってる",
              "maleVariant": "",
              "stringId": "3256218718434422784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう何も言わないで",
              "maleVariant": "もう何も言うんじゃない",
              "stringId": "3256220994347659264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すぐにリードが来る。助け出すから",
              "maleVariant": "すぐにリードが来る。助け出すからな",
              "stringId": "3256223922240286720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無理よ… 彼らが… 許さない…",
              "maleVariant": "",
              "stringId": "3256224667366780928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分のものかわからない記憶を夢見て… さまよい続けるアルゴリズムになる…",
              "maleVariant": "",
              "stringId": "3256236518003634176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ソミを殺す] わかった",
              "maleVariant": "",
              "stringId": "3256241591098208256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなことはできない",
              "maleVariant": "",
              "stringId": "3256241591114985476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リードはFIAが力になると言っていた",
              "maleVariant": "",
              "stringId": "3256241591114985492"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FIAには優秀なリパーがいるってリードに聞いた。大丈夫よ",
              "maleVariant": "FIAには優秀なリパーがいるってリードに聞いた。大丈夫だ",
              "stringId": "3256243115761266688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼は… 自分を騙し続けてる…",
              "maleVariant": "",
              "stringId": "3256245073612042240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FIAと… ブラックウォールは… 飾りつけが違うだけで、同じ地獄よ。V、お願い… 死なせて...",
              "maleVariant": "",
              "stringId": "3256246105998028800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手を貸して…？",
              "maleVariant": "",
              "stringId": "3256295468459814912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[触れる] ソミ…",
              "maleVariant": "",
              "stringId": "3274994198488244228"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ…",
              "maleVariant": "",
              "stringId": "3274994876388433920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "来て… くれたのね…",
              "maleVariant": "",
              "stringId": "3292310715511271424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほかの… 道はなかったのか…",
              "maleVariant": "",
              "stringId": "3292337154423975936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "運び出そう",
              "maleVariant": "",
              "stringId": "3292339300464783372"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それも… あんたの自由だものね。わかった",
              "maleVariant": "それも… お前の自由だからな。わかった",
              "stringId": "3298552481746653184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それを決められるのはあんただけね。わかった。あたしは尊重する",
              "maleVariant": "それを決められるのはお前だけだ。わかった。俺は尊重する",
              "stringId": "3298553051417022464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう",
              "maleVariant": "",
              "stringId": "3298553548408492064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それと… ごめんなさい。何もかも",
              "maleVariant": "",
              "stringId": "3298553548425269248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダメ、それじゃ解決にならない…",
              "maleVariant": "ダメだ、それじゃ解決にならない…",
              "stringId": "3298559993627676692"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お願い… どうか、わかって…",
              "maleVariant": "",
              "stringId": "3298559993644453892"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしには… できない。あんたは殺せない",
              "maleVariant": "俺には… できない。お前は殺せない",
              "stringId": "3298559993644453908"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ、よく考えて。FIAが助けられなくても他の手はきっとある。ここで諦めちゃダメよ",
              "maleVariant": "ソミ、よく考えるんだ。FIAが助けられなくても他の手はきっとある。ここで諦めちゃダメだ",
              "stringId": "3298560864130306048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、あなたとは… 違うのよ。私は… 死ぬしかない。お願い… こんな地獄で… 生きたくない",
              "maleVariant": "",
              "stringId": "3298562519622094848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんな最期は認めない。だから、あんたを殺すことはできない",
              "maleVariant": "こんな最期は認めない。だから、お前を殺すことはできない",
              "stringId": "3298562965241729024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ… ごめんなさい。あたしには、約束があるの",
              "maleVariant": "ソミ… すまない。俺には、約束がある",
              "stringId": "3298564385198182400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "約束…？ じゃあやっぱり… リード、リードが…",
              "maleVariant": "",
              "stringId": "3298565406477651968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "違う。あんたにチャンスを与えたいの。だから頼みは聞けない。できないの、絶対に",
              "maleVariant": "違う。お前にチャンスを与えたいんだ。だから頼みは聞けない。できないんだ、絶対に",
              "stringId": "3298565602754301952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしもすべてを失った。でも、どん底から這い上がって、戦ってる。あたしは勝つまで諦めない",
              "maleVariant": "俺もすべてを失った。だが、どん底から這い上がって、戦ってる。俺は勝つまで諦めない",
              "stringId": "3344546610410614784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vは… わかってない…",
              "maleVariant": "",
              "stringId": "3344549761171779584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ…",
              "maleVariant": "ああ…",
              "stringId": "3344562801564905472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行くぞ",
              "maleVariant": "",
              "stringId": "3344563738371092480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "終わらせて",
              "maleVariant": "",
              "stringId": "3356372392304898048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、手を貸してくれ…",
              "maleVariant": "",
              "stringId": "3359185671986831360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お願い、V…",
              "maleVariant": "",
              "stringId": "3359189554620489728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V…",
              "maleVariant": "",
              "stringId": "3359190173582319616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう時間が…",
              "maleVariant": "",
              "stringId": "3359190423613169664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ… それじゃ、だめよ… 何も解決しない！",
              "maleVariant": "ソミ… それじゃ、だめだ… 何も解決しない！",
              "stringId": "3360487956686237696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お願いよ… V…",
              "maleVariant": "",
              "stringId": "3360489228650868736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他に… 手はないの…",
              "maleVariant": "",
              "stringId": "3360489619341897728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ソミを殺す] わかった",
              "maleVariant": "",
              "stringId": "3360490234864398340"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ソミを殺さない] 断る",
              "maleVariant": "",
              "stringId": "3360490495532003328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ！",
              "maleVariant": "",
              "stringId": "3363290860363116544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしたちには神経マトリックスという希望がある",
              "maleVariant": "俺たちには神経マトリックスという希望がある",
              "stringId": "3541813331407073280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい、あたしたちには神経マトリックスがある。それを使って治療するの。2人とも… 助かるのよ…",
              "maleVariant": "いいか、俺たちには神経マトリックスがある。それを使って治療するんだ。2人とも… きっと助かる…",
              "stringId": "3541814667410337792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "てお… くれよ… あたしはもう… ブラックウォールに… 蝕まれすぎた…",
              "maleVariant": "",
              "stringId": "3541816293592330240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死が… ただひとつの希望… それしかない… お願い、V…",
              "maleVariant": "",
              "stringId": "3541817841793822720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ソミを殺さない] あたしにはFIAの助けがいる。死なせるわけにいかない",
              "maleVariant": "[ソミを殺さない] 俺にはFIAの助けがいる。死なせるわけにいかない",
              "stringId": "3541830823634464768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダメ… それはできない。あたしには神経マトリックスが必要なの",
              "maleVariant": "ダメだ… それはできない。俺には神経マトリックスが必要なんだ",
              "stringId": "3541832422217936896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私が死んでから… 使えばいいでしょ… お願い…",
              "maleVariant": "",
              "stringId": "3541835607590498304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ、本当にごめんなさい… あんたを生きて連れ戻すことが、FIAとの取引条件なのよ。そうするしかないの",
              "maleVariant": "ソミ、本当にすまない… お前を生きて連れ戻すことが、FIAとの取引条件なんだ。そうするしかない",
              "stringId": "3541836669907038208"
            },
