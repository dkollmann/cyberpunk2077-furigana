  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティともおさらばね",
              "maleVariant": "ナイトシティともおさらばだな",
              "stringId": "3114464713326964740"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さらばナイトシティ、ね",
              "maleVariant": "さらばナイトシティ、か",
              "stringId": "3114465151229079556"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手術の手配が整ったら、連絡する",
              "maleVariant": "",
              "stringId": "3114664433131433984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "上からD.C.へ移るよう言われた",
              "maleVariant": "",
              "stringId": "3121495671057063936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたを頼ってマイヤーズに差し出されることを恐れたのよ",
              "maleVariant": "お前を頼ってマイヤーズに差し出されることを恐れたんだ",
              "stringId": "3282882510949019648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "車で行くなら長旅ね",
              "maleVariant": "車で行くなら長旅だな",
              "stringId": "3505624410927570944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "列車とは相性が悪いんだ…",
              "maleVariant": "",
              "stringId": "3505624622874140672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "車の薬は補充した？",
              "maleVariant": "車の薬は補充したか？",
              "stringId": "3505637321616039936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。10年かかったよ…",
              "maleVariant": "",
              "stringId": "3506858171623186432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一度あんたを裏切って、会わす顔がなかった",
              "maleVariant": "一度お前を裏切って、会わす顔がなかった",
              "stringId": "3506862114235392000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは騙せても、あんたは騙せないことがわかってた",
              "maleVariant": "俺は騙せても、お前は騙せないことがわかってた",
              "stringId": "3506862508164423680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "似た境遇だから、共感してくれると思ったのよ",
              "maleVariant": "似た境遇だから、共感してくれると思ったんだろう",
              "stringId": "3506862774603390976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズ側の人間という懸念が、ぬぐい切れなかったんでしょうね",
              "maleVariant": "マイヤーズ側の人間という懸念が、ぬぐい切れなかったんだろうな",
              "stringId": "3506863916343271424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "哀れな野郎だ…",
              "maleVariant": "",
              "stringId": "3506896011946008576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "するならとっくの昔にしてる",
              "maleVariant": "",
              "stringId": "3506896436308910080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴は文字通りすべてを失った。自分じゃまだ気づいてないようだがな",
              "maleVariant": "",
              "stringId": "3506896859514183680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっと来たか。もう出発するところだった",
              "maleVariant": "",
              "stringId": "3507208108546367488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それにゆっくり考える時間も欲しかった。色んなことがあったからな",
              "maleVariant": "",
              "stringId": "3509848602246029312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど。ソロモン・リードの自分探しの旅ね",
              "maleVariant": "なるほど。ソロモン・リードの自分探しの旅か",
              "stringId": "3509850872941551616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このドライブが懺悔の道ってわけ？",
              "maleVariant": "このドライブが懺悔の道ってわけか？",
              "stringId": "3509870523088019456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "答えを見つけるための、典型的な自分探しの旅って感じね",
              "maleVariant": "答えを見つけるための、典型的な自分探しの旅って感じだな",
              "stringId": "3509877967675629568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分への罰のつもり？ 懺悔の道を行きながら安モーテルで感慨にふけるわけ？",
              "maleVariant": "自分への罰のつもりか？ 懺悔の道を行きながら安モーテルで感慨にふけるとか？",
              "stringId": "3509878221582016512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、急に呼び出したりして、どうしたの？",
              "maleVariant": "で、急に呼び出したりして、どうしたんだ？",
              "stringId": "3509887043193184256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミは7年前、あんたが死ぬよう仕向けた。会わす顔がなかったんでしょ…",
              "maleVariant": "ソミは7年前、お前が死ぬよう仕向けた。会わす顔がなかったんだろう…",
              "stringId": "3509945960547917824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは騙せないとわかってたのよ。近い存在だったし、彼女のことをよく知ってた",
              "maleVariant": "お前は騙せないとわかってたんだろう。近い存在だったし、彼女のことをよく知ってた",
              "stringId": "3509946662456303616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしなら彼女の気持ちがわかると思ったのよ。死刑宣告を受けた者どうしね…",
              "maleVariant": "俺なら彼女の気持ちがわかると思ったんだろ。死刑宣告を受けた者どうしな…",
              "stringId": "3509946924935847936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "急いでるの？",
              "maleVariant": "急いでるのか？",
              "stringId": "3509962512764674048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ずいぶん急に発つのね。ホロで言ってよ",
              "maleVariant": "ずいぶん急に発つんだな。ホロで言えよ",
              "stringId": "3509963298005491712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "入れ違いになればいい、そう思ってたのかもしれん",
              "maleVariant": "",
              "stringId": "3509968671647113216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "同情してるの？ あんたが？",
              "maleVariant": "同情してるのか？ お前が？",
              "stringId": "3510016339308093440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう遠くないうちに首でも吊るんじゃないか",
              "maleVariant": "",
              "stringId": "3510017204274241536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふざけないで",
              "maleVariant": "ふざけるな",
              "stringId": "3510017744030834688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "祖国の大統領は悪党同然",
              "maleVariant": "",
              "stringId": "3510023475262038016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "生きていけねえだろ",
              "maleVariant": "",
              "stringId": "3510025372966494208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、笑えない冗談はよしてよ、ジョニー",
              "maleVariant": "おい、笑えない冗談はよせよ、ジョニー",
              "stringId": "3510025608568938496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "するなら7年の間にしてるでしょ。今のリードにはやり直すチャンスがある",
              "maleVariant": "するなら7年の間にしてるだろ。今のリードにはやり直すチャンスがある",
              "stringId": "3510025980519817216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは困る。あたしを助けると約束したのに",
              "maleVariant": "それは困る。俺を助けると約束したからな",
              "stringId": "3510032331014840320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手術の手配を済ませてからにしてほしいね…",
              "maleVariant": "手術の手配を済ませてからにしてほしいがな…",
              "stringId": "3510033181921681408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "元気でな、V",
              "maleVariant": "",
              "stringId": "3510060757943754752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あれからずっと眠れずに、自分に問い続けてることがある… だから… お前の意見を聞いてみたかった",
              "maleVariant": "",
              "stringId": "3510075603514990592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "車にまだ予備のアスピリンはある？",
              "maleVariant": "車にまだ予備のアスピリンはあるか？",
              "stringId": "3510088841761824768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「路上、黒のメリマックの助手席、3分後」だったっけ",
              "maleVariant": "",
              "stringId": "3510089336354152448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "古い習慣は抜けなくてな",
              "maleVariant": "",
              "stringId": "3510095069883834368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうとも限らない。こうしてお前を呼び出したしな",
              "maleVariant": "",
              "stringId": "3510146418684108800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少なくとも俺は聖人じゃない",
              "maleVariant": "",
              "stringId": "3510146897958838272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7年間、一度も連絡しようとしなかったんでしょ",
              "maleVariant": "7年間、一度も連絡しようとしなかったんだろ",
              "stringId": "3510173107023683588"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は指示に逆らってまで、ソミを助けた",
              "maleVariant": "",
              "stringId": "3510174013714767872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういう命令だったんだ。その後の俺の苦労についても、あいつのせいだとは思わない",
              "maleVariant": "",
              "stringId": "3510175716618969088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうか？ 昔一緒に働いていた頃とは別人になっていたがな",
              "maleVariant": "",
              "stringId": "3510177409305845760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は死刑を執行されたようなものだ。ソミも覚えていたはず",
              "maleVariant": "",
              "stringId": "3510228912724942848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは7年も姿を消して、彼女をほったらかしにした",
              "maleVariant": "お前は7年も姿を消して、彼女をほったらかしにした",
              "stringId": "3510229498954424320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしには自分の考えしか言えない。明確な答えはないのよ",
              "maleVariant": "俺には自分の考えしか言えない。明確な答えはないさ",
              "stringId": "3510229591094894592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分で本人に聞くべきよ",
              "maleVariant": "自分で本人に聞くんだな",
              "stringId": "3510235162724442112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい加減諦めたらどうなの",
              "maleVariant": "いい加減諦めたらどうなんだ",
              "stringId": "3510236022539984900"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これ以上に誠意を示す方法があったか？",
              "maleVariant": "",
              "stringId": "3510504419139952640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うかつに接触できなかったんだ。俺にも、自分の役目があった",
              "maleVariant": "",
              "stringId": "3510517125330935808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたが反逆者じゃないってことに、ソミは気づいてたのに",
              "maleVariant": "お前が反逆者じゃないってことに、ソミは気づいてたのに",
              "stringId": "3510518048379805696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺としては、あいつを守るためだった",
              "maleVariant": "",
              "stringId": "3510519336182128640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分で本人に聞いたらどう？ D.C.に行くんでしょ。彼女はそこにいるはず…",
              "maleVariant": "自分で本人に聞いたらどうだ？ D.C.に行くんだろ。彼女はそこにいるはず…",
              "stringId": "3510521484672409600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの何が問題かわかる？ 諦めが悪いところよ",
              "maleVariant": "お前の何が問題かわかるか？ 諦めが悪いところだよ",
              "stringId": "3510526174659256320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、その機会はなさそうだ…",
              "maleVariant": "",
              "stringId": "3510527366714007552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "率直な意見に感謝する、V",
              "maleVariant": "",
              "stringId": "3510545732430372864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "後悔してるなら、それを抱えて生きるしかない",
              "maleVariant": "",
              "stringId": "3510554859101659136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "選択を誤ったのかもしれないけど、あたしが決めることじゃない",
              "maleVariant": "選択を誤ったのかもしれないが、俺が決めることじゃない",
              "stringId": "3510559894850150400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“友達”からは中指を突き立てられ",
              "maleVariant": "",
              "stringId": "3537533618962735104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "抱いた理想は下水の底でクソまみれ",
              "maleVariant": "",
              "stringId": "3537534349174284288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「路上、黒のメリマック」…",
              "maleVariant": "",
              "stringId": "3546190058405404672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミがスペースフォース・ワンから… 俺でなく、お前にホロを掛けた理由だ",
              "maleVariant": "",
              "stringId": "3546217175251853312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そろそろ行くよ",
              "maleVariant": "",
              "stringId": "3546239854457561088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうかもな。なんせ金も希望もないんだ",
              "maleVariant": "",
              "stringId": "3546245305911910400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫さ、高潔な奴だからな。そこが一番の問題だが",
              "maleVariant": "",
              "stringId": "3546247247153242112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "助言があるとしたら、どうにもできないことで、自分自身を苦しめるのはやめることね",
              "maleVariant": "助言があるとしたら、どうにもできないことで、自分自身を苦しめるのはやめるんだな",
              "stringId": "3547734741967347712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふむ",
              "maleVariant": "",
              "stringId": "3547737253499805696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもひとつ確かなのは… その結果を受け止めて生きるしかない… ってこと",
              "maleVariant": "だがひとつ確かなのは… その結果を受け止めて生きるしかない… ってことだ",
              "stringId": "3547742768606912512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これ以上ソングバードのことを考えたって無駄よ",
              "maleVariant": "これ以上ソングバードのことを考えたって無駄だ",
              "stringId": "3550778646979846144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "理由がわからない？ あんたはソミが感情で動いてると思ってるだろうけど、見当外れ",
              "maleVariant": "理由がわからないか？ お前はソミが感情で動いてると思ってるだろうが、見当外れだ",
              "stringId": "3550813253645279232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すべては策略。あたしたちは利用された",
              "maleVariant": "すべては策略。俺たちは利用された",
              "stringId": "3550816184994013184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "感情じゃなく、現実的な可能性を追い求めただけ",
              "maleVariant": "",
              "stringId": "3550821261074931712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "友情や信頼、忠誠心の問題でもない",
              "maleVariant": "",
              "stringId": "3550839969801478144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "苛立つ気持ちはわかる。だが俺たちも純粋な被害者ではない",
              "maleVariant": "",
              "stringId": "3550843274728480768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "記憶力は大丈夫そうだな",
              "maleVariant": "",
              "stringId": "3550895161154641920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの時、俺がホロを受けていたら、正しい選択をしてやれただろうか",
              "maleVariant": "",
              "stringId": "3552000041726418944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リード、あんたはできるだけのことをやった。ソングバードのことは忘れなって",
              "maleVariant": "リード、お前はできるだけのことをやった。ソングバードのことは忘れろって",
              "stringId": "3552030547000074240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今度は、自分のことを考えないと。人生をやり直す機会なんて、なかなか得られるもんじゃない",
              "maleVariant": "",
              "stringId": "3552035547432525824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソングバードには会うの？",
              "maleVariant": "ソングバードには会うのか？",
              "stringId": "3576617609460097024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミの手術は？ 会いに行くの？",
              "maleVariant": "ソミの手術は？ 会いに行くのか？",
              "stringId": "3576618243152322560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
