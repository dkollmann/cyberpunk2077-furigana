  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バブスは小声で勇気づけてくれたけど、正直、ユーリの代わりに俺が殺されるんだと思ったよ",
              "maleVariant": "",
              "stringId": "2960048727131590656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バブスは俺がやりすぎたって言うんだが、あんなうますぎる話は見逃せなかったんだって",
              "maleVariant": "",
              "stringId": "2960054624507564032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "続きを聞くから、ダイブをちょうだい",
              "maleVariant": "続きを聞くから、ダイブをよこせ",
              "stringId": "2960054819844689920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうだ、ついてきてるか？",
              "maleVariant": "",
              "stringId": "3041742075596189696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。こっちも気分が乗ってきた",
              "maleVariant": "ああ。こっちも気分が乗ってきた",
              "stringId": "3041743813816430596"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんなのが人生最高の日？",
              "maleVariant": "",
              "stringId": "3041744254570672128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "幻覚がリアルすぎて、あたしも一戦交えたくなってきたよ",
              "maleVariant": "幻覚がリアルすぎて、俺も一戦交えたくなってきたよ",
              "stringId": "3043146509266006016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ… あれだけボコボコにされた日が、人生最高だったって？",
              "maleVariant": "なあ… あれだけボコボコにされた日が、人生最高だったって？",
              "stringId": "3043146713427947520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "殴られたのは、一人前になるための儀式だよ",
              "maleVariant": "",
              "stringId": "3043156959827406848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どう意味をこじつけたって、リンチはリンチでしょ。顔面に銃弾を撃ち込まれても、儀式だって納得できる？",
              "maleVariant": "",
              "stringId": "3043159677384077312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "勘弁してくれ。まだ傷が癒えてないんだ！",
              "maleVariant": "",
              "stringId": "3043180748913061888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V。まだ終わりじゃないの。こっからが、仕事の話なんだから",
              "maleVariant": "",
              "stringId": "3043181044158509056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話のオチが見えてきた",
              "maleVariant": "",
              "stringId": "3043184034898587652"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何を運ぼうとしてたの？",
              "maleVariant": "何を運ぼうとしてたんだ？",
              "stringId": "3043184927144488960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何を運んでたの？",
              "maleVariant": "何を運んでたんだ？",
              "stringId": "3043191035527507968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも時々、自分用に拝借してた。最初は出来心だったんだが… 欲張っちまって、ピンチに陥ったんだ",
              "maleVariant": "",
              "stringId": "3043191647963000832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほっとけよ。ハンセンから直々に仕事を任されたことがないから、嫉妬してるんだろ",
              "maleVariant": "",
              "stringId": "3073542939910418432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最悪だろ？ 俺はそこでおろおろ成り行きを眺めてたんだ",
              "maleVariant": "",
              "stringId": "3229542813381595136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その後は重い沈黙。そりゃそうだろ、2人は軍にいた頃からの友人だ。そろってミリテクに捨てられたんだよな",
              "maleVariant": "",
              "stringId": "3229545977614864384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "貴様、名前は？",
              "maleVariant": "",
              "stringId": "3229565280070307864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パ… パコ・トーレスです！",
              "maleVariant": "",
              "stringId": "3229565280070307876"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "貴様の名は野良犬だ。だが、立派な猟犬にしてやる",
              "maleVariant": "",
              "stringId": "3229566073783619676"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "は… はい！",
              "maleVariant": "",
              "stringId": "3229566073800396800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前は何の役に立つ？",
              "maleVariant": "",
              "stringId": "3229570958134513712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すばしっこいし… 頭も切れます！",
              "maleVariant": "",
              "stringId": "3229570958134513724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この時、俺はまるで… 生まれ変わったみたいだった。顔は血まみれだったけど、嬉しくて叫び出しそうになったよ！",
              "maleVariant": "",
              "stringId": "3229571677709307944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "警察本部長の件も新米レベルの仕事だった。私の代理を任せたのに、今じゃNCPDの半分に筒抜けだ",
              "maleVariant": "",
              "stringId": "3262498752338145280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっとパコ、話を盛らないで！",
              "maleVariant": "",
              "stringId": "3262514255442030592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シー、邪魔すんな！",
              "maleVariant": "",
              "stringId": "3262515070009421824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "試してやろう！",
              "maleVariant": "",
              "stringId": "3262516997057228800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、初めての仕事だが、まあありがちなタイプだった。 俺は輸送隊の護衛を任されたんだ",
              "maleVariant": "",
              "stringId": "3262521063049080832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スカベンジャーに渡す物資さ。普段から薬や水、発電機を運んでる。ここに住んでる連中のためにな",
              "maleVariant": "",
              "stringId": "3262523047105867776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話のオチが見えてきた。その仕事で大失敗したんでしょ",
              "maleVariant": "話のオチが見えてきた。その仕事で大失敗したんだろ",
              "stringId": "3262524734692470784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっとパコはリンチにあって吊るされる。輸送隊はスカベンジャーに渡す予定の物資を運んでたんだけど、厄介なことになったの",
              "maleVariant": "",
              "stringId": "3262526929370427392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ユーリと話をした後、カートはイライラしてて、檻の中の虎みたいに歩き回ってた",
              "maleVariant": "",
              "stringId": "3289177703403917312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カートは謎に包まれた男だ。あいつはその場を歩き回ってた。一言も発さずにな",
              "maleVariant": "",
              "stringId": "3289179132889829384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は… きっとこれはテストだと思ったんだ。だってそうだろ？ だからずっと黙って、ひたすら待った",
              "maleVariant": "",
              "stringId": "3289179620536389640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで黙ってこっち見ててさ、どうやっても敵わねえと思ったよ",
              "maleVariant": "",
              "stringId": "3289179987756093448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさか俺のパンチがヒットするなんて… 興奮したよ！",
              "maleVariant": "",
              "stringId": "3289180423091294216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何発かお見舞いしてやった！",
              "maleVariant": "",
              "stringId": "3289180881008627720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいパンチ持ってやがってさ！ 思い出すだけで痛え…",
              "maleVariant": "",
              "stringId": "3289181555352047624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本気で私を殴ってみろ",
              "maleVariant": "",
              "stringId": "3294177069457813508"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ガードしてみろ、小僧",
              "maleVariant": "",
              "stringId": "3294177761903849472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "かかってこい",
              "maleVariant": "",
              "stringId": "3294179356561453056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ガードしてみろ、小僧！ さあ！",
              "maleVariant": "",
              "stringId": "3294180066992664576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "と、いうと？",
              "maleVariant": "",
              "stringId": "3294181450760663040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ、本気で殴ってみろ！",
              "maleVariant": "",
              "stringId": "3294181716042002432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつは、使える男だ",
              "maleVariant": "",
              "stringId": "3294247476487598080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、もう一度痛めつけたほうが…",
              "maleVariant": "",
              "stringId": "3294248183076827136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、そのうちな",
              "maleVariant": "",
              "stringId": "3294248627706605568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[パコに手を貸す] 群れへようこそ",
              "maleVariant": "",
              "stringId": "3294250690482089988"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "群れへようこそ、パコ",
              "maleVariant": "",
              "stringId": "3294261316784828416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハハハハ！ そんなこと言うわけないでしょ！",
              "maleVariant": "",
              "stringId": "3294261856591753216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "当然だろ、落ち着けよ。こんなのは新米でもやれる",
              "maleVariant": "",
              "stringId": "3302787348370370560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話がしたいと無線で言ってたな。要件は？",
              "maleVariant": "",
              "stringId": "3302818407325708288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[寄りかかる] ドッグタウンのスカベンジャーとの取引はどうなってる？",
              "maleVariant": "",
              "stringId": "3302824508561633280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[寄りかかる] 今度はしくじるな。失敗は許されない",
              "maleVariant": "",
              "stringId": "3302826409151770624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前がしくじったスカベンジャーとの取引はどうなってる",
              "maleVariant": "",
              "stringId": "3302911575635341312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スカベンジャーの件、滞りなく進めろよ。決してしくじるな。私の目を見るんだ",
              "maleVariant": "",
              "stringId": "3302911755470319616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ、何のことだ",
              "maleVariant": "",
              "stringId": "3302912388004917248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カート、どれだけ長い付き合いだと思ってる？",
              "maleVariant": "",
              "stringId": "3302925329999671296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "次の輸送はお前に任せる。実力を見せてもらうぞ",
              "maleVariant": "",
              "stringId": "3303164060606214144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、よこして",
              "maleVariant": "わかった、よこせ",
              "stringId": "3307997901095342088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は訳もわからず、待つしかなかった。正直、チビってたよ",
              "maleVariant": "",
              "stringId": "3335346998856814620"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて、トーレス、覚悟はいいか？",
              "maleVariant": "",
              "stringId": "3338337707679453184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大佐にやられたら歯も残らないぜ",
              "maleVariant": "",
              "stringId": "3338339559112347648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大佐、このクソガキをボコボコにしてやってください！  やっちまえ！",
              "maleVariant": "",
              "stringId": "3338339778994540544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パコ！ 顔を上げて… 死なないでよ！ いい！？",
              "maleVariant": "",
              "stringId": "3338357947628949504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうした、腰抜け！ やり返してみろ！",
              "maleVariant": "",
              "stringId": "3338362102288388096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いけー！ 顔だ、顔をやっちまえ！",
              "maleVariant": "",
              "stringId": "3338362144147542020"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブハハハ！ おいどうした、泣きそうじゃねえか！ ハハハー！",
              "maleVariant": "",
              "stringId": "3338362592199872516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセン大佐の相手が務まるわけないだろ！",
              "maleVariant": "",
              "stringId": "3338378130921787400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "異常ありません。全て順調です",
              "maleVariant": "",
              "stringId": "3338392187225673728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うむ。ご苦労",
              "maleVariant": "",
              "stringId": "3338396052864012288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "警察の件も、新米がやれる仕事だった",
              "maleVariant": "",
              "stringId": "3338762769532268548"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信頼を取り戻すまで、いつまでも新米の仕事をしていろ",
              "maleVariant": "",
              "stringId": "3338764149022076928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もううんざりだ。自分が役に立つと証明できるまで、雑用でもしてろ",
              "maleVariant": "",
              "stringId": "3338777892565102592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中はやる気だ。備えが必要、わかるな？",
              "maleVariant": "",
              "stringId": "3344797757985861632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カート、どうした？",
              "maleVariant": "",
              "stringId": "3357272199718670336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの… 俺はどうすれば？",
              "maleVariant": "",
              "stringId": "3357276173184176136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺はガクブル状態でさ。しばらく動けなかったよ",
              "maleVariant": "",
              "stringId": "3357278250002190344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞きたいだろ？",
              "maleVariant": "",
              "stringId": "3357280665132765192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今の見ただろ？ ユーリを無視してる。よっぽどのことをやらかしたんだろうな",
              "maleVariant": "",
              "stringId": "3368168150436057088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ディープダイブを吸う] 続きを聞かせて",
              "maleVariant": "[ディープダイブを吸う] 続きを聞かせてくれ",
              "stringId": "3665405690425806852"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち去る]",
              "maleVariant": "",
              "stringId": "3853689102787756032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] じゃあね",
              "maleVariant": "[立ち上がる] じゃあな",
              "stringId": "3873905308077473792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ディープダイブを吸う] よこして",
              "maleVariant": "[ディープダイブを吸う] よこせ",
              "stringId": "3873978218150445068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブチギレ・カート・ハンセンだ",
              "maleVariant": "",
              "stringId": "3895511492891848704"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}