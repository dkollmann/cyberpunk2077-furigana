      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フム… この弾痕はおそらく… 50口径？ MK.31 HMGか",
              "maleVariant": "",
              "stringId": "3348864492264329216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "強く殴りつけた跡…？",
              "maleVariant": "",
              "stringId": "3348865172479774720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "犯人は身体改造をしまくって、頭がおかしくなっちまった殺戮マシンだな",
              "maleVariant": "",
              "stringId": "3348865752921116672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "威力もそうだが、重量もヘビー級の武器だ。サイバーウェアなしの生身で運用するのはまず無理だな",
              "maleVariant": "",
              "stringId": "3349070166973845504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "元ミリテク海兵隊のラインダーが選びそうな武器ね。他の弾丸はスキャンで検出できない。つまり、単独での犯行ということ…",
              "maleVariant": "元ミリテク海兵隊のラインダーが選びそうな武器だな。他の弾丸はスキャンで検出できない。つまり、単独での犯行ということだ…",
              "stringId": "3349070439637159936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コンバットブーツの足跡… 走ってこの小屋の屋根に飛び上がり、手すりに脚をぶつけたのか… へこみ具合から見ると、体重が360ポンドはありそうだけど… とんでもないジャンプ力ね",
              "maleVariant": "コンバットブーツの足跡だ… 走ってこの小屋の屋根に飛び上がり、手すりに脚をぶつけたのか… へこみ具合から見ると、体重が360ポンドはありそうだが… とんでもないジャンプ力だな",
              "stringId": "3349071375050838016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまりこのラインダーって奴は… 走り高跳びが得意なデブか、大腿筋まで改造してるヘビーボーグ野郎ってことか",
              "maleVariant": "",
              "stringId": "3349071656538968064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "拳でじゃねえな。くぼみと血を見ろ。こりゃ誰かの頭を叩きつけたんだ。何度もな",
              "maleVariant": "",
              "stringId": "3349075197823389696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正気の沙汰じゃない…",
              "maleVariant": "",
              "stringId": "3349075872082923520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "同意見よ",
              "maleVariant": "同意見だ",
              "stringId": "3349078061308284928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コンバットブーツ…",
              "maleVariant": "",
              "stringId": "3359364033289351172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大口径の弾痕…",
              "maleVariant": "",
              "stringId": "3359381655624052736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[くぼみを調べる]",
              "maleVariant": "",
              "stringId": "3359418088455892992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どっちでしょうね…",
              "maleVariant": "どっちだろうな…",
              "stringId": "3378427835383648256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コレクションの隠し場所？",
              "maleVariant": "コレクションの隠し場所か？",
              "stringId": "3378435966746595332"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いろんなタイプの弾丸を集めてたようね。でもなぜ？",
              "maleVariant": "いろんなタイプの弾丸を集めてたようだな。だがなぜ？",
              "stringId": "3378447657832124416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドローンに何か記録されてるかも。センサーは無事みたい",
              "maleVariant": "ドローンに何か記録されてるかもな。センサーは無事のようだ",
              "stringId": "3378448801467830272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドローンに何か記録されてるかも",
              "maleVariant": "",
              "stringId": "3378449298559963140"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ガキの頃、銃撃戦があると、地面に落ちてる薬莢を拾いにいったもんだ。そのあと、みんなで交換し合うのさ。きっと子供のもんだろ",
              "maleVariant": "",
              "stringId": "3378454090233516032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインダーの手がかりにはならないけど、あんたの子供時代が聞けただけで調べた甲斐があったね",
              "maleVariant": "ラインダーの手がかりにはならないが、お前の子供時代が聞けただけで調べた甲斐があったな",
              "stringId": "3378454987026042880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただのゴミね",
              "maleVariant": "ただのゴミだな",
              "stringId": "3378458807634333696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、さっさとスキャンしろよ",
              "maleVariant": "",
              "stringId": "3378462010018992128"
            }
          ]
        }
      }
    },
