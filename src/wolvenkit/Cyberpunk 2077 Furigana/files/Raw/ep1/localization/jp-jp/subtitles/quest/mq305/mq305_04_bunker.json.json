  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早く、これを見てみて",
              "maleVariant": "",
              "stringId": "3233518984948203520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "助かったわ、V。残りのファイルは自分で落とすから、あなたはもう行っていいわよ",
              "maleVariant": "",
              "stringId": "3234867741598007296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうこと？",
              "maleVariant": "どういうことだ？",
              "stringId": "3236500862261116928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おせっかいな隣人ってわけじゃなかったのね。一体何者なの？",
              "maleVariant": "おせっかいな隣人ってわけじゃなかったのか。一体何者なんだ？",
              "stringId": "3236501646377861120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつは何者なの？",
              "maleVariant": "こいつは何者なんだ？",
              "stringId": "3236501712496869376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いったい何の話？",
              "maleVariant": "いったい何の話だ？",
              "stringId": "3237783234067677184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうことなの？",
              "maleVariant": "どういうことなんだ？",
              "stringId": "3237794194153127936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミリテクは科学者たちを殺し、今度は私の番だって脅したいのよ",
              "maleVariant": "",
              "stringId": "3237794352966254592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この情報を公にしなければ、連中は関係者を皆殺しにする",
              "maleVariant": "",
              "stringId": "3237794782194548736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何の話？",
              "maleVariant": "何の話だ？",
              "stringId": "3237799203997597696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリーの目当ては金さ。それだけじゃない。そいつのせいで、プロジェクトに関わった全員が始末される",
              "maleVariant": "",
              "stringId": "3237799427688218624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただの脅し文句よ。信じないで",
              "maleVariant": "",
              "stringId": "3237801262494892032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よく考えろ！ こいつを庇って一体何になる？",
              "maleVariant": "",
              "stringId": "3237810504190771200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それが仕事よ",
              "maleVariant": "それが仕事だ",
              "stringId": "3242154767359295496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリー！ 隠れて！",
              "maleVariant": "ブリー！ 隠れろ！",
              "stringId": "3242169906078826496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを取る] これは？",
              "maleVariant": "",
              "stringId": "3245147341322305540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、これで終わり？",
              "maleVariant": "で、これで終わりか？",
              "stringId": "3245150790365593600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、これで終わり？",
              "maleVariant": "じゃあ、これで終わりか？",
              "stringId": "3245152594235080704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "終わったよ",
              "maleVariant": "終わったぞ",
              "stringId": "3245184617393799168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "終わったよ",
              "maleVariant": "終わったぞ",
              "stringId": "3245184952652906496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう、V。本当に",
              "maleVariant": "",
              "stringId": "3245186309560582144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "満足した？",
              "maleVariant": "満足したか？",
              "stringId": "3246232660985626624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "教えてよ――企業のために人を殺すのはどんな気分？",
              "maleVariant": "教えてくれ――企業のために人を殺すのはどんな気分だ？",
              "stringId": "3246234307468050432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこかで聞いたようなセリフだな。もう少し工夫したらどうだ",
              "maleVariant": "",
              "stringId": "3246234766744338432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前は下がってろ",
              "maleVariant": "",
              "stringId": "3256295650090434560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつは何者なの？",
              "maleVariant": "こいつは何者なんだ？",
              "stringId": "3256369680931766272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] お目当てのものは手に入ったでしょ",
              "maleVariant": "[立ち上がる] お目当てのものは手に入っただろ",
              "stringId": "3257826046343004160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しょせん大昔の話でしょ",
              "maleVariant": "しょせん大昔の話だろ",
              "stringId": "3265390153733095424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大したネタでもないし、しょせん大昔の話でしょ",
              "maleVariant": "大したネタでもないし、しょせん大昔の話だろ",
              "stringId": "3265392087458234368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "は？ ソウルキラーのことは知ってるでしょ？ これは、言わばソウルキラーのミリテク版みたいなもの",
              "maleVariant": "",
              "stringId": "3265392382435246080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少し大げさにしすぎじゃない？",
              "maleVariant": "少し大げさにしすぎじゃないか？",
              "stringId": "3265392793493815296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブラックウォールのAIと接触を試みた人はたくさんいる。驚くような話？",
              "maleVariant": "ブラックウォールのAIと接触を試みた人はたくさんいる。驚くような話か？",
              "stringId": "3265393135614803968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "壁に穴をあけようとしてるヴードゥー・ボーイズとは話が違う。世界でもっとも強力な巨大企業のひとつが、AI兵器を開発しようとしてるのよ",
              "maleVariant": "",
              "stringId": "3265396411383005184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "中立であるはずのネットウォッチが企業を特別扱いしてると世間が知ったら… 大騒ぎになるわ",
              "maleVariant": "",
              "stringId": "3265396933942951936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "目当てのものを手に入れたなら、もう行きましょ",
              "maleVariant": "目当てのものを手に入れたなら、もう行くぞ",
              "stringId": "3265399379524173824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "巻き添えはごめんだろ？ ん？",
              "maleVariant": "",
              "stringId": "3265524486989004800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "逆だ。お前のせいでミハレスは死んだ。それも何のために？ 金か？",
              "maleVariant": "",
              "stringId": "3265533450669637632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すっかり騙されたのね。まさか金目当てだったとは",
              "maleVariant": "すっかり騙されたのか。まさか金目当てだったとは",
              "stringId": "3265713843154829312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ。世のためにやってると思ってたのに",
              "maleVariant": "クソ。世のためにやってると思ってたんだが",
              "stringId": "3265715795284574208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "分け前がないと割に合わない",
              "maleVariant": "",
              "stringId": "3265719890938544128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "データはどうするの",
              "maleVariant": "データはどうするんだ",
              "stringId": "3265745915068137472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "データはどうするの？ あたしも記録を見た。ミリテクがブラックウォールを突破しようとしてたんでしょ",
              "maleVariant": "データはどうするんだ？ 俺も記録を見た。ミリテクがブラックウォールを突破しようとしてたんだろ",
              "stringId": "3265746124615565312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうもしない――都合の悪いことは忘れ、元の生活に戻るだけだ",
              "maleVariant": "",
              "stringId": "3265746503478657024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダンテの言ってたことは本当なの？",
              "maleVariant": "ダンテの言ってたことは本当なのか？",
              "stringId": "3265751718558810112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼が言ってたこと… 本当なの？",
              "maleVariant": "彼が言ってたこと… 本当なのか？",
              "stringId": "3265754025476341760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ついさっき私たちを殺そうとした男の言葉に耳を貸すつもり？",
              "maleVariant": "",
              "stringId": "3265754393736232960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから… 生き方を変えることにしたのよ。最初は怖かったけど、今は満足してる",
              "maleVariant": "",
              "stringId": "3265754945421426688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かに、ゴミみたいな企業のPR記事は書いた。生活のためにね",
              "maleVariant": "",
              "stringId": "3265755431943913472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ところで、迷惑料くらいくれてもいいんじゃない？",
              "maleVariant": "ところで、迷惑料くらいくれてもいいんじゃないか？",
              "stringId": "3265786225097818112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうこと？",
              "maleVariant": "どういうことだ？",
              "stringId": "3266270132368457740"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ、一番古いヤツは暗号化されてる。いや、でも…",
              "maleVariant": "",
              "stringId": "3302464817398956048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新しいのもある。日付は2068年",
              "maleVariant": "",
              "stringId": "3302464817398956064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっぱり戻ってきてたのね。ほら、見てみて",
              "maleVariant": "",
              "stringId": "3302464817398956080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさかここまでとは… ミリテクが不良AIを使って古い兵器を改良しようとしてたなんて… 大スクープよ！",
              "maleVariant": "",
              "stringId": "3302556597897281540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダンテ？！",
              "maleVariant": "",
              "stringId": "3302583774403510284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やあ、ブリー",
              "maleVariant": "",
              "stringId": "3302583774403510308"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信じられない。第三次企業戦争のファイルまである",
              "maleVariant": "",
              "stringId": "3302963012349050880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。当時ミリテクは必死だった。なんとかアラサカの連中にぶっちぎられないようにな",
              "maleVariant": "",
              "stringId": "3303034857505013760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリーを探していたところ、お前が現れた。お前を追えば彼女のもとに案内してくれると思ってな。感謝する",
              "maleVariant": "",
              "stringId": "3303081381932941312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そいつを庇う必要はないぞ。色々と吹き込まれたかもしれんが、すべてデタラメだ",
              "maleVariant": "",
              "stringId": "3303171827703070720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたみたいな連中に命を狙われてるから、あたしを雇ったって聞いたけど",
              "maleVariant": "あんたみたいな連中に命を狙われてるから、俺を雇ったって聞いたが",
              "stringId": "3303172003075309568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリーと直接話せばいい",
              "maleVariant": "",
              "stringId": "3303179461420236804"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたに追われてるって聞いた",
              "maleVariant": "",
              "stringId": "3303180409685262336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ、笑わせる。メディアの人間だとも言われたか？",
              "maleVariant": "",
              "stringId": "3303181358990475264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリーと直接話せばいいでしょ",
              "maleVariant": "ブリーと直接話せばいいだろ",
              "stringId": "3303182928230596608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話して何になる？ またウソをつかれるだけさ。それが彼女の特技だ",
              "maleVariant": "",
              "stringId": "3303189068322553856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私を信じて。すべては真実のためだった",
              "maleVariant": "",
              "stringId": "3303190324969586688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やれやれ… 見解の相違だな。言っておくが、お前の考えはお見通しだ、ブリー",
              "maleVariant": "",
              "stringId": "3303190589999267840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早く、ダンテを殺して！",
              "maleVariant": "",
              "stringId": "3303201386355867648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダンテ・カルーソ、ミリテクの特殊エージェント。二流だけどね",
              "maleVariant": "",
              "stringId": "3304468035880992768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "観念しろ、ブリー",
              "maleVariant": "",
              "stringId": "3304468140772147200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうやってここに？",
              "maleVariant": "",
              "stringId": "3304468444490088448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちなみに、お前の情報源、シーザー・ミハレス――奴はすでに殺されたぞ",
              "maleVariant": "",
              "stringId": "3304479085690994688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪いな、ブリー。もはや弁論の余地はない",
              "maleVariant": "",
              "stringId": "3304559476909129728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、お願い！",
              "maleVariant": "",
              "stringId": "3304559771584151552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいえ",
              "maleVariant": "いや",
              "stringId": "3304569450209067012"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信じたあたしがバカだった",
              "maleVariant": "信じた俺がバカだった",
              "stringId": "3304569626520829952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "地獄に落ちる者に一切の善意がないとは限らない。こうやって気づけただけでも儲けもんさ",
              "maleVariant": "",
              "stringId": "3304573716487323648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前は慈善でこの仕事を受けたのか？ 違うだろう",
              "maleVariant": "",
              "stringId": "3304576281186779136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最後にひとつ。協力してくれた礼だ。受け取れ",
              "maleVariant": "",
              "stringId": "3304580374038953984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん… まあ、それくらいはいいだろう",
              "maleVariant": "",
              "stringId": "3304580825899712512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ではさっさと消えろ。俺は迷子のデータを“家”に帰す",
              "maleVariant": "",
              "stringId": "3304581139113558016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あと、覚えておけ。人に期待するのは結構だが、決して信頼はするな",
              "maleVariant": "",
              "stringId": "3304581653234565120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダンテとは知り合いなの？",
              "maleVariant": "ダンテとは知り合いなのか？",
              "stringId": "3304635125208301568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも私は生き延びた。せっかくチャンスをもらったんだから、活かさないと",
              "maleVariant": "",
              "stringId": "3304642550267232256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっと私のような人を大勢殺してきたんでしょう",
              "maleVariant": "",
              "stringId": "3304643246370701312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これからどうするつもり？",
              "maleVariant": "これからどうするつもりだ？",
              "stringId": "3304652713082601472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "記事が楽しみね",
              "maleVariant": "記事が楽しみだ",
              "stringId": "3304652907262099456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これからどうするの？",
              "maleVariant": "これからどうするんだ？",
              "stringId": "3304655101352853504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "記事を楽しみにしてる",
              "maleVariant": "",
              "stringId": "3304655211327504384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "記事を公開したら、バカンスに行くわ。どこか遠いところ――誰にも… 特に企業のネズミ共に見つからないようなところにね",
              "maleVariant": "",
              "stringId": "3304655542526525440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。仕上がったら真っ先に知らせる",
              "maleVariant": "",
              "stringId": "3304656402140741632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "終わったらバカンスに行くわ。どこか遠いところ――企業のネズミ共に見つからないようなところにね",
              "maleVariant": "",
              "stringId": "3304656698275381248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、これは？",
              "maleVariant": "",
              "stringId": "3306820562592612368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ネットウォッチとのやり取りだ。ブリーは研究所のデータを連中に売り、金を受け取ったら逃げるつもりだったのさ。で、どうだ？",
              "maleVariant": "",
              "stringId": "3306820562592612384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ彼女に同情するか？",
              "maleVariant": "",
              "stringId": "3306820562609389568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、自分の目で確かめてみろ",
              "maleVariant": "",
              "stringId": "3306847454490148904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女は、ミリテクの競合相手の醜聞を漁る程度の仕事しかしたことがない",
              "maleVariant": "",
              "stringId": "3307416925479485440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は別に、人に感謝されるためにやっているわけではない",
              "maleVariant": "",
              "stringId": "3313124884335411200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やはり… ミハレスを売ったのは彼女だったのか",
              "maleVariant": "",
              "stringId": "3313125471051431936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "賢明な判断だ",
              "maleVariant": "",
              "stringId": "3313127298576793600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリーがこの情報を公にしていれば、大勢が巻き込まれていた。現代の魔女狩りだ",
              "maleVariant": "",
              "stringId": "3313127934433280000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミリテクは研究を続けるつもり？",
              "maleVariant": "ミリテクは研究を続けるつもりか？",
              "stringId": "3313131818241343488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミリテクはプロジェクトを再開させ、研究を続けるの？",
              "maleVariant": "ミリテクはプロジェクトを再開させ、研究を続けるのか？",
              "stringId": "3313132676177838080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本音を言えば… 俺が死んでからにしてほしいが",
              "maleVariant": "",
              "stringId": "3313133244740911104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "かもな。だがすぐにというわけではないだろう。つまり我々が気にする必要はない",
              "maleVariant": "",
              "stringId": "3313133331059687424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも、気づいたのよ。ゴミだったのはその生活自体で、人生を無駄にしてるって",
              "maleVariant": "",
              "stringId": "3313134747643920384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "我々のために記事を書いてたことは聞いたか？",
              "maleVariant": "",
              "stringId": "3313137225236070400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マックス・ジョーンズの次はブリー・ホイットニーか… どうやらお前は、メディアの人間とは相性が悪いみたいだな",
              "maleVariant": "",
              "stringId": "3325673450001547264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "プロジェクトを再開させようとしてたのね",
              "maleVariant": "",
              "stringId": "3325848854821748736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞いた？ ブラックウォールを突破しようとしてたのよ",
              "maleVariant": "",
              "stringId": "3325850151465664512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "世界を変える代物よ――それもコーポの連中の都合のいいようにね",
              "maleVariant": "",
              "stringId": "3329327230404902912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダンテ・カルーソ、ミリテクの特殊エージェント。邪魔をするな。俺の目的はブリーだ。お前は関係ない",
              "maleVariant": "",
              "stringId": "3329335732359774208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリーを探していたところ、お前が現れた。お前を追えば彼女のもとに案内してくれると思ってな。感謝する",
              "maleVariant": "",
              "stringId": "3329336886548348928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ある日思い出して眠れなくなったら、その時は… そうだな、寝返りをうてばいい",
              "maleVariant": "",
              "stringId": "3329363077191811072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて、俺は迷子のデータを“家”に帰す。達者でな",
              "maleVariant": "",
              "stringId": "3329365325657522176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お似合いの2人ね",
              "maleVariant": "お似合いの2人だな",
              "stringId": "3337823739224113152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほどね。コーポの秘密と闇を暴くとか、もっともらしいことを言ってたけど、結局ただの同僚同士のいざこざだったわけ？",
              "maleVariant": "なるほどな。コーポの秘密と闇を暴くとか、もっともらしいことを言ってたが、結局ただの同僚同士のいざこざだったわけか",
              "stringId": "3337824466466095104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ち、違う。仕事を受けたのは、ミリテクのアーカイブにアクセスするため",
              "maleVariant": "",
              "stringId": "3337837685268135936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フッ… ようやく理解が追いついたようだな。そう、ブリーはミリテクに雇われていたのさ",
              "maleVariant": "",
              "stringId": "3337843880708128768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリー、本当なの？",
              "maleVariant": "ブリー、本当なのか？",
              "stringId": "3337844013617233920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当なの？",
              "maleVariant": "本当なのか？",
              "stringId": "3337846538118479872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中のアーカイブにアクセスするために、仕方なくよ",
              "maleVariant": "",
              "stringId": "3337847093914091520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気持ちはわかる。あたしも元コーポだから。最初は辛かったけど、今は離れてよかったと思ってる",
              "maleVariant": "気持ちはわかる。俺も元コーポだからな。最初は辛かったが、今は離れてよかったと思ってる",
              "stringId": "3337851633124098048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気持ちはわかる",
              "maleVariant": "",
              "stringId": "3337852250139770880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう。とにかく、これでやっと自分のために生きることができる",
              "maleVariant": "",
              "stringId": "3337852927620530176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずはバカンスに行こうかな。またコーポの連中に追われるのはごめんだからね",
              "maleVariant": "",
              "stringId": "3337853253031411712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうせ昇進を邪魔されたとか、そんな話でしょ",
              "maleVariant": "どうせ昇進を邪魔されたとか、そんな話だろ",
              "stringId": "3337958869448114176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミリテクが秘密兵器を持ってるって噂は大昔からあったぜ。他にも、「火曜日に世界が終わる」とか、「レイシィ・バートモスが冷凍庫で冬眠してる」とかな",
              "maleVariant": "",
              "stringId": "3343414497029242880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウソばっかり",
              "maleVariant": "",
              "stringId": "3344943067685830656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中のアーカイブにアクセスするために仕事を受けただけよ",
              "maleVariant": "",
              "stringId": "3344945082864353280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつとはあんたのアパートで会った。何者なの？",
              "maleVariant": "こいつとはあんたのアパートで会った。何者なんだ？",
              "stringId": "3357798775724818432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何者だったの？",
              "maleVariant": "何者だったんだ？",
              "stringId": "3357799192034656256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これでいいんでしょ",
              "maleVariant": "これでいいんだろ",
              "stringId": "3357799479008935936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダンテとは――元々知り合い？",
              "maleVariant": "ダンテとは――元々知り合いか？",
              "stringId": "3357803153168908288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別に。彼は腐るほどいるミリテクの殺し屋の一人ってだけ。企業を裏切った人間を狩る猟犬よ",
              "maleVariant": "",
              "stringId": "3357805225490628608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼まれた仕事をこなしただけ。相手が誰だろうが関係ない",
              "maleVariant": "頼まれた仕事をこなしただけだ。相手が誰だろうが関係ない",
              "stringId": "3357807802756218880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別に誰でもないわ。腐るほどいる企業の殺し屋の一人よ",
              "maleVariant": "",
              "stringId": "3357809871185625088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつはなぜここに？",
              "maleVariant": "",
              "stringId": "3357812386056749060"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕事をしただけ",
              "maleVariant": "仕事をしただけだ",
              "stringId": "3357812869324455936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミリテクの特殊エージェントか。本当に企業を怒らせたのね",
              "maleVariant": "ミリテクの特殊エージェントか。本当に企業を怒らせたんだな",
              "stringId": "3357814919852572672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この情報を葬ろうと、企業が直接刺客を送ってきたのよ",
              "maleVariant": "",
              "stringId": "3357825756373827584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「REC_09/25/68.vid」を再生する",
              "maleVariant": "",
              "stringId": "3388479328236486660"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「REC_10/31/68.vid」を再生する",
              "maleVariant": "",
              "stringId": "3388485201369382912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「REC_11/11/68.vid」を再生する",
              "maleVariant": "",
              "stringId": "3388485364259373056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしかしてネットウォッチから逃げてた…？",
              "maleVariant": "",
              "stringId": "3388518847203655680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私は事実しか言ってない。そっちこそ、どんなウソをついてここに忍び込んだの？",
              "maleVariant": "",
              "stringId": "3389702912745783296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "事実ね… 我々のために記事を書いていたことも、正直に言ったのか？",
              "maleVariant": "",
              "stringId": "3389708779419451392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "男が言ってたバンカー… ここはサイノシュア計画の研究施設だったのね。他にもあるんでしょうけど",
              "maleVariant": "男が言ってたバンカー… ここはサイノシュア計画の研究施設だったんだな。他にもあるんだろうが",
              "stringId": "3389786619594784768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サイノシュア計画… 聞き覚えがある",
              "maleVariant": "",
              "stringId": "3389787160626446336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "間違いないわ。巨額が投資された研究よ。ドッグタウンの地下に、とんでもない量のテックが隠されてる可能性が出てきた",
              "maleVariant": "",
              "stringId": "3389788862658232320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "次から次へと忙しい女だな。今は誰に雇われてるんだ？ アラサカ？ それともネットウォッチか？",
              "maleVariant": "",
              "stringId": "3434953850120945664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ回収、回収、回収… 重要データを回収…",
              "maleVariant": "",
              "stringId": "3446378943074123776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何なの...",
              "maleVariant": "何なんだ...",
              "stringId": "3855054377593417728"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}