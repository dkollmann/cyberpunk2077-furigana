  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで、要件は？",
              "maleVariant": "",
              "stringId": "3292606539053887492"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うまくいってるようね",
              "maleVariant": "うまくいってるようだな",
              "stringId": "3302726556265181184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい暮らしをしてるのね… ここが新しい家？",
              "maleVariant": "いい暮らしをしてるんだな… ここが新しい家か？",
              "stringId": "3302731920679333888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。コーポ・プラザのペントハウスとまではいかないが、そこそこ気に入ってる。一生ローンを払い続ける必要もないしな。ハハ",
              "maleVariant": "",
              "stringId": "3302733213045059584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そろそろ教えて。なんであたしを呼びつけたの？",
              "maleVariant": "そろそろ教えろ。なんで俺を呼びつけたんだ？",
              "stringId": "3302734340423643136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "礼がしたかったんだ。あんたは命の恩人だ。だけど僕は… 色々と伝えそびれていたというか…",
              "maleVariant": "",
              "stringId": "3302738051308941312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やり直すのは大変じゃない？",
              "maleVariant": "やり直すのは大変じゃないか？",
              "stringId": "3302755426582360064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今は何をしてるの？",
              "maleVariant": "今は何をしてるんだ？",
              "stringId": "3302755670388862976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう、ハサン",
              "maleVariant": "",
              "stringId": "3302766873290960896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、どう？ 人生をやり直すのは大変でしょ？",
              "maleVariant": "で、どうだ？ 人生をやり直すのは大変だろ？",
              "stringId": "3302769826097754112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最近は何をしてるの？",
              "maleVariant": "最近は何をしてるんだ？",
              "stringId": "3302769887519141888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直に言うと、ゼータテク時代と変わらない",
              "maleVariant": "",
              "stringId": "3302771322205990912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハサン？",
              "maleVariant": "",
              "stringId": "3302779516886151172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハサン？",
              "maleVariant": "",
              "stringId": "3302779608372310016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え、誰だ？ ハサンなんて奴は知らん！",
              "maleVariant": "",
              "stringId": "3302784314549170176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゼータテクも… スカベンジャーも… 試作品も全部クソ食らえ…",
              "maleVariant": "",
              "stringId": "3302788829448990720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰か、オプティクスを！ オプティクスくれないか？ キロシじゃなくていい…",
              "maleVariant": "",
              "stringId": "3302791164233478152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ… 絶対許さないぞ。僕を誰だと思ってるんだ…",
              "maleVariant": "",
              "stringId": "3302791189902618624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "助けが必要？",
              "maleVariant": "助けが必要か？",
              "stringId": "3302799086216769540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何があったの？",
              "maleVariant": "何があったんだ？",
              "stringId": "3302799207650258944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "酷い姿ね。何があったの？",
              "maleVariant": "酷い姿だな。何があった？",
              "stringId": "3302799814415052800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何って、片目をえぐり取られたんだよ！ 今や基本的なインプラントさえ買えない",
              "maleVariant": "",
              "stringId": "3302801816608010240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リパーの所へ行ったら、気がついたときは瀕死の状態で片目がなくなってた… 何もかも失ったんだ！",
              "maleVariant": "",
              "stringId": "3302833281202876416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "えっと… 助けが必要？",
              "maleVariant": "あー… 助けが必要か？",
              "stringId": "3302835019557335040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "必要だとも。まずは僕の体を切り刻んだ奴を探し出してくれ！",
              "maleVariant": "",
              "stringId": "3302836164468109312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実際、迷ったのよ。下手に動くと、企業に見つかるかもしれない。やられたままでいるような連中じゃないしね",
              "maleVariant": "実際、迷ったんだ。下手に動くと、企業に見つかるかもしれない。やられたままでいるような連中じゃないしな",
              "stringId": "3302935058487173120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "たらればはよせよ。僕もスカベンジャーに殺されてたかもしれないし、言い出したらキリがない",
              "maleVariant": "",
              "stringId": "3302965440263741440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭金はあたしが払ったようなもんよ。少しは感謝してもらわないと",
              "maleVariant": "頭金は俺が払ったようなもんだぞ。少しは感謝してもらわないと",
              "stringId": "3302974276890517504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう",
              "maleVariant": "",
              "stringId": "3302979067003789316"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "せっかくだけど… あんたも今は色々と入用でしょ？",
              "maleVariant": "せっかくだが… あんたも今は色々と入用だろ？",
              "stringId": "3302979354397499392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もらえない",
              "maleVariant": "",
              "stringId": "3302979552821633024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いやいや、こちらこそ",
              "maleVariant": "",
              "stringId": "3302980012601237504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気に入らないなら、スタジアムで売ればいい。とにかくもらってほしいんだ",
              "maleVariant": "",
              "stringId": "3302980853223645184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この銃は？",
              "maleVariant": "",
              "stringId": "3302986898893430784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふーん、見たことないモデルね…",
              "maleVariant": "ふーん、見たことないモデルだな…",
              "stringId": "3302987173838446592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、僕が開発したものさ。多少ウソをついたかもしれないが、エンジニアだというのは本当なんだ",
              "maleVariant": "",
              "stringId": "3302987480190410752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、これで貸し借りなしだ",
              "maleVariant": "",
              "stringId": "3302992014233034752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはどうかな",
              "maleVariant": "",
              "stringId": "3302992334224875520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはまあ… ドッグタウンは楽園とは程遠いからな。でも、企業から生きて逃げ延びたんだ。ここでも十分やっていけるさ",
              "maleVariant": "",
              "stringId": "3303014392254160896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人を騙して、嘲笑ってるってこと？",
              "maleVariant": "人を騙して、嘲笑ってるってことか？",
              "stringId": "3303016073129877504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハハ。武器やインプラントの修理と改造をしてるってことさ。依頼があれば何でもやる。ここのテックは古いし、部品も安物だが…",
              "maleVariant": "",
              "stringId": "3303016563108470784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "せき立てるスーツがいない分、よっぽどマシだ",
              "maleVariant": "",
              "stringId": "3303017616130433024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何だ、あんたか。何の用だ？",
              "maleVariant": "",
              "stringId": "3304491940712538112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただ通りすがっただけ",
              "maleVariant": "ただ通りすがっただけだ",
              "stringId": "3307355135529705472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "通りがかっただけなんだけど…",
              "maleVariant": "通りがかっただけなんだが…",
              "stringId": "3307355672132182016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、さっさと通り抜けろ。ほら、行けよ！",
              "maleVariant": "",
              "stringId": "3307356328440094720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フン… つまり、騙してたってことでしょ",
              "maleVariant": "フン… つまり、騙してたってことだろ",
              "stringId": "3359309295069945856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあな。でも… 才能はきちんと評価してくれる。求められるスキルさえあれば、やり直すにはもってこいの場所だ",
              "maleVariant": "",
              "stringId": "3359329242340909056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "元コーポを嫌う人も多いんじゃない？",
              "maleVariant": "元コーポを嫌う人も多いんじゃないか？",
              "stringId": "3359347508383834112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スタジアムでかるーく腕前を見せたら… 出資してもらえることになってね。それが記念すべき一作目だ。是非受け取ってくれ",
              "maleVariant": "",
              "stringId": "3363815224566079488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "テックには買い手がついてるって言ってなかった？",
              "maleVariant": "テックには買い手がついてるって言ってなかったか？",
              "stringId": "3363835175377035264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、いたさ。そいつにまんまと騙されたんだよ",
              "maleVariant": "",
              "stringId": "3363835506508947456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で… その次はゼータテクだ。あとスカベンジャーも！ 僕とあんたで、前みたいに… いいぞ…",
              "maleVariant": "",
              "stringId": "3363844530822438912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…もう諦めた方がいい、ハサン。せっかく救った命を、そんな風に捨ててほしくない",
              "maleVariant": "",
              "stringId": "3363845944051228672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうか… あんたも奴らの仲間だな！ フィクサーの飼い犬が。僕に近寄るな！ あんたに用はない！",
              "maleVariant": "",
              "stringId": "3363847994545790976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜゼータテクに戻らなかったの？",
              "maleVariant": "なぜゼータテクに戻らなかったんだ？",
              "stringId": "3366393273719447552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って… ゼータテクに戻るんじゃなかったの？",
              "maleVariant": "待て… ゼータテクに戻るんじゃなかったのか？",
              "stringId": "3366397246178320400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハサン、ウソはもういい。最初からインプラントを返す気はなかったんでしょ？",
              "maleVariant": "ハサン、ウソはもういい。最初からインプラントを返す気はなかったんだろ",
              "stringId": "3366397246195097600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしを騙したでしょ",
              "maleVariant": "俺を騙しただろ",
              "stringId": "3366399832201617408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… あたしを騙したでしょ。何があったのか知らないけど、自業自得よ",
              "maleVariant": "はあ… 俺を騙しただろ。何があったのか知らないが、自業自得だ",
              "stringId": "3366400624404979724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "も… 戻りたかったが、できなかった。連中に襲われて… その、試作品を奪われたんだ！",
              "maleVariant": "",
              "stringId": "3366401716819845120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あれにどれほどの価値があったかわかるか？ 新しい人生を始めるのに、金が必要だったんだよ！",
              "maleVariant": "",
              "stringId": "3366406548809048064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "取られたのは本当だ。気がついたときは瀕死の状態で片目がなくなってた。何もかも失ったんだ。 クソ！",
              "maleVariant": "",
              "stringId": "3366544780687302656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おお、遅かったな！ 来ないんじゃないかと思ってた",
              "maleVariant": "",
              "stringId": "3366576597888917504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、あー… まあ、その通りだ。だから埋め合わせがしたくて… これを",
              "maleVariant": "",
              "stringId": "3367868368711311360"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}