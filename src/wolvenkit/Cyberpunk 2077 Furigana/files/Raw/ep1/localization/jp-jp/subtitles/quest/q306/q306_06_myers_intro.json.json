        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "民間人が避難してる",
              "maleVariant": "",
              "stringId": "3002803697359499272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[持ち上げる] 中へ！",
              "maleVariant": "",
              "stringId": "3002807043105468420"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OKよ",
              "maleVariant": "",
              "stringId": "3031669045647093780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行こう",
              "maleVariant": "",
              "stringId": "3031669047358369804"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って。この信号… 新合衆国の周波数よ",
              "maleVariant": "",
              "stringId": "3031669048599883788"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここへ来る… あと、45秒",
              "maleVariant": "",
              "stringId": "3031669048599883796"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "静かに行動して。これ以上のヘマは許されない",
              "maleVariant": "",
              "stringId": "3031669050848030732"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "着陸するわ",
              "maleVariant": "",
              "stringId": "3031734156931870720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "我々はどうすれば？",
              "maleVariant": "",
              "stringId": "3031758574408151040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞いたろ",
              "maleVariant": "",
              "stringId": "3031791368647716864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フム。総力をぶつけてくる気ね",
              "maleVariant": "フム。総力をぶつけてくる気だな",
              "stringId": "3033251429374996480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "武器よ",
              "maleVariant": "",
              "stringId": "3052640490720047104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ところで、下では見事だったわ。亡霊みたいにすり抜けた。プロね",
              "maleVariant": "",
              "stringId": "3052642205972279296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リードの言葉はハッタリじゃなかった",
              "maleVariant": "",
              "stringId": "3052644508544512004"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソッ、リードの言ったとおり、FIAが来た",
              "maleVariant": "",
              "stringId": "3052644803655741440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いったい何が起きてるの？",
              "maleVariant": "いったい何が起きてるんだ？",
              "stringId": "3052645028118114304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中は何を？",
              "maleVariant": "",
              "stringId": "3052646231749144576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "攻撃ヘリが4機来る。信号を偽装してた。所属を知られたくないのよ",
              "maleVariant": "",
              "stringId": "3052648467229290496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで？ ここからどうする？",
              "maleVariant": "",
              "stringId": "3091358915562549248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭を使う",
              "maleVariant": "",
              "stringId": "3091364463032905728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "準備できたら言って",
              "maleVariant": "準備できたら言え",
              "stringId": "3091369952688975872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっぱり銃がないと",
              "maleVariant": "",
              "stringId": "3093105053156225024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シッ。静かに",
              "maleVariant": "",
              "stringId": "3093114429254381568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V！",
              "maleVariant": "",
              "stringId": "3093138955447021568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ！ 中へ！",
              "maleVariant": "",
              "stringId": "3093143129148600320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新合衆国の戦闘ヘリがナイトシティに… いよいよヤバそうね",
              "maleVariant": "新合衆国の戦闘ヘリがナイトシティに… いよいよヤバそうだな",
              "stringId": "3093150969930166272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V！",
              "maleVariant": "",
              "stringId": "3093175753636294656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "潜入がバレた者は少数です。誰一人として、口を割りはしないでしょう",
              "maleVariant": "",
              "stringId": "3093204697219575808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仮にここを解体して部品と共に帰っても、誰も気づかないでしょう",
              "maleVariant": "",
              "stringId": "3093205298749878276"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オービタル・エアーはどこまで？",
              "maleVariant": "",
              "stringId": "3093205840167415808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大統領",
              "maleVariant": "",
              "stringId": "3093212553704615936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここNCXにVがいるのは間違いありません",
              "maleVariant": "",
              "stringId": "3093213768257613824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vはまだ見つかっていないようです",
              "maleVariant": "",
              "stringId": "3093216116983967744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソングバードは？",
              "maleVariant": "",
              "stringId": "3093218354544488448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "衰弱して動き回れない状態かと。恐らくどこかに潜伏中と思われます",
              "maleVariant": "",
              "stringId": "3093218851032641536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残りはソングバードを捜索中です",
              "maleVariant": "",
              "stringId": "3093220469379674112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "作戦中のミスについては後で聞く",
              "maleVariant": "",
              "stringId": "3093221136106237952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうか。では総力戦だ。パーティーを始めよう",
              "maleVariant": "",
              "stringId": "3093221355099238400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うちのエージェントは？",
              "maleVariant": "",
              "stringId": "3093222134753243136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オービタル・エアーにはすでにブリーチできたか？",
              "maleVariant": "",
              "stringId": "3093222418657292288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから、こちらから連れて来てやったんだ",
              "maleVariant": "",
              "stringId": "3093225212499963904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいや。すでに収拾がつかない状況だ。いい加減認めろ",
              "maleVariant": "",
              "stringId": "3093225610052874240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "事態は完全に掌握できています",
              "maleVariant": "",
              "stringId": "3093226160546885632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前はソミのことを気にかけてるだろう",
              "maleVariant": "",
              "stringId": "3093242619482320916"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "守りたいのはわかる。だがお前の務めは、何よりもまず国に仕えることだ",
              "maleVariant": "",
              "stringId": "3093242619482320924"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女が関わっていたプロジェクト… あれを表に出すわけにはいかない",
              "maleVariant": "",
              "stringId": "3093246652825710592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これがマイヤーズの… 裏の顔",
              "maleVariant": "",
              "stringId": "3093252471717867520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいか、私とて心苦しいのだ。彼女を良く思い、信頼していたからな。だが取り返しのつかない選択をしてしまった",
              "maleVariant": "",
              "stringId": "3093270464845819904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの性悪女…",
              "maleVariant": "",
              "stringId": "3093967642660679680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズはとんだ性悪ね",
              "maleVariant": "マイヤーズはとんだ性悪だな",
              "stringId": "3093968275396603904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずい… これじゃあ完全におしまいよ",
              "maleVariant": "",
              "stringId": "3093969310634717184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "途中までしか屋上に上がれなかった。そっちまで登るのに手を貸して",
              "maleVariant": "",
              "stringId": "3101677951474114560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だって、仕方なかったでしょ！",
              "maleVariant": "だって、仕方なかったろ！",
              "stringId": "3101724269005414400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたのおかげよ",
              "maleVariant": "あんたのおかげさ",
              "stringId": "3101733147508121600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズ？",
              "maleVariant": "",
              "stringId": "3102596317856571392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「恐らく」… では不十分だ",
              "maleVariant": "",
              "stringId": "3102715379701948416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エージェント・リード",
              "maleVariant": "",
              "stringId": "3102814210019991552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "貴様は自ら助けを求めるタイプじゃないからな",
              "maleVariant": "",
              "stringId": "3102815152513654784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ついに本性を現した",
              "maleVariant": "",
              "stringId": "3102824268615294976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自ら雇い、育てた",
              "maleVariant": "",
              "stringId": "3102834779826331648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "選択には相応の結果が伴うことを、ソングバードにわからせます",
              "maleVariant": "",
              "stringId": "3102836359636099072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんだと？",
              "maleVariant": "",
              "stringId": "3102860729498640384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "イエッサー！",
              "maleVariant": "",
              "stringId": "3102861990071541760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女はこんなもんじゃない",
              "maleVariant": "",
              "stringId": "3102872936768774144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつらの思い通りにはさせない",
              "maleVariant": "",
              "stringId": "3134503091098263552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫よ。あたしがついてる。逃げ切れるから",
              "maleVariant": "大丈夫だ。俺がついてる。逃げ切れるから",
              "stringId": "3134510603096727552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[受け取る] 丸腰ってのは落ち着かない",
              "maleVariant": "",
              "stringId": "3165099822424694784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "説明して",
              "maleVariant": "説明しろ",
              "stringId": "3191145243244666880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OKよ",
              "maleVariant": "",
              "stringId": "3191277949060292608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もし見つかったら最悪よ",
              "maleVariant": "",
              "stringId": "3194044840879235072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ。あの女が来る！",
              "maleVariant": "",
              "stringId": "3194058304796401664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うちのエージェントは？",
              "maleVariant": "",
              "stringId": "3194130897612218368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの女は、復讐のためなら、地獄からでも私を引きずり出す",
              "maleVariant": "",
              "stringId": "3194220273750941696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行きましょ",
              "maleVariant": "",
              "stringId": "3194312651098726400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "光が見える。警備よ。ちょっと待って…",
              "maleVariant": "",
              "stringId": "3215739680401211392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FIAにずっと追跡されてたのね",
              "maleVariant": "",
              "stringId": "3215767789821313024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V！",
              "maleVariant": "",
              "stringId": "3224475555933011984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[つかむ]",
              "maleVariant": "",
              "stringId": "3233387263621677060"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うう…",
              "maleVariant": "",
              "stringId": "3233392011422810112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[つかむ]",
              "maleVariant": "",
              "stringId": "3234504184379293696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ぐ…",
              "maleVariant": "",
              "stringId": "3234504184479956996"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちよ",
              "maleVariant": "",
              "stringId": "3234814628926980096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕方ないじゃ済まされない。いいから集中して",
              "maleVariant": "",
              "stringId": "3285544162680991744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はい。Vは偽造IDでセキュリティを突破しました",
              "maleVariant": "",
              "stringId": "3285622647789285376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誤った選択をしたのは、あなたでは？",
              "maleVariant": "",
              "stringId": "3285682169039908864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ファンを乗っ取った",
              "maleVariant": "",
              "stringId": "3297141822437289984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、連中はもう来るよ。急いで",
              "maleVariant": "",
              "stringId": "3297143021655912448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[覗く]",
              "maleVariant": "",
              "stringId": "3298406949461245952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[離れる]",
              "maleVariant": "",
              "stringId": "3298409725184524288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早く！ 羽根を止めて！",
              "maleVariant": "",
              "stringId": "3302546998729719808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[覗く]",
              "maleVariant": "",
              "stringId": "3302709529133793280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[離れる]",
              "maleVariant": "",
              "stringId": "3302709529368674304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ファンを止めて！",
              "maleVariant": "",
              "stringId": "3302771261420158984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ホースを投げる] これを掴んで！",
              "maleVariant": "[ホースを投げる] これを掴め！",
              "stringId": "3302993647528656900"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… よかった…",
              "maleVariant": "",
              "stringId": "3302994559286460416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしたの？",
              "maleVariant": "どうした？",
              "stringId": "3339005882616160256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無線信号をキャッチした。クソ… 暗号チャンネルね",
              "maleVariant": "",
              "stringId": "3339005960781209600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それ、傍受できそう？",
              "maleVariant": "それ、傍受できそうか？",
              "stringId": "3339011894798622720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やってみる",
              "maleVariant": "",
              "stringId": "3339012121106489344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "羽根をつかんで。そこを通るから",
              "maleVariant": "",
              "stringId": "3339054448143409152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずい、最悪の状況ね",
              "maleVariant": "",
              "stringId": "3339093360664551424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今の聞いた？",
              "maleVariant": "",
              "stringId": "3339098951705337864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こうなるとは思ってた",
              "maleVariant": "",
              "stringId": "3339099958304743432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズは過ちを犯そうとしてる",
              "maleVariant": "",
              "stringId": "3348875823763927044"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズは必死よ。そういう時に人はミスを犯す",
              "maleVariant": "マイヤーズは必死だ。そういう時に人はミスを犯す",
              "stringId": "3348877406207717376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それが何？ 私たちは今にも死体になろうとしてるのよ",
              "maleVariant": "",
              "stringId": "3348878198545297408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴らよ！",
              "maleVariant": "",
              "stringId": "3359548564898398212"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このままじゃ見つかる！ 身を隠して！",
              "maleVariant": "",
              "stringId": "3365039096670277632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつらあなたをマークしてたのね！",
              "maleVariant": "",
              "stringId": "3365039817620803584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしソミが間違った相手の手に渡れば… 面倒なことになる",
              "maleVariant": "",
              "stringId": "3376520704247808068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この件のことは… 我々しか知りません",
              "maleVariant": "",
              "stringId": "3376520704247808084"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "我が国を破滅させかねないものだ。リスクは冒せない",
              "maleVariant": "",
              "stringId": "3376520704264585216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "承知しています",
              "maleVariant": "",
              "stringId": "3376520704264585232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リスクは排除します、必ず",
              "maleVariant": "",
              "stringId": "3376520704264585248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[引き上げる] しっかりつかんで！",
              "maleVariant": "[引き上げる] しっかりつかめ！",
              "stringId": "3378355137359724544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つかんでて！",
              "maleVariant": "しっかりつかめ！",
              "stringId": "3378357826965553168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう少しよ！",
              "maleVariant": "もう少しだ！",
              "stringId": "3378361761507917864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ぐっ… ウソでしょ… ああ…",
              "maleVariant": "ぐっ… ウソだろ… ああ…",
              "stringId": "3378458596055883776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "急いで！",
              "maleVariant": "",
              "stringId": "3378459957996724224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落としは… しない！",
              "maleVariant": "",
              "stringId": "3378461069403398144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ！ ひらめいた！",
              "maleVariant": "",
              "stringId": "3378486027324137472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これを掴んで！",
              "maleVariant": "これを掴め！",
              "stringId": "3378487972390674432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "グッ、クソ！",
              "maleVariant": "",
              "stringId": "3378494825111658496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "危なかった",
              "maleVariant": "",
              "stringId": "3378498134098747392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？ V、大丈夫？",
              "maleVariant": "",
              "stringId": "3378502208210771968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死にかけてると言ったはず",
              "maleVariant": "",
              "stringId": "3378503597666246660"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミが重すぎた",
              "maleVariant": "",
              "stringId": "3378503944635850752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫。問題ない",
              "maleVariant": "",
              "stringId": "3378504072478236672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあね…",
              "maleVariant": "さあな…",
              "stringId": "3378506122536591360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "疲れただけ。あんたが重くてさ",
              "maleVariant": "疲れただけだ。あんたが重くてな",
              "stringId": "3378506160838975492"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "平気よ。心配はいらない",
              "maleVariant": "平気だ。心配はいらない",
              "stringId": "3378506187145650176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしたちは、どっちも死にかけだから",
              "maleVariant": "俺たちは、どっちも死にかけなんだ",
              "stringId": "3378507865873883136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ごめん",
              "maleVariant": "",
              "stringId": "3378510416883130368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やめてよ。もうすぐ解放される。あんたのおかげでね",
              "maleVariant": "やめてくれ。もうすぐ解放される。あんたのおかげでな",
              "stringId": "3378510968652210176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無茶よ",
              "maleVariant": "無茶だ",
              "stringId": "3378532056706752512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんな状態で大道芸みたいな真似は無茶よ",
              "maleVariant": "そんな状態で大道芸みたいな真似は無茶だ",
              "stringId": "3378532292611186688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大胆ね。気に入った",
              "maleVariant": "大胆だな。気に入った",
              "stringId": "3378532508483624960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういうの、嫌いじゃない。ちょっと待ってて",
              "maleVariant": "そういうの、嫌いじゃない。ちょっと待ってろ",
              "stringId": "3378533019802836992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "120から… 30フィートくらいでしょ。なんとかなる",
              "maleVariant": "",
              "stringId": "3378536679114973184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "急いで。時間がない",
              "maleVariant": "",
              "stringId": "3378537947556388864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし！",
              "maleVariant": "",
              "stringId": "3379433545142804480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちよ！",
              "maleVariant": "こっちだ！",
              "stringId": "3379443705760358400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "消火ホース…",
              "maleVariant": "",
              "stringId": "3379478830288117760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫？",
              "maleVariant": "",
              "stringId": "3405702548942016520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早くここから移動しないと",
              "maleVariant": "",
              "stringId": "3405702548992348168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オービタル・エアーからナイトシティを牛耳る企業まで… 新合衆国はすべてを敵に回すことになる",
              "maleVariant": "",
              "stringId": "3449608958565064704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの女は新たな戦争を始める数歩手前ね。たとえミリテクの戦力をいくら投じたって勝てない戦争を",
              "maleVariant": "あの女は新たな戦争を始める数歩手前だな。たとえミリテクの戦力をいくら投じたって勝てない戦争を",
              "stringId": "3449609631616638976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "必ずシャトルに乗せるから。約束する",
              "maleVariant": "必ずシャトルに乗せてやる。約束だ",
              "stringId": "3449609857991614464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[調べる] パイプ？",
              "maleVariant": "",
              "stringId": "3458030068714418180"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これじゃだめね",
              "maleVariant": "これじゃだめだな",
              "stringId": "3459125304584810496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、進める。周りを警戒して",
              "maleVariant": "",
              "stringId": "3476825500332146688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無力化しました",
              "maleVariant": "",
              "stringId": "3476829056078528512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行くよ。ここだと見つかる",
              "maleVariant": "",
              "stringId": "3476842732596576256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "急がないとまずい",
              "maleVariant": "",
              "stringId": "3477094003416408068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "急がないと。ここが地獄になる前にね",
              "maleVariant": "急がないと。ここが地獄になる前にな",
              "stringId": "3477096051746709504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "発射台まではリニアで行ける。でもまずティコ・ターミナルを通り抜けないと",
              "maleVariant": "",
              "stringId": "3477102555182833664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっと助かる",
              "maleVariant": "",
              "stringId": "3478020223834431488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、ターミナルに戻った",
              "maleVariant": "",
              "stringId": "3478021049407676416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたをシャトルに乗せる… やるべきことはそれだけよ",
              "maleVariant": "あんたをシャトルに乗せる… やるべきことはそれだけだ",
              "stringId": "3478023947889844224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "中へ… 早く！",
              "maleVariant": "",
              "stringId": "3478035120542621696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "目測でわかるのか？ たまげたな",
              "maleVariant": "",
              "stringId": "3487123170027823104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これじゃ重さに耐えられない",
              "maleVariant": "",
              "stringId": "3487124162668584960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、聞こえてるんだけど？",
              "maleVariant": "",
              "stringId": "3487128110548836352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うらめしい雨…",
              "maleVariant": "",
              "stringId": "3489829444083163136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あらゆる手段を講じる許可を与える。だが、頭を撃ち抜くのは避けろ",
              "maleVariant": "",
              "stringId": "3489860073256558592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それと、もうひとつ",
              "maleVariant": "",
              "stringId": "3495656134256533504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞こえたはず。とにかく、私はすべきことをするだけです",
              "maleVariant": "",
              "stringId": "3495665319211868160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この宇宙港は厳密に言うとナイトシティに含まれない。治外法権の中立帯よ。マイヤーズは国際法と国際協定を無視してる",
              "maleVariant": "",
              "stringId": "3496788423531208704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あーはいはい…",
              "maleVariant": "",
              "stringId": "3497077817873965056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、使えるものがないか探して",
              "maleVariant": "",
              "stringId": "3497150102374883328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リードは今、あんたのことを裏切った",
              "maleVariant": "",
              "stringId": "3536138162277883904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リードがあんなに簡単に裏切るとは… 思わなかった",
              "maleVariant": "",
              "stringId": "3536239203061243904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私が彼を信用するのが怖かった理由… 今のでわかったでしょ？",
              "maleVariant": "",
              "stringId": "3537399735336214528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたを助けたいって話、全部嘘だったと思う？",
              "maleVariant": "あんたを助けたいって話、全部嘘だったと思うか？",
              "stringId": "3537400064924622848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私とマイヤーズのどちらかを選べなかった、だからリードは自分に嘘をついてたのよ",
              "maleVariant": "",
              "stringId": "3537400506333175808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも結局、彼は新合衆国の忠実なしもべだった",
              "maleVariant": "",
              "stringId": "3537401159671521280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[強く引く] …っ！",
              "maleVariant": "",
              "stringId": "3538868566470598660"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ぐう…",
              "maleVariant": "",
              "stringId": "3538905008831897600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "次にやる時は… いや、こんなのは二度とごめんよ",
              "maleVariant": "",
              "stringId": "3538907576769355776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[調べる] 消化ホース！",
              "maleVariant": "",
              "stringId": "3559497312563011596"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[調べる] はしごか…",
              "maleVariant": "",
              "stringId": "3560690215083745292"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[調べる] ケーブルね",
              "maleVariant": "[調べる] ケーブルだな",
              "stringId": "3560695392901701644"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "短すぎる",
              "maleVariant": "",
              "stringId": "3566657751814152192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[引く] もう少し！",
              "maleVariant": "",
              "stringId": "3569426593682464768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早くして！",
              "maleVariant": "",
              "stringId": "3601332535277559808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズはとことんやる気ね",
              "maleVariant": "マイヤーズはとことんやる気だな",
              "stringId": "3765348211642654724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ...",
              "maleVariant": "",
              "stringId": "4045084668270526464"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}