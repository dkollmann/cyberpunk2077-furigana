    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ずっと考えてたんだが…",
              "maleVariant": "",
              "stringId": "3073522778261401600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これからも一緒ってことね…",
              "maleVariant": "これからも一緒ってことか…",
              "stringId": "3073828283613687812"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心が躍るねぇ…",
              "maleVariant": "",
              "stringId": "3073868696454148096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ諦める気はない",
              "maleVariant": "",
              "stringId": "3073868871960604672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだこれで終わりじゃない",
              "maleVariant": "",
              "stringId": "3073868925211488256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これからどうする",
              "maleVariant": "",
              "stringId": "3073903734159814660"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "末永く幸せに暮らしましたとさ… って結末を見送っちまったわけだ",
              "maleVariant": "",
              "stringId": "3073907820569538560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "成り行きを見守るしかない",
              "maleVariant": "",
              "stringId": "3073908433105694720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "運命に任せるしかない",
              "maleVariant": "",
              "stringId": "3073909083004710912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいか、V… 新合衆国の計画をぶち壊して死にかけのサイボーグを宇宙に打ち上げるのはまさに“伝説的”だった。だが、俺たちには時間がない",
              "maleVariant": "",
              "stringId": "3073913437816336384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ？",
              "maleVariant": "",
              "stringId": "3073930618658598912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気の毒に思ってるのは自分のことでしょ",
              "maleVariant": "気の毒に思ってるのは自分のことだろ",
              "stringId": "3073931273154572288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの間違いじゃなくて？",
              "maleVariant": "俺の間違いじゃなくてか？",
              "stringId": "3073931273154572300"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたが気の毒に思ってるのはソミじゃない。自分よ",
              "maleVariant": "お前が気の毒に思ってるのはソミじゃない。自分だろ",
              "stringId": "3073931273238458380"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、誰かのことを忘れてない？ あたしの不運にも同情したらどう？",
              "maleVariant": "なあ、誰かのことを忘れてないか？ 俺の不運にも同情したらどうだ？",
              "stringId": "3073931274194759684"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、まあお前の境遇も泥沼だが、少なくとも独りじゃないだろ",
              "maleVariant": "",
              "stringId": "3073934250439323648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ… あんたにしては気の利いたこというじゃない",
              "maleVariant": "へえ… お前にしちゃ気の利いたこというじゃないか",
              "stringId": "3073936788731441152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前は死にかけてる。何か手を打つんだ",
              "maleVariant": "",
              "stringId": "3128825808257720320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんつうか… クソ。今になって、あの女が気の毒に思えてきた",
              "maleVariant": "",
              "stringId": "3128835078541975552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "健全なエゴと呼んでくれ。ってそんな話じゃねえよ。あの女が心底ツイてないと思ってな",
              "maleVariant": "",
              "stringId": "3128851945415417856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "図に乗るなよ",
              "maleVariant": "",
              "stringId": "3128940579447545856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "結果として敵を作っちまったけどな",
              "maleVariant": "",
              "stringId": "3308175203751145472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一番忠実な猟犬を殺しちまったんだからな",
              "maleVariant": "",
              "stringId": "3308175942015758336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "3363730432768163840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "3363735132586459136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "紺碧プラザでのあたしもツイてなかった",
              "maleVariant": "紺碧プラザでの俺もツイてなかった",
              "stringId": "3501305419648114688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どちらも無謀に高みを望み、太陽に近づき過ぎたんだ",
              "maleVariant": "",
              "stringId": "3501310289620934656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "中国の古いことわざにこんなのがある。自分のケツより高く跳ぶな、ってな",
              "maleVariant": "",
              "stringId": "3501318584259747840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズは根に持つぞ。間違いねえ",
              "maleVariant": "",
              "stringId": "3501441139675144192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これからもあんたと一緒ってことね…",
              "maleVariant": "これからもお前と一緒ってことか…",
              "stringId": "3548023647891791872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ま、政治家とスパイの戦争に巻き込まれて生き延びたんだから、ツいてたとも言えるね",
              "maleVariant": "ま、政治家とスパイの戦争に巻き込まれて生き延びたんだから、ツいてたとも言えるな",
              "stringId": "3557695164569264128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズにはあたしより大きな問題がある",
              "maleVariant": "マイヤーズには俺より大きな問題がある",
              "stringId": "3596976651772534788"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズはリードなんて気にかけない",
              "maleVariant": "",
              "stringId": "3596977808981008384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズの狙いはソミただ1人だった",
              "maleVariant": "",
              "stringId": "3596978518019710976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仮に新たな戦争が始まって、リードが駆り出されてたとしたら、真っ先に死んだでしょうね",
              "maleVariant": "仮に新たな戦争が始まって、リードが駆り出されてたとしたら、真っ先に死んだだろうな",
              "stringId": "3597012764830638080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズを持ち上げ過ぎよ。リードは一度あの女に嵌められたけど、今回は自ら死に方を選択してる",
              "maleVariant": "マイヤーズを持ち上げ過ぎだ。リードは一度あの女に嵌められたが、今回は自ら死に方を選択してる",
              "stringId": "3597012855427604480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴は自分の戦争を戦った。稀有な理想のために。そういう奴ってのは、誰の記憶にも残らないのさ",
              "maleVariant": "",
              "stringId": "3597021171943907328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズの狙いはソミ。リードも含め、ほかはどうでもよかったのよ",
              "maleVariant": "マイヤーズの狙いはソミ。リードも含め、ほかはどうでもよかったんだ",
              "stringId": "3597028308317282304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、自分の意志でな… あいつに望める最高の結末。いや、誰しもにとってか",
              "maleVariant": "",
              "stringId": "3597035821339156480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "己の理想をズタズタに切り裂かれたまま生きるよりずっといい",
              "maleVariant": "",
              "stringId": "3597053538481905664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リード… 忠誠心が奴のアキレス腱だったな。どんな状況でもそれを捨てられなかった",
              "maleVariant": "",
              "stringId": "3597072221941964800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あれは、奴にとって最高の結末だったかもな",
              "maleVariant": "",
              "stringId": "3597074629053030400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭を切り替えて、自分が今後どうすべきかを考えないと",
              "maleVariant": "",
              "stringId": "3597075429007466496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "生存術としては下の下だ。人はいずれ誰かを裏切る。例外はない。そしてその相手は大抵、自分自身なのさ",
              "maleVariant": "",
              "stringId": "3597378000326664192"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}