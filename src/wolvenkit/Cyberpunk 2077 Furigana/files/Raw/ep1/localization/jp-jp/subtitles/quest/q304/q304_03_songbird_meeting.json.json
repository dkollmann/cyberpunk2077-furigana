  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちへ",
              "maleVariant": "",
              "stringId": "2892531733876461568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よくわかる",
              "maleVariant": "",
              "stringId": "2892538594583400452"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ずっと聞きたかったんだけど… ブラックウォールの向こう側へ行くと、具体的に何が起こるの？",
              "maleVariant": "ずっと聞きたかったんだが… ブラックウォールの向こう側へ行くと、具体的に何が起こるんだ？",
              "stringId": "2892538724153839616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんな経験はない？ 人生の半分を過ごした場所なのに… 住所をまったく思い出せないの",
              "maleVariant": "",
              "stringId": "2892540996157984768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "計画の詳細が聞きたい",
              "maleVariant": "",
              "stringId": "2892753967228407812"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの話が本当だという保証は？",
              "maleVariant": "",
              "stringId": "2892756050455318528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フム… あんたを信じるとして… 次は何を？",
              "maleVariant": "",
              "stringId": "2892757013803061248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうやってこれが巧妙な罠じゃないと信じればいい？ 何か確証をくれないと",
              "maleVariant": "",
              "stringId": "2892757710997049344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "神経マトリックスを手に入れる。そしてリードとアレックスがハンセンの相手をしてる間に2人で逃げる",
              "maleVariant": "",
              "stringId": "2892760003586838528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはできない。でも私からの信頼は示したはずよ。私をリードに売り渡すか、協力して2人が助かる道を選ぶかはあなたの自由。確証がないのはお互い様",
              "maleVariant": "",
              "stringId": "2892762666718220288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これで話は以上かな… リードに連絡して、ハンセンに会う準備はできたと伝えて",
              "maleVariant": "",
              "stringId": "2892774422932901888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう、V… 信じてくれて",
              "maleVariant": "",
              "stringId": "2892778340714827776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この場所の何がそう特別なの？",
              "maleVariant": "この場所の何がそう特別なんだ？",
              "stringId": "2941693953351905284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、この場所はいったい何？",
              "maleVariant": "で、この場所はいったい何だ？",
              "stringId": "2941697269704192000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、状況を",
              "maleVariant": "",
              "stringId": "2970578838720892928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リード。挨拶もなし？ 調子はどう？",
              "maleVariant": "リード。挨拶もなしか？ 調子はどうだ？",
              "stringId": "2970581573071556608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "夜になると、みんなここに集まってた。バーベキューをしながらビールを飲んで… とりとめのない話をするの",
              "maleVariant": "",
              "stringId": "3050331006776135680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とても特別な場所よ… 私にとってね。故郷のブルックリンを思い出すから",
              "maleVariant": "",
              "stringId": "3050331738631852032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほかに誰か潜んでたりしない？",
              "maleVariant": "ほかに誰か潜んでたりしないか？",
              "stringId": "3053661313646641156"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて… 会えたわね",
              "maleVariant": "",
              "stringId": "3053661322790223872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしたちだけでしょうね？ 厄介なサプライズはご免よ…",
              "maleVariant": "俺たちだけだろうな？ 厄介なサプライズはご免だぞ…",
              "stringId": "3053661497944358912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "適当に選んだ場所ってわけじゃなさそうね",
              "maleVariant": "適当に選んだ場所ってわけじゃなさそうだな",
              "stringId": "3053661502071554048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "適当に選んだ場所ではなさそうね",
              "maleVariant": "適当に選んだ場所ではなさそうだな",
              "stringId": "3053661597248700416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "安心して。でもその疑りよう… Vもすっかりこっち側の人間ね",
              "maleVariant": "",
              "stringId": "3053685239399497728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのとおりよ。見せたいものがあって… 大事なことなの。状況を正確に理解してもらうために",
              "maleVariant": "",
              "stringId": "3053686909219028992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは… まるであたしみたい。気持ちはわかる",
              "maleVariant": "それは… まるで俺みたいだ。気持ちはわかる",
              "stringId": "3053719272837791744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブルックリンのこと、聞かせてよ",
              "maleVariant": "ブルックリンのこと、聞かせてくれ",
              "stringId": "3063652305997053952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね… よく覚えてるのは… ミリテクのデータ要塞を襲う前の晩のこと",
              "maleVariant": "",
              "stringId": "3063675075514515456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "非常階段に座ってジョイントに火をつけたら、道端で大勢が盛り上がってる様子が見えた",
              "maleVariant": "",
              "stringId": "3063675421947248640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "楽しそうに叫んだり笑ったり… 誰かの玄関口からファンクが流れてて。街一番のスコップドッグ屋、ウィリーの声も聞こえたっけ。彼がチューマとバカ話してて、辺りには焚き火でもしてるみたいに煙が漂ってた。安心する風景よ",
              "maleVariant": "",
              "stringId": "3063676359441301504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何かのお祝い？",
              "maleVariant": "何かのお祝いか？",
              "stringId": "3063681552090316800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。木曜日のね",
              "maleVariant": "",
              "stringId": "3063681627906555904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手袋に雪が落ちたら、私はそのフラクタル構造を計算できる… でもママの声がどんなだったかは思い出せない",
              "maleVariant": "",
              "stringId": "3063886711135322112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時々自分を… 制御できなくなる。他の誰かに自分を明け渡すような感覚に襲われる。とてもひどい感覚",
              "maleVariant": "",
              "stringId": "3063887470287568896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "記憶が消えてるってこと？",
              "maleVariant": "記憶が消えてるってことか？",
              "stringId": "3063890077886365696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カート・ハンセン、新合衆国、リード… すべてから逃げ出したいの",
              "maleVariant": "",
              "stringId": "3064848551562625024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それから？",
              "maleVariant": "",
              "stringId": "3065333868824543232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティから離れる。すでに、とある場所にある闇クリニックと話をつけてるの",
              "maleVariant": "",
              "stringId": "3065333967256469504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう。いつも背後に誰かが立ってるような気がする… でもそこには誰もいない",
              "maleVariant": "",
              "stringId": "3067835020313018368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すべて片づいたら冷えたビールを片手に、無駄話に興じてやる",
              "maleVariant": "",
              "stringId": "3067845263826796544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はまず、状況を教えてくれ",
              "maleVariant": "",
              "stringId": "3067845400946982912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こうやって、顔を合わせて",
              "maleVariant": "",
              "stringId": "3200065652400504832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信頼してるからこそ",
              "maleVariant": "",
              "stringId": "3200069337784434688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2ブロック先に住んでたの… ナイトシティでの任務の時にね",
              "maleVariant": "",
              "stringId": "3200072937436790784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リードは？ 彼を信用できないの？",
              "maleVariant": "リードは？ あいつを信用できないのか？",
              "stringId": "3200173790885756932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "故郷に帰る選択肢もある",
              "maleVariant": "",
              "stringId": "3200175027333021696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっと正直に話してくれたのね",
              "maleVariant": "やっと正直に話してくれたんだな",
              "stringId": "3200175166835572736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "故郷に帰れば…",
              "maleVariant": "",
              "stringId": "3200179281095475200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リードは信用できないの？ あんたを救いたがってる",
              "maleVariant": "リードは信用できないのか？ あんたを救いたがってる",
              "stringId": "3200179284434141184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「救う」ね。既にリードには一度救われてる。ミリテクのハックに失敗した19歳の私の家に突然現れて、断りようのない話を突きつけたときに",
              "maleVariant": "",
              "stringId": "3200180836058185728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "世間知らずの女の子はあそこで死んだ。今の私は、その生まれ変わりよ",
              "maleVariant": "",
              "stringId": "3200181526071525376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "従うか、ネットウォッチに捕まるか…",
              "maleVariant": "",
              "stringId": "3200182710207430656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リードが来て、私はブルックリンの暖かい家を捨てざるを得なかった",
              "maleVariant": "",
              "stringId": "3200184216717541376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう帰る場所はないの",
              "maleVariant": "",
              "stringId": "3200184959646220288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "初めて正直に話をしてくれたのね",
              "maleVariant": "初めて正直に話をしてくれたんだな",
              "stringId": "3200186971150532608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたにも、私自身にももうウソはつかない",
              "maleVariant": "",
              "stringId": "3200187367361265664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティはあんたが脱出するための中継地点？",
              "maleVariant": "",
              "stringId": "3200190380196937732"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よりによってハンセンと組むなんて",
              "maleVariant": "",
              "stringId": "3200225744991207424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よりによって、ハンセンなんかと手を組むなんて… 他にもう少しマシな相手はいなかったの？",
              "maleVariant": "よりによって、ハンセンなんかと手を組むなんて… 他にもう少しマシな相手はいなかったのか？",
              "stringId": "3200230750909808640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズと距離を置くために力を借りたの。彼のことはナイトシティ時代に知った",
              "maleVariant": "",
              "stringId": "3200232284515123200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リスキーな手を選んだもんね。無謀と言ってもいい",
              "maleVariant": "リスキーな手を選んだもんだな。無謀と言ってもいい",
              "stringId": "3200232557799194624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だからあなたにも声をかけた",
              "maleVariant": "",
              "stringId": "3200232733423091712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティはあんたが脱出するための中継地点。そういう計画ね？",
              "maleVariant": "ナイトシティはあんたが脱出するための中継地点。そういう計画だな？",
              "stringId": "3200235000024973312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。ただ… 事態が想定以上にややこしくなってしまった",
              "maleVariant": "",
              "stringId": "3200235436249366528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさか同僚たちまで出てくるとは… ハンセンに囚われたのも誤算だった",
              "maleVariant": "",
              "stringId": "3200235565115162624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私は逃げたい。V、そのためにあなたが必要なのよ。ほかに… 頼れる人がいない",
              "maleVariant": "",
              "stringId": "3200236362519461888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これが計画のすべて。肩の荷が下りたわ。で、私に手を貸してくれるのは…",
              "maleVariant": "",
              "stringId": "3200252649672982528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなた以上に理解してくれる人はいないでしょうね",
              "maleVariant": "",
              "stringId": "3201570641874698240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信用できるのはあなただけ",
              "maleVariant": "",
              "stringId": "3202555965203464192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも問題は、すべてを聞いたあなたが、私を信じてくれるかってことね",
              "maleVariant": "",
              "stringId": "3202556313347473408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "間違いを犯したら、結果を受けとめて生きるしかない",
              "maleVariant": "",
              "stringId": "3202565192504492032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたを理解してるし、信用してる",
              "maleVariant": "",
              "stringId": "3202584976600252420"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたを理解してるし、信用してる。生き続けるために戦う気持ちがわかるから",
              "maleVariant": "あんたを理解してるし、信用してる。生き続けるために戦う気持ちがわかるからな",
              "stringId": "3202585005138296832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしだって生き延びたいからよ",
              "maleVariant": "俺だって生き延びたいからだ",
              "stringId": "3202585010305679360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "両方ってのは？",
              "maleVariant": "",
              "stringId": "3202585015271735296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしも生き延びたい。あんたと同じよ",
              "maleVariant": "俺も生き延びたい。あんたと同じだ",
              "stringId": "3202585411079815168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その両方よ",
              "maleVariant": "その両方だ",
              "stringId": "3202585535617089536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死にかけてる人間ならではね",
              "maleVariant": "",
              "stringId": "3202587589399986176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フッ… 正直ね",
              "maleVariant": "",
              "stringId": "3202588445272883200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D.C.向きの性格ね。本心を見せつつ、核心は隠す",
              "maleVariant": "",
              "stringId": "3202588879685337088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあいいわ",
              "maleVariant": "",
              "stringId": "3202589139765739520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブルックリンのことを聞かせて",
              "maleVariant": "ブルックリンのことを聞かせてくれ",
              "stringId": "3202793117646663680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ホームシック？",
              "maleVariant": "ホームシックか？",
              "stringId": "3212708096306917376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "帰りたいのね",
              "maleVariant": "帰りたいんだな",
              "stringId": "3212709285777977344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "元気にしてた？",
              "maleVariant": "元気にしてたか？",
              "stringId": "3222865126268882960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ネットランナーの車を追跡する準備ができた",
              "maleVariant": "",
              "stringId": "3222866452004491264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソングバードと話した",
              "maleVariant": "",
              "stringId": "3222867271856705536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アレックスと一緒にレンタカー会社のシステムに侵入した。準備完了よ",
              "maleVariant": "アレックスと一緒にレンタカー会社のシステムに侵入した。準備完了だ",
              "stringId": "3222868281777999872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また後で話しましょ",
              "maleVariant": "また後で話そう",
              "stringId": "3222868577157664768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし。では暗号化キーを送れ",
              "maleVariant": "",
              "stringId": "3222871127848144896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[暗号化キーを送る]",
              "maleVariant": "",
              "stringId": "3222872020547031044"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今送ってる…",
              "maleVariant": "",
              "stringId": "3222874012271005696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、それともうひとつ… 行動インプリントを扱うためには新しいクロームが必要だ",
              "maleVariant": "",
              "stringId": "3222876358950539264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うちのクリニックを使え。リパーも承諾済みだ",
              "maleVariant": "",
              "stringId": "3222877780752486400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "統一戦争の記念品を集めてると聞いた――これが仲間うちの合言葉だ",
              "maleVariant": "",
              "stringId": "3222880990988881920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソングバードはどうだ。客をもてなす準備は整ったか？",
              "maleVariant": "",
              "stringId": "3222882478758842368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ連絡はない",
              "maleVariant": "",
              "stringId": "3222884541098119172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかったら知らせる",
              "maleVariant": "",
              "stringId": "3222885283154382848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いえ、まだよ…",
              "maleVariant": "いや、まだだ…",
              "stringId": "3222885814874689536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連絡が来たら知らせろ",
              "maleVariant": "",
              "stringId": "3222886946766344192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何かわかったらすぐ知らせる",
              "maleVariant": "",
              "stringId": "3222887600121466880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ",
              "maleVariant": "",
              "stringId": "3222887908587360256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミから連絡があった…",
              "maleVariant": "",
              "stringId": "3222896400945664000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実はもう話をしたの",
              "maleVariant": "実はもう話をした",
              "stringId": "3222898648236969984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほう。なら話を聞こう",
              "maleVariant": "",
              "stringId": "3222899785044348928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミから連絡があった。知らせもある",
              "maleVariant": "",
              "stringId": "3222900467877040128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞こう",
              "maleVariant": "",
              "stringId": "3222900830533341184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まず、ゴーサインが出た。VIPの準備は整ってる",
              "maleVariant": "",
              "stringId": "3223005058081771520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待っていたぞ。他には？",
              "maleVariant": "",
              "stringId": "3223008957173878784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいか、まずは車の追跡からだ。でないと始まらない",
              "maleVariant": "",
              "stringId": "3223264163727953920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "システムから暗号化キーを取得したら、すぐに知らせろ",
              "maleVariant": "",
              "stringId": "3223296700286259200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、来た",
              "maleVariant": "",
              "stringId": "3225991173202006036"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時間は巻き戻せない",
              "maleVariant": "",
              "stringId": "3228843158473457664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何かを恋しく思った時は、大抵手遅れなのよ",
              "maleVariant": "",
              "stringId": "3228843880195735552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしたちは過去に間違いを犯した。そのせいで苦しんでる。あたしは生体チップ、あんたはブラックウォール",
              "maleVariant": "俺たちは過去に間違いを犯した。そのせいで苦しんでる。俺は生体チップ、あんたはブラックウォール",
              "stringId": "3228913371843837952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまり… 自分が悪いってこと？",
              "maleVariant": "",
              "stringId": "3228990580373766144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうじゃない。ただ、自分のことを救おうとすると、たいてい他の誰かが代償を払うってこと",
              "maleVariant": "そうじゃない。ただ、自分のことを救おうとすると、たいてい他の誰かが代償を払うってことだ",
              "stringId": "3228990706152554496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、そうかもしれない… 残酷すぎる真実よ",
              "maleVariant": "",
              "stringId": "3228991459114008576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私に共感したから？ それとも命を救ってあげられるから？",
              "maleVariant": "",
              "stringId": "3229194904970379264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "肝心の質問は？",
              "maleVariant": "",
              "stringId": "3244821165888589888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "嘘まみれのこのゴタゴタに、真実は含まれてるの？",
              "maleVariant": "嘘まみれのこのゴタゴタに、真実は含まれてるのか？",
              "stringId": "3244821172213600276"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真実ね",
              "maleVariant": "",
              "stringId": "3244821172213600292"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "故郷は場所じゃなくて、ファミリーのいる場所よ",
              "maleVariant": "故郷は場所じゃなくて、ファミリーのいる場所だ",
              "stringId": "3285653817923751936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの故郷はナイトシティのヘイウッド",
              "maleVariant": "俺の故郷はナイトシティのヘイウッドだ",
              "stringId": "3285654042839109632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしにも気持ちはわかる",
              "maleVariant": "俺にも気持ちはわかる",
              "stringId": "3285660210932240384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかるよ… あたしの故郷はブルックリンとは全然違うけど…",
              "maleVariant": "わかるよ… 俺の故郷はブルックリンとは全然違うがな…",
              "stringId": "3285660273175711752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大事な思い出が？",
              "maleVariant": "",
              "stringId": "3291102211941253120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしのいたところはこんなに穏やかじゃない。ただどこにいようと、あたしはヘイウッドの通りを歩いてる気がしてる",
              "maleVariant": "俺のいたところはこんなに穏やかじゃない。ただどこにいようと、俺はヘイウッドの通りを歩いてる気がしてる",
              "stringId": "3291102631572979712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたにも大事に思う場所が？",
              "maleVariant": "",
              "stringId": "3291141970638061568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ノーマッドは場所を変え続ける。あたしの心に残ってるのは、一緒に過ごしたファミリーの記憶",
              "maleVariant": "ノーマッドは場所を変え続ける。俺の心に残ってるのは、一緒に過ごしたファミリーの記憶だ",
              "stringId": "3291142067341934592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何が聞きたいの、V",
              "maleVariant": "",
              "stringId": "3297079673908187144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら。話をするんでしょ",
              "maleVariant": "",
              "stringId": "3297079855773208584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの身に具体的に何が起きてるの？",
              "maleVariant": "あんたの身に具体的に何が起きてるんだ？",
              "stringId": "3349117239612850176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、いいだろう",
              "maleVariant": "",
              "stringId": "3353550515056328704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時々ここに足を運んでた",
              "maleVariant": "",
              "stringId": "3365199234528006144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、私の未来はあなたの活躍にかかってる… だから…",
              "maleVariant": "",
              "stringId": "3373612584908537856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈Relic〉のファームウェアのロックをいくつか解除した。これで本来の性能を発揮できる",
              "maleVariant": "",
              "stringId": "3373761121386414080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よくわかる",
              "maleVariant": "",
              "stringId": "3405859413477576704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "間違いを犯したら、結果を受けとめて生きるしかない",
              "maleVariant": "",
              "stringId": "3405861233721626624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティはあんたが脱出するための中継地点？",
              "maleVariant": "",
              "stringId": "3405867914727804932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセンと組むなんて結構な趣味ね",
              "maleVariant": "ハンセンと組むなんて結構な趣味だな",
              "stringId": "3405868455608471552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カロン・エキゾティクスの暗号化キーを受け取った。よくやった",
              "maleVariant": "",
              "stringId": "3448936538417680384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信用はしてないけど、どうしようもない",
              "maleVariant": "信用はしてないが、どうしようもない",
              "stringId": "3508947137432043520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたを信用してるとは言えないけど、リスクに見合う価値はあると判断した。それだけよ",
              "maleVariant": "あんたを信用してるとは言えないが、リスクに見合う価値はあると判断した。それだけだ",
              "stringId": "3508947760487514112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気持ちはわかる。どう思われていようと、正直に答えてくれたことには感謝するわ",
              "maleVariant": "",
              "stringId": "3508948705699086336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズ襲撃――仕掛けたのはあんたね",
              "maleVariant": "マイヤーズ襲撃――仕掛けたのはあんただろ",
              "stringId": "3511306460544213000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お願いだから正直に話して",
              "maleVariant": "お願いだから正直に話せ",
              "stringId": "3511307929238478848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズ襲撃はあんたとハンセンで仕掛けたんでしょ",
              "maleVariant": "マイヤーズ襲撃はあんたとハンセンで仕掛けたんだろ",
              "stringId": "3511308288220569600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソングバード… 正直に話して",
              "maleVariant": "ソングバード… 正直に話してくれ",
              "stringId": "3511308788651368448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのときのリードも、もういないけどね… 7年前に彼も“死んだ”",
              "maleVariant": "",
              "stringId": "3511319353381724160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新合衆国に死刑宣告をされながら、それでも国に仕え続けてる。もはや何の意味もないものにしがみついてるのよ",
              "maleVariant": "",
              "stringId": "3511321632767844352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜそんなことをしたの？",
              "maleVariant": "なぜそんなことをした？",
              "stringId": "3511403940346699776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもなぜそんなことを？ 他にもやり方はあったはずでしょ",
              "maleVariant": "でもなぜそんなことを？ 他にもやり方はあったはずだろ",
              "stringId": "3511404313841082368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他の選択肢もあったはず",
              "maleVariant": "",
              "stringId": "3511404544729128960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それ以外の選択肢も… あったはずよ",
              "maleVariant": "それ以外の選択肢も… あったはずだ",
              "stringId": "3511405066483769344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズと新合衆国にとって私は兵器の1つに過ぎない。ブラックウォールの向こう側に行くための道具よ",
              "maleVariant": "",
              "stringId": "3511410042002055168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そして兵器や道具には何の決定権もない。引き際すら選べないの",
              "maleVariant": "",
              "stringId": "3511411520443883520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今回ばかりは… なかった",
              "maleVariant": "",
              "stringId": "3511429810847326208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソングバードが… 事情を話してくれた",
              "maleVariant": "",
              "stringId": "3511455700406870016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうか… なら、なおさらしくじるわけにはいかないな。なんとしても治療薬を手に入れなければ",
              "maleVariant": "",
              "stringId": "3511459480363966464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "状況は最悪… テックがないと彼女はおしまいよ",
              "maleVariant": "状況は最悪… テックがないと彼女はおしまいだ",
              "stringId": "3511462492981534720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何ひとつ計画通りにいってない。もうメチャクチャよ",
              "maleVariant": "",
              "stringId": "3511532532506402844"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "裏切ったのね",
              "maleVariant": "裏切ったんだな",
              "stringId": "3511535171159826432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それだけじゃ…",
              "maleVariant": "",
              "stringId": "3511535345156333568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ… あたしもあのキメラ戦車と戦った時に感じた",
              "maleVariant": "ああ… 俺もあのキメラ戦車と戦った時に感じたよ",
              "stringId": "3511579013615558656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あったら… こんなことになってない",
              "maleVariant": "",
              "stringId": "3511594299790041088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いけない… そろそろ現実に戻らないと",
              "maleVariant": "",
              "stringId": "3542126521546358784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "神経マトリックスのためには仕方なかった。でも、脅しのつもりがまさか殺そうとするとは",
              "maleVariant": "",
              "stringId": "3542132610467807232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その… マイヤーズ襲撃の件でハンセンに手を貸した… 神経マトリックスを手に入れるためにね",
              "maleVariant": "",
              "stringId": "3542153380996759552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "元はどういう計画だったの？",
              "maleVariant": "元はどういう計画だったんだ？",
              "stringId": "3542154231383506944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "乗務員が一人残らず命を落として、マイヤーズ自身も死にかけたのよ",
              "maleVariant": "乗務員が一人残らず命を落として、マイヤーズ自身も死にかけたんだぞ",
              "stringId": "3542154260408090624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたのせいで大勢が死んだ",
              "maleVariant": "",
              "stringId": "3542154776945016832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "元々はどんな計画を？",
              "maleVariant": "",
              "stringId": "3542156022435201024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "強制着陸させる予定だった。それをまさか… 撃ち落とすなんて",
              "maleVariant": "",
              "stringId": "3542157541846990848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言い訳にならないよね",
              "maleVariant": "",
              "stringId": "3542157957418631168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大変な事態を招いたのはわかってるの… でも仕方なかった",
              "maleVariant": "",
              "stringId": "3542164580174315520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたがマイヤーズを救出し、私がマトリックスを確保して2人で逃げるはずだった",
              "maleVariant": "",
              "stringId": "3542203505764909056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも状況が変わって、ハンセンも別の手を打つことになった。新合衆国の報復は避けられないからね",
              "maleVariant": "",
              "stringId": "3556534792164130816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だからデータさえ手に入れれば私を解放すると言ってる… でも、消されるのは明白よ",
              "maleVariant": "",
              "stringId": "3556538337055653888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも強制着陸させて、マイヤーズを引き渡すはずだった。機体を撃墜するなんて、話が違う",
              "maleVariant": "",
              "stringId": "3558144475442364416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴はサイノシュアのメインフレームの中身が何なのかわかってない。そのデータを武器に、うまく立ち回ろうとしてるだけ",
              "maleVariant": "",
              "stringId": "3558147263262015488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "神経マトリックスのためには仕方なかった。でも、脅しのつもりがまさか殺そうとするとは",
              "maleVariant": "",
              "stringId": "3581253048732045320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズ襲撃はあんたとハンセンで仕掛けたんでしょ",
              "maleVariant": "マイヤーズ襲撃はあんたとハンセンで仕掛けたんだろ",
              "stringId": "3581254624515280904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも強制着陸させて、マイヤーズを引き渡すはずだった。機体を撃墜するなんて、話が違う",
