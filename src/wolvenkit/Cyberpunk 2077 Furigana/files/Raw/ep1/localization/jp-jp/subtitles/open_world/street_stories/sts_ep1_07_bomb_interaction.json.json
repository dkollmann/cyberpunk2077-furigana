    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "良心のあるテロリストは企業よりタチが悪い。自ら手を汚す度胸がない奴はテロリスト失格だ",
              "maleVariant": "",
              "stringId": "3095754152745873408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたがアラサカ・タワーを襲ってから半世紀が経った――",
              "maleVariant": "お前がアラサカ・タワーを襲ってから半世紀が経った――",
              "stringId": "3192742004460859392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "良心があって何が悪いの？",
              "maleVariant": "良心があって何が悪い？",
              "stringId": "3192745666910334976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あれはきっかけに過ぎなかった。だがネルのような腰抜けどものせいで戦いが泥沼化しちまったのさ",
              "maleVariant": "",
              "stringId": "3192746707886256128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大勢の人が死に、できたものと言えばもっとでかいタワーよ",
              "maleVariant": "大勢の人が死に、できたものと言えばもっとでかいタワーだ",
              "stringId": "3233386972131532800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "罪無き人々を救おうとしたネルを責めるの？",
              "maleVariant": "罪無き人々を救おうとしたネルを責めるのか？",
              "stringId": "3233419720435081216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業に戦争を仕掛けるには、それなりの覚悟が必要だ。罪無き人間も犠牲になる。それが戦争だ",
              "maleVariant": "",
              "stringId": "3233420238163189760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分が死んだことで大義が成されたと思ってるようだけど――",
              "maleVariant": "自分が死んだことで大義が成されたと思ってるようだが――",
              "stringId": "3233523469279207424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ネルは信念のためにリスクを冒してる。あんたのアラサカ・タワー襲撃と同じよ",
              "maleVariant": "ネルは信念のためにリスクを冒してる。お前のアラサカ・タワー襲撃と同じだ",
              "stringId": "3233523858393178116"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "戦争の何を知ってるっていうのよ。あんたは脱走兵でしょ",
              "maleVariant": "戦争の何を知ってるっていうんだ。お前は脱走兵だろ",
              "stringId": "3233523920233996292"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "戦争の何を知ってるのよ",
              "maleVariant": "戦争の何を知ってるんだ",
              "stringId": "3233524098005377024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカ・タワーを爆破して何が変わった？",
              "maleVariant": "",
              "stringId": "3233524098005377040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの場合は、世界を変えられなかったけど",
              "maleVariant": "お前の場合は、世界を変えられなかったがな",
              "stringId": "3234582267821465600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ネルよりあんたのほうが現実を見えてない",
              "maleVariant": "ネルよりお前のほうが現実を見えてない",
              "stringId": "3234582977162158080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少なくとも俺は、戦うこともせずに武器を捨てたりはしねえ",
              "maleVariant": "",
              "stringId": "3234586287155879936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "変わらなかったのはネルのような奴らのせいだ。人が払った犠牲のありがたみを理解できない自己中どものな",
              "maleVariant": "",
              "stringId": "3234628137317212160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんにしたって俺は信念を貫いて死んだ。ネルは自分の命を懸けてねえ",
              "maleVariant": "",
              "stringId": "3234657869916184576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ネルは勇敢よ",
              "maleVariant": "ネルは勇敢だ",
              "stringId": "3241939752907612160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分を英雄だとでも思ってるの？",
              "maleVariant": "自分を英雄だとでも思ってるのか？",
              "stringId": "3242249132622389248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分の死に様を覚えてる？",
              "maleVariant": "自分の死に様を覚えてるのか？",
              "stringId": "3242249132639166472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはアラサカに虐げられた人々のために犠牲になったんじゃない",
              "maleVariant": "お前はアラサカに虐げられた人々のために犠牲になったんじゃない",
              "stringId": "3242290699047444480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの時スマッシャーがきっちりトドメを刺してくれてたら、お前の偉そうな説教も聞かずに済んだのにな",
              "maleVariant": "",
              "stringId": "3242293444554956800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分の女を助けようとして、殺されただけよ",
              "maleVariant": "自分の女を助けようとして、殺されただけだ",
              "stringId": "3242296271633293312"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}