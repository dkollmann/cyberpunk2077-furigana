    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンズ、頼みがあるの",
              "maleVariant": "ハンズ、頼みがあるんだ",
              "stringId": "3197044392203370496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": "3197067495134445572"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時間がないの",
              "maleVariant": "時間がないんだ",
              "stringId": "3197068955255554048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "単刀直入だな。どうした",
              "maleVariant": "",
              "stringId": "3197069123010936832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブラック・サファイアで開かれるパーティーに潜り込みたい",
              "maleVariant": "",
              "stringId": "3197070310183518208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "招待状は用意できないが、何とかしてみよう。だが代わりに私からも頼みがある…",
              "maleVariant": "",
              "stringId": "3197070417205379072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰かを買収できないの？",
              "maleVariant": "誰かを買収できないのか？",
              "stringId": "3197076070086983680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。取引成立よ",
              "maleVariant": "わかった。取引成立だ",
              "stringId": "3197084749746126848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "賄賂じゃダメなの？",
              "maleVariant": "賄賂じゃダメなのか？",
              "stringId": "3197084950972055552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フム… いくつか腕をねじり上げる必要があるな。だが、お前のおかげで選択肢は豊富にある",
              "maleVariant": "",
              "stringId": "3197112534342549504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前にはドッグタウンでの地盤固めを手伝ってもらった。そのお返しだ",
              "maleVariant": "",
              "stringId": "3197113329683251200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう少し簡単にできない？ 時間がないのよ…",
              "maleVariant": "もう少し簡単にできないか？ 時間がないんだ…",
              "stringId": "3197132197071929344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "物事を円滑に進めるには、歯車に油を差す必要があるのさ",
              "maleVariant": "",
              "stringId": "3197134770763972608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やれやれ… 金は貧者の通貨だ、V。一方で評判、情報、貸し借りは黄金に等しい。そして、それらの重みは持ち主のハートよりヘビーだ",
              "maleVariant": "",
              "stringId": "3197136813574569984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よろしい。ではさらばだ、V",
              "maleVariant": "",
              "stringId": "3197239215107588096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、わかる",
              "maleVariant": "ああ、わかる",
              "stringId": "3294110893810520068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "要はあんたの“評判”を上げるために協力しろってこと？",
              "maleVariant": "要はあんたの“評判”を上げるために協力しろってことか？",
              "stringId": "3294113050974621696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのとおりだ…",
              "maleVariant": "",
              "stringId": "3294114194811654144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私のために、ドッグタウンで依頼をいくつかこなしてくれ。そうすれば、ブラック・サファイアの図面はお前のものだ",
              "maleVariant": "",
              "stringId": "3294114744819126272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "評判とは通貨だ。そして信用を集めるにつれ、価値も上昇する。言いたいことがわかるか？",
              "maleVariant": "",
              "stringId": "3294116548789276672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何より、ブラック・サファイアに買い取れるハートを持つ者はいない。ハンセンがすでに始末した。黄金ではなく、鉛でな",
              "maleVariant": "",
              "stringId": "3294205667146076160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手配できるの…？",
              "maleVariant": "手配できるのか…？",
              "stringId": "3294227198588882944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんとかしてくれるってこと？",
              "maleVariant": "なんとかしてくれるってことか？",
              "stringId": "3294227198639214604"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "直接会って話そう。少し時間がいる。ヘビー・ハーツに来てくれ",
              "maleVariant": "",
              "stringId": "3294230069875183616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう、ハンズ",
              "maleVariant": "",
              "stringId": "3294232580853993472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンズ。約束はちゃんと覚えてる？",
              "maleVariant": "ハンズ。約束はちゃんと覚えてるか？",
              "stringId": "3294234999189016600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろんだ、ブラック・サファイアだろう。お前のために完璧な作戦を用意した。ヘビー・ハーツに来てくれ",
              "maleVariant": "",
              "stringId": "3294234999189016612"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。それじゃあ",
              "maleVariant": "",
              "stringId": "3294241213939585024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと待って。約束は覚えてる？",
              "maleVariant": "ちょっと待ってくれ。約束は覚えてるか？",
              "stringId": "3478073911228129280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー、ちょっと待って。助けてほしいことがあるの",
              "maleVariant": "あー、ちょっと待ってくれ。助けてほしいことがあるんだ",
              "stringId": "3478372513494470656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほう… なんだ、言ってみろ",
              "maleVariant": "",
              "stringId": "3478372866336100352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪魔とチークダンスでもする気か？ いいか、V。よく聞け。危険であることは言うまでもないが、そもそも潜入は不可能だ。お前のよく知る紺碧プラザとはわけが違う",
              "maleVariant": "",
              "stringId": "3510213264230453248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたなら何とかできるでしょ",
              "maleVariant": "あんたなら何とかできるだろ",
              "stringId": "3510218008558702592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… そこまで言うなら仕方あるまい",
              "maleVariant": "",
              "stringId": "3510218705920462848"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}