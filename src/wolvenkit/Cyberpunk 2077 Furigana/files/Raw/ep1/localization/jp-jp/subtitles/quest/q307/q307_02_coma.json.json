  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心配すんな、V。大丈夫だ",
              "maleVariant": "",
              "stringId": "3843340670091259912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よう、V。なんだ、寝不足か？ まあいい、さっさと起きろ！",
              "maleVariant": "",
              "stringId": "3843340670141591568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたが何度か叫ぶから… 悪夢でも見た？",
              "maleVariant": "",
              "stringId": "3843340670208700436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたがいて、助かるよ…",
              "maleVariant": "",
              "stringId": "3843340670259032084"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここで台無しにしたくない",
              "maleVariant": "",
              "stringId": "3843340670342918164"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V… 話したいことがある",
              "maleVariant": "",
              "stringId": "3843340670393249812"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今どこにいんの！？",
              "maleVariant": "",
              "stringId": "3843340670426804244"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、こっち",
              "maleVariant": "",
              "stringId": "3843340673329262608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私にできることがあれば言って… 遠慮はいらないから、いつでも連絡してね",
              "maleVariant": "",
              "stringId": "3843340673463480320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え？ あなたの声以外には何も聞こえないけど",
              "maleVariant": "",
              "stringId": "3843340673547366416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おかげで計画が台無しね",
              "maleVariant": "",
              "stringId": "3843340673648029712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当はナイトシティを出てくつもりで、荷造りまで始めてたのに",
              "maleVariant": "",
              "stringId": "3843340673698361360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今は… 残りたいと思ってる",
              "maleVariant": "",
              "stringId": "3843340673731915792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、何かあったのか？ 様子が変だぞ",
              "maleVariant": "",
              "stringId": "3843340673832579088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今どこにいる？ 病院は？",
              "maleVariant": "",
              "stringId": "3843340673882910736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあなV。また連絡する",
              "maleVariant": "",
              "stringId": "3843340673983574032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新たな始まりと、生の円環を祝うのさ",
              "maleVariant": "",
              "stringId": "3843340674033905680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ、俺は… お前を失いたくないんだ。ほら、これで満足か？",
              "maleVariant": "",
              "stringId": "3843340674117791760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前は現状を揺り動かす。お前と会うまでの俺は行き詰まってて… 自分の影を見るのも怖かったんだ",
              "maleVariant": "",
              "stringId": "3843340674168123408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんな風に怖がらせないでくれる？",
              "maleVariant": "",
              "stringId": "3843340674302341136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞こえるか？",
              "maleVariant": "",
              "stringId": "3843340674403004432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうか。俺はもう少しここにいる。ありがとうな、V",
              "maleVariant": "",
              "stringId": "3843340674587553808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、考えてたんだが…",
              "maleVariant": "",
              "stringId": "3843340674637885456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昨夜のことだけど… 俺たち… この先どうなる？ 長期的に見てどうなんだ？",
              "maleVariant": "",
              "stringId": "3843340674688217096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "試してみるつもりはあるか？",
              "maleVariant": "",
              "stringId": "3843340674721771536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前が好きだ、V",
              "maleVariant": "",
              "stringId": "3843340674772103184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、起きたか",
              "maleVariant": "",
              "stringId": "3843340674822434832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "逃げ切れたようで良かった",
              "maleVariant": "",
              "stringId": "3843340674872766480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜ戻った！ 俺と一緒に死ぬつもりか！",
              "maleVariant": "",
              "stringId": "3843340674923098128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前を信用できるかわからなかった。だが状況が変わった",
              "maleVariant": "",
              "stringId": "3843340674956652560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、しっかりしろ！",
              "maleVariant": "",
              "stringId": "3843340675006984208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死者の無念を晴らすのは、生者の務めだ",
              "maleVariant": "",
              "stringId": "3843340675057315856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "遅いぞ、V",
              "maleVariant": "",
              "stringId": "3843340675141201936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼れる仲間… 家族は誰にも必要だ。お前もナイトシティでそういう奴が見つかるといいな",
              "maleVariant": "",
              "stringId": "3843340675191533584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃ、おやすみ",
              "maleVariant": "",
              "stringId": "3843340675241865228"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アフターライフで… 待ってるぜ！",
              "maleVariant": "",
              "stringId": "3843340675275419664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、そいつは夢じゃない。記憶だ",
              "maleVariant": "",
              "stringId": "3843352004157632532"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "元気でな、V",
              "maleVariant": "",
              "stringId": "3843352115659010064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？ 大丈夫か…？",
              "maleVariant": "",
              "stringId": "3843352167718711312"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}