  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まったく… 最初からそうしてればよかったものを",
              "maleVariant": "",
              "stringId": "3267989084935593984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ニカに真実を伝えるべきだった",
              "maleVariant": "",
              "stringId": "3268124831890055172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待て――ドアの向こうに誰かいる。連中は始末したんじゃないのか？",
              "maleVariant": "",
              "stringId": "3268138122431471616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前のせいで、罪なき人々が死ぬことになる",
              "maleVariant": "",
              "stringId": "3272557257634652160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人のことが言えるわけ？",
              "maleVariant": "人のことが言えるのか？",
              "stringId": "3273826891792949248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ニカの気持ちもわかる",
              "maleVariant": "",
              "stringId": "3275493056793583616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ガスパル以外にも同じようなことを？",
              "maleVariant": "",
              "stringId": "3275493314407735296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいか。俺のクリニックで、一日俺と同じ仕事をしたことがない限り、お前に俺を非難する権利はない",
              "maleVariant": "",
              "stringId": "3275494391924432896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "助かる可能性があれば治療はする",
              "maleVariant": "",
              "stringId": "3275494523189370880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしがいなくても、あんたがニカに真実を伝えれば済んでた",
              "maleVariant": "俺がいなくても、あんたがニカに真実を伝えれば済んでた",
              "stringId": "3314187929542631424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはあらゆる患者をクリニックに受け入れ、そこから“寄付”用を選んでたってこと？",
              "maleVariant": "あんたはあらゆる患者をクリニックに受け入れ、そこから“寄付”用を選んでたってことか？",
              "stringId": "3314213049665495040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クリニックは何もかもが不足してるんだ。現実を見て、困難な決断を下さないとやっていけない",
              "maleVariant": "",
              "stringId": "3314221865337528320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ニカの弟は文字通り「救いようのない」ジャンキーだった。それを言ったところで、あいつは聞く耳を持たなかっただろう",
              "maleVariant": "",
              "stringId": "3314223862379880448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは依頼をこなしただけ。ニカが間違ってたとは思わない",
              "maleVariant": "俺は依頼をこなしただけだ。ニカが間違ってたとは思わない",
              "stringId": "3314231361057234944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん… その通りだな",
              "maleVariant": "",
              "stringId": "3314240315728502784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、これからどうするの？",
              "maleVariant": "で、これからどうするんだ？",
              "stringId": "3314291449881075712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "忙しいんだがな",
              "maleVariant": "",
              "stringId": "3334464154773295112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手は大丈夫？",
              "maleVariant": "手は大丈夫か？",
              "stringId": "3335044127607136256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メスはどっちの手で持つの？",
              "maleVariant": "メスはどっちの手で持つんだ？",
              "stringId": "3335044763094523904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無事な方だ",
              "maleVariant": "",
              "stringId": "3335045579373191168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "不幸中の幸いね",
              "maleVariant": "不幸中の幸いだな",
              "stringId": "3335046494016675840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ガスパル以外にも同じようなことを？",
              "maleVariant": "",
              "stringId": "3335879274384498692"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手は大丈夫？",
              "maleVariant": "手は大丈夫か？",
              "stringId": "3335879855933136896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ニカの気持ちもわかる",
              "maleVariant": "",
              "stringId": "3335881434635608064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたも大変ね",
              "maleVariant": "あんたも大変だな",
              "stringId": "3335883237884338176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんなことはもうやめた方がいい",
              "maleVariant": "",
              "stringId": "3335883637064638464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“寄付”を続ける限り、また誰かに狙われる。その時はどうするの？",
              "maleVariant": "“寄付”を続ける限り、また誰かに狙われる。その時はどうするんだ？",
              "stringId": "3335884343603535872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしも傭兵の世界で色々見てきたけど… あんたと代わりたいとは思えない",
              "maleVariant": "俺も傭兵の世界で色々見てきたが… あんたと代わりたいとは思えないな",
              "stringId": "3335884382744780800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クリニックのやり方について口を出すつもりはない。でも、ニカには真実を伝えるべきだった",
              "maleVariant": "クリニックのやり方について口を出すつもりはない。だが、ニカには真実を伝えるべきだった",
              "stringId": "3335884432774438916"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前を呼ばないことだけは確かだな",
              "maleVariant": "",
              "stringId": "3335892262566928384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "幸い、その必要もなければ、非難される筋合いもない",
              "maleVariant": "",
              "stringId": "3335900857232695296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "モラルに囚われない人間はナイトシティでも引く手あまたよ",
              "maleVariant": "モラルに囚われない人間はナイトシティでも引く手あまただぞ",
              "stringId": "3335901821167640588"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ノース・オークに住み、レイフィールドを転がしながら、朝にはセンツォンを嗜めと？",
              "maleVariant": "",
              "stringId": "3335901821184417792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜナイトシティで働かないの？",
              "maleVariant": "なぜナイトシティで働かないんだ？",
              "stringId": "3335901984896491520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "伝えるべきことなど何もない。患者がすべてだ、他は知ったことじゃない",
              "maleVariant": "",
              "stringId": "3335924011216838656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "場所を移したらどう？",
              "maleVariant": "場所を移したらどうだ？",
              "stringId": "3337372258871451648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すでに経験済みだ",
              "maleVariant": "",
              "stringId": "3337377831994171392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴に終わったと伝えろ。仕事に戻らないとな",
              "maleVariant": "",
              "stringId": "3338929207870136320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ残ってたみたいね",
              "maleVariant": "まだ残ってたようだな",
              "stringId": "3343379596644360192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、他の患者たちは？",
              "maleVariant": "",
              "stringId": "3343389545416007680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オデルと一緒に、外で待ってる",
              "maleVariant": "",
              "stringId": "3343389545416007692"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何をぼさっとしてるんだ。早く始末しろ",
              "maleVariant": "",
              "stringId": "3366452040601477120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "終わったら、外にいるオデルに伝えてくれ",
              "maleVariant": "",
              "stringId": "3367862511886225408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ニカと話せば解決してた",
              "maleVariant": "",
              "stringId": "3377994979001540608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ニカと話せば解決してた。フィクサーに依頼するまでもないでしょ",
              "maleVariant": "ニカと話せば解決してた。フィクサーに依頼するまでもないだろ",
              "stringId": "3377995705337552896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ガスパルを見つけた",
              "maleVariant": "",
              "stringId": "3378049357465034756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別の部屋でずっとガスパルの遺体を保管してたとはね",
              "maleVariant": "別の部屋でずっとガスパルの遺体を保管してたとはな",
              "stringId": "3378050232765947904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "捜索を依頼した覚えはないぞ",
              "maleVariant": "",
              "stringId": "3378051082766819328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ガスパルの死因はモルヒネの過剰摂取だった",
              "maleVariant": "",
              "stringId": "3378052546444697604"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "金に困ってるわりには、高そうな冷蔵庫を使ってるのね",
              "maleVariant": "金に困ってるわりには、高そうな冷蔵庫を使ってるだな",
              "stringId": "3378053047680802816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "代謝エディターがイカれてたうえ、強力なモルヒネを投与されてる",
              "maleVariant": "",
              "stringId": "3378053777691025408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ずいぶんと金がかかりそうな冷蔵庫だったけど――金に困ってたんじゃないの？",
              "maleVariant": "ずいぶんと金がかかりそうな冷蔵庫だったが――金に困ってたんじゃないのか？",
              "stringId": "3378053923837353984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あれは医療ミス？",
              "maleVariant": "あれは医療ミスか？",
              "stringId": "3379388118615891968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "運び込まれてきたとき、奴はヤクでぶっ飛んでた。手の施しようがなかったから、安らかな最期を与えたまでだ",
              "maleVariant": "",
              "stringId": "3379392145248395264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あれは投資だ。利益を得るためには、準備を整えなければならない場合がある",
              "maleVariant": "",
              "stringId": "3379395521579229184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ガスパルのインプラントを抜いたでしょ。なぜ？",
              "maleVariant": "ガスパルのインプラントを抜いただろ。なぜだ？",
              "stringId": "3379409947065753600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ガスパルのインプラントが抜かれてた",
              "maleVariant": "",
              "stringId": "3379732750130388992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼にはもう必要ないが、他の患者たちは違う",
              "maleVariant": "",
              "stringId": "3379734235249549312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "結局、ニカが怖くて弟を殺したって言えなかったんでしょ",
              "maleVariant": "結局、ニカが怖くて弟を殺したって言えなかったんだろ",
              "stringId": "3379740906357170176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それと――インプラントを無駄にするわけにはいかなった",
              "maleVariant": "",
              "stringId": "3379742520342126592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分の身を案じていたわけではない。私の死は、患者たちの死を意味するからだ",
              "maleVariant": "",
              "stringId": "3379743896023506944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何を言っても、あいつは俺を殺してたさ",
              "maleVariant": "",
              "stringId": "3379783210946174976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他に何か用はある？",
              "maleVariant": "他に何か用はあるか？",
              "stringId": "3397149627085398016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… さっさとこれを片づけないと",
              "maleVariant": "",
              "stringId": "3397152468743135232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "中に戻るよう伝えろ。さっさと片づけないとな",
              "maleVariant": "",
              "stringId": "3397153502018314260"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}