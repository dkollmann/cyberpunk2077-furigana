  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よく来てくれた。ブラジル情報局所属のスティーブン・サントスだ",
              "maleVariant": "",
              "stringId": "2765121586487603200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "了解",
              "maleVariant": "",
              "stringId": "2931661998404231172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スカベンジャーはどうやって信号の追跡を？",
              "maleVariant": "",
              "stringId": "2931755600941768704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。さっそく始めましょ",
              "maleVariant": "わかった。さっそく始めるか",
              "stringId": "2931757300591190016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中はどうやって信号を？",
              "maleVariant": "",
              "stringId": "2931758498652184576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはわからない",
              "maleVariant": "",
              "stringId": "2931760787769090048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "現地でバナの信号を捕捉したら、アナから連絡する",
              "maleVariant": "",
              "stringId": "2931768759865925632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リバコフについてもっと情報はない？",
              "maleVariant": "リバコフについてもっと情報はないか？",
              "stringId": "3083831983447191552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "歓迎をしてやる前に、リバコフについて知っておくべきことは？",
              "maleVariant": "",
              "stringId": "3083832866583064576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "記録されてるだけで167人殺してる、凄腕のスナイパー",
              "maleVariant": "",
              "stringId": "3083834951001157632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんな結果が出てくるかしら…",
              "maleVariant": "どんな結果が出てくるやら…",
              "stringId": "3092885108334592000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんな情報が出てくるやら…",
              "maleVariant": "",
              "stringId": "3092890931236503556"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど、前は組織にいたんだな。アラサカの防諜部か… 即日解雇、退職金はなし。ハッ、なかなか辛い思いをしたようだな",
              "maleVariant": "",
              "stringId": "3092891623078559744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんだ、昔は組織にいたんじゃないか。ノーマッドの… バッカー？ 聞いたことがないな。まあ、ノーマッドのクランをすべて把握するなんて無理な話か",
              "maleVariant": "",
              "stringId": "3092900206084161536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘイウッド出身？ フッ、どうりで経歴が派手なわけだ。窃盗、暴行、強盗… まさにフルコースだな。我々とは住む世界が違うようだ",
              "maleVariant": "",
              "stringId": "3092900273260134408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[コンピューターを閉じる] 断る",
              "maleVariant": "",
              "stringId": "3094136341007499264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪いけどスキャンはなしよ。何をされるかわかったもんじゃない",
              "maleVariant": "悪いがスキャンはなしだ。何をされるかわかったもんじゃない",
              "stringId": "3094139959802658816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうかバナを見つけて… せめて生体モニターだけでも",
              "maleVariant": "",
              "stringId": "3095723666732810240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "期待したのに、紺碧プラザの情報はなし？",
              "maleVariant": "期待したのに、紺碧プラザの情報はなしか？",
              "stringId": "3195655396473417728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フム… 暗号化されてるうえ、アクセス不可… 権限レベル4が必要？ あんた… いったい何者なんだ？",
              "maleVariant": "",
              "stringId": "3195657112581287936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バナの信号は現在、テラ・コグニータの自然史博物館、オーガニトピアから出てる",
              "maleVariant": "",
              "stringId": "3195722792563355648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "問題は、スカベンジャーの連中も信号に気づいたことだ。それだけでも十分最悪だが――",
              "maleVariant": "",
              "stringId": "3195726344199319552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "元スペツナズのボリス・リバコフがついてる。墓地を1つ埋められるほど殺してきた男よ",
              "maleVariant": "",
              "stringId": "3195727643712139264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分たちで捜索すればいいんじゃ？",
              "maleVariant": "",
              "stringId": "3197101732901138436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "契約相手は全員スキャンするのが局の方針だ",
              "maleVariant": "",
              "stringId": "3214490303908085760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ならあたしを雇うのはやめておく？",
              "maleVariant": "なら俺を雇うのはやめておくか？",
              "stringId": "3236426501529636864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "採用よ",
              "maleVariant": "",
              "stringId": "3236429644371955712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アナ…",
              "maleVariant": "",
              "stringId": "3236430716637384704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信号はいつ消えてもおかしくない。時間がないのよ",
              "maleVariant": "",
              "stringId": "3236430986398240768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "動かないでくれ",
              "maleVariant": "",
              "stringId": "3237847679431446528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうすべきかもな",
              "maleVariant": "",
              "stringId": "3237904575819517952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "紺碧プラザの情報はないの？",
              "maleVariant": "紺碧プラザの情報はないのか？",
              "stringId": "3237905733111877636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それだけ？",
              "maleVariant": "それだけか？",
              "stringId": "3237905955409989632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それだけ？ あらゆるデータベースにアクセスできるはずなのに、そんなつまらない情報しかないの？",
              "maleVariant": "それだけ？ あらゆるデータベースにアクセスできるはずなのに、そんなつまらない情報しかないのか？",
              "stringId": "3237906504108838912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "探してる人物ね",
              "maleVariant": "探してる人物だな",
              "stringId": "3239327310412091396"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "責任者はあんた？ 詳細を教えて",
              "maleVariant": "責任者はあんたか？ 詳細を教えてくれ",
              "stringId": "3239332958461857792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたが責任者のようね。詳細を教えてくれる？",
              "maleVariant": "あんたが責任者のようだな。詳細を教えてくれるか？",
              "stringId": "3239333535765860352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハチの巣を自らつつくタイプ？ あなたとは気が合いそうね",
              "maleVariant": "",
              "stringId": "3239333850640650240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "十分だ。あんたの素性はわかってる",
              "maleVariant": "",
              "stringId": "3241573680363704320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とんだ素人ね",
              "maleVariant": "とんだ素人だな",
              "stringId": "3241580327194693632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさか依頼人がこんな素人だとは",
              "maleVariant": "",
              "stringId": "3241581076179947520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ… 我々が素人？",
              "maleVariant": "",
              "stringId": "3241582516336168960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "素性のチェックなんて事前にするものよ。もしかして、これが初めての任務？",
              "maleVariant": "素性のチェックなんて事前にするもんだ。もしかして、これが初めての任務か？",
              "stringId": "3241582647684993024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "な… バカなことを…",
              "maleVariant": "",
              "stringId": "3241584761010229248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "街の大半の人間もね。それなりに名は通ってるの",
              "maleVariant": "街の大半の人間もな。それなりに名は通ってるんでね",
              "stringId": "3241588555513171968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンズから人を探してるって聞いた。この人がそう？",
              "maleVariant": "ハンズから人を探してるって聞いた。この人がそうか？",
              "stringId": "3241590792788480000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よくわかったな",
              "maleVariant": "",
              "stringId": "3241591585478381568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マーク・バナ、37歳。情報局に在籍して12年。身長6フィート、茶髪、筋肉質。戦闘訓練経験あり",
              "maleVariant": "",
              "stringId": "3241597669668655104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ずいぶん時間が経ってる… まだ生きてると思うの？",
              "maleVariant": "ずいぶん時間が経ってる… まだ生きてると思うのか？",
              "stringId": "3241609030226251776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、やってみる",
              "maleVariant": "ああ、やってみる",
              "stringId": "3241726614737096704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スカベンジャーに元スペツナズ、6ヶ月間止まっていた生体モニター… はあ…",
              "maleVariant": "",
              "stringId": "3241732650256666624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちも嫌な予感はしてるさ",
              "maleVariant": "",
              "stringId": "3241732957397159936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "武器の強化型照準システムとリンクした視覚インプラントを使用。その他にも反応加速器、アドレナリンポンプ…",
              "maleVariant": "",
              "stringId": "3242022664416890880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "試験段階のソビエト製テックもだ。どうやら奴は敵のオプティクスをハックできるらしい",
              "maleVariant": "",
              "stringId": "3242028786724999168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やられた… クソ… 追われてる… うう… 応援を… 応援を… 頼む…",
              "maleVariant": "",
              "stringId": "3246530062338854912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もういい",
              "maleVariant": "",
              "stringId": "3246530594998685696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バナは6ヶ月前にドッグタウンで行方不明になった。消息を絶つ前の最後の連絡が… これだ",
              "maleVariant": "",
              "stringId": "3246532251128008704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ何か隠してる気がするけど",
              "maleVariant": "まだ何か隠してる気がするが",
              "stringId": "3246597269919993860"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分で信号を調べられないの？",
              "maleVariant": "自分で信号を調べられないのか？",
              "stringId": "3246598484959531008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "画面を見て、V",
              "maleVariant": "",
              "stringId": "3246827192672686080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ何か隠してるんじゃない？",
              "maleVariant": "まだ何か隠してるんじゃないか？",
              "stringId": "3246834899706286080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたたちで信号を調べられないの？",
              "maleVariant": "あんたたちで信号を調べられないのか？",
              "stringId": "3246834981948198912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私たちはアナリストよ。フィールドワークの権限も経験もない",
              "maleVariant": "",
              "stringId": "3246835609097310208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、残念ながら",
              "maleVariant": "",
              "stringId": "3246838883724660736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはただのV",
              "maleVariant": "俺はただのVだ",
              "stringId": "3285654765451972608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここは？",
              "maleVariant": "",
              "stringId": "3285671494517362688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも。無所属のVよ",
              "maleVariant": "どうも。無所属のVだ",
              "stringId": "3285671845345726464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミリタリズムのシックなインテリア。いかにもな造りね",
              "maleVariant": "ミリタリズムのシックなインテリア。いかにもな造りだな",
              "stringId": "3285671976996540416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうか。ちょっと情報をスキャンさせてくれ",
              "maleVariant": "",
              "stringId": "3285679148165861376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "情報局が所有する作戦拠点だ。テックについてもすぐに見てもらうが、まずは君をスキャンさせてくれ",
              "maleVariant": "",
              "stringId": "3285698258220191744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、プロセッサーは見つかった… ええ… 交渉してるわ…",
              "maleVariant": "",
              "stringId": "3287100471610019840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さっきの録音を聞けばわかるでしょ。きっともう死んでる。なぜわざわざ探すの？",
              "maleVariant": "さっきの録音を聞けばわかるだろ。きっともう死んでる。なぜわざわざ探すんだ？",
              "stringId": "3287185886463496192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死んだと決まったわけじゃない。生きてる可能性が少しでもある限り、探すのが務めよ",
              "maleVariant": "",
              "stringId": "3287204821263028224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "情報局ならエージェントが大勢いるでしょ。あたしの手伝いなんて必要？",
              "maleVariant": "情報局ならエージェントが大勢いるだろ。俺の手伝いなんて必要か？",
              "stringId": "3287207273018265600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "制約があって動けないから、傭兵に依頼したんだ。特殊部隊を投入すれば国際問題になりかねない",
              "maleVariant": "",
              "stringId": "3287207761939894272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "組織は捜索を許可してないんでしょ。なぜ独自で行動を？",
              "maleVariant": "組織は捜索を許可してないんだろ。なぜ独自で行動を？",
              "stringId": "3294055720795082752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンズの情報か。抜け目のない奴だ",
              "maleVariant": "",
              "stringId": "3294056814736674816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さすがプロのフィクサーね。確かに私たちは独自に動いてる。わけを知りたい？",
              "maleVariant": "",
              "stringId": "3294059982023397376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…数日前に彼の生体モニターが信号を送り始めた。発信源はドッグタウンだが、移動している。何か質問は？",
              "maleVariant": "",
              "stringId": "3294065562779750400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バナが消息を絶ったとき、局は死亡したと断定した。遺体も見つかってないのに",
              "maleVariant": "",
              "stringId": "3294071423413620736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その後、彼の信号が復活したにも関わらず、「根拠が不十分。アルゴリズムによると生存率は4%しかない」とか言って何もしなかった",
              "maleVariant": "",
              "stringId": "3294158019450212352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜそこまでバナにこだわるの？",
              "maleVariant": "なぜそこまでバナにこだわるんだ？",
              "stringId": "3294161483123232772"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハチの巣を自らつついてるのは、そっちの方ね",
              "maleVariant": "ハチの巣を自らつついてるのは、そっちの方だな",
              "stringId": "3294163805542912000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "情報局はバナの件を隠蔽しようとしてる",
              "maleVariant": "",
              "stringId": "3294163997423931392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バナにこだわる理由は？",
              "maleVariant": "",
              "stringId": "3294181540586950656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "要は、局が避けたがるほどのヤバい事態にあたしを巻き込むつもり？",
              "maleVariant": "要は、局が避けたがるほどのヤバい事態に俺を巻き込むつもりか？",
              "stringId": "3294183038406463488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも怪しい。局は明らかにバナの件を隠蔽しようとしてる",
              "maleVariant": "",
              "stringId": "3294183135714316288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく――重要なのは、バナが我々の友人であり、彼の信号を調べるのがあなたの仕事だってこと。わかった？",
              "maleVariant": "",
              "stringId": "3294183249933602816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その先は俺たちに任せろ",
              "maleVariant": "",
              "stringId": "3294184385281363968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもなぜ？",
              "maleVariant": "",
              "stringId": "3294189955400962048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "情報局のような組織からしたら、知らないフリをした方がいいときもある。少なくとも表向きにはね",
              "maleVariant": "情報局のような組織からしたら、知らないフリをした方がいいときもある。少なくとも表向きにはな",
              "stringId": "3294190031217201152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふざけてるわ。私たちはアルゴリズムより信念と義理を選ぶ。たとえ考えが甘いと言われようが、局が見捨てようがね",
              "maleVariant": "",
              "stringId": "3294254080604868608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼には恩がある。俺たちを情報局に紹介してくれたのはバナだ",
              "maleVariant": "",
              "stringId": "3294267836428816384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それと… 許しがたい不義理を目の当たりにして、放っておくことはできない",
              "maleVariant": "",
              "stringId": "3294271395513864192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "局が何よ",
              "maleVariant": "",
              "stringId": "3294282745334042624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜ情報局に黙って捜索を？",
              "maleVariant": "",
              "stringId": "3298227934687760384"
            }
          ]
        }
