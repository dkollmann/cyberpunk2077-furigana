      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やあ、V。ナイトシティ市長のジェファーソン・ペラレスだ",
              "maleVariant": "",
              "stringId": "1790713264939638784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、ペラレスだ",
              "maleVariant": "",
              "stringId": "1790713335756267524"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "色々と政策に取り組んでるんだが、君に相談がある",
              "maleVariant": "",
              "stringId": "1801097216932954112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、警護部隊の責任者になってくれないかね？",
              "maleVariant": "",
              "stringId": "1801097369924386816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知っての通り、私が信頼を置ける人間は少ないからな… しかも来週からは増税が始まるから、特に警戒しないといけないんだ…",
              "maleVariant": "",
              "stringId": "1801097781452718080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "事務所まで来てくれないか。安全のためなら金は惜しまない",
              "maleVariant": "",
              "stringId": "1801098777348259840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "では… 連絡を待ってるぞ、V",
              "maleVariant": "",
              "stringId": "1801099272192245760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実は… とても重要な話がある。まず誰にも聞かれない場所へ移動してくれ",
              "maleVariant": "",
              "stringId": "1801100374941876224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "妻のエリザベス… どうやらあいつも元からグルだったみたいなんだ",
              "maleVariant": "",
              "stringId": "1801100576922779648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“ビタミン剤”だとか言って、私によくわからない薬を飲ませようとしてくる",
              "maleVariant": "",
              "stringId": "1801100731424161792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "研究所で解析してもらった結果、問題ないとは言われたが、どうも信用できない",
              "maleVariant": "",
              "stringId": "1801100898642673664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時間がある時に連絡をくれ。それとこのメッセージは削除しろ、いいな",
              "maleVariant": "",
              "stringId": "1801101104415227904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us][db_db]Peralez says he tried to call V for a while",
              "maleVariant": "",
              "stringId": "3192623125638586368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us][db_db]She probably must be busy now as a corpo bigshot",
              "maleVariant": "",
              "stringId": "3192862592446144512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us][db_db]He has this great project about tackling homelessness",
              "maleVariant": "",
              "stringId": "3192863031019347968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us][db_db]And he hopes she can help fund it or pass it on to bosses",
              "maleVariant": "",
              "stringId": "3192863458016272384"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}