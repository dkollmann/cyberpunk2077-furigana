    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウソ…",
              "maleVariant": "ウソだろ…",
              "stringId": "1973425649686134788"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん…",
              "maleVariant": "",
              "stringId": "2253214837272469512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞こえたな？",
              "maleVariant": "",
              "stringId": "2253304926258323464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、嫌な予感がする",
              "maleVariant": "",
              "stringId": "2253305156508835848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マジかよ…",
              "maleVariant": "",
              "stringId": "2253331246690328584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい！ こっちだ！",
              "maleVariant": "",
              "stringId": "2450563902790680576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8年？ 20歳の時からじゃ、言い訳もできねえな。ピニャータみてえに戦闘用クロームを入れまくったらどうなるか想像できなかったのか？ 遅かれ早かれ、内側から破裂してたさ",
              "maleVariant": "",
              "stringId": "3359781174220017664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おいおい、落ち着けよ。話をしよう。それとも丸腰の相手を殺す気か？",
              "maleVariant": "",
              "stringId": "3416322151028199440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたがラインダー？ 想像してた姿と… だいぶ違う",
              "maleVariant": "お前がラインダーか？ 想像してた姿と… だいぶ違うな",
              "stringId": "3416322152605257736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[武器を下ろす] ドッグタグを渡して",
              "maleVariant": "[武器を下ろす] ドッグタグを渡せ",
              "stringId": "3416322153880326144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[撃つ] 人と約束したのよ",
              "maleVariant": "[撃つ] 人と約束したんだ",
              "stringId": "3416322153880326160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レジーナに連絡して",
              "maleVariant": "レジーナに連絡しろ",
              "stringId": "3416322153880326176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたを見逃したとしたら、どうするつもり？",
              "maleVariant": "お前を見逃したとしたら、どうするつもりだ？",
              "stringId": "3416322153897103360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人と約束したのよ",
              "maleVariant": "人と約束したんだ",
              "stringId": "3416322153964212244"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正義は果たされたな。ブリアナの注文通りだ",
              "maleVariant": "",
              "stringId": "3416322153964212260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正義ね…",
              "maleVariant": "正義か…",
              "stringId": "3416322153980989444"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分で選んだ稼業だろ。お前は仕事をしただけだ",
              "maleVariant": "",
              "stringId": "3416322153980989456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんなことを続けてたら、いつかはあたしが銃口を向けられる側になる気がする",
              "maleVariant": "こんなことを続けてたら、いつかは俺が銃口を向けられる側になる気がする",
              "stringId": "3416322153980989472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたがラインダー？ 想像してたのと少し違う",
              "maleVariant": "お前がラインダーか？ 想像してたのと少し違うな",
              "stringId": "3416322172553367552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたがスタックスで無残に殺した人たちよ",
              "maleVariant": "お前がスタックスで無残に殺した人たちだ",
              "stringId": "3416322177318096912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何だと？ 待て、待て！ 確かにスタックスに行って、ぼったくられそうになったが… 俺は誰も殺しちゃいねえ。ただ…",
              "maleVariant": "",
              "stringId": "3416322177334874116"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ロングショア・スタックスの人たちから、お届け物よ",
              "maleVariant": "ロングショア・スタックスの人たちから、お届け物だ",
              "stringId": "3416322182049271808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソが！ ふざけるな！",
              "maleVariant": "",
              "stringId": "3416322182049271820"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの症状は典型的なサイバーサイコシスよ。レジーナ・ジョーンズの連絡先を送る。彼女に事情を説明して、迎えをよこしてもらって",
              "maleVariant": "お前の症状は典型的なサイバーサイコシスだ。レジーナ・ジョーンズの連絡先を送る。彼女に事情を説明して、迎えをよこしてもらえ",
              "stringId": "3416322228639600660"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺はサイバーサイコなんかじゃねえ",
              "maleVariant": "",
              "stringId": "3416322228639600672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連絡しないなら、この場で殺す",
              "maleVariant": "",
              "stringId": "3416322228656377864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインダー！",
              "maleVariant": "",
              "stringId": "3416322228656377876"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしもし？ その… この番号にかけて助けてもらえと言われたんだ… サイバーサイコシスだって… そうだ… 今送る… ここで待ってるよ…",
              "maleVariant": "",
              "stringId": "3416322228673155072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これでいいか？",
              "maleVariant": "",
              "stringId": "3416322238638821388"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "許してもらえると思ってるの？",
              "maleVariant": "許してもらえると思ってるのか？",
              "stringId": "3416322254845612032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "命令に従っただけ？ ふざけないで",
              "maleVariant": "命令に従っただけ？ ふざけるなよ",
              "stringId": "3416322254862389252"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「何をしたにしても」？ 何をしたか教えてあげる",
              "maleVariant": "「何をしたにしても」？ 何をしたか教えてやるよ",
              "stringId": "3416322254862389268"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "現場を検証したけど、使用された銃は軍用モデル。あんた以外に扱える代物じゃない",
              "maleVariant": "現場を検証したが、使用された銃は軍用モデル。お前以外に扱える代物じゃない",
              "stringId": "3416322263167111192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それからあんたはジェイソンの頭を地面に打ちつけてる。他の人を惨殺し、辺りを血の海にした",
              "maleVariant": "それからお前はジェイソンの頭を地面に打ちつけてる。他の人を惨殺し、辺りを血の海にした",
              "stringId": "3416322263183888396"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺がそんなことするわけ… 本当にやってたら――",
              "maleVariant": "",
              "stringId": "3416322263183888408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "嘘はついてねえな。マジで覚えてねえんだ。極端な改造と短気な性格を組み合わせりゃ、あっという間に殺人マシンの出来上がりだ",
              "maleVariant": "",
              "stringId": "3416322263183888420"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サンバーサイコシスのようね",
              "maleVariant": "サイバーサイコシスのようだな",
              "stringId": "3416322273954861056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レジーナに連絡すべき？",
              "maleVariant": "レジーナに連絡すべきか？",
              "stringId": "3416322273971638284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインダー… 記憶が飛んだり、我を失ったり… 自分がサイバーサイコシスかもと思ったことはない？",
              "maleVariant": "ラインダー… 記憶が飛んだり、我を失ったり… 自分がサイバーサイコシスかもと思ったことはないか？",
              "stringId": "3416322278417600524"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サイバーサイ…？ じゃあ、お前はカモられそうになっても抵抗しねえのか？ 冗談じゃねえ！",
              "maleVariant": "",
              "stringId": "3416322278434377736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "謝って済むなら傭兵はいらない。ここで見逃したら、別の誰かを殺しに行くだけでしょ",
              "maleVariant": "謝って済むなら傭兵はいらない。ここで見逃したら、別の誰かを殺しに行くだけだろ",
              "stringId": "3416322282343469068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この状態で人を殺せると思うか？  ああ？",
              "maleVariant": "",
              "stringId": "3416322282343469080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今度は命令を言い訳にするの？ あんたは、あんたの意志で罪のない人々を殺したのよ",
              "maleVariant": "今度は命令を言い訳にするのか？ お前は、お前の意志で罪のない人々を殺したんだ",
              "stringId": "3416322286370000908"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺を化け物に仕立て上げたきゃ勝手にしろ！ その方が都合がいいもんな！？",
              "maleVariant": "",
              "stringId": "3416322286386778116"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺を見ろよ！ これ以上失うものがあるか！？ 何もかも、すべて奪われた！ 昔は兵士だったが、今はこのザマだ！",
              "maleVariant": "",
              "stringId": "3416322290732077056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "泥沼にはまる前に、抜け出すことはできたはずよ",
              "maleVariant": "泥沼にはまる前に、抜け出すことはできたはずだ",
              "stringId": "3416322290748854272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "当時はまだ20歳だぞ？ ハンセンを信じてたんだ。他の連中と同じようにな。今になって全部俺が悪いのか？",
              "maleVariant": "",
              "stringId": "3416322290748854284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スタックスで俺に殺されただと？ 知るかよ！ ドッグタウンもクソ食らえ！ あのゴミ溜めを、中にいる奴らもまとめて焼き尽くしてやりてえ！",
              "maleVariant": "",
              "stringId": "3416322290765631496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ラインダーの頭に銃を突きつける] 言葉には気をつけるのね",
              "maleVariant": "[ラインダーの頭に銃を突きつける] 言葉には気をつけるんだな",
              "stringId": "3416322303784751104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[撃つ] あんたの話は聞き飽きた",
              "maleVariant": "[撃つ] お前の話は聞き飽きた",
              "stringId": "3416322303784751120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言葉は慎重に選ぶのね",
              "maleVariant": "言葉は慎重に選ぶんだな",
              "stringId": "3416322312156581908"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたを見逃したとして、その後は？ どうするつもり？",
              "maleVariant": "お前を見逃したとして、その後は？ どうするつもりだ？",
              "stringId": "3416322322759782416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え…？ あー… どっかの有名なリパーに助けを求める。こんな状態のまま生きてくわけにもいかねえからな",
              "maleVariant": "",
              "stringId": "3416322322776559616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしサイバーサイコシスなら、まだ希望はあるかも。レジーナがいる",
              "maleVariant": "もしサイバーサイコシスなら、まだ希望はあるかもな。レジーナがいる",
              "stringId": "3416322327558066180"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手っ取り早く稼ぐために軍に入った奴はたくさん知ってる。俺だってその1人さ。だが、8年経って良心が芽生えるなんて話は聞いたことがねえな",
              "maleVariant": "",
              "stringId": "3416322327574843396"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはつまり、殺せって言ってるの？",
              "maleVariant": "それはつまり、殺せって言ってるのか？",
              "stringId": "3416322327574843408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "因果応報さ。ま、お前がブルーになってる奴に出会うたびに、いちいち救いの手を差し伸べるつもりなら話は別だがな",
              "maleVariant": "",
              "stringId": "3416322327608397824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだ。いや、ラインダーという男の残骸だな。で、誰に雇われた？ ハンセンは死んだだろ",
              "maleVariant": "",
              "stringId": "3416322335409803272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだ。いや、ラインダーという男の残骸だな。で、誰に雇われたんだ？ ハンセンか？",
              "maleVariant": "",
              "stringId": "3416322337641173000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインダーじゃない。でけえスカベンジャーだ",
              "maleVariant": "",
              "stringId": "3416322356096110600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "銃を突きつけられた途端、これかよ。情にほだされるなよ、V。こいつは人殺しのクズ野郎だ",
              "maleVariant": "",
              "stringId": "3416322357555728392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつはドッグタウンが丸飲みし、ケツからひねり出したクソのひとつだ。何もかもハンセンのせいにできりゃ話は簡単だが、実際はそうじゃねえ",
              "maleVariant": "",
              "stringId": "3416322359468331016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ラインダーを撃つ]  死んでもらう",
              "maleVariant": "",
              "stringId": "3416327443132551168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "たまに… 記憶が飛ぶときがあるんだ。病気なんだよ！ むしろ何があったのか教えてくれ",
              "maleVariant": "",
              "stringId": "3420474882580123648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはジェイソンと、他に7人も殺したのよ",
              "maleVariant": "お前はジェイソンと、他に7人も殺したんだ",
              "stringId": "3420494871458582532"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "記憶喪失？ ずいぶん都合のいい話ね",
              "maleVariant": "記憶喪失？ ずいぶん都合のいい話だな",
              "stringId": "3420497012449447936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スカベンジャーにやられたの？",
              "maleVariant": "スカベンジャーにやられたのか？",
              "stringId": "3420498423060992000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの体… スカベンジャーの連中にやられたの？",
              "maleVariant": "お前の体… スカベンジャーの連中にやられたのか？",
              "stringId": "3420498736425832448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしを襲ったあのデカブツは、リパーには見えなかったけど",
              "maleVariant": "俺を襲ったデカブツは、リパーには見えなかったぞ",
              "stringId": "3420501638699651072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはジェイソンって子と、他にも7人殺した。なぜ？ なんであんなことを？",
              "maleVariant": "お前はジェイソンって奴と、他にも7人殺した。なぜだ？ なぜあんなことを？",
              "stringId": "3420503964994867200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインダー… 誰もが、あんたのことをイカれた殺人鬼だって言ってる。つまらない言い訳が通用すると思わないで",
              "maleVariant": "ラインダー… 誰もが、お前のことをイカれた殺人鬼だって言ってる。つまらない言い訳が通用すると思うなよ",
              "stringId": "3420504065406504960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜバーゲストを抜けたの？",
              "maleVariant": "なぜバーゲストを抜けたんだ？",
              "stringId": "3420504402846650368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は病気なんだよ。スカベンジャーにクロームを売って治療費にするつもりだった。それでジェイソンに橋渡し役を依頼したんだ",
              "maleVariant": "",
              "stringId": "3420512963471446016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "違う、奴が先に俺をゆすろうとしたんだ！ でかい銃を振り回して… 野蛮人め。その後は… お、覚えてねえ…",
              "maleVariant": "",
              "stringId": "3420518075640156160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも、ジェイソンは言われた通りやったんでしょ",
              "maleVariant": "だが、ジェイソンは言われた通りやったんだろ",
              "stringId": "3420538932873760768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、あそこはどういう場所だって言うの？",
              "maleVariant": "じゃあ、あそこはどういう場所だって言うんだ？",
              "stringId": "3420586978424754176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセンとは何があったの？ 見捨てられた？",
              "maleVariant": "ハンセンとは何があったんだ？ 見捨てられたのか？",
              "stringId": "3420591378165764096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見りゃわかるだろ？ 用済みになった途端、お払い箱さ。8年間もあいつの下で働いたのによ…",
              "maleVariant": "",
              "stringId": "3424942648271933440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺をハメたのはよそ者のスカベンジャーじゃねえ――ドッグタウンのジェイソンなんだよ！ あそこにはあいつみてえなクズしかいねえ！ バーゲストのほうがまだマシだ！",
              "maleVariant": "",
              "stringId": "3424997478210310144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コンテナに住んでる連中を見ただろ？ 一度入ったら、終わりだ。来る者は拒まず、去る者は許さず――ドッグタウンはそういう場所なんだよ",
              "maleVariant": "",
              "stringId": "3425002185494466560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "野蛮人はあんたよ",
              "maleVariant": "野蛮人はお前だ",
              "stringId": "3425704111647330308"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセンの汚れ仕事を何年もやってたことも“覚えてない”わけ？",
              "maleVariant": "ハンセンの汚れ仕事を何年もやってたことも“覚えてない”のか？",
              "stringId": "3425704301716410368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "病気って？",
              "maleVariant": "",
              "stringId": "3425704338777280512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちゃんとした病名はあるわけ？ リパーの診断は？",
              "maleVariant": "ちゃんとした病名はあるのか？ リパーの診断は？",
              "stringId": "3425706447975321600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺が好きで魚みてえに捌かれたと思うのか？ わざわざ第三世代の戦闘ウェアを剥ぎ取って、代わりに粗末なクロームを入れるとでも？ バカバカしい",
              "maleVariant": "",
              "stringId": "3425733229411667968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはこの目で現場を見てきた。野蛮なのはあんたよ",
              "maleVariant": "俺はこの目で現場を見てきた。野蛮なのはお前だ",
              "stringId": "3425749719955378176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはハンセンのために大勢の人間を殺してきた。その記憶も消えたっていうの？",
              "maleVariant": "お前はハンセンのために大勢の人間を殺してきた。その記憶も消えたっていうのか？",
              "stringId": "3425749847831318528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺にどうしろって言うんだよ？ 記憶にないものもひっくるめて、犯した罪を認めりゃ満足するのか？ クソ…！",
              "maleVariant": "",
              "stringId": "3425749940659654656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "後悔はしてるんだ！ ハンセンんとこに入った時はまだガキで、ただただ命令に従ってた。俺が何をしたにしても、悪かったよ！ これでいいか？ そもそも、ドッグタウンのことを何も知らねえくせに…",
              "maleVariant": "",
              "stringId": "3427142301763604480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レオン・ラインダーは死んだと伝えたって、嘘にはならねえ。そうしてくれたら… 隠しておいた物資をお前にやる",
              "maleVariant": "",
              "stringId": "3427221040459517952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや… クソ！ やっぱ命乞いはしねえ。殺すなら殺せ！",
              "maleVariant": "",
              "stringId": "3427221623769763840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺から頼んだんだよ。クロームを売ったんだ。相当ヤバい連中だが、約束は守る",
              "maleVariant": "",
              "stringId": "3427235720456409088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの木偶の坊か？ ハッ、ホント、使えねえ連中だな",
              "maleVariant": "",
              "stringId": "3427237277465939968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあドッグタウンはどういう場所だって言うの？",
              "maleVariant": "じゃあドッグタウンはどういう場所だって言うんだ？",
              "stringId": "3427306660766781440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼む、聞いてくれ！ 俺はもう用済みのスクラップ、歩く死体だ！ 殺しても意味ねえだろ！",
              "maleVariant": "",
              "stringId": "3436223492533882880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたのドッグタグはどこ？ 渡して",
              "maleVariant": "お前のドッグタグはどこだ？ 渡せ",
              "stringId": "3436351220583075840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あ、ああ… ほら",
              "maleVariant": "",
              "stringId": "3436355448206626816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあね",
              "maleVariant": "じゃあな",
              "stringId": "3436357171075715072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "座標を送る。そこに… まあ行きゃわかる。なんつうか… ありがとな",
              "maleVariant": "",
              "stringId": "3436358076357513216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、失せな",
              "maleVariant": "",
              "stringId": "3436361477183082496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインダーはここで取引を？ 内臓泥棒どもが巣食う廃ホテルで？",
              "maleVariant": "",
              "stringId": "3571163262837166080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインダーの銃があるってことは、ここにいるはずよ",
              "maleVariant": "ラインダーの銃があるってことは、ここにいるはずだ",
              "stringId": "3571163705738891264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スカベンジャーの連中、生体モニターの信号を妨害できるように、プログラムを書き換えてる。サンドラ・ドーセットの時と一緒",
              "maleVariant": "スカベンジャーの連中、生体モニターの信号を妨害できるように、プログラムを書き換えてる。サンドラ・ドーセットの時と一緒だ",
              "stringId": "3571164793556799488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バーゲストのベストを着けた奴に、「ラインダー」って書かれてる… 腕と足には値段… クロームの価格？",
              "maleVariant": "バーゲストのベストを着けた奴に、「ラインダー」って書かれてる… 腕と足には値段… クロームの価格か？",
              "stringId": "3571165018035949568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スカベンジャーか… 手術されたばかり… 意識はないけど生きてはいる",
              "maleVariant": "",
              "stringId": "3571165436325498880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて、奴さんは1階にいるかな？ 少なくとも、手足くらいは見つかるかもしれねえな",
              "maleVariant": "",
              "stringId": "3571188333198807040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインダーのヤツもあるぞ、ほら",
              "maleVariant": "",
              "stringId": "3571199003088752640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど…",
              "maleVariant": "",
              "stringId": "3571199346669359104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあな。このマネキンの腕と足につけられた値段じゃねえとは限らねえだろ",
              "maleVariant": "",
              "stringId": "3571208042820505600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バカじゃないの。とりあえずドッグタグだけでも回収しないと",
              "maleVariant": "バカじゃないのか。とりあえずドッグタグだけでも回収しないと",
              "stringId": "3571212187598163968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フン… クズのくせに、仕事が早い連中だ。抜きたてほやほやのクロームを、もうインストールしてやがる",
              "maleVariant": "",
              "stringId": "3571214643497373696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "血痕… まだ新しい。ここで誰かのクロームを抜いたのね",
              "maleVariant": "血痕… まだ新しい。ここで誰かのクロームを抜いたのか",
              "stringId": "3571216069225189376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "臓物だらけの壁、血まみれの床、目ん玉が飾られたテーブル――スカベンジャー流インテリアだな",
              "maleVariant": "",
              "stringId": "3571217896045907968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつじゃねえな",
              "maleVariant": "",
              "stringId": "3572536851410817024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはもう死んでるようなもの",
              "maleVariant": "お前はもう死んでるようなもんだ",
              "stringId": "3576841134809649152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話は聞き飽きた",
              "maleVariant": "",
              "stringId": "3576849150611132416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "殺すつもりだったけど、あんたはもう死んでるようなもんね",
              "maleVariant": "殺すつもりだったが、お前はもう死んでるようなもんだ",
              "stringId": "3576927413689749504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんだと！",
              "maleVariant": "",
              "stringId": "3576927835083083776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "叫びたきゃ、あたしがいなくなってから、一人で寂しく叫んでくれる？",
              "maleVariant": "叫びたきゃ、俺がいなくなってから、一人で寂しく叫んでくれるか？",
              "stringId": "3576928045502926848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うるせえ！ クソが！",
              "maleVariant": "",
              "stringId": "3576928498051551232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フン… いい？ 大人しくドッグタグを渡せば、すぐに出てってあげる。あるいは… この続きを聞きたい？",
              "maleVariant": "フン… いいか？ 大人しくドッグタグを渡せば、すぐに出てってやる。あるいは… この続きを聞きたいか？",
              "stringId": "3576929747333386240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ…",
              "maleVariant": "",
              "stringId": "3576940338353639424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仮にここから生き延びたとしても、ナイトシティがとどめを刺してくれる",
              "maleVariant": "",
              "stringId": "3576940776826179584"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}