  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[待つ]",
              "maleVariant": "",
              "stringId": "3233427751339687940"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いたぞ。アーロンの坊やだ。どんな夢を見てるんだろうね",
              "maleVariant": "",
              "stringId": "3233455180124913664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アーロンは元々うちの用心棒でね。債務者の足を折ったり、家に火をつけたり… とにかく狂暴だった",
              "maleVariant": "",
              "stringId": "3233534754678505472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも、ただの狂犬で終わらせるにはもったいなくてね。インプラントやトレーニング代を肩代わりしてやったのさ",
              "maleVariant": "",
              "stringId": "3233536783262023680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": "3233779123268485124"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたと取引するつもりはない",
              "maleVariant": "",
              "stringId": "3233779552412893184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お似合いの二人ね。あたしもこれ以上首を突っ込む気はない",
              "maleVariant": "お似合いの二人だな。俺もこれ以上首を突っ込む気はない",
              "stringId": "3233780766177361920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんな過去があったにせよ、アーロンの未来を奪う権利はない",
              "maleVariant": "",
              "stringId": "3233780972084133888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっちまいな！",
              "maleVariant": "",
              "stringId": "3233791587683008512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もっといい案があるんだけど――",
              "maleVariant": "もっといい案があるんだが――",
              "stringId": "3233794373841715200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "賢明だな。では、試合が終わったらまた連絡する",
              "maleVariant": "",
              "stringId": "3233797858201042944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうか… 残念だよ",
              "maleVariant": "",
              "stringId": "3233801085566976000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウソをついてたでしょ",
              "maleVariant": "ウソをついてただろ",
              "stringId": "3234894948088020996"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっとね… お友達とはどうも見解が一致しなくて、仕方なくよ",
              "maleVariant": "ちょっとな… お友達とはどうも見解が一致しなくて、仕方なくだ",
              "stringId": "3234902119240564736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "した方がいい。その方が安全よ",
              "maleVariant": "した方がいい。その方が安全だ",
              "stringId": "3234966229881847812"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アニマルズなんかクソ食らえよ",
              "maleVariant": "アニマルズなんかクソ食らえだ",
              "stringId": "3234967548755574784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何もキャリアがすべてじゃない。そうでしょ？ すべては命あってこそよ",
              "maleVariant": "何もキャリアがすべてじゃない。そうだろ？ すべては命あってこそだ",
              "stringId": "3234968352736542720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アーロンが無事ならあたしは何もしない",
              "maleVariant": "アーロンが無事なら俺は何もしない",
              "stringId": "3239460278539472900"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう起きてる",
              "maleVariant": "",
              "stringId": "3241990906371723268"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "面倒ならもう起きてるでしょ。すぐに解決できそうだけど",
              "maleVariant": "面倒ならもう起きてるだろ。すぐに解決できそうだがな",
              "stringId": "3241991324812267520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話し合いで解決できるならその方がいい",
              "maleVariant": "",
              "stringId": "3241991451346030592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのつもりはない",
              "maleVariant": "",
              "stringId": "3241991693323816960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあまあ、待ちなよ。話す時間はあるさ",
              "maleVariant": "",
              "stringId": "3241993746737614848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さすがに、1人で来るほどバカじゃなかったか",
              "maleVariant": "",
              "stringId": "3241996056540827648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そいつが起きたときに何を伝えるかは… よく考えるんだな",
              "maleVariant": "",
              "stringId": "3242097249241878528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やり直すチャンスをもらったのよ",
              "maleVariant": "やり直すチャンスをもらったんだぞ",
              "stringId": "3242413266979606528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンジーからあんたについて聞いた",
              "maleVariant": "",
              "stringId": "3242420980690538496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やり直すチャンスをもらったのよ。無駄にしないで",
              "maleVariant": "やり直すチャンスをもらったんだ。無駄にするなよ",
              "stringId": "3242448732621721600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたも金もクソ食らえよ",
              "maleVariant": "あんたも金もクソ食らえだ",
              "stringId": "3242537136738881536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボクシングより、よほど大事なものがかかってる。わかる？",
              "maleVariant": "ボクシングより、よほど大事なものがかかってる。わかるか？",
              "stringId": "3243377254034919424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これで問題を解決できたわけじゃない",
              "maleVariant": "",
              "stringId": "3243381791718199296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "試合までに体調は戻りそう？",
              "maleVariant": "試合までに体調は戻りそうか？",
              "stringId": "3243430859739844628"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、多分な… 少し耳鳴りがするぐらいだ",
              "maleVariant": "",
              "stringId": "3243430859756621828"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これで、ようやく自分の人生を歩める",
              "maleVariant": "",
              "stringId": "3243430859756621840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手術ひとつで自由になれるとは… おかしな話だ",
              "maleVariant": "",
              "stringId": "3243430859756621852"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アニマルズにせよ、スカベンジャーにせよ… 何もしない限り、このままじゃずっと追われ続ける",
              "maleVariant": "",
              "stringId": "3243455373953552384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "勝ち取る、か…",
              "maleVariant": "",
              "stringId": "3243455696378089472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前には手を出さないよう、スカベンジャーに伝えておく",
              "maleVariant": "",
              "stringId": "3244973029235970048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、大丈夫じゃない。ここに来たってことは、試合の後にも俺に会いに来るはずだ",
              "maleVariant": "",
              "stringId": "3244995587930935296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自由が欲しいんでしょ？ だったら、戦って勝ち取るのよ",
              "maleVariant": "自由が欲しいんだろ？ だったら、戦って勝ち取るんだ",
              "stringId": "3245000303771471872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダミール。久しぶりだな",
              "maleVariant": "",
              "stringId": "3245188466442371072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手を貸したとして、それがアンジーにバレたら、俺はおしまいだ。手足をセメントで固められて海の底で魚の餌だ",
              "maleVariant": "",
              "stringId": "3255192726061535232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前はドッグタウン一のリパーだ。アンジーがお前を殺すわけないだろ",
              "maleVariant": "",
              "stringId": "3255195171642757120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V…",
              "maleVariant": "",
              "stringId": "3255202911643586560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着いて。アーロンが無事ならあたしは何もしない",
              "maleVariant": "落ち着けよ。アーロンが無事なら俺は何もしない",
              "stringId": "3265274468923428864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アーロンがトラブルを懸念しててね。あたしは彼の… 用心棒ってところよ",
              "maleVariant": "アーロンがトラブルを懸念しててな。俺は彼の… 用心棒ってところだ",
              "stringId": "3265274532307750912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アーロン…",
              "maleVariant": "",
              "stringId": "3265275652153364480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここには来るなと言ったはずだぞ",
              "maleVariant": "",
              "stringId": "3265275877890805760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼み事がある",
              "maleVariant": "",
              "stringId": "3265278817510707200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なのに護衛まで連れてくるとは… そいつは誰なんだ？",
              "maleVariant": "",
              "stringId": "3265280492329848832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "用心棒よ",
              "maleVariant": "用心棒だ",
              "stringId": "3265281019486752768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー… クソ…！",
              "maleVariant": "",
              "stringId": "3265294482162405376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で… 面倒を起こす気かい？",
              "maleVariant": "",
              "stringId": "3265304352500568064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしもだ。現実主義でね。気が合いそうじゃないか",
              "maleVariant": "",
              "stringId": "3265311772979318784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前、この男の正体を知ってるのか？",
              "maleVariant": "",
              "stringId": "3265318474722136064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボクサーでしょ",
              "maleVariant": "ボクサーだろ",
              "stringId": "3265319130526732292"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "逃げ場を失ったネズミ",
              "maleVariant": "",
              "stringId": "3265325299475501056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "元ギャング",
              "maleVariant": "",
              "stringId": "3265325690032312320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたにキャリアを潰されようとしてるボクサーでしょ",
              "maleVariant": "あんたにキャリアを潰されようとしてるボクサーだろ",
              "stringId": "3265329303207104512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "追い詰められたネズミは、猫を噛むって知ってる？",
              "maleVariant": "追い詰められたネズミは、猫を噛むって知ってるか？",
              "stringId": "3265329329396338688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ギャングから足を洗ったって聞いてる",
              "maleVariant": "",
              "stringId": "3265329388032708608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ、なるほど… アーロンに何を聞いたか知らんが――",
              "maleVariant": "",
              "stringId": "3265334733069176832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アーロンにはかなり出資した。ボクサーになれたのも、うちのおかげだ",
              "maleVariant": "",
              "stringId": "3265335395467218944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ… どんなに洗っても、取れない汚れはあるのさ",
              "maleVariant": "",
              "stringId": "3265339824903901184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うちとしては、アーロンに筋を通してもらいたいだけだ。ヴィンス戦で負けてくれさえすれば、借りをチャラにしてやるさ",
              "maleVariant": "",
              "stringId": "3265372229727506432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アーロンはもう十分働いた",
              "maleVariant": "",
              "stringId": "3265374606471811072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いえ、ダメよ。アーロンは何年もあんたのドル箱として働いてきた。もう十分でしょ",
              "maleVariant": "いや、ダメだ。アーロンは何年もあんたのドル箱として働いてきた。もう十分だろ",
              "stringId": "3265375099990396928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前もどうだ？ アーロンが予定通りにヤオをすれば、収益の10%をお前にやろう",
              "maleVariant": "",
              "stringId": "3265375191090679808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人生最大の試合なのよ。なのに八百長をやらせる気？",
              "maleVariant": "人生最大の試合なんだぞ。なのに八百長をやらせる気か？",
              "stringId": "3265381468906803200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人生最大の試合なのよ",
              "maleVariant": "人生最大の試合なんだぞ",
              "stringId": "3265381616412086272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドル箱？ そいつで稼いだ金なんてたかが知れてる。でも、ようやくまともな試合が組めた。金持ちも賭けにくるだろう",
              "maleVariant": "",
              "stringId": "3265391098995232768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これまでと何が違う？ 奴の試合のほとんどはヤオだ。別に騒ぐような話じゃない",
              "maleVariant": "",
              "stringId": "3265396584406667264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[脅迫する] もっといい案がある",
              "maleVariant": "",
              "stringId": "3265645729470042112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アーロンにこれほどの傭兵を雇う金があったとはね…甘く見てたよ",
              "maleVariant": "",
              "stringId": "3265654771315724288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お友達が会いにきた",
              "maleVariant": "",
              "stringId": "3265665999433977860"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お友達が会いにきた",
              "maleVariant": "",
              "stringId": "3265668076772085760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気分はどう？",
              "maleVariant": "気分はどうだ？",
              "stringId": "3265668250164613120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え？ …ウソだろ… アンジー…？ 何があったんだ？",
              "maleVariant": "",
              "stringId": "3265674136501178368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気分はどう？",
              "maleVariant": "気分はどうだ？",
              "stringId": "3265675815531401216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたが寝てる間にアンジーがきて、話をさせてもらった",
              "maleVariant": "",
              "stringId": "3265677334557315072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マジか。で、どうなった？",
              "maleVariant": "",
              "stringId": "3265680438996586496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫、手を引くよう説得した",
              "maleVariant": "大丈夫だ、手を引くよう説得した",
              "stringId": "3265680580696952832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが、もう関係ない。あんたのおかげだ",
              "maleVariant": "",
              "stringId": "3265700937801748480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チップを無効化するだけじゃダメかもしれない… クソ、どうすればいいんだ",
              "maleVariant": "",
              "stringId": "3265709906884644864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "試合までに回復できると思う？",
              "maleVariant": "試合までに回復できると思うか？",
              "stringId": "3265729486264815616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンジーからすべて聞いた",
              "maleVariant": "",
              "stringId": "3265732525038063616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アーロンはあんたたちと縁を切る。サスカッチと同じ目に遭いたくはないでしょ",
              "maleVariant": "アーロンはあんたたちと縁を切る。サスカッチと同じ目に遭いたくはないだろ",
              "stringId": "3278003087957381120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンジーめ… 相変わらず諦めの悪い女だ",
              "maleVariant": "",
              "stringId": "3278010286003802112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "終わったぞ。ブロッカーは無効化した。そろそろ目覚めるだろう",
              "maleVariant": "",
              "stringId": "3278019941341941760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ぐっ… クソ",
              "maleVariant": "",
              "stringId": "3278025398198554624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭にメスを入れるなんて、二度とごめんだ",
              "maleVariant": "",
              "stringId": "3278025929499430912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ、クソ！ はあ… わかったよ。さっさと終わらせるぞ。そこに座れ",
              "maleVariant": "",
              "stringId": "3282650832861220864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、そうだな… あんたの言う通りだ。ありがとよ、V",
              "maleVariant": "",
              "stringId": "3282662802314317824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "念のためだ。頼む",
              "maleVariant": "",
              "stringId": "3285407172700491776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼むからチップを抜いてくれ。お願いだ",
              "maleVariant": "",
              "stringId": "3285409071831011328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なあ… 俺が目を覚ますまでいてくれ",
              "maleVariant": "",
              "stringId": "3285410793794129920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昔の奴を知ってれば、お前もわかるさ…",
              "maleVariant": "",
              "stringId": "3285414021579493376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうする気？ スイッチを入れてアーロンを気絶させる？",
              "maleVariant": "どうする気だ？ スイッチを入れてアーロンを気絶させるか？",
              "stringId": "3285417911209250816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チップのことか？ 使ったことは一度もないぞ。アーロンはいつも要求通りに倒れてくれた",
              "maleVariant": "",
              "stringId": "3285418370233880576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつはそうやっておいしい思いをしてきたのさ",
              "maleVariant": "",
              "stringId": "3285418626640072704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この連中よりはマシさ… いったい何があったんだ？",
              "maleVariant": "",
              "stringId": "3285424158172073984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最悪だ。とにかく終わってよかった",
              "maleVariant": "",
              "stringId": "3285425710735642624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…ヤオをするしかないのか？",
              "maleVariant": "",
              "stringId": "3285428678390272000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "デタラメだぞ",
              "maleVariant": "",
              "stringId": "3285443015678971904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アーロンのことはうちらに任せておけ。もう行きな",
              "maleVariant": "",
              "stringId": "3287015908118446080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "20%よ",
              "maleVariant": "20%だ",
              "stringId": "3302783942730797056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昔、あるプロジェクトが炎上して、上司を救ったの。その時も、10%のボーナスを提案された",
              "maleVariant": "昔、あるプロジェクトが炎上して、上司を救ったんだ。その時も、10%のボーナスを提案された",
              "stringId": "3302785725494546432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何が言いたい？",
              "maleVariant": "",
              "stringId": "3302786444264034304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "20%なら考えてもいい",
              "maleVariant": "",
              "stringId": "3302786515718197248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ、強気だな。まあいい、20で手を打とう",
              "maleVariant": "",
              "stringId": "3302787463798673408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも…",
              "maleVariant": "",
              "stringId": "3302803661579964416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] 無効化？ 摘出するんじゃなかったの？",
              "maleVariant": "[立ち上がる] 無効化？ 摘出するんじゃなかったのか？",
              "stringId": "3302847046571347968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無効化？ 摘出するはずじゃ…",
              "maleVariant": "",
              "stringId": "3302847473534717952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チップは内耳に埋め込まれてる。取り除くのは危険だから、安全策をとってレセプターをオフにしたのね",
              "maleVariant": "チップは内耳に埋め込まれてる。取り除くのは危険だから、安全策をとってレセプターをオフにしたのか",
              "stringId": "3302849887960649728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チップは機能しないし、アーロンの負担も少ない。いい腕じゃない",
              "maleVariant": "チップは機能しないし、アーロンの負担も少ない。いい腕をしてる",
              "stringId": "3302853187837710336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] 手術は成功したの？",
              "maleVariant": "[立ち上がる] 手術は成功したのか？",
              "stringId": "3302860174809976832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これでもう大丈夫ってこと？",
              "maleVariant": "これでもう大丈夫ってことか？",
              "stringId": "3302860478376923136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。難しい手術だったが、とりあえず成功した。アーロンは強い男だ。きっとすぐに回復するだろう",
              "maleVariant": "",
              "stringId": "3302860788990300160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてアーロンがここにいるとわかったの？",
              "maleVariant": "どうしてアーロンがここにいるとわかったんだ？",
              "stringId": "3316350548773785600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜここが？ 尾行されてたの？",
              "maleVariant": "なぜここが？ 尾行されてたのか？",
              "stringId": "3316351078212390912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここはうちのシマだ",
              "maleVariant": "",
              "stringId": "3316352836682731520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが、そんなことはどうでもいい。本題に入ろうじゃないか",
              "maleVariant": "",
              "stringId": "3317370032796028928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すべてを失いかけたのに、ずいぶんとケチでしょ",
              "maleVariant": "すべてを失いかけたのに、ずいぶんとケチだろ",
              "stringId": "3336119404966793216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] 妙な真似をしてないでしょうね",
              "maleVariant": "[立ち上がる] 妙な真似をしてないだろうな",
              "stringId": "3336121636252635136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "妙な真似をしてないでしょうね",
              "maleVariant": "妙な真似をしてないだろうな",
              "stringId": "3336122088885145600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アーロンとは古い付き合いだ。彼は俺のことを信用してる。お前もそうすべきだ",
              "maleVariant": "",
              "stringId": "3336148862704635904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、だが…",
              "maleVariant": "",
              "stringId": "3336153752541679616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、聞いてるのか？",
              "maleVariant": "",
              "stringId": "3345176067935551496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何とか言ったらどうだ？",
              "maleVariant": "",
              "stringId": "3345176067969105928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やればボクサーとしては終わりだ",
              "maleVariant": "",
              "stringId": "3363559385158995968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティのボクサーとしては――でしょ？",
              "maleVariant": "ナイトシティのボクサーとしては――だろ？",
              "stringId": "3363559801032626176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少し考えてみる。ありがとな、V",
              "maleVariant": "",
              "stringId": "3363560299970252800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞いてるのか？",
              "maleVariant": "",
              "stringId": "3366682326126120960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ、クソ…",
              "maleVariant": "",
              "stringId": "3471121871931158528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダミール、消えろ。お前は後だ",
              "maleVariant": "",
              "stringId": "3480838964934684672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンジー、聞いてくれ… 俺はこいつらに脅されてだな… 無理やり協力させられたんだ",
              "maleVariant": "",
              "stringId": "3480886122098536448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいから、行け… これ以上、余計なことはしないでくれ。ただでさえ大変なんだ…",
              "maleVariant": "",
              "stringId": "3498603149557481472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ… クソ！ なんてことをしてくれたんだ！ さっさと出ていけ！ スカベンジャーは手を出してこないはずだ… 俺は片づけないと…",
              "maleVariant": "",
              "stringId": "3498609492502757376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大したものだ… あのアンジーを黙らせるとは。だが、もう行った方がいい。スカベンジャーは手を出してこないはずだ",
              "maleVariant": "",
              "stringId": "3498618256098312192"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}