  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰だ…？",
              "maleVariant": "",
              "stringId": "2766269310700752896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっぱりさっきのは銃声だったんだよ",
              "maleVariant": "",
              "stringId": "2896754403974762496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョルジーナね？ 話がある",
              "maleVariant": "ジョルジーナだな？ 話がある",
              "stringId": "3263572223716675588"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイケルに暴力をふるって証言を強要したでしょ",
              "maleVariant": "マイケルに暴力をふるって証言を強要しただろ",
              "stringId": "3263630233323360260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたと仲間がマイケルに暴力を振るったと",
              "maleVariant": "",
              "stringId": "3263634153269547008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたのことじゃない、ハンク。もしかしてマルドナド氏に“ショック療法”を施したの？",
              "maleVariant": "",
              "stringId": "3263635077610262528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさか。そんなことしてる暇はない",
              "maleVariant": "",
              "stringId": "3263636421230383104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だからって、証言の捏造が許されるとでも？",
              "maleVariant": "",
              "stringId": "3263789557282689028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたを信じる理由がない",
              "maleVariant": "",
              "stringId": "3263789748207407104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイケルの話と違う",
              "maleVariant": "",
              "stringId": "3263795496769474564"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よく考えるのね、ハンク。引退まであと何年なのか。あたしがそれをどれだけ早められるか",
              "maleVariant": "よく考えるんだな、ハンク。引退まであと何年なのか。俺がそれをどれだけ早められるか",
              "stringId": "3264306942095839232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョルジーナ・ゼンビンスキーね。マイケル・マルドナドのことで話がある",
              "maleVariant": "ジョルジーナ・ゼンビンスキーだな。マイケル・マルドナドのことで話がある",
              "stringId": "3300424923940737048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの酔っ払いのことで？ 何かしら？",
              "maleVariant": "",
              "stringId": "3300424923940737060"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "息子を売るよう強要したんでしょ。その証言を消して",
              "maleVariant": "息子を売るよう強要したんだろ。その証言を消してくれ",
              "stringId": "3300424923940737072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "証言を消す…？ あの腰抜け、今さらそんなことを",
              "maleVariant": "",
              "stringId": "3300424923940737084"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼に何と言われたの？",
              "maleVariant": "",
              "stringId": "3300424923940737096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エリックに犯罪歴があるからって、証言を捏造してもいいって言うの？",
              "maleVariant": "エリックに犯罪歴があるからって、証言を捏造してもいいって言うのか？",
              "stringId": "3300459612327989248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それが現実だ。正義を果たすためには、正しい方向へ導く必要もある",
              "maleVariant": "",
              "stringId": "3300460127640178688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。あんたは信用できないけど、マイケルはなおさらだから",
              "maleVariant": "わかった。あんたは信用できないが、マイケルはなおさらだからな",
              "stringId": "3304253149876146176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それでいい。じゃ",
              "maleVariant": "",
              "stringId": "3304259937618972672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何者だ？",
              "maleVariant": "",
              "stringId": "3305543113339392000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お客さんのようね",
              "maleVariant": "",
              "stringId": "3305543846738612224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "な？ 奴は信用できないと言ったろ",
              "maleVariant": "",
              "stringId": "3305545259816407040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これで全員が満足できるでしょう。もちろん、エリックを除いてだけど",
              "maleVariant": "",
              "stringId": "3307208646644289536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう用は済んだでしょ。出てって",
              "maleVariant": "",
              "stringId": "3312160128718262272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "取引成立。証言は匿名とする。あなたの“依頼主”もこれで安心ね",
              "maleVariant": "",
              "stringId": "3317073457049849856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誘拐、恐喝、武装強盗… 誰の逮捕歴かわかる？",
              "maleVariant": "",
              "stringId": "3317379305764638720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エリック・マルドナドはクズのいいお手本よ。刑務所に入れる必要がある。そのための一押しが必要だっただけ",
              "maleVariant": "",
              "stringId": "3317380476663320576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サクリスタンを捕まえるなら、決め手になる確実な証拠が必要よ",
              "maleVariant": "",
              "stringId": "3326122239657857028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それなら心配ない。証言の準備を進めてる",
              "maleVariant": "",
              "stringId": "3326122239657857052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "目撃者1人じゃ心もとない。今日はそのためにあと2人ばかり呼んでる",
              "maleVariant": "",
              "stringId": "3326122239657857064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこへ行け、何をしろって… ったく、俺はあんたの使い走りだな",
              "maleVariant": "",
              "stringId": "3326122239657857076"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "息子の犯罪をでっち上げようとしてるとか",
              "maleVariant": "",
              "stringId": "3327308922076090368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイケルの息子をはめる気らしいね",
              "maleVariant": "マイケルの息子をはめる気らしいな",
              "stringId": "3327309392777662464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でっち上げる？ 冗談でしょ？ 彼はれっきとした犯罪者よ",
              "maleVariant": "",
              "stringId": "3327310197027065856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当に？",
              "maleVariant": "本当か？",
              "stringId": "3327310581963509760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そして父親のマイケルは、出来損ないの息子より金を選んだ",
              "maleVariant": "",
              "stringId": "3334502804912164864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マルドナドは… 自分が証言したことを息子に知られるのが怖いだけよ",
              "maleVariant": "",
              "stringId": "3334509410622197760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろん、それを回避することはできる…",
              "maleVariant": "",
              "stringId": "3334510439820513280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうこと？",
              "maleVariant": "どういうことだ？",
              "stringId": "3334512264913178624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "依頼主が満足ならそれでいい",
              "maleVariant": "",
              "stringId": "3334856006413144064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。取引成立よ",
              "maleVariant": "わかった。取引成立だ",
              "stringId": "3338957782942830596"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信用できない。証言のデータを消して",
              "maleVariant": "信用できない。証言のデータを消せ",
              "stringId": "3338957947057557504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたよりずっと手強い相手と渡り合ってきた。連中に比べればあんたなんてハエ同然よ",
              "maleVariant": "お前よりずっと手強い相手と渡り合ってきた。連中に比べればお前なんてハエ同然だ",
              "stringId": "3338969819135692800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら。データは削除した",
              "maleVariant": "",
              "stringId": "3339174503184158720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずはマイケルの証言を見たら？ 真実を確かめればいい",
              "maleVariant": "",
              "stringId": "3346299813377691648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[録画を見る]",
              "maleVariant": "",
              "stringId": "3346306195497766916"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんな暇はない",
              "maleVariant": "",
              "stringId": "3346306564999172096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんな暇はないし、あたしは気が短いの",
              "maleVariant": "そんな暇はないし、俺は気が短いんだ",
              "stringId": "3346308146419552256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "では最後の質問よ。エリックは去年の5月16日にあなたにホロコールをかけている",
              "maleVariant": "",
              "stringId": "3346312412177715200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その会話の最中、彼はヘクター・サクリスタンがマルコス・フェレイラを撃ち殺したと話した",
              "maleVariant": "",
              "stringId": "3346316724777865216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今述べた情報に偽りはある？",
              "maleVariant": "",
              "stringId": "3346325868780015616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "い… いいえ。事実です",
              "maleVariant": "",
              "stringId": "3346325992511983616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "素晴らしい。質問は以上よ",
              "maleVariant": "",
              "stringId": "3346326654457040896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "提案がある。マイケルの証言を匿名にするというのはどう？ これでエリックには知られずに済む",
              "maleVariant": "",
              "stringId": "3346374222377472000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは頼まれた仕事を最後までやり遂げるつもりよ。これ以上茶番に付き合う気はない",
              "maleVariant": "俺は頼まれた仕事を最後までやり遂げるつもりだ。これ以上茶番に付き合う気はない",
              "stringId": "3346376367227097116"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "断ると言ったら？",
              "maleVariant": "",
              "stringId": "3346410587379027968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大人しく受け入れておけ。危険な目に遭いたくはないだろう",
              "maleVariant": "",
              "stringId": "3346412239062388736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この後、マイケルは迷うことなく謝礼を受け取った",
              "maleVariant": "",
              "stringId": "3346438178299269120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大したビジネスマンだ",
              "maleVariant": "",
              "stringId": "3346439405166743552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゴホン… あー、ジョルジーナ。俺はこんなことで死ぬのはご免だ。誰かのつまらん証言のためにな",
              "maleVariant": "",
              "stringId": "3346443617489473536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンク！ そんな… 本当に殺すなんて！",
              "maleVariant": "",
              "stringId": "3346451782339743744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "データを",
              "maleVariant": "",
              "stringId": "3346456174665224192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、ハンク… 正気なの？ 後で話をさせてもらうから",
              "maleVariant": "",
              "stringId": "3346459882044084224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私を信用できないのはわかる。でもせめて話を聞いてくれない？ 手短に済ませるから",
              "maleVariant": "",
              "stringId": "3347478796563730432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残念だけどそれはできない。どうやら、話し合いはここまでのようね",
              "maleVariant": "",
              "stringId": "3347480487438667776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンク？",
              "maleVariant": "",
              "stringId": "3347480950691155968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ、了解…",
              "maleVariant": "",
              "stringId": "3347481193205813248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "茶番ですって？ さっき証拠を見せたでしょう",
              "maleVariant": "",
              "stringId": "3347482049431031808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたに都合がいい話をね。信用はできない。さあ、証言のデータを",
              "maleVariant": "あんたに都合がいい話をな。信用はできない。さあ、証言のデータを",
              "stringId": "3347482463576608768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早く証言のデータを消して",
              "maleVariant": "早く証言のデータを消せ",
              "stringId": "3347483013617635328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼の言葉に価値なんてないわ",
              "maleVariant": "",
              "stringId": "3347488856937418752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "命を大事にするのね、ハンク",
              "maleVariant": "命を大事にするんだな、ハンク",
              "stringId": "3347501848777281540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘクター・サクリスタンってのは？",
              "maleVariant": "",
              "stringId": "3347507005539831808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴァレンティーノズの幹部よ。麻薬王、連続殺人犯――この2年、奴のことを追ってきた",
              "maleVariant": "",
              "stringId": "3347509316383232000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エリック・マルドナドは最後のピースなの。でも彼を使うには、牢屋に入っていてもらう必要がある",
              "maleVariant": "",
              "stringId": "3347509855435182080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さらにフェレイラの死体をコロナド湾に捨てるようサクリスタンが指示したとも",
              "maleVariant": "",
              "stringId": "3347845173229211648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ロングショア・スタックスへ行ってエステバンと接触して。場所の情報と経費を送っておく",
              "maleVariant": "",
              "stringId": "3347886974182969344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘクター・サクリスタンってのは何者？",
              "maleVariant": "ヘクター・サクリスタンってのは何者だ？",
              "stringId": "3348990129419407360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺はどうする？",
              "maleVariant": "",
              "stringId": "3356617617289072640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、これを見て",
              "maleVariant": "",
              "stringId": "3366438933452214272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうするの",
              "maleVariant": "",
              "stringId": "3366441376013537280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "3408862541521858564"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}