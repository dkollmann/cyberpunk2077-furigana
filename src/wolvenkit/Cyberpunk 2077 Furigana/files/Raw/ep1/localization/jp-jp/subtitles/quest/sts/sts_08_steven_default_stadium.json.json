  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？ Vか？ 俺を覚えてるだろ",
              "maleVariant": "",
              "stringId": "3363664436854624264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スティーブン？",
              "maleVariant": "スティーブンか？",
              "stringId": "3363664438079361024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ごめん、わからない",
              "maleVariant": "悪い、わからない",
              "stringId": "3363664438079361040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スティーブン？ 元気でやってるようね",
              "maleVariant": "スティーブン？ 元気でやってるようだな",
              "stringId": "3363664438163247112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おかげさまでな",
              "maleVariant": "",
              "stringId": "3363664438163247124"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、悪いけど…",
              "maleVariant": "いや、悪いが…",
              "stringId": "3363664440528834560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふざけるな、スティーブンだよ。ブラジル情報局の！ アナや生体モニターのことも忘れたのか？",
              "maleVariant": "",
              "stringId": "3363664440528834572"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、思い出した",
              "maleVariant": "",
              "stringId": "3363664443397738496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その辺にしときなよ",
              "maleVariant": "その辺にしとけ",
              "stringId": "3363664443397738512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんか怒ってる？",
              "maleVariant": "なんか怒ってるか？",
              "stringId": "3363664443464847360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセンの下で何の仕事を？",
              "maleVariant": "",
              "stringId": "3363664443464847376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんでまだドッグタウンにいるの？",
              "maleVariant": "なんでまだドッグタウンにいるんだ？",
              "stringId": "3363664443464847392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大声でバカじゃない？ 局で何を教わったの？ いい？ あたしとあんたは他人同士よ",
              "maleVariant": "大声でバカじゃないのか？ 局で何を教わったんだ？ いいか？ 俺とあんたは他人同士だ",
              "stringId": "3363664443531956272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうかい",
              "maleVariant": "",
              "stringId": "3363664443531956284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、そうよ。じゃあね",
              "maleVariant": "ああ、そうだ。じゃあな",
              "stringId": "3363664443531956296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大佐の下で働いてるの？",
              "maleVariant": "大佐の下で働いてるのか？",
              "stringId": "3363664448984551440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、そうだ。この俺が世界最大規模のテロ組織に手を貸してるのさ。ハッ。人生、わかんないもんだろ？",
              "maleVariant": "",
              "stringId": "3363664449001328648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スタジアムの顧客データを収集するのが俺の仕事だ。安全性の確保、脅威の分析――情報局の頃とほとんど変わらない… ほとんどな",
              "maleVariant": "",
              "stringId": "3363664449001328660"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アナのことは尋ねもしないんだな",
              "maleVariant": "",
              "stringId": "3363664453162078224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… 彼女はどうしてるの？",
              "maleVariant": "はあ… 彼女はどうしてるんだ？",
              "stringId": "3363664453178855424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アナは刑務所にいる",
              "maleVariant": "",
              "stringId": "3363664453178855436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんか怒ってるみたいだけど",
              "maleVariant": "なんか怒ってるようだが",
              "stringId": "3363664456249085968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの顔を見てると、どうしても怒りが込み上げてくるんだよ。なんでだろうな？",
              "maleVariant": "",
              "stringId": "3363664456249085980"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、そうだ。俺の人生をぶっ壊したからだ",
              "maleVariant": "",
              "stringId": "3363664456249085992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの後に何があったの？",
              "maleVariant": "あの後に何があったんだ？",
              "stringId": "3363664459721969664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連絡は取ってるの？",
              "maleVariant": "連絡は取ってるのか？",
              "stringId": "3363664459738746892"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アナ以外にも、痛い目を見た人は？",
              "maleVariant": "",
              "stringId": "3363664459738746908"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんでまだドッグタウンにいるの？",
              "maleVariant": "なんでまだドッグタウンにいるんだ？",
              "stringId": "3363664459789078544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんでだと！？ ふざけるな、全部あんたのせいだ",
              "maleVariant": "",
              "stringId": "3363664459789078556"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連絡は取ってる？ いつ出てこれるの？",
              "maleVariant": "連絡は取ってるか？ いつ出てこれるんだ？",
              "stringId": "3363664462574096404"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "出てこれるかどうかも怪しい。国家反逆罪だからな。連絡は… 今のところまったく取ってない。差し入れを送ってるだけだ",
              "maleVariant": "",
              "stringId": "3363664462590873600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アナがバナのデータを公開した後、何があったの？",
              "maleVariant": "アナがバナのデータを公開した後、何があったんだ？",
              "stringId": "3363664466164420624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "局がソヴオイルとの契約を取りやめたのは知ってるだろ。ご覧の通り俺はドッグタウンに逃げてきたが、アナは隠れるのを嫌がって街に残った… だが…",
              "maleVariant": "",
              "stringId": "3363664466181197824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "案の定あっさり捕まり、ブラジルに送還――略式裁判で即有罪、即刑務所送りだ",
              "maleVariant": "",
              "stringId": "3363664466181197836"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女が選んだことでしょ",
              "maleVariant": "彼女が選んだことだろ",
              "stringId": "3363664471029813248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残念ね",
              "maleVariant": "残念だな",
              "stringId": "3363664471046590464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなことになってたとはね",
              "maleVariant": "そんなことになってたとはな",
              "stringId": "3363664471096922136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "生体モニターを破壊してくれればよかったんだ。あの時もそう言っただろう… なのに…",
              "maleVariant": "",
              "stringId": "3363664471096922148"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全員が、それぞれの選択をしたのよ",
              "maleVariant": "全員が、それぞれの選択をしたんだ",
              "stringId": "3363664471113699328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだな。でもなぜか、ツケを払ったのは俺たちだけだ",
              "maleVariant": "",
              "stringId": "3363664471113699340"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… じゃあね、スティーブン",
              "maleVariant": "はあ… じゃあな、スティーブン",
              "stringId": "3363664471113699352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アナはそれを承知で行動した。自ら選んだ結果よ",
              "maleVariant": "アナはそれを承知で行動した。自ら選んだ結果だ",
              "stringId": "3363664477539373080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうかもしれないが… 結局あいつは感情に流されたんだ",
              "maleVariant": "",
              "stringId": "3363664477539373092"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたもその場にいた",
              "maleVariant": "",
              "stringId": "3363664477539373104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "止めればよかったのか？ それとも殺すべきだった？ 俺は… いや、いい。もう行ってくれ",
              "maleVariant": "",
              "stringId": "3363664477539373116"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。じゃあね",
              "maleVariant": "わかった。じゃあな",
              "stringId": "3363664477556150276"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バナの件は、結局誰かが責任を取ったの？",
              "maleVariant": "バナの件は、結局誰かが責任を取ったのか？",
              "stringId": "3363664483914715152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "意外にも、中間管理職が解雇された。しかし、しょせんその程度だ",
              "maleVariant": "",
              "stringId": "3363664483914715164"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バナは名誉の勲章を与えられ、残された奥さんにもいくらかの見舞金が出た。そんなところだ",
              "maleVariant": "",
              "stringId": "3363664483914715176"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}