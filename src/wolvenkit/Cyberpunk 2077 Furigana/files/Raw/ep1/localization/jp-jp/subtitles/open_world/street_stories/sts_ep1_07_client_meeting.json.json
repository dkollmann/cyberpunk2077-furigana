  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "狙うべきは血肉… 人間なのよ",
              "maleVariant": "",
              "stringId": "2663713260714045440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見られてないか確認しないと。あなたは行って",
              "maleVariant": "",
              "stringId": "3043161985007726592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クリムゾン・ハーベストの目的に賛同してるなら、なぜ連中の邪魔をするの？",
              "maleVariant": "クリムゾン・ハーベストの目的に賛同してるなら、なぜ連中の邪魔をするんだ？",
              "stringId": "3192666304437530624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫",
              "maleVariant": "大丈夫だ",
              "stringId": "3215789601945530372"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クリムゾン・ハーベストのことを心配してるの？",
              "maleVariant": "クリムゾン・ハーベストのことを心配してるのか？",
              "stringId": "3215789894322073600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さすがにつけられるようなヘマはしない",
              "maleVariant": "",
              "stringId": "3215792577300901888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いえ。でも… 嫌な予感がするのよ。それに時間もない",
              "maleVariant": "",
              "stringId": "3215792632313393152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クリムゾン・ハーベストをなめないで。どこに潜んでるかわからないんだから",
              "maleVariant": "",
              "stringId": "3215793246309167104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "善良なテロリストだって聞いてる",
              "maleVariant": "",
              "stringId": "3215881022807453696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "テロリストのチューマたちと縁を切って、そいつらの悪しき計画を阻止したいんでしょ？",
              "maleVariant": "テロリストのチューマたちと縁を切って、そいつらの悪しき計画を阻止したいんだろ？",
              "stringId": "3215881269952622592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一度失敗したところでお友達は止まらないと思うけど。むしろ逆効果よ",
              "maleVariant": "一度失敗したところでお友達は止まらないと思うが。むしろ逆効果だ",
              "stringId": "3217425728045559808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンズから話は聞いてる？",
              "maleVariant": "",
              "stringId": "3222971619749900288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうせ署名を集めて、デモの看板を作る程度だと思ってたんだろ",
              "maleVariant": "",
              "stringId": "3223013327623663616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんで？ クリムゾン・ハーベストに何か勘付かれてるとか？",
              "maleVariant": "なんでだ？ クリムゾン・ハーベストに何か勘付かれてるとか？",
              "stringId": "3224861954779029504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "不要な危険を冒してる",
              "maleVariant": "",
              "stringId": "3225909561576038400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつまでここにいる気？",
              "maleVariant": "いつまでここにいる気だ？",
              "stringId": "3226023458001821696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "立派なことをしようとしてる",
              "maleVariant": "",
              "stringId": "3227482786588172288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたがネル？",
              "maleVariant": "あんたがネルか？",
              "stringId": "3234682653571981312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ネル・スプリンガー？",
              "maleVariant": "",
              "stringId": "3234683596401188864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待たせたみたいね",
              "maleVariant": "待たせたようだな",
              "stringId": "3234683681914658820"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待たせちゃったみたいね",
              "maleVariant": "待たせちまったようだな",
              "stringId": "3234683846348152832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたは？",
              "maleVariant": "",
              "stringId": "3234685923468156928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "尾行は大丈夫？",
              "maleVariant": "",
              "stringId": "3234687236234977280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "テロリストだと聞いた",
              "maleVariant": "",
              "stringId": "3234713629882753028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "組織を裏切るつもりだとか",
              "maleVariant": "",
              "stringId": "3234714577527021568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私たちは世界から飢餓をなくそうとしてるのよ",
              "maleVariant": "",
              "stringId": "3234735486753218560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クリムゾン・ハーベストの真の目的が何かわかってる？",
              "maleVariant": "",
              "stringId": "3234770339389415424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうやってクリムゾン・ハーベストに加わったの？",
              "maleVariant": "どうやってクリムゾン・ハーベストに加わったんだ？",
              "stringId": "3234774258194751492"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クリムゾン・ハーベストに入った経緯は？ 志願したの？",
              "maleVariant": "クリムゾン・ハーベストに入った経緯は？ 志願したのか？",
              "stringId": "3234778282428112896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "飢餓？ どこの話よ？ ナイトシティのゴミ箱は食べ物であふれかえってる",
              "maleVariant": "飢餓？ どこの話だ？ ナイトシティのゴミ箱は食べ物であふれかえってる",
              "stringId": "3234778339705528324"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私を含め、仲間の大半は農場で育った。企業に虐げられながら生きてきた。反撃に打って出ようと決めるまではね",
              "maleVariant": "",
              "stringId": "3234826013590855680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？",
              "maleVariant": "",
              "stringId": "3234894068706615296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "都会人は何もわかってない。もしかして奴隷制は過去のものだと思ってる？ バイオテクニカが農民をどう扱ってるか知らないでしょ？",
              "maleVariant": "",
              "stringId": "3234936457097814016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "爆弾テロをやってる",
              "maleVariant": "",
              "stringId": "3234950120647073792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを受け取る] これは？",
              "maleVariant": "",
              "stringId": "3234971923192913920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ビルを数棟吹っ飛ばしたところで、バイオテクニカが新しいものを建てるだけ",
              "maleVariant": "",
              "stringId": "3234989984755130368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "爆発と破壊を生まない爆弾に意味があるの？ テロリストはインパクトが大事でしょ",
              "maleVariant": "爆発と破壊を生まない爆弾に意味があるのか？ テロリストはインパクトが大事だろ",
              "stringId": "3234991296129441792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何をすればいいの？",
              "maleVariant": "何をすればいいんだ？",
              "stringId": "3235006082007343104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一切の感謝を期待せずに、これだけの危険を冒せる人はそういない。尊敬する",
              "maleVariant": "",
              "stringId": "3235020441072869376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "土地を追われて遺伝子操作小麦を作らされてるのよ。食糧用と思ったら大間違い。ペトロケムに売るCHOOH2のため",
              "maleVariant": "",
              "stringId": "3235037824651010048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V。フィクサーの使いよ",
              "maleVariant": "V。フィクサーの使いだ",
              "stringId": "3236125461566627840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ご名答よ",
              "maleVariant": "ご名答だ",
              "stringId": "3236130128266698752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なのに組織を裏切るの？",
              "maleVariant": "なのに組織を裏切るのか？",
              "stringId": "3236159100975226880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "理由がなんであれ、あんたたちは爆弾を使って大勢殺してる",
              "maleVariant": "",
              "stringId": "3236163231139930112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コーポへの憎しみで周りが見えなくなってるのよ。理念は間違ってない",
              "maleVariant": "",
              "stringId": "3236194257111261184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティに飢餓なんてない",
              "maleVariant": "",
              "stringId": "3236195131439095808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何をすればいい？",
              "maleVariant": "何をすればいいんだ？",
              "stringId": "3237313882859413504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "建物を爆破した方がいいんじゃないの？",
              "maleVariant": "建物を爆破した方がいいんじゃないのか？",
              "stringId": "3237313882876190744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クリムゾン・ハーベストはハンセンからサイバー核を買った。標的はヨーロッパ… ベルリンのバイオテクニカ本部",
              "maleVariant": "",
              "stringId": "3237314789466611724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この計画を阻止してほしいの",
              "maleVariant": "",
              "stringId": "3237314789466611736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私にとってクリムゾン・ハーベストは家族みたいなもの。家族のためならなんだってする… 夜に眠れなくなるようなこともね…",
              "maleVariant": "",
              "stringId": "3237628047620743168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ま、まさか… 本当に人を殺すなんて。私もひどいことをやらされた… 考えが甘かったわ",
              "maleVariant": "",
              "stringId": "3237652406628425728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さっさと本題に入りましょ",
              "maleVariant": "さっさと本題に入ろうか",
              "stringId": "3237662127146049536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "爆弾を見つけてこのチップのプログラムをアップロードして。そうすればベルリンで起爆した時、ウイルスを無力化して拡散を防いでくれる",
              "maleVariant": "",
              "stringId": "3237705682812051456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私はずっと自分のしたことを後悔してきた。それも今日でおしまいよ",
              "maleVariant": "",
              "stringId": "3237738061916184576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "目的じゃなくて、手段が許せないのよ",
              "maleVariant": "",
              "stringId": "3240679026813476864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サイバー核の仕組みは？",
              "maleVariant": "",
              "stringId": "3240723956785127424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サイバー核？ 初耳ね",
              "maleVariant": "サイバー核？ 初耳だな",
              "stringId": "3240724163144884224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "音なき爆弾――相手の地位も信条も考慮しない、真の平等主義的兵器よ",
              "maleVariant": "",
              "stringId": "3240731738695442432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サブネットに仕掛ければ、接続しているすべての人間が焼かれる",
              "maleVariant": "",
              "stringId": "3240733557026889728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "例外はないわ。重役、科学者に平社員。カフェテリアの従業員から用務員まで一人残らず、皆殺しよ",
              "maleVariant": "",
              "stringId": "3240733854201716736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "考えが甘いというか、考えてなかったでしょ",
              "maleVariant": "考えが甘いというか、考えてなかっただろ",
              "stringId": "3246227111216062464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、仕事にかかる",
              "maleVariant": "",
              "stringId": "3246278099708846084"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "罪滅ぼし？",
              "maleVariant": "罪滅ぼしか？",
              "stringId": "3246278413526671360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何かの罪滅ぼしがしたいようね…",
              "maleVariant": "何かの罪滅ぼしがしたいようだな…",
              "stringId": "3246279091594633216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ仕事にかかる",
              "maleVariant": "",
              "stringId": "3246283691286503424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "済んだらそのチップは返して",
              "maleVariant": "",
              "stringId": "3246287044833878016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "記念に取っておくの？",
              "maleVariant": "記念に取っておくのか？",
              "stringId": "3246288158220263424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クリムゾン・ハーベストの手に渡ったら、私はおしまいよ。油断はできない",
              "maleVariant": "",
              "stringId": "3246288224087613440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "結局は自分のためなのかも",
              "maleVariant": "",
              "stringId": "3246317537474289664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… 詮索はよして",
              "maleVariant": "",
              "stringId": "3246318809707356160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "批判は結構よ",
              "maleVariant": "",
              "stringId": "3246386111123464192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やめて！ …私たちはテロリストなんかじゃない！",
              "maleVariant": "",
              "stringId": "3255292013087989760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時間は戻せない。それでも今を、そしてこれから起こることは変えられる",
              "maleVariant": "",
              "stringId": "3256386136511520768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセンはスタジアム西側のスポーツセラピーセンターを兵器工場に作り替えた。入口のコードは1-7-0-1。忘れないようにね",
              "maleVariant": "",
              "stringId": "3262200462312906752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "中には製造ラインとか倉庫とか、色々ある。あんたの目標はプール跡の実験ラボよ。そこに爆弾が保管されてる",
              "maleVariant": "",
              "stringId": "3262210839775977472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼んだよ、V",
              "maleVariant": "",
              "stringId": "3262220232701681664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを受け取る] 不用心ね",
              "maleVariant": "[チップを受け取る] 不用心だな",
              "stringId": "3264952227793903616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "不用心ね。こういうのは初めて？",
              "maleVariant": "不用心だな。こういうのは初めてか？",
              "stringId": "3265002001146486784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これを",
              "maleVariant": "",
              "stringId": "3267855438469046272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チップの中身は？",
              "maleVariant": "",
              "stringId": "3267857668077166592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "順を追って説明する",
              "maleVariant": "",
              "stringId": "3267857967214927872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私の伝記を書いてほしいわけじゃない",
              "maleVariant": "",
              "stringId": "3318524140369387520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バイオテクニカから感謝されることはない",
              "maleVariant": "",
              "stringId": "3324562111392555008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バイオテクニカに恩を売ってクリムゾン・ハーベストから守ってもらおうって考えなら、がっかりすることになる",
              "maleVariant": "",
              "stringId": "3324562761073467400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CHOOH2で誰のお腹が満たせるっていうの？ バカげた話よ",
              "maleVariant": "",
              "stringId": "3324586539857911808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ノーマッドのルーツも農民だった",
              "maleVariant": "",
              "stringId": "3324597340291375104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "農場を失っても、道はある。最初のノーマッドたちも農民だった",
              "maleVariant": "",
              "stringId": "3324614477143777280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん、クランが企業の仕事を引き受けてるって聞いた。なんとも皮肉な話ね",
              "maleVariant": "",
              "stringId": "3324627944735154176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "生きていくためには理想ばっか追ってられないのよ",
              "maleVariant": "生きていくためには理想ばっか追ってられないんだよ",
              "stringId": "3324630469035073536"
            },
