  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リード… 私は… 多くの間違いを犯した…",
              "maleVariant": "",
              "stringId": "3083771203703410688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "責任は全員にある。俺も含めて",
              "maleVariant": "",
              "stringId": "3083776380799946752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何か計画があるの？",
              "maleVariant": "何か計画があるのか？",
              "stringId": "3083894942147416064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふう、ただいま。よかった… どういうわけか、さっき触れたときにリンクが再構築されたみたいね",
              "maleVariant": "",
              "stringId": "3085493928272728064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[離れる] いったい何が起きてるの？",
              "maleVariant": "[離れる] いったい何が起きてるんだ？",
              "stringId": "3085496508709212160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やってみる",
              "maleVariant": "",
              "stringId": "3085887587895873536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。で、肝心の標的はどこに？",
              "maleVariant": "",
              "stringId": "3085890213597286400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこのシャンパングラス… 必要になる情報を隠しておいた。リードに渡して",
              "maleVariant": "",
              "stringId": "3085891859760943104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おや、ソミ。お友達を紹介してくれるか？",
              "maleVariant": "",
              "stringId": "3105649733322174464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どちら様かな？ パーティーの出席者は、全員知っているはずなんだが…",
              "maleVariant": "",
              "stringId": "3105667787200425984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "顔を覚えるのは得意でね。カート・ハンセンだ",
              "maleVariant": "",
              "stringId": "3105681629225709568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほう… 2人ともか？ まあいい。是非楽しんでいってくれ",
              "maleVariant": "",
              "stringId": "3105742582982144000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて… 行こうか、ソミ。街の政治家連中がお前に会いたがっている",
              "maleVariant": "",
              "stringId": "3134838864933580800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "楽しそうね、カート",
              "maleVariant": "",
              "stringId": "3134839679719075840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[握手しない] ソミの友人のVよ",
              "maleVariant": "[握手しない] ソミの友人のVだ",
              "stringId": "3152086130576617472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また後でね、V",
              "maleVariant": "",
              "stringId": "3152140886577803264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは… できないわ",
              "maleVariant": "",
              "stringId": "3154829605023649792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ…",
              "maleVariant": "",
              "stringId": "3156322065239379968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vよ。ソミに招待されたの",
              "maleVariant": "Vだ。ソミに招待された",
              "stringId": "3156355956876644352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "下の階よ。赤髪の双子。どちらもICEで完全防護してる。スキャンしてもダミーデータしか拾えない",
              "maleVariant": "",
              "stringId": "3156465819623493632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、上を見ろ。ありゃ囚われのお姫様じゃないか？",
              "maleVariant": "",
              "stringId": "3176784785327415296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[寄りかかる]",
              "maleVariant": "",
              "stringId": "3244740934456602628"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[離れる]",
              "maleVariant": "",
              "stringId": "3244774545360711684"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[握手する] Vよ",
              "maleVariant": "[握手する] Vだ",
              "stringId": "3257799998927147008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vよ。ただのV",
              "maleVariant": "Vだ。ただのV",
              "stringId": "3261169577015513088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうか… あまり社交場には慣れてなさそうだな",
              "maleVariant": "",
              "stringId": "3261177017006166016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミとは久々の再会なのよ",
              "maleVariant": "ソミとは久々の再会なんだ",
              "stringId": "3261179552311939072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほう、そうだったか。私からすれば、再会は常に… 痛みが伴う儀式だ。大抵の場合、会わなかった方がよかったと思ってしまう",
              "maleVariant": "",
              "stringId": "3261181896525221888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カート、この人たちは古い友人なの",
              "maleVariant": "",
              "stringId": "3261187518838738944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ何か隠してるでしょ",
              "maleVariant": "まだ何か隠してるだろ",
              "stringId": "3261278706027597824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだな。まずは脱出が先決だ",
              "maleVariant": "",
              "stringId": "3265134465052184576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[話しかける] ソングバード？",
              "maleVariant": "",
              "stringId": "3275270066099326976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パーティーにドレス、シャンパン… ずいぶんと自由な捕虜ね",
              "maleVariant": "パーティーにドレス、シャンパン… ずいぶんと自由な捕虜だな",
              "stringId": "3275273073314631680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソングバード？",
              "maleVariant": "",
              "stringId": "3275273124300591104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "檻が派手なだけよ。ここから出ることは許されてない",
              "maleVariant": "",
              "stringId": "3275276623457423360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… 説明が難しいな。ブラックウォールの突破には、それなりの代償があってね…",
              "maleVariant": "",
              "stringId": "3275280015055523840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたじゃない、ブラックウォールが殺した",
              "maleVariant": "",
              "stringId": "3275285130730672132"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V… 来てくれたのね",
              "maleVariant": "",
              "stringId": "3275291851767287808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話は後にしましょ",
              "maleVariant": "話は後にしよう",
              "stringId": "3275337993725022208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2人とも… 今はゆっくり話してる場合じゃない",
              "maleVariant": "",
              "stringId": "3275337993792131084"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すごいクロームね… 〈Relic〉を通した姿は…",
              "maleVariant": "すごいクロームだな… 〈Relic〉を通した姿は…",
              "stringId": "3276826373604380672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全部説明するから、話を聞いてくれる？",
              "maleVariant": "",
              "stringId": "3301249341150380032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[離れる] どうも騙されてるような気がする",
              "maleVariant": "",
              "stringId": "3301250200177393664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの命を救えるってのはウソだったのね。結局人任せなうえ、こんな大事に巻き込むなんて",
              "maleVariant": "俺の命を救えるってのはウソだったんだな。結局人任せなうえ、こんな大事に巻き込むなんて",
              "stringId": "3301250274651455488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを取る]",
              "maleVariant": "",
              "stringId": "3323146913461460996"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭を整理したい… 酒がいるな。俺は下に戻る",
              "maleVariant": "",
              "stringId": "3336062295474311168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": "3336076144428584960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何かあったら連絡して",
              "maleVariant": "何かあったら連絡してくれ",
              "stringId": "3336076534163312640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": "3336078797845958656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何かあったら連絡して",
              "maleVariant": "何かあったら連絡してくれ",
              "stringId": "3336078890657517568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "びっくりした…？  髪型を変えたからかな",
              "maleVariant": "",
              "stringId": "3495653735400607744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのネットランナー… 再接続を手伝ってくれた人は… 私… 私が殺しちゃったんでしょ？",
              "maleVariant": "",
              "stringId": "3495668034235928576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "違う。ブラックウォールが殺したのよ",
              "maleVariant": "違う。ブラックウォールが殺したんだ",
              "stringId": "3495669936621228032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、責任はこっちにもある",
              "maleVariant": "",
              "stringId": "3495669972658688000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしのせいでもある",
              "maleVariant": "俺のせいでもある",
              "stringId": "3495670659383693312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたまで死ななくてよかった",
              "maleVariant": "",
              "stringId": "3495674915998273536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしたちは運がよかった",
              "maleVariant": "俺たちは運がよかった",
              "stringId": "3495677084872871936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの時は… すべてを説明してる時間がなくて…",
              "maleVariant": "",
              "stringId": "3495688199342264320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズたちがお前に何をさせていたかは知ってる。お前を追い込み… 俺はそれを止めることができなかった",
              "maleVariant": "",
              "stringId": "3495696221485535232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "入ることができたんだ、出ることもできる。俺を信じろ",
              "maleVariant": "",
              "stringId": "3495706720264654848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "違うのよ。リード、私は… 死にかけてるの。Vと一緒。Vは〈Relic〉… そして私はブラックウォールのせいで…",
              "maleVariant": "",
              "stringId": "3495708679809929216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え… あんたもあたしと同じ… なんで今まで黙ってたの？",
              "maleVariant": "え… あんたも俺と同じ… なんで今まで黙ってたんだ？",
              "stringId": "3495786779663011840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時間がなかった。今もないわ。でも… 助けが必要なのは変わらない。あなたの助けが",
              "maleVariant": "",
              "stringId": "3495787417935417344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんでもっと早く言わなかったの？",
              "maleVariant": "なんでもっと早く言わなかったんだ？",
              "stringId": "3495788325532471296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセンは、私とVの命を救えるテックを持ってる。地下バンカーから掘り出したの",
              "maleVariant": "",
              "stringId": "3495807573579403264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "腹を空かせたネズミたちだ。ちゃんと餌をやらんと、ドッグタウンを食いつくされてしまう",
              "maleVariant": "",
              "stringId": "3495847210909831168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもハンセンが私たちの“治療薬”を持ってる。だから逃げるわけにはいかない",
              "maleVariant": "",
              "stringId": "3496979123653447680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウソはついてない… すべてを伝えてなかっただけ",
              "maleVariant": "",
              "stringId": "3496990807843540992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とりあえず計画について教えて",
              "maleVariant": "とりあえず計画について教えてくれ",
              "stringId": "3497008290793132036"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気に食わない",
              "maleVariant": "",
              "stringId": "3497009105226305536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、あんたの計画は？",
              "maleVariant": "",
              "stringId": "3497013658747277312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… まったくもって気に食わない…",
              "maleVariant": "",
              "stringId": "3497013701126524928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうことなの、ソミ。カクテルドレスを着てパーティー会場を歩き回る捕虜なんて聞いたことがない",
              "maleVariant": "どういうことなんだ、ソミ。カクテルドレスを着てパーティー会場を歩き回る捕虜なんて聞いたことがない",
              "stringId": "3497025996661592064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただの演技よ。生き延びるために、必要なことをやってるまで。ハンセンのご機嫌取りもそのうちのひとつ",
              "maleVariant": "",
              "stringId": "3497027642640699392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは私も一緒よ",
              "maleVariant": "",
              "stringId": "3497060038421385216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "簡単に言えば、特定の境界条件で拘束されたAI用のストレージ",
              "maleVariant": "",
              "stringId": "3497061915993182208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "神経マトリックスを奪取する。ブラックウォールの向こう側のAIを取り込んだテックよ",
              "maleVariant": "",
              "stringId": "3497181198274400256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マトリックスは、ハンセンがバンカーから掘り出したメインフレームに入ってる。ただ、ハンセンは解除する鍵を持ってない",
              "maleVariant": "",
              "stringId": "3497181534607249408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼らがメインフレームを開き、私がマトリックスとデータを抽出することになってる。隣り合わせで作業を行うの",
              "maleVariant": "",
              "stringId": "3497181714744217600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうやってマトリックスを盗むつもりなの？",
              "maleVariant": "どうやってマトリックスを盗むつもりなんだ？",
              "stringId": "3497186461891039240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "焦ってるようだけど",
              "maleVariant": "焦ってるようだが",
              "stringId": "3497186461891039260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その神経マトリックスって何なの？",
              "maleVariant": "その神経マトリックスって何なんだ？",
              "stringId": "3497195820289232908"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AIを起動すると、その条件で定められたプログラムを実行する… でも今は詳しく説明してる時間がない",
              "maleVariant": "",
              "stringId": "3497195820289232928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、どうやって盗むの？",
              "maleVariant": "で、どうやって盗むんだ？",
              "stringId": "3497215663440793600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この計画も… ハンセンに関する話も… ソミ、色々と怪しすぎる",
              "maleVariant": "",
              "stringId": "3497231903030906880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すべてを説明してる時間はない。とにかく、マトリックスが絶対に必要なの。まずはランナーたちを優先して",
              "maleVariant": "",
              "stringId": "3497236931581972480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセンは過去にこのテックに関わっていた2人組のランナーを雇った。その2人はアクセスコードを持ってる。金と引き換えにハンセンに渡すのよ",
              "maleVariant": "",
              "stringId": "3499918701829951488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "過去の姿を投影したの。その、まだ…",
              "maleVariant": "",
              "stringId": "3507414162874699776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたも？",
              "maleVariant": "",
              "stringId": "3507432972163878912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死にかけてるうえ、逃げられもしないのか",
              "maleVariant": "",
              "stringId": "3508721034382675968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "相変わらず世話のかかる…",
              "maleVariant": "",
              "stringId": "3508726327242002432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しかも、自分を捕らえた張本人とイチャイチャして",
              "maleVariant": "",
              "stringId": "3508780848462827520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ランナーに成り済ますって、どうやって？",
              "maleVariant": "ランナーに成り済ますって、どうやってだ？",
              "stringId": "3508842901378568192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ランナーに成り済ます… そんなことができるの？ どうやって？",
              "maleVariant": "ランナーに成り済ます… そんなことができるのか？ どうやってだ？",
              "stringId": "3508843238801936384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人格模倣テック――FIAの専売特許よ。アレックスとリードに聞けばわかるわ",
              "maleVariant": "",
              "stringId": "3508844320110284800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドレス、似合ってるよ",
              "maleVariant": "",
              "stringId": "3534925371341410304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "素敵なドレスね",
              "maleVariant": "素敵なドレスだ",
              "stringId": "3534925441939935236"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すごいクロームね… ホログラムの姿とは全然違う",
              "maleVariant": "すごいクロームだな… ホログラムの姿とは全然違う",
              "stringId": "3534932148950798336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "捕虜のようには見えないけど",
              "maleVariant": "捕虜のようには見えないが",
              "stringId": "3534933784444145664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう。もっと他に気になるところがあると思うけど…",
              "maleVariant": "",
              "stringId": "3534946186833301504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく、V――来てくれてよかった。あなただけが頼りよ",
              "maleVariant": "",
              "stringId": "3534951484843233280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "来るしかなかった",
              "maleVariant": "",
              "stringId": "3534960500147359748"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたを助けにきた",
              "maleVariant": "",
              "stringId": "3534967300103999488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "来るしかなかったのよ。場所だけ告げて、すぐに消えるんだもの",
              "maleVariant": "来るしかなかったんだ。場所だけ告げて、すぐに消えるんだからな",
              "stringId": "3534968336919171072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ようやく直接会うことができてうれしいよ。ずっと… 心配だったから",
              "maleVariant": "ようやく直接会うことができてうれしいよ。ずっと… 心配だったからな",
              "stringId": "3534968380456046592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、そうよ",
              "maleVariant": "ああ、そうだ",
              "stringId": "3534993284219932672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、あんたが殺した",
              "maleVariant": "ああ、あんたが殺した",
              "stringId": "3535025447401295872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう… そのつもりはなかったんだけど",
              "maleVariant": "",
              "stringId": "3535030325376847872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実際はリードのせいよ",
              "maleVariant": "実際はリードのせいだ",
              "stringId": "3535035148356685824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、あれはリードが無理やり接続させたようなものよ",
              "maleVariant": "いや、あれはリードが無理やり接続させたようなもんだ",
              "stringId": "3535035702508130304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それに、あたしたちは無事だったでしょ",
              "maleVariant": "それに、俺たちは無事だっただろ",
              "stringId": "3535037351372918784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すでに計画がある――そうなんでしょ？",
              "maleVariant": "すでに計画がある――そうなんだろ？",
              "stringId": "3535082497368002560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。でもあなたの協力が必要になる",
              "maleVariant": "",
              "stringId": "3535085903394508800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V… 今… 今はすべてを説明できない。でも、どうしてもあなたの力が必要なの",
              "maleVariant": "",
              "stringId": "3536111444478267392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたと、私自身を救うために… そこだけはブレてない",
              "maleVariant": "",
              "stringId": "3536114871040086016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼らに成り済ましてハンセンと会う。そして現場にいる私と一緒にマトリックスを盗むのよ",
              "maleVariant": "",
              "stringId": "3536212168306462720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんか焦ってない？",
              "maleVariant": "なんか焦ってないか？",
              "stringId": "3536256193969393664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本物の私は、今ハンセンの隣にいるのよ。早くこっちの会話を切り上げないと、怪しまれちゃう",
              "maleVariant": "",
              "stringId": "3536257218553974784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、もう行かないと。V、頼んだわよ",
              "maleVariant": "",
              "stringId": "3536287654705500160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほどね。他には？",
              "maleVariant": "なるほどな。他には？",
              "stringId": "3536294921672724480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ…？ はあ… ったく…",
              "maleVariant": "",
              "stringId": "3536300218021711872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誤魔化さないで…",
              "maleVariant": "誤魔化すなよ…",
              "stringId": "3536310237425438720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソミ…？ はあ… ったく…",
              "maleVariant": "",
              "stringId": "3536312428529848320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その神経マトリックスって何なの？",
              "maleVariant": "その神経マトリックスって何なんだ？",
              "stringId": "3537567501557829632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全員が、それぞれ別のクソ溜めに放り込まれてる",
              "maleVariant": "",
              "stringId": "3539388510313254912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいんだ、ソミ",
              "maleVariant": "",
              "stringId": "3552299072114266112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話は後だ。まずはお前をここから連れ出さないと",
              "maleVariant": "",
              "stringId": "3552324092714684416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それを奪う。そのためには、捕虜として奴の傍にい続ける必要があるの",
              "maleVariant": "",
              "stringId": "3552350085185671168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたたちの協力は必ず必要になる。チャンスが訪れるまで私は残り、テックを手に入れたらすぐに脱出するわ",
              "maleVariant": "",
              "stringId": "3552352827522289664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたは事前にランナーたちからアクセスコードを盗み――",
              "maleVariant": "",
              "stringId": "3559632461964120064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よりによって、あの2人が協力してくれることになるとは思わなかった…",
              "maleVariant": "",
              "stringId": "3559651498953674752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ何か隠してるでしょ",
              "maleVariant": "まだ何か隠してるだろ",
              "stringId": "3559655999928406016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ… 過ぎたことよ",
              "maleVariant": "まあ… 過ぎたことだ",
              "stringId": "3559672151991783424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここにいる。下の階よ",
              "maleVariant": "",
              "stringId": "3601578103744999424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "赤髪の双子。どちらもICEで完全防護してる。スキャンすらできない",
              "maleVariant": "",
              "stringId": "3601579791264493568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[寄りかかる] 素敵なドレスね",
              "maleVariant": "[寄りかかる] 素敵なドレスだ",
              "stringId": "3721806416471371788"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[寄りかかる]すごいクロームね… ホログラムの姿とは全然違う",
              "maleVariant": "[寄りかかる] すごいクロームだな… ホログラムの姿とは全然違う",
              "stringId": "3721806416471371812"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[寄りかかる] 捕虜のようには見えないけど",
              "maleVariant": "[寄りかかる] 捕虜のようには見えないが",
              "stringId": "3721806416471371836"
            }
          ]
        }
      }
