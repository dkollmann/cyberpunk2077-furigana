  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、大事な話をさせて",
              "maleVariant": "",
              "stringId": "1603775375583342592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジュディは何度も私を助けてくれた。誰よりも信用できる子よ",
              "maleVariant": "",
              "stringId": "1603775375583342608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも彼女はモックスの一人であって、決してあなたの仕事仲間じゃない",
              "maleVariant": "",
              "stringId": "1603775375583342624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それを忘れないで。余計なことは言わないよう、注意して",
              "maleVariant": "",
              "stringId": "1603775375600119816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "デクスから色々聞いてるわよ。相当腕が立つんですってね…",
              "maleVariant": "",
              "stringId": "1603775375633674240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "腕は確かよ。チームにも優秀な人材が揃ってる。仕事でヘマしたことは一度もない",
              "maleVariant": "腕は確かだ。チームにも優秀な人材が揃ってる。仕事でヘマしたことは一度もないさ",
              "stringId": "1603775375667228672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは結構。でも今回は、そこらのデータ端末から情報を盗むのとはわけが違う。それはわかってる？",
              "maleVariant": "",
              "stringId": "1603775375667228688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、任せて。かなり危険な仕事だってことも、デクスから聞いてる",
              "maleVariant": "ああ、任せてくれ。かなり危険な仕事だってことも、デクスから聞いてる",
              "stringId": "1603775375667228704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ところで、デクスとは組んで長いの？",
              "maleVariant": "",
              "stringId": "1603775375684005916"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本題に入りましょ。仕事の内容を教えて",
              "maleVariant": "本題に入ろう。仕事の内容を教えてくれ",
              "stringId": "1603775375717560320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もうひとつ教えて。その〈Relic〉ってチップには何が入ってるの？",
              "maleVariant": "もうひとつ教えてくれ。その〈Relic〉ってチップには何が入っているんだ？",
              "stringId": "1603775375835000832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたには、一切関係ない",
              "maleVariant": "",
              "stringId": "1603775375835000848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブがアラサカから盗み出したうえ、ネットウォッチに流そうとしてる品物でしょ。その中身を関係ないで済ます気？",
              "maleVariant": "ヨリノブがアラサカから盗み出したうえ、ネットウォッチに流そうとしてる品物だろ。その中身を関係ないで済ます気か？",
              "stringId": "1603775375835000860"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V。〈Relic〉のことは、こっちの問題よ。あなたは自分の仕事の心配をしてればいい",
              "maleVariant": "",
              "stringId": "1603775375851778048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "約束どおりしっかりこなせば、報酬はたっぷり支払うわ",
              "maleVariant": "",
              "stringId": "1603775375851778064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今回の目標については… すでに聞いてるわね",
              "maleVariant": "",
              "stringId": "1603775375969218560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈Relic〉、通称“魂の救済”。目当てはそのテクノロジーでしょ",
              "maleVariant": "〈Relic〉、通称“魂の救済”。目当てはそのテクノロジーだろ",
              "stringId": "1603775375985995780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカはこの人格移植テクノロジーに何十億もつぎ込んでる。でも私が欲しいのは中にあるデータよ",
              "maleVariant": "",
              "stringId": "1603775375985995792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チップは紺碧プラザホテルの一室に保管されてる。行ったことは？",
              "maleVariant": "",
              "stringId": "1603775376002772996"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] もう十分よ",
              "maleVariant": "[立ち上がる] もう十分だ",
              "stringId": "1603775376036327424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブについて、他に情報は？",
              "maleVariant": "",
              "stringId": "1603775376053104648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈Relic〉には何が入ってるの？",
              "maleVariant": "〈Relic〉には何が入ってるんだ？",
              "stringId": "1603775376053104664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう十分。で、次は？",
              "maleVariant": "もう十分だ。で、次は？",
              "stringId": "1603775376103436288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブについてもっと情報はないの？",
              "maleVariant": "ヨリノブについてもっと情報はないのか？",
              "stringId": "1603775376136990720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "山ほどあるわ。彼は東京で財政学とバイオテクノロジーを学んでる。後継ぎなだけに、他の道はなかったみたいね",
              "maleVariant": "",
              "stringId": "1603775376136990736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブは〈Relic〉に、すべてを賭けてるってわけね",
              "maleVariant": "ヨリノブは〈Relic〉に、すべてを賭けてるんだな",
              "stringId": "1603775376187322368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。彼はこの街が作り出す幻に心を奪われたのよ…",
              "maleVariant": "",
              "stringId": "1603775376187322384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何事も思いのままにできるって… 自分が支配する側だという幻に",
              "maleVariant": "",
              "stringId": "1603775376204099604"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BDはすぐ見れるの？",
              "maleVariant": "BDはすぐ見れるのか？",
              "stringId": "1603775376237654016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジュディがやってくれる。モックスの子でね。彼女とは長い付き合いなの",
              "maleVariant": "",
              "stringId": "1603775376254431248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは優秀よ",
              "maleVariant": "俺は優秀だ",
              "stringId": "1603775376858411008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そもそもデクスの言葉なんて信じてないんでしょ",
              "maleVariant": "そもそもデクスの言葉なんて信じてないんだろ",
              "stringId": "1603775376875188232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早く本題に入りましょ",
              "maleVariant": "早く本題に入ろう",
              "stringId": "1603775376875188248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね。あなたは人生を変えるほどの知識を得るために来た",
              "maleVariant": "",
              "stringId": "1603775376925519884"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ずいぶんと意味深じゃない",
              "maleVariant": "ずいぶんと意味深だな",
              "stringId": "1603775376925519896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここからが本題よ",
              "maleVariant": "",
              "stringId": "1603775377093292032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そもそもデクスの言葉なんて、ハナから信じてないくせに",
              "maleVariant": "",
              "stringId": "1603775377143623680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誉めてるのに、素直に喜べないの？",
              "maleVariant": "",
              "stringId": "1603775377160400896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vよ",
              "maleVariant": "Vだ",
              "stringId": "1603775377244286976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "未来の上客よ",
              "maleVariant": "未来の上客だ",
              "stringId": "1603775377244286992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vよ。エヴリンと… デートの約束があるの",
              "maleVariant": "Vだ。エヴリンと、デートの約束があってな",
              "stringId": "1603775377311395840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうか。よろしく。俺はマテオだ",
              "maleVariant": "",
              "stringId": "1603775377311395852"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも… で、エヴリンは？",
              "maleVariant": "",
              "stringId": "1603775377311395864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいから質問に答えて",
              "maleVariant": "いいから質問に答えろ",
              "stringId": "1603775377395281920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着けよ、チューマ。ちょっとボックスにでも行ってきたらどうだ",
              "maleVariant": "",
              "stringId": "1603775377395281932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で？ お気に召した？",
              "maleVariant": "で？ お気に召したか？",
              "stringId": "1603775377764380672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あら、聞くまでもないと思うけど。でしょ？",
              "maleVariant": "",
              "stringId": "1603775377764380684"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本題に入らない？ 頼みたい仕事があるんでしょ",
              "maleVariant": "本題に入らないか？ 頼みたい仕事があるんだろ",
              "stringId": "1603775377865043968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。でもここじゃダメ。ついて来て",
              "maleVariant": "",
              "stringId": "1603775377865043980"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラウンジにいるわ、マテオ。誰か訪ねてきても、いないことにして",
              "maleVariant": "",
              "stringId": "1603775377865043992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ついて来て。誰にも聞かれずに話せる場所があるから",
              "maleVariant": "",
              "stringId": "1603775377932152832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラウンジにいるわ、マテオ。誰か訪ねてきても、いないことにして",
              "maleVariant": "",
              "stringId": "1603775377948930052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんで声をかけなかったの？",
              "maleVariant": "なぜ声をかけなかった？",
              "stringId": "1603775378083147776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "センツォン？ 確かに好きだけど…",
              "maleVariant": "センツォン？ 確かに好きだが…",
              "stringId": "1603775378099924992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして知ってるの？",
              "maleVariant": "どうして知ってるんだ？",
              "stringId": "1603775378116702224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る]",
              "maleVariant": "",
              "stringId": "1603775378586464256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ。何か隠してる情報があるなら、もったいぶらずに教えて",
              "maleVariant": "何か隠してる情報があるなら、もったいぶらずに教えてくれ",
              "stringId": "1603775378922008576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふっ、なかなか鋭いわね。ヨリノブはアラサカの研究所からチップを盗んだの。すでにネットウォッチとは交渉済み――彼らに売るつもりよ",
              "maleVariant": "",
              "stringId": "1603775378922008588"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "1603775379240775680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それに信用もできるとか",
              "maleVariant": "",
              "stringId": "1603775379425325056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、中身は？",
              "maleVariant": "",
              "stringId": "1603775380280963072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブの部屋の記録よ。中の様子がわかるわ",
              "maleVariant": "",
              "stringId": "1603775380297740288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“お宝”は自分で見つけて。十分な情報が録れてるといいんだけど",
              "maleVariant": "",
              "stringId": "1603775380297740316"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと待って… あんたが記録したの？",
              "maleVariant": "ちょっと待て… あんたが記録したのか？",
              "stringId": "1603775380297740340"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、座って。話すことがたくさんあるから",
              "maleVariant": "",
              "stringId": "1603775380952051712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕事の話をしたい",
              "maleVariant": "",
              "stringId": "1608061306301706240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜこの場所を？",
              "maleVariant": "",
              "stringId": "1608067444497838080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "デクスもなかなか見る目があるのね。彼との付き合いは長いの？",
              "maleVariant": "",
              "stringId": "1608083953345490944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼と組み始めたのは最近よ",
              "maleVariant": "彼と組み始めたのは最近だ",
              "stringId": "1608103371513393152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "デクスの伝記でも書くの？ 悪いけど、彼のことはそこまで知らない",
              "maleVariant": "デクスの伝記でも書くつもりか？ 悪いが、彼のことはそこまで知らない",
              "stringId": "1608103543211421696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フィクサーは2種類いるって話、知ってる？ 一方は厳しく管理して決まった人材を使い続けるタイプ",
              "maleVariant": "",
              "stringId": "1608106087560114176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまり、他人同然ってことね… なのに、彼はあなたにこの仕事を任せた",
              "maleVariant": "",
              "stringId": "1608174639902224384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ひとつ聞かせて。どうしてこんなところに呼んだの？",
              "maleVariant": "聞かせてくれ。なぜこんなところに呼んだんだ？",
              "stringId": "1608423499014729728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直、場違いに見えるんだけど",
              "maleVariant": "正直、場違いに見えるんだが",
              "stringId": "1608424576783085568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誉め言葉と取っておくわね",
              "maleVariant": "",
              "stringId": "1608435177332797440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブは厳重な警備を敷いてるはず",
              "maleVariant": "ヨリノブは厳重な警備を敷いてるはずだ",
              "stringId": "1608458988497268736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブ・アラサカが標的？ 本気なの？",
              "maleVariant": "ヨリノブ・アラサカが標的？ 本気か？",
              "stringId": "1608460659793195008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブほどの大物なら、かなりの警備を敷いてるはず。ホテルは要塞化してるでしょうね",
              "maleVariant": "ヨリノブほどの大物なら、かなりの警備を敷いてるはずだ。ホテルは要塞化してるだろう",
              "stringId": "1608472871744172032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼は警護をつけないの。一切ね。昔、全員クビにしてから、ずっとそう",
              "maleVariant": "",
              "stringId": "1608479055674994688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "えっ、どうして？",
              "maleVariant": "あ？ なぜだ？",
              "stringId": "1608480623187062784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞いたことない？ アラサカの情報力は――",
              "maleVariant": "",
              "stringId": "1608481050318204928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまり標的はアラサカ社じゃなく、ヨリノブ一人ってことね",
              "maleVariant": "つまり標的はアラサカ社じゃなく、ヨリノブ一人ってことか",
              "stringId": "1608482763104186368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブはお飾りに過ぎない。何年も前に父親の信頼をなくしてから、力を奪われてる",
              "maleVariant": "",
              "stringId": "1608484961708011520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもその後、自らの道を進むためにアラサカ社との繋がりを絶った。色々あって結局戻ってはきたけど、その時のわだかまりは残ったまま――まさに異端児ね",
              "maleVariant": "",
              "stringId": "1608505276165124096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫よ、マテオ。私のお客だから",
              "maleVariant": "",
              "stringId": "1611199584982921216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エヴリン・パーカーよ",
              "maleVariant": "",
              "stringId": "1611205480328851456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一目見ただけで、あなただってわかったわ",
              "maleVariant": "",
              "stringId": "1611208450466508800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ならなぜ声をかけなかったの？",
              "maleVariant": "ならなぜ声をかけなかった？",
              "stringId": "1611211140592431104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕事相手のことは、何でも知っておきたいの",
              "maleVariant": "",
              "stringId": "1611212536356139008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしくは、ただの勘かもね",
              "maleVariant": "",
              "stringId": "1611213446285225984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずは観察したかったから",
              "maleVariant": "",
              "stringId": "1611214283350863872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で？ お気に召した？",
              "maleVariant": "で？ 気に召したか？",
              "stringId": "1611215513338900484"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕事の話をしましょ",
              "maleVariant": "仕事の話をしよう",
              "stringId": "1611215992496189440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてこんなところに呼んだの？",
              "maleVariant": "なぜこんなところに呼んだ？",
              "stringId": "1611216236755677184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "評判どおりだといいんだけど",
              "maleVariant": "",
              "stringId": "1611327554540138496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "忠誠心と統制を何よりも重んじ、相手に信用を置く。デクスター・デショーンは、もう一方のタイプよ",
              "maleVariant": "",
              "stringId": "1611357726131744768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "高級ホテルとは無縁なの",
              "maleVariant": "高級ホテルとは無縁でね",
              "stringId": "1611401675491897344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのホテルチェーンはよく知ってる",
              "maleVariant": "",
              "stringId": "1611402053566459904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行ったことはない",
              "maleVariant": "",
              "stringId": "1611403469043396608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業の高級ホテルでしょ？ あいにくそんな贅沢とは無縁でね",
              "maleVariant": "企業の高級ホテルだろ？ あいにくそんな贅沢とは無縁でな",
              "stringId": "1611412242319405056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フフッ、とっても素敵なところよ。きっと気に入るわ",
              "maleVariant": "",
              "stringId": "1611412651096272896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここの作りは、東京の紺碧とまったく同じよ",
              "maleVariant": "",
              "stringId": "1611414474427330560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいにく、その辺のエリアには縁がなくて",
              "maleVariant": "あいにく、その辺のエリアには縁がなくてな",
              "stringId": "1611419780607766528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もったいない。あそこの料理は罪深いほど絶品よ。シェフが悪魔と取引でもしたんじゃないかってぐらいね",
              "maleVariant": "",
              "stringId": "1611421212710924288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、チップの正確な場所は？",
              "maleVariant": "",
              "stringId": "1611422426290843648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最上階よ。ヨリノブ・アラサカのスイートにあるわ",
              "maleVariant": "",
              "stringId": "1611439751249391616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もったいぶらないで教えて",
              "maleVariant": "もったいぶらないで教えてくれ",
              "stringId": "1612437064163913728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブはどこに〈Relic〉を保管してるの？",
              "maleVariant": "ヨリノブはどこに〈Relic〉を保管してるんだ？",
              "stringId": "1612475737190297604"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ネットウォッチ？ そんなバカな",
              "maleVariant": "",
              "stringId": "1612494579664809984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブが密かに盗み出したなら、チップはアラサカに警備されてないってことね。で、どこに保管されてるの？",
              "maleVariant": "ヨリノブが密かに盗み出したなら、チップはアラサカに警備されてないってことか。で、どこに保管されてるんだ？",
              "stringId": "1612498689478750208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケースはどこに…？",
              "maleVariant": "",
              "stringId": "1612502708477620224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブはずいぶん危険な賭けに出たものね",
              "maleVariant": "ヨリノブはずいぶん危険な賭けに出たな",
              "stringId": "1612514914606903300"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブが急に権力に目覚めた可能性は？",
              "maleVariant": "",
              "stringId": "1612515709209407488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブも所詮、権力に飢えた企業の犬なのね",
              "maleVariant": "ヨリノブも所詮、権力に飢えた企業の犬ってことか",
              "stringId": "1612528632715669504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さながら皇帝の子として生きるのがどんなことか、想像してみたことある？",
              "maleVariant": "",
              "stringId": "1612531778175234048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何もかも手に入るけど、何者にもなれない。サブロウ・アラサカの名声の影にいる限りはね",
              "maleVariant": "",
              "stringId": "1612533166137536512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "哀れな人なのよ。でもだからこそ、私にはその葛藤がよくわかる",
              "maleVariant": "",
              "stringId": "1612593552018763776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこに付け入るってわけね",
              "maleVariant": "そこに付け入るってわけか",
              "stringId": "1612816419901796352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かにネットウォッチは、すべての企業にセキュリティを提供している。それが彼らのビジネスだから",
              "maleVariant": "",
              "stringId": "1614227769609809920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "有機体の神経環境を模倣する、専用コンテナがあるみたい。見た目は、一般的なブリーフケースに近いはずよ",
              "maleVariant": "",
              "stringId": "1614239914854576128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "問題が起きたら？ 予備のプランはあるの？",
              "maleVariant": "問題が起きたら？ 予備のプランはあるのか？",
              "stringId": "1618372624007163904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなものはない。だから必ず成功させて",
              "maleVariant": "",
              "stringId": "1618375777133916160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "質問に答えて",
              "maleVariant": "質問に答えろ",
              "stringId": "1618567008388509696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "未来の上客よ。質問に答えたらチップもはずむ",
              "maleVariant": "未来の上客だ。質問に答えたらチップもはずむ",
              "stringId": "1618567784116645888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パーッとやってリフレッシュしたら、また来な。酒を出してやるからさ",
              "maleVariant": "",
              "stringId": "1618569698363437056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがたい話だね",
              "maleVariant": "",
              "stringId": "1618571682218897408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが、悪いな。オシャベリのサービスはしてないんだ",
              "maleVariant": "",
              "stringId": "1618573421311553536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ、ここは広いからな。探し回ってみたらどうだ",
              "maleVariant": "",
              "stringId": "1618579991688769536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはその目で確かめてもらうわ。そうね… 他に質問がなければ、すぐにでも",
              "maleVariant": "",
              "stringId": "1637147179181797376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サブロウは彼をずっと飼い殺しにしてたけど、また何か企んでると知ったら、いよいよ切り捨てるでしょうね",
              "maleVariant": "",
              "stringId": "1639917172638216192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「ナイトシティでくしゃみをすれば東京で桜が散る」なんて言われるほどよ。警護班を通して父親に情報が筒抜けになるのを恐れてるの",
              "maleVariant": "",
              "stringId": "1639917880418627584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもそれは、ヨリノブの1つの顔に過ぎない。自身が考えるアラサカのあるべき未来を見据える、切れ者としての顔もある",
              "maleVariant": "",
              "stringId": "1639931289440743424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フッ",
              "maleVariant": "",
              "stringId": "1679127720151871488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "来て",
              "maleVariant": "",
              "stringId": "1679145201507627008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "計画に役立つものを用意したわ。紺碧プラザのブレインダンスよ",
              "maleVariant": "",
              "stringId": "1679145701871316992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんでBDを？ 欲しいのは情報よ、スリルじゃない",
              "maleVariant": "BDがなんの役に立つ？ 欲しいのは情報だ、スリルじゃない",
              "stringId": "1679149009985990656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなた、BDの使い道がボックスで見るバーチャルポルノだけだと思ってるの？",
              "maleVariant": "",
              "stringId": "1679149187455381504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BDは究極の分析ツールよ。ブースターを使っても知覚できないような細かいディテールすら、漏れなく把握できる。まさに必要な情報でしょ",
              "maleVariant": "",
              "stringId": "1679151038166855680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブの部屋が記録されてるってことは、中に入ったのね。面識があるってこと？",
              "maleVariant": "ヨリノブの部屋が記録されてるってことは、中に入ったんだな。面識があるのか？",
              "stringId": "1679188187671031808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私がどうやってこれだけの情報を得たと思ってるの？ 彼のことはよく知ってる",
              "maleVariant": "",
              "stringId": "1679188187671031820"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブとは… 互いに利益をもたらすビジネスパートナーってところかしら",
              "maleVariant": "",
              "stringId": "1679188187671031832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "入って",
              "maleVariant": "",
              "stringId": "1705512620357525504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早くして",
              "maleVariant": "",
              "stringId": "1705512922095755264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？",
              "maleVariant": "",
              "stringId": "1705512976202276864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもね、今はまさに戦国時代。自分だけ聖人でいることなんてできない。勝つには、なりふり構っていられないのよ",
              "maleVariant": "",
              "stringId": "1721215017030381568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブ・アラサカがナイトシティに？ なんだ、スクリームシートの記事はデマじゃなかったのね",
              "maleVariant": "ヨリノブ・アラサカがナイトシティに？ なんだ、スクリームシートの記事は、デマじゃなかったのか",
              "stringId": "1721229910014029824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのヨリノブ・アラサカが、ナイトシティに？",
              "maleVariant": "あのヨリノブ・アラサカが？ ナイトシティに？",
              "stringId": "1721230021280526336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スクリームシートを見てないの？ メディアが騒ぎ立ててたじゃない。最近のニュースは彼のことばっかりよ",
              "maleVariant": "",
              "stringId": "1721233674334760960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカ家の真の狙いを知る人は、ナイトシティにもごく僅かしかいないわ",
              "maleVariant": "",
              "stringId": "1721235139623251968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはどうなの？",
              "maleVariant": "あんたはどうなんだ？",
              "stringId": "1721235663240163328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サブロウ・アラサカに残された唯一の息子。アラサカ帝国の次期社長",
              "maleVariant": "",
              "stringId": "1721242107486269440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう周知の事実よ。サブロウ・アラサカに残された唯一の息子… つまりヨリノブが、アラサカ帝国の次期社長だもの",
              "maleVariant": "",
              "stringId": "1721242702557343744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。記録用インプラントでね。いけない？",
              "maleVariant": "",
              "stringId": "1730119903607463936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む] このテキーラは…",
              "maleVariant": "",
              "stringId": "1731634166587138052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早く仕事の話をしましょ",
              "maleVariant": "早く仕事の話をしよう",
              "stringId": "1735649404186652676"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう一方のタイプ？",
              "maleVariant": "",
              "stringId": "1735650402565222400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう一方のタイプ？ どういうこと？",
              "maleVariant": "もう一方のタイプ？ どういうことだ",
              "stringId": "1735652097298919460"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いわゆるヘッドハンターよ。彼らは他人に信用を置くような真似はしない。あくまでも、自分と自分の直感のみを信じ、可能性に賭けていくタイプ。もしその賭けに失敗したら…",
              "maleVariant": "",
              "stringId": "1735652097298919472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "途端に全てを失うことになる",
              "maleVariant": "",
              "stringId": "1735653302456676352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今回は、デクスの直感が正しいことを祈るわ",
              "maleVariant": "",
              "stringId": "1735654169251540992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕事の話をしましょ",
              "maleVariant": "仕事の話をしよう",
              "stringId": "1735656508817534980"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心配なら、直接デクスに聞けばいい",
              "maleVariant": "",
              "stringId": "1735656944756715520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなに心配なら、デクスに今すぐ連絡してみればいいじゃない",
              "maleVariant": "そんなに心配なら、デクスに今すぐ連絡してみればいい",
              "stringId": "1735657405442289712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "意味がないわ。どうせ聞こえのいい言葉を並べるだけだから",
              "maleVariant": "",
              "stringId": "1735657405459066880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フィクサーなんて嘘で商売してるようなものよ。セールスマンと一緒ね。今は自分の直感を信じるわ",
              "maleVariant": "",
              "stringId": "1735657405459066892"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いえ。お世辞を言うタイプには見えないだけよ",
              "maleVariant": "いや。お世辞を言うタイプには見えないだけだ",
              "stringId": "1752090779263197184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕事の話をしない？ 世間話をするために、ここまで来たわけじゃない",
              "maleVariant": "仕事の話をしないか？ 世間話をするために、ここまで来たわけじゃない",
              "stringId": "1752103319007420416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカのホテルチェーンだからね… 高級感はあるけど、いまいちかな。やっぱ紺碧といえば東京のでしょ",
              "maleVariant": "アラサカのホテルチェーンだからな… 高級感はあるが、いまいちだ。やっぱ紺碧といえば東京のだろ",
              "stringId": "1752129291496742912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "作りは一緒でも、日本のスタッフとサービスは別格。シンガポール、パリ、ワシントン… 他の場所なんて足元にも及ばない",
              "maleVariant": "作りは一緒でも、日本のスタッフとサービスは別格だ。シンガポール、パリ、ワシントン… 他の場所なんて足元にも及ばない",
              "stringId": "1752133372772638720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ… 確かにそれは言えてるわね",
              "maleVariant": "",
              "stringId": "1752140409288024064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何が言いたいかわかる？ それともまだ説明が必要かしら？",
              "maleVariant": "",
              "stringId": "1752159522697797632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ組んで間もない",
              "maleVariant": "",
              "stringId": "1755995785052942340"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "デクスの伝記でも書くつもり？",
              "maleVariant": "デクスの伝記でも書くつもりか？",
              "stringId": "1755996059024879616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕事の話をしましょ",
              "maleVariant": "仕事の話をしよう",
              "stringId": "1755996235789627392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "デクスのことはまだよく知らない",
              "maleVariant": "",
              "stringId": "1791001898382913536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう言われてもね。デクスがあたしについて何て言ってたかは知らない。だって…",
              "maleVariant": "そう言われてもな。デクスが俺について、何て言ってたかは知らない。そもそも…",
              "stringId": "1791002879178960896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは？",
              "maleVariant": "",
              "stringId": "1824330029781102608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心配しないで。その辺の分別はある。仕事でもプライベートでもね",
              "maleVariant": "心配するな。その辺の分別はある。仕事でもプライベートでもな",
              "stringId": "1915465624640417792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "相手はあのアラサカ。つまり、相当危険な作戦になる",
              "maleVariant": "相手はあのアラサカだ。つまり、相当危険な作戦になる",
              "stringId": "1915595084182188032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブは見返りに何を得るの？",
              "maleVariant": "ヨリノブは見返りに何を得るんだ？",
              "stringId": "1915604655013490688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブは見返りに何を得るの？",
              "maleVariant": "ヨリノブは見返りに何を得るんだ？",
              "stringId": "1915605087328792576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこは重要じゃない。あなたはチップに集中して",
              "maleVariant": "",
              "stringId": "1915605088754855936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ネットの番人が差し出せるものと言ったら… 敵に関する情報とか？",
              "maleVariant": "",
              "stringId": "1915605090164142080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だからそこはどうでもいいのよ。あなたがチップを手に入れたら、取引そのものがなくなるんだから",
              "maleVariant": "",
              "stringId": "1915605091539873792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ネットウォッチは中立だからこそ成り立ってる。盗品に手を出して評判を傷つけるようなことをするわけがない",
              "maleVariant": "",
              "stringId": "1964563205055967232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え？ ネットウォッチは中立の存在じゃ？",
              "maleVariant": "待て。ネットウォッチは中立の存在じゃ？",
              "stringId": "1964564055946031104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブと知り合いなの？",
              "maleVariant": "ヨリノブと知り合いなのか？",
              "stringId": "1964595848703242244"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別に構わない",
              "maleVariant": "",
              "stringId": "1964604602299576320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうは言ってない",
              "maleVariant": "",
              "stringId": "1964609328105779200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "150年もトップに座り続けた皇帝が、今さらその座を譲るとは思えない",
              "maleVariant": "150年もトップに座り続けた皇帝が、今さらその座を譲るとは思えないが",
              "stringId": "1964647565276868608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "次期社長？ ナイトシティにいる間に玉座を奪うとでも？",
              "maleVariant": "",
              "stringId": "1964648037974929408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それでも… まさかヨリノブが相手だなんて",
              "maleVariant": "それでも… まさかあのヨリノブが相手だとは",
              "stringId": "1964652421811138560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブのことなら少し知ってる",
              "maleVariant": "",
              "stringId": "1964667684279414784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブは確か、東京で財政学とバイオテクノロジーを学んでるのよね",
              "maleVariant": "ヨリノブは確か、東京で財政学とバイオテクノロジーを学んでるんだよな",
              "stringId": "1964674574782689280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。サブロウは彼を後継者に育て上げようとしてた",
              "maleVariant": "",
              "stringId": "1964677213067014144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもヨリノブは突如消えた。そして東京のノーマッド集団〈鋼鉄の龍〉と共に、アラサカに対するクーデターを画策したとか",
              "maleVariant": "だがヨリノブは突如消えた。そして東京のノーマッド集団〈鋼鉄の龍〉と共に、アラサカに対するクーデターを画策したとか",
              "stringId": "1964677666706157568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。だけど若き反抗は、お決まりの結末を迎える。ヨリノブは結局会社に戻ったけど、わだかまりは今も消えてない",
              "maleVariant": "",
              "stringId": "1964683738816835584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知識はあるようね。でもそれは彼の1つの顔に過ぎない。アラサカのあるべき未来を見据える、切れ者としての顔もある",
              "maleVariant": "",
              "stringId": "1964693455962238976"
            }
          ]
        }
      }
    },
