  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ",
              "maleVariant": "",
              "stringId": "3065018288488996864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっぱり頼りになる",
              "maleVariant": "",
              "stringId": "3065022600938151936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何がお望み？",
              "maleVariant": "何が望みだ？",
              "stringId": "3068501882625200132"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちは緊急なの、後にして",
              "maleVariant": "こっちは緊急なんだ、後にしてくれ",
              "stringId": "3068502112405950464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。それで、内容は？",
              "maleVariant": "ああ。それで、内容は？",
              "stringId": "3068502325677920256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼みは聞くけど、今すぐは無理",
              "maleVariant": "頼みは聞くが、今すぐは無理だ",
              "stringId": "3068502406879645696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話を聞かせて",
              "maleVariant": "話を聞こう",
              "stringId": "3068502731535552516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何が言いたいのかわからない",
              "maleVariant": "",
              "stringId": "3068502836628033536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それが本当だとしても… これ以上話すつもりはない。用心のためよ",
              "maleVariant": "それが本当だとしても… これ以上話すつもりはない。用心のためだ",
              "stringId": "3068503002470813696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何が言いたいのかわからない",
              "maleVariant": "何が言いたいのかわからないな",
              "stringId": "3068503090584752128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。で、頼みってのは？",
              "maleVariant": "",
              "stringId": "3068503358651109380"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは自分の意思で動いてる",
              "maleVariant": "実際、俺のためのデータってわけじゃない",
              "stringId": "3068503456210620416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サンドラ、あたしは自分の意思で動いてる",
              "maleVariant": "サンドラ、俺は自分の意思で動いてる",
              "stringId": "3068503582677274624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チップを2つ渡す。1つはご希望のデータ、もう1つはウイルスよ",
              "maleVariant": "",
              "stringId": "3068503935586013184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チャンスがあったらね",
              "maleVariant": "チャンスがあったらな",
              "stringId": "3068504296010944516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "断ると言ったら？",
              "maleVariant": "",
              "stringId": "3068504396774903808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうウイルス？",
              "maleVariant": "どういうウイルスだ？",
              "stringId": "3068504432627814400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった… そのチャンスがあったらね",
              "maleVariant": "わかった… そのチャンスがあったらな",
              "stringId": "3068504575687135232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もし丁重にお断りすると言ったら？",
              "maleVariant": "",
              "stringId": "3068504653432754176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仮に引き受けたとして… ウイルスは何をするの？ その効果は？",
              "maleVariant": "仮に引き受けたとして… ウイルスは何をするんだ？ その効果は？",
              "stringId": "3068504723964170240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よかった",
              "maleVariant": "",
              "stringId": "3068505507560820736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ終わってないでしょ。私の頼みも聞いてもらう約束よ",
              "maleVariant": "",
              "stringId": "3068507724737024000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたがマックス・タックの輸送隊を襲う気なのはわかってる。私の依頼は、それに直接関係することなの",
              "maleVariant": "",
              "stringId": "3068515647810834432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バカにしないで。ヘタな芝居を打っても時間とエネルギーの無駄よ",
              "maleVariant": "",
              "stringId": "3068518230579351552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうは見えないけど",
              "maleVariant": "",
              "stringId": "3068521170736123904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで結構",
              "maleVariant": "",
              "stringId": "3068521512588677120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vも大したことないのね",
              "maleVariant": "",
              "stringId": "3068521710677266432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "がっかりさせちゃったかしら",
              "maleVariant": "がっかりさせちまったか",
              "stringId": "3068521870899679232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうぞ。でもこれで終わりじゃないから",
              "maleVariant": "",
              "stringId": "3068522306083885056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話さなくていい。ただウィンウィンの取引をしたいの",
              "maleVariant": "",
              "stringId": "3068523996136738816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "例の車列は、マックス・タックの精鋭部隊に護衛されてる。ナイトシティのサイバーサイコ案件じゃお目にかかれないようなエリートたちよ",
              "maleVariant": "",
              "stringId": "3068524618084913152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中が持ってるアクセス権を利用したい。私のクイックハックを隊員の1人に仕掛けて。そうすれば勝手にバックドアを設置してくれる",
              "maleVariant": "",
              "stringId": "3068525497848565760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "終わったらまた連絡する",
              "maleVariant": "",
              "stringId": "3068528948804456448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずウイルスのチップがポケットに戻る",
              "maleVariant": "",
              "stringId": "3068529695944216576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ、別の誰かに頼む時間があるといいけど？",
              "maleVariant": "",
              "stringId": "3068530048467079168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "降参よ。チップを渡して",
              "maleVariant": "降参だ。チップを渡してくれ",
              "stringId": "3068530396728528896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リンクを張る。ほんの一瞬だけど、ちょっとしたサプライズを仕込むのにはそれで十分",
              "maleVariant": "",
              "stringId": "3068531642638143488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼んだデータは？",
              "maleVariant": "",
              "stringId": "3070739558049181696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心配ない。データは？",
              "maleVariant": "",
              "stringId": "3070740331025854464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼んだものは、手に入った？",
              "maleVariant": "頼んだものは、手に入ったか？",
              "stringId": "3070741102761013248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふむ、物騒な話になってきた。何を企んでるか知らないけど",
              "maleVariant": "ふむ、物騒な話になってきた。何を企んでるか知らないが",
              "stringId": "3070826044111122432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そしてもう片方は1秒もしないうちにただの板になる。記念に持っててもいいわよ",
              "maleVariant": "",
              "stringId": "3070840612388532224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰の指示でマックス・タックを嗅ぎまわってる？",
              "maleVariant": "",
              "stringId": "3070843286089560064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これだけだけど、どう？ やれそう？",
              "maleVariant": "",
              "stringId": "3070848345879801856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "状況が見えてきた。あんたにとっても得がなければ、データを盗んでくれるわけないものね",
              "maleVariant": "状況が見えてきた。あんたにとっても得がなければ、データを盗んでくれるわけないからな",
              "stringId": "3070848898605182976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正確には木馬を渡したギリシア人のほうだけど。心配しないで、面倒は起きない。誰にも気づかれないから",
              "maleVariant": "",
              "stringId": "3070851248153620480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしにトロイの木馬になれってのね？ なるほど",
              "maleVariant": "俺にトロイの木馬になれってんだな？ なるほど",
              "stringId": "3070852047470522368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "片手で借りを返しながら、もう一方でコーポのハシゴを昇ろうとしてる… 誰の指示？",
              "maleVariant": "片手で借りを返しながら、もう一方でコーポのハシゴを昇ろうとしてる… 誰の指示だ？",
              "stringId": "3070855518475186176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "回りくどい表現は苦手なの",
              "maleVariant": "",
              "stringId": "3070857232301355008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マックス・タックの内部事情を掴めば… 街でとんでもない影響力を手に入れ、ナイト・コープにも超特大の釣り針を仕掛けられる。誰なの、サンドラ",
              "maleVariant": "マックス・タックの内部事情を掴めば… 街でとんでもない影響力を手に入れ、ナイト・コープにも超特大の釣り針を仕掛けられる。誰なんだ、サンドラ",
              "stringId": "3070858176204304384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ノーコメント",
              "maleVariant": "",
              "stringId": "3070859634916126720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手を貸さないならここまでよ",
              "maleVariant": "",
              "stringId": "3070859829011738624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちの案件は急を要するの",
              "maleVariant": "こっちの案件は急を要する",
              "stringId": "3070862445770895360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "欲しがってたデータを見ればわかる",
              "maleVariant": "",
              "stringId": "3070866117917601792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたが単に壮大な計画に巻き込まれただけで、裏には黒幕がいるってなら話は別だけど",
              "maleVariant": "",
              "stringId": "3070866844421386240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今度はこっちが貸しを作った。必ず返してもらうから",
              "maleVariant": "",
              "stringId": "3070873334452514816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを取り立ち上がる] 狙われるのは慣れてる",
              "maleVariant": "",
              "stringId": "3115644715712339972"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを受け取る] 狙われるのは慣れてる",
              "maleVariant": "",
              "stringId": "3115645759053848576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを取り立ち上がる] 狙われるのは慣れてる",
              "maleVariant": "",
              "stringId": "3115872673299390464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを受け取る] 狙われるのは慣れてる",
              "maleVariant": "",
              "stringId": "3115872673400053760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "詳細を話すつもりはない",
              "maleVariant": "",
              "stringId": "3283861882795847680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "好奇心旺盛なのは結構だけど、引き際は見極めたほうがいい",
              "maleVariant": "好奇心旺盛なのは結構だが、引き際は見極めたほうがいい",
              "stringId": "3283863212222443520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何が言いたいの？",
              "maleVariant": "",
              "stringId": "3283878557234622464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの推測どおりだとして、データを渡すのがどんな相手か知らないでしょ。下手に首を突っ込まないほうがいい。ホントよ",
              "maleVariant": "あんたの推測どおりだとして、データを渡すのがどんな相手か知らないだろ。下手に首を突っ込まないほうがいい。ホントだ",
              "stringId": "3283878965071966208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それ脅してるつもり？",
              "maleVariant": "",
              "stringId": "3283881245598937088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしが組んでる相手を調べようとして何も出なかった。そうでしょ？ これ以上深入りしないで。あんたのために言ってるの",
              "maleVariant": "俺が組んでる相手を調べようとして何も出なかった。そうだろ？ これ以上深入りするな。あんたのために言ってる",
              "stringId": "3283881976649347072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかったわ。なら話はここまで。さよなら",
              "maleVariant": "",
              "stringId": "3283888862404669440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まず言っておくけど… 今回会ったことは口外しないで",
              "maleVariant": "",
              "stringId": "3357878999056257024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心配しないで",
              "maleVariant": "心配するな",
              "stringId": "3357888921017913344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫。こっちも秘密を守るのが仕事よ",
              "maleVariant": "大丈夫だ。こっちも秘密を守るのが仕事でね",
              "stringId": "3357890378119122944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だといいけど",
              "maleVariant": "",
              "stringId": "3357890718075850752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのデータを持ってるだけで死ぬほど危険だってのはわかってるよな？",
              "maleVariant": "",
              "stringId": "3357894258085203968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "狙われるのは慣れてる",
              "maleVariant": "",
              "stringId": "3357917646292185088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かにな。よし、我らがスーパースパイのリードに見せて、そいつの活用法を探ってもらうとするか",
              "maleVariant": "",
              "stringId": "3357917895534505984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時間がないんじゃないの",
              "maleVariant": "",
              "stringId": "3359398815495753728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？",
              "maleVariant": "",
              "stringId": "3359400377655889920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[待つ]",
              "maleVariant": "",
              "stringId": "3374112281271857152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待つのって一番辛いかも…",
              "maleVariant": "待つのって一番辛いかもな…",
              "stringId": "3376894007521964032"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}