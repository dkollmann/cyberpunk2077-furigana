    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、座ってよ",
              "maleVariant": "",
              "stringId": "1751653949060923392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] おはよう",
              "maleVariant": "",
              "stringId": "1751654786579546116"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、昨日のことだけど… あの意味ってさ…、その…",
              "maleVariant": "",
              "stringId": "1751657975072669696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい関係が始まりそう",
              "maleVariant": "",
              "stringId": "1751658319592800260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "単なる気晴らしでしょ",
              "maleVariant": "",
              "stringId": "1751658742043099136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "てっきり、そっちも同じつもりだと思ってたけど…",
              "maleVariant": "",
              "stringId": "1751659209204678656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その… これで… いい関係になればいいなって思ってた",
              "maleVariant": "",
              "stringId": "1751659374745468928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうこと？",
              "maleVariant": "",
              "stringId": "1751663244695552000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ガス抜きってやつか。うん、そうよね",
              "maleVariant": "",
              "stringId": "1751663978883301376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジュディが嫌じゃなかったらだけど",
              "maleVariant": "",
              "stringId": "1751669740279939072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私ね… 実は昨日、ある決心をしたの…",
              "maleVariant": "",
              "stringId": "1751670915859464192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたを呼び出したのは、お別れを言うため",
              "maleVariant": "",
              "stringId": "1751671777755381760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうこと？",
              "maleVariant": "どういうことだ？",
              "stringId": "1751673126509330436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティ暮らしが向いてない人もいる",
              "maleVariant": "",
              "stringId": "1751675690588028928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジュディは不安定で怒りっぽくて、ずっと振り回されっぱなしだった",
              "maleVariant": "",
              "stringId": "1751676894034509824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手を出して、V",
              "maleVariant": "",
              "stringId": "1751688305896833024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[手を差し出す]",
              "maleVariant": "",
              "stringId": "1763534162501812228"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして？",
              "maleVariant": "どうしてだ？",
              "stringId": "1763534572738297856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え？ どうしてよ？",
              "maleVariant": "え？ どうしてだよ？",
              "stringId": "1763535795797344256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何て言ったらいいか…",
              "maleVariant": "",
              "stringId": "1763536185364299780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当にいいの？",
              "maleVariant": "本当にいいのかよ？",
              "stringId": "1763536510573854720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんなとんとん拍子で進むなんて…",
              "maleVariant": "",
              "stringId": "1763536567079518208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何て言ったらいいか… こういうのは初めてよ。あたしのことを信頼してくれてるのね",
              "maleVariant": "何て言ったらいいか… こういうのは初めてだ。俺のことを信頼してくれてるんだな",
              "stringId": "1763537070899314688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいから手を出してって言ってるの",
              "maleVariant": "",
              "stringId": "1763538702735228928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はい、おめでとう。必要な時にいつでも私の家を使ってくれていいから",
              "maleVariant": "",
              "stringId": "1763539730272600064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたが望むならね。それか、使いたいときに使うだけでも",
              "maleVariant": "",
              "stringId": "1763540350408835072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいの？ これ結構重大な決断だよ…",
              "maleVariant": "",
              "stringId": "1763541263978577920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんなとんとん拍子で進むなんて…",
              "maleVariant": "",
              "stringId": "1763541469163929600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今から行く？",
              "maleVariant": "",
              "stringId": "1763541804339150848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティは誰もが暮らしやすい場所じゃない。あたしはこの街で育ち、すべてを知ったつもりでいた",
              "maleVariant": "ナイトシティは誰もが暮らしやすい場所じゃない。俺はこの街で育ち、すべてを知ったつもりでいた",
              "stringId": "1765038645287305216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこへ行ったって何も変わらないよ",
              "maleVariant": "どこへ行ったって何も変わらないぞ",
              "stringId": "1765039078659571712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人は一ヶ所に固まって窮屈に生きるべきじゃない",
              "maleVariant": "",
              "stringId": "1765039389658824704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんぐりの背比べっていうけど、どんな街だって大差はない",
              "maleVariant": "どんぐりの背比べっていうが、どんな街だって大差はない",
              "stringId": "1765040187331559424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは、他では絶対にめぐり合えないチャンスがあると思って、ナイトシティまでやって来た",
              "maleVariant": "俺は、他では絶対にめぐり合えないチャンスがあると思って、ナイトシティまでやって来た",
              "stringId": "1765040403556319232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "違ったのね？",
              "maleVariant": "",
              "stringId": "1765041234733154304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かに、表の顔は同じかもしれない… でもしばらく住んでみたら、本当の姿が見えてくると思うんだ",
              "maleVariant": "",
              "stringId": "1765043703282384896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかるよ…",
              "maleVariant": "",
              "stringId": "1765043852817711104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "覚えてないの？ それともあたしに言わせたいだけ？",
              "maleVariant": "",
              "stringId": "1799689038386126848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何が言いたいかわかるでしょ。あたしのこと、どう思ってるの？",
              "maleVariant": "",
              "stringId": "1799690050371018752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "単なる気晴らしでしょ。リラックスして、のんびり夜を過ごせればいいと思ってた…",
              "maleVariant": "",
              "stringId": "1799698894799200256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私、帰る",
              "maleVariant": "",
              "stringId": "1799699640026357760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vってほんとに鈍いんだから",
              "maleVariant": "",
              "stringId": "1799719924435476480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そろそろ… ナイトシティから離れようと思って",
              "maleVariant": "",
              "stringId": "1799725237377130496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私もよ、V",
              "maleVariant": "",
              "stringId": "1799747156356849664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしだって、荷物をまとめて出ていきたいよ…",
              "maleVariant": "俺だって、荷物をまとめて出ていきたい…",
              "stringId": "1799755172879753216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも… 今はまだその時じゃない",
              "maleVariant": "",
              "stringId": "1799755534428758016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業のせいで、何もかも似たり寄ったり",
              "maleVariant": "企業のせいで、何もかも似たり寄ったりだ",
              "stringId": "1799760435238100992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはまだ、ここを離れるわけにはいかない",
              "maleVariant": "俺はまだ、ここを離れるわけにはいかない",
              "stringId": "1799762395437686784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それが今や、底なし沼でもがいているみたい。誰にでもできる生き方じゃない",
              "maleVariant": "それが今や、底なし沼でもがいているみたいだ。誰にでもできる生き方じゃない",
              "stringId": "1799765144753627136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はまだ、ナイトシティを離れられない。でも… 未来はわからない",
              "maleVariant": "",
              "stringId": "1799767162566172672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなただけ先に行っててもいいよ。私はもう何日か、ここにいるつもり",
              "maleVariant": "",
              "stringId": "1799810038117392384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたのことが好きなの。もっとたくさん一緒に居たい。私にとっては、それがすべて",
              "maleVariant": "",
              "stringId": "1799811422556479488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気が合うってことよ",
              "maleVariant": "",
              "stringId": "1799813255802871808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レズビアンは2回目のデートに引っ越しトラックを持ってくるってジョーク知ってる？",
              "maleVariant": "",
              "stringId": "1799813395892625408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハハ、そんなの、古すぎて誰も知らないって",
              "maleVariant": "",
              "stringId": "1799814666280198144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って",
              "maleVariant": "",
              "stringId": "1837495541293604868"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って… 言い方がよくなかった",
              "maleVariant": "",
              "stringId": "1837496417886363648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあね",
              "maleVariant": "",
              "stringId": "1837496711454089216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあね、ジュディ",
              "maleVariant": "",
              "stringId": "1837497600344547328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うん。楽しい人生を送ってね",
              "maleVariant": "",
              "stringId": "1837498515004809216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもそれが本心なんでしょ。取り消せるとは思わないで",
              "maleVariant": "",
              "stringId": "1837502173259980800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これ以上話しても意味ないでしょ？ じゃあね、V",
              "maleVariant": "",
              "stringId": "1837504886521683968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジュディ… ごめん。そっちも同じだと思い込んでて",
              "maleVariant": "",
              "stringId": "1837507113915211776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ううん、私たちは別のことを考えてた。行き違いはよくある",
              "maleVariant": "",
              "stringId": "1837509961579192320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って。引っ越せってこと？",
              "maleVariant": "待てよ。引っ越せってことか？",
              "stringId": "1846011584026730496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おはよう",
              "maleVariant": "",
              "stringId": "1856223289511993344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当にいいの？ 知り合って日も浅いのに…",
              "maleVariant": "本当にいいのか？ 知り合って日も浅いのに…",
              "stringId": "1869373333740150784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一生付き合える、いい友達になれそうだから。これからも、仲良くしましょ",
              "maleVariant": "",
              "stringId": "1869377518497914880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、元気でね",
              "maleVariant": "",
              "stringId": "1869386072193720324"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当にこれでお終い？ 会うのはこれっきりなの？ そんなに急に出て行かなくてもいいでしょ？",
              "maleVariant": "本当にこれでお終いか？ 会うのはこれっきりなのかよ？ そんなに急に出て行かなくてもいいだろ？",
              "stringId": "1869386075045847040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "遅らせる理由もないから。もう何日かここに残って… そしたら出発する",
              "maleVariant": "",
              "stringId": "1869390033227309056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたと出会えて良かった",
              "maleVariant": "",
              "stringId": "1869391729823277056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあね… たまにはホロコールしてよ",
              "maleVariant": "",
              "stringId": "1869395868645023744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "1894067942620987392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] じゃあね",
              "maleVariant": "[立ち上がる] じゃあな",
              "stringId": "1895239887110602752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあね、ジュディ",
              "maleVariant": "じゃあな、ジュディ",
              "stringId": "1895239889526521856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お待ちかねのコーヒーもどうぞ",
              "maleVariant": "",
              "stringId": "1915739148995223552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう振り回されないと思うと、寂しくなる",
              "maleVariant": "",
              "stringId": "1917219553321639936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "寂しくなるね",
              "maleVariant": "寂しくなるな",
              "stringId": "1917262112001581056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おかげで計画が台無しね",
              "maleVariant": "",
              "stringId": "1918361810695315456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当はナイトシティを出てくつもりで、荷造りまで始めてたのに",
              "maleVariant": "",
              "stringId": "1918376736511975424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜ街を出ようと思ったの？",
              "maleVariant": "",
              "stringId": "1918384194991120388"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜ街を出ようと？",
              "maleVariant": "",
              "stringId": "1918384482703597568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃ、何かしら埋め合わせをしないとね？",
              "maleVariant": "",
              "stringId": "1918384511778512896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの人生にジュディは突然現れたから、きっと別れも同じくらい突然になるだろうなとは思ってたよ",
              "maleVariant": "俺の人生にジュディは突然現れたから、きっと別れも同じくらい突然になるだろうなとは思ってたよ",
              "stringId": "1918389346535956480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "埋め合わせはするよ",
              "maleVariant": "",
              "stringId": "1918396618234355712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今は… 残りたいと思ってる",
              "maleVariant": "",
              "stringId": "1918404726344081408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "色んなことがあったでしょ… しんどくなっちゃって",
              "maleVariant": "",
              "stringId": "1918406390945898496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それがどう？ あなたのおかげで、今はナイトシティに残りたいって思ってる",
              "maleVariant": "",
              "stringId": "1918410556913180672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ… ぜひそうしてちょうだい",
              "maleVariant": "",
              "stringId": "1918415779878625280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジュディ、どういうこと？ 今すぐ街を出るって言うの？",
              "maleVariant": "ジュディ、どういうことだ？ 今すぐ街を出るって言うのか？",
              "stringId": "1924289750914260992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "以前ドールに言われたのよ… あたしは戦わなきゃいけないって。その時は「何のために？」って思ったのを覚えてる",
              "maleVariant": "以前ドールに言われたんだ… 俺は戦わなきゃいけないって。その時は「何のために？」って思ったのを覚えてる",
              "stringId": "1924290877487538176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別に絶縁するってわけじゃないんだからさ",
              "maleVariant": "",
              "stringId": "1924291545237512192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その答えがようやくわかった… 友情や、信頼のためだって。なのにいなくなるなんて",
              "maleVariant": "",
              "stringId": "1924291792433012736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[服を取る]",
              "maleVariant": "",
              "stringId": "2240452560525975556"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[服を取る]",
