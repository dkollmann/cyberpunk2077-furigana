    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V… お疲れ",
              "maleVariant": "",
              "stringId": "3357905005481840640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "声かけてくれてどうも。会えて嬉しい",
              "maleVariant": "",
              "stringId": "3359398816615780352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] 来ないと思った？",
              "maleVariant": "[座る] 来ないと思ったか？",
              "stringId": "3373631725765849088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] よく1人で飲むの？",
              "maleVariant": "[座る] よく1人で飲むのか？",
              "stringId": "3373631956049915904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] 会えて嬉しい",
              "maleVariant": "",
              "stringId": "3373636618421465088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連絡したのは… 話ができる相手が欲しかったから。“組織”の外の人間でね。でも誰でもいいわけじゃない",
              "maleVariant": "",
              "stringId": "3377779797144039424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんな話がしたいの？",
              "maleVariant": "どんな話がしたいんだ？",
              "stringId": "3377784686259662852"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "作戦前で緊張してるの？",
              "maleVariant": "作戦前で緊張してるのか？",
              "stringId": "3377784925469208576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] 誘われて驚いた",
              "maleVariant": "",
              "stringId": "3377786843574759424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。どんな話がしたい？",
              "maleVariant": "",
              "stringId": "3377788797063458816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別に話題のリストを用意してるわけじゃないのよ",
              "maleVariant": "",
              "stringId": "3377789636259803136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私はただ… 少しの間、普通の人間になりたいだけ。自分自身に。ただの… アレックスに",
              "maleVariant": "",
              "stringId": "3377794377601708032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういうことはよくあるの？",
              "maleVariant": "そういうことはよくあるのか？",
              "stringId": "3377796915491172356"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "引退を望むわけがわかった",
              "maleVariant": "",
              "stringId": "3377798404150325248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしも同じ気持ちよ",
              "maleVariant": "俺も同じ気持ちだよ",
              "stringId": "3377798704277942272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昔の自分に戻りたいって感じることは、よくあるの？",
              "maleVariant": "昔の自分に戻りたいって感じることは、よくあるのか？",
              "stringId": "3377800513381920768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最近は… ずっとかな。でもかつての私はもういない。今はまったくの別人になってる。いくつもの仮面の下でね",
              "maleVariant": "",
              "stringId": "3377801704262266880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "緊張で一人は嫌だったとか？",
              "maleVariant": "",
              "stringId": "3377880310837604352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よく自分に言い聞かせてた。「アレックス。あんたは緊張したりなんかしない」って",
              "maleVariant": "",
              "stringId": "3377881343659798528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "任務を引き受ける条件で引退を望んだのは、そういうわけね",
              "maleVariant": "任務を引き受ける条件で引退を望んだのは、そういうわけか",
              "stringId": "3377889217458925568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "燃え尽きたってやつかな。あるいは、ただ普通になりたいのかも",
              "maleVariant": "",
              "stringId": "3377891400929701888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "来ないと思った？",
              "maleVariant": "来ないと思ったか？",
              "stringId": "3378057545867522048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕事柄、あらゆる可能性を考慮してる",
              "maleVariant": "",
              "stringId": "3378058164107931648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誘われるなんて驚いた",
              "maleVariant": "",
              "stringId": "3378058910459162624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "驚いた？ 本当に？",
              "maleVariant": "",
              "stringId": "3378059239175155712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どう受け取っていいかわからなくて。でも、たまには一休みしないと",
              "maleVariant": "どう受け取っていいかわからなくて。だが、たまには一休みしないと",
              "stringId": "3378059586413195264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "普段からよく1人で飲むの？",
              "maleVariant": "普段からよく1人で飲むのか？",
              "stringId": "3378061535455932416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ま、「よく」ってことはないかな",
              "maleVariant": "",
              "stringId": "3378061990403698688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私もだよ",
              "maleVariant": "",
              "stringId": "3378066621838061568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それがどんなものかも曖昧だけど…",
              "maleVariant": "",
              "stringId": "3405299050420039680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ… 事の成り行きが違ったら、いま自分はどうなってたろうって考えたことある？",
              "maleVariant": "",
              "stringId": "3405299821786099712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっと今もバッカー・ファミリーと一緒にいる",
              "maleVariant": "",
              "stringId": "3405301330678575108"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカで成り上がってた",
              "maleVariant": "",
              "stringId": "3405309166611525632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘイウッドの大物になってた",
              "maleVariant": "",
              "stringId": "3405310302412271616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ道を探し続けていた",
              "maleVariant": "",
              "stringId": "3405314263831736320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "結局今と変わりなかった",
              "maleVariant": "",
              "stringId": "3405314729584029696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティを離れてた",
              "maleVariant": "",
              "stringId": "3405314983020654592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ファミリーを離れずに旅を続けてたかもね",
              "maleVariant": "ファミリーを離れずに旅を続けてたかもな",
              "stringId": "3405316739695833088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティのアラサカ防諜部でトップになってたかな。元上司は今ごろ死んでて、あいつのデスクに座ってる",
              "maleVariant": "",
              "stringId": "3405316825276411904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっとストリートを仕切ってた。少なくともヘイウッドは確実にね",
              "maleVariant": "きっとストリートを仕切ってた。少なくともヘイウッドは確実にな",
              "stringId": "3405316934932295680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "んー、どうだろ、まだ道を探し続けてたかも。来る日も来る日も、懲りずに間違いを犯しながらね",
              "maleVariant": "んー、どうだろ、まだ道を探し続けてたかも。来る日も来る日も、懲りずに間違いを犯しながらな",
              "stringId": "3405317079166021632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "たどる道は違ったとしても… 結局は同じ場所に行き着いたんじゃないかって思う",
              "maleVariant": "",
              "stringId": "3405317178738798592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティを離れてたかな。できるだけ早く、遠くへね",
              "maleVariant": "ナイトシティを離れてたかな。できるだけ早く、遠くへ",
              "stringId": "3405317285358006272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ノーマッドの？",
              "maleVariant": "",
              "stringId": "3405318290447462400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。バッカー・ファミリーよ。今ごろ中西部で、トウモロコシの収穫を手伝って日銭を稼いでたに違いない",
              "maleVariant": "ああ。バッカー・ファミリーだ。今ごろ中西部で、トウモロコシの収穫を手伝って日銭を稼いでたに違いない",
              "stringId": "3405319063071813632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ビールを飲みながら、仲間と「そろそろスネーク族に加わるべきじゃないか？」とか延々話してるの… 大空の下でね…",
              "maleVariant": "ビールを飲みながら、仲間と「そろそろスネーク族に加わるべきじゃないか？」とか延々話してるんだ… 大空の下でな…",
              "stringId": "3405322814826409984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "惨めに聞こえるけど",
              "maleVariant": "",
              "stringId": "3405325702235615232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあね",
              "maleVariant": "まあな",
              "stringId": "3405326176846278656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コーポ・プラザを見下ろす角部屋のオフィスで、すべてを支配する感慨に浸る。どんなことでも思いのままだって",
              "maleVariant": "",
              "stringId": "3405327723890143232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなの、パドレが黙ってないんじゃない？",
              "maleVariant": "",
              "stringId": "3405331698295173120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知り合いなの？",
              "maleVariant": "知り合いなのか？",
              "stringId": "3405334874171830272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ… こっちは知ってる。でも向こうはどうかしらね",
              "maleVariant": "",
              "stringId": "3405335022868295680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "トラブルには事欠かないもんね",
              "maleVariant": "",
              "stringId": "3405338197973200896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ",
              "maleVariant": "ああ",
              "stringId": "3405338401849929728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも、命の危険はなかったかも",
              "maleVariant": "だが、命の危険はなかったかも",
              "stringId": "3405338712849182720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "運命を信じるの？",
              "maleVariant": "",
              "stringId": "3405341983600996352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "運の悪さ、をね",
              "maleVariant": "運の悪さ、をな",
              "stringId": "3405342078526484480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今よりずっと悪い状況になってた可能性だってある",
              "maleVariant": "",
              "stringId": "3405342381858549760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かに… 想像はできないけど、そうかもね",
              "maleVariant": "確かに… 想像はできないけど、そうかもな",
              "stringId": "3405342834407174144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この街にどんな仕打ちを受けたわけ？",
              "maleVariant": "",
              "stringId": "3405343990994890752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪いことは全部この街のせい",
              "maleVariant": "悪いことは全部この街のせいだ",
              "stringId": "3405344670170148864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいこともでしょ",
              "maleVariant": "",
              "stringId": "3405344881848283136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よくこの街でポジティブでいれるね",
              "maleVariant": "よくこの街でポジティブでいれるな",
              "stringId": "3405345047154192384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そっちの番よ",
              "maleVariant": "そっちの番だ",
              "stringId": "3405349869731377156"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む] 乾杯",
              "maleVariant": "",
              "stringId": "3405350071745835008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そっちの番よ",
              "maleVariant": "そっちの番だ",
              "stringId": "3405353294045048832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしエージェントじゃなかったら、何をしてた？",
              "maleVariant": "",
              "stringId": "3405354412380712960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俳優",
              "maleVariant": "",
              "stringId": "3405355197353095168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ポスターに載るのをずっと夢見てた。ミュージカル映画が大好きなの",
              "maleVariant": "",
              "stringId": "3405355594738233344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え、ホントに？",
              "maleVariant": "",
              "stringId": "3405355955381268480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言い方が古いか。ま、BDスターってとこかな",
              "maleVariant": "",
              "stringId": "3405356147966930944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "乾杯",
              "maleVariant": "",
              "stringId": "3405358564892667904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "先にやってるよ。少し落ち着きたくてさ",
              "maleVariant": "",
              "stringId": "3560978477853360128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だけど… 最近は、もっと自分に正直になろうかなと思って",
              "maleVariant": "",
              "stringId": "3560988004912906240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかるよ。あたしもナイトシティの傭兵って皮を脱ぎ捨てて、ただの自分になりたい",
              "maleVariant": "わかるよ。俺もナイトシティの傭兵って皮を脱ぎ捨てて、ただの自分になりたい",
              "stringId": "3561056214647488512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ていうか… 実際、あんたって何者なの？",
              "maleVariant": "",
              "stringId": "3561059496371601408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい質問ね。このところ色んなことがあったから… なおのこと答えるのが難しいかも。ただ、あたしはまだ何者でもない",
              "maleVariant": "いい質問だな。このところ色んなことがあったから… なおのこと答えるのが難しいよ。ただ、俺はまだ何者でもない",
              "stringId": "3561059767105536000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それか、CHOOH2臭いぼろモーテルでね",
              "maleVariant": "",
              "stringId": "3561061942959456256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フッ、確かに",
              "maleVariant": "",
              "stringId": "3561062064107732992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "即答じゃない",
              "maleVariant": "即答じゃないか",
              "stringId": "3561066964111769600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてミュージカル？",
              "maleVariant": "",
              "stringId": "3561070285631438848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "演技の経験は？",
              "maleVariant": "",
              "stringId": "3561070497745780736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミュージカルに思い入れが？",
              "maleVariant": "",
              "stringId": "3561074245222187008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "歌の中の世界は現実より美しいから。目も眩むほどにね",
              "maleVariant": "",
              "stringId": "3561074586336542720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それにダンスが好きなの",
              "maleVariant": "",
              "stringId": "3561075574816555008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "演技の経験はあるの？ もちろん、エージェントの任務以外で",
              "maleVariant": "演技の経験はあるのか？ もちろん、エージェントの任務以外で",
              "stringId": "3561076746554097664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いえ… 観客として見たことがあるだけ。でもライトの下に立つことに憧れてる",
              "maleVariant": "",
              "stringId": "3561076924107374592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実際、向いてると思うよ。誰かになりきるプロなんだから",
              "maleVariant": "実際、向いてると思うぞ。誰かになりきるプロなんだから",
              "stringId": "3561077356137463808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分でない誰かになりきるのは得意だものね",
              "maleVariant": "自分でない誰かになりきるのは得意だもんな",
              "stringId": "3561078596443488256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私もそう思う。演技っていうのは素晴らしい技能よ。見る人の中に思いもしなかった感情を呼び起こして、退屈な日常や、辛い出来事を忘れさせてくれる",
              "maleVariant": "",
              "stringId": "3561086722320285696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エージェントとしての演技には… そういう尊さがない",
              "maleVariant": "",
              "stringId": "3561087177754591232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[乾杯] あんたの未来のキャリアに",
              "maleVariant": "",
              "stringId": "3561089555522306052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[乾杯] 普通の人生に",
              "maleVariant": "",
              "stringId": "3561089863065452544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[乾杯] あたしたちの夢に",
              "maleVariant": "[乾杯] 俺たちの夢に",
              "stringId": "3561090049611317248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "未来に乾杯。きっと実現するよ",
              "maleVariant": "",
              "stringId": "3561090469192712192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "乾杯。普通の人生が訪れますように",
              "maleVariant": "",
              "stringId": "3561090479628140544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしたちの夢に。おかげで正気を保てる",
              "maleVariant": "俺たちの夢に。おかげで正気を保てる",
              "stringId": "3561090489879019520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう。嬉しいよ",
              "maleVariant": "",
              "stringId": "3561090740379631616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アーメン",
              "maleVariant": "",
              "stringId": "3561091356204122112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まったくね",
              "maleVariant": "",
              "stringId": "3561091913157361664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あ、私の好きな曲だ",
              "maleVariant": "",
              "stringId": "3561092109903773696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲んで音楽を聴く]",
              "maleVariant": "",
              "stringId": "3561094189053820928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アレックス… ありがとう。最近はこういう素朴な楽しさを忘れてた",
              "maleVariant": "",
              "stringId": "3561094945202946048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいのよ",
              "maleVariant": "",
              "stringId": "3561095373944700928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立つ] あんたならいつかスターになれるよ",
              "maleVariant": "",
              "stringId": "3561096264932311044"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立つ] きっと上手くいく",
              "maleVariant": "",
              "stringId": "3561096913371709440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫。きっと明日は上手くいく",
              "maleVariant": "大丈夫だ。きっと明日は上手くいく",
              "stringId": "3561099391836274688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アレックスならすぐにBDスターになれる。きっとね",
              "maleVariant": "アレックスならすぐにBDスターになれる。きっとな",
              "stringId": "3561099402506584064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってる",
              "maleVariant": "",
              "stringId": "3561099703741497344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずは、リタイアしないと",
              "maleVariant": "",
              "stringId": "3561100026065371136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあまたね",
              "maleVariant": "",
              "stringId": "3561100392362328064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ステージでみんなが注目してくれるの。でもそれは幻だって、誰もがわかってる",
              "maleVariant": "",
              "stringId": "3561971700700725248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "作戦がうまくいくことを祈ってる",
              "maleVariant": "",
              "stringId": "3561972357579063296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今回の作戦がうまくいって、夢がかなうことを祈ってる",
              "maleVariant": "",
              "stringId": "3561973064923267072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私もよ。祈れる限り祈っておきましょ",
              "maleVariant": "",
              "stringId": "3561973408805863424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "絵空事ね",
              "maleVariant": "絵空事だな",
              "stringId": "3561973700813307904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "夢を見るのはいいけど… あたしは元の暮らしには戻れないし、あんたは映画スターじゃない",
              "maleVariant": "夢を見るのはいいが… 俺は元の暮らしには戻れないし、あんたは映画スターじゃない",
              "stringId": "3561974147624124416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "現実を見て生きないと",
              "maleVariant": "",
              "stringId": "3561975617744453632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "任務を忘れて、別の誰かに成り済ますことからわずかでも解放されたかったの。伝わるといいけど…",
              "maleVariant": "",
              "stringId": "3568212774553305088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "演じてみたい役はある？",
              "maleVariant": "演じてみたい役はあるか？",
              "stringId": "3569241438011179008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "演じたい役は？",
              "maleVariant": "",
              "stringId": "3569307804483371008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "『サンセット大通り』のノーマ・デズモンド。きっと聞いたことないでしょうね",
              "maleVariant": "",
              "stringId": "3569314834455642112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ないかな",
              "maleVariant": "",
              "stringId": "3569325976003461120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悲劇的なキャラクターに惹かれるの。自分を重ねて見てる部分はある。引き際がわからないところなんかね",
              "maleVariant": "",
              "stringId": "3569326302588747776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ当然ね。大昔の映画だから",
              "maleVariant": "",
              "stringId": "3569328228411817984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "3569385060392157184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立つ] ちゃんと休んだほうがいい",
              "maleVariant": "",
              "stringId": "3569385871973203968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しっかり休んで。忙しくなる",
              "maleVariant": "しっかり休めよ。忙しくなる",
              "stringId": "3569389007567765504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうする",
              "maleVariant": "",
              "stringId": "3569389312913096704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダンスなんて全然",
              "maleVariant": "",
              "stringId": "3601507303385382916"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダンスのこと？ 正直、あんまり経験はないかな",
              "maleVariant": "ダンスのことか？ 正直、あんまり経験はないかな",
              "stringId": "3601507588614832128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こういうの、って？",
              "maleVariant": "",
              "stringId": "3601507611448623104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リズム感は悪くない。その調子よ",
              "maleVariant": "",
              "stringId": "3601507691593383936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こういうのはあんまりやらない？",
              "maleVariant": "",
              "stringId": "3601538927598682112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こういうのって？",
              "maleVariant": "",
              "stringId": "3601540287022940160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダンスのことっていうか… ま、いいわ",
              "maleVariant": "",
              "stringId": "3601570710021128192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "意味のないこと。何も考えずに、ただ楽しいって思える瞬間のことよ",
              "maleVariant": "",
              "stringId": "3601571589197578240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アレックスならスターになれる",
              "maleVariant": "",
              "stringId": "3708469477189373960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少し休んだほうがいい",
              "maleVariant": "",
              "stringId": "3708469487557693448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっと明日は上手くいく",
              "maleVariant": "",
              "stringId": "3708469498714542088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[アレックスに加わる]",
              "maleVariant": "",
              "stringId": "3711840701293875204"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[店を出る] 話せてよかったよ",
              "maleVariant": "",
              "stringId": "3711841798960009216"
            }
          ]
        }
      }
    },
