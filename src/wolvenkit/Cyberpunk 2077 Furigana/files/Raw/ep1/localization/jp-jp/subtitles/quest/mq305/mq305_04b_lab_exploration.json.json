        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "動くな！",
              "maleVariant": "",
              "stringId": "3294326179800694824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着いて！ あたしを雇ったのはそっちの方でしょ",
              "maleVariant": "落ち着け！ 俺を雇ったのはそっちの方だろ",
              "stringId": "3294347608550162432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これでしょ？",
              "maleVariant": "これだろ？",
              "stringId": "3294350456130256896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… わかった、じゃあ行きましょ",
              "maleVariant": "はあ… わかった、じゃあ行くとするか",
              "stringId": "3294350468880941060"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて、うまくいくといいんだけど",
              "maleVariant": "",
              "stringId": "3295583799626842116"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ！ カードが反応しない",
              "maleVariant": "",
              "stringId": "3296832653324075008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[カードを見せる] ええ",
              "maleVariant": "[カードを見せる] ああ",
              "stringId": "3297140067655106564"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[カードを渡す] 気にしすぎじゃない？",
              "maleVariant": "[カードを渡す] 気にしすぎじゃないか？",
              "stringId": "3297144312710184960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "セキュリティがまだ作動してるかもしれないから… 先に行って",
              "maleVariant": "",
              "stringId": "3298196993876836352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "タレットがあるけど、作動してない。大丈夫よ",
              "maleVariant": "タレットがあるが、作動してない。大丈夫だ",
              "stringId": "3298205793929281540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "油断はしないでよ。ここのシステムはよくわかってないから",
              "maleVariant": "",
              "stringId": "3298218353118298116"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう大丈夫よ",
              "maleVariant": "もう大丈夫だ",
              "stringId": "3298237055050072068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "顔認証よ！ スキャンに引っかかった！",
              "maleVariant": "",
              "stringId": "3298389411297378308"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "セキュリティが機能してるってことは、期待してもいいのかしら",
              "maleVariant": "",
              "stringId": "3298391391679954948"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふう… あと少しよ",
              "maleVariant": "",
              "stringId": "3298398502702956544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やった。これがメインフレーム――ホストよ",
              "maleVariant": "",
              "stringId": "3298401087249858560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリーね？ ハンズに言われて来た",
              "maleVariant": "ブリーだな？ ハンズに言われて来た",
              "stringId": "3298489697039400960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着いて。あんたが依頼主でしょ",
              "maleVariant": "落ち着け。あんたが依頼主だろ",
              "stringId": "3298492616191098884"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリーね？ ハンズに言われて来た",
              "maleVariant": "ブリーだな？ ハンズに言われて来た",
              "stringId": "3298493126084247552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私の知る限り、ここには何年も人が来てない。システムも2013年のもの",
              "maleVariant": "",
              "stringId": "3298510637890650112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼むわよ",
              "maleVariant": "",
              "stringId": "3298545690913914880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その… 悪かったわ。でも、誰も信用できなくて",
              "maleVariant": "",
              "stringId": "3298614721339572224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ… そうよ",
              "maleVariant": "",
              "stringId": "3298620221984718848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちよ、行きましょ",
              "maleVariant": "",
              "stringId": "3298628172304728064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って… あなたもしかして… やっぱり、検問所にいた傭兵でしょ",
              "maleVariant": "",
              "stringId": "3307113942631464968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "世間は狭いわね。ハンズの保証がなければ、ストーカーだと思うところよ",
              "maleVariant": "",
              "stringId": "3307440290973990912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫よ、任せて",
              "maleVariant": "大丈夫だ、任せろ",
              "stringId": "3324791102540234756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こういうセキュリティは全部連動してるはず",
              "maleVariant": "こういうセキュリティは全部連動してるはずだ",
              "stringId": "3324791990273064960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この手のタレットはシーケンス回路で制御されてるはず。ひとつをハックすると、他のも起動する",
              "maleVariant": "この手のタレットはシーケンス回路で制御されてるはずだ。ひとつをハックすると、他のも起動する",
              "stringId": "3324792522412802048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "問題ない、任せて",
              "maleVariant": "問題ない、任せろ",
              "stringId": "3324792558500593668"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "約束の時間と違う",
              "maleVariant": "",
              "stringId": "3327368713288171520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ早速カードを。誰かに見られる前に入りましょ",
              "maleVariant": "",
              "stringId": "3327421951085527040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "銃を握ったのは初めて？",
              "maleVariant": "銃を握ったのは初めてか？",
              "stringId": "3327760094398103552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "銃を使うのは初めてなんでしょ？ お願いだから、怪我する前にしまって",
              "maleVariant": "銃を使うのは初めてなんだろ？ 頼むから、怪我する前にしまっておけ",
              "stringId": "3327760457725493248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[カードを渡す] どうぞ。ずいぶんと面白いカードね",
              "maleVariant": "[カードを渡す] どうぞ。ずいぶんと面白いカードだな",
              "stringId": "3327763063193583616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さすがに気にしすぎじゃない？",
              "maleVariant": "さすがに気にしすぎじゃないか？",
              "stringId": "3327766967385964544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら。ミリテク絡みなら、先に言っといてほしかった",
              "maleVariant": "ほら。ミリテク絡みなら、先に言っといてほしかったが",
              "stringId": "3327767108079697920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕方ない",
              "maleVariant": "",
              "stringId": "3327779320483659780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "説明が先よ",
              "maleVariant": "説明が先だ",
              "stringId": "3327779514461831168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "断る。まずはこっちの質問に答えて",
              "maleVariant": "断る。まずはこっちの質問に答えろ",
              "stringId": "3327779998501289984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私は探偵を雇った覚えはないんだけど、まあいいわ。何が知りたいわけ？",
              "maleVariant": "",
              "stringId": "3327780419777183744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カードはどこで手に入れたの？",
              "maleVariant": "カードはどこで手に入れたんだ？",
              "stringId": "3327782421936586752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここには何があるの？",
              "maleVariant": "ここには何があるんだ？",
              "stringId": "3327782421936586772"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずはカード――どこで手に入れたの？",
              "maleVariant": "まずはカードだ――どこで手に入れた？",
              "stringId": "3327782422003695632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミリテクの保管庫よ。埃をかぶらせとくにはもったいなくて",
              "maleVariant": "",
              "stringId": "3327782422003695644"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私は真実を暴きたいの。連中に聞いても答えるわけないしね",
              "maleVariant": "",
              "stringId": "3327782422003695656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、進みましょ",
              "maleVariant": "わかった、進もう",
              "stringId": "3327782424117624832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜそこまでこだわるの？",
              "maleVariant": "なぜそこまでこだわるんだ？",
              "stringId": "3327782424117624852"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここには一体何があるっていうの？",
              "maleVariant": "ここには一体何があるっていうんだ？",
              "stringId": "3327782431449268236"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミリテクから“借りた”ファイルの情報が正しければ、中の研究所はまさに秘密の宝庫よ",
              "maleVariant": "",
              "stringId": "3327782431449268248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私はそれを暴きたい",
              "maleVariant": "",
              "stringId": "3327785271311843328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこまでこだわる理由がわからないんだけど",
              "maleVariant": "そこまでこだわる理由がわからないんだが",
              "stringId": "3327794316647186432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他の企業同様、ミリテクには世間に知られたくない秘密がたくさんある。これもそのうちのひとつ",
              "maleVariant": "",
              "stringId": "3327794493562929152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "先に相手の尻尾を掴んだ方が勝ちってわけ",
              "maleVariant": "",
              "stringId": "3327795345644175360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ネットウォッチも長年ミリテクを調べ続けてる。そして私もね",
              "maleVariant": "",
              "stringId": "3327797534584324096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、早く中の安全を確かめて",
              "maleVariant": "",
              "stringId": "3327799620579475456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずは安全確認。頼んだわよ",
              "maleVariant": "",
              "stringId": "3327801371365851136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと別の入り口を探してみる",
              "maleVariant": "",
              "stringId": "3327805127130202112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあハックせずに進んでちょうだい",
              "maleVariant": "",
              "stringId": "3327810416030105600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お先にどうぞ",
              "maleVariant": "",
              "stringId": "3327813498793213952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、聞いてる？",
              "maleVariant": "おい、聞いてるのか？",
              "stringId": "3327823712359661568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいから急ぐわよ",
              "maleVariant": "",
              "stringId": "3327824036076044288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、いけた！",
              "maleVariant": "",
              "stringId": "3327824939378130944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カードは？",
              "maleVariant": "",
              "stringId": "3328702848074629120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは地雷探知機ってわけね…",
              "maleVariant": "俺は地雷探知機ってわけか…",
              "stringId": "3328712908012109824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞いてる？",
              "maleVariant": "聞いてるのか？",
              "stringId": "3328810471281246208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。でも銃を向けられながら話す気はない。あと、セーフティがかかったままよ",
              "maleVariant": "ああ。だが銃を向けられながら話す気はない。あと、セーフティがかかったままだぞ",
              "stringId": "3336276261853052928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ… お見通しってわけね",
              "maleVariant": "",
              "stringId": "3336282748260302848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その調子よ、頼りになるわ",
              "maleVariant": "",
              "stringId": "3337311560783249408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたみたいなメディアの人間を一人知ってる",
              "maleVariant": "",
              "stringId": "3338009092480417792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたみたいに、コーポの闇を暴こうとしたメディアの人間を知ってる",
              "maleVariant": "",
              "stringId": "3338009452704022528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その人のことは知らないけど、立派な仕事をしてるじゃない",
              "maleVariant": "",
              "stringId": "3338009830459817984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業の退役兵援助プログラムが実は詐欺でね。イメージ戦略ってヤツ。WNSのマックス・ジョーンズ――知ってる？",
              "maleVariant": "企業の退役兵援助プログラムが実は詐欺でな。イメージ戦略ってヤツさ。WNSのマックス・ジョーンズ――知ってるか？",
              "stringId": "3338010930407653376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もうWNSにはいないけど",
              "maleVariant": "もうWNSにはいないがな",
              "stringId": "3338011834380828672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そもそも公にならなかった。その前に命を落としたから",
              "maleVariant": "そもそも公にならなかった。その前に命を落としたからな",
              "stringId": "3338013588086439936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いえ、公開させなかった。してたら、コーポに殺されてたから",
              "maleVariant": "いや、公開させなかった。してたら、コーポに殺されてたからな",
              "stringId": "3338013721649856512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふうん、面白い話ね。おかげさまで元気が出たわ",
              "maleVariant": "",
              "stringId": "3338014330360807424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、中の安全を確認して",
              "maleVariant": "",
              "stringId": "3338015137596555264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "結局、話題にならなかったってこと？",
              "maleVariant": "",
              "stringId": "3338018313288663040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何か見つけた",
              "maleVariant": "",
              "stringId": "3343612978557730820"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "古いテックね… コアかバッテリーみたい。何に使うものなの…？",
              "maleVariant": "古いテックだな… コアかバッテリーのようだが、何に使うものなんだ…？",
              "stringId": "3343615047205240832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふむ… このコンピューターだけ新しいみたいね",
              "maleVariant": "ふむ… このコンピューターだけ新しいようだな",
              "stringId": "3343615170920431616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "数年前にミリテクの調査チームが来たのよ。きっと連中が置きっぱなしにしたんでしょ",
              "maleVariant": "",
              "stringId": "3343617030775488512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はいはい、そういうのは後にしてくれる？",
              "maleVariant": "",
              "stringId": "3343617113772376064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、時間がないのよ",
              "maleVariant": "",
              "stringId": "3343619636948221952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これは… んー… 古い熱交換器？ フム… 引火したら大変そうね",
              "maleVariant": "これは… んー… 古い熱交換器か？ フム… 引火したら大変そうだな",
              "stringId": "3389630896327507968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "近づかないで！",
              "maleVariant": "",
              "stringId": "3446332932230332416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何とかしてよ！",
              "maleVariant": "",
              "stringId": "3549084374287400960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウソでしょ！",
              "maleVariant": "",
              "stringId": "3549085709686685704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やられちゃう！",
              "maleVariant": "",
              "stringId": "3549085858282487812"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、モタモタしないで",
              "maleVariant": "",
              "stringId": "3549151591733542912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[カードを見せる] ええ",
              "maleVariant": "[カードを見せる] ああ",
              "stringId": "3660631057906679816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[近寄る] 銃を握ったのは初めて？",
              "maleVariant": "[近寄る] 銃を握ったのは初めてか？",
