  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう安全だろう… ふう！ おかげで助かった。奴らは人質に容赦ないからな",
              "maleVariant": "",
              "stringId": "2776549988211060736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気にしないで。目当てのものは手に入ったしね",
              "maleVariant": "気にするな。目当てのものは手に入ったしな",
              "stringId": "2776644328627335168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ… この後は一人で大丈夫だ。あんたの素晴らしい活躍はうちの上司に報告しとく",
              "maleVariant": "",
              "stringId": "3272497799097184256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウソをついてたのね",
              "maleVariant": "ウソをついてたのか",
              "stringId": "3275412691563278340"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜ会社を裏切ったの？",
              "maleVariant": "なぜ会社を裏切ったんだ？",
              "stringId": "3275414391917342720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "試作品を届けるのがあたしの仕事よ",
              "maleVariant": "試作品を届けるのが俺の仕事だ",
              "stringId": "3297107171393658884"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。じゃあ気をつけて",
              "maleVariant": "わかった。じゃあ気をつけろよ",
              "stringId": "3297107880180703232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう、好きにすれば。また捕まっても連絡してこないでよ",
              "maleVariant": "そうか、好きにしろ。また捕まっても連絡してくるなよ",
              "stringId": "3349403579983060992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハハ！ 大丈夫、大丈夫。同じ轍は踏まない。少し休んだら行くよ… 改めてありがとう。よい旅を、さよなら！",
              "maleVariant": "",
              "stringId": "3349405382208380928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はいはい。じゃあ気をつけてね、ハサン",
              "maleVariant": "はいはい。じゃあ気をつけろよ、ハサン",
              "stringId": "3349408466951200768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、待ってくれ！ あー… その… ゼータテクには戻れないんだ",
              "maleVariant": "",
              "stringId": "3349425939599581184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうこと？ いい加減なことばっか言って… 本当のことを話して",
              "maleVariant": "どういうことだ？ いい加減なことばっか言いやがって… 本当のことを話せ",
              "stringId": "3349432318414544896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、その… クソ… ゼータテクに戻るつもりはない。試作品をいただいてドッグタウンに雲隠れするつもりだった",
              "maleVariant": "",
              "stringId": "3349434599058956288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの面子はどうなるのよ",
              "maleVariant": "俺の面子はどうなるんだ",
              "stringId": "3349443531903066112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい？ あたしたちは出会わなかった。スカベンジャーは設計図を手に入れたけど、試作品は行方不明。わかった？",
              "maleVariant": "いいか…？ 俺たちは出会わなかった。スカベンジャーは設計図を手に入れたが、試作品は行方不明。わかったか？",
              "stringId": "3349444371200073728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほ、本当にいいのか？ ああ、わかった！ あ… ありがとう！",
              "maleVariant": "",
              "stringId": "3349444371200073756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜそこまでテックにこだわるの？",
              "maleVariant": "なぜそこまでテックにこだわるんだ？",
              "stringId": "3349444517631614992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これにどれほどの価値があるか、わからないのか？",
              "maleVariant": "",
              "stringId": "3349444517648392192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "要はウソをついてたのね。恩を仇で返すつもり？",
              "maleVariant": "要はウソをついてたんだな。恩を仇で返すつもりか？",
              "stringId": "3349445576710152192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはずらかるから、あんたはどこか安全なところに身を隠すのね。ゼータテクに嗅ぎつけられたら、あんたは終わりよ",
              "maleVariant": "俺はずらかるから、あんたはどこか安全なところに身を隠すんだな。ゼータテクに嗅ぎつけられたら、あんたは終わりだぞ",
              "stringId": "3349450466832408576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って… あたしのことはどうでもいいわけ？ フィクサーに試作品は手に入らなかったって謝れば済むとでも？",
              "maleVariant": "待て… 俺のことはどうでもいいのか？ フィクサーに試作品は手に入らなかったって謝れば済むとでも？",
              "stringId": "3349455432368250896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業とお前、両方を躍らせるとはな。2-0でハサンの勝ち。ま、幸運はいつまでも続かないだろうが",
              "maleVariant": "",
              "stringId": "3349470548052664328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分の命よりはあるようね",
              "maleVariant": "自分の命よりはあるようだな",
              "stringId": "3349484207273402368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "くっ… 僕には家族も友人もいない。会社に命を捧げてきたんだ… 何もかも犠牲にして… でも何のために？",
              "maleVariant": "",
              "stringId": "3349484385531322368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "僕は人生そのものを奪われたんだ！ …試作品を売った金で、一からやり直すつもりだったのに…",
              "maleVariant": "",
              "stringId": "3349485799951294464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "買い手がいて… 合図を待ってたんだ… そしたらスカベンジャーに襲われて、すべて台無しだよ！ 今さら会社に戻ったら… 確実に殺される",
              "maleVariant": "",
              "stringId": "3356703237327544320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見つからなかったと言えばいい。信号は切った。フィクサーには設計図を渡せ。十分価値がある！",
              "maleVariant": "",
              "stringId": "3356715269846196224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フィクサーと話す。動かないで",
              "maleVariant": "フィクサーと話す。動くな",
              "stringId": "3356741536054239232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしたちは出会わなかった。いい？",
              "maleVariant": "俺たちは出会わなかった。いいか？",
              "stringId": "3357703025955168256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうするかはあたしが決める。フィクサーと話すからじっとしてて",
              "maleVariant": "どうするかは俺が決める。フィクサーと話すからじっとしてろ",
              "stringId": "3357705428586717192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "僕にどうしろと！ 本当のことを言ったら助けてくれなかっただろ！",
              "maleVariant": "",
              "stringId": "3357711907897204736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ハサンを撃つ] 残念ね",
              "maleVariant": "[ハサンを撃つ] 残念だ",
              "stringId": "3357737341485572096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあね、ハサン… 残念よ",
              "maleVariant": "じゃあな、ハサン… 残念だ",
              "stringId": "3357739091298869260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "襲撃を生き残り、檻から脱出し、企業に一杯食わせた。大したもんだが、運はいつか尽きるもんだ…",
              "maleVariant": "",
              "stringId": "3357757060301185040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分で蒔いた種よ。ブツをいただいて、さっさと帰りましょ",
              "maleVariant": "自分で蒔いた種だ。ブツをいただいて、さっさと帰るぞ",
              "stringId": "3357757060317962260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ… わかった…",
              "maleVariant": "",
              "stringId": "3357766756290519040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "襲撃を生き残り、檻から脱出し、企業に一杯食わせた。大したもんだが、運はいつか尽きるもんだ…",
              "maleVariant": "",
              "stringId": "3357768297982451724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼をゼータテクに引き渡さないで。リパーにテックを回収させたら、逃がしてやってほしい",
              "maleVariant": "彼をゼータテクに引き渡さないでくれ。リパーにテックを回収させたら、逃がしてやってほしい",
              "stringId": "3357777398363840528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあな。では善意のしるしとして手を貸そうじゃないか。持ちつ持たれつ、ということだ",
              "maleVariant": "",
              "stringId": "3357777398363840540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "引き渡したらどうなるかは、嫌というほど知ってるから。企業の手助けをするつもりはない",
              "maleVariant": "引き渡したらどうなるかは、嫌というほど知ってるからな。企業の手助けをするつもりはない",
              "stringId": "3357777398380617728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "インプラントだけ取って、ハサンは逃がしてやって",
              "maleVariant": "インプラントだけ取って、ハサンは逃がしてやってくれ",
              "stringId": "3357777404319752224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンズ？ 面倒なことになった。スカベンジャーに囚われてたゼータテクの男を助けたの",
              "maleVariant": "ハンズか？ 面倒なことになった。スカベンジャーに囚われてたゼータテクの男を助けたんだ",
              "stringId": "3357777404386861076"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど。で、任務の遂行に何の影響が？",
              "maleVariant": "",
              "stringId": "3357777404403638276"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そいつが自分の目に例の試作品を移植したのよ。迎えをよこして",
              "maleVariant": "そいつが自分の目に例の試作品を移植したんだ。迎えをよこしてくれ",
              "stringId": "3357777404403638288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "不都合だが、手に負えないこともない。座標を指定しろ。迎えを寄越し、ゼータテクまで送り届けよう",
              "maleVariant": "",
              "stringId": "3357777404403638300"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハァハァ… Vか？ ハンズの使いだ。設計図を受け取りにきた",
              "maleVariant": "",
              "stringId": "3357777412641251348"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。ほら",
              "maleVariant": "ああ。ほらよ",
              "stringId": "3357777412641251360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… よし。後は任せろ",
              "maleVariant": "",
              "stringId": "3357777412658028548"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しかし… お前にとってそこまでする価値がある男なのか？",
              "maleVariant": "",
              "stringId": "3357888462745231360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいだろう",
              "maleVariant": "",
              "stringId": "3357922373844066312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "かばってくれてありがとう。本当に",
              "maleVariant": "",
              "stringId": "3357960011850440708"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何言ってるの。フィクサーに連絡するから、迎えが来るまで待ってて。試作品の引き渡しは彼が行う",
              "maleVariant": "何言ってるんだ。フィクサーに連絡するから、迎えが来るまで待ってろ。試作品の引き渡しは彼が行う",
              "stringId": "3359691270301671424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪かったよ！ まさかこんなことになるなんて… 頼む、連れ戻されたらおしまいだ！",
              "maleVariant": "",
              "stringId": "3359786132942348288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうかもしれない。でもあたしを騙そうとしたのは大きな間違いよ",
              "maleVariant": "そうかもしれない。だが俺を騙そうとしたのは大きな間違いだ",
              "stringId": "3359820919241433088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "呆れた男ね。その程度のオツムでアラサカ、ゼータテク、スカベンジャー全員にケンカを売るなんて",
              "maleVariant": "呆れた男だ。その程度のオツムでアラサカ、ゼータテク、スカベンジャー全員にケンカを売るなんてな",
              "stringId": "3359821321324191744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なら… いっそ殺せ。かっこつけてるわけじゃない。拷問されるくらいなら、あんたに殺された方がマシだ",
              "maleVariant": "",
              "stringId": "3359833756647022592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無理よ。テックを持ち逃げする前に、見つかって消されるのがオチね",
              "maleVariant": "無理だ。テックを持ち逃げする前に、見つかって消されるのがオチだな",
              "stringId": "3359838737785675776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いま送る",
              "maleVariant": "",
              "stringId": "3359876008874336256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "座標を送る",
              "maleVariant": "",
              "stringId": "3359884556429680640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほう… で？ お友達の手術を手配してくれる親切な後援者の当てはあるのかね？",
              "maleVariant": "",
              "stringId": "3359887538210725888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お願いよ、ハンズ。あんたに恩を売りたい連中はいくらでもいるでしょ",
              "maleVariant": "頼むよ、ハンズ。あんたに恩を売りたい連中はいくらでもいるだろ",
              "stringId": "3359888332309913600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ハンズの使いを待つ]",
              "maleVariant": "",
              "stringId": "3387006098202390532"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "3407166056182820868"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ま、待て… そうだ、取引を――ああああ！",
              "maleVariant": "",
              "stringId": "3440366530109366272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゼータテクを出し抜くつもりだったの？",
              "maleVariant": "ゼータテクを出し抜くつもりだったのか？",
              "stringId": "3440442086821715968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業を出し抜こうとしたってことは、事前に計画を立ててたんでしょ",
              "maleVariant": "企業を出し抜こうとしたってことは、事前に計画を立ててたんだろ",
              "stringId": "3440444859910971412"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、でも1人では無理だった。ドッグタウンのスタジアムで買い手と落ち合うつもりだったんだ。スカベンジャーに拉致されるまではな",
              "maleVariant": "",
              "stringId": "3440444859927748616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "買い手も信用できないでしょ",
              "maleVariant": "買い手も信用できないだろ",
              "stringId": "3440444859944525824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさしく。だから、保険のために試作品を自分に入れたんだ。僕を殺そうとしたら、テックも無事じゃ済まないからな",
              "maleVariant": "",
              "stringId": "3440444859944525848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "検問所で検査される際に、姿を消すつもりだった。車と、警備隊への賄賂まで手配したのに…襲撃さえされなければ…",
              "maleVariant": "",
              "stringId": "3440444859961303052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お願いだ。会社に戻るわけにはいかない…",
              "maleVariant": "",
              "stringId": "3440515363678183424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[設計図を渡す] ほら",
              "maleVariant": "",
              "stringId": "3440526427648155648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて、橋を動かせないようにするか。足止めだ… フンッ！",
              "maleVariant": "",
              "stringId": "3478039294681014272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エンジニアらしい方法ね",
              "maleVariant": "エンジニアらしい方法だな",
              "stringId": "3478042886095757316"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エンジニアなら、もっとマシなやり方があるんじゃないの",
              "maleVariant": "エンジニアなら、もっとマシなやり方があるんじゃないのか",
              "stringId": "3478046535526113280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "方法より結果が大事だ",
              "maleVariant": "",
              "stringId": "3478048155919966208"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}