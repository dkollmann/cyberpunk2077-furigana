        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヘイウッドは荒れてるね",
                    "maleVariant": "ヘイウッドは荒れてるな",
                    "stringId": 1948679986532511748
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "個人的なことを聞いても？",
                    "maleVariant": null,
                    "stringId": 1948680108016332800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヘイウッドはかなり荒れてる。ここで商売してくのは大変そうね",
                    "maleVariant": "ヘイウッドはかなり荒れてる。ここで商売してくのは大変そうだな",
                    "stringId": 1948680119575834624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうして神父になったの？",
                    "maleVariant": "どうして神父になったんだ？",
                    "stringId": 1948680799120191488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ママ・ウェルズに会いに行くことは？",
                    "maleVariant": null,
                    "stringId": 1948680894096011264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "デクスの情報が欲しい",
                    "maleVariant": null,
                    "stringId": 1948680953621573632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "パドレ… ママ・ウェルズの様子を見に行ってる？ ジャッキーが死んで、誰かを必要としてるかも",
                    "maleVariant": "パドレ… ママ・ウェルズの様子を見に行ってるか？ ジャッキーが死んで、誰かを必要としてるかも",
                    "stringId": 1948796032656535552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいが、答えるとは限らないぞ？",
                    "maleVariant": null,
                    "stringId": 1948797157803749376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうして神父になったの？",
                    "maleVariant": "どうして神父になったんだ？",
                    "stringId": 1948797371545481216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "信仰は神のお恵みだ。昔はそれを理解できていなかった。しかし、やがて実感するようになったのだ",
                    "maleVariant": null,
                    "stringId": 1948797525694541824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私は神を信じる。そして私が死ぬ時には、神に信じていてもらいたい",
                    "maleVariant": null,
                    "stringId": 1948797992369582080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "パドレ、昔なじみのあんたに聞きたいことがあるの",
                    "maleVariant": "パドレ、昔なじみのあんたに聞きたいことがあるんだ",
                    "stringId": 1948801617540083712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼女は強い。私がいなくても大丈夫だろう",
                    "maleVariant": null,
                    "stringId": 1948842891185942528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうじゃなくて… ジャッキーは秘密を抱えてた。報復を狙う奴がいるかも",
                    "maleVariant": null,
                    "stringId": 1948842985507450880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "誰にも手は出させない。安心しろ",
                    "maleVariant": null,
                    "stringId": 1948843254798544896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それに、お前こそ会いに行くべきだろう。それが筋だ",
                    "maleVariant": null,
                    "stringId": 1948843353918337024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そう言われると、なかなか断れないな",
                    "maleVariant": null,
                    "stringId": 1948847472557092864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "デクスター・デショーンについて何か知ってる？",
                    "maleVariant": "デクスター・デショーンについて何か知ってるか？",
                    "stringId": 1948847649858711552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "噂が絶えない男だが…",
                    "maleVariant": null,
                    "stringId": 1948847806172033024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これだけは確かだ。デクスは天国に行けない。聖ペトロを買収しないかぎりな",
                    "maleVariant": null,
                    "stringId": 1948847972081922048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オオカミどもに食い殺されない程度には、敬意を払ってもらっている",
                    "maleVariant": null,
                    "stringId": 1948853771495731200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたのシマであんたに逆らう奴なんているの？",
                    "maleVariant": "あんたのシマで、あんたに逆らう奴なんているのか？",
                    "stringId": 1948855801169768448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私を恐れるオオカミは多いだろうな",
                    "maleVariant": null,
                    "stringId": 1948855926696898560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "また後で話しましょ",
                    "maleVariant": "また後で話そう",
                    "stringId": 1956127317200662528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "また後で",
                    "maleVariant": null,
                    "stringId": 1956127435714916352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バヤ・コン・ディオス",
                    "maleVariant": null,
                    "stringId": 1956127846739931136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "戻ったのか",
                    "maleVariant": null,
                    "stringId": 1959175430979121152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヘイウッドに新顔がいると聞いてな。セバスチャン・イバラだ。パドレと呼ばれてる",
                    "maleVariant": null,
                    "stringId": 1959175568350965760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "相変わらず、耳が早いのね",
                    "maleVariant": "相変わらず、耳が早いんだな",
                    "stringId": 1959197086791430144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "当たり前だ。とにかく、何か必要な時は声をかけてくれ",
                    "maleVariant": null,
                    "stringId": 1959197341939331072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "Vよ。よろしく",
                    "maleVariant": "Vだ。よろしく",
                    "stringId": 1959200703489437696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "他人に敬意を払える人間は好きだ。またな、V",
                    "maleVariant": null,
                    "stringId": 1959201131073564672
                  }
                }
              ]
            }
          }
        }
      }
    },
