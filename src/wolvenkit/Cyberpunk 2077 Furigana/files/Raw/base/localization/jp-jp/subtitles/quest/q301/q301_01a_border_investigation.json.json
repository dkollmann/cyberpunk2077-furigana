  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Traces of the campsite.",
                    "maleVariant": null,
                    "stringId": 3092608710846595072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Looks like campsite marks. Anyone tried to live here?",
                    "maleVariant": null,
                    "stringId": 3092609873373446152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]The garage has solid walls and a roof overhead. What more could you want?",
                    "maleVariant": null,
                    "stringId": 3092630589175119892
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Need to get into every corner?",
                    "maleVariant": null,
                    "stringId": 3092948759312220172
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]NUSA forces were purging refugees from Night City here ...",
                    "maleVariant": null,
                    "stringId": 3092955607117479948
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]War only looks logical and clean from the headquarters position.",
                    "maleVariant": null,
                    "stringId": 3092955607117479956
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Uh ... Luckily it has been lying here a long time and it has almost stopped stinking.",
                    "maleVariant": null,
                    "stringId": 3101112363958026248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Let me guess. You wanna check what's in his pockets, just in case?",
                    "maleVariant": null,
                    "stringId": 3101113640855158784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Those who control the Zone have done their best to finish off the competition.",
                    "maleVariant": null,
                    "stringId": 3101586034896027660
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Sure. You control biz, you control everything.",
                    "maleVariant": null,
                    "stringId": 3101586034896027672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]They knew they had no chance. And yet they did not give up.",
                    "maleVariant": null,
                    "stringId": 3110125058556313600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Sometimes, setting down your guns just isn't an option.",
                    "maleVariant": null,
                    "stringId": 3110125058556313612
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]NorCal flag. So Night City forces camped here.",
                    "maleVariant": null,
                    "stringId": 3110126556560375820
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]It is impossible to hide.",
                    "maleVariant": null,
                    "stringId": 3110126556560375832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Bullet marks. There is also heavily weathered blood. They had a fight.",
                    "maleVariant": null,
                    "stringId": 3110126783589662732
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I was in town then ... But this whole war was fought somewhere on the outskirts, and it didn't concern me any more than a gang tussle did.",
                    "maleVariant": null,
                    "stringId": 3110127231004459016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Then you were lucky.",
                    "maleVariant": null,
                    "stringId": 3110127231004459028
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]The equipment looks military. Have they tried to set up a base here?",
                    "maleVariant": null,
                    "stringId": 3115778095182397452
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Some kind of temporary encampment.",
                    "maleVariant": null,
                    "stringId": 3115778095182397464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Maybe I can find something useful.",
                    "maleVariant": null,
                    "stringId": 3123048315610402820
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I will eliminate any possible threat.",
                    "maleVariant": null,
                    "stringId": 3123054424849059840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Maybe I can find something that I can use later.",
                    "maleVariant": null,
                    "stringId": 3123056005799677952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]In places like this, it's always better to know where the exits are and if there's a ticking bomb somewhere.",
                    "maleVariant": null,
                    "stringId": 3123056109281546240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You never know what is hiding in such places.",
                    "maleVariant": null,
                    "stringId": 3123059836910505984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Tether? Cap? A dead rat?",
                    "maleVariant": null,
                    "stringId": 3123062125624758272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Sounds paranoid.",
                    "maleVariant": null,
                    "stringId": 3123071652482977792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Everyone has been telling me lately that I'm paranoid. It's probably some kind of conspiracy.",
                    "maleVariant": null,
                    "stringId": 3123079541968801792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Hehe. Good one.",
                    "maleVariant": null,
                    "stringId": 3123080030101901312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]A nasty business.",
                    "maleVariant": null,
                    "stringId": 3123101453834981376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]There was a war. Civilians hid in much worse places.",
                    "maleVariant": null,
                    "stringId": 3123156866060193792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]It's been here a long time.",
                    "maleVariant": null,
                    "stringId": 3123222101899022336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I wasn't in Night City then.",
                    "maleVariant": null,
                    "stringId": 3136036759362015232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]All in all, it's probably better.",
                    "maleVariant": null,
                    "stringId": 3136043657314373632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Weird characters.",
                    "maleVariant": null,
                    "stringId": 3140383632883933184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Interesting. There are some lines on this map.",
                    "maleVariant": null,
                    "stringId": 3140384129455972352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Let's check.",
                    "maleVariant": null,
                    "stringId": 3140575113817055232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT][move away]",
                    "maleVariant": null,
                    "stringId": 3140580042996670464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]The equipment looks worn out.",
                    "maleVariant": null,
                    "stringId": 3142096822273531904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]It will definitely be useful for something.",
                    "maleVariant": null,
                    "stringId": 3142098242901073920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]So be it.",
                    "maleVariant": null,
                    "stringId": 3142101051692244992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Bingo.",
                    "maleVariant": null,
                    "stringId": 3142112537944743936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]During the war, we used similar markings.",
                    "maleVariant": null,
                    "stringId": 3142123581362626560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]For checking off enemy bases we've cleared.",
                    "maleVariant": null,
                    "stringId": 3142123883604172800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Bullet marks and blood.",
                    "maleVariant": null,
                    "stringId": 3144664190829608960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Flag of the North.",
                    "maleVariant": null,
                    "stringId": 3144666027112677376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Everything is badly damaged ... But some parts can be recovered.",
                    "maleVariant": null,
                    "stringId": 3145039971497238528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I won't even ask what you are doing with this.",
                    "maleVariant": null,
                    "stringId": 3145040796181291008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Will be useful.",
                    "maleVariant": null,
                    "stringId": 3145041418565672960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]For what?",
                    "maleVariant": null,
                    "stringId": 3146298661623238656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Completely looted.",
                    "maleVariant": null,
                    "stringId": 3162249789102317572
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Someone tried hard to completely gut this car.",
                    "maleVariant": null,
                    "stringId": 3162249914008690688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I support it.",
                    "maleVariant": null,
                    "stringId": 3162251205669773312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Marked ... But intact.",
                    "maleVariant": null,
                    "stringId": 3162254863505514500
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]There are signs here too ... But no sign of looting.",
                    "maleVariant": null,
                    "stringId": 3162255091524657152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Hmm. Well, maybe your collecting passion will pay off in the end.",
                    "maleVariant": null,
                    "stringId": 3162257789670088704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You can see traces of a struggle here. They fired hard.",
                    "maleVariant": null,
                    "stringId": 3162259438065115136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]More markings.",
                    "maleVariant": null,
                    "stringId": 3162259474001911812
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]During the Unification War, the civilians from Night City hid here. NUSA forces secured the area.",
                    "maleVariant": null,
                    "stringId": 3162260246290718720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Vandalism?",
                    "maleVariant": null,
                    "stringId": 3192788261157670912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Recycling. Saving. Giving things new life. Ingenuity.",
                    "maleVariant": null,
                    "stringId": 3192788445505720320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Hmm.",
                    "maleVariant": null,
                    "stringId": 3192789720389586944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Gang scores?",
                    "maleVariant": null,
                    "stringId": 3192796099472986112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Nice euphemism for genocide.",
                    "maleVariant": null,
                    "stringId": 3192797565583560704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You're right. It was not a balanced fight.",
                    "maleVariant": null,
                    "stringId": 3192806493310619648
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}