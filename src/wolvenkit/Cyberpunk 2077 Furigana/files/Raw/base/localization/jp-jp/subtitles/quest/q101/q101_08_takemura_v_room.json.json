    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゴロウ・タケムラだ",
              "maleVariant": "",
              "stringId": 1624399017996865536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしに何の用？",
              "maleVariant": "俺に何の用だ？",
              "stringId": 1624399657544339460
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんであんたと話さなきゃいけないの？",
              "maleVariant": "なぜあんたと話さなきゃいけないんだ？",
              "stringId": 1624400073938063360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺がお前の命を救ってやったからだ",
              "maleVariant": "",
              "stringId": 1624400305497198592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話がある。トムズ・ダイナーに来い",
              "maleVariant": "",
              "stringId": 1640250002186489856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今は無理よ",
              "maleVariant": "今は無理だ",
              "stringId": 1640251309668487168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪いけど今は無理。こっちもいっぱいいっぱいなの",
              "maleVariant": "悪いが今は無理だ。こっちもいっぱいいっぱいでな",
              "stringId": 1640251883264724992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "命を救ってやったことを忘れたのか。借りは返してもらう",
              "maleVariant": "",
              "stringId": 1640257168356200448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "座れ",
              "maleVariant": "",
              "stringId": 1641641109176766464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る]",
              "maleVariant": "",
              "stringId": 1641641692772225028
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "思ったより元気そうだ",
              "maleVariant": "",
              "stringId": 1641642480680620032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "用件を言って",
              "maleVariant": "用件を言ってくれ",
              "stringId": 1641663205256777732
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴィクターにインプラントを診てもらったみたいね",
              "maleVariant": "ヴィクターにインプラントを診てもらったようだな",
              "stringId": 1641666556757331968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴィクにインプラントを直してもらったみたいね",
              "maleVariant": "ヴィクにインプラントを直してもらったようだな",
              "stringId": 1641667544515923968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼はいい医者だ。余計な詮索をしない",
              "maleVariant": "",
              "stringId": 1641678423147991040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい！ 勝手に変えるな！",
              "maleVariant": "",
              "stringId": 1641704191743807488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うるせえ。コーポのメスブタなんか、あんた以外誰も興味ねえんだよ",
              "maleVariant": "",
              "stringId": 1641705240068149248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんだと？ 貴様",
              "maleVariant": "",
              "stringId": 1641761934072664064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "文句あんのか？",
              "maleVariant": "",
              "stringId": 1641762426517508096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん。連れの方はまともらしいな",
              "maleVariant": "",
              "stringId": 1641763912139984896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よく考えさせて",
              "maleVariant": "よく考えさせてくれ",
              "stringId": 1641780348442955780
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "目立つ行動は控えた方がいい",
              "maleVariant": "",
              "stringId": 1641780834965442560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "申し出はありがたいけど、少し考えさせて。また連絡する",
              "maleVariant": "申し出はありがたいが、少し考えさせてくれ。また連絡する",
              "stringId": 1641782984714014720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "目立つ行動は控えないと",
              "maleVariant": "目立つ行動は控えるんだな",
              "stringId": 1641783045413982208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すまない… つい反射的に",
              "maleVariant": "",
              "stringId": 1641786016675713024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エヴリン・パーカーに賭ける",
              "maleVariant": "",
              "stringId": 1641827729934962692
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈Relic〉の開発者なら誰よりも詳しいはず",
              "maleVariant": "〈Relic〉の開発者なら誰よりも詳しいはずだ",
              "stringId": 1641828587217145856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうにかしてエヴリンの居場所を掴めるかもしれない",
              "maleVariant": "",
              "stringId": 1641829573499342848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これは… 俺の提案は… お前にとっても決して悪い話ではないはずだ",
              "maleVariant": "",
              "stringId": 1642956411873316864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これ以上の長居は危険だ",
              "maleVariant": "",
              "stringId": 1643004076447289344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜ助けてくれたの？",
              "maleVariant": "なぜ助けてくれた？",
              "stringId": 1644593492395610112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話ってチップに関すること？",
              "maleVariant": "話ってチップに関することか？",
              "stringId": 1644593830456512512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんであたしを助けたの？",
              "maleVariant": "なぜ俺を助けたんだ？",
              "stringId": 1644599869516075008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、話って何？ 生体チップの件？",
              "maleVariant": "で。話って何だ？ 生体チップの件か？",
              "stringId": 1644599942748622848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。チップは修復できないほど損傷したと聞いている。かと言って、外そうとすればお前は死ぬ",
              "maleVariant": "",
              "stringId": 1644610170508595200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死んでもらっては困るからだ",
              "maleVariant": "",
              "stringId": 1644619994256429056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ったく、またアラサカか",
              "maleVariant": "",
              "stringId": 1644716394512175104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいや、てめえが聞きな",
              "maleVariant": "",
              "stringId": 1673626057769013248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よく聞け…",
              "maleVariant": "",
              "stringId": 1673666405262217216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「盗人にも仁義あり」って聞いたことない？",
              "maleVariant": "「盗人にも仁義あり」って聞いたことないか？",
              "stringId": 1682118893703319552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パーカーがお前を助けることはない",
              "maleVariant": "",
              "stringId": 1682118893736873988
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ザッパーギョーザか… ったく、ナイトシティの食文化にゃ進歩がねえな",
              "maleVariant": "",
              "stringId": 1682262182150590464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが、ローグが生きていたとはな",
              "maleVariant": "",
              "stringId": 1682263449182720000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "助け合う？ 冗談でしょ",
              "maleVariant": "助け合う？ 冗談だろ",
              "stringId": 1682269540855631876
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信用できない",
              "maleVariant": "",
              "stringId": 1682269698695680000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バカじゃないの、あんたを信用できるわけないでしょ",
              "maleVariant": "バカじゃないのか、お前を信用できるわけないだろ",
              "stringId": 1682270551750004736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心配すんな。ローグはもっとバカげた話を散々聞いてきてる。お前がまだ父ちゃんのタマん中にいたときからな",
              "maleVariant": "",
              "stringId": 1682273024527093760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかくローグのことは俺に任せておけ。お前はアフターライフへ向かいな、相棒",
              "maleVariant": "",
              "stringId": 1682273793980551168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信用しようがしまいが、知ったこっちゃないね。問題はそこじゃないからな",
              "maleVariant": "",
              "stringId": 1682274738420371456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカは絶対的権力をもって君臨、世界は破滅への道をまっしぐら。結局、何も変わっちゃいねえ",
              "maleVariant": "",
              "stringId": 1682316270385025024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はそれしかない。他に頼れる知り合いもいないうえ、自分は逃亡者として追われる身だ",
              "maleVariant": "",
              "stringId": 1685003110992891904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブの失脚を望む人間は社内にもいる。あと必要なのは証拠だけだ",
              "maleVariant": "",
              "stringId": 1685005576790659072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカの重役の前で、ヨリノブの罪を告発しろってこと？",
              "maleVariant": "アラサカの重役の前で、ヨリノブの罪を告発しろってことか？",
              "stringId": 1685043950058524676
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "断ったら？",
              "maleVariant": "",
              "stringId": 1685046062259687424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "断ったら？",
              "maleVariant": "",
              "stringId": 1685061243241025536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ何？ アラサカ・タワーに乗り込んで、ヨリノブがサブロウを、実の父親を殺しましたって宣言しろってこと？",
              "maleVariant": "じゃあ何だ？ アラサカ・タワーに乗り込んで、ヨリノブがサブロウを、実の父親を殺しましたって宣言しろってことか？",
              "stringId": 1685061358198509568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "安全な場で、分別ある人々を前に語る機会を作る。それが真実だと証明するには… ある手段を用いる必要があるが",
              "maleVariant": "",
              "stringId": 1685065753191567360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "嘘発見器なら、お断りよ",
              "maleVariant": "嘘発見器なら、お断りだ",
              "stringId": 1685066156750721024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一つだけ頼みがある。絶対にこのナイトシティを離れないでくれ",
              "maleVariant": "",
              "stringId": 1685075442218356736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンダース・ヘルマン… 〈Relic〉の発明者なら、チップを取り出せるはず",
              "maleVariant": "アンダース・ヘルマン… 〈Relic〉の発明者なら、チップを取り出せるはずだ",
              "stringId": 1685179396197502976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう一度アラサカと喧嘩してでも、必ず尻尾を掴んで見せる",
              "maleVariant": "もう一度アラサカと喧嘩してでも、必ず尻尾を掴んでやる",
              "stringId": 1685180631487139840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘルマンはアラサカから逃亡した",
              "maleVariant": "",
              "stringId": 1685185247218028544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ある日忽然と姿を消してしまったんだ。俺自身、何日もかけて街を捜索したが、徒労に終わった",
              "maleVariant": "",
              "stringId": 1685208627375820800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんでヘルマンを探していたの？",
              "maleVariant": "なぜヘルマンを探してたんだ？",
              "stringId": 1685216827273695236
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "逃亡？ よそに引き抜かれたとか？",
              "maleVariant": "逃亡？ よそに引き抜かれた可能性は？",
              "stringId": 1685217035143401472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんでヘルマンを探してたの？",
              "maleVariant": "なぜヘルマンを探してたんだ？",
              "stringId": 1685217486416957440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "逃亡ってどこへ？ 他の企業に引き抜かれたとか？",
              "maleVariant": "逃亡ってどこへだ？ 他の企業に引き抜かれたとか？",
              "stringId": 1685217615081426944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺の知る限り、ヤツの単独行動だ。周到に計画を立てたんだろう。足跡を辿ることすらできなかった",
              "maleVariant": "",
              "stringId": 1685218491036651520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘルマンはヨリノブの企みについて、サブロウ様に警告していた。重要な証人になるかもしれない",
              "maleVariant": "",
              "stringId": 1685222187409657856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただの傭兵の言葉が証拠になるわけ？",
              "maleVariant": "ただの傭兵の言葉が、証拠になるのか？",
              "stringId": 1685323066242551808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "協力の見返りを伝え、再び助力を願う",
              "maleVariant": "",
              "stringId": 1685324978442522624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "直接行ってみたが… 門前払いを食らった。ローグという名の女フィクサーにな",
              "maleVariant": "",
              "stringId": 1685386039707357184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はもう… フリーランスだ",
              "maleVariant": "",
              "stringId": 1691237679496744960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "では、これでどうだ。お前は死に向かっている。そして助かる方法を知らない",
              "maleVariant": "",
              "stringId": 1691294960368275456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "常々皮肉な言い回しだと思っていた。この世に仁義は存在しても、盗人にそんなものはない",
              "maleVariant": "",
              "stringId": 1691306291884511232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "元凶は例の生体チップ、〈Relic〉だ。アラサカが開発し、アラサカのみが知る技術",
              "maleVariant": "",
              "stringId": 1691312048902295552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だがここは俺の店だ。俺には客を選ぶ権利がある。さっさとそいつを飲んで出ていきな",
              "maleVariant": "",
              "stringId": 1719917412501475328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "てめえらみてえな連中がこの国をダメにしたんだ。俺たちの土地や仕事だけじゃなく、尊厳までも奪いやがった",
              "maleVariant": "",
              "stringId": 1719918569777057792
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "友達にでもなったつもり？",
              "maleVariant": "友達にでもなったつもりか？",
              "stringId": 1731325950855532548
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしに構わないで",
              "maleVariant": "俺に構わないでくれ",
              "stringId": 1731329797703389184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何なの、あんたは。あたしを殺そうとしたくせに、今度は友達気取り？ まるで何もなかったみたいに",
              "maleVariant": "何なんだ、お前は。俺を殺そうとしたくせに、今度は友達気取りか？ まるで何もなかったみたいに",
              "stringId": 1731358607991689216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしに構わないで！",
              "maleVariant": "俺に構わないでくれ！",
              "stringId": 1731359307098279936
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しばらく考えて、気が変わった。今はお前を殺そうと思っちゃいない",
              "maleVariant": "",
              "stringId": 1731361451394256896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃ、さっさと消えて！",
              "maleVariant": "じゃあ、さっさと消えろ！",
              "stringId": 1731361790361128960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだこないだのことで怒ってんのか？",
              "maleVariant": "",
              "stringId": 1731362487152463872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "殺されかけたのよ！",
              "maleVariant": "殺されかけたんだぞ！",
              "stringId": 1731362766979649536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実はな、あれから考えが変わったんだ。もうお前を殺そうなんて思っちゃいない",
              "maleVariant": "",
              "stringId": 1731363354937184256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うるさい、もうどっか行って",
              "maleVariant": "うるさい、もうどっか行け",
              "stringId": 1731363738363678720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのな、こっちも大変だったんだぞ。お前はゴミ捨て場で目覚め、俺はお前の頭ん中で目覚めた。お前の思考や記憶と格闘しながらな",
              "maleVariant": "",
              "stringId": 1731390591908499456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴィクは普段、企業の客を診ない。運が良かったね",
              "maleVariant": "ヴィクは普段、企業の客を診ない。運が良かったな",
              "stringId": 1734376296288931840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの時は――とても助かるとは思わなかったが",
              "maleVariant": "",
              "stringId": 1770895661285793792
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴィクのお喋り野郎め…",
              "maleVariant": "",
              "stringId": 1770904531936763904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼がいなければお前はここにいない。感謝するんだな",
              "maleVariant": "",
              "stringId": 1770905310684798976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前と同じ、はみ出し者だな。そうなったいきさつは異なるが",
              "maleVariant": "",
              "stringId": 1770907494507896832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブは父親殺しの罪を償わなければならない",
              "maleVariant": "",
              "stringId": 1770930426512695296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正義のため？ このナイトシティで？",
              "maleVariant": "正義のためか？ このナイトシティで？",
              "stringId": 1770931703426605056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "復讐のためだ。この街にもふさわしい動機だろう",
              "maleVariant": "",
              "stringId": 1770932128393486336
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまりアラサカは、お前を消すのも救うのも意のままというわけだ。死にたくなければ、然るべき人間を味方にしなければならない",
              "maleVariant": "",
              "stringId": 1770955722108461056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "トムは悪い奴じゃない",
              "maleVariant": "",
              "stringId": 1770969778093580288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "トムは悪い奴じゃない。思ったことをすぐ口にしちゃうだけ",
              "maleVariant": "トムは悪い奴じゃない。思ったことをすぐ口にする癖があるだけだ",
              "stringId": 1770970150346448896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつを見ていると、ずっと厨房に立っていた父のことを思い出す",
              "maleVariant": "",
              "stringId": 1770970858781171712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行方に心当たりは？",
              "maleVariant": "",
              "stringId": 1770987966860648448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まったくない",
              "maleVariant": "",
              "stringId": 1770988109131440128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブ・アラサカにも、同じことを言えるの？",
              "maleVariant": "ヨリノブ・アラサカにも同じことを言えるのか？",
              "stringId": 1771001949579665408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それにこないだの後、冷静になって考えたんだが… 俺たちは互いに助け合えるはずだ。まずはローグに会いに行こう。あいつとは昔なじみみてえなもんだ",
              "maleVariant": "",
              "stringId": 1771028947106553856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そもそも、あんたがローグの知り合いだから何？ 会いに行って「あたしの脳腫瘍が挨拶したがってます」とでも言うわけ？",
              "maleVariant": "そもそも、お前がローグの知り合いだから何だ？ 会いに行って「俺の脳腫瘍が挨拶したがってます」とでも言うのか？",
              "stringId": 1771034567071260672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰が相棒よ！",
              "maleVariant": "誰が相棒だ！",
              "stringId": 1771038435276513280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "過去の亡霊が何言ってるの？ 当時のお友達はみんな死んでるか、ボケてあんたのことなんか忘れてるに決まってる",
              "maleVariant": "過去の亡霊が何言ってんだ？ 当時のお友達はみんな死んでるか、ボケてお前のことなんか忘れてるに決まってる",
              "stringId": 1773326763971600384
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は伝説になった。忘れることなんかできねえよ",
              "maleVariant": "",
              "stringId": 1773327133036797952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知ってる。あんたの記憶を見たもの。気持ち悪い",
              "maleVariant": "知ってるよ。お前の記憶を見た。気持ち悪い",
              "stringId": 1773328288936648704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "思ってた以上に色々知ってるのね",
              "maleVariant": "思ってた以上に色々知ってるんだな",
              "stringId": 1786799673373646848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "色々調べまわってたみたいね",
              "maleVariant": "色々調べまわってたようだな",
              "stringId": 1786800805349187584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "指をくわえてじっとしているわけにはいかない。情報を探り、来たるべき事態に備えなければ",
              "maleVariant": "",
              "stringId": 1786801307893915648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それが仕事だしね",
              "maleVariant": "それが仕事だしな",
              "stringId": 1786802190224482304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "かつての、だ",
              "maleVariant": "",
              "stringId": 1786802354523758592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ネズミは尻尾を掴まれるとよく喋る",
              "maleVariant": "",
              "stringId": 1786961475176783872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "数日間に渡って情報を集めた結果――とある場所が繰り返し出てきた。アフターライフというクラブだ",
              "maleVariant": "",
              "stringId": 1786969696247504896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業の人間と思われた？",
              "maleVariant": "企業の人間と思われたか？",
              "stringId": 1786974884853211136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サブロウ様の殺害犯だと思われたようだ",
              "maleVariant": "",
              "stringId": 1786975074637078528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグはこの街の情勢に精通してる。ヘルマンについて、尋ねてみる価値はあるかもしれない",
              "maleVariant": "",
              "stringId": 1786976439648149504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪いことは言わん。別のツテを探したほうがいい",
              "maleVariant": "",
              "stringId": 1786981893166096384
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "健闘を祈る。気難しくて金に汚い、無礼な女だ",
              "maleVariant": "",
              "stringId": 1786982675638673408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パーカーでもヘルマンでも、好きに探せばいい。俺は何人か友人を頼ることにする",
              "maleVariant": "",
              "stringId": 1786985856884039680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカ内部の仲間と話がついたら、こちらからまた連絡する",
              "maleVariant": "",
              "stringId": 1786996739525861376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあまた",
              "maleVariant": "じゃあまたな",
              "stringId": 1787000658431860736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "万一ヘルマンを見つけた時は、俺にも知らせてくれ。まだ彼に聞きたいことがいくつかある",
              "maleVariant": "",
              "stringId": 1787001655938015232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[タケムラをなだめる] よしなって",
              "maleVariant": "[タケムラをなだめる] よせって",
              "stringId": 1888082248731418624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺と話すのに声を張り上げる必要はないだろ？",
              "maleVariant": "",
              "stringId": 1956122601813729280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺と話すのに声を張り上げる必要はない",
              "maleVariant": "",
              "stringId": 1956123065653420032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あまりに色んなことが起きて、いまだに頭の整理がつかない",
              "maleVariant": "",
              "stringId": 1968040962986369024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時間がかかるのは当然だろう。だから何だ？",
              "maleVariant": "",
              "stringId": 1968042236024741888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "生きるつもりがあるなら、もう一度リングに上がるしかない。ゴングは既に鳴っている",
              "maleVariant": "",
              "stringId": 1968044273466630144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "トムズ・ダイナーに来い。待ってるぞ",
              "maleVariant": "",
              "stringId": 1968044843321548800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何がしたいの！？",
              "maleVariant": "何がしたいんだ！？",
              "stringId": 1968136842997424128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしにどうしてほしいの？",
              "maleVariant": "俺にどうしてほしいんだ？",
              "stringId": 1968137507895271424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしも彼女に用があったところよ。チップの摘出を手伝ってもらう約束なの",
              "maleVariant": "俺も彼女に用があったところだ。チップの摘出を手伝ってもらう約束でね",
              "stringId": 1975153275317018624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エヴリン… どうしてよ？",
              "maleVariant": "エヴリン… なんでだよ？",
              "stringId": 1975157035846098944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女とヨリノブは親密な関係だった… きっと、奴にたどり着く方法を知っているはずだ",
              "maleVariant": "",
              "stringId": 1975196934833987584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なら居場所を聞いても仕方ないでしょ",
              "maleVariant": "なら居場所を聞いても仕方ないだろ",
              "stringId": 1975199393065230340
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そいつは無理だろうな。彼女は既に、遠くへ行ってしまったらしい",
              "maleVariant": "",
              "stringId": 1975199687371153408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女の行方を追ってるの？",
              "maleVariant": "彼女の行方を追ってるのか？",
              "stringId": 1975201555950362624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜ彼女がチップの摘出に協力できると？ 彼女には企業の後ろ盾でもあるのか？",
              "maleVariant": "",
              "stringId": 1975201943655047168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜ彼女が遠くへ行ったと？",
              "maleVariant": "",
              "stringId": 1975202048227434496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今自分で質問に答えたじゃない。エヴリンは遠くへ行ってしまったって",
              "maleVariant": "今自分で質問に答えたろ。エヴリンは遠くへ行ってしまったと",
              "stringId": 1975203609733259264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手は尽くしたが、居場所が一向に掴めない。痕跡を消して街を出た可能性がある",
              "maleVariant": "",
              "stringId": 1975263707902009344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パーカーにチップの摘出を手伝ってもらうと言っていたが… 彼女は企業と繋がっているのか？",
              "maleVariant": "",
              "stringId": 1975265415923261440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだと思う",
              "maleVariant": "",
              "stringId": 1975309848936996868
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "質問攻めはやめて",
              "maleVariant": "質問攻めはやめてくれ",
              "stringId": 1975311328871997440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで？ 何の用なの？",
              "maleVariant": "それで？ 何の用だ？",
              "stringId": 1975311797342199808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはわからない",
              "maleVariant": "",
              "stringId": 1975314094327619584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはないと思う",
              "maleVariant": "",
              "stringId": 1975314295100563456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わからない",
              "maleVariant": "",
              "stringId": 1975314398951530496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだと思う",
              "maleVariant": "そうだろうな",
              "stringId": 1975314775600029696
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいえ。あんたのは… ただの悪臭",
              "maleVariant": "いいや。あんたのは… ただの悪臭だ",
              "stringId": 1975314947583270912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは飼い主に小屋から追い出された野良犬でしょ。忘れたの？",
              "maleVariant": "あんたは飼い主に小屋から追い出された野良犬だ。忘れたのか？",
              "stringId": 1975315851338342400
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正体はわからないけど、あたしは彼女にコーポ特有の狡猾さを感じたの… わかるでしょ？",
              "maleVariant": "正体はわからないが、俺は彼女にコーポ特有の狡猾さを感じたんだ… わかるだろ？",
              "stringId": 1975317989040873472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺にも、その“狡猾さ”とやらがあるのか？",
              "maleVariant": "",
              "stringId": 1975318143877799936
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはないと思う",
              "maleVariant": "",
              "stringId": 1975320338404761600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その根拠は何だ？",
              "maleVariant": "",
              "stringId": 1975320455409065984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エヴリンは〈Relic〉についてかなり詳しかったし、機密情報を色々知っていた",
              "maleVariant": "",
              "stringId": 1975322078118174720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺も“欺瞞の悪臭”とやらを放ってるのか？",
              "maleVariant": "",
              "stringId": 1975327239595122688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コーポが朝にどれだけ香水をつけたって、昼頃には欺瞞の悪臭が漂いだす。だけどエヴリンからは、固い決意の香りしかしなかった",
              "maleVariant": "コーポが朝にどれだけ香水をつけようが、昼頃には欺瞞の悪臭が漂いだす。だがエヴリンからは、固い決意の香りしかしなかった",
              "stringId": 1975327388560023552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] あたしじゃ力になれない",
              "maleVariant": "[立ち上がる] 俺じゃ力になれない",
              "stringId": 1975331903241740292
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "色々と興味深い話をするものでな",
              "maleVariant": "",
              "stringId": 1975335444073177088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "たまにはそっちが興味深い話をしたらどう？",
              "maleVariant": "たまにはそっちが興味深い話をしたらどうだ？",
              "stringId": 1975335525123907584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんまり質問攻めにしないでよ",
              "maleVariant": "おい、質問攻めはよしてくれ",
              "stringId": 1975335616408739840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブ・アラサカは罪を犯した。俺は奴にその報いを受けさせるつもりだ",
              "maleVariant": "",
              "stringId": 1975338905045065728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] もう行く",
              "maleVariant": "",
              "stringId": 1975340258194644992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうやら力にはなれないみたい。時間もないし、あたしはこれで…",
              "maleVariant": "どうやら力にはなれないみたいだ。時間もないし、俺はこれで…",
              "stringId": 1975350338516443136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "終わり？ 急いでるからもう行くね",
              "maleVariant": "終わりか？ 急いでるからもう行くぞ",
              "stringId": 1975350473422036992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、待て。まだ用がある",
              "maleVariant": "",
              "stringId": 1975374091480752128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待て",
              "maleVariant": "",
              "stringId": 1975376929380392960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの女がこの事態を招いたのだろ。役に立つとは思えんが",
              "maleVariant": "",
              "stringId": 1975390998955057152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まず初めに、エヴリン・パーカー… 彼女の居場所を教えてくれ",
              "maleVariant": "",
              "stringId": 1988093739773157376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]  誰が相棒よ！",
              "maleVariant": "[立ち上がる]  誰が相棒だ！",
              "stringId": 1988492330756956164
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "たとえばアンダース・ヘルマンとか？",
              "maleVariant": "",
              "stringId": 1996634640818397188
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはどんな人間のこと？",
              "maleVariant": "それはどんな人間のことだ？",
              "stringId": 1996636842878328832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "具体的にどんな人間のことを言ってるの？",
              "maleVariant": "具体的にどんな人間のことを言ってるんだ？",
              "stringId": 1996637674994688000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "たとえばアンダース・ヘルマンとか？",
              "maleVariant": "",
              "stringId": 1996637783174176768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカを心から大切に思っている人間。会社の安定的成長を望んでいる人間だ",
              "maleVariant": "",
              "stringId": 1996639344680001536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いったいどこでその名を？",
              "maleVariant": "",
              "stringId": 1996667211568668672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チップ強奪の際に調べたからよ。ヘルマンは〈Relic〉を設計した人物",
              "maleVariant": "チップ強奪の際に調べたからだ。ヘルマンは〈Relic〉を設計した人物",
              "stringId": 1996667388467634176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴はただの駒に過ぎない。俺が言ってるのはもっと上の人間だ",
              "maleVariant": "",
              "stringId": 1996668206105255936
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
