    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 2004115944562987008,
            "femaleVariant": "まるで、ゴミ捨て場で見つけたあの時みたいなザマだな。起きろ"
          },
          {
            "stringId": 2004115944579764232,
            "femaleVariant": "タケムラ？ どうしてあんたが？"
          },
          {
            "stringId": 2004115944630095872,
            "femaleVariant": "[座る] なんであんたがいるの？",
            "maleVariant": "[座る] なんであんたがいるんだ？"
          },
          {
            "stringId": 2004115944646873112,
            "femaleVariant": "[座る] 相変わらず無愛想ね",
            "maleVariant": "[座る] 相変わらず無愛想だな"
          },
          {
            "stringId": 2004115944680427532,
            "femaleVariant": "タケムラ。相変わらず無愛想ね",
            "maleVariant": "タケムラ。相変わらず無愛想だな"
          },
          {
            "stringId": 2004115944680427548,
            "femaleVariant": "お前に真実を伝えるよう頼まれてな"
          },
          {
            "stringId": 2004115944713981952,
            "femaleVariant": "酷い場所だ… 気がおかしくなるのも無理はない"
          },
          {
            "stringId": 2004115944764313604,
            "femaleVariant": "わざわざ迎えに来てくれたの？",
            "maleVariant": "わざわざ迎えに来てくれたのか？"
          },
          {
            "stringId": 2004115944764313620,
            "femaleVariant": "話をするよう頼まれた"
          },
          {
            "stringId": 2004115944781090816,
            "femaleVariant": "話って？"
          },
          {
            "stringId": 2004115944781090832,
            "femaleVariant": "あんたの最愛のボスが蘇った"
          },
          {
            "stringId": 2004115944797868040,
            "femaleVariant": "あんたはどうしてるの？",
            "maleVariant": "あんたはどうしてるんだ？"
          },
          {
            "stringId": 2004115944797868056,
            "femaleVariant": "誰に頼まれたの？",
            "maleVariant": "誰に頼まれたんだ？"
          },
          {
            "stringId": 2004115944831422508,
            "femaleVariant": "あれからどうしてた？ サブロウの警護に戻ったの？",
            "maleVariant": "あれからどうしてた？ サブロウの警護に戻ったのか？"
          },
          {
            "stringId": 2004115944831422524,
            "femaleVariant": "俺は日本へ異動になった。ここから高松へ飛ぶ"
          },
          {
            "stringId": 2004115944831422540,
            "femaleVariant": "どうして？"
          },
          {
            "stringId": 2004115944831422556,
            "femaleVariant": "時代が変われば務めも変わる。だが俺の話はいい"
          },
          {
            "stringId": 2004115944864976932,
            "femaleVariant": "何の話をしに来たわけ？",
            "maleVariant": "何の話をしに来たんだ？"
          },
          {
            "stringId": 2004115944864976948,
            "femaleVariant": "残念だが、いい話ではない"
          },
          {
            "stringId": 2004115944898531368,
            "femaleVariant": "話って、誰に頼まれたの？ ハナコ？",
            "maleVariant": "話って、誰に頼まれたんだ？ ハナコか？"
          },
          {
            "stringId": 2004115944898531408,
            "femaleVariant": "何て言われたの？",
            "maleVariant": "何て言われたんだ？"
          },
          {
            "stringId": 2004115944948862988,
            "femaleVariant": "はっきり言おう。手術は上手くいかなかった"
          },
          {
            "stringId": 2004115944948863004,
            "femaleVariant": "もって冬までの命だ"
          },
          {
            "stringId": 2004115944965640192,
            "femaleVariant": "どうして… 最高の医療チームが手術をしたんでしょ",
            "maleVariant": "どうしてだ… 最高の医療チームが手術をしたんだろ"
          },
          {
            "stringId": 2004115944982417420,
            "femaleVariant": "そんなの信じられるわけがない"
          },
          {
            "stringId": 2004115944982417436,
            "femaleVariant": "クソ"
          },
          {
            "stringId": 2004115945015971856,
            "femaleVariant": "どうして… アラサカの最高の人材が手術をしたんでしょ？",
            "maleVariant": "どうしてだ… アラサカの最高の人材が手術をしたんだろ？"
          },
          {
            "stringId": 2004115945049526284,
            "femaleVariant": "ウソよ。そんなわけない",
            "maleVariant": "ウソだ。そんなわけない"
          },
          {
            "stringId": 2004115945049526300,
            "femaleVariant": "俺にできるのは、真実を告げることだけだ"
          },
          {
            "stringId": 2004115945049526316,
            "femaleVariant": "こ… これで何もかも解決すると思ってたのに"
          },
          {
            "stringId": 2004115945083080732,
            "femaleVariant": "クソ"
          },
          {
            "stringId": 2004115945099857920,
            "femaleVariant": "どういうことよ",
            "maleVariant": "どういうことだ"
          },
          {
            "stringId": 2004115945099857936,
            "femaleVariant": "あと数ヶ月の命ってことね…",
            "maleVariant": "あと数ヶ月の命ってことか…"
          },
          {
            "stringId": 2004115945099857952,
            "femaleVariant": "アラサカが助けてくれる約束だったでしょ",
            "maleVariant": "アラサカが助けてくれる約束だっただろ"
          },
          {
            "stringId": 2004115945133412368,
            "femaleVariant": "じゃあ… あと数ヶ月であたしは死ぬのね",
            "maleVariant": "じゃあ… あと数ヶ月で俺は死ぬんだな"
          },
          {
            "stringId": 2004115945133412384,
            "femaleVariant": "はじめは体調も回復したように感じるだろう。だがやがて、症状は本格化していく"
          },
          {
            "stringId": 2004115945133412400,
            "femaleVariant": "最期の数週間はベッドの上だ"
          },
          {
            "stringId": 2004115945183744028,
            "femaleVariant": "あんたに手を貸せばアラサカが助けてくれる約束じゃなかった？",
            "maleVariant": "あんたに手を貸せばアラサカが助けてくれる約束だろ"
          },
          {
            "stringId": 2004115945183744044,
            "femaleVariant": "助けられると思ってたんだ"
          },
          {
            "stringId": 2004115945183744060,
            "femaleVariant": "有力なツテがたくさんあるんでしょ！？ そいつらはどこ行ったの！",
            "maleVariant": "有力なツテがたくさんあるんじゃなかったのか！？ そいつらはどこ行った！"
          },
          {
            "stringId": 2004115945183744076,
            "femaleVariant": "俺も手ぶらで来たわけじゃない。別の選択肢の話をしよう"
          },
          {
            "stringId": 2004115945183744092,
            "femaleVariant": "ハナコ自身があたしに約束したのに",
            "maleVariant": "ハナコ自身が俺に約束したんだぞ"
          },
          {
            "stringId": 2004115945200521216,
            "femaleVariant": "わからんか。だからこそ、ハナコ様は俺をここへ遣わせたんだ"
          },
          {
            "stringId": 2004115945234075720,
            "femaleVariant": "ふざけないで。こっちは一度あんたたちを信じて、裏切られてるのよ？",
            "maleVariant": "ふざけるなよ。こっちは一度あんたらを信じて、裏切られてるんだぞ？"
          },
          {
            "stringId": 2004115945234075780,
            "femaleVariant": "“魂の救済”プログラムに参加するんだ。ハナコ様も既に同意されている"
          },
          {
            "stringId": 2004115945250852876,
            "femaleVariant": "お前は記憶痕跡となって肉体を離れ、新しい体に意識を移す方法が見つかるまで〈神輿〉に保管される"
          },
          {
            "stringId": 2004115945250852900,
            "femaleVariant": "保管？ すぐ新しい体に移せないの？",
            "maleVariant": "保管？ すぐ新しい体に移せないのか？"
          },
          {
            "stringId": 2004115945250852912,
            "femaleVariant": "残念ながら現在の技術では不可能だ"
          },
          {
            "stringId": 2004115945250852928,
            "femaleVariant": "じゃあサブロウ・アラサカはどうして蘇ったのよ？",
            "maleVariant": "じゃあサブロウ・アラサカはどうして蘇ったんだ？"
          },
          {
            "stringId": 2004115945250852940,
            "femaleVariant": "自分の子供の体を使ったからだ。体組織の適合度が格段に違う"
          },
          {
            "stringId": 2004115945284407296,
            "femaleVariant": "〈神輿〉になんて行きたくない"
          },
          {
            "stringId": 2004115945284407312,
            "femaleVariant": "アラサカは魂のコレクションが欲しいだけでしょ",
            "maleVariant": "アラサカは魂のコレクションが欲しいだけだろ"
          },
          {
            "stringId": 2004115945317961760,
            "femaleVariant": "せっかくだけど、アラサカのコレクションにされるのはごめんよ",
            "maleVariant": "せっかくだが、アラサカのコレクションにされるのはごめんだ"
          },
          {
            "stringId": 2004115945317961776,
            "femaleVariant": "あまり自分を買いかぶりすぎないことだ。アラサカ社にとって、お前にそんな価値など存在しない"
          },
          {
            "stringId": 2004115945317961788,
            "femaleVariant": "これはお前の働きに報おうとするハナコ様のお心遣いだ。お前が勘ぐるような裏は何もない"
          },
          {
            "stringId": 2004115945385070624,
            "femaleVariant": "契約書が安全を保証してくれる。サインすれば生きられるんだぞ"
          },
          {
            "stringId": 2004115945418625064,
            "femaleVariant": "サブロウ・アラサカは、ヨリノブの体で蘇った"
          },
          {
            "stringId": 2004115945418625080,
            "femaleVariant": "ああ。正義が為された"
          },
          {
            "stringId": 2004115945418625096,
            "femaleVariant": "で… あんたは復讐を果たせて満足？",
            "maleVariant": "で… あんたは復讐を果たせて満足か？"
          },
          {
            "stringId": 2004115945435402240,
            "femaleVariant": "ハナコ様は間違いなく最善の選択をされた"
          },
          {
            "stringId": 2004115945452179456,
            "femaleVariant": "イカれた家族よね",
            "maleVariant": "イカれた家族だな"
          },
          {
            "stringId": 2004115945452179472,
            "femaleVariant": "サブロウは蘇るべきじゃなかった"
          },
          {
            "stringId": 2004115945452179488,
            "femaleVariant": "ヨリノブが気の毒",
            "maleVariant": "ヨリノブが気の毒だ"
          },
          {
            "stringId": 2004115945485733924,
            "femaleVariant": "揃いも揃ってイカれた家族ね。そう思わない？",
            "maleVariant": "揃いも揃ってイカれた家族だ。そう思わないか？"
          },
          {
            "stringId": 2004115945502511108,
            "femaleVariant": "おい、言葉に気をつけろ"
          },
          {
            "stringId": 2004115945502511124,
            "femaleVariant": "妹が兄を殺した挙句、その亡骸に殺された父親の意識を植え付けたのよ？ どうかしてるでしょ",
            "maleVariant": "妹が兄を殺した挙句、その亡骸に殺された父親の意識を植え付けたんだぞ？ どうかしてるだろ"
          },
          {
            "stringId": 2004115945502511136,
            "femaleVariant": "住む世界が違うんだ。俺たちにどうこう言う資格はない"
          },
          {
            "stringId": 2004115945536065564,
            "femaleVariant": "サブロウは蘇るべきじゃなかった"
          },
          {
            "stringId": 2004115945536065580,
            "femaleVariant": "あの御方は、誰よりも生き永らえる運命にある"
          },
          {
            "stringId": 2004115945586397224,
            "femaleVariant": "アラサカ家ってのは、絵にかいたような悲劇の一族ね。シェイクスピアの劇に出てきてもおかしくない",
            "maleVariant": "アラサカ家ってのは、絵にかいたような悲劇の一族だな。シェイクスピアの劇に出てきてもおかしくない"
          },
          {
            "stringId": 2004115945586397240,
            "femaleVariant": "特にヨリノブは"
          },
          {
            "stringId": 2004115945586397256,
            "femaleVariant": "同情しているのか？"
          },
          {
            "stringId": 2004115945603174412,
            "femaleVariant": "父親の影から抜け出そうと必死だったのに、その手にかかって死ぬなんて悪夢でしょ",
            "maleVariant": "父親の影から抜け出そうと必死だったのに、その手にかかって死ぬなんて悪夢だろ"
          },
          {
            "stringId": 2004115945670283268,
            "femaleVariant": "真実を受け入れて俺の話を聞け。お前には選択肢がある"
          },
          {
            "stringId": 2004115945787723808,
            "femaleVariant": "こんな所に何日も閉じ込められていたのか？ だとしたら、信じられん"
          },
          {
            "stringId": 2004115945854832640,
            "femaleVariant": "[体を起こす]"
          },
          {
            "stringId": 2004115945905164288,
            "femaleVariant": "[契約書を手に取る]"
          },
          {
            "stringId": 2004115945989050372,
            "femaleVariant": "「全ての権利を放棄する」とあるが心配するな。法律が時代に追いついてないだけだ。記憶痕跡は法的に人とみなされない"
          },
          {
            "stringId": 2004115945989050388,
            "femaleVariant": "あたしは企業の所有物になるってこと？",
            "maleVariant": "俺は企業の所有物になるってことか？"
          },
          {
            "stringId": 2004115945989050404,
            "femaleVariant": "ブレインダンス界のスターや政治家も大勢この契約を結んでる。プログラムは好評で、何の苦情も出ていない"
          },
          {
            "stringId": 2004115945989050420,
            "femaleVariant": "で、断ったら？",
            "maleVariant": "断ったらどうなる？"
          },
          {
            "stringId": 2004115945989050436,
            "femaleVariant": "ここは刑務所じゃないんだ。荷物をまとめてナイトシティに帰ればいい"
          },
          {
            "stringId": 2004116215800238112,
            "femaleVariant": "あんた名前は？"
          },
          {
            "stringId": 2004116215800238128,
            "femaleVariant": "ハジメです"
          },
          {
            "stringId": 2004116215800238144,
            "femaleVariant": "ハジメ、ひとつ聞かせて。死にたくない一心で、宿敵の監獄にまで来た人間に何て声をかける？",
            "maleVariant": "ハジメ、ひとつ聞かせてくれ。死にたくない一心で、宿敵の監獄にまで来た人間に、何て声をかける？"
          },
          {
            "stringId": 2004116215800238160,
            "femaleVariant": "えーっと、そうですね。「帰り道だけは忘れるな」、でしょうか"
          },
          {
            "stringId": 2004116216051896320,
            "femaleVariant": "[立ち上がる] また会えるよね？",
            "maleVariant": "[立ち上がる] また会えるよな？"
          },
          {
            "stringId": 2004116216085450752,
            "femaleVariant": "[契約書にサインする]"
          },
          {
            "stringId": 2004116216085450764,
            "femaleVariant": "[荷物をまとめて地球に帰る]"
          },
          {
            "stringId": 2004116216119005188,
            "femaleVariant": "もし俺がお前の立場だったとしたらサインするが、選ぶのはお前だ。よく考えて決めろ"
          },
          {
            "stringId": 2004116216337108992,
            "femaleVariant": "[パズルを手に取る]"
          },
          {
            "stringId": 2004116216404217880,
            "femaleVariant": "アラサカ社は必ず約束を守る"
          },
          {
            "stringId": 2004116216504881156,
            "femaleVariant": "死にたくない"
          },
          {
            "stringId": 2004116216504881168,
            "femaleVariant": "賢明な判断だ"
          },
          {
            "stringId": 2004116216504881180,
            "femaleVariant": "記憶痕跡ステーションの用意はできてる。症状が悪化する前に、急いだほうがいい"
          },
          {
            "stringId": 2004116216605544448,
            "femaleVariant": "[アラサカに魂を売る]"
          },
          {
            "stringId": 2004116216722984964,
            "femaleVariant": "よし、決めた"
          },
          {
            "stringId": 2004116216739762176,
            "femaleVariant": "地球に… 家に帰る"
          },
          {
            "stringId": 2004116216739762188,
            "femaleVariant": "死ぬだけだぞ"
          },
          {
            "stringId": 2004116217192747008,
            "femaleVariant": "[中指を立てる]"
          },
          {
            "stringId": 2004116217293410304,
            "femaleVariant": "[地球に帰る]"
          },
          {
            "stringId": 2004116217545068544,
            "femaleVariant": "また会えるよね？",
            "maleVariant": "また会えるよな？"
          },
          {
            "stringId": 2004116217545068560,
            "femaleVariant": "ああ、きっとな"
          },
          {
            "stringId": 2004116217545068576,
            "femaleVariant": "じゃあ… さよなら"
          },
          {
            "stringId": 2004116217628954628,
            "femaleVariant": "ありがとう、タケムラ。こんな所まで来てくれるなんて、あんたはいい奴ね",
            "maleVariant": "ありがとな、タケムラ。こんな所まで来てくれるなんて、あんたはいい奴だ"
          },
          {
            "stringId": 2004116217628954640,
            "femaleVariant": "それは何だ？"
          },
          {
            "stringId": 2004116217662509056,
            "femaleVariant": "[立ち上がる] じゃあね、タケムラ",
            "maleVariant": "[立ち上がる] じゃあな、タケムラ"
          },
          {
            "stringId": 2004116217796726792,
            "femaleVariant": "〈美しきものは消え去らない〉"
          },
          {
            "stringId": 2004116217830281224,
            "femaleVariant": "〈お前の目、いつも俺を見てる、どこにでも現れるお前はまるでゴースト〉"
          },
          {
            "stringId": 2018716791898398720,
            "femaleVariant": "お前と話をしに来た"
          },
          {
            "stringId": 2018794699115487232,
            "femaleVariant": "悪いけど、あたしにはあいつを慕う気持ちがわからない",
            "maleVariant": "悪いが、俺にはあいつを慕う気持ちがわからない"
          },
          {
            "stringId": 2018795035968430080,
            "femaleVariant": "おそばでお仕えしなければわかるまい… まあとにかく、アラサカ家の話はここまでにしておこう"
          },
          {
            "stringId": 2018845020630048768,
            "femaleVariant": "ハナコ様はもうお前のことなどお忘れだ。ここのクリニックから連絡を受けた"
          },
          {
            "stringId": 2018845795351552000,
            "femaleVariant": "誰か、お前のことを知る者から伝えるべき内容だと"
          },
          {
            "stringId": 2018876965657272320,
            "femaleVariant": "手術自体は成功したが、身体がチップの影響を受けすぎていたんだ。世界屈指の神経外科医ですら手の施しようがないまでにな"
          },
          {
            "stringId": 2018877801548505088,
            "femaleVariant": "ウソよ！ アラサカの力があれば、何だってできるはずでしょ！",
            "maleVariant": "ウソだ！ アラサカの力があれば、何だってできるはずだろ！"
          },
          {
            "stringId": 2018886455924383744,
            "femaleVariant": "そんな…"
          },
          {
            "stringId": 2018886547477651456,
            "femaleVariant": "気を鎮めろ。お前に残されたもう1つの選択肢について話したい"
          },
          {
            "stringId": 2018907086330028032,
            "femaleVariant": "お断りよ。そんな話に乗ったら、それこそ何をされるかわかったもんじゃない",
            "maleVariant": "お断りだ。そんな話に乗ったら、それこそ何をされるかわかったもんじゃない"
          },
          {
            "stringId": 2018919758379376640,
            "femaleVariant": "いつか香川に来い。うまいものでも食わせてやるよ"
          },
          {
            "stringId": 2018923194051223552,
            "femaleVariant": "でも1つだけ、あんたには理解できないことがある",
            "maleVariant": "だが1つだけ、あんたには決して理解できないことがある"
          },
          {
            "stringId": 2018924516079067136,
            "femaleVariant": "自由がどんなに素晴らしいかってこと",
            "maleVariant": "自由がどんなに素晴らしいかってことさ"
          },
          {
            "stringId": 2018925922160762880,
            "femaleVariant": "強がりを言ってる場合ではなかろう。考え直せ"
          },
          {
            "stringId": 2018926809893593088,
            "femaleVariant": "もう決めたの。これでお別れだけど、あんたのことは忘れない。今までありがとう",
