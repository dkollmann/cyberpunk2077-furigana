  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "危険だと警告されたの。オダとも連絡が取れないし、何か変だわ",
                    "maleVariant": null,
                    "stringId": 1669447913484337152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私が参加したのは、あくまでもお父様を追悼するため",
                    "maleVariant": null,
                    "stringId": 1669449579881316352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "操り人形になるためじゃないわ",
                    "maleVariant": null,
                    "stringId": 1669450106887225344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "パレードを中止すれば、弱さを露呈したと思われる",
                    "maleVariant": null,
                    "stringId": 1714211719007453184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それは… あなたの弱さのこと？",
                    "maleVariant": null,
                    "stringId": 1714212267806965760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だが、お前が言うなら中止してもいい。全部やめだ。お前の安全が第一だからな",
                    "maleVariant": null,
                    "stringId": 1714212716832374784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お父様…",
                    "maleVariant": null,
                    "stringId": 1714220354475409408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お許しを。どうか、話だけでもお聞きください",
                    "maleVariant": null,
                    "stringId": 1714224100794187776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お父上は…",
                    "maleVariant": null,
                    "stringId": 1714225132743966720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "父のことを口にしないで！",
                    "maleVariant": null,
                    "stringId": 1714225355629281280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハナコ様、無礼を働きたくはありません。しかし…",
                    "maleVariant": null,
                    "stringId": 1714225488303505408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、逃げろ！",
                    "maleVariant": null,
                    "stringId": 1714227320375492608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと、ウソでしょ",
                    "maleVariant": "おいおい、ウソだろ",
                    "stringId": 1714228292917149696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "兄さん…",
                    "maleVariant": null,
                    "stringId": 1724068500475875328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やり終えましょう",
                    "maleVariant": null,
                    "stringId": 1724113942068727808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ありがとう、ハナコ。せっかくの式典だ――やり終えなければ、悔いが残る",
                    "maleVariant": null,
                    "stringId": 1724116336999534592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "...",
                    "maleVariant": null,
                    "stringId": 1724118030575603712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハナコ様",
                    "maleVariant": null,
                    "stringId": 1724119896722116608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あなたは…",
                    "maleVariant": null,
                    "stringId": 1724121657927143424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これが、俺にできる最後の務めなのです…",
                    "maleVariant": null,
                    "stringId": 1724149966509576192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サブロウ様の死の真実を明かすことが！",
                    "maleVariant": null,
                    "stringId": 1724150642950148096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この見せ物では、みんなに役割がある。ミチコは文句を言ってないぞ",
                    "maleVariant": null,
                    "stringId": 1724223690596290560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なぜ止まったんだ？",
                    "maleVariant": null,
                    "stringId": 1724231251970101248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺たちだ",
                    "maleVariant": null,
                    "stringId": 1724233151520051200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何したにしても、最悪にマズいってこと！",
                    "maleVariant": "何したにしても、最悪にマズいってことだ！",
                    "stringId": 1771027611004047360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハナコは中よ",
                    "maleVariant": "ハナコは中だ",
                    "stringId": 1773724698127908864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "通信を試みてる",
                    "maleVariant": null,
                    "stringId": 1773725998731247616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "聞き耳を立てろ",
                    "maleVariant": null,
                    "stringId": 1773726175479218176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "警報の解除も忘れないでくれ。でないと俺が入れない",
                    "maleVariant": null,
                    "stringId": 1773726519227596800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ゴロウ、システムを無効化した。今しかない",
                    "maleVariant": null,
                    "stringId": 1773729105720655872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何をされている？",
                    "maleVariant": null,
                    "stringId": 1780695188894519296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "急げ、V！",
                    "maleVariant": null,
                    "stringId": 1790741196538200068
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V！ 手遅れになるぞ！",
                    "maleVariant": null,
                    "stringId": 1790741196554977284
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何の権利でこんなことを！",
                    "maleVariant": null,
                    "stringId": 1795474982084558848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "全員、地面に伏せろ！",
                    "maleVariant": null,
                    "stringId": 1795475678641012736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふざけるな！ こんなのは市民権の侵害だ！",
                    "maleVariant": null,
                    "stringId": 1795476053259468800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "止まれ！",
                    "maleVariant": null,
                    "stringId": 1795479668128759808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "他に市民権について意見したい奴はいるか！？ こいつの後を追わせてやる！",
                    "maleVariant": null,
                    "stringId": 1795483864295030784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "押しつぶされるところだった… 肋骨が折れたかも…",
                    "maleVariant": null,
                    "stringId": 1801295829407907840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここから逃げないと！",
                    "maleVariant": null,
                    "stringId": 1801296487729086464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "髪乱れてない？ 撮影始めるけど",
                    "maleVariant": null,
                    "stringId": 1801297323049893888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "3、2、1…",
                    "maleVariant": null,
                    "stringId": 1801298631119093760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "N54ニュースのジリアン・ジョーダンがジャパンタウンからお伝えします。本日行われたアラサカ社のサブロウ・アラサカ氏をしのぶ記念パレードは大混乱に陥りました",
                    "maleVariant": null,
                    "stringId": 1801299000486281216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "目撃者の話によると、パレードの山車の一つで銃撃戦が巻き起こったということで、その山車にはハナコ・アラサカ氏が乗っていたと言われています",
                    "maleVariant": null,
                    "stringId": 1801299245702070272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "残念ながら、現時点ではこれ以上の詳細はわかっていません",
                    "maleVariant": null,
                    "stringId": 1801300495923429376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "事件に関するアラサカ社からの正式な声明も、追って発表されるものと思われます",
                    "maleVariant": null,
                    "stringId": 1801300622893400064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "カメラに入らないで！",
                    "maleVariant": null,
                    "stringId": 1801303594255794176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう… 撮り直しよ！",
                    "maleVariant": null,
                    "stringId": 1801304953780715520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おかげさまで台無し！",
                    "maleVariant": null,
                    "stringId": 1801305575091355648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "現場は依然として混乱が続いています。カンザキ通りには近寄らないほうがいいでしょう",
                    "maleVariant": null,
                    "stringId": 1863315822830493696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だから信用できねえって言ったんだ！ 好き勝手させるなとな！",
                    "maleVariant": null,
                    "stringId": 1906936140640829440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さっさと退散しないとまずいぞ",
                    "maleVariant": null,
                    "stringId": 1906937283303456768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "撃ちやがったのか！？",
                    "maleVariant": null,
                    "stringId": 2007084788928552960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[警報システムを解除する]",
                    "maleVariant": null,
                    "stringId": 2034753975066525696
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
