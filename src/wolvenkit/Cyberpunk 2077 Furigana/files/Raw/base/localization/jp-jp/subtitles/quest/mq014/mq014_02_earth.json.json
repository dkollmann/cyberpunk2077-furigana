    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今からそなたに教えるのは大地の瞑想だ",
                    "maleVariant": null,
                    "stringId": 1815424808219807744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大いなる大自然の精霊、母なる大地よ。我々は常に一つの共同体。我はそなたであり、そなたは我",
                    "maleVariant": null,
                    "stringId": 1816921764108668928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "我の身体と魂から苦痛を、病を、穢れを取り除き、害ある人々、社会、次元から害ある気を取り除きたまえ",
                    "maleVariant": null,
                    "stringId": 1822658939899056128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "命の気を与え、健康、若さ、知恵、知識、強さ、喜び、幸せ、そして内なる安らぎを与えたまえ",
                    "maleVariant": null,
                    "stringId": 1822659844442656768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "我々の心と魂は、喜び、平穏、安心、純潔を感じるだろう",
                    "maleVariant": null,
                    "stringId": 1822667226233147392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "我々はぬくもりと喜びに包み込まれる。大地の気が脚を伝い、全細胞を満たしていく。我々の魂と精神が清められていく",
                    "maleVariant": null,
                    "stringId": 1822667455175036928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "母なる大地よ、心と、身体と、魂をもって感謝申し上げる。四大元素なくして、命は存在しえない",
                    "maleVariant": null,
                    "stringId": 1822684701398110208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "我々は豊かな大地を歩み、草原を駆け抜ける",
                    "maleVariant": null,
                    "stringId": 1918338054580310016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "母なる大地よ、我も他の生き物も、全てはそなたが生み出し存在",
                    "maleVariant": null,
                    "stringId": 1925539654566645760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "我々は靴を脱ぎ、裸足になることで土を直接感じ、大地の気を吸収する",
                    "maleVariant": null,
                    "stringId": 1925541063785680896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "我々は、咲き誇る花にチョウが休み、ハチが蜜を集める大地を歩んでいく",
                    "maleVariant": null,
                    "stringId": 1925541516384636928
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
