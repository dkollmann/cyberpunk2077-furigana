    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1722898438891032576,
            "femaleVariant": "さて、ギリシャ人いわく… 人生は宴――去るタイミングが肝心だ"
          },
          {
            "stringId": 1722898438891032592,
            "femaleVariant": "私もそろそろ行くわ。本番前に通信の準備をしとかないと"
          },
          {
            "stringId": 1722898438958141456,
            "femaleVariant": "で、急かすわけじゃねえが、金はいつ貰えるんだ？"
          },
          {
            "stringId": 1722898438974918668,
            "femaleVariant": "それはミス・パーカー次第だ。まあ1、2週間といったところだろう"
          },
          {
            "stringId": 1722898438991695872,
            "femaleVariant": "その間は？ ただ口を開けてボーッとしてろと？"
          },
          {
            "stringId": 1722898438991695888,
            "femaleVariant": "身を潜めていろ。アラサカに気づかれたら終わりだからな"
          },
          {
            "stringId": 1722898439545344008,
            "femaleVariant": "俺はトップクラスの連中としか組まない。デラマンはそれにふさわしい"
          },
          {
            "stringId": 1722898439545344024,
            "femaleVariant": "詮索好きな人間の運転手より、“クリーン”なAIの方が色々と安全だしね"
          },
          {
            "stringId": 1722898440702971904,
            "femaleVariant": "よろしく、ラモン・ヴィクトリノ"
          },
          {
            "stringId": 1722898440702971920,
            "femaleVariant": "あなたはハンナ・コンウェル",
            "maleVariant": "あなたはハリー・コンウェル"
          },
          {
            "stringId": 1722898440719749132,
            "femaleVariant": "ラモンね… で、何しに来たって体なんだ？"
          },
          {
            "stringId": 1724200577630261252,
            "femaleVariant": "大丈夫。よくわかった",
            "maleVariant": "大丈夫だ。よくわかった"
          },
          {
            "stringId": 1724204416878039040,
            "femaleVariant": "質問はもうない"
          },
          {
            "stringId": 1724204660047007744,
            "femaleVariant": "行きも帰りもデラマンに乗ってくの？",
            "maleVariant": "行きも帰りもデラマンに乗ってくのか？"
          },
          {
            "stringId": 1724204703013457920,
            "femaleVariant": "どんな偽名を使うの？",
            "maleVariant": "どんな偽名を使うんだ？"
          },
          {
            "stringId": 1724204799868325888,
            "femaleVariant": "大丈夫そう",
            "maleVariant": "大丈夫だ"
          },
          {
            "stringId": 1724204845066145792,
            "femaleVariant": "OK、大体理解できた"
          },
          {
            "stringId": 1724207133193195520,
            "femaleVariant": "ペントハウスにはどうやって入るの？",
            "maleVariant": "ペントハウスにはどうやって入るんだ？"
          },
          {
            "stringId": 1724207582738698240,
            "femaleVariant": "素晴らしい"
          },
          {
            "stringId": 1724208004719235072,
            "femaleVariant": "ジャッキー、仕方ないよ",
            "maleVariant": "ジャッキー、仕方ない"
          },
          {
            "stringId": 1724208004987670528,
            "femaleVariant": "そうだぞ。フラットヘッドを調達して、ブレインダンスで情報を集めたのは誰だ？"
          },
          {
            "stringId": 1724208005021224960,
            "femaleVariant": "仕方ないって。構わないでしょ",
            "maleVariant": "仕方ない。構わないだろ"
          },
          {
            "stringId": 1724209704855195652,
            "femaleVariant": "金はそこまで重要じゃない"
          },
          {
            "stringId": 1724209880026107904,
            "femaleVariant": "最後にひとつ。紺碧は武器の持ち込み厳禁だ。セキュリティゲートがある"
          },
          {
            "stringId": 1724210353865019392,
            "femaleVariant": "わかった。あんたに従う"
          },
          {
            "stringId": 1724210473016807424,
            "femaleVariant": "金はそこまで重要じゃないよ、ジャック",
            "maleVariant": "金はそこまで重要じゃないだろ、ジャック"
          },
          {
            "stringId": 1724210520731209728,
            "femaleVariant": "わかった。あんたに従う"
          },
          {
            "stringId": 1724254502169399296,
            "femaleVariant": "移動にデラマンを使うの？",
            "maleVariant": "移動にデラマンを使うのか？"
          },
          {
            "stringId": 1724272059744047104,
            "femaleVariant": "偽名って… 誰になりすますの？",
            "maleVariant": "偽名って… 誰になりすますんだ？"
          },
          {
            "stringId": 1724272831680532480,
            "femaleVariant": "ビジネス――武器取引さ。聞かれたら商談があると言え。相手はアラサカの防衛担当… ハジメ・タキだ"
          },
          {
            "stringId": 1724285183855480832,
            "femaleVariant": "帰りも、デラマンがお前たちをここまで送り届けるが――"
          },
          {
            "stringId": 1724285244790329344,
            "femaleVariant": "っていうと…？"
          },
          {
            "stringId": 1724285396154372096,
            "femaleVariant": "ノーテル・モーテル――あそこなら、余計な詮索を受けずに済む。だがまあ、きっと使うことにはならないだろう"
          },
          {
            "stringId": 1724301559693365248,
            "femaleVariant": "ペントハウスにはどうやって？"
          },
          {
            "stringId": 1724304051244158976,
            "femaleVariant": "手はずは私たちが整えておいた。作戦自体はシンプルよ"
          },
          {
            "stringId": 1724304566086586368,
            "femaleVariant": "具体的には？ 聞かせてくれ"
          },
          {
            "stringId": 1724304763101433856,
            "femaleVariant": "まずは… デラマンに乗って紺碧まで行ってもらう。偽名を使って、正面から堂々と入れ"
          },
          {
            "stringId": 1724306932127027200,
            "femaleVariant": "他に質問は？"
          },
          {
            "stringId": 1724309093082779648,
            "femaleVariant": "当然、隠密行動が基本よ。犠牲者ゼロが理想ね。いい？"
          },
          {
            "stringId": 1724309884849934336,
            "femaleVariant": "バグが常時無線で援護する"
          },
          {
            "stringId": 1724310178031783936,
            "femaleVariant": "何か質問はあるか？"
          },
          {
            "stringId": 1724322454189166592,
            "femaleVariant": "フラットヘッドを使うのよ"
          },
          {
            "stringId": 1724322635399876608,
            "femaleVariant": "換気シャフトから侵入させて、ドゥウェラーのところまで行ってもらう。あとは、わかるでしょ"
          },
          {
            "stringId": 1724324756157419520,
            "femaleVariant": "待てよ。ここに集まった肝心の目的がまだだ"
          },
          {
            "stringId": 1724327409373466624,
            "femaleVariant": "ジャッキーの言うとおり、それじゃ割に合わない"
          },
          {
            "stringId": 1724327655260344320,
            "femaleVariant": "本気かよ？"
          },
          {
            "stringId": 1724327721966555136,
            "femaleVariant": "のむしかない。それに、これで十分よ",
            "maleVariant": "のむしかない。それに、これで十分だ"
          },
          {
            "stringId": 1724331941386047488,
            "femaleVariant": "それにアラサカだらけのホテルに乗り込み、奴らの御曹司のオモチャを盗むのも俺たちなんだぞ？"
          },
          {
            "stringId": 1724332125146894336,
            "femaleVariant": "では聞くが、車や一流のネットランナー、情報を用意しているのは誰だ？"
          },
          {
            "stringId": 1724332275302977536,
            "femaleVariant": "交渉できる立場とは思えんがな"
          },
          {
            "stringId": 1724333465512235008,
            "femaleVariant": "ジャッキーの言うとおりよ。30%じゃ、仕事の内容と釣り合わない",
            "maleVariant": "ジャッキーの言うとおりだ。30%じゃ、仕事の内容と釣り合わない"
          },
          {
            "stringId": 1724333806710476800,
            "femaleVariant": "挑戦することよ",
            "maleVariant": "挑戦することだ"
          },
          {
            "stringId": 1724333806710476816,
            "femaleVariant": "名を残すことよ",
            "maleVariant": "名を残すことだ"
          },
          {
            "stringId": 1724333806710476832,
            "femaleVariant": "報復かな？"
          },
          {
            "stringId": 1724334039410462720,
            "femaleVariant": "野心よ。挑戦することに意味がある",
            "maleVariant": "野心だ。挑戦することに意味がある"
          },
          {
            "stringId": 1724334039444017152,
            "femaleVariant": "望むことはただ一つ、名を残すことよ",
            "maleVariant": "望むことはただ一つ、名を残すことだ"
          },
          {
            "stringId": 1724334039477571584,
            "femaleVariant": "そろそろ誰かがアラサカに報復してもいい頃でしょ…",
            "maleVariant": "そろそろ誰かがアラサカに報復してもいい頃だろ…"
          },
          {
            "stringId": 1724334360459268096,
            "femaleVariant": "敬意を表して、取り分を35にしてやろう"
          },
          {
            "stringId": 1724344037993664512,
            "femaleVariant": "はあ？ 何言ってんだ、V？"
          },
          {
            "stringId": 1724344348607041536,
            "femaleVariant": "面白い。なら何が重要だと言うんだ？"
          },
          {
            "stringId": 1724344890561449984,
            "femaleVariant": "賢明だな"
          },
          {
            "stringId": 1724345055548592128,
            "femaleVariant": "はあ… では35。それ以上は無理だ"
          },
          {
            "stringId": 1724345551835418624,
            "femaleVariant": "35か… それで手を打とう"
          },
          {
            "stringId": 1724345729405472768,
            "femaleVariant": "ふん。この俺と交渉しようなんて奴は久しぶりだな"
          },
          {
            "stringId": 1724347721683095552,
            "femaleVariant": "若いのに随分と立派なことを言うじゃないか"
          },
          {
            "stringId": 1724347905292947456,
            "femaleVariant": "よく言った。この街に名もなき者の居場所はない"
          },
          {
            "stringId": 1724348131080720384,
            "femaleVariant": "過去との決別は大事だ"
          },
          {
            "stringId": 1724376268401332224,
            "femaleVariant": "銃は車の中に置いて、フラットヘッドをケースに入れて持っていけ"
          },
          {
            "stringId": 1724377841969975296,
            "femaleVariant": "それらしく見えるよう、スーツも用意しておいたから"
          },
          {
            "stringId": 1724387410603462656,
            "femaleVariant": "ああ、“エクセルシオール”で頼む…"
          },
          {
            "stringId": 1724387410603462672,
            "femaleVariant": "支払いはいつもどおり、現ナマで。じゃあな"
          },
          {
            "stringId": 1724387410653794304,
            "femaleVariant": "ついにファミリー全員がそろったな"
          },
          {
            "stringId": 1724387410754457600,
            "femaleVariant": "ようこそ、ミスV",
            "maleVariant": "ようこそ、ミスターV"
          },
          {
            "stringId": 1724404035398455296,
            "femaleVariant": "[座る]"
          },
          {
            "stringId": 1724404035532673036,
            "femaleVariant": "これを"
          },
          {
            "stringId": 1724404035599781888,
            "femaleVariant": "[チップを取る] 見せてもらう"
          },
          {
            "stringId": 1724409002511355904,
            "femaleVariant": "この計画をどう思う？"
          },
          {
            "stringId": 1724409002528133120,
            "femaleVariant": "デクスとはどういう関係なの？",
            "maleVariant": "デクスとはどういう関係なんだ？"
          },
          {
            "stringId": 1724409002612019200,
            "femaleVariant": "この計画――どう思う？"
          },
          {
            "stringId": 1724409002628796416,
            "femaleVariant": "やり遂げれば、ようやくクレタ島に豪邸を買って、サイバー稼業から足を洗えるかもね"
          },
          {
            "stringId": 1724409002628796432,
            "femaleVariant": "悪いけど… そうなったら、きっと過去とは決別してるから"
          },
          {
            "stringId": 1724409002712682496,
            "femaleVariant": "デクスとは長い付き合いなの？",
            "maleVariant": "デクスとは長い付き合いなのか？"
          },
          {
            "stringId": 1724409002729459724,
            "femaleVariant": "ええと… どうして？"
          },
          {
            "stringId": 1724409002729459740,
            "femaleVariant": "色んな噂話が、耳に入ってくるからよ"
          },
          {
            "stringId": 1724409002746236932,
            "femaleVariant": "「耳に入るもの全ては意見であり、事実ではない」。アウレリウスの言葉よ"
          },
          {
            "stringId": 1724409003031449600,
            "femaleVariant": "じゃ、頑張りましょ"
          },
          {
            "stringId": 1724421829095641088,
            "femaleVariant": "ほら"
          },
          {
            "stringId": 1724439376016756736,
            "femaleVariant": "至れり尽くせりだな"
          },
          {
            "stringId": 1754990122054184960,
            "femaleVariant": "デクス、30%は低すぎる"
          },
          {
            "stringId": 1756243230520504324,
            "femaleVariant": "ちょっと少なくない？",
            "maleVariant": "ちょっと少なくないか？"
          },
          {
            "stringId": 1770783210117750784,
            "femaleVariant": "V、デクスに見せてやろうぜ"
          },
          {
            "stringId": 1785209981909127168,
            "femaleVariant": "ハッスルはほとんどいないけど、問題は部屋のセキュリティね"
          },
          {
            "stringId": 1785212574727196672,
            "femaleVariant": "待てよ、入れない部屋にどうやって入れって言うんだ？"
          },
          {
            "stringId": 1786443766465032192,
            "femaleVariant": "他に何かあれば、今のうちに言って"
          },
          {
            "stringId": 1786444846548643840,
            "femaleVariant": "もう大丈夫。聞くことはない",
            "maleVariant": "もう大丈夫だ。聞くことはない"
          },
          {
            "stringId": 1786445722017329152,
            "femaleVariant": "大丈夫。もう聞くことはない",
            "maleVariant": "大丈夫だ。もう聞くことはない"
          },
          {
            "stringId": 1790723717528866816,
            "femaleVariant": "ありがと",
            "maleVariant": "助かるよ"
          },
          {
            "stringId": 1790771748349464576,
            "femaleVariant": "じゃ、お言葉に甘えて"
          },
          {
            "stringId": 1800975128058159104,
            "femaleVariant": "アパートでの支援、ありがとう",
            "maleVariant": "アパートでの支援、助かったよ"
          },
          {
            "stringId": 1800976505215934464,
            "femaleVariant": "あれで街のゴミが少しは減ったかな"
          },
          {
            "stringId": 1801145176869593088,
            "femaleVariant": "絵ハガキに期待しとく",
            "maleVariant": "絵ハガキに期待しとくよ"
          },
          {
            "stringId": 1852040796507467780,
            "femaleVariant": "バグ、スカベンジャーの件で手伝ってくれてありがとう"
          },
          {
            "stringId": 1866384261048578052,
            "femaleVariant": "[立ち上がる] 行きましょ",
            "maleVariant": "行こう"
          },
          {
            "stringId": 1866385467464626176,
            "femaleVariant": "さて… 準備はいいか？"
          },
          {
            "stringId": 1866390322153402368,
            "femaleVariant": "[立ち上がる] 先に片付けたいことがある"
          },
          {
            "stringId": 1866391453860507648,
            "femaleVariant": "行きましょ。善は急げよ",
            "maleVariant": "行こう。善は急げだ"
          },
          {
            "stringId": 1866392246432968704,
            "femaleVariant": "“銭”じゃなくてか？"
          },
          {
            "stringId": 1866394191885381632,
            "femaleVariant": "ちょっと待っててもらえる？",
            "maleVariant": "ちょっと待っててもらえるか？"
          },
          {
            "stringId": 1866394972076257280,
            "femaleVariant": "わかった。じゃあデラマンのとこで会おう"
          },
          {
            "stringId": 1894178268167008256,
            "femaleVariant": "いつまで突っ立ってるつもりだ？"
          },
          {
            "stringId": 1894332070996209664,
            "femaleVariant": "スカベンジャーの件、手伝ってくれてありがとう",
            "maleVariant": "スカベンジャーの件、手伝ってくれて助かった"
          },
          {
            "stringId": 1898125072844771328,
            "femaleVariant": "ナイトシティ一のタクシー会社だ… 悪くないな…"
          },
          {
            "stringId": 1898134698923446272,
            "femaleVariant": "トラブった場合は、セーフハウスに行ってもらう"
          },
          {
            "stringId": 1898194527599775744,
            "femaleVariant": "なんだ、お前も哲学オタクか。どうりでデクスと気が合うわけだ"
          },
          {
            "stringId": 1898199790011543552,
            "femaleVariant": "さて、と…"
          },
          {
            "stringId": 1902710645647618048,
            "femaleVariant": "スーツを忘れるなよ"
          },
          {
            "stringId": 1927027160366522368,
            "femaleVariant": "別の申し入れ？"
          },
          {
            "stringId": 1927027365568651264,
            "femaleVariant": "ええ。フィクサー抜きで、儲けを山分けにしないかって",
            "maleVariant": "ああ。フィクサー抜きで、儲けを山分けにしないかってな"
          },
          {
            "stringId": 1927028281923411968,
            "femaleVariant": "うまくいったと思う。彼女が記録したナマモノのBDで、紺碧プラザの中を歩き回れた"
          },
          {
            "stringId": 1927030536076599296,
            "femaleVariant": "ああ、バグからすでに聞いている"
          },
          {
            "stringId": 1927030741396168704,
            "femaleVariant": "で、彼女は打ち合わせ以外に何か言ってたか？"
          },
          {
            "stringId": 1927031733013520384,
            "femaleVariant": "..."
          },
          {
            "stringId": 1927049764729069568,
            "femaleVariant": "それだけよ。計画の話をしましょ",
            "maleVariant": "それだけだ。計画の話をしよう"
          },
          {
            "stringId": 1927052855394246656,
            "femaleVariant": "エヴリンはあんたを切りたがってる"
          },
          {
            "stringId": 1927054158950375424,
            "femaleVariant": "話してくれて感謝する、ミスV。信頼ってのは――",
            "maleVariant": "話してくれて感謝する、ミスターV。信頼ってのは――"
          },
          {
            "stringId": 1927055596271247360,
            "femaleVariant": "長く、実りある関係を続けるのに必要不可欠だ"
          },
          {
            "stringId": 1927056812183199744,
            "femaleVariant": "フィクサー抜きで？"
          },
          {
            "stringId": 1927058431301984256,
            "femaleVariant": "あたしと会ってみたかっただけみたい",
            "maleVariant": "俺と会ってみたかっただけのようだ"
          },
          {
            "stringId": 1927059266807341056,
            "femaleVariant": "うまくいったよ。すっかり気に入られて、別の申し入れをしてきたくらい",
            "maleVariant": "うまくいったよ。すっかり気に入られて、別の申し入れをしてきたくらいだ"
          },
          {
            "stringId": 1927059809298620416,
            "femaleVariant": "実を言うと、パーカーは別の申し入れをしてきたの",
            "maleVariant": "実を言うと、パーカーは別の申し入れをしてきた"
          },
          {
            "stringId": 1927115458233573376,
            "femaleVariant": "エヴリンはどうも怪しい"
          },
          {
            "stringId": 1927115840183672832,
            "femaleVariant": "パーカーは… 言動がどうもちぐはぐで、どう受け取っていいかわからない"
          },
          {
            "stringId": 1927121786700111872,
            "femaleVariant": "ハハハ、そいつはまた大胆だな…"
          },
          {
            "stringId": 1927123685394423808,
            "femaleVariant": "ハハハ… ワハハハハ！"
          },
          {
            "stringId": 1927125129979830272,
            "femaleVariant": "なかなか困ったお客様だ"
          },
          {
            "stringId": 1927129305812115460,
            "femaleVariant": "仕事の話をしましょ",
            "maleVariant": "仕事の話をしよう"
          },
          {
            "stringId": 1927131811069894656,
            "femaleVariant": "感謝だけじゃ足りない"
          },
          {
            "stringId": 1927133979978047488,
            "femaleVariant": "もっと怒るかと思った"
          },
          {
            "stringId": 1927135927242399744,
            "femaleVariant": "聞いたら怒り狂うかと思ったのに",
