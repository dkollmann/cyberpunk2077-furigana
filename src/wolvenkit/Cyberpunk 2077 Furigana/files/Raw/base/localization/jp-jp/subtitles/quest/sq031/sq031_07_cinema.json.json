  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シルバー・ピクセルだけはまだ残ってる",
              "maleVariant": "",
              "stringId": "1321835944315936768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それでだ… V、本当のことを教えな",
              "maleVariant": "",
              "stringId": "1321836921689432064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "デートには最高の場所ね",
              "maleVariant": "デートには最高の場所だな",
              "stringId": "1570532363560771584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当って？",
              "maleVariant": "",
              "stringId": "1570536269095329792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ。信じられねえ",
              "maleVariant": "",
              "stringId": "1570763549688160256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しばらく経つからね",
              "maleVariant": "",
              "stringId": "1570768282993778688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "完全に潰れちまったか",
              "maleVariant": "",
              "stringId": "1570768405903663104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "入口はこっちだ",
              "maleVariant": "",
              "stringId": "1570928690224390144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "『ブシドーX - フェードアウト』。今じゃ懐かしの映画だろ",
              "maleVariant": "",
              "stringId": "1570967026733375488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今日はおとり役でもするのか？",
              "maleVariant": "",
              "stringId": "1760902348966420480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "褒めてるつもり？",
              "maleVariant": "",
              "stringId": "1760903041127243776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつもと… 違うから",
              "maleVariant": "",
              "stringId": "1760903459920109568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やり直し",
              "maleVariant": "",
              "stringId": "1760904214458621952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグ。準備はいい？",
              "maleVariant": "ローグ。準備はいいか？",
              "stringId": "1760905606564896768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またあんたか",
              "maleVariant": "",
              "stringId": "1760907709404372992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行ける？",
              "maleVariant": "行けるか？",
              "stringId": "1760908046978736128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーはどこ？ やっぱりすっぽかすのかい",
              "maleVariant": "",
              "stringId": "1760909665694867456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "後で来るよ",
              "maleVariant": "後で来る",
              "stringId": "1760910851189727236"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはただの運転手",
              "maleVariant": "俺はただの運転手だ",
              "stringId": "1760911134741454848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはただの運転手",
              "maleVariant": "俺はただの運転手だ",
              "stringId": "1760911822171103232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "後で来る",
              "maleVariant": "",
              "stringId": "1760913731502170112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドライブイン式はずいぶん時代遅れだけど――",
              "maleVariant": "",
              "stringId": "1760951635209314304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だろ",
              "maleVariant": "",
              "stringId": "1760952662998343680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "閉まってても関係ない。あたしたちに任せといて",
              "maleVariant": "閉まってても関係ない。俺たちに任せとけ",
              "stringId": "1760966723462979584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグ？",
              "maleVariant": "",
              "stringId": "1761109194507509760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと考えててね",
              "maleVariant": "",
              "stringId": "1761109194507509772"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無私無欲のジョニーなんて",
              "maleVariant": "",
              "stringId": "1761111209451151360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シルバー・ピクセルは、あんたのお気に入り？",
              "maleVariant": "シルバー・ピクセルは、あんたのお気に入りか？",
              "stringId": "1761147999335702528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "着いた",
              "maleVariant": "",
              "stringId": "1761151765820694528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "映写機らしいのを見つけた",
              "maleVariant": "",
              "stringId": "1761253806861672448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとな",
              "maleVariant": "",
              "stringId": "1779967069262667776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいのよ",
              "maleVariant": "いいんだ",
              "stringId": "1779967893040750592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "前はね。もうずっと行ってない",
              "maleVariant": "",
              "stringId": "1780003495685611520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいよ、やってみな。私は見てる",
              "maleVariant": "",
              "stringId": "1781967527372087296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[薬を飲む] 楽しんで",
              "maleVariant": "[薬を飲む] 楽しめよ",
              "stringId": "1782086298149543940"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2人とも楽しんで",
              "maleVariant": "2人とも楽しめよ",
              "stringId": "1782091963983159296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺が連れてくはずだった場所だ。50年前にな",
              "maleVariant": "",
              "stringId": "1822873091378057216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく今回は街中で暴れまわらないで。タトゥーもなし。無駄遣いもダメよ",
              "maleVariant": "とにかく今回は街中で暴れまわるな。タトゥーもなし。無駄遣いもダメだからな",
              "stringId": "1822891917561892864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3回くらいやれば、気の利いた台詞が出てくるだろ？",
              "maleVariant": "",
              "stringId": "1822904972383424512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すごくきれい",
              "maleVariant": "すごく綺麗だ",
              "stringId": "1822918847761813504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すごく… 綺麗",
              "maleVariant": "すごく… 綺麗だな",
              "stringId": "1822920251243040768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはどっちが言ったんだい？ ジョニーはいつ来るのさ",
              "maleVariant": "",
              "stringId": "1822921934366904320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつを尻に敷いてるんだね",
              "maleVariant": "",
              "stringId": "1822925555527651328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "歳のわりにやるじゃないか",
              "maleVariant": "",
              "stringId": "1822926432422400000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつの案じゃなかったんだ",
              "maleVariant": "あいつの案じゃなかったのか",
              "stringId": "1823030719949377536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だからジョニーに連れてって、って頼んだんだ。大昔にね",
              "maleVariant": "",
              "stringId": "1823034147853373440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさか",
              "maleVariant": "",
              "stringId": "1823035520934285312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつのデートプランなんて、アラサカのビル爆破ぐらいのもんだね",
              "maleVariant": "",
              "stringId": "1823044875658268672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーがやることにはいつだって裏がある",
              "maleVariant": "",
              "stringId": "1824439212782718976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今回は何が目的なんだ？",
              "maleVariant": "",
              "stringId": "1824444991325671424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーは変わったの",
              "maleVariant": "ジョニーは変わったんだ",
              "stringId": "1824447356091043840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたと一緒にいたいだけよ",
              "maleVariant": "あんたと一緒にいたいだけさ",
              "stringId": "1824447754969354240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だますつもりも、下心もない",
              "maleVariant": "",
              "stringId": "1824448077511331840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふうん…",
              "maleVariant": "",
              "stringId": "1824448721739649024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたがいい影響を与えてんのかね",
              "maleVariant": "",
              "stringId": "1824454395139788800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、中に入れ",
              "maleVariant": "",
              "stringId": "1824462193894428672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、これがパスワードね",
              "maleVariant": "よし、これがパスワードだな",
              "stringId": "1881339081475899392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当に映画が観れるんだね",
              "maleVariant": "",
              "stringId": "1881339798450225152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジャジャジャジャーン",
              "maleVariant": "",
              "stringId": "1895759908900323328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、行けそう！",
              "maleVariant": "よし、行けそうだ！",
              "stringId": "1895773731279486976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よくやった",
              "maleVariant": "",
              "stringId": "1895808615289077760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "閉まってる。何年もたってるからね",
              "maleVariant": "",
              "stringId": "1913960967086821376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "安心して。実は前に来たことがあるの",
              "maleVariant": "ご安心を。実は前に来たことがあるんだ",
              "stringId": "1913965546880913408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "入り方を知ってる。あたしたちの貸し切りよ",
              "maleVariant": "入り方を知ってる。俺たちの貸し切りだ",
              "stringId": "1913985264807333888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ",
              "maleVariant": "",
              "stringId": "1913988120708927488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "準備はいい？",
              "maleVariant": "準備はいいか？",
              "stringId": "1922721542259339268"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[使う] 見てみよう",
              "maleVariant": "",
              "stringId": "1922734488901382148"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "動かし方はお前のほうでなんとかなるか？",
              "maleVariant": "",
              "stringId": "1934328665788403712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "映写機を回してみな。私は席を選んでおく",
              "maleVariant": "",
              "stringId": "1953427813204639744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": "1953430858034462720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、映写機を調べに行こう",
              "maleVariant": "",
              "stringId": "1953435073326759936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見てみよう",
              "maleVariant": "見てみるか",
              "stringId": "1953442911105204224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お、最後に再生されたのは『ブシドーX』だ！ 完璧だな",
              "maleVariant": "",
              "stringId": "1953443484701442048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どう見てもC級映画でしょ",
              "maleVariant": "どう見てもC級映画だろ",
              "stringId": "1953444269606715392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、V。少し2人きりにしてくれ",
              "maleVariant": "",
              "stringId": "1953445991049740292"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのなV、ここはドライブインだぞ。内容なんて誰も気にしない",
              "maleVariant": "",
              "stringId": "1954552420875653120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと女の裸が見れて、血が噴き出りゃいいんだ。背景音みたいなもんさ",
              "maleVariant": "",
              "stringId": "1954552785780101120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "『ブシドー』の宣伝文句ね",
              "maleVariant": "『ブシドー』の宣伝文句だな",
              "stringId": "1954553932788027392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行こう",
              "maleVariant": "",
              "stringId": "1962231657350971396"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーの… ポルシェ？",
              "maleVariant": "",
              "stringId": "1962275180083298304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこでこれを？",
              "maleVariant": "",
              "stringId": "1962276016477847552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "グレイソンが話してた、あのコンテナに入ってたの",
              "maleVariant": "グレイソンが話してた、あのコンテナに入ってたんだ",
              "stringId": "1962276317897310208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ… この車にはいろんな思い出が詰まってる",
              "maleVariant": "",
              "stringId": "1962276661528248320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "閉まってても関係ない",
              "maleVariant": "",
              "stringId": "2074766443780390916"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[夜まで待つ]",
              "maleVariant": "",
              "stringId": "2511473844437118980"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}