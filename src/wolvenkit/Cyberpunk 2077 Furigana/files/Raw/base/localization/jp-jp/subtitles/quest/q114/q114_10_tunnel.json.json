    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1457524143908995072,
            "femaleVariant": "ここらで分かれよう"
          },
          {
            "stringId": 1549142580350959616,
            "femaleVariant": "自動操縦に切り替えて接続を解除する"
          },
          {
            "stringId": 1549145911265869828,
            "femaleVariant": "計画が甘かった"
          },
          {
            "stringId": 1549146359318200320,
            "femaleVariant": "計画通りに進んでないから、心配なの",
            "maleVariant": "計画通りに進んでないから、心配なんだ"
          },
          {
            "stringId": 1549148276299677696,
            "femaleVariant": "ボブとテディーを失った"
          },
          {
            "stringId": 1549149471424659456,
            "femaleVariant": "こんなはずじゃなかった。この先だって、うまくいくか…"
          },
          {
            "stringId": 1549150388148518912,
            "femaleVariant": "ボブとテディーを失った。あたしのせいだ",
            "maleVariant": "ボブとテディーを失った。俺のせいだ"
          },
          {
            "stringId": 1549151462309773312,
            "femaleVariant": "ええ…",
            "maleVariant": "ああ…"
          },
          {
            "stringId": 1549152837236178944,
            "femaleVariant": "こんな計画を考えたのが… そもそも間違ってたのよ",
            "maleVariant": "こんな計画を考えたのが… そもそも間違ってたんだ"
          },
          {
            "stringId": 1708878631188320256,
            "femaleVariant": "あんたの考えじゃない"
          },
          {
            "stringId": 1801175986296639488,
            "femaleVariant": "そうだね。キャロルとキャシディはここを死守して"
          },
          {
            "stringId": 1801176137945894912,
            "femaleVariant": "わかった"
          },
          {
            "stringId": 1801176216329048064,
            "femaleVariant": "ミッチは私たちと一緒に"
          },
          {
            "stringId": 1801176374303313920,
            "femaleVariant": "イエス、マム"
          },
          {
            "stringId": 1801176456025133056,
            "femaleVariant": "時間がない。行こう"
          },
          {
            "stringId": 1801251916453040128,
            "femaleVariant": "こうなるとわかってれば…"
          },
          {
            "stringId": 1801252930165985280,
            "femaleVariant": "大丈夫？ 何か様子が変だけど"
          },
          {
            "stringId": 1801256149411745792,
            "femaleVariant": "じゃあ誰のよ？",
            "maleVariant": "じゃあ誰のだ？"
          },
          {
            "stringId": 1801256182009876480,
            "femaleVariant": "私が考えた。だから責任は私にある"
          },
          {
            "stringId": 1801256588572151808,
            "femaleVariant": "あたしのせいよ",
            "maleVariant": "俺のせいだ"
          },
          {
            "stringId": 1801256761142595584,
            "femaleVariant": "ボブもテディーも… 危険は承知だった。みんなそうだ"
          },
          {
            "stringId": 1801256908513660928,
            "femaleVariant": "私もね"
          },
          {
            "stringId": 1801257424446607360,
            "femaleVariant": "心配したってしょうがないでしょ"
          },
          {
            "stringId": 1801257464326049792,
            "femaleVariant": "何よ。あんたを優先してるんじゃないかって？"
          },
          {
            "stringId": 1801257784569548800,
            "femaleVariant": "ええ、そう",
            "maleVariant": "ああ、そうだ"
          },
          {
            "stringId": 1801257811949965312,
            "femaleVariant": "あんた、自分で何言ってるかわかってるの？"
          },
          {
            "stringId": 1801257929910571008,
            "femaleVariant": "この作戦や仲間とはまったく関係ない。責任は全部私にあるんだ。あんたは悪くない"
          },
          {
            "stringId": 1801257953398673408,
            "femaleVariant": "二人のことは二人のこと"
          },
          {
            "stringId": 1801258337412370432,
            "femaleVariant": "それでもやってた。ファミリーのためならね"
          },
          {
            "stringId": 1887891435360481280,
            "femaleVariant": "パナム… もしパナムが、あたしを…",
            "maleVariant": "パナム… もしパナムが、俺を…"
          },
          {
            "stringId": 2025706785119793152,
            "femaleVariant": "〈神輿〉だけど――外部の協力者が一人いるってソウルから聞いた"
          },
          {
            "stringId": 2025708101258833920,
            "femaleVariant": "ええ、オルトよ",
            "maleVariant": "ああ、オルトだ"
          },
          {
            "stringId": 2025708294817574912,
            "femaleVariant": "何者なの？"
          },
          {
            "stringId": 2025708823920635908,
            "femaleVariant": "AIよ",
            "maleVariant": "AIだ"
          },
          {
            "stringId": 2025709642095128576,
            "femaleVariant": "ナイトシティで一番のネットランナーよ",
            "maleVariant": "ナイトシティで一番のネットランナーだ"
          },
          {
            "stringId": 2025711607864750080,
            "femaleVariant": "ジョニー・シルヴァーハンドの元カノなの",
            "maleVariant": "ジョニー・シルヴァーハンドの元カノなんだ"
          },
          {
            "stringId": 2025712307726315520,
            "femaleVariant": "オルトは… はぐれAI"
          },
          {
            "stringId": 2025712405386489856,
            "femaleVariant": "ネットランナーのトップに君臨する、女王よ",
            "maleVariant": "ネットランナーのトップに君臨する、女王だ"
          },
          {
            "stringId": 2025712526702538752,
            "femaleVariant": "そうね。簡単に言えば――ジョニーの元カノなの",
            "maleVariant": "そうだな。簡単に言えば――ジョニーの元カノなんだ"
          },
          {
            "stringId": 2025713597827117056,
            "femaleVariant": "AIが協力者ってこと？ もう何を聞いても驚かないね…"
          },
          {
            "stringId": 2025717611474055168,
            "femaleVariant": "アラサカのシステムに侵入できるのは彼女だけ",
            "maleVariant": "アラサカのシステムに侵入できるのは彼女だけだ"
          },
          {
            "stringId": 2025718603376619520,
            "femaleVariant": "つまり、彼女なしじゃ〈神輿〉に入れない？"
          },
          {
            "stringId": 2025718778648195072,
            "femaleVariant": "少なくとも出られないのは確かね",
            "maleVariant": "少なくとも出られないのは確かだな"
          },
          {
            "stringId": 2025719075789467648,
            "femaleVariant": "あんたは、そいつを信用してるの？"
          },
          {
            "stringId": 2025719910338523140,
            "femaleVariant": "信用してる"
          },
          {
            "stringId": 2025720125674090496,
            "femaleVariant": "信用してない"
          },
          {
            "stringId": 2025722920070410240,
            "femaleVariant": "女王だった、か"
          },
          {
            "stringId": 2025724618746753024,
            "femaleVariant": "ネットランナーをやめたってこと？"
          },
          {
            "stringId": 2025724922666020864,
            "femaleVariant": "まあね。“住処”も違う",
            "maleVariant": "まあな。“住処”も違う"
          },
          {
            "stringId": 2025725542751924224,
            "femaleVariant": "でも助けてくれる",
            "maleVariant": "だが助けてくれる"
          },
          {
            "stringId": 2025725708091387904,
            "femaleVariant": "ふうん… 信用してるの？"
          },
          {
            "stringId": 2025726896455151616,
            "femaleVariant": "冗談でしょ…"
          },
          {
            "stringId": 2025727061291298816,
            "femaleVariant": "本当よ。でもネットに関して、オルトの右に出る者はいない",
            "maleVariant": "本当だ。でもネットに関して、オルトの右に出る者はいない"
          },
          {
            "stringId": 2025727725652275200,
            "femaleVariant": "彼女が〈神輿〉へのチケットなの",
            "maleVariant": "彼女が〈神輿〉へのチケットなんだ"
          },
          {
            "stringId": 2025728384409661440,
            "femaleVariant": "ジョニーにオルト… 世界は狭いというか…"
          },
          {
            "stringId": 2025728845028126720,
            "femaleVariant": "信用してるの？"
          },
          {
            "stringId": 2025729693670043648,
            "femaleVariant": "ええ。信用してる",
            "maleVariant": "ああ。信用してる"
          },
          {
            "stringId": 2025733030305984512,
            "femaleVariant": "信用はしてないけど、他に選択肢がない。オルトが何か別のことを企んでいないよう、祈るだけ",
            "maleVariant": "信用はしてないが、他に選択肢がない。オルトが何か別のことを企んでいないよう、祈るだけだ"
          },
          {
            "stringId": 2025733367125372928,
            "femaleVariant": "私は… あんたを信じるだけ"
          },
          {
            "stringId": 2025734068597551104,
            "femaleVariant": "そのあんたが信じるなら、オルトってのも信じるよ"
          },
          {
            "stringId": 2025734739652636672,
            "femaleVariant": "きっとうまくいく。ここまで来て、失敗なんてありえない"
          },
          {
            "stringId": 2025737531196829696,
            "femaleVariant": "何か心配ごと？",
            "maleVariant": "何か心配ごとか？"
          },
          {
            "stringId": 2025738035335393280,
            "femaleVariant": "何か、心配してることがあるの？",
            "maleVariant": "何か、心配してることがあるのか？"
          },
          {
            "stringId": 2237928257921765376,
            "femaleVariant": "ちょっと… ちゃんと答えてよ、V"
          }
        ]
      }
    }
  }
}