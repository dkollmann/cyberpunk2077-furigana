  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ワトソンについて教えて",
              "maleVariant": "ワトソンについて教えてくれ",
              "stringId": "1956220983743279108"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レジーナ、ワトソンを一言で表すとしたら？",
              "maleVariant": "",
              "stringId": "1956221285481508864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんた… 前はメディアにいた？",
              "maleVariant": "あんた… 前はメディアにいたか？",
              "stringId": "1956221292779597824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ロイスという男について、何か知ってる？",
              "maleVariant": "ロイスという男について、何か知ってるか？",
              "stringId": "1956225175463587840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とある人物について情報が欲しいんだけど",
              "maleVariant": "とある人物について情報が欲しいんだが",
              "stringId": "1956225321073045508"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふぅん、名前は？",
              "maleVariant": "",
              "stringId": "1956225704130441216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サイモン・ランドール。通称ロイスよ",
              "maleVariant": "サイモン・ランドール。通称ロイスだ",
              "stringId": "1956225767766421504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メイルストロームね。デクスが彼に興味を示してるって噂を聞いた",
              "maleVariant": "",
              "stringId": "1956225825463267328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他のフィクサーと同じことを言うか確かめてるの？ 賢いけど、賢明じゃないわね",
              "maleVariant": "",
              "stringId": "1956225934347399168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "元記者なのよね",
              "maleVariant": "元記者なんだよな",
              "stringId": "1956226567821520896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "隠してるつもりもないけど、察しがいいってことにしといてあげる",
              "maleVariant": "",
              "stringId": "1956228055289491456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてやめたの？",
              "maleVariant": "どうしてやめたんだ？",
              "stringId": "1956228106443223040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やめてはいないわ。情報は武器よ、V。私はただもっと大きな銃に持ち替えただけ",
              "maleVariant": "",
              "stringId": "1956228632979369984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあまた",
              "maleVariant": "",
              "stringId": "1956232568377372672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあね、メディアの嘘には気をつけるのよ",
              "maleVariant": "",
              "stringId": "1956232900717244416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カブキは日本人が築いたけど、今住んでるのはほとんど中国人。ノースサイドは今や廃れきってる",
              "maleVariant": "",
              "stringId": "1956368441882374144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…ワトソンよ",
              "maleVariant": "",
              "stringId": "1956368568181256192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あと、コーポの連中がたくさん詰まった水槽がある。例えじゃなく文字通りにね",
              "maleVariant": "",
              "stringId": "1956378255094116352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一言で、って言ったんだけど",
              "maleVariant": "一言で、って言ったんだが",
              "stringId": "1956378545675497472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V。レジーナ・ジョーンズよ。ワトソンの仕事なら、私に任せて",
              "maleVariant": "",
              "stringId": "1959212455610478592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうやってあたしの連絡先を？ それに名前も",
              "maleVariant": "どうやって俺の連絡先を？ それに名前も",
              "stringId": "1959213887646527488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "情報集めは得意な方なのよ。じゃあね、V",
              "maleVariant": "",
              "stringId": "1959214839350882304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マックスとは仲良くしてる？",
              "maleVariant": "マックスとは仲良くしてるか？",
              "stringId": "1995381535222894592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マックスとはどう？",
              "maleVariant": "マックスとはどうだ？",
              "stringId": "1995382056776208384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "難しいところね",
              "maleVariant": "",
              "stringId": "1995383525688578048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "想像はつく。今のあんたを認めてないんでしょ？",
              "maleVariant": "想像はつく。今のあんたを認めてないんだろ？",
              "stringId": "1995383619640987648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私にだって嫌いなものはある。でも、それを我慢することを覚えたのよ",
              "maleVariant": "",
              "stringId": "1995384526415958016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼はどうするつもりなの？",
              "maleVariant": "彼はどうするつもりなんだ？",
              "stringId": "1995384964737503232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本人もわかってないでしょうね",
              "maleVariant": "",
              "stringId": "1995385055737122816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マックスの話を聞いた",
              "maleVariant": "",
              "stringId": "1995385431026667520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マックスのことだけど――",
              "maleVariant": "マックスのことだが――",
              "stringId": "1995386048679874560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V",
              "maleVariant": "",
              "stringId": "1995386662038114304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なに？",
              "maleVariant": "なんだ？",
              "stringId": "1995386707152048128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話題を変えましょ",
              "maleVariant": "",
              "stringId": "1995386758154784768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サイバーサイコシスの研究はどうなってる？",
              "maleVariant": "",
              "stringId": "2353281384965120000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、あなたは多くのサイバーサイコにやりなおすチャンスを与えてくれた",
              "maleVariant": "",
              "stringId": "2353282682397564928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サイバーサイコの脅威は去ったと言ってもいいわ。Vにはお礼を言わなきゃね",
              "maleVariant": "",
              "stringId": "2353284692912021504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、あなたは1人も犠牲者を出すことなく、サイバーサイコに対処してくれた",
              "maleVariant": "",
              "stringId": "2353285594603495424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく、報酬はあなたのものよ",
              "maleVariant": "",
              "stringId": "2353286366976188416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、デリケートな案件があってね。だからあなたに連絡したの",
              "maleVariant": "",
              "stringId": "2354617741692018688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "街でサイバーサイコによる襲撃事件が増えてる。よくある話だと思うかもしれないけど、私にとっては重要な問題なの。色々とあってね",
              "maleVariant": "",
              "stringId": "2354619319052312576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実は、サイバーサイコシスを治せると言ってる人たちがいるのよ",
              "maleVariant": "",
              "stringId": "2354619536820576256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、もう最近はサイバーサイコの事件をすっかり聞かなくなった。私たちの役目も終わったようね",
              "maleVariant": "",
              "stringId": "2358007747399819264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今度遊びに来て。直接話しましょ",
              "maleVariant": "",
              "stringId": "2358009586199470080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サイバーサイコシスの仕事？ まだないわよ",
              "maleVariant": "",
              "stringId": "2358012066962513920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もっとデータが必要なの。実験療法をやってる団体は… 正直、私も信頼していいかわからない",
              "maleVariant": "",
              "stringId": "2358015254532890624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バカげた話に聞こえるのはわかってる。でも命を奪うより、その実験療法を試すべきだと思うの",
              "maleVariant": "",
              "stringId": "2364822319809650688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今後、サイバーサイコ襲撃の情報が入った時は連絡する。マックス・タックが到着する前に、あなたに調査してほしい",
              "maleVariant": "",
              "stringId": "2364828564289445888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも、忘れないで。あなたを行かせるのは、処刑させるためじゃない。襲撃者の動きを封じてくれたら、回収の車を送るから",
              "maleVariant": "",
              "stringId": "2364831616887119872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼んだわよ、V",
              "maleVariant": "",
              "stringId": "2364837434168664064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "命を救うより、奪うことのほうが簡単だったはず。あなたは本当に立派よ",
              "maleVariant": "",
              "stringId": "2364923180254715904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "簡単にはできないことよ",
              "maleVariant": "",
              "stringId": "2364939915527675904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "発症者の治療も始まってる。たとえ完治できなくても、病気について多くのことがわかるはずよ",
              "maleVariant": "",
              "stringId": "2364946895721947160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私はサイバーウェアが“暴走”しただけなんて話は一切信じてない",
              "maleVariant": "",
              "stringId": "2364946895772278784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつか、このことを記事にできるかもしれないわね…",
              "maleVariant": "",
              "stringId": "2364966194100527104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもまずは、被験体を集めないことには始まらないわ",
              "maleVariant": "",
              "stringId": "2364977114574970880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも正直に言うと、もっと多くの人を救えたはず…",
              "maleVariant": "",
              "stringId": "2365003941225914368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サイバーサイコシスの件？ 特に進展はないわ。でも協力してくれてありがとう",
              "maleVariant": "",
              "stringId": "2365015432394137600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "状況を教えてくれる？",
              "maleVariant": "状況を教えてくれるか？",
              "stringId": "2369211545112240128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあまたね",
              "maleVariant": "じゃあまたな",
              "stringId": "2421308452675690496"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}