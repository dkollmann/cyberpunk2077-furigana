              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ルーシー？",
                    "maleVariant": null,
                    "stringId": 1977789212930707460
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ルーシー？",
                    "maleVariant": null,
                    "stringId": 1977798815118626816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ルーシー？ ここで何してるの？",
                    "maleVariant": "ルーシー？ ここで何してる？",
                    "stringId": 1977799062381236224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしてここに？",
                    "maleVariant": null,
                    "stringId": 1977801245449359360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここにはリパードクが2人必要なの？",
                    "maleVariant": "ここにはリパードクが2人必要なのか？",
                    "stringId": 1977801402970640384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さっさと街の外に拠点を移したらどう？ ここにはもうリパーがいるし",
                    "maleVariant": "さっさと街の外に拠点を移したらどうだ？ ここにはもうリパーがいるだろ",
                    "stringId": 1977801495949971456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしてここに？",
                    "maleVariant": null,
                    "stringId": 1977801637918773248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "静かに。今はルーシーじゃなくてミアよ",
                    "maleVariant": null,
                    "stringId": 1977896742151360512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "楽な生活じゃないけど、生きてる意味を感じる。ところで… 一つ頼みがあるの",
                    "maleVariant": null,
                    "stringId": 1977904708845932544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "静かに。ルーシーじゃなくてミアよ。いいから、もう行って",
                    "maleVariant": null,
                    "stringId": 1977906705217196032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "偽名なんて通用しない。街を出た方が身のためよ",
                    "maleVariant": "偽名なんて通用しないぞ。街を出た方が身のためだ",
                    "stringId": 1977924266885484544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "弟が出てっちゃったから、ここには私が必要なのよ",
                    "maleVariant": null,
                    "stringId": 1977925037295243264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オクタビオ？ いい人だけど、腕はまだまだね",
                    "maleVariant": null,
                    "stringId": 1977931421764128768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何？",
                    "maleVariant": "何だ？",
                    "stringId": 1977934768634171392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "レジーナからエル・キャピタンを紹介されて、そこからオクタビオにたどり着いたの。身を隠す場所に関して彼はプロだから",
                    "maleVariant": null,
                    "stringId": 1977951320179838976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それに、伝染病やら薬剤耐性の感染症なんかに、いつでも対処できるように人手が要るのよ",
                    "maleVariant": null,
                    "stringId": 1977954992427208704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここでうまくやっていけそう？",
                    "maleVariant": "ここでうまくやっていけそうか？",
                    "stringId": 1977955500273537024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人間なら誰だって、たとえどんな境遇でも、助けを求めて生き延びる権利があるのよ。わかる？",
                    "maleVariant": null,
                    "stringId": 1977957290469593088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "子供がワクチンを必要としてるけどお金がなくて。できれば、オクタビオに少し渡してよ",
                    "maleVariant": null,
                    "stringId": 1977964068179644416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "考えておく",
                    "maleVariant": null,
                    "stringId": 1977964623270612992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ありがとう",
                    "maleVariant": null,
                    "stringId": 1977972016788377600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "メイルストロームに追われてるんでしょ",
                    "maleVariant": "メイルストロームに追われてるんだろ",
                    "stringId": 1983649777695690756
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こんな隠れ方じゃ、すぐにメイルストロームに見つかる。街を出たほうがいい",
                    "maleVariant": "こんな隠れ方じゃ、すぐにメイルストロームに見つかるぞ。街を出たほうがいい",
                    "stringId": 1983650348943118336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしの顔なんか見たくないのね。じゃあもう行くよ",
                    "maleVariant": "俺の顔なんか見たくないんだな。じゃあもう行くよ",
                    "stringId": 1983650436587294720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まだいたの？ オクタビオ！",
                    "maleVariant": null,
                    "stringId": 1983650926146457600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何だ？",
                    "maleVariant": null,
                    "stringId": 1983651276370841600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こいつには手を貸さないで。いい？",
                    "maleVariant": null,
                    "stringId": 1983651410051698688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかったよ",
                    "maleVariant": null,
                    "stringId": 1983651669913997312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それじゃ",
                    "maleVariant": null,
                    "stringId": 1983653669288382464
                  }
                }
              ]
            }
          }
        }
