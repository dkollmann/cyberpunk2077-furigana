        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "二人で何してるの？",
              "maleVariant": "二人で何してるんだ？",
              "stringId": "1756560177814589440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何してるの？",
              "maleVariant": "何してるんだ？",
              "stringId": "1756561680298168324"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "嵐の前に心を静めようと思ってな。テディーは特に何もしてないが",
              "maleVariant": "",
              "stringId": "1756564388560261120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一緒にやれてうれしい",
              "maleVariant": "",
              "stringId": "1756565536155389956"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もし失敗したら、責任はパナムが取ることになる",
              "maleVariant": "",
              "stringId": "1756566120522600448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よろしくね",
              "maleVariant": "よろしくな",
              "stringId": "1756567532711202816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "邪魔しないでおく",
              "maleVariant": "",
              "stringId": "1756567752945717248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一行目を読むだけで、こいつは1000ページの大傑作が始まるとわかる",
              "maleVariant": "",
              "stringId": "1756569524670058496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなこと俺たちがさせるわけない",
              "maleVariant": "",
              "stringId": "1756570776200040448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほらよ",
              "maleVariant": "",
              "stringId": "1756571459301167104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「オーガスタスがポーチに出ると、青豚がガラガラヘビを食っていた。さほど大きくはない」",
              "maleVariant": "",
              "stringId": "1756572961147211776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一冊全部、暗記したのか？",
              "maleVariant": "",
              "stringId": "1756573190726635520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "楽しんで",
              "maleVariant": "楽しめよ",
              "stringId": "1756576583113265152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「自らの努力を称えろ。自尊心は自制心を生む」",
              "maleVariant": "",
              "stringId": "1802287115006185472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「その二つを備えた者に、力は宿る」",
              "maleVariant": "",
              "stringId": "1802288241495576576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "飲むといつも哲学的な話をするの？",
              "maleVariant": "飲むといつも哲学的な話をするのか？",
              "stringId": "1802288767410966532"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「本物の男を生むのは、目標と愛と夢である」",
              "maleVariant": "",
              "stringId": "1802290244309291008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もし失敗したら、責任はパナムが取ることになる",
              "maleVariant": "",
              "stringId": "1802292636555743232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「本物の男を生むのは、目標と愛と夢である」",
              "maleVariant": "",
              "stringId": "1802292821558104064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まったくだな",
              "maleVariant": "",
              "stringId": "1802293335964323840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "飲むとうんちくを語るタイプ？",
              "maleVariant": "飲むとうんちくを語るタイプか？",
              "stringId": "1802298368609030144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ二人で楽しんで",
              "maleVariant": "",
              "stringId": "1802299005874802688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺はそういう役回りを求められてると思ってる",
              "maleVariant": "",
              "stringId": "1802303878649417728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "邪魔しないでおくよ",
              "maleVariant": "",
              "stringId": "1802307440385265664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソウルが俺たちを無視してパナムに全責任を負わせるって？",
              "maleVariant": "",
              "stringId": "1802319826064314368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムを利用してソウルを失脚させるつもり？",
              "maleVariant": "パナムを利用してソウルを失脚させるつもりか？",
              "stringId": "1802320600953589764"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "前回は、どうしてパナムの味方をしなかったの？",
              "maleVariant": "前回は、どうしてパナムの味方をしなかった？",
              "stringId": "1802322175260430336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムを利用してソウルを失脚させるつもり？",
              "maleVariant": "パナムを利用してソウルを失脚させるつもりか？",
              "stringId": "1802323141829398528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "前にパナムがクランを出た時、どうして見捨てたの？",
              "maleVariant": "前にパナムがクランを出た時、どうして見捨てた？",
              "stringId": "1802323529433419776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こないだのは、パナムがとんでもなくバカな真似をしたからだ",
              "maleVariant": "",
              "stringId": "1802325059179974656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "テッド、こいつは面白いな",
              "maleVariant": "",
              "stringId": "1802325601688031232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムのことを、俺たち家族より気にかけてるつもりらしい",
              "maleVariant": "",
              "stringId": "1802326256687321088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その時よりは、成長してる？",
              "maleVariant": "その時よりは、成長してると？",
              "stringId": "1802332639763697664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。外の世界で色々と学んで賢くなった。戻るべきタイミングもわかってたしな",
              "maleVariant": "",
              "stringId": "1802333237821116416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "同じ場所にしがみついて、言われたことしかできない奴より、パナムのほうが何倍も役に立つ",
              "maleVariant": "",
              "stringId": "1802334103944892416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいか、V。いいリーダーには何が必要だと思う？",
              "maleVariant": "",
              "stringId": "1802340425885093888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "古くさい格言じゃないのは確かね",
              "maleVariant": "古くさい格言じゃないのは確かだな",
              "stringId": "1802359720354582528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "逆境を乗り越え、クラン内のもめ事を解決して、自分の立場を脅かそうとする動きを封じる力だ",
              "maleVariant": "",
              "stringId": "1802361414702403584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまり、パナムをわざと千尋の谷に突き落としてるってわけ？ いいリーダーに育て上げるために",
              "maleVariant": "つまり、パナムをわざと千尋の谷に突き落としてるってわけか？ いいリーダーに育て上げるために",
              "stringId": "1802365437442592768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナム？ ソウルの話をしてるんだ",
              "maleVariant": "",
              "stringId": "1802369291403988992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムはクソ面倒な奴だが、ちょうどいいことに、ソウルを成長させるのにうってつけなのさ",
              "maleVariant": "",
              "stringId": "1802370069699039232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、一文目だけだ",
              "maleVariant": "",
              "stringId": "1802393272135782400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ニンジンのくだりも好きだがな",
              "maleVariant": "",
              "stringId": "1802394816025530368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "始まりはこうだ",
              "maleVariant": "",
              "stringId": "1802395196667006976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もし失敗したら、責任はパナムが取ることになるのよ",
              "maleVariant": "もし失敗したら、責任はパナムが取ることになるんだぞ",
              "stringId": "1824180115122491392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "失敗したら、責任はパナムが取ることになる",
              "maleVariant": "",
              "stringId": "1875070758500978688"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
