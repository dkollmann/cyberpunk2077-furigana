    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1561834548131422208,
            "femaleVariant": "遅かったわね"
          },
          {
            "stringId": 1561845962845872128,
            "femaleVariant": "その… 飛び入り参加がいてね"
          },
          {
            "stringId": 1561846529244684288,
            "femaleVariant": "スタジオの人？"
          },
          {
            "stringId": 1561849602897764352,
            "femaleVariant": "V、ズレイカだ"
          },
          {
            "stringId": 1561932080983465984,
            "femaleVariant": "ズレイカは私が殺した男性の姉なんだ"
          },
          {
            "stringId": 1561943568947240960,
            "femaleVariant": "服役してる時に手紙をくれてね"
          },
          {
            "stringId": 1561961856397893632,
            "femaleVariant": "ヨシュア・スティーブンソン… また会ったわね。あなたは？"
          },
          {
            "stringId": 1772039950399672324,
            "femaleVariant": "[立ち上がる] 行きましょ",
            "maleVariant": "[立ち上がる] 行こう"
          },
          {
            "stringId": 1772040432862072832,
            "femaleVariant": "[立ち上がる] もういいでしょ。あたしは降りる",
            "maleVariant": "[立ち上がる] もういいだろ、俺は降りる"
          },
          {
            "stringId": 1772043282858086400,
            "femaleVariant": "ねえ、そろそろ行きましょ",
            "maleVariant": "おい、そろそろ行くぞ"
          },
          {
            "stringId": 1772043536814804992,
            "femaleVariant": "もういいでしょ、ヨシュア",
            "maleVariant": "もういいだろ、ヨシュア"
          },
          {
            "stringId": 1772044680601505792,
            "femaleVariant": "エルアフマルさん…"
          },
          {
            "stringId": 1772045029550821376,
            "femaleVariant": "まだだ"
          },
          {
            "stringId": 1772176648135741440,
            "femaleVariant": "彼女はまさに、君が最後の手紙で書いていた人物そのものだ",
            "maleVariant": "彼はまさに、君が最後の手紙で書いていた人物そのものだ"
          },
          {
            "stringId": 1773651305661698048,
            "femaleVariant": "うるせえ"
          },
          {
            "stringId": 1811138549722406912,
            "femaleVariant": "Vよ。よろしく",
            "maleVariant": "Vだ。よろしく"
          },
          {
            "stringId": 1811139031480164356,
            "femaleVariant": "はじめまして"
          },
          {
            "stringId": 1811148222190084096,
            "femaleVariant": "誰でもいいから、いい加減どういうことか教えてよ",
            "maleVariant": "誰でもいいから、いい加減どういうことか教えてくれ"
          },
          {
            "stringId": 1811155094926286848,
            "femaleVariant": "いい加減どういうことなのか教えて",
            "maleVariant": "いい加減どういうことなのか教えてくれ"
          },
          {
            "stringId": 1811156189337640960,
            "femaleVariant": "まさか何も話してないの？"
          },
          {
            "stringId": 1811156888880439296,
            "femaleVariant": "会ったばかりだ。時間がなかった"
          },
          {
            "stringId": 1811157650431827968,
            "femaleVariant": "母は出かけてるの。戻るまで中で話をしましょ。入って"
          },
          {
            "stringId": 1811157890446680064,
            "femaleVariant": "どうも"
          },
          {
            "stringId": 1811157976312471552,
            "femaleVariant": "ヨシュアの友達なの？"
          },
          {
            "stringId": 1811159385883828224,
            "femaleVariant": "ホント、どうかしてる…"
          },
          {
            "stringId": 1811161631665184772,
            "femaleVariant": "知り合ったばかりよ",
            "maleVariant": "知り合ったばかりだ"
          },
          {
            "stringId": 1811162098726100992,
            "femaleVariant": "話すと長い"
          },
          {
            "stringId": 1811162537618071552,
            "femaleVariant": "友達じゃない"
          },
          {
            "stringId": 1811162793973932032,
            "femaleVariant": "いえ、知り合ったばかりよ",
            "maleVariant": "いや、知り合ったばかりだ"
          },
          {
            "stringId": 1811162857945456640,
            "femaleVariant": "あー… まあ、いろいろあってね",
            "maleVariant": "あー… まあ、いろいろあってな"
          },
          {
            "stringId": 1811162962501066752,
            "femaleVariant": "いいえ、友達じゃない",
            "maleVariant": "いや、友達じゃない"
          },
          {
            "stringId": 1811176682589544448,
            "femaleVariant": "はあ…"
          },
          {
            "stringId": 1811179700106059776,
            "femaleVariant": "母は出かけてるの。戻るまで中で話しましょ。入って"
          },
          {
            "stringId": 1811269762651021312,
            "femaleVariant": "そんなことした人を許せるなんて",
            "maleVariant": "そんなことした人を許せるとはな"
          },
          {
            "stringId": 1811269951226929152,
            "femaleVariant": "何のためにここに来たの？",
            "maleVariant": "何のためにここに来たんだ？"
          },
          {
            "stringId": 1811269990988931072,
            "femaleVariant": "ヨシュアはあんたの影響で罪滅ぼしを？"
          },
          {
            "stringId": 1811270692494663680,
            "femaleVariant": "自分の弟を殺した男を許すなんて、そうできることじゃない"
          },
          {
            "stringId": 1811270860988243968,
            "femaleVariant": "ヨシュアはあんたの影響で罪滅ぼしを？"
          },
          {
            "stringId": 1811271103570010112,
            "femaleVariant": "で、ここへはいったい何をしに来たの？",
            "maleVariant": "で、ここへはいったい何をしに来たんだ？"
          },
          {
            "stringId": 1811439695431229440,
            "femaleVariant": "そして絆で結ばれた。許しを通して"
          },
          {
            "stringId": 1811440619100856320,
            "femaleVariant": "〈咎めることなかれ、さすればその身咎められん。許せ、さすればその身許されん〉"
          },
          {
            "stringId": 1811442825942294528,
            "femaleVariant": "すべては彼女の手紙から始まった"
          },
          {
            "stringId": 1811444224205807616,
            "femaleVariant": "弟を失ったら、神が彼を遣わされた"
          },
          {
            "stringId": 1811447492457816068,
            "femaleVariant": "その神は無実の人を見殺しにした"
          },
          {
            "stringId": 1811447936164847616,
            "femaleVariant": "信心深いのね。近頃じゃ珍しい",
            "maleVariant": "信心深いんだな。近頃じゃ珍しい"
          },
          {
            "stringId": 1811448304441516032,
            "femaleVariant": "全然説得力ない",
            "maleVariant": "説得力にかける話だな"
          },
          {
            "stringId": 1811448378596810752,
            "femaleVariant": "ナイトシティで敬けんな信者に出くわすとはね",
            "maleVariant": "ナイトシティで敬けんな信者に出くわすとはな"
          },
          {
            "stringId": 1811454099476471808,
            "femaleVariant": "慈悲深い神を信じてるようだけど、その神はあんたの弟を見殺しにしたのよ",
            "maleVariant": "慈悲深い神を信じてるようだが、その神はあんたの弟を見殺しにしたんだぞ"
          },
          {
            "stringId": 1811459055650627584,
            "femaleVariant": "何のためにここに来たの？",
            "maleVariant": "何のためにここに来たんだ？"
          },
          {
            "stringId": 1811485843697197056,
            "femaleVariant": "もう帰らないルーベンを想って、私はさんざん嘆き悲しんだ… でもそんな時、主が心の平穏に導いてくださったの"
          },
          {
            "stringId": 1811487559301750784,
            "femaleVariant": "私がその行いを悔いていることを神はご存じだ。だが彼のおかげで、神へと通ずる道を見つけられた"
          },
          {
            "stringId": 1811489986914574340,
            "femaleVariant": "まだ納得できない"
          },
          {
            "stringId": 1811490264359395328,
            "femaleVariant": "全ては神の導きだったと？"
          },
          {
            "stringId": 1811491176150753280,
            "femaleVariant": "まだ納得できない",
            "maleVariant": "まだ納得できないな"
          },
          {
            "stringId": 1811491257050488832,
            "femaleVariant": "神の導く道なんてわかりようがない"
          },
          {
            "stringId": 1811494094664916992,
            "femaleVariant": "これも神のお導き。神がお引き合わせくださったのよ"
          },
          {
            "stringId": 1811497254469001216,
            "femaleVariant": "V、神を信じるか？"
          },
          {
            "stringId": 1811497641250938880,
            "femaleVariant": "では、創造主を信じないのか？"
          },
          {
            "stringId": 1811497981828423684,
            "femaleVariant": "信じる"
          },
          {
            "stringId": 1811498165773819904,
            "femaleVariant": "信じない"
          },
          {
            "stringId": 1811498617584246784,
            "femaleVariant": "人智を超えた力はあるかもしれない"
          },
          {
            "stringId": 1811499676612452352,
            "femaleVariant": "もちろん、信じる"
          },
          {
            "stringId": 1811499757478633472,
            "femaleVariant": "信じてはいない",
            "maleVariant": "信じちゃいない"
          },
          {
            "stringId": 1811499828228153344,
            "femaleVariant": "人智を超えた力が存在する可能性はある。といっても、それを証明することは不可能よ",
            "maleVariant": "人智を超えた力が存在する可能性はある。だが、それを証明することは不可能だ"
          },
          {
            "stringId": 1811500367246548992,
            "femaleVariant": "そもそもなんでこんな話を？"
          },
          {
            "stringId": 1811501415302455296,
            "femaleVariant": "そもそもなんでこんな話を？",
            "maleVariant": "そもそもなぜこんな話を？"
          },
          {
            "stringId": 1811507218289811456,
            "femaleVariant": "信じるかどうかを決めるのはあなたよ"
          },
          {
            "stringId": 1811508137177931776,
            "femaleVariant": "ズレイカと私が延々と議論していることがある。解決するのを手伝ってくれないか"
          },
          {
            "stringId": 1811509781345099776,
            "femaleVariant": "相手が敵でも隣人でも、神の愛に境界線はない。私たちもそうすべきなのよ"
          },
          {
            "stringId": 1811511187946889216,
            "femaleVariant": "V、君は… 神を信じるか？"
          },
          {
            "stringId": 1811512360724619264,
            "femaleVariant": "ちょっと、そんなこと人に頼むなんて嫌よ！"
          },
          {
            "stringId": 1811512786631024644,
            "femaleVariant": "ぜひ聞かせて",
            "maleVariant": "ぜひ聞かせてくれ"
          },
          {
            "stringId": 1811513129020448768,
            "femaleVariant": "宗教の議論なんて勘弁して",
            "maleVariant": "宗教の議論なんて勘弁してくれ"
          },
          {
            "stringId": 1811513682937012224,
            "femaleVariant": "ぜひ聞かせて",
            "maleVariant": "聞かせてくれ"
          },
          {
            "stringId": 1811513760464527360,
            "femaleVariant": "宗教の議論なんて勘弁してよ",
            "maleVariant": "宗教の議論なんて勘弁してくれ"
          },
          {
            "stringId": 1811514137784115200,
            "femaleVariant": "ヨシュアは… 自分が救世主だと考えていて、命を絶ちたがってるの"
          },
          {
            "stringId": 1811517334867173376,
            "femaleVariant": "それは違う―― 救世主だなんて言ったことはないよ"
          },
          {
            "stringId": 1811519190846693376,
            "femaleVariant": "自殺だなんて！ 神があなたにそんなことをお望みになると思う？"
          },
          {
            "stringId": 1811521289223430144,
            "femaleVariant": "これは自殺じゃない"
          },
          {
            "stringId": 1811522415494717440,
            "femaleVariant": "そんなのどうかしてる"
          },
          {
            "stringId": 1811528246265143296,
            "femaleVariant": "ズレイカ、説明して",
            "maleVariant": "ズレイカ、説明してくれ"
          },
          {
            "stringId": 1811531016921702400,
            "femaleVariant": "ヨシュア、どういうことなの？",
            "maleVariant": "ヨシュア、どういうことなんだ？"
          },
          {
            "stringId": 1811531101126549504,
            "femaleVariant": "ズレイカ、まだ話が見えないんだけど",
            "maleVariant": "ズレイカ、まだ話が見えないんだが"
          },
          {
            "stringId": 1811532787136094208,
            "femaleVariant": "檻の中で信仰に目覚めたことがメディアに伝わると、翌日には私はスクリームシートの一面を飾っていた"
          },
          {
            "stringId": 1811533748437016576,
            "femaleVariant": "そこにブレインダンスのスタジオが目を付け、出演オファーを持ってきたんだ"
          },
          {
            "stringId": 1811536365414277120,
            "femaleVariant": "ハイエナたちめ…"
          },
          {
            "stringId": 1811536696126758912,
            "femaleVariant": "おかげで、電気椅子に座らされるはずだった私は、より崇高な死を迎えられるようになったんだ"
          },
          {
            "stringId": 1811538281774997504,
            "femaleVariant": "連中はヨシュアが十字架刑になるBDを撮ろうとしてるの。たとえローブを着ていても、死刑には変わりない"
          },
          {
            "stringId": 1811542239067267072,
            "femaleVariant": "ブレインダンス？ 何の目的で？"
          },
          {
            "stringId": 1811542324681400320,
            "femaleVariant": "法律的に許されるの？",
            "maleVariant": "法律的に許されるのか？"
          },
          {
            "stringId": 1811543037796966400,
            "femaleVariant": "待って。囚人の処刑方法をBDスタジオの意向で変えられるの？ そんなこと法律で許される？",
            "maleVariant": "待て。囚人の処刑方法をBDスタジオの意向で変えるのか？ 法律的に許されるとは思えない"
          },
          {
            "stringId": 1811544639719104512,
            "femaleVariant": "いまや企業が法を作り、企業が判事を選ぶ。スタジオのCEOと州検事が話し合うだけで済むことさ"
          },
          {
            "stringId": 1811545251198296064,
            "femaleVariant": "私は今日、監視付きの一時出所ということになってる。そのあいだ、友人と過ごすことを禁じる法律はどこにもない"
          },
          {
            "stringId": 1811551495007002624,
            "femaleVariant": "ヨシュアが収監された時は、スクリームシートに小さく記事が載っただけ"
          },
          {
            "stringId": 1811552475333287936,
            "femaleVariant": "でも彼が信仰に目覚めたと知ると、急にテレビが取り上げるようになった"
          },
          {
            "stringId": 1811553203632234496,
            "femaleVariant": "私の信仰と神の思し召しを人々に広めるにはテレビが一番だ。いけないか？"
          },
          {
            "stringId": 1811553690540597248,
            "femaleVariant": "もうやめて、ヨシュア！ この人は死刑になる代わりに、十字架刑になって… 挙句の果てにその苦しみをブレインダンス用に撮影させるって言うのよ"
          },
          {
            "stringId": 1811561695805988864,
            "femaleVariant": "汚職も罪のはず。そのおかげでここにいるのよ",
            "maleVariant": "汚職も罪のはずだ。そのおかげでここにいるんだろ"
          },
          {
            "stringId": 1811561871379554304,
            "femaleVariant": "もはや人の法など及ばない次元の話だ。このBDで神の御言葉を世界中の人たちに届けられる"
          },
          {
            "stringId": 1811566712260681728,
            "femaleVariant": "狂気の沙汰としか思えない"
          },
          {
            "stringId": 1811566786432753664,
            "femaleVariant": "どうしてブレインダンスを？ 何が目的なの？",
            "maleVariant": "どうしてブレインダンスを？ 何が目的だ？"
          },
          {
            "stringId": 1811567998704050176,
            "femaleVariant": "狂気よりひどい。神への冒とくよ"
          },
          {
            "stringId": 1811577709037903872,
            "femaleVariant": "ズレイカは私を神の子に変えてくれた。今度は私が恩を返す番だ"
          },
          {
            "stringId": 1811578731273674752,
            "femaleVariant": "人々は家畜のようにフィーダーにつながれ、子供たちはギャングだらけの街で育ってる。そんな彼らが愛を知ってると思うか？"
          },
          {
            "stringId": 1811580596883316736,
            "femaleVariant": "ジグジグ・ストリートのジョイトイ、クスリをキメたコーポの連中、街を歩き回る殺人者たち！ 彼らに伝えたい、私が伝えるんだ"
          },
          {
            "stringId": 1811598778872389632,
            "femaleVariant": "君みたいな殺し屋を考えてみろ。V、君は愛を知っているか？ どうだ？"
          },
          {
            "stringId": 1811599568592723968,
            "femaleVariant": "母さんよ"
          },
          {
            "stringId": 1811600494409834496,
            "femaleVariant": "V、君は… 神を信じるか？"
          },
          {
            "stringId": 1811602284085796864,
            "femaleVariant": "議論することなんかない。BDスタジオの連中は、ヨシュアに普通の死刑でなく十字架の刑を受けるよう説得したの"
          },
          {
            "stringId": 1811603067397234688,
            "femaleVariant": "彼も彼で、他人にも自分の苦しみを体験させたいなんて言うのよ！"
          },
          {
            "stringId": 1811604787800072192,
            "femaleVariant": "この世に愛が存在することを示したいんだ。ありとあらゆる迷える魂が、あまねく理解できるような形でね"
          },
          {
            "stringId": 1811607145183469568,
            "femaleVariant": "どうして？",
            "maleVariant": "どうしてだ？"
          },
          {
            "stringId": 1811607228113248256,
            "femaleVariant": "無償の愛が存在するなら、神だって存在するはずだ。今こそ世界に神の存在を思い出させるんだ"
          },
          {
            "stringId": 1812391006913978368,
            "femaleVariant": "Vよ",
            "maleVariant": "Vだ"
          },
          {
            "stringId": 1812391364671332352,
            "femaleVariant": "この人を待ってたの？",
            "maleVariant": "この人を待ってたのか？"
          },
          {
            "stringId": 1812391461794635776,
            "femaleVariant": "あたしはV",
            "maleVariant": "俺はVだ"
          },
          {
            "stringId": 1812391529104826368,
            "femaleVariant": "この人を待ってたの？",
            "maleVariant": "この人を待ってたのか？"
          },
          {
            "stringId": 1812392991306309632,
            "femaleVariant": "出てってちょうだい"
          },
          {
            "stringId": 1812395297636638720,
            "femaleVariant": "そうだ"
          },
          {
            "stringId": 1812395582614429696,
            "femaleVariant": "エルアフマルさん… あなたの許しを請いに来た"
          },
          {
            "stringId": 1812402402586288128,
            "femaleVariant": "あなたは息子を殺したのよ！"
          },
          {
            "stringId": 1812402544857079808,
            "femaleVariant": "許せるはずがないでしょう！"
          },
          {
            "stringId": 1812402743499317248,
            "femaleVariant": "今すぐ家から出てって！"
          },
          {
            "stringId": 1812402916908621824,
            "femaleVariant": "あなたたち全員よ！"
          },
          {
            "stringId": 1812403482518904832,
            "femaleVariant": "あなたを苦しませて本当にすまない…"
          },
          {
            "stringId": 1812407937003970560,
            "femaleVariant": "あたしはもう知らないから",
            "maleVariant": "俺はもう知らないぞ"
          },
          {
            "stringId": 1831726537145606144,
            "femaleVariant": "ズレイカ…？"
          },
          {
            "stringId": 1831812314202398724,
            "femaleVariant": "[座る] あたしも水をもらっていい？",
            "maleVariant": "[座る] 俺も水をもらっていいか？"
          },
          {
            "stringId": 1831812422063120384,
            "femaleVariant": "どうぞ、座って"
          },
          {
            "stringId": 1831812734270332928,
            "femaleVariant": "飲み物でも… どう？"
          },
          {
            "stringId": 1831813890925158400,
            "femaleVariant": "[座る] 何か… 強いのある？",
            "maleVariant": "[座る] 何か… 強いのあるか？"
          },
          {
            "stringId": 1831814457072312320,
            "femaleVariant": "[座る] おかまいなく"
          },
          {
            "stringId": 1831816429049503744,
            "femaleVariant": "一杯もらえる？",
            "maleVariant": "一杯もらえるか？"
