  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー…",
              "maleVariant": "",
              "stringId": 1905126991447220224
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "潮風の匂いはいいな",
              "maleVariant": "",
              "stringId": 1905127307865513984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "起きろ。この時間の海はきれいだぞ",
              "maleVariant": "",
              "stringId": 1905128076379447296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] 死ぬかと思った",
              "maleVariant": "",
              "stringId": 1905129545509920772
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今回のは死ぬかと思った",
              "maleVariant": "",
              "stringId": 1905129714070609920
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だな",
              "maleVariant": "",
              "stringId": 1905130373901737984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここは？",
              "maleVariant": "",
              "stringId": 1905131018079723524
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きれいな海ね",
              "maleVariant": "きれいな海だな",
              "stringId": 1905131192361443328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きれいね。いつまでも見ていたい",
              "maleVariant": "きれいだ。いつまでも見ていたい",
              "stringId": 1905131520188243968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここは… パシフィカ？",
              "maleVariant": "",
              "stringId": 1905131796727095296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この建物は？",
              "maleVariant": "",
              "stringId": 1905132280883994624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ピスティス・ソフィアって古いホテルだ。今のところ貸し切りだぜ",
              "maleVariant": "",
              "stringId": 1905132670903934976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[離れる] なぜあたしをここに？",
              "maleVariant": "[離れる] なぜ俺をここに？",
              "stringId": 1905149533499187204
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんでまた… こんなところに？",
              "maleVariant": "",
              "stringId": 1905149715431317504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[離れる] ミスティの薬を手にしてた",
              "maleVariant": "",
              "stringId": 1905152542593540096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "疑似エンドトライジンを手に取ってたでしょ",
              "maleVariant": "疑似エンドトライジンを手に取ってただろ",
              "stringId": 1905155167086993408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはもう… 長くないと思う",
              "maleVariant": "俺はもう… 長くないと思う",
              "stringId": 1905156366641160192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それがないとこの体を上手く動かせないんだろ？",
              "maleVariant": "",
              "stringId": 1905163910650552320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハナコの話に本気で乗るつもりなのか？",
              "maleVariant": "",
              "stringId": 1905471416283193348
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他にいい考えがある？",
              "maleVariant": "他にいい考えがあるか？",
              "stringId": 1905471416316747776
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハナコとは共通の目的がある",
              "maleVariant": "",
              "stringId": 1905471416316747792
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しつこい",
              "maleVariant": "しつこいな",
              "stringId": 1905471416333524992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "生きるために必要なことをするまで",
              "maleVariant": "生きるために必要なことをするまでだ",
              "stringId": 1905471416333525008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またその話？",
              "maleVariant": "またその話か？",
              "stringId": 1905471416350302212
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "生きるために必要なことをするだけ。この手を汚すことになってもね",
              "maleVariant": "生きるために必要なことをするだけだ。この手を汚すことになってもな",
              "stringId": 1905471416383856644
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何？",
              "maleVariant": "何だ？",
              "stringId": 1905471416400633860
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺を倒した野郎、アダム・スマッシャー…",
              "maleVariant": "",
              "stringId": 1905471416417411072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この先、俺がどうなろうと、あいつだけは絶対に生かしちゃおけねえ",
              "maleVariant": "",
              "stringId": 1905471416417411084
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他にいいアイデアが？",
              "maleVariant": "",
              "stringId": 1905471416450965508
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はないが、頼みたいことがある",
              "maleVariant": "",
              "stringId": 1905471416450965520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "共通の目的があるのよ",
              "maleVariant": "共通の目的があるんだ",
              "stringId": 1905471416484519936
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フン… そうか",
              "maleVariant": "",
              "stringId": 1905471416484519948
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "決めるのはお前だ。自分の思うようにすりゃあいい。だが一つ頼みがある",
              "maleVariant": "",
              "stringId": 1905471416501297156
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "復讐が最期の望みなの？",
              "maleVariant": "復讐が最期の望みなのか？",
              "stringId": 1905471416534851584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなことに命は懸けたくない",
              "maleVariant": "",
              "stringId": 1905471416534851600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": 1905471416534851616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それが最期の望みなの？ 復讐が？",
              "maleVariant": "それが最期の望みかよ？ 復讐が？",
              "stringId": 1905471416568406020
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前にはわからん。あと、ローグも同席させてくれ",
              "maleVariant": "",
              "stringId": 1905471416568406032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの復讐なんかのために、命を懸けるのはお断りよ",
              "maleVariant": "お前の復讐なんかのために、命を懸けるのはお断りだ",
              "stringId": 1905471416601960452
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグが手伝ってくれる。危険はない",
              "maleVariant": "",
              "stringId": 1905471416601960464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、わかった",
              "maleVariant": "ああ、わかった",
              "stringId": 1905471416635514884
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あと、ローグも同席させてくれ",
              "maleVariant": "",
              "stringId": 1905471416652292096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺だけじゃなく、あいつにとっても重要なことなんだ",
              "maleVariant": "",
              "stringId": 1905471416669069316
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなことしてローグが喜ぶ？",
              "maleVariant": "そんなことしてローグが喜ぶか？",
              "stringId": 1905471416702623744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグは現場を退いてる",
              "maleVariant": "",
              "stringId": 1905471416719400960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグに全てを説明しろってこと？",
              "maleVariant": "ローグに全てを説明しろってことか？",
              "stringId": 1905471416719400976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで、ローグが涙を流すとでも？",
              "maleVariant": "",
              "stringId": 1905471416752955396
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かに今は冷てえ面をしてる。それに、あれから時間も経った",
              "maleVariant": "",
              "stringId": 1905471416752955408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが俺があいつの記憶を呼び覚ます",
              "maleVariant": "",
              "stringId": 1905471416752955420
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今のローグがそんなことをしたがるとは思えない。出世したのよ",
              "maleVariant": "今のローグがそんなことをしたがるとは思えない。出世したんだぞ",
              "stringId": 1905471416786509828
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わざわざ危険を冒す理由がどこにあるの？",
              "maleVariant": "わざわざ危険を冒す理由がどこにあるんだ？",
              "stringId": 1905471416786509840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺が頼み込めば話は別さ",
              "maleVariant": "",
              "stringId": 1905471416803287048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしに説明しろっていうわけ？ これ全部",
              "maleVariant": "俺に説明しろっていうのか？ これ全部",
              "stringId": 1905471416820064260
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺が個人的に話した方がいいだろうな",
              "maleVariant": "",
              "stringId": 1905471416820064272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミスティの薬を飲んで、しばらく俺に運転を任せてくれ",
              "maleVariant": "",
              "stringId": 1905471416853618692
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは全然乗り気じゃないんだけど… 気づいてる？",
              "maleVariant": "俺は全然乗り気じゃないんだが… 気づいてるか？",
              "stringId": 1905471416853618704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグとスマッシャーのことを話すだけだ。終わったらすぐに出る。約束だ",
              "maleVariant": "",
              "stringId": 1905471416903950340
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何をする気なの？",
              "maleVariant": "何をする気なんだ？",
              "stringId": 1905471416954281984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダメよ",
              "maleVariant": "ダメだ",
              "stringId": 1905471416971059204
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたを信用していいかわからない",
              "maleVariant": "お前を信用していいかわからない",
              "stringId": 1905471416971059220
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの企みはわかってる",
              "maleVariant": "お前の企みはわかってる",
              "stringId": 1905471416971059236
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘルマンの言ってたとおりね",
              "maleVariant": "ヘルマンの言ってたとおりだな",
              "stringId": 1905471416987836424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何をする気なのかすべて教えて",
              "maleVariant": "何をする気なのかすべて教えろ",
              "stringId": 1905471417021390852
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグに真実を話し、協力を仰ぐんだ",
              "maleVariant": "",
              "stringId": 1905471417021390864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アダム・スマッシャーの名を出して食いつかせ、俺と戦ったあの頃を思い出させてやるだけだ。簡単さ",
              "maleVariant": "",
              "stringId": 1905471417038168080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダメよ。調子に乗りすぎ",
              "maleVariant": "ダメだ。調子に乗るな",
              "stringId": 1905471417071722500
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いずれは〈神輿〉にたどり着かなきゃならないんだぞ",
              "maleVariant": "",
              "stringId": 1905471417071722512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグは切り札になる",
              "maleVariant": "",
              "stringId": 1905471417088499720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘルマンの言ってたとおりね。いつかはあんたに主導権を奪われるって",
              "maleVariant": "ヘルマンの言ってたとおりだな。いつかはお前に主導権を奪われるわけか",
              "stringId": 1905471417155608576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンダース・ヘルマン？ あいつが何か力になってくれたか？",
              "maleVariant": "",
              "stringId": 1905471417155608588
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少なくとも、あたしに危害を加えてない",
              "maleVariant": "少なくとも、俺に危害を加えてない",
              "stringId": 1905471417155608600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたを信用していいかわからない",
              "maleVariant": "お前を信用していいかわからない",
              "stringId": 1905471417222717444
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なあ、フェアにやろうぜ、V",
              "maleVariant": "",
              "stringId": 1905471417222717456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前はいつも俺を自由に支配してるじゃねえか",
              "maleVariant": "",
              "stringId": 1905471417222717468
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん。自分の体なんだから当然でしょ",
              "maleVariant": "ふん。自分の体なんだから当然だろ",
              "stringId": 1905471417239494656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "判断を誤ってばかりのお前に、俺は助言ぐらいしかできない",
              "maleVariant": "",
              "stringId": 1905471417239494672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それか今みたいに、頼み込むかだ",
              "maleVariant": "",
              "stringId": 1905471417239494684
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。運転を譲るよ",
              "maleVariant": "",
              "stringId": 1905471417289826304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "絶対にいや",
              "maleVariant": "絶対にいやだ",
              "stringId": 1905471417289826320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "考えさせて",
              "maleVariant": "考えさせてくれ",
              "stringId": 1905471417306603528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かに。いい考えかもしれない",
              "maleVariant": "",
              "stringId": 1905471417356935168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話は終わりよ",
              "maleVariant": "話は終わりだ",
              "stringId": 1905471417356935184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "考えさせて",
              "maleVariant": "考えさせてくれ",
              "stringId": 1905471417356935200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、やるよ… あんたのためにね",
              "maleVariant": "わかった、やるよ… お前のためにな",
              "stringId": 1905471417407266820
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "恩に着るぜ",
              "maleVariant": "",
              "stringId": 1905471417407266832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アフターライフに連れてってくれ。絶対に後悔はさせない",
              "maleVariant": "",
              "stringId": 1905471417407266844
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグは口実なんでしょ",
              "maleVariant": "ローグは口実なんだろ",
              "stringId": 1905471417491152900
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ？ 俺がお前の体を奪おうとしてるって言いたいのか？",
              "maleVariant": "",
              "stringId": 1905471417491152912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それかアラサカにまた自爆攻撃を仕掛けるか。いい加減お見通しなのよ、ジョニー",
              "maleVariant": "それかアラサカにまた自爆攻撃を仕掛けるか。いい加減お見通しなんだよ、ジョニー",
              "stringId": 1905471417491152924
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何度も言わせるな… 俺は何も企んじゃいねえ",
              "maleVariant": "",
              "stringId": 1905471417491152940
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "嫌よ。またあんたに操られる気はない",
              "maleVariant": "嫌だ。またお前に操られる気はない",
              "stringId": 1905471417524707332
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "交渉決裂か",
              "maleVariant": "",
              "stringId": 1905471417524707344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね。話は終わりよ",
              "maleVariant": "そうだな。話は終わりだ",
              "stringId": 1905471417541484548
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わーったよ。気が変わったら、アフターライフに連れてってくれ",
              "maleVariant": "",
              "stringId": 1905471417558261780
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "考えさせて",
              "maleVariant": "考えさせてくれ",
              "stringId": 1905471417608593412
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼むぜ",
              "maleVariant": "",
              "stringId": 1905471417608593424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、重要なことなんだ。アフターライフに行き、しばらく運転を譲ってくれりゃいい",
              "maleVariant": "",
              "stringId": 1905471417625370624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待てって。すぐにわかる",
              "maleVariant": "",
              "stringId": 1905508894117486592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "乗っ取ったのね…",
              "maleVariant": "乗っ取ったのか…",
              "stringId": 1905519912688312320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、そのようだな…",
              "maleVariant": "",
              "stringId": 1905529335578124288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あそこに放っておくわけにもいかないだろ？",
              "maleVariant": "",
              "stringId": 1905580754658156544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またハンドルを握れていい気分だった？",
              "maleVariant": "またハンドルを握れていい気分だったか？",
              "stringId": 1905582449811283968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、最高だった… 人生で一番な",
              "maleVariant": "",
              "stringId": 1905582898652143616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミスティの薬を飲もうって、本気で考えてたの？",
              "maleVariant": "ミスティの薬を飲もうって、本気で考えてたのか？",
              "stringId": 1905586734074716164
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "助けてくれてありがとう",
              "maleVariant": "",
              "stringId": 1905587155719708672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おかげで助かった",
              "maleVariant": "",
              "stringId": 1905587320455192576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつかこの恩は必ず返す",
              "maleVariant": "",
              "stringId": 1905591141348696064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前は昏睡状態だってのに、随分と抵抗してきやがったがな。なかなかのバトルだった",
              "maleVariant": "",
              "stringId": 1905742358808911872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "薬を飲もうとしたの？",
              "maleVariant": "薬を飲もうとしたのか？",
              "stringId": 1905759417328492548
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言い逃れしても無駄よ",
              "maleVariant": "言い逃れしても無駄だ",
              "stringId": 1905761846971359232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言い訳したって無駄。薬を飲もうとしたでしょ",
              "maleVariant": "言い訳しても無駄だからな。薬を飲もうとしただろ",
              "stringId": 1905762272038903808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "薬を飲もうとしたの？",
              "maleVariant": "薬を飲もうとしたのか？",
              "stringId": 1905762325910544384
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着けよ。体を奪うつもりなら、そうしてる。とっくの昔にな",
              "maleVariant": "",
              "stringId": 1905763793161969664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今のは聞かなかったことにしてやる",
              "maleVariant": "",
              "stringId": 1905765737943293952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "偉そうに。あんたの本性はわかってる",
              "maleVariant": "偉そうに。お前の本性はわかってる",
              "stringId": 1905766976521265152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、待てよ。俺はお前を助けてやったんだぞ！？ それでもまだ疑うってのか？",
              "maleVariant": "",
              "stringId": 1905767696750370816
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "間に合ってよかった",
              "maleVariant": "",
              "stringId": 1905768607149219844
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "踏みとどまったのね",
              "maleVariant": "踏みとどまったんだな",
              "stringId": 1905769277684211712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたを信じるよ",
              "maleVariant": "お前を信じるよ",
              "stringId": 1905769514041630720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしタイミングよく目を覚ましてなかったら危ないところだった",
              "maleVariant": "",
              "stringId": 1905769773467721728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さすがのあんたも迷ったわけだ。すんでのところで、踏みとどまってくれたのね",
              "maleVariant": "さすがのお前も迷ったわけだ。すんでのところで、踏みとどまってくれたんだな",
              "stringId": 1905769901612097536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死の恐怖でまともな判断ができなくなってるようだな",
              "maleVariant": "",
              "stringId": 1905779147384840192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー・シルヴァーハンドにも良心があったってよかったろ",
              "maleVariant": "",
              "stringId": 1905782146362531840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だがそれはどうでもいい",
              "maleVariant": "",
              "stringId": 1905783793784479744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、信じるよ",
              "maleVariant": "",
              "stringId": 1905785153946935296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも",
              "maleVariant": "",
              "stringId": 1905785752574779392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてこれをあたしに？",
              "maleVariant": "どうしてこれを俺に？",
              "stringId": 1905787725088841732
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見せたいものがあるんだ。来てくれ",
              "maleVariant": "",
              "stringId": 1905789492635656192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当にミスティの薬を飲もうとしてたの？",
              "maleVariant": "本当にミスティの薬を飲もうとしてたのか？",
              "stringId": 1905795529480626176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "持つべきもんは友達だろ？",
              "maleVariant": "",
              "stringId": 1905796694775717888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはもう長くない",
              "maleVariant": "俺はもう長くない",
              "stringId": 1905798773355339780
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メキシコ紛争の時の？",
              "maleVariant": "メキシコ紛争の時のか？",
              "stringId": 1905799204647231488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この恩はいつか返すよ",
              "maleVariant": "",
              "stringId": 1905805044208918528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待ってるぜ",
              "maleVariant": "",
              "stringId": 1905805974874644480
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはもう長くない",
              "maleVariant": "俺はもう長くない",
              "stringId": 1905806375917215748
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "体でだと思ったでしょ？",
              "maleVariant": "体でだと思っただろ？",
              "stringId": 1905806633095159808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いま体でって思ったでしょ？",
              "maleVariant": "いま体でって思っただろ？",
              "stringId": 1905807778458918912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "互いに見せつけ合うしかできねえだろ？ タイプじゃねえよ",
              "maleVariant": "なら、二人で一緒にセンズリでもこくか？ タイプじゃねえよ",
              "stringId": 1905808404735614976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺の足元に、ある物が隠されてる。そいつを開けてみろ",
              "maleVariant": "",
              "stringId": 1905811364051968000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[開ける]",
              "maleVariant": "",
              "stringId": 1905812496983810052
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの？",
              "maleVariant": "お前のか？",
              "stringId": 1905812780183216128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。今はお前のだ",
              "maleVariant": "",
              "stringId": 1905813054339702784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "50年前だから… メキシコ紛争？",
              "maleVariant": "50年前だから… メキシコ紛争か？",
              "stringId": 1905815453196378112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前みたいな若造だった頃、俺は企業軍に所属してたんだ",
              "maleVariant": "",
              "stringId": 1905816495665471488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メキシコで俺は気づいた。紛争が起きたところで、勝つのはいつも企業。一般人は必ず敗北する",
              "maleVariant": "",
              "stringId": 1905816971349876736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから、歌で真実を伝えようとしたの？",
              "maleVariant": "だから、歌で真実を伝えようとしたのか？",
              "stringId": 1905817266343665664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずは逃亡した。ここにたどり着き、この部屋で1ヶ月、ベッドに横たわってただ天井を見つめてたのさ",
              "maleVariant": "",
              "stringId": 1905817350145859584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしもそうしたい気分。いつまでも天井を見ていたい…",
              "maleVariant": "俺もそうしたい気分だ。いつまでも天井を見ていたいよ…",
              "stringId": 1905854141607469056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前には何週間も時間を無駄にするような真似はしてほしくない",
              "maleVariant": "",
              "stringId": 1905854950957146112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてこれをあたしに？",
              "maleVariant": "どうしてこれを俺に？",
              "stringId": 1905856009012273152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "受けられる",
              "maleVariant": "",
              "stringId": 1905856704075554820
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もし俺たちが同じ部隊で共に戦う仲間だとしたら…",
              "maleVariant": "",
              "stringId": 1905856921541828608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "受けられない",
              "maleVariant": "",
              "stringId": 1905858466639536128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わからない",
              "maleVariant": "",
              "stringId": 1905858575121014784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、受けられる",
              "maleVariant": "ああ、受けられる",
              "stringId": 1905859151015731200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無理ね… 受けられない",
              "maleVariant": "無理だ… 受けられない",
              "stringId": 1905859248810123264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わからない",
              "maleVariant": "",
              "stringId": 1905859326236975104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バカげた質問ね",
              "maleVariant": "バカげた質問だ",
              "stringId": 1905859458525323264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バカげた質問ね",
              "maleVariant": "バカげた質問だ",
              "stringId": 1905859650137907200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのタグは、メキシコで俺のために命を落としちまった奴のものだ",
              "maleVariant": "",
              "stringId": 1905861055347187712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちのことを考えてた…",
              "maleVariant": "",
              "stringId": 1905863214205788160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はっきり言っておく。俺はお前を裏切らない。その時が来たら、俺はお前が生きるために消えてやる",
              "maleVariant": "",
              "stringId": 1905864164869955584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "タグはその約束の証だ",
              "maleVariant": "",
              "stringId": 1905865422389403648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしも裏切らない",
              "maleVariant": "俺も裏切らない",
              "stringId": 1905865876951293952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしも… きっと裏切らない",
              "maleVariant": "俺も… きっと裏切らない",
              "stringId": 1905868585364381696
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ご厚意はありがたいけど、あたしのものをあたしに譲るなんて、あんたにはできない",
              "maleVariant": "ご厚意はありがたいが、俺のものを俺に譲るなんて、お前にはできない",
              "stringId": 1905868679618781184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この体は元からあたしのものよ",
              "maleVariant": "この体は元から俺のものだ",
              "stringId": 1905879287953121280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺のために銃弾を受けられるか？",
              "maleVariant": "",
              "stringId": 1911095734761447424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうなんだ？",
              "maleVariant": "",
              "stringId": 1911097118814658560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから連れて来たんだ",
              "maleVariant": "",
              "stringId": 1911181482625884160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この体は元からずっとあたしのものよ。決めるのはよそ者じゃなく、このあたし",
              "maleVariant": "この体は元からずっと俺のものだ。決めるのはよそ者じゃなく、この俺だ",
              "stringId": 1911245960671420416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "デタラメね",
              "maleVariant": "デタラメだな",
              "stringId": 1911303970160664576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "デタラメね",
              "maleVariant": "デタラメだな",
              "stringId": 1911304362311311360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう大丈夫そう。行きましょ",
              "maleVariant": "もう大丈夫そうだ。行こう",
              "stringId": 1911343588230848512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心臓辺りにまだ違和感があるがな",
              "maleVariant": "",
              "stringId": 1911345390472945664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは消えなさそうよ",
              "maleVariant": "それは消えなさそうだ",
              "stringId": 1911346624739172352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ホテル・ピスティス・ソフィア… だっけ？",
              "maleVariant": "",
              "stringId": 1911347338794262528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ死なないよ",
              "maleVariant": "まだ死なないさ",
              "stringId": 1911348527174803456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。死に場所にはいいだろ",
              "maleVariant": "",
              "stringId": 1911348865671913472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「地獄から追い出されちまったから、顔見に来てやったぜ」",
              "maleVariant": "",
              "stringId": 1955066068768718848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「魂のムショから出て来たんだ。おニューのケツを見てくれよ」",
              "maleVariant": "",
              "stringId": 1955066210301313024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "答えるんだ",
              "maleVariant": "",
              "stringId": 1956059203080052736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[窓から入る]",
              "maleVariant": "",
              "stringId": 1969065519484710912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "窓から入れ。それくらいなんとかやれるだろ",
              "maleVariant": "",
              "stringId": 2014060381025624064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ひとまず… この話はここまでにしよう",
              "maleVariant": "",
              "stringId": 2014079717001945088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう信じてるぜ",
              "maleVariant": "",
              "stringId": 2014080431610683392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前をここに連れて来た理由だが――",
              "maleVariant": "",
              "stringId": 2014088402868658176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして突然スマッシャーの話を？",
              "maleVariant": "",
              "stringId": 2018587550526754816
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "は？ スマッシャー？ なんで急にあいつの話を？",
              "maleVariant": "",
              "stringId": 2018588018258759680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつと前回会った時は…",
              "maleVariant": "",
              "stringId": 2018588756221382656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まともに挨拶すらできなかったからな。今度こそケリをつけるんだ",
              "maleVariant": "",
              "stringId": 2018589496969023488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またハンドルを握れて満足した？",
              "maleVariant": "またハンドルを握れて満足したか？",
              "stringId": 2061785429834412036
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バレバレよ",
              "maleVariant": "バレバレだ",
              "stringId": 2061866634848186372
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
