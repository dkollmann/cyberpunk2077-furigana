    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "説得するようパナムに頼まれたのか？",
              "maleVariant": "",
              "stringId": 1882126218098921472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムはこのことを知らない",
              "maleVariant": "",
              "stringId": 1882127030334279684
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたと話すとは言ってない",
              "maleVariant": "",
              "stringId": 1882127643491192832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムはキャシディたちを説得した",
              "maleVariant": "",
              "stringId": 1882130791534448644
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バシリスクの件だけど、パナムはキャシディやキャロルたち数人を引き入れたよ",
              "maleVariant": "バシリスクの件だが、パナムはキャシディやキャロルたち数人を引き入れたぞ",
              "stringId": 1882131091645288448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "二人のことには口出ししたくない",
              "maleVariant": "",
              "stringId": 1882150141972729856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "口出しはしたくない",
              "maleVariant": "",
              "stringId": 1882152400085340160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほう。それで？",
              "maleVariant": "",
              "stringId": 1882156311710027776
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "賢明だな",
              "maleVariant": "",
              "stringId": 1882156921310171136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ！ まだわからないのか！",
              "maleVariant": "",
              "stringId": 1882159333487955968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あと何人犠牲にしたら、勝手な行動をやめるんだ！",
              "maleVariant": "",
              "stringId": 1882159810799751168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムはよかれと思って――",
              "maleVariant": "",
              "stringId": 1882160431154089984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムは何もわかっちゃいない！ ラフェンとの問題があるってのに、あいつはミリテクまで敵に回そうとしてる",
              "maleVariant": "",
              "stringId": 1882182217224704000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バシリスクを奪うなんて無謀よ",
              "maleVariant": "バシリスクを奪うなんて無謀だ",
              "stringId": 1883884492749033472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてバイオテクニカとの取引に躍起になってるの？",
              "maleVariant": "どうしてバイオテクニカとの取引に躍起になってるんだ？",
              "stringId": 1883885066815033344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バシリスクを奪うなんて無謀よ。嫌な予感がする",
              "maleVariant": "バシリスクを奪うなんて無謀だ。嫌な予感がする",
              "stringId": 1883889573141696512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バイオテクニカとの取引に躍起になってるのはどうして？",
              "maleVariant": "バイオテクニカとの取引に躍起になってるのはどうしてだ？",
              "stringId": 1883889893318086656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "安全で、報酬もいい",
              "maleVariant": "",
              "stringId": 1883892930648494080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムはその仕事のことを…",
              "maleVariant": "",
              "stringId": 1883893268524847104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見下してるか？ そりゃあ俺だって、できるならもっと派手なことをしたい",
              "maleVariant": "",
              "stringId": 1883894403604172800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "車に火が付いてる時に、新しいホイールのことなんか考えんだろ",
              "maleVariant": "",
              "stringId": 1883894741564411904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だがパナムと違い、俺はこのクランのひどい現状をわかってる",
              "maleVariant": "",
              "stringId": 1883897032224821248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クランの全員がそう思ってるさ。バカでもわかる… ああ、言い方が悪かったな",
              "maleVariant": "",
              "stringId": 1883923832283877376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全員じゃない",
              "maleVariant": "",
              "stringId": 1883926812437532672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一体、何が言いたいんだ、V？",
              "maleVariant": "",
              "stringId": 1883927256563994624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムも同じくらい賢明で、面倒に足を突っ込まなきゃいいんだが",
              "maleVariant": "",
              "stringId": 1884978982100422656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね",
              "maleVariant": "そうだな",
              "stringId": 1884980403600379904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "動きがあったら教えてくれ",
              "maleVariant": "",
              "stringId": 1884980601152098304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しかしなぜ俺に？ あいつの味方だと思ったが",
              "maleVariant": "",
              "stringId": 1885038179819212800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは… よくないね…",
              "maleVariant": "そいつは… よくないな…",
              "stringId": 1885039366119378944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰の味方でもなく、クランのため",
              "maleVariant": "",
              "stringId": 1885040302959120388
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムのことが心配で",
              "maleVariant": "",
              "stringId": 1885040767537008640
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知りたいだろうと思って",
              "maleVariant": "",
              "stringId": 1885041096336887808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんな計画はあり得ない",
              "maleVariant": "",
              "stringId": 1885044184401596416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰の味方でもない。あたしが考えてるのはクランのことよ",
              "maleVariant": "誰の味方でもない。俺が考えてるのはクランのことだ",
              "stringId": 1885051703580708864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはパナムのことが心配なだけ。いつも… 考えも無しに飛び出してくから",
              "maleVariant": "俺はパナムのことが心配なだけだ。いつも… 考えも無しに飛び出してくから",
              "stringId": 1885052064139857920
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知りたいだろうと思っただけ",
              "maleVariant": "知りたいだろうと思っただけだ",
              "stringId": 1885052380893696000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムの計画はあり得ない。はっきり言うけど、あれは自殺行為よ",
              "maleVariant": "パナムの計画はあり得ない。はっきり言うが、あれは自殺行為だ",
              "stringId": 1885053368249634816
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それに今回は、あんたが正しいと思う。パナムは自分と仲間の命を、意味もなく危険にさらしてる",
              "maleVariant": "",
              "stringId": 1885057654224343040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう",
              "maleVariant": "",
              "stringId": 1885058477096456192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつも手遅れになってから事態に気付くんだ",
              "maleVariant": "",
              "stringId": 1885063310578831360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムはきっとブチギレるでしょうね",
              "maleVariant": "パナムはきっとブチギレるだろうな",
              "stringId": 1885067329560924160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "冷静さを取り戻したら、自分の過ちに気づくはずだ",
              "maleVariant": "",
              "stringId": 1885068010497789952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろんだ",
              "maleVariant": "",
              "stringId": 1885076742602510336
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつは俺を追放したがっているようだな",
              "maleVariant": "",
              "stringId": 1885079450025742336
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムだって、クランのためを思ってるのよ",
              "maleVariant": "パナムだって、クランのためを思ってるんだ",
              "stringId": 1885082408603897856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "分裂させてどうするんだ",
              "maleVariant": "",
              "stringId": 1885083837049622528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ではまたな",
              "maleVariant": "",
              "stringId": 1885084544729370624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よく話してくれたな",
              "maleVariant": "",
              "stringId": 1885095825158983680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "たった数人で警備の厳しいミリテクのコンボイを襲うなんて",
              "maleVariant": "",
              "stringId": 1885100206394724352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よりによって、クランの力を合わせないといけないこの大事な時に勝手なことを",
              "maleVariant": "",
              "stringId": 1885149625982930944
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}