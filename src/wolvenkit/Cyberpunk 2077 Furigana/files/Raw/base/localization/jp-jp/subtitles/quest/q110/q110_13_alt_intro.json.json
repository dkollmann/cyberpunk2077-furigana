    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1695167131789840384,
            "femaleVariant": "これは… まるで… すべてが生きてるみたい",
            "maleVariant": "これは… まるで… すべてが生きてるみたいだ"
          },
          {
            "stringId": 1698455700167266304,
            "femaleVariant": "サイバー空間、常に動き、呼吸する"
          },
          {
            "stringId": 1698456372195430400,
            "femaleVariant": "脳の潜在能力に合わせて、データフローの満ち引きが調整される"
          },
          {
            "stringId": 1698458056896352256,
            "femaleVariant": "お前の場合… かなり特殊なようだ"
          },
          {
            "stringId": 1698461584406679552,
            "femaleVariant": "で、いつ始める？",
            "maleVariant": "で、いつ始めるんだ？"
          },
          {
            "stringId": 1698463147875438592,
            "femaleVariant": "いつでも。準備はできてる"
          },
          {
            "stringId": 1698465897224933376,
            "femaleVariant": "断片を回収するため、シルヴァーハンドの神経網とのリンクを強化した"
          },
          {
            "stringId": 1698468660935725056,
            "femaleVariant": "これから、オルトのデータのみを見つけ出す"
          },
          {
            "stringId": 1698476514971291648,
            "femaleVariant": "時間はそれほど…"
          },
          {
            "stringId": 1698498645444595712,
            "femaleVariant": "ああ。必要となるシルヴァーハンドの断片をコピーした"
          },
          {
            "stringId": 1698500107344089088,
            "femaleVariant": "これでオルトとコンタクトが取れるだろう"
          },
          {
            "stringId": 1698504043446734848,
            "femaleVariant": "あんたも向こう側に何があるか知らないわけ？",
            "maleVariant": "あんたも向こう側に何があるか知らないのか？"
          },
          {
            "stringId": 1698521865128316928,
            "femaleVariant": "役目は果たした。次はそっちの番よ",
            "maleVariant": "役目は果たした。次はそっちの番だ"
          },
          {
            "stringId": 1698524569649090560,
            "femaleVariant": "壁を超える前に、ジョニーの精神を取り除いて",
            "maleVariant": "壁を超える前に、ジョニーの精神を取り除いてくれ"
          },
          {
            "stringId": 1698526104495919104,
            "femaleVariant": "私、壁を越えない"
          },
          {
            "stringId": 1710203183566229504,
            "femaleVariant": "お前をサイバー空間に連れて行く。コンストラクトと対話するために"
          },
          {
            "stringId": 1710204274185932800,
            "femaleVariant": "まずジョニーが必要な理由を教えて",
            "maleVariant": "まずジョニーが必要な理由を教えろ"
          },
          {
            "stringId": 1710205290918776832,
            "femaleVariant": "戻れる保証はあるの？",
            "maleVariant": "戻れる保証はあるのか？"
          },
          {
            "stringId": 1710207009811664896,
            "femaleVariant": "どうやってオルトと接触を？"
          },
          {
            "stringId": 1710208961991741440,
            "femaleVariant": "一応聞くけど、本当に戻ってこれるんでしょうね",
            "maleVariant": "一応聞くが、本当に戻ってこれるんだろうな"
          },
          {
            "stringId": 1710217524378144768,
            "femaleVariant": "我々がしっかりとお前を監視する"
          },
          {
            "stringId": 1710221128845561856,
            "femaleVariant": "サイバー空間がどんな場所か知れば、文句言えなくなるだろう"
          },
          {
            "stringId": 1710222880621793280,
            "femaleVariant": "余計に不安ね",
            "maleVariant": "余計に不安だ"
          },
          {
            "stringId": 1710228160076124160,
            "femaleVariant": "普通のネットランニングとどう違うの？",
            "maleVariant": "普通のネットランニングとどう違うんだ？"
          },
          {
            "stringId": 1710230618508693504,
            "femaleVariant": "普通のネットランニングとどう違うの？",
            "maleVariant": "普通のネットランニングとどう違うんだ？"
          },
          {
            "stringId": 1710230618508693516,
            "femaleVariant": "ネットのデータ層を直接見るだけでなく、中身も読み取れる"
          },
          {
            "stringId": 1710230618525470728,
            "femaleVariant": "普通のネットランナーが見るの、ただの地図――表面だけ。それが限界だ"
          },
          {
            "stringId": 1710232243432394752,
            "femaleVariant": "チップからシルヴァーハンドの記憶痕跡を一部切り取る"
          },
          {
            "stringId": 1710236211378196480,
            "femaleVariant": "向こう側にいるオルトに、人間らしさが欠片でも残っていれば、必ず応える"
          },
          {
            "stringId": 1710237982498557952,
            "femaleVariant": "ジョニー。あんたが餌よ。それでいい？",
            "maleVariant": "ジョニー。お前が餌だ。それでいいか？"
          },
          {
            "stringId": 1710239780093366272,
            "femaleVariant": "構わねえよ。さっさとやれ"
          },
          {
            "stringId": 1710241058919878656,
            "femaleVariant": "あんたたちは、なんでオルトを必要としてるの？",
            "maleVariant": "あんたたちは、なぜオルトを必要としてるんだ？"
          },
          {
            "stringId": 1710258908199981056,
            "femaleVariant": "あんたたちは、なんでオルトを必要としてるの？",
            "maleVariant": "あんたたちは、なぜオルトを必要としてるんだ？"
          },
          {
            "stringId": 1710260073914503168,
            "femaleVariant": "ブラックウォールができたとき、皆ICEで十分だと思っていた。危険なAIを防げると"
          },
          {
            "stringId": 1710262772647137280,
            "femaleVariant": "これがBBS、データ要塞――"
          },
          {
            "stringId": 1710263956044845056,
            "femaleVariant": "深淵の水際"
          },
          {
            "stringId": 1710265920874942464,
            "femaleVariant": "この先に果てはない"
          },
          {
            "stringId": 1710266412363485184,
            "femaleVariant": "黒き壁の先"
          },
          {
            "stringId": 1710272156311818240,
            "femaleVariant": "ブラックウォールって結局何なの？",
            "maleVariant": "ブラックウォールって結局何なんだ？"
          },
          {
            "stringId": 1710272501117161472,
            "femaleVariant": "実際ブラックウォールって何なの？ 何となく知ってはいるけど…",
            "maleVariant": "実際ブラックウォールって何なんだ？ 何となく知ってはいるが…"
          },
          {
            "stringId": 1710272501117161484,
            "femaleVariant": "ネットウォッチに聞いたのか？"
          },
          {
            "stringId": 1710272501117161496,
            "femaleVariant": "「AIに対する最後の砦。我々人類が混沌に勝利した証」"
          },
          {
            "stringId": 1710272501133938712,
            "femaleVariant": "だが実のところ、ブラックウォールもAIだ。どちら側にいようが、何者も通さない――"
          },
          {
            "stringId": 1710298337761366016,
            "femaleVariant": "AIからすれば、ブラックウォール、“裏切り者”だ"
          },
          {
            "stringId": 1710299612158693376,
            "femaleVariant": "ネットウォッチがずっと言い続けてるヤツね",
            "maleVariant": "ネットウォッチがずっと言い続けてるヤツだな"
          },
          {
            "stringId": 1710302965689290752,
            "femaleVariant": "だが技術の進歩を止めることできない。それに、カタストロフの再来すぐそこに迫っている。避けることできない"
          },
          {
            "stringId": 1710308714486239232,
            "femaleVariant": "オルトの力を借り、ブラックウォールの向こう側に行く"
          },
          {
            "stringId": 1710309208692690944,
            "femaleVariant": "変革？"
          },
          {
            "stringId": 1710312539423051776,
            "femaleVariant": "変革の時が来ても、我々が生き残るために"
          },
          {
            "stringId": 1710318153465401344,
            "femaleVariant": "そして戻った者いない"
          },
          {
            "stringId": 1710319172849045504,
            "femaleVariant": "向こう側へ行った者、少ない"
          },
          {
            "stringId": 1710319765554532352,
            "femaleVariant": "彼女が… 初めてになる"
          },
          {
            "stringId": 1711432609935200256,
            "femaleVariant": "企業の連中、危険に気づいていない。だが我々、違う。今から備えておく"
          },
          {
            "stringId": 1711450250540290052,
            "femaleVariant": "まさかこれに入るの？",
            "maleVariant": "まさかこれに入るのか？"
          },
          {
            "stringId": 1711453696647565312,
            "femaleVariant": "氷風呂？ 勘弁してよ",
            "maleVariant": "氷風呂？ 勘弁してくれ"
          },
          {
            "stringId": 1711457799851282432,
            "femaleVariant": "サイバー空間に送り込むために必要だ"
          },
          {
            "stringId": 1711465607598510080,
            "femaleVariant": "神経への負荷を軽減する"
          },
          {
            "stringId": 1711466435956772864,
            "femaleVariant": "V、諦めて言われたとおりにしろ。丸焼きになるよりマシだ"
          },
          {
            "stringId": 1711482561461817344,
            "femaleVariant": "ふうう…"
          },
          {
            "stringId": 1712793838918373376,
            "femaleVariant": "<kiroshi l=\"creo\" o=\"Vire kouran.\" t=\"電源を\" b=\"\" a=\"\"/>"
          },
          {
            "stringId": 1712929946431983616,
            "femaleVariant": "ブラックウォールを超える前に、コンストラクトを取り除いて",
            "maleVariant": "ブラックウォールを超える前に、コンストラクトを取り除いてくれ"
          },
          {
            "stringId": 1744394999535276032,
            "femaleVariant": "<kiroshi l=\"creo\" o=\"Tout bagay pare. Nou ka kòmanse.\" t=\"準備完了。いつでもいけます\" b=\"\" a=\"\"/>"
          },
          {
            "stringId": 1744601818568364036,
            "femaleVariant": "あんたが餌よ",
            "maleVariant": "お前が餌だ"
          },
          {
            "stringId": 1744726950779588608,
            "femaleVariant": "入れ"
          },
          {
            "stringId": 1745841678133301248,
            "femaleVariant": "<kiroshi l=\"creo\" o=\"Bon.\" t=\"よし\" b=\"\" a=\"\"/>"
          },
          {
            "stringId": 1745849880077996032,
            "femaleVariant": "<kiroshi l=\"creo\" o=\"Li pare.\" t=\"準備完了\" b=\"\" a=\"\"/>"
          },
          {
            "stringId": 1751949491834261504,
            "femaleVariant": "[入る] 始めましょ",
            "maleVariant": "[入る] 始めよう"
          },
          {
            "stringId": 1751952783792476160,
            "femaleVariant": "よし"
          },
          {
            "stringId": 1754712582155177988,
            "femaleVariant": "ここはどこ？",
            "maleVariant": "ここはどこだ？"
          },
          {
            "stringId": 1761880934798659584,
            "femaleVariant": "つ、次は？"
          },
          {
            "stringId": 1761887312422440964,
            "femaleVariant": "次は？"
          },
          {
            "stringId": 1761895032894590980,
            "femaleVariant": "ジョニー？"
          },
          {
            "stringId": 1761895789412814848,
            "femaleVariant": "ジョニー？"
          },
          {
            "stringId": 1766150373731799040,
            "femaleVariant": "何だ、こんなもんなの",
            "maleVariant": "何だ、こんなもんなのか"
          },
          {
            "stringId": 1766150894630801408,
            "femaleVariant": "何だ、こんなもんなの",
            "maleVariant": "何だ、こんなもんなのか"
          },
          {
            "stringId": 1766151244821630976,
            "femaleVariant": "ここはどこ？",
            "maleVariant": "ここは？"
          },
          {
            "stringId": 1770694349373640704,
            "femaleVariant": "<kiroshi l=\"creo\" o=\"Pare plonje.\" t=\"ダイブできるぞ\" b=\"\" a=\"\"/>"
          },
          {
            "stringId": 1770696815926382592,
            "femaleVariant": "<kiroshi l=\"creo\" o=\"Ki moun ki pral pwoteje li?\" t=\"誰がこいつを守るんだ？\" b=\"\" a=\"\"/>"
          },
          {
            "stringId": 1770707973932650496,
            "femaleVariant": "<kiroshi l=\"creo\" o=\"Leon avec Jean-ki-ri.\" t=\"レオンとジャン・キリよ\" b=\"\" a=\"\"/>"
          },
          {
            "stringId": 1770712812934729728,
            "femaleVariant": "<kiroshi l=\"creo\" o=\"Direksyon konekte.\" t=\"フローをリンク中\" b=\"\" a=\"\"/>"
          },
          {
            "stringId": 1770718902963474432,
            "femaleVariant": "<kiroshi l=\"creo\" o=\"Koneksyon estab.\" t=\"接続安定\" b=\"\" a=\"\"/>"
          },
          {
            "stringId": 1792268198003150848,
            "femaleVariant": "独自のシンステックインターフェースがあるようね",
            "maleVariant": "独自のシンステックインターフェースがあるようだな"
          },
          {
            "stringId": 1947384246858174464,
            "femaleVariant": "まず、どうしてシルヴァーハンドが必要なのか教えて",
            "maleVariant": "まず、どうしてシルヴァーハンドが必要なのか教えろ"
          },
          {
            "stringId": 1947388088622534656,
            "femaleVariant": "オルト・カニンガムと接触するためだ。二人は親しかった"
          },
          {
            "stringId": 1947388315484049408,
            "femaleVariant": "オルト？"
          },
          {
            "stringId": 1947389177849729024,
            "femaleVariant": "アラサカの〈Relic〉。それが唯一の突破口だ"
          },
          {
            "stringId": 1947390385842835456,
            "femaleVariant": "オルト・カニンガム… 聞いたことがある"
          },
          {
            "stringId": 1947390613627097088,
            "femaleVariant": "ほう？ 誰から聞いたんだ？"
          },
          {
            "stringId": 1947392754903175172,
            "femaleVariant": "どうやってオルトと接触するの？",
            "maleVariant": "どうやってオルトと接触するんだ？"
          },
          {
            "stringId": 1947396937211465728,
            "femaleVariant": "ジョニーから聞いた"
          },
          {
            "stringId": 1947403995973439488,
            "femaleVariant": "ジョニーよ。詳しくは聞いてないけど",
            "maleVariant": "ジョニーだ。詳しくは聞いてないが"
          },
          {
            "stringId": 1947404777103839232,
            "femaleVariant": "ジョニー？"
          },
          {
            "stringId": 1947406234456707084,
            "femaleVariant": "V、こうなったら話した方がいい"
          },
          {
            "stringId": 1947406234456707100,
            "femaleVariant": "何を？",
            "maleVariant": "何をだ？"
          },
          {
            "stringId": 1947406234473484288,
            "femaleVariant": "俺たちのことを"
          },
          {
            "stringId": 1947406900193415172,
            "femaleVariant": "どうやってオルトと接触を？"
          },
          {
            "stringId": 1947407260115030016,
            "femaleVariant": "ジョニーのコンストラクトは覚醒してる"
          },
          {
            "stringId": 1947407790761594880,
            "femaleVariant": "ジョニーは覚醒してる。おかげでうるさくてたまらない"
          },
          {
            "stringId": 1947409036872536064,
            "femaleVariant": "向こう側って何よ？ 壁を超えるなんて聞いてない",
            "maleVariant": "向こう側ってなんだ？ 壁を超えるなんて聞いてない"
          },
          {
            "stringId": 1947409476016164864,
            "femaleVariant": "今聞いただろう"
          },
          {
            "stringId": 1947487816991395840,
            "femaleVariant": "どういうことよ。あたしが妄想してるって言いたいわけ？",
            "maleVariant": "どういうことだ。俺が妄想してるって言いたいのか？"
          },
          {
            "stringId": 1947491792621162496,
            "femaleVariant": "まあ、ついでに調べてもいいが…"
          },
          {
            "stringId": 1947494987271524352,
            "femaleVariant": "記憶痕跡が人体に及ぼす影響、わからない"
          },
          {
            "stringId": 1947496159713710080,
            "femaleVariant": "当人にとって厄介だってことはわかってるけどね",
            "maleVariant": "当人にとって厄介だってことはわかってるけどな"
          },
          {
            "stringId": 1948740867425325056,
            "femaleVariant": "独自のシンステックインターフェースがあるようね",
            "maleVariant": "独自のシンステックインターフェースがあるようだな"
          },
          {
            "stringId": 1948742621315485696,
            "femaleVariant": "シンステックだけではない。機材はすべてカスタムだ"
          },
          {
            "stringId": 1954797615491862528,
            "femaleVariant": "何が起きてるの！？",
            "maleVariant": "何が起きてるんだ！？"
          },
          {
            "stringId": 1954797615575748608,
            "femaleVariant": "BBSに侵入された。我々の仕業でない！"
          },
          {
            "stringId": 1954797615726743552,
            "femaleVariant": "ジョリス、撤退だ！"
          },
          {
            "stringId": 1954797615911292928,
            "femaleVariant": "我々の――"
          },
          {
            "stringId": 1955045356872683520,
            "femaleVariant": "コンストラクトの状態はどう？",
            "maleVariant": "コンストラクトの状態はどうだ？"
          },
          {
            "stringId": 1955046255426818048,
            "femaleVariant": "これだけ無傷のデータあれば、オルトも信用する"
          },
          {
            "stringId": 1955048562663116800,
            "femaleVariant": "それはどういう意味？",
            "maleVariant": "それはどういう意味だ？"
          },
          {
            "stringId": 1967659568530739200,
            "femaleVariant": "オルト…？ どうなってるの？",
            "maleVariant": "オルト…？ どうなってるんだ？"
          },
          {
            "stringId": 1973499905258049536,
            "femaleVariant": "何をしている？ グズグズしてる間に氷が溶けるぞ"
          },
          {
            "stringId": 2009650982999052288,
            "femaleVariant": "…かからない"
          },
          {
            "stringId": 2009652183140421632,
            "femaleVariant": "上手くいったの？",
            "maleVariant": "上手くいったのか？"
          },
          {
            "stringId": 2009653045237665792,
            "femaleVariant": "ずいぶん時間が経ったように感じたけど",
            "maleVariant": "ずいぶん時間が経ったように感じたが"
          },
          {
            "stringId": 2009653692972421120,
            "femaleVariant": "すごく長く感じたんだけど",
            "maleVariant": "すごく長く感じたんだが"
          },
          {
            "stringId": 2009656985299734528,
            "femaleVariant": "で？ 上手くいったの？",
            "maleVariant": "で… 上手くいったのか？"
          },
          {
            "stringId": 2009667208429195264,
            "femaleVariant": "ジョニーと… オルトの過去をたっぷり見た"
          },
          {
            "stringId": 2009670276612902912,
            "femaleVariant": "あり得ることだ。オルトに関するデータ、コンストラクト内の極めて強烈な記憶と繋がっている"
          },
          {
            "stringId": 2009674972119453696,
            "femaleVariant": "その記憶が強い感情反応を引き起こし、お前の意識に投影されたのだろう"
          },
          {
            "stringId": 2009682154932162560,
            "femaleVariant": "感情ね… 確かにすごかった"
          },
          {
            "stringId": 2009706083100938244,
            "femaleVariant": "で、どうするの？",
            "maleVariant": "で、どうするんだ？"
          },
          {
            "stringId": 2009706832958607360,
            "femaleVariant": "すべてが… 生きてるみたい",
            "maleVariant": "すべてが… 生きてるみたいだ"
          },
          {
            "stringId": 2009712094075441152,
            "femaleVariant": "何か言いたげね",
            "maleVariant": "何か言いたげだな"
          },
          {
            "stringId": 2009712572427423744,
            "femaleVariant": "ああ。サイバー空間は常に… 美しい"
          },
          {
            "stringId": 2009729601771425792,
            "femaleVariant": "もっと深く潜るぞ"
          },
          {
            "stringId": 2009740179369906176,
            "femaleVariant": "コンストラクトの状態は？"
          },
          {
            "stringId": 2009831679554506756,
            "femaleVariant": "断れる状況じゃないようね",
            "maleVariant": "断れる状況じゃないようだな"
          },
          {
            "stringId": 2009831885293506560,
            "femaleVariant": "断れる状況じゃないようね",
            "maleVariant": "断れる状況じゃないようだな"
          },
          {
            "stringId": 2009833316390031360,
            "femaleVariant": "あたしをハメたのね…",
            "maleVariant": "俺をハメたのか…"
          },
          {
            "stringId": 2009834419575869440,
            "femaleVariant": "つまり… あんたも向こう側に何があるか知らないのね",
