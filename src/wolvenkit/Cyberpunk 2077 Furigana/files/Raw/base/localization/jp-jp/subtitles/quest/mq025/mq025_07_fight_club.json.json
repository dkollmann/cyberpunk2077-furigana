  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあ、次は誰？ 他にいないの？",
                    "maleVariant": null,
                    "stringId": 1856056797705367564
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "試合をしにきた",
                    "maleVariant": null,
                    "stringId": 1856056797755699200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "試合をしにきた",
                    "maleVariant": null,
                    "stringId": 1856056797822808068
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "へえ… 見慣れない顔だね。新参者かい？",
                    "maleVariant": null,
                    "stringId": 1856056797856362504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "準備OK",
                    "maleVariant": null,
                    "stringId": 1856056797873139712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "また後で来る",
                    "maleVariant": null,
                    "stringId": 1856056797873139728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "このクラブを仕切ってるのは誰？",
                    "maleVariant": "このクラブを仕切ってるのは誰だ？",
                    "stringId": 1856056797889916936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待ってて。また後で来る",
                    "maleVariant": "待っててくれ。また後で来る",
                    "stringId": 1856056797973803012
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいよ。だが気を付けるんだね… 脚を折られちまわないようにさ",
                    "maleVariant": null,
                    "stringId": 1856056797973803020
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何事にも初めてはあるでしょ。敗北の味を教えてあげる",
                    "maleVariant": "何事にも初めてはあるだろ。敗北の味を教えてやるよ",
                    "stringId": 1856056798024134660
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハハ！ そうだね… 確かに。よし、やるかい？",
                    "maleVariant": null,
                    "stringId": 1856056798057689092
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よしきた。なら試合開始だ！",
                    "maleVariant": null,
                    "stringId": 1856056798141575172
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ！ わかった、負けだ！ もういい！",
                    "maleVariant": null,
                    "stringId": 1856056798259015684
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あーっ！！！ クソ！！！！",
                    "maleVariant": null,
                    "stringId": 1856056798292570112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあまあ、落ち着いて…",
                    "maleVariant": "まあまあ… 落ち着けよ…",
                    "stringId": 1856056798292570120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "勝った勝った、勝った勝ったー！",
                    "maleVariant": null,
                    "stringId": 1856056798326124548
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかってる…",
                    "maleVariant": null,
                    "stringId": 1856056798326124556
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[試合を開始する] やりましょ",
                    "maleVariant": "[試合を開始する] やろう",
                    "stringId": 1856056798477119488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "始めましょ",
                    "maleVariant": "始めよう",
                    "stringId": 1856056798544228356
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここを仕切ってるのは誰なの？",
                    "maleVariant": "ここを仕切ってるのは誰なんだ？",
                    "stringId": 1856056798577782788
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はっ！ それはこっちの台詞だよ。来な",
                    "maleVariant": null,
                    "stringId": 1856056798611337220
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここはよそ者を嫌う。一度怪しい真似をするだけで、脚を折られて床をはい回ることになるよ",
                    "maleVariant": null,
                    "stringId": 1856064206993715200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だけど… 私を倒せば、仲間に紹介してやろう。そうすれば機材を使えたり、トレーナーに会えたりするわけだ",
                    "maleVariant": null,
                    "stringId": 1856064550473658368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どういたしまして、と言いたいとこだけど、私を倒すのが先だよ。ちなみに、私は負けなしだから",
                    "maleVariant": null,
                    "stringId": 1856065094592327680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "賭け金はどうするの？",
                    "maleVariant": "賭け金はどうするんだ？",
                    "stringId": 1856144665253605376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここじゃエディーは賭けないの？",
                    "maleVariant": "ここじゃエディーは賭けないのか？",
                    "stringId": 1856144895923548160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "賭けない。カネが絡むと色々面倒だ。私は純粋に戦いたいからここにいるんだよ",
                    "maleVariant": null,
                    "stringId": 1856145241064435712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ローガン・ガルシアって元ボクサーだ。昔はナイトシティで一番って言われてた。全米一だったかもな",
                    "maleVariant": null,
                    "stringId": 1856145741159690240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "戦うところを見ればわかる",
                    "maleVariant": null,
                    "stringId": 1856146763244466176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これで体が温まったろ。もう一戦やるかい？",
                    "maleVariant": null,
                    "stringId": 1856158739408449536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "落ち着け？ 落ち着け！？ 落ち着けって言ったのかい！？ ウォォォ！！！",
                    "maleVariant": null,
                    "stringId": 1856160907142197248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなとこ",
                    "maleVariant": "そんなとこだ",
                    "stringId": 1908317442657275904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "へえ、それは助かるね",
                    "maleVariant": "へえ、それは助かるな",
                    "stringId": 1908321770122039296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いくら何でも大げさじゃない？",
                    "maleVariant": "いくら何でも大げさじゃないか？",
                    "stringId": 1908329323224682496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はあ… す… すまない…",
                    "maleVariant": null,
                    "stringId": 1908350099189452800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "負けるのに慣れてないのね",
                    "maleVariant": "負けるのに慣れてないんだな",
                    "stringId": 1908350566401363968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ。慣れたくもないしな",
                    "maleVariant": null,
                    "stringId": 1908350800762294272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はあ… あんまり調子に乗らないようにだけ、気をつけるんだね",
                    "maleVariant": null,
                    "stringId": 1908352263534202880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いい戦いだった",
                    "maleVariant": null,
                    "stringId": 1908353207017721860
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなつもりはない。ただお礼を言いたくて",
                    "maleVariant": "そんなつもりはない。ただ礼を言いたくてな",
                    "stringId": 1908353709008801792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたの負けよ。愚痴はやめて",
                    "maleVariant": "あんたの負けだ。愚痴はやめろ",
                    "stringId": 1908354573454856192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "惨めな台詞ね。あんたは負けたの。それを認めるのね",
                    "maleVariant": "惨めな台詞だな。あんたは負けた。それを認めろ",
                    "stringId": 1908354942385836032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "説教なんていらないよ。さっさと消えな",
                    "maleVariant": null,
                    "stringId": 1908355866122571776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何の？",
                    "maleVariant": null,
                    "stringId": 1908357714552344576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "フェアな戦いをしてくれたことによ。卑劣な手を使わないなんて、最近じゃ珍しいから",
                    "maleVariant": "フェアな戦いをしてくれたことにだ。卑劣な手を使わないなんて、最近じゃ珍しいからな",
                    "stringId": 1908357864876199936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたいい奴ね",
                    "maleVariant": "あんたはいい奴だ",
                    "stringId": 1908361404331905024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なあ、私は今日は上がりだ。誰にもちょっかい出させないから、くつろいでっていいよ",
                    "maleVariant": null,
                    "stringId": 1908362368015192064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どんなテックを隠してる？ 感覚フィルター？ それともローターや強化コアか？",
                    "maleVariant": null,
                    "stringId": 1932994912752816128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "すぐにわかる",
                    "maleVariant": null,
                    "stringId": 1932998249304870912
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
