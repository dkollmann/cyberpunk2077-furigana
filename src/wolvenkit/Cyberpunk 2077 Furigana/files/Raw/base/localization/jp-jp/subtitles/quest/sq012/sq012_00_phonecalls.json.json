  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしもし。はじめまして、エリザベスよ",
              "maleVariant": "",
              "stringId": "1692469337170497536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "夫と私は今… デリケートな問題を抱えてて… あなたなら何とかしてくれるんじゃないかと思って連絡したの",
              "maleVariant": "",
              "stringId": "1692471575301443584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウォードだが。どちら様？",
              "maleVariant": "",
              "stringId": "1698336762961186816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vよ。ルシアス・ラインの死について調べてる",
              "maleVariant": "Vだ。ルシアス・ラインの死について調べてる",
              "stringId": "1698337584054915072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜだ？ 警官じゃないだろ",
              "maleVariant": "",
              "stringId": "1698340385984204800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインのことだけど――何が起きたのかはだいたいわかった",
              "maleVariant": "ラインのことだが――何が起きたのかはだいたいわかった",
              "stringId": "1699864349030076416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よかった。じゃあ来て。デリケートな問題だから、直接会って話したいの",
              "maleVariant": "",
              "stringId": "1699865822136750080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして私を選んだの？",
              "maleVariant": "なぜ俺を選んだんだ？",
              "stringId": "1811388777152237572"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてわざわざ今回あたしを指名したの？",
              "maleVariant": "どうしてわざわざ今回俺を指名したんだ？",
              "stringId": "1811390129546842112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はちょっと忙しくて… 話を聞けるような状態じゃない",
              "maleVariant": "",
              "stringId": "1811390764530913280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V。連絡してくれてありがとう",
              "maleVariant": "",
              "stringId": "1811392118250917888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、会いましょ",
              "maleVariant": "ああ、会おう",
              "stringId": "1892554973396033540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "夫って誰なの？",
              "maleVariant": "夫って誰なんだ？",
              "stringId": "1893669675165507584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕事の内容は？",
              "maleVariant": "",
              "stringId": "1893757636917813248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いえ。今は忙しすぎて時間がない",
              "maleVariant": "いや。今は忙しすぎて時間がない",
              "stringId": "1893829476470185984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "デリケートな問題って？",
              "maleVariant": "",
              "stringId": "1893850605410803712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "夫って？ そもそも、あんたの正体もわかってないんだけど",
              "maleVariant": "夫って？ そもそも、あんたの正体もわかってないんだが",
              "stringId": "1893851792180731904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、会いましょ",
              "maleVariant": "ああ、会おう",
              "stringId": "1893852004395737088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "電話では話したくないの。お願い、私たちと会って。ちゃんと説明するから",
              "maleVariant": "",
              "stringId": "1893887746207604736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "推薦されたのよ",
              "maleVariant": "",
              "stringId": "1893889853761478656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "電話で名前を出すのは控えておくわ。直接会って、話し合えないかしら？",
              "maleVariant": "",
              "stringId": "1893890480692486144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "繰り返すけど――デリケートな問題なの",
              "maleVariant": "",
              "stringId": "1893897678621466624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "可能であれば、直接話したいの",
              "maleVariant": "",
              "stringId": "1893901033326469120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "会ってくれる？",
              "maleVariant": "",
              "stringId": "1893901989929771008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "住所を送るから。待ってるわ",
              "maleVariant": "",
              "stringId": "1893917909444157440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう… 私たちも余裕があるわけじゃないんだけど、あと数日なら待てるかもしれないわ。あなたじゃないとダメなのよ",
              "maleVariant": "",
              "stringId": "1893957585009860608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "都合がついたら、是非連絡して",
              "maleVariant": "",
              "stringId": "1893958390618218496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたならきっとわかってくれると思ってた。来てもらえるのね？ 直接話がしたいの",
              "maleVariant": "",
              "stringId": "1893965370141401088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待ってるわ",
              "maleVariant": "",
              "stringId": "1893982585460842496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ペラレス夫妻に依頼されたの",
              "maleVariant": "ペラレス夫妻に依頼されたんだ",
              "stringId": "1893985781453381636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私的な調査をしてる",
              "maleVariant": "",
              "stringId": "1894000523425308672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "市長に警告しようとしたらしいじゃない",
              "maleVariant": "市長に警告しようとしたらしいじゃないか",
              "stringId": "1894000820130373632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジェファーソンとエリザベスに雇われてるの",
              "maleVariant": "ジェファーソンとエリザベスに雇われてるんだ",
              "stringId": "1894004565643845632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この件について私的な調査をしてるところなの",
              "maleVariant": "この件について私的な調査をしてるところなんだ",
              "stringId": "1894004587689107456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインにサイバーサイコについて警告しようとしたんでしょ",
              "maleVariant": "ラインにサイバーサイコについて警告しようとしたんだろ",
              "stringId": "1894004607016460288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は… チャビー・バッファローに行くところだ",
              "maleVariant": "",
              "stringId": "1894016676428210176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すぐ行く",
              "maleVariant": "",
              "stringId": "1894020900545323008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "市長候補が、前市長の死因についてかぎ回ってるのか？ 興味深いな",
              "maleVariant": "",
              "stringId": "1894034385132019712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちが得た情報もなかなか興味深いけどね",
              "maleVariant": "こっちが得た情報もなかなか興味深いがな",
              "stringId": "1894037550791352320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他に何を知ってる？",
              "maleVariant": "",
              "stringId": "1894041451896725504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしと会えばわかる",
              "maleVariant": "俺と会えばわかるさ",
              "stringId": "1894041711943573504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうか、頑張ってくれ。あと、この番号は消せ。探偵はカリエンテのスコップと同じくらい嫌いなんだ",
              "maleVariant": "",
              "stringId": "1894044059428413440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは探偵じゃない",
              "maleVariant": "俺は探偵じゃない",
              "stringId": "1894047806787379204"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "警官と探偵は協力し合うものでしょ",
              "maleVariant": "警官と探偵は協力し合うものだろ",
              "stringId": "1894049398374752256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは探偵じゃない",
              "maleVariant": "俺は探偵じゃない",
              "stringId": "1894060060983386112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "探偵と警察は、協力し合うものだと思ってたけど",
              "maleVariant": "探偵と警察は、協力し合うものだと思ってたが",
              "stringId": "1894074500797652992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ何なんだ？",
              "maleVariant": "",
              "stringId": "1894075499025227776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこで得た情報だ？ ニューシネマの作品か？",
              "maleVariant": "",
              "stringId": "1894081305250586624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、探偵じゃないなら、何者なんだ？",
              "maleVariant": "",
              "stringId": "1894083993044475904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "直接会えばわかる",
              "maleVariant": "",
              "stringId": "1894085834461372416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰に…？",
              "maleVariant": "",
              "stringId": "1944748168821039104"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}