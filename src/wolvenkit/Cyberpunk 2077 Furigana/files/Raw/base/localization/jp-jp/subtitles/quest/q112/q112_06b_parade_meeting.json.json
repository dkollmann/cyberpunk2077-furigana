    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ひどい顔色ね、ゴロウ",
                    "maleVariant": "ひどい顔色だな、ゴロウ",
                    "stringId": 1698151850928259076
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、来たか",
                    "maleVariant": null,
                    "stringId": 1698152285609148416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この手の仕事は慣れっこよ",
                    "maleVariant": "この手の仕事には慣れてる",
                    "stringId": 1714151792838721540
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "無茶な計画だったけど、どうにかここまで来た",
                    "maleVariant": "無茶な計画だったが、どうにかここまで来たな",
                    "stringId": 1714152521171222528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待って。プランBはないの？",
                    "maleVariant": "待て。プランBはないのか？",
                    "stringId": 1714158420795342848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ゴロウ。ひどい顔色だけど、いったいどうしたの?",
                    "maleVariant": "ゴロウ。ひどい顔色だぞ。いったいどうしたんだ",
                    "stringId": 1754342590336024576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ。そっちこそ準備はいいの？",
                    "maleVariant": "ああ。そっちこそ準備はいいのか？",
                    "stringId": 1770705954393690112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ。そっちこそ準備はいいの？",
                    "maleVariant": "ああ。そっちこそ準備はいいのか？",
                    "stringId": 1770706659019984896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何を言ってる？ 俺が立てた作戦だ",
                    "maleVariant": null,
                    "stringId": 1770843777897730048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でもずっと無視してきた問題がある。“お友達”のオダよ",
                    "maleVariant": "だがずっと無視してきた問題がある。“お友達”のオダだ",
                    "stringId": 1770844515239596032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "心配するな、オダは誇り高い男だ。俺を襲うような真似は絶対にしない",
                    "maleVariant": null,
                    "stringId": 1770847150118146048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも、あんたがハナコの前に突然現れたら？",
                    "maleVariant": "だが、あんたがハナコの前に突然現れたら？",
                    "stringId": 1770848521236123648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふむ、確かにその状況では、体が反射的に動く可能性もあるな",
                    "maleVariant": null,
                    "stringId": 1770852759982968832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で？",
                    "maleVariant": null,
                    "stringId": 1770861909186056192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まだあいつより速く動けることを祈るのみだ",
                    "maleVariant": null,
                    "stringId": 1770863709364555776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ。それもお前のおかげだ",
                    "maleVariant": null,
                    "stringId": 1770867945963917312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "礼なら終わってからにして。始めるよ",
                    "maleVariant": "礼なら終わってからにしろ。始めるぞ",
                    "stringId": 1770869128707313664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。俺の合図を待て",
                    "maleVariant": null,
                    "stringId": 1770869846486945792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待って、問題が起きた時のプランBはないの？",
                    "maleVariant": "待て、問題が起きた時のプランBはないのか？",
                    "stringId": 1770879157623721984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "聞くまでもないだろ？",
                    "maleVariant": null,
                    "stringId": 1770879606464581632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカにしてみれば俺はれっきとした反逆者だ。そんな奴がCEOの妹と直接接触しようとしている",
                    "maleVariant": null,
                    "stringId": 1770880081846996992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "代替案など存在しない。必ず成功させるんだ",
                    "maleVariant": null,
                    "stringId": 1770884174934052864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "早まらないで",
                    "maleVariant": "早まるなよ",
                    "stringId": 1770890998546567168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ゴロウ？ もし何か思い詰めてるなら、24時間やってるホットラインに相談しても…",
                    "maleVariant": null,
                    "stringId": 1770891731811569664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "見てわからんか？ 最悪の気分だ",
                    "maleVariant": null,
                    "stringId": 1770923087203536896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "笑えない冗談だ…",
                    "maleVariant": null,
                    "stringId": 1770923903230545920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "“スコップバーガー”とやらを食べてみた",
                    "maleVariant": null,
                    "stringId": 1857764568758820864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "へえ。どうだった？ 気に入った？",
                    "maleVariant": "へえ。どうだった？ 気に入ったか？",
                    "stringId": 1857765017146695680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "拾った奴に聞いてみろ",
                    "maleVariant": null,
                    "stringId": 1857765096502927360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "パレードはもう間もなく始まる",
                    "maleVariant": null,
                    "stringId": 1978141322518740992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、お前も確認しておいてくれ",
                    "maleVariant": null,
                    "stringId": 1978141885562109952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オカダさんにもらったデータを調整してみた",
                    "maleVariant": null,
                    "stringId": 1978145846998351872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "既に目と耳はそこら中にある。さっそくこの近辺にある監視カメラを確認して、狙撃手の位置を把握しておいた",
                    "maleVariant": null,
                    "stringId": 1978148528970547200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "連中への対処の仕方はいろいろある",
                    "maleVariant": null,
                    "stringId": 1978148928234733568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "1人目は距離も近いし、おそらく問題ないだろう。2人目は少々厄介で… 上に登らないといけない",
                    "maleVariant": null,
                    "stringId": 1978149303910154240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そして3人目は、通りの上の歩道橋内に隠れている",
                    "maleVariant": null,
                    "stringId": 1978149543891451904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺たちはここ… ハナコ様の山車はここで止まる。最後の脱出に使う非常口は、そのすぐそばにある",
                    "maleVariant": null,
                    "stringId": 1978149836385435648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "とはいえ俺の役目と比べれば気楽なもんだろ。なにせこっちは、山車に飛び移って窓から侵入し、ハナコ様と対面しなきゃならない",
                    "maleVariant": null,
                    "stringId": 1978150247628554240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一番心配なのは最後の部分だ",
                    "maleVariant": null,
                    "stringId": 1978150578089377792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "作戦は理解したな？ 準備はいいか？",
                    "maleVariant": null,
                    "stringId": 1978150777251708928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[チップを受け取る]",
                    "maleVariant": null,
                    "stringId": 1986660066284093444
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この複数のポイントからパレードの様子を見渡せるが、狙撃手はもっといい場所に陣取っている",
                    "maleVariant": null,
                    "stringId": 1989373431602233344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "奴らに見つかったら… バン！ 一巻の終わりだ",
                    "maleVariant": null,
                    "stringId": 1989373892472356864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあ、準備はいいか？",
                    "maleVariant": null,
                    "stringId": 1995277047608188928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まず例のウイルスだが、うまく機能してくれた。正直なところ、予想以上の働きだといってもいい",
                    "maleVariant": null,
                    "stringId": 1996736918754181120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "相手は3人。全員を無力化してくれ… 俺が撃たれる前にな",
                    "maleVariant": null,
                    "stringId": 2005737742766252032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[待つ] 気をつけて",
                    "maleVariant": "[待つ] 気をつけろよ",
                    "stringId": 2091267062757818372
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
