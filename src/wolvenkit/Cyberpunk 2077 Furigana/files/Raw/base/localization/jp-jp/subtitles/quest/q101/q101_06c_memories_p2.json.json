        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今行く！",
                    "maleVariant": null,
                    "stringId": 1496823839929032704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカだ！",
                    "maleVariant": null,
                    "stringId": 1525981087599058944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニー！",
                    "maleVariant": null,
                    "stringId": 1526837356035727360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニー！ 早く！",
                    "maleVariant": null,
                    "stringId": 1548722201804300288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "急げ！",
                    "maleVariant": null,
                    "stringId": 1548722677237047296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[ヘリに飛び乗る]",
                    "maleVariant": null,
                    "stringId": 1653079083777675268
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "スマッシャー…",
                    "maleVariant": null,
                    "stringId": 1653086404952084480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言ったろ、ジョニーボーイ",
                    "maleVariant": null,
                    "stringId": 1653087655475433472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "急ぐよ！",
                    "maleVariant": null,
                    "stringId": 1663547393697341440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "外の大騒ぎは計画のうちか？",
                    "maleVariant": null,
                    "stringId": 1693656338659438592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニーのアイデアだ。ウェイランドがアラサカの注意をそらそうとしてる",
                    "maleVariant": null,
                    "stringId": 1693665733430636544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "目標射程に到達",
                    "maleVariant": null,
                    "stringId": 1693695872138899456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よし、いけるぞ！",
                    "maleVariant": null,
                    "stringId": 1693703221968793600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マーフィー？",
                    "maleVariant": null,
                    "stringId": 1693731886244982784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "接続する",
                    "maleVariant": null,
                    "stringId": 1693743224589545472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何これ、簡単すぎ。はいはい、1+1=2、2+2=4…",
                    "maleVariant": null,
                    "stringId": 1693743833082392576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マーフィー、遊んでないでさっさと終わらせな",
                    "maleVariant": null,
                    "stringId": 1693744183642320896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと、この声明文って誰が書いたの？",
                    "maleVariant": null,
                    "stringId": 1693744703517913088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今答えてる暇はない",
                    "maleVariant": null,
                    "stringId": 1693744786045038592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マーフィー？",
                    "maleVariant": null,
                    "stringId": 1693759611265511424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニー、爆弾を",
                    "maleVariant": null,
                    "stringId": 1693762108671553536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[爆弾を仕掛ける]",
                    "maleVariant": null,
                    "stringId": 1693765898996969476
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "『ブシドーII』の爆弾… 憶えてるか？",
                    "maleVariant": null,
                    "stringId": 1693766755859722240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "早く、急がないとまずい！",
                    "maleVariant": null,
                    "stringId": 1693767433441144832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「デモリトロン」だ。セット完了",
                    "maleVariant": null,
                    "stringId": 1693815196396711936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカの精鋭部隊だ！ 逃げろ！",
                    "maleVariant": null,
                    "stringId": 1693815525817348096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "早くそこから逃げな、ジョニー！",
                    "maleVariant": null,
                    "stringId": 1693820285479972864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ケーブルを撃って！",
                    "maleVariant": null,
                    "stringId": 1693823502880239616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "離陸準備を！ すぐそっちに行く！",
                    "maleVariant": null,
                    "stringId": 1693882087995764736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なおテロリストらは犯行声明を出しており、「企業による植民地主義の象徴を打ち倒す」と主張しています",
                    "maleVariant": null,
                    "stringId": 1693899159299584000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "またナイトシティのムボール・エブニケ市長は、いかなるテロ行為に対しても「法的に許される最大限の武力をもって立ち向かう」としています",
                    "maleVariant": null,
                    "stringId": 1693899940765528064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マズい！ アダム・スマッシャーよ！",
                    "maleVariant": null,
                    "stringId": 1693903269935607808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニー、逃げて！",
                    "maleVariant": null,
                    "stringId": 1693904046603268096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ぐああ！クソ！",
                    "maleVariant": null,
                    "stringId": 1734499174250082304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニー！ シャイタンが！",
                    "maleVariant": null,
                    "stringId": 1734500204236926976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今代わる！",
                    "maleVariant": null,
                    "stringId": 1734500299766394880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "戦闘準備！",
                    "maleVariant": null,
                    "stringId": 1740097226251857920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一般人の巻き添えも上等かよ",
                    "maleVariant": null,
                    "stringId": 1764895715386482688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ガキの使いじゃないんだ。気にしてる場合じゃない",
                    "maleVariant": null,
                    "stringId": 1764896940307800064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "派手にやりな",
                    "maleVariant": null,
                    "stringId": 1764901065909100544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アクセスポイントを見つけた。移動して",
                    "maleVariant": null,
                    "stringId": 1764944441169698816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "緊急避難勧告を――全周波数で流して。まだやることはある",
                    "maleVariant": null,
                    "stringId": 1764949676197961728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ウソでしょ、ジョニー。マジでイッちゃってるね。この私が言うんだから相当だよ",
                    "maleVariant": null,
                    "stringId": 1764951325196967936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニー、作戦はわかってるでしょうね？",
                    "maleVariant": null,
                    "stringId": 1764958748813058048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "エレベーターに爆弾を仕掛けて起動。あとは重力がやってくれる",
                    "maleVariant": null,
                    "stringId": 1764961242326786048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "爆発で建物の基礎が割れ、タワーが崩壊。混沌と悲鳴のなか、エンドロールが流れる",
                    "maleVariant": null,
                    "stringId": 1764987594820050944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だからすぐに脱出しないと",
                    "maleVariant": null,
                    "stringId": 1764988099914915840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "行くよ！",
                    "maleVariant": null,
                    "stringId": 1764990680720498688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "指ぬきで探した、丁寧に探した、フォークと希望で追い立てた…",
                    "maleVariant": null,
                    "stringId": 1764998173106397184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まだやるべきことがある",
                    "maleVariant": null,
                    "stringId": 1765031469605478400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まだだ。奴らのサブネットにこいつを流さないといけない",
                    "maleVariant": null,
                    "stringId": 1765032055818182656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まだだ。奴らのサブネットにこいつを流さないといけない",
                    "maleVariant": null,
                    "stringId": 1765032153595797504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "力を貸してほしい",
                    "maleVariant": null,
                    "stringId": 1765032258017189888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やっぱり！",
                    "maleVariant": null,
                    "stringId": 1770693714205237248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "結局“企業の植民地主義”なんて関係ない、あのグルーピーの女のためなんだろ…！",
                    "maleVariant": null,
                    "stringId": 1770702881745821696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前にはわからないさ、ローグ",
                    "maleVariant": null,
                    "stringId": 1770703336509038592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "4分だけ待つ。1秒でも過ぎたら置いていくからね",
                    "maleVariant": null,
                    "stringId": 1770703430998319104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたって奴は！ 結局アラサカなんて関係ない、あのグルーピーの女のためなんだろ…！",
                    "maleVariant": null,
                    "stringId": 1770710576733380608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前にはわかりっこねえよ、ローグ",
                    "maleVariant": null,
                    "stringId": 1770714420494229504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう勝手にしな。ヘリは4分で出る。乗るつもりなら急ぐんだね",
                    "maleVariant": null,
                    "stringId": 1770714503424008192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "次に、アラサカ・タワーの状況をお伝えします。同社は正体不明のテロリストグループによる脅迫を受け、現在もなお、避難が行われている模様です",
                    "maleVariant": null,
                    "stringId": 1770727429916614656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それではここで、現地からアラサカ・タワーの様子を伝えてもらいましょう。現在の状況が少しでもわかるといいのですが――",
                    "maleVariant": null,
                    "stringId": 1770752326684401664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "完了よ。急いで脱出して",
                    "maleVariant": null,
                    "stringId": 1770754317166862336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ICEブレーカーね。外国製？ 誰かさんがサブネットプロトコルを切り替えてさえくれれば…",
                    "maleVariant": null,
                    "stringId": 1770789571952926720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふざけやがって。手伝わないなら帰れ",
                    "maleVariant": null,
                    "stringId": 1770791098511810560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "手伝ってるじゃない。そっちこそちゃんと仕事してよ",
                    "maleVariant": null,
                    "stringId": 1770791197581271040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ありがとな",
                    "maleVariant": null,
                    "stringId": 1770792797456588800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[端末を接続する]",
                    "maleVariant": null,
                    "stringId": 1773588978119434244
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[バッグを掴んで跳ぶ]",
                    "maleVariant": null,
                    "stringId": 1774969095748018180
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "跳び降りろ！",
                    "maleVariant": null,
                    "stringId": 1774977621006442496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前の息の根を止めてやるってな",
                    "maleVariant": null,
                    "stringId": 1776711517152501760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マーフィー！？",
                    "maleVariant": null,
                    "stringId": 1786550810992340992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ドアは塞いだけど、長くは持たない",
                    "maleVariant": null,
                    "stringId": 1786609673804345344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マジかよ",
                    "maleVariant": null,
                    "stringId": 1801402833668956160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "埠頭は炎上、パシフィカは封鎖され孤立。ワトソンには装甲輸送車が出動してる",
                    "maleVariant": null,
                    "stringId": 1801402890845708288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ドアロックを突破した。アラサカの犬どもが迫ってるよ",
                    "maleVariant": null,
                    "stringId": 1831217882242117632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "愛してるぜ、スパイダー",
                    "maleVariant": null,
                    "stringId": 1831218938871181312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、知ってる",
                    "maleVariant": null,
                    "stringId": 1831219904567734272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もうすぐアクセスポイントに着く",
                    "maleVariant": null,
                    "stringId": 1831222627275010048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "接続して",
                    "maleVariant": null,
                    "stringId": 1831223121011699712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニー、さっさと行くよ",
                    "maleVariant": null,
                    "stringId": 1946107591439036420
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "下の階は制圧されてる。屋上へ！",
                    "maleVariant": null,
                    "stringId": 1947464762246631428
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "敵が来る！ 屋上へ急いで！",
                    "maleVariant": null,
                    "stringId": 1947468734353182724
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オラ！",
                    "maleVariant": null,
                    "stringId": 1952988037627670528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うおおお！",
                    "maleVariant": null,
                    "stringId": 1952989214163496960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "敵襲！ スマッシャーを呼ぶんだ！",
                    "maleVariant": null,
                    "stringId": 1953162978977280000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニー、全速力で逃げて！",
                    "maleVariant": null,
                    "stringId": 1954926712763998208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よし、あとは念のため…",
                    "maleVariant": null,
                    "stringId": 1968145847295696896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "…ってウソ。見てよ！ 私たちがテレビに出てる",
                    "maleVariant": null,
                    "stringId": 1994149901557587968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[機銃を使う]",
                    "maleVariant": null,
                    "stringId": 1999989547619540992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[銃を取る]",
                    "maleVariant": null,
                    "stringId": 2160415885500309508
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何してるの？ 早く爆弾を！",
                    "maleVariant": null,
                    "stringId": 2222802868110716928
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}