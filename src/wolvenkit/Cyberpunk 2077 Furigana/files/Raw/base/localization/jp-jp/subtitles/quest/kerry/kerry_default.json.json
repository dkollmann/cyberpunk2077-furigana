          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自然の成り行きで",
                    "maleVariant": null,
                    "stringId": 1927363080495951872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "デスクワークに飽きたから",
                    "maleVariant": "デスクワークに飽きたからだ",
                    "stringId": 1927363080495951888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クランを出たから、ナイトシティで仕事が必要だった",
                    "maleVariant": null,
                    "stringId": 1927363080512729096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ストリートで生きていく方法しか知らないから",
                    "maleVariant": "ストリートで生きていく方法しか知らないからだ",
                    "stringId": 1927363080512729112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "空調の効いた部屋でデスクワークばっかしてると、つまらないでしょ",
                    "maleVariant": "空調の効いた部屋でデスクワークばっかしてると、つまらないだろ",
                    "stringId": 1927363080563060740
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待て待て。それってコーポじゃなくて、郵便局かなんかの話だよな",
                    "maleVariant": null,
                    "stringId": 1927363080563060752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカよ",
                    "maleVariant": "アラサカだ",
                    "stringId": 1927363080579837964
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "冗談だろ",
                    "maleVariant": null,
                    "stringId": 1927363080579837976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "“一身上の都合”で辞めざるを得なくなったの。ま、円満退社ね",
                    "maleVariant": "“一身上の都合”で辞めざるを得なくなってな。ま、円満退社さ",
                    "stringId": 1927363080596615172
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そりゃ… すごいな。アラサカを抜ける時は常に足からだってジョニーが言ってたが、お前はピンピンしてる",
                    "maleVariant": null,
                    "stringId": 1927363080596615184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今のところはね",
                    "maleVariant": "今のところはな",
                    "stringId": 1927363080596615200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "んー、別に面白い話はないよ。自然にこうなったの",
                    "maleVariant": "んー、別に面白い話はない。自然にこうなったんだ",
                    "stringId": 1927363080646946820
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前ってのは、生まれながらにして謙虚なのか、鈍感なのか、退屈なのか、正直よくわからん",
                    "maleVariant": null,
                    "stringId": 1927363080646946832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そういう風が吹いただけ。ここに来て初めて仕事をして、そしたら別の仕事が転がり込んで… ってね",
                    "maleVariant": "そういう風が吹いただけだ。ここに来て初めて仕事をして、そしたら別の仕事が転がり込んで… ってな",
                    "stringId": 1927363080697278468
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティに来る前は、お前どこにいたんだ？",
                    "maleVariant": null,
                    "stringId": 1927363080697278484
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ノーマッドのバッカー・ファミリー。でも解散して、今は一人よ",
                    "maleVariant": "ノーマッドのバッカー・ファミリー。だが解散して、今は一人だ",
                    "stringId": 1927363080714055684
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あー、だからあんなに早くサツから逃げられたのか。ノーマッドにとって車は体の一部なんだろ？",
                    "maleVariant": null,
                    "stringId": 1927363080714055696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何そのステレオタイプ。あたし以外に何人ノーマッドを知ってるの？",
                    "maleVariant": "何だそのステレオタイプ。俺以外に何人ノーマッドを知ってる？",
                    "stringId": 1927363080714055708
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "実は、ノーマッドのBDにハマってた時があってな。荒野を派手に駆け回ってた。あと、どこかのクランのスタントマンに『Born Stupid』のPVに出てもらったこともあるぜ",
                    "maleVariant": null,
                    "stringId": 1927363080730832896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなのだと思った",
                    "maleVariant": null,
                    "stringId": 1927363080730832908
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティのストリートにいれば、すぐフィクサーに目をつけられる。そこからはあっという間よ",
                    "maleVariant": "ナイトシティのストリートにいれば、すぐフィクサーに目をつけられる。そこからはあっという間だ",
                    "stringId": 1927363080764387332
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やめようと思ったことは？",
                    "maleVariant": null,
                    "stringId": 1927363080781164552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "え？ あるわけないじゃない",
                    "maleVariant": "え？ あるわけないだろ",
                    "stringId": 1927363080781164568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl][patrzy na V, jakby zastanawiał się nad odpowiedzią - jakby to, co V powiedział, było dla niego czymś nowym]",
                    "maleVariant": null,
                    "stringId": 1927363080781164580
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前はどこで覚えてんだ？ ほら、この手の…",
                    "maleVariant": "なかなかの暴れっぷりだったが… お前はどこで覚えてんだ？ ほら、この手の…",
                    "stringId": 1927363080932159488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この仕事を始めた理由？",
                    "maleVariant": "この仕事を始めた理由か？",
                    "stringId": 1927363080932159504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "〈Samurai〉が解散した後は何してたの？",
                    "maleVariant": "〈Samurai〉が解散した後は何してたんだ？",
                    "stringId": 1938651366342389764
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "新しいアルバム制作は順調？",
                    "maleVariant": "新しいアルバム制作は順調か？",
                    "stringId": 1938651616608120832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "コヴァチェクの様子は？",
                    "maleVariant": null,
                    "stringId": 1938651664070864896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "〈Us Cracks〉とはうまくいってる？",
                    "maleVariant": "〈Us Cracks〉とはうまくいってるか？",
                    "stringId": 1938653786858450944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "コヴァチェクはどう？ もうデルコロナドに身投げした？",
                    "maleVariant": "コヴァチェクはどうだ？ もうデルコロナドに身投げしたか？",
                    "stringId": 1938662758726365184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マネージャーの様子はどう？",
                    "maleVariant": "マネージャーの様子はどうだ？",
                    "stringId": 1938662833116540928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "〈Us Cracks〉とはどう？ うまくいってる？",
                    "maleVariant": "〈Us Cracks〉とはどうだ？ うまくいってるか？",
                    "stringId": 1938664927282196480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "〈Us Cracks〉の件は、もう落ち着いた？",
                    "maleVariant": "〈Us Cracks〉の件は、もう落ち着いたか？",
                    "stringId": 1938666012583522304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[ケリーにキス] 調子はどうだ？",
                    "maleVariant": null,
                    "stringId": 1938668772183896064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大暴れできてスッキリした",
                    "maleVariant": null,
                    "stringId": 1938669534272155648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "〈Samurai〉が解散した後は何を？",
                    "maleVariant": null,
                    "stringId": 1938671919421845504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうせフィリピンのちんけな浜辺に、“自分探し”でもしに行ったんだろ",
                    "maleVariant": null,
                    "stringId": 1938672923085238272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "調子を取り戻すまで少し時間がかかったな。故郷のマスバテ島で2年ほど休んだんだ",
                    "maleVariant": null,
                    "stringId": 1938674093732261888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言ったろ？ 昼間はずっとハイになって、夜は手当たり次第にヤりまくってたんだろな",
                    "maleVariant": null,
                    "stringId": 1938674604548157440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で、いい経験になった？",
                    "maleVariant": "で、いい経験になったか？",
                    "stringId": 1938674792184541184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最終的にはな。あとの話はメディアで見たことあるだろ",
                    "maleVariant": null,
                    "stringId": 1938675237351190528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "新しいアルバムは？ 順調？",
                    "maleVariant": "新しいアルバムは？ 順調か？",
                    "stringId": 1938677750544592896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で、ケリー・ユーロダインの新アルバムはいつ出る予定？",
                    "maleVariant": "で、ケリー・ユーロダインの新アルバムはいつ出るんだ？",
                    "stringId": 1938677811294892032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほんとのことを言うと、トラックを無茶苦茶にして、スッキリした",
                    "maleVariant": null,
                    "stringId": 1938678496526725120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺もだ。〈Us Cracks〉の連中も気に入らないといいがな",
                    "maleVariant": null,
                    "stringId": 1938679622479245312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、それを聞いちまうか？",
                    "maleVariant": null,
                    "stringId": 1938688780758806528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アイデアはいくつかあるんだが、どうにも形にならなくてな。試行錯誤中だ",
                    "maleVariant": null,
                    "stringId": 1938689005456060416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今は、別のことで少々忙しくてよ",
                    "maleVariant": null,
                    "stringId": 1938689639500607488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "例えば？",
                    "maleVariant": null,
                    "stringId": 1938720911241375744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあ、主に酒だな… スランプでさ",
                    "maleVariant": null,
                    "stringId": 1938721091109908480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ。“ケリー・ユーロダイン”ってタイトルにするつもりだ。俺の復活を象徴してさ",
                    "maleVariant": null,
                    "stringId": 1938722775592726528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "聞かせてくれる？",
                    "maleVariant": "聞かせてくれよ",
                    "stringId": 1938727148892844032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、まだダメだ。完成するまで待ってくれよ。もうすぐだ！",
                    "maleVariant": null,
                    "stringId": 1938727327687634944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "新しい曲を聴くと… ろう人形館で昔の自分を見てるみたいな気分になるんだ",
                    "maleVariant": null,
                    "stringId": 1938732309061169152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ。どうしてなんだ？",
                    "maleVariant": null,
                    "stringId": 1938749869857042432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "型破りだが、すげえ曲が書けてるよ",
                    "maleVariant": null,
                    "stringId": 1941473471257767936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、チェック用にデモを送ってきた",
                    "maleVariant": null,
                    "stringId": 1942806058295226368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "知ってのとおり、俺のスタイルには合わないが… 連中のフェイクポップにちょっと味付けするくらい、朝飯前さ",
                    "maleVariant": null,
                    "stringId": 1942826872881147904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あのな、1回しか言わないから良く聞け。俺の前であいつらの名前は二度と口にするな",
                    "maleVariant": "V、やめてくれ。俺の前であいつらの名前は二度と口にするな",
                    "stringId": 1942831312786812928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はいはい。わかったよ",
                    "maleVariant": null,
                    "stringId": 1942832515176329216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "へっ、そのうちな。あんなクズとの契約は終了だ。少し金を出してでも、縁を切って、ゴミと一緒に放り出してやる",
                    "maleVariant": null,
                    "stringId": 1942870573250637824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "と言うより、コヴァチェクはまだマネージャーなの？",
                    "maleVariant": "と言うより、コヴァチェクはまだマネージャーなのか？",
                    "stringId": 1942872079710416896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "心配すんな。どうするかは考えてある",
                    "maleVariant": null,
                    "stringId": 1942872741957464064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "後はどうなろうが知るか",
                    "maleVariant": null,
                    "stringId": 1942880176847843328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はっ！ そうこなくっちゃ",
                    "maleVariant": null,
                    "stringId": 1942880318548209664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": null,
                    "maleVariant": "元気か？",
                    "stringId": 1942882704536760320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": null,
                    "maleVariant": "...",
                    "stringId": 1942884975618158592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": null,
                    "maleVariant": "うん、会えなくて寂しかったか？",
                    "stringId": 1943210723453300736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあまた",
                    "maleVariant": null,
                    "stringId": 2342903124070670336
                  }
                }
              ]
            }
          }
        }
