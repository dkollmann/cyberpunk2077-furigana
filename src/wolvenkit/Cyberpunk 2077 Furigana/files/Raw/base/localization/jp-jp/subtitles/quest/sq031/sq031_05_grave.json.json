    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わざわざこんなとこまで運んだとは、お手数なこった",
              "maleVariant": "",
              "stringId": 1759527621505015808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしが棄てられたゴミ捨て場よりマシよ",
              "maleVariant": "俺が棄てられたゴミ捨て場よりマシだろ",
              "stringId": 1759530032390955008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈冷めた視線を投げかけよ。生に、死に〉",
              "maleVariant": "",
              "stringId": 1760466663507714048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈旅人よ、行け〉",
              "maleVariant": "",
              "stringId": 1760469278975025152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行こう。ナイトシティが待ってる",
              "maleVariant": "",
              "stringId": 1760500499612393472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何を期待してたの？ 墓石や花でもあると思った？",
              "maleVariant": "何を期待してたんだ？ 墓石や花でもあると思ったか？",
              "stringId": 1898260234274361344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあな。まあ、しるしとか？",
              "maleVariant": "",
              "stringId": 1898261060820684800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "せめて、何かあってもいいだろ",
              "maleVariant": "",
              "stringId": 1898263257696456704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ジョニーのイニシャルを刻む] じゃあ何か残そう",
              "maleVariant": "",
              "stringId": 1898263364533768196
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなもの必要？",
              "maleVariant": "そんなもの必要か？",
              "stringId": 1898265297285828608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少しはマシ？",
              "maleVariant": "少しはマシか？",
              "stringId": 1898268332619747328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "形だけのシンボルなんて、本当に必要？",
              "maleVariant": "形だけのシンボルなんて、本当に必要か？",
              "stringId": 1898269423323336704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これが俺の墓かよ",
              "maleVariant": "",
              "stringId": 1898269945816174592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る]墓石でもあると思った？",
              "maleVariant": "[座る]墓石でもあると思ったか？",
              "stringId": 1898270619706609668
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何もないんだな",
              "maleVariant": "",
              "stringId": 1898271676268564480
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ないよりはな。ここに来れば章を一つ終えられると思ったんだ",
              "maleVariant": "",
              "stringId": 1898280375154397184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昔のシルヴァーハンドに別れを告げ、新しく始めるみたいにな…",
              "maleVariant": "",
              "stringId": 1898281763737456640
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少しはな。でもこれが本物の俺の墓だったとしたら、何て書いた？",
              "maleVariant": "",
              "stringId": 1898282555085180928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「ジョニー・シルヴァーハンド、ここに眠る――」",
              "maleVariant": "",
              "stringId": 1898283620622946304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「執念深きロッカーボーイ」",
              "maleVariant": "",
              "stringId": 1898284271880916992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「ナイトシティの伝説」",
              "maleVariant": "",
              "stringId": 1898286555125796864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「テロリストにして嫌なヤツ」",
              "maleVariant": "",
              "stringId": 1898287619187167232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「あたしの命を救った男」",
              "maleVariant": "「俺の命を救った男」",
              "stringId": 1898287996724858880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実際はどうなの？",
              "maleVariant": "実際はどうだ？",
              "stringId": 1898292126721789952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "初めからいなかったか、まだ〈神輿〉の中にいる気分だ",
              "maleVariant": "",
              "stringId": 1898292542914187264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「何事にも屈しない執念深きロッカーボーイ」。死んだ後でもね",
              "maleVariant": "「何事にも屈しない執念深きロッカーボーイ」。死んだ後でもな",
              "stringId": 1898300040954007552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「ナイトシティの伝説」",
              "maleVariant": "",
              "stringId": 1898300519473762304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「テロリストにして嫌なヤツ」",
              "maleVariant": "",
              "stringId": 1898300940799987712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「あたしの命の恩人」",
              "maleVariant": "「俺の命を救った男」",
              "stringId": 1898301193565523968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今の俺にとって、それに何の意味がある？",
              "maleVariant": "",
              "stringId": 1898301952382226432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞こえはいいが、いったい何の伝説だよ？",
              "maleVariant": "",
              "stringId": 1898303755060531200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "キツいな",
              "maleVariant": "",
              "stringId": 1898305214007234560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもそんな墓石を見たら、そいつに会ってみたくなる",
              "maleVariant": "",
              "stringId": 1898305707811033088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V… 俺だってそれが現実になってほしいと思ってる",
              "maleVariant": "",
              "stringId": 1898306947294973952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "急にどうしたの？",
              "maleVariant": "急にどうしたんだ？",
              "stringId": 1898308846073171968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカと戦って、何を成し遂げた？",
              "maleVariant": "",
              "stringId": 1898308994769637376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一人で勝てる戦いじゃない",
              "maleVariant": "",
              "stringId": 1898310204071366660
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "たくさんの死人を出したが、奴らは何も変わっちゃいねえ",
              "maleVariant": "",
              "stringId": 1898313659406110720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "反抗期のニキビ面したガキどもに、「Chippin' In」って叫んで、何になった？",
              "maleVariant": "",
              "stringId": 1898314517829144576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "希望を与えた",
              "maleVariant": "",
              "stringId": 1898318641601728516
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今度はいじけだしたの？",
              "maleVariant": "今度はいじけだしたのかよ？",
              "stringId": 1898319079789056000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分を憐れみたいだけでしょ",
              "maleVariant": "自分を憐れみたいだけだろ",
              "stringId": 1898319646741516288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "急にどうしたの？",
              "maleVariant": "急にどうしたんだ？",
              "stringId": 1898320291204714496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "急にどうしたの？",
              "maleVariant": "急にどうしたんだ？",
              "stringId": 1898320603831357440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は… たくさんの間違いを犯してきた",
              "maleVariant": "",
              "stringId": 1898321549529468928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "褒めたわけじゃない",
              "maleVariant": "",
              "stringId": 1898323561436434436
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分を憐れむためにここに来たの？",
              "maleVariant": "自分を憐れむためにここに来たのか？",
              "stringId": 1898324016820408320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一人で勝てる戦いじゃない",
              "maleVariant": "",
              "stringId": 1898328542038216704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一緒に戦えてよかったでしょ",
              "maleVariant": "一緒に戦えてよかっただろ",
              "stringId": 1898329427220910080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこなんだ。こんな泥沼に、お前を巻き込みたくない",
              "maleVariant": "",
              "stringId": 1898331214782943232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分を憐れみたいだけでしょ",
              "maleVariant": "自分を憐れみたいだけだろ",
              "stringId": 1898332559057375232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コーポの金でも買えないものがあるって、みんなに希望を与えたじゃない",
              "maleVariant": "コーポの金でも買えないものがあるって、みんなに希望を与えただろ",
              "stringId": 1898336405183811584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ステージでは… たとえ死んだとしても、それが理想のためだったら、その価値はあるって思ってたんだ",
              "maleVariant": "",
              "stringId": 1898337288269352960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "違う。やっとはっきりわかったんだ",
              "maleVariant": "",
              "stringId": 1898341994412658688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺様こそ世界一のジョニー・シルヴァーハンドが、まるで別人みたい",
              "maleVariant": "俺様こそ世界一のジョニー・シルヴァーハンドが、まるで別人みたいだ",
              "stringId": 1898346962246979584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "虚勢を張るのはもうやめだ",
              "maleVariant": "",
              "stringId": 1898347473599746048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこにお前が現れた…",
              "maleVariant": "",
              "stringId": 1898348702094610432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "険しい道のりだったけど、ここまで来れたね",
              "maleVariant": "険しい道のりだったが、ここまで来れたな",
              "stringId": 1898364526532513796
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どの口で言ってるわけ？",
              "maleVariant": "どの口で言ってんだ？",
              "stringId": 1898366872775839744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー、別にあたしは気にしてないから",
              "maleVariant": "ジョニー、別に俺は気にしてないって",
              "stringId": 1898367531214458880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "長く険しい道のりだったけどね",
              "maleVariant": "長く険しい道のりだったけどな",
              "stringId": 1898369052052312064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー、どの口が言ってるわけ？",
              "maleVariant": "ジョニー、どの口が言ってるんだ？",
              "stringId": 1898373196813193216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "散々あたしを利用して、嘘をつき倒したあんたなんか… 信用できない",
              "maleVariant": "散々俺を利用して、 嘘をつき倒したお前なんか… 信用できるか",
              "stringId": 1898373894057512960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…よかったら、もう一度チャンスをくれないか？",
              "maleVariant": "",
              "stringId": 1898375509485309952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー、別にあたしは気にしてないから",
              "maleVariant": "ジョニー、別に俺は気にしてないって",
              "stringId": 1898375845298065408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってる。でも聞いてくれ",
              "maleVariant": "",
              "stringId": 1898376421612212224
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう遅い。あんたはチャンスを無駄にした",
              "maleVariant": "もう遅い。お前はチャンスを無駄にした",
              "stringId": 1898377951392321540
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしに何を期待してるの？",
              "maleVariant": "俺に何を期待してるんだ？",
              "stringId": 1898378367081402368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう遅い。あんたはチャンスを棒に振った",
              "maleVariant": "もう遅い。お前はチャンスを棒に振った",
              "stringId": 1898381341396586496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "せっかく生き返ったのに、二度目の人生をただ無駄にしてるだけでしょ",
              "maleVariant": "せっかく生き返ったってのに、二度目の人生をただ無駄にしてるだけだろ",
              "stringId": 1898381789885124608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前の言うとおりだ",
              "maleVariant": "",
              "stringId": 1898382875589103616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしにどうしてほしいの？",
              "maleVariant": "俺にどうしてほしいんだ？",
              "stringId": 1898386989295357952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺が友達だと思ってた連中は、同じ部屋にいることすら耐えられなかったもんだ",
              "maleVariant": "",
              "stringId": 1898389573859037184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "てこたぁ、お前はどんな友達より身近な存在だ。24時間一緒だからな",
              "maleVariant": "",
              "stringId": 1898392341663469568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しかも… 俺を死ぬほど嫌ってはいなさそうだ",
              "maleVariant": "",
              "stringId": 1898393022818439168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少なくとも、今まではそう思ってた",
              "maleVariant": "",
              "stringId": 1898398247579045888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最初の頃はほんとにひどい奴だったね",
              "maleVariant": "最初の頃はほんとにひどい奴だったな",
              "stringId": 1898398402600521732
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。でもこれが最後のチャンスよ",
              "maleVariant": "わかった。でもこれが最後のチャンスだ",
              "stringId": 1898402574339166208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "好き嫌いじゃない… もううんざりなの",
              "maleVariant": "好き嫌いじゃない… もううんざりなんだ",
              "stringId": 1898404193206292480
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。でもこれが最後のチャンスよ。あたしもそこまで甘くない",
              "maleVariant": "わかった。だがこれが最後のチャンスだ。俺もそこまで甘くない",
              "stringId": 1898405185863831552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もうがっかりさせねえ",
              "maleVariant": "",
              "stringId": 1898409465312202752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "好き嫌いじゃない… もううんざりなの。今だって、こうしてあたしを操ろうとしてる",
              "maleVariant": "好き嫌いじゃない… もううんざりなんだ。今だって、こうして俺を操ろうとしてる",
              "stringId": 1898415130189516800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ。俺は本当にクズだ",
              "maleVariant": "",
              "stringId": 1898416097446350848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう戻ってもいい？",
              "maleVariant": "もう戻ってもいいか？",
              "stringId": 1898416994356318208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、悪かったな。ハナコに会って、先のことを相談すりゃいい",
              "maleVariant": "",
              "stringId": 1898417000261898240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、そのつもりよ",
              "maleVariant": "ああ、そのつもりだ",
              "stringId": 1898417006050037760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー・シルヴァーハンド。テロリストにして嫌なヤツだ",
              "maleVariant": "",
              "stringId": 1898431241433927680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V。街一番のお人よしよ",
              "maleVariant": "V。街一番のお人よしだ",
              "stringId": 1898432346800803840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もっと早く、こうして話せばよかった",
              "maleVariant": "",
              "stringId": 1898436243057561600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最初の頃はほんとにクソ野郎だったよね",
              "maleVariant": "最初の頃はほんとにクソ野郎だったよな",
              "stringId": 1898436984006529024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー・シルヴァーハンド。ナイトシティの伝説だ",
              "maleVariant": "",
              "stringId": 1898438395993804800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー・シルヴァーハンド。何にも屈しない執念深きロッカーボーイだ",
              "maleVariant": "",
              "stringId": 1898439740805107712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もっと早くこうして話してれば、いろいろ楽だったのに",
              "maleVariant": "",
              "stringId": 1898441894261444608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっかけが掴めなくてな",
              "maleVariant": "",
              "stringId": 1898442783403560960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "褒めたわけじゃないんだけど",
              "maleVariant": "褒めたわけじゃないんだが",
              "stringId": 1898444007100141568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってる",
              "maleVariant": "",
              "stringId": 1898444518033477632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴィクターんとこで最初に起きた時のこと覚えてるか？ 「幻覚が見える、怖いよー」ってな",
              "maleVariant": "",
              "stringId": 1898447232066932736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ頭の中は居心地悪い？",
              "maleVariant": "まだ頭の中は居心地悪いか？",
              "stringId": 1898448071129059332
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたがタバコ吸いたいって駄々こねて、あたしを殺せないことに怒ってたのは覚えてる",
              "maleVariant": "お前がタバコ吸いたいって駄々こねて、俺を殺せないことに怒ってたのは覚えてる",
              "stringId": 1898470680524771328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お次はミスティんとこで死にたくないって嘆いてた",
              "maleVariant": "",
              "stringId": 1898471180066377728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはお前もだろ",
              "maleVariant": "",
              "stringId": 1898474707224383488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その俺たちがここまで来るとはな",
              "maleVariant": "",
              "stringId": 1898477952374165504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "友達を失望させたって言ったのは…",
              "maleVariant": "",
              "stringId": 1898478867873288192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さっき言ってた、失望させた友達って… ローグのこと？",
              "maleVariant": "さっき言ってた、失望させた友達って… ローグのことか？",
              "stringId": 1898485285410066432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ頭の中は居心地悪い？",
              "maleVariant": "まだ頭の中は居心地悪いか？",
              "stringId": 1898485659474874368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや。快適とは言えないが… 変わってきた",
              "maleVariant": "",
              "stringId": 1898486863709884416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "起きた後しばらく、自分が戻ってる感じがする時がある",
              "maleVariant": "",
              "stringId": 1898488332471259136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この体が自分のものになっちまったような、自由な感じだ",
              "maleVariant": "",
              "stringId": 1898488839394840576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それでほんの少しの間、何か大切なものを失くしたような気分になる",
              "maleVariant": "",
              "stringId": 1898489196682432512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え？ 戻ってるって？",
              "maleVariant": "",
              "stringId": 1898489311287595008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その後、いつものようにお前がいることに気づいて、わけわかんねえ安心感が体中を駆け巡るんだ",
              "maleVariant": "",
              "stringId": 1898490715507019776
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "友達を失望させたって言ってたけど…",
              "maleVariant": "友達を失望させたって言ってたが…",
              "stringId": 1898491283935875076
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "同じ夢を見る…",
              "maleVariant": "",
              "stringId": 1898491863857123328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしも時々似た夢を見るよ。あんたは死んでなくて、あたしがあんたなの",
              "maleVariant": "俺も時々似た夢を見るよ。お前は死んでなくて、俺がお前なんだ",
              "stringId": 1898492190492741632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「ジョニーならどうするだろう？」って考えながら動くわけでもなく、ただ、自分が好きなことをやってる夢…",
              "maleVariant": "",
              "stringId": 1898497054241214464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっとこの世界に馴染み始めたって錯覚に陥る",
              "maleVariant": "",
              "stringId": 1898498758386929664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふむ、そいつはいい兆候じゃねぇな。早く、何とかしないと",
              "maleVariant": "",
              "stringId": 1898499279940243456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグ、オルト、ケリー、サンチァゴ…",
              "maleVariant": "",
              "stringId": 1898501047738716160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少なくとも… ローグはまだ挽回できる",
              "maleVariant": "",
              "stringId": 1898502255312392192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "50年間何も変わらなかったようなふりして、突然あいつの人生に入り込めるか",
              "maleVariant": "",
              "stringId": 1898503292228227072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう入り込んでる",
              "maleVariant": "",
              "stringId": 1898517033741209604
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スマッシャーの件で思い詰めてた",
              "maleVariant": "",
              "stringId": 1898519874576863232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグはスマッシャーの件で何か思い詰めてた。放っておけないんじゃない？",
              "maleVariant": "ローグはスマッシャーの件で何か思い詰めてた。放っておけないんじゃないか？",
              "stringId": 1898521710977372160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう入り込んでるでしょ",
              "maleVariant": "もう入り込んでるだろ",
              "stringId": 1898522041136205824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあな",
              "maleVariant": "",
              "stringId": 1898523347980668928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言われてみれば、様子が変だった",
              "maleVariant": "",
              "stringId": 1898524363220340736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昔、あいつを映画に連れてくって約束したんだ",
              "maleVariant": "",
              "stringId": 1898525471657447424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話をしたほうがいい",
              "maleVariant": "",
              "stringId": 1898525965243142144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい考えね",
              "maleVariant": "いい考えだ",
              "stringId": 1898527651303018496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつなら空いてるか、ローグに聞いてくれるか？",
              "maleVariant": "",
              "stringId": 1898528074390851584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただ… また俺と代わってもらうことになるが…",
              "maleVariant": "",
              "stringId": 1898529252503736320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいよ、ローグに連絡する",
              "maleVariant": "わかった、ローグに連絡する",
              "stringId": 1898530024289226756
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それを忘れてた…",
              "maleVariant": "",
              "stringId": 1898530734535888896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいよ、連絡する",
              "maleVariant": "ああ、連絡してやる",
              "stringId": 1898531220538281984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それを忘れてた",
              "maleVariant": "",
              "stringId": 1898532885240762368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "思いつきだ。無理は言わない",
              "maleVariant": "",
              "stringId": 1898533919438688256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": 1898534681862492160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行こう。長居しても仕方ねえしな",
              "maleVariant": "",
              "stringId": 1898535567548502016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼む",
              "maleVariant": "",
              "stringId": 1898536066721009664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "来たかいはあった？",
              "maleVariant": "来たかいはあったか？",
              "stringId": 1898536670734340096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろんだ。ありがとな、V",
              "maleVariant": "",
              "stringId": 1898538534716592128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺が入ったのが他の誰でもなく、お前の頭でよかったよ",
              "maleVariant": "",
              "stringId": 1898546176654594048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "偶然だけど、ここは知ってる。昔、休憩に寄ったのよね",
              "maleVariant": "偶然だが、ここは知ってる。昔、休憩に寄ったんだよな",
              "stringId": 1902800024470573060
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここに？",
              "maleVariant": "",
              "stringId": 1902800026584502272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "旅するノーマッドたちのための冷蔵庫があったの。ビールを1本もらう代わりに、自分の地元のビールを入れる",
              "maleVariant": "旅するノーマッドたちのための冷蔵庫があってな。ビールを1本もらう代わりに、自分の地元のビールを入れる",
              "stringId": 1902800028849426432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ",
              "maleVariant": "",
              "stringId": 1902800031114350592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見て。まだあった",
              "maleVariant": "見ろ。まだあった",
              "stringId": 1902800035560312836
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "冷蔵庫ね",
              "maleVariant": "",
              "stringId": 1902801855720476672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今から残せばいい",
              "maleVariant": "",
              "stringId": 1902809088260853764
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信用してくれた人を裏切ったり、利用したりしてな。世間知らずで、自分勝手な男だった",
              "maleVariant": "",
              "stringId": 1902816086457077760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも唯一、お前との関係だけは壊さずに、ここまでやってくることができた",
              "maleVariant": "",
              "stringId": 1902816090248728576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "好かれてないのは十分わかってるんだ",
              "maleVariant": "",
              "stringId": 1902818835521359872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "好かれてないどころじゃない",
              "maleVariant": "",
              "stringId": 1902837180752744452
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また真面目な話？",
              "maleVariant": "また真面目な話か？",
              "stringId": 1902837783658778624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "遠回しに言えばね",
              "maleVariant": "遠回しに言えばな",
              "stringId": 1902838919576965120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、また大げさな自分語り？ もう飽きてきた",
              "maleVariant": "おい、また大げさな自分語りか？ もう飽きてきた",
              "stringId": 1902839731577442304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "急にしおらしくなってどうしたの？ 何を言い出すのか興味あるから、続けて",
              "maleVariant": "急にしおらしくなってどうしたんだ？ 何を言い出すのか興味あるから、続けろよ",
              "stringId": 1902841048890888192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺に二度目のチャンスをくれ",
              "maleVariant": "",
              "stringId": 1902841052095336448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "友達を失望させ、お前にも最低なとこを見せた",
              "maleVariant": "",
              "stringId": 1902842799710818304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知ってるさ。ずっとお前を見てきたからな",
              "maleVariant": "",
              "stringId": 1902845774613204992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。なあ… 最後にひとつ頼みがある",
              "maleVariant": "",
              "stringId": 1905315289959780352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼み？ こんな時に？",
              "maleVariant": "",
              "stringId": 1905316683559227396
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言ってみて",
              "maleVariant": "言ってみろ",
              "stringId": 1905325723123208192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼みって、本気なの？ よく今そんなこと言えるね？",
              "maleVariant": "頼みって、本気かよ？ よく今そんなこと言えるな？",
              "stringId": 1905326211038203904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。都合のいい時でいいから、ノース・オークに行ってくれ",
              "maleVariant": "",
              "stringId": 1905329310897295360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言ってみて",
              "maleVariant": "言ってみろ",
              "stringId": 1905341523787796480
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ノース・オークへ連れてってくれ",
              "maleVariant": "",
              "stringId": 1905351081918746624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして？",
              "maleVariant": "どうしてだ？",
              "stringId": 1905354467846148096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケリーだ。詳しくは向こうで説明する",
              "maleVariant": "",
              "stringId": 1908068490328952832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": 1908068969989558272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "前にも来た",
              "maleVariant": "",
              "stringId": 1911628114735022080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": 2008457079267520516
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": 2008512730668724224
            }
          ]
        }
      }
    },
