  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "魔術書、持ってる？",
                    "maleVariant": "魔術書、持ってるか？",
                    "stringId": 1816743451208622080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "“魔術書”を探してる",
                    "maleVariant": null,
                    "stringId": 1816743921994080260
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ。安全な場所に隠してある",
                    "maleVariant": null,
                    "stringId": 1816746369538236416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どこか知りたい？ ならお金を見せて",
                    "maleVariant": null,
                    "stringId": 1816746530297520128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "相当胡散臭いけど… 仕方ない、言うとおりにする",
                    "maleVariant": "相当胡散臭いが… 仕方ない、言うとおりにする",
                    "stringId": 1816747669822808064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あらら。ハードワイヤーのカスタマーサービスも落ちたものね",
                    "maleVariant": "まったく。ハードワイヤーのカスタマーサービスも落ちたもんだ",
                    "stringId": 1816747716849344512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "は？ 何の話だかさっぱりわからないんだけど",
                    "maleVariant": null,
                    "stringId": 1816759903986819072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかってるはずよ。あんたが何者かは大体察しがついてる。安物のスーツ、型落ちのインプラント… 明らかに企業の人間ね",
                    "maleVariant": "わかってるはずだ。あんたが何者かは大体察しがついてる。安物のスーツ、型落ちのインプラント… 明らかに企業の人間だろ",
                    "stringId": 1816760151769522176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今座標を送った",
                    "maleVariant": null,
                    "stringId": 1816760924058329088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうなることやら",
                    "maleVariant": null,
                    "stringId": 1816761262001790976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "えーと、何？",
                    "maleVariant": null,
                    "stringId": 1842970190716129280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何でもない",
                    "maleVariant": null,
                    "stringId": 1842970895644413952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何でもない",
                    "maleVariant": null,
                    "stringId": 1842971269256237056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あなたが誰だか知らないけど、もう私に構わないで。あー、はいはい、もう終わり",
                    "maleVariant": null,
                    "stringId": 1842972782594674688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何なの、ムシャクシャする",
                    "maleVariant": null,
                    "stringId": 1842973922103185408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[支払う] しょうがない",
                    "maleVariant": null,
                    "stringId": 1842978145213665284
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "騙そうったってそうはいかない",
                    "maleVariant": null,
                    "stringId": 1842978469685022720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最近のハードワイヤーのサービスって最悪ね…",
                    "maleVariant": "最近のハードワイヤーのサービスって最悪だな…",
                    "stringId": 1842979295140827136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "後悔はさせない",
                    "maleVariant": null,
                    "stringId": 1842985877027213312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だといいけど。じゃないとそっちが後悔することになるからね。保証する",
                    "maleVariant": "だといいが。でないとそっちが後悔することになるからな。保証するよ",
                    "stringId": 1842986108301135872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしをなめないで",
                    "maleVariant": "俺をなめるなよ",
                    "stringId": 1842990076683145216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "取引はやめよ。本はいらない",
                    "maleVariant": "取引はやめだ。本はいらない",
                    "stringId": 1842990278194286592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "え、私はただ――",
                    "maleVariant": null,
                    "stringId": 1842991550830006272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたみたいな詐欺師は心底嫌いなの",
                    "maleVariant": "あんたみたいな詐欺師は心底嫌いでね",
                    "stringId": 1842991927562391552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そういう奴と出くわしたら、いつもこんな風に会話を終わらせたくなる",
                    "maleVariant": null,
                    "stringId": 1842992844554686464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと、勘違いよ。私は別に騙そうとなんてしてない",
                    "maleVariant": null,
                    "stringId": 1842996132083716096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それに… こっちだって、おいそれとやられるつもりはないから",
                    "maleVariant": null,
                    "stringId": 1842997392102969344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[支払う] わかった。あんたの言葉を信用してあげる",
                    "maleVariant": "[支払う] わかった。あんたの言葉を信用してやる",
                    "stringId": 1843000023324086272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "話は終わりよ",
                    "maleVariant": "話は終わりだ",
                    "stringId": 1843000241562112000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "詐欺師と取引するつもりはない",
                    "maleVariant": null,
                    "stringId": 1843000646346002432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "街であたしに見つからないよう、せいぜい祈るのね",
                    "maleVariant": "街で俺に見つからないよう、せいぜい祈ってるんだな",
                    "stringId": 1843001699703508992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もっと当ててあげる… 会社の倉庫からモノをくすねて、それを横流ししようとしてる、ってとこね",
                    "maleVariant": "もっと当ててやろう… 会社の倉庫からモノをくすねて、それを横流ししようとしてる、ってとこか",
                    "stringId": 1843005532290732032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "上司は何て言うかしら？ 今月のあんたの成績で、横領がきちんと評価されればいいんだけど",
                    "maleVariant": "上司は何て言うだろうな？ 今月のあんたの成績で、横領がきちんと評価されればいいんだが",
                    "stringId": 1843006316927569920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "要求を言って",
                    "maleVariant": null,
                    "stringId": 1843007566511394816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[支払う] バカな真似はしないことね",
                    "maleVariant": "[支払う] バカな真似はしないことだ",
                    "stringId": 1843008091504037892
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[交渉する] 割引よ。それも大幅の",
                    "maleVariant": "[交渉する] 割引だ。それも大幅の",
                    "stringId": 1843008323482603520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "落ち着いて。バカな真似をしなけりゃそれでいい",
                    "maleVariant": "落ち着け。バカな真似をしなけりゃそれでいい",
                    "stringId": 1843008741939924992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "金は払う――でも、もしこれが詐欺だったとしたら、あたしは地の果てまでもあんたを追いかける。わかった？",
                    "maleVariant": "金は払う――でも、もしこれが詐欺だったとしたら、俺は地の果てまでもあんたを追いかける。いいな？",
                    "stringId": 1843009559242002432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ。わかったわよ",
                    "maleVariant": null,
                    "stringId": 1843010050210451456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大幅に値引きしてくれたら、上司には黙っておいてあげる",
                    "maleVariant": "大幅に値引きしてくれれば、上司には黙っておいてやる",
                    "stringId": 1843010818002964480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はあ？ ふざけないで",
                    "maleVariant": null,
                    "stringId": 1843011425505955840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何をしてるの！？",
                    "maleVariant": null,
                    "stringId": 1853066139082334208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと、やったわね！ 自分が何してるかわかってるの！？",
                    "maleVariant": null,
                    "stringId": 1853142200168128512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これはビジネスの話よ。で、取引するの？ それともあたしに電話をしてほしい？",
                    "maleVariant": "これはビジネスの話さ。で、取引するか？ 嫌なら電話を一本かけるだけだがな",
                    "stringId": 1908151277620350976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハードワイヤーのサービスも落ちたものね…",
                    "maleVariant": "ハードワイヤーのサービスも落ちたもんだな…",
                    "stringId": 2399923177605586944
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}