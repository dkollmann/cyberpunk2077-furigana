    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1692517901624004608,
            "femaleVariant": "V様？"
          },
          {
            "stringId": 1692563268675686400,
            "femaleVariant": "どうぞ、ご乗車ください"
          },
          {
            "stringId": 1692563738639060992,
            "femaleVariant": "やあ、V。ジェファーソン・ペラレスだ"
          },
          {
            "stringId": 1692564911735558144,
            "femaleVariant": "妻のエリザベスとはもう話したな"
          },
          {
            "stringId": 1692576723600265220,
            "femaleVariant": "[チップを受け取る] わかった。記録を見せて",
            "maleVariant": "[チップを受け取る] わかった。記録を見せてくれ"
          },
          {
            "stringId": 1692588839669452800,
            "femaleVariant": "わかった。やってみる"
          },
          {
            "stringId": 1692596960932515840,
            "femaleVariant": "よかった。どんな情報でもいい――市長の死の真相に少しでも近づきたいんだ"
          },
          {
            "stringId": 1692618948061286400,
            "femaleVariant": "ブレインダンスは――今日中に元の場所に戻さないと、いろんな人に迷惑がかかるんだ"
          },
          {
            "stringId": 1895403992388427780,
            "femaleVariant": "[乗る] わかった"
          },
          {
            "stringId": 1895405202512240640,
            "femaleVariant": "わかった"
          },
          {
            "stringId": 1895430810952753152,
            "femaleVariant": "はじめまして、ペラレスさん"
          },
          {
            "stringId": 1895430840296103936,
            "femaleVariant": "知ってると思うけど、ルシアス・ライン市長が最近亡くなったでしょう"
          },
          {
            "stringId": 1895437736436523008,
            "femaleVariant": "なるほどな"
          },
          {
            "stringId": 1895438108639059968,
            "femaleVariant": "妻は君なら絶対大丈夫だと言ってたんだが"
          },
          {
            "stringId": 1895439288094121984,
            "femaleVariant": "理由がわかったよ"
          },
          {
            "stringId": 1895445033468518404,
            "femaleVariant": "仕事の内容を教えて",
            "maleVariant": "仕事の内容を教えてくれ"
          },
          {
            "stringId": 1895451939306168320,
            "femaleVariant": "はじめまして"
          },
          {
            "stringId": 1895456546866552832,
            "femaleVariant": "実に単刀直入だな"
          },
          {
            "stringId": 1898073547883634692,
            "femaleVariant": "世間話がしたいの？",
            "maleVariant": "世間話がしたいのか？"
          },
          {
            "stringId": 1898073604221526016,
            "femaleVariant": "で、何をしてほしいの？",
            "maleVariant": "で、何をしてほしいんだ？"
          },
          {
            "stringId": 1898092288285564928,
            "femaleVariant": "お互い時間を無駄にできないでしょ",
            "maleVariant": "お互い時間を無駄にできないだろ"
          },
          {
            "stringId": 1898097348176470016,
            "femaleVariant": "天気の話をしたり、おいしいラーメン屋の情報を交換したいの？",
            "maleVariant": "天気の話をしたり、おいしいラーメン屋の情報を交換したいのか？"
          },
          {
            "stringId": 1898097373342294016,
            "femaleVariant": "お互い、時間を無駄にしてる場合じゃないはずよ",
            "maleVariant": "お互い、時間を無駄にしてる場合じゃないはずだ"
          },
          {
            "stringId": 1898100265231015936,
            "femaleVariant": "まあ、別にいいけど…",
            "maleVariant": "まあ、別にいいが…"
          },
          {
            "stringId": 1898100707042222080,
            "femaleVariant": "ハハ。いや、いい。では本題に入ろう"
          },
          {
            "stringId": 1898107700003954688,
            "femaleVariant": "それと、奥さんも"
          },
          {
            "stringId": 1898113524667473920,
            "femaleVariant": "エリザベス"
          },
          {
            "stringId": 1898124911363973120,
            "femaleVariant": "そうだな"
          },
          {
            "stringId": 1898125643521679360,
            "femaleVariant": "あの事件はもう解決済みでしょ",
            "maleVariant": "あの事件はもう解決済みだろ"
          },
          {
            "stringId": 1898125681253638148,
            "femaleVariant": "事件は解決済みでしょ",
            "maleVariant": "事件は解決済みだろ"
          },
          {
            "stringId": 1898167619713458176,
            "femaleVariant": "報道にも出てない何かがあると思うの？",
            "maleVariant": "報道にも出てない何かがあると思うのか？"
          },
          {
            "stringId": 1898168678120906752,
            "femaleVariant": "亡くなる数日前に暗殺未遂があった"
          },
          {
            "stringId": 1898176246557925376,
            "femaleVariant": "確か、ラインは死ぬ数日前に、誰かに殺されそうになったはず…",
            "maleVariant": "確か、ラインは死ぬ数日前に、誰かに殺されそうになったんだよな…"
          },
          {
            "stringId": 1898176268049539072,
            "femaleVariant": "ニュースでやってた"
          },
          {
            "stringId": 1898177668426981376,
            "femaleVariant": "ええ。警察は自然死だと決めつけた。ラインは自宅で心臓インプラントの不調が原因で亡くなったって"
          },
          {
            "stringId": 1898293739632943104,
            "femaleVariant": "NCPDは関係性を否定してるわ。ただのサイバーサイコによる襲撃だって"
          },
          {
            "stringId": 1898328188441550848,
            "femaleVariant": "ええ。メディアにとってはこれ以上ないネタだもの。何週間も取り上げてた"
          },
          {
            "stringId": 1898330350521376768,
            "femaleVariant": "市長は自然死したって言ってるのね",
            "maleVariant": "市長は自然死したって言ってるのか"
          },
          {
            "stringId": 1898331076521844736,
            "femaleVariant": "警察が間違ってるって言いきれる証拠はあるの？",
            "maleVariant": "警察が間違ってるって言いきれる証拠はあるのか？"
          },
          {
            "stringId": 1898343528772886528,
            "femaleVariant": "ラインは自宅で心臓インプラントの不調が原因で亡くなった――事件の報告書にもそう記載されてるわ"
          },
          {
            "stringId": 1898354419434217472,
            "femaleVariant": "どのように、どうして死んだのか、全て知りたいんだ"
          },
          {
            "stringId": 1898368044714647552,
            "femaleVariant": "で、何か見落としがあると思うの？",
            "maleVariant": "で、何か見落としがあると思うのか？"
          },
          {
            "stringId": 1898368395073249280,
            "femaleVariant": "そう考えられる根拠があるのよ"
          },
          {
            "stringId": 1898371169018142720,
            "femaleVariant": "ただ、その数日前に市庁舎でサイバーサイコの襲撃があったんだ"
          },
          {
            "stringId": 1898372570049896452,
            "femaleVariant": "どうして警察が間違ってると思うの？",
            "maleVariant": "なぜ警察が間違ってると思うんだ？"
          },
          {
            "stringId": 1898374735669714944,
            "femaleVariant": "警察は無関係だって言ってるけど…"
          },
          {
            "stringId": 1898411669653774336,
            "femaleVariant": "これって、今度の選挙に関係あるの？",
            "maleVariant": "これって、今度の選挙に関係あるのか？"
          },
          {
            "stringId": 1924389261430972416,
            "femaleVariant": "市長に立候補してるペラレス？"
          },
          {
            "stringId": 1924390084974174208,
            "femaleVariant": "ペラレスって、ナイトシティ市長に立候補してる、あの？"
          },
          {
            "stringId": 1924394652655116288,
            "femaleVariant": "そうよ"
          },
          {
            "stringId": 1924401832968019968,
            "femaleVariant": "かつては地方検事だったけど、最近になって議員にくら替えしたんだっけ？",
            "maleVariant": "かつては地方検事だったが、最近になって議員にくら替えしたんだっけか？"
          },
          {
            "stringId": 1924401942288359424,
            "femaleVariant": "これで、慎重にならざるを得ない理由をわかってくれただろう"
          },
          {
            "stringId": 1924436682752090112,
            "femaleVariant": "前市長の死を調査ね… 今度の選挙と何か関係あったりするの？",
            "maleVariant": "前市長の死を調査ね… 今度の選挙と何か関係あったりするのか？"
          },
          {
            "stringId": 1924453433560752128,
            "femaleVariant": "もちろんだ。ライン氏が殺害されたなら、知っておきたい。知る必要がある"
          },
          {
            "stringId": 1924454857711509504,
            "femaleVariant": "ラインは亡くなる前、NCPDの予算を削減したの。それで恨みを買ったのかも…"
          },
          {
            "stringId": 1924463438435409920,
            "femaleVariant": "サイバーサイコ襲撃の様子がBDに残っている"
          },
          {
            "stringId": 1924464487044964352,
            "femaleVariant": "警備主任がスキャンしたけど、特に不審なものはなかった… でも中身は生データだから、BDエディターに見てもらいたいの"
          },
          {
            "stringId": 1924465207374733312,
            "femaleVariant": "引き受けてくれるなら、すぐにでも見せよう"
          },
          {
            "stringId": 1924492786148270080,
            "femaleVariant": "どこで手に入れたの？",
            "maleVariant": "どこで手に入れたんだ？"
          },
          {
            "stringId": 1924494741398577152,
            "femaleVariant": "どこで手に入れたの？",
            "maleVariant": "どこで手に入れたんだ？"
          },
          {
            "stringId": 1924506122088833024,
            "femaleVariant": "政治家がイベントなどに出席する際は、BDの記録を残すのが通例だ"
          },
          {
            "stringId": 1924508021890441216,
            "femaleVariant": "襲撃後、BDはNCPDに押収された"
          },
          {
            "stringId": 1924508423469883392,
            "femaleVariant": "手に入れるのは簡単じゃなかったわ"
          },
          {
            "stringId": 1947712436352409600,
            "femaleVariant": "さっそく見させて",
            "maleVariant": "さっそく見させてくれ"
          },
          {
            "stringId": 1957819339482095616,
            "femaleVariant": "カーター、出して"
          },
          {
            "stringId": 2065155347913728004,
            "femaleVariant": "[座って待つ]"
          }
        ]
      }
    }
  }
}