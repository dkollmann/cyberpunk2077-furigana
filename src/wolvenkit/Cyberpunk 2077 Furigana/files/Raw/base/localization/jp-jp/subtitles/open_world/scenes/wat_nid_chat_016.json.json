          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l=\"rus\" o=\"Ну чё? Как тебе новая кислота?\" t=\"どう？ 新しいアシッドは気に入った？\" b=\"\" a=\"\"/>",
                    "maleVariant": null,
                    "stringId": 2156387014512467968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l=\"rus\" o=\"Это охренеть, чумба. На восемь из десяти.\" t=\"チューマ… こいつはいい。10点中8点だ\" b=\"\" a=\"\"/>",
                    "maleVariant": null,
                    "stringId": 2170811953511645184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l=\"rus\" o=\"Ух ты… Я прямо чувствую, как внутри что-то раскрывается… и это… кричит… по-жёлтому.\" t=\"ねえ… なんだか… 体の内側で、何かが弾けて… 叫び出しそう… 黄色に包まれて\" b=\"\" a=\"\"/>",
                    "maleVariant": null,
                    "stringId": 2170812207434809344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l=\"rus\" o=\"Жёлтый — это как молодость и грусть… как кирпичная дорога… и цифровая меланхолия.\" t=\"黄色は不満の色… レンガの道と… デジタルなメランコリー…\" b=\"\" a=\"\"/>",
                    "maleVariant": null,
                    "stringId": 2170812611446947840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l=\"rus\" o=\"Я слышу… Слышу эпохи. Они зовут меня во тьме без начала и конца… Мой разум будто… убегает, улетает…\" t=\"ああ… 無限の時が聞こえる。暗闇の中から呼んでる。始まりも終わりもない暗闇。俺の心が… 走り、飛び去っていく\" b=\"\" a=\"\"/>",
                    "maleVariant": null,
                    "stringId": 2170814091600052224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l=\"rus\" o=\"Смерть… Смерти нет. Есть только вечная жизнь.\" t=\"死が… いや、死じゃない。純粋な、無限の生だ\" b=\"\" a=\"\"/>",
                    "maleVariant": null,
                    "stringId": 2170814440280932352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l=\"rus\" o=\"Бля… Чумба, я что, андроид? Только скажи правду, окей?\" t=\"そんな！ チューマ、私… 私ってアンドロイドなの？ ねえ… 本当のこと言って\" b=\"\" a=\"\"/>",
                    "maleVariant": null,
                    "stringId": 2170814583893901312
                  }
                }
              ]
            }
          }
        }
      }
    },
