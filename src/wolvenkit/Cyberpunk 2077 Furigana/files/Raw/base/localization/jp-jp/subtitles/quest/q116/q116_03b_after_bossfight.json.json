    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "防衛網を突破した",
              "maleVariant": "",
              "stringId": 1511333766146580480
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー…",
              "maleVariant": "",
              "stringId": 1511334303017492480
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コアへの道は開けたが、時間があまりない",
              "maleVariant": "",
              "stringId": 1511339406243946496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この先どうなるかはわからない。でもね、ここまで来れたのはあんたのおかげ",
              "maleVariant": "この先どうなるかはわからない。だが、ここまで来れたのはお前のおかげだ",
              "stringId": 1511360496060096512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞こえてるかわからないけど…",
              "maleVariant": "聞こえてるかわからないが…",
              "stringId": 1511361608725061632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "むしろその方がいいか。今回ばかりは途中で口を挟まれたくないから",
              "maleVariant": "むしろその方がいいか。今回ばかりは途中で口を挟まれたくないからな",
              "stringId": 1512980104001118208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ムカつく皮肉野郎のね…",
              "maleVariant": "ムカつく皮肉野郎のな…",
              "stringId": 1512980814247780352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう。感謝してるよ",
              "maleVariant": "ありがとう。感謝してる",
              "stringId": 1512981485084762112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナム？",
              "maleVariant": "",
              "stringId": 1792153176280932352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アダム・スマッシャー",
              "maleVariant": "",
              "stringId": 1792153903908790272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグ…",
              "maleVariant": "",
              "stringId": 1792154242909216768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいか… このクソボーグ野郎",
              "maleVariant": "",
              "stringId": 1792174083376422912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつも喧嘩ばっかだったけど… さみしくなるよ、ソウル",
              "maleVariant": "",
              "stringId": 1792421297447788544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "覚えてるよ。シルヴァーハンドを殺すと誓った奴ね",
              "maleVariant": "覚えてるぞ。シルヴァーハンドを殺すと誓った奴だな",
              "stringId": 1792425430095519744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シルヴァーハンドがよろしく言ってた",
              "maleVariant": "",
              "stringId": 1792427163718803460
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソウルは立派な男だった",
              "maleVariant": "",
              "stringId": 1792427721510903808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前を殺すべきか考えてる",
              "maleVariant": "",
              "stringId": 1792427975685726212
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグはやっとケリをつけられた",
              "maleVariant": "",
              "stringId": 1792428076382576640
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいこと教えてあげる",
              "maleVariant": "いいことを教えてやる",
              "stringId": 1792428571981537280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたが殺した男は、立派な奴だったのよ",
              "maleVariant": "あんたが殺した男は、立派な奴だった",
              "stringId": 1792428659575382016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "50年前は、美しい世界を守るために手を汚した",
              "maleVariant": "",
              "stringId": 1792428801074421760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグはお前を憎んでた… そしてようやくケリをつけた",
              "maleVariant": "",
              "stringId": 1792428871857496064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分で望んだわけでもないのに、ここまで来て、命を捧げたのよ。仲間のため、ファミリーのために",
              "maleVariant": "自分で望んだわけでもないのに、ここまで来て、命を捧げたんだ。仲間のため、ファミリーのために",
              "stringId": 1792429553599668224
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたにはわかりっこない",
              "maleVariant": "お前にはわからないさ",
              "stringId": 1792430803435151360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今ここに、ジョニーがいるの。話すことはできないけど、あたしの中に存在してる",
              "maleVariant": "今ここに、ジョニーがいるんだ。話すことはできないが、俺の中に存在してる",
              "stringId": 1792435485268271104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最後に笑うのは、あんたじゃなく、シルヴァーハンドだってこと",
              "maleVariant": "最後に笑うのは、お前じゃなく、シルヴァーハンドだってことさ",
              "stringId": 1792436981275844608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグは、俺やお前なんかとはまるで違う。生きなきゃならねえ奴だったんだ",
              "maleVariant": "",
              "stringId": 1792442369027104768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前にはわかりっこない",
              "maleVariant": "",
              "stringId": 1792443702094356480
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今俺が殺すのは、その美しい思い出を守るためだけだ",
              "maleVariant": "",
              "stringId": 1792445712139051008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ、クソ、クソ！",
              "maleVariant": "",
              "stringId": 1841442353290174464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よくもあいつらを！ このクソボーグ！",
              "maleVariant": "",
              "stringId": 1841443441846280192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "覚悟の上で来たのだろ",
              "maleVariant": "",
              "stringId": 1841447423213854720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前の宿敵、シルヴァーハンドを覚えてるか？ 殺したくてたまらなかったんだよな。この俺がそうだ",
              "maleVariant": "",
              "stringId": 1841448309554176000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってねえな、スマッシャー。あいつなりのけじめをつけたんだよ",
              "maleVariant": "",
              "stringId": 1841449782476300288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いったい、何を言ってるんだ？",
              "maleVariant": "",
              "stringId": 1995048870861103104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無謀だとわかってたはずだ",
              "maleVariant": "",
              "stringId": 1995076448980328448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前は変わらず、腰抜けだ",
              "maleVariant": "",
              "stringId": 1995167848803622912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "くだらん。あの老いぼれはもう死んだんだ",
              "maleVariant": "",
              "stringId": 1995168576565698560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シルヴァーハンド？ そんなことがあってたまるか",
              "maleVariant": "",
              "stringId": 1995324830177058816
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "運命の再会だな。ただ、あの頃の俺なら迷わずとどめを刺してたが…",
              "maleVariant": "",
              "stringId": 1995329907650486272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってねえな。俺たちはみんな変わったんだよ。過去の決意なんて今さら何の意味もない",
              "maleVariant": "",
              "stringId": 1995333152733159424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "許さねえ、よくもローグを",
              "maleVariant": "",
              "stringId": 1995412551344087040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スマッシャー、覚えてるよ",
              "maleVariant": "スマッシャー、覚えてるぞ",
              "stringId": 1995413380742537220
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫？",
              "maleVariant": "大丈夫か？",
              "stringId": 1995643429710225408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[拾う] じゃあな、ローグ",
              "maleVariant": "",
              "stringId": 1995644293887844356
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スマッシャー、このクソ野郎…",
              "maleVariant": "",
              "stringId": 1995645722098688000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつも… 一番かっこよかったぜ",
              "maleVariant": "",
              "stringId": 2020243204199702528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わからずやで… 家族のためなら自らの身も顧みない",
              "maleVariant": "",
              "stringId": 2228711302282702848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最後まであんたらしかった",
              "maleVariant": "",
              "stringId": 2228715324519575552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "親父はいつも言ってた。無人島にひとつだけ持っていけるものがあるとすれば、ローグにするってな",
              "maleVariant": "",
              "stringId": 2229942650917974016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この世代の連中には… 逆立ちしても敵わねえ",
              "maleVariant": "",
              "stringId": 2230130368520220672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これで終わりだと思うな！ この決断を後悔させてやる！",
              "maleVariant": "",
              "stringId": 2262373572170575872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうだかね",
              "maleVariant": "どうだかな",
              "stringId": 2262373785761312768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "所詮人間か",
              "maleVariant": "",
              "stringId": 2262439708241260544
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}