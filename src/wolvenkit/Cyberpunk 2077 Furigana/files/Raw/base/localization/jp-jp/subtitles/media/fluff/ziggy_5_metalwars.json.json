    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティの皆さん、こんばんは！ 今夜のゲストは… と言ってももうほとんどレギュラーですね。ま、一応ゲストとして拍手でお迎えください。BD界の一流スター、ロドリゴ・マルティネス！ やあ、ロディ。久しぶり",
                    "maleVariant": null,
                    "stringId": 1793549754874896384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "また来てやったぜ！",
                    "maleVariant": null,
                    "stringId": 1804033423207809024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その目いいね。あ、もしかして… キロシ？",
                    "maleVariant": null,
                    "stringId": 1804033609518792704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "決まってる。お前はまだ、バイオダインか？",
                    "maleVariant": null,
                    "stringId": 1804033775126691840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まだ使えるし、変える必要ないだろう？ でも人に勧めるなら迷わずモデル300XSだけどね。そんなことより本題に入ろう。明日のリリースが迫る注目の新作ブレインダンス、『鋼の戦車乗り』を紹介してくれるかい？",
                    "maleVariant": null,
                    "stringId": 1804033962947624960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こいつは傑作中の傑作だ。戦車の操縦士になった感覚、燃料のにおい。マシンとの一体感… ああ！ もうとにかく体験してくれ。口じゃうまく説明できない",
                    "maleVariant": null,
                    "stringId": 1804034270943756288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "すまない、興奮して。OK、俺が演じたのはルイス・モラレス。実話に基づいた話なんだが… リッジクレストの戦いで、モラレスは第6機甲部隊の指揮を執った…",
                    "maleVariant": null,
                    "stringId": 1804034507049517056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "記憶が確かなら、自由州連合は敵軍の6倍の戦力だった…",
                    "maleVariant": null,
                    "stringId": 1804034804174012416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そのとおり。罠にかかった！ 自由州連合は無条件降伏を要求したが、マイヤーズ大統領はミリテク、つまりは、新合衆国の徹底抗戦を決意する！",
                    "maleVariant": null,
                    "stringId": 1804034986978557952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "イメージしてくれ、ジギー。砂漠にいて、陽が沈もうとしている。突然、戦車の隊列が目に入るんだ。ネブト戦車が轟轟と迫ってくる。1枚だけ持ってきた家族の写真にキスして、熱く灼けた革張りのハンドルグリップを握るんだ…",
                    "maleVariant": null,
                    "stringId": 1804035550810456064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあ発進だ。揺れる戦車。キャタピラの下の石を感じる。まるで裸足で歩いているかのようだ。あちこちで爆発が起き、飛び散ったガラスや破片が車体にはね返る音がする… 実際はこんなもんじゃないんだ",
                    "maleVariant": null,
                    "stringId": 1804035809448017920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いやあ、ロディ。なかなかの臨場感だよ",
                    "maleVariant": null,
                    "stringId": 1804036065015349248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあ何にせよ、こいつは本当にみんなに体験してほしい。感じてほしいんだ",
                    "maleVariant": null,
                    "stringId": 1804036237250248704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しかしロディ、こういうブレインダンスはまだ早すぎるとは思わなかったのかい？ 何万人という人が亡くなって、ほんの数年前終わったところじゃないか",
                    "maleVariant": null,
                    "stringId": 1804036648879243264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "カリフォルニアがまっぷたつに分かれ、隣人相手、兄弟相手に戦う羽目になった…",
                    "maleVariant": null,
                    "stringId": 1804036988332654592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あー、それは俺がとやかく言えることじゃない。別にどちらが善でどちらが悪だという表現をしてるわけじゃないからな。こいつは勇気の物語、勝利の美酒を、自分の口で味わう作品なんだ",
                    "maleVariant": null,
                    "stringId": 1804037543876608000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうだな、俺も味わってみたくなってきたよ",
                    "maleVariant": null,
                    "stringId": 1804037796692475904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それなら、任務成功だな！",
                    "maleVariant": null,
                    "stringId": 1804038044609396736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "番組をご覧の皆さん。ご紹介した『鋼の戦車乗り』はフィーダーで明日リリース予定です。それではまたCMの後で！",
                    "maleVariant": null,
                    "stringId": 1804038191963684864
                  }
                }
              ]
            }
          }
        }
      }
    },
