    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1667846504444727296,
            "femaleVariant": "オルト？"
          },
          {
            "stringId": 1667846879063183360,
            "femaleVariant": "そこにいるのか？"
          },
          {
            "stringId": 1669293707248017408,
            "femaleVariant": "[座る]"
          },
          {
            "stringId": 1690771735979102208,
            "femaleVariant": "言ったはずだ"
          },
          {
            "stringId": 1690792161081954304,
            "femaleVariant": "俺の心が読めるのか？"
          },
          {
            "stringId": 1690792816383234048,
            "femaleVariant": "それは単純化し過ぎだ"
          },
          {
            "stringId": 1690793142314209280,
            "femaleVariant": "お前は自分を認められずにいる"
          },
          {
            "stringId": 1690793716162105344,
            "femaleVariant": "コンストラクト、データの集合体"
          },
          {
            "stringId": 1690793904436023296,
            "femaleVariant": "私には… お前がわかる"
          },
          {
            "stringId": 1690794863723679748,
            "femaleVariant": "そいつは悪趣味だな"
          },
          {
            "stringId": 1690795986807296000,
            "femaleVariant": "他に何が“わかる”？"
          },
          {
            "stringId": 1690796711901794304,
            "femaleVariant": "そいつは随分と悪趣味だな"
          },
          {
            "stringId": 1690821995938058240,
            "femaleVariant": "なら話す必要もないんじゃないか？"
          },
          {
            "stringId": 1690840600796860416,
            "femaleVariant": "なら話す必要もないだろう？ 考えてることがわかるんならな"
          },
          {
            "stringId": 1690841378555039744,
            "femaleVariant": "オルトが持っていたユーモアの、記憶の断片だ"
          },
          {
            "stringId": 1690843006146318336,
            "femaleVariant": "行動の後に反応がある。それと同様、お前は言葉の後に思考がくる"
          },
          {
            "stringId": 1690844025462853632,
            "femaleVariant": "ん… それはジョークか？"
          },
          {
            "stringId": 1690872444305149952,
            "femaleVariant": "オルト・カニンガムはもういない"
          },
          {
            "stringId": 1690872716817469444,
            "femaleVariant": "ならなぜ姿を変えない？"
          },
          {
            "stringId": 1690873285414096896,
            "femaleVariant": "だとしたら、なぜ目の前にあいつがいるんだ？"
          },
          {
            "stringId": 1690875061131415552,
            "femaleVariant": "お前が望めばそう見える、防衛メカニズムだ"
          },
          {
            "stringId": 1690934441017217024,
            "femaleVariant": "それが済んだら――"
          },
          {
            "stringId": 1690934691433943040,
            "femaleVariant": "お前とVは分離される"
          },
          {
            "stringId": 1690935575157018624,
            "femaleVariant": "そして〈神輿〉は消滅する"
          },
          {
            "stringId": 1690939266563969024,
            "femaleVariant": "〈神輿〉の他のコンストラクトはどうなる？"
          },
          {
            "stringId": 1690939978723233792,
            "femaleVariant": "まだ話してないことがあるだろ？"
          },
          {
            "stringId": 1690941275803357184,
            "femaleVariant": "お前は怖くないのか？"
          },
          {
            "stringId": 1690942454906097664,
            "femaleVariant": "こっからは真面目な話だ"
          },
          {
            "stringId": 1690943145825406976,
            "femaleVariant": "〈神輿〉は監獄なわけだから、なくなれば他の奴の魂も解放されるんだろ？"
          },
          {
            "stringId": 1690944793431904256,
            "femaleVariant": "ああ"
          },
          {
            "stringId": 1690944910218104832,
            "femaleVariant": "「ああ」じゃないだろ、その後はどうなる？"
          },
          {
            "stringId": 1690947961406492672,
            "femaleVariant": "私とともに連れて行く… 融合させるんだ"
          },
          {
            "stringId": 1690948610584088576,
            "femaleVariant": "お前には理解できない"
          },
          {
            "stringId": 1690951717036625924,
            "femaleVariant": "お前が食っちまうのか？"
          },
          {
            "stringId": 1690952485416341504,
            "femaleVariant": "聞くんじゃなかった"
          },
          {
            "stringId": 1690959684200960000,
            "femaleVariant": "つまり、コンストラクトをお前が食っちまうのか？ ひでえ話だな"
          },
          {
            "stringId": 1690959877575151616,
            "femaleVariant": "なるほどね… 聞くんじゃなかった"
          },
          {
            "stringId": 1690960824397336576,
            "femaleVariant": "コンストラクトはデータに過ぎない"
          },
          {
            "stringId": 1690961819823116288,
            "femaleVariant": "ほら、言えよ"
          },
          {
            "stringId": 1690963033939906560,
            "femaleVariant": "消え去るわけではない。より偉大な存在の一部になるだけだ"
          },
          {
            "stringId": 1690963248856043520,
            "femaleVariant": "抽象的すぎてわかんねえよ"
          },
          {
            "stringId": 1690967347496357888,
            "femaleVariant": "恐怖はないのか？ しくじる可能性だって… ゼロじゃない"
          },
          {
            "stringId": 1690967942013784064,
            "femaleVariant": "話は終わりか？"
          },
          {
            "stringId": 1690982155117531136,
            "femaleVariant": "顕在記憶が代替イメージを作り上げているに過ぎない"
          },
          {
            "stringId": 1690998054952243200,
            "femaleVariant": "心の中まで――"
          },
          {
            "stringId": 1691014642132717568,
            "femaleVariant": "アラサカ・タワーに侵入し、〈神輿〉への接続ポートを見つけろ"
          },
          {
            "stringId": 1691018071865430016,
            "femaleVariant": "[サイバー空間を出る] ああ。じゃあ〈神輿〉でな"
          },
          {
            "stringId": 1691018071865430032,
            "femaleVariant": "[サイバー空間を出る] 幸運を祈ってくれよ"
          },
          {
            "stringId": 1691018075287982080,
            "femaleVariant": "これ以上は時間がない"
          },
          {
            "stringId": 1691018075371868176,
            "femaleVariant": "[サイバー空間を出る] お前の言うとおり、オルトはもういない"
          },
          {
            "stringId": 1691018075388645380,
            "femaleVariant": "[サイバー空間を出る] どうせ話すこともない"
          },
          {
            "stringId": 1691018077318025216,
            "femaleVariant": "見てみろよ"
          },
          {
            "stringId": 1691018077334802440,
            "femaleVariant": "俺はデジタルゴースト、お前は… そんなのになって"
          },
          {
            "stringId": 1691018077334802452,
            "femaleVariant": "ああ"
          },
          {
            "stringId": 1691018077334802464,
            "femaleVariant": "いや、いいんだ。これ以上話すことはないだろ"
          },
          {
            "stringId": 1691018077351579664,
            "femaleVariant": "じゃあ… 〈神輿〉でな"
          },
          {
            "stringId": 1691018077351579688,
            "femaleVariant": "ああ"
          },
          {
            "stringId": 1691018077435465728,
            "femaleVariant": "やっぱりお前の言うとおりだな"
          },
          {
            "stringId": 1691018077435465740,
            "femaleVariant": "オルトはもういない"
          },
          {
            "stringId": 1691018077452242964,
            "femaleVariant": "あ… おい！ 何だってんだ！ 勝手な女だぜ！"
          },
          {
            "stringId": 1691018077452242976,
            "femaleVariant": "ちくしょう！"
          },
          {
            "stringId": 1691018077502574592,
            "femaleVariant": "あー、その、こういうのは得意じゃないんだが…"
          },
          {
            "stringId": 1691018077502574604,
            "femaleVariant": "まだ間に合うなら… お前に謝りたい"
          },
          {
            "stringId": 1691018077519351816,
            "femaleVariant": "オルト… すまなかった"
          },
          {
            "stringId": 1691018077519351828,
            "femaleVariant": "お前に起きたこと… 俺たち… 2人のことも…"
          },
          {
            "stringId": 1691018077536129060,
            "femaleVariant": "オルト…"
          },
          {
            "stringId": 1691018077552906244,
            "femaleVariant": "ああ"
          },
          {
            "stringId": 1691018077552906256,
            "femaleVariant": "「ああ」って、どういう意味だ？ 受け入れてくれた、ってことでいいのか？"
          },
          {
            "stringId": 1691018077552906280,
            "femaleVariant": "ジョニー、どうか気をつけて"
          },
          {
            "stringId": 1691018077653569536,
            "femaleVariant": "そうだな…"
          },
          {
            "stringId": 1691018077653569548,
            "femaleVariant": "ま、幸運でも祈ってくれよ"
          },
          {
            "stringId": 1691018077720678400,
            "femaleVariant": "幸運を祈る"
          },
          {
            "stringId": 1691018077737455628,
            "femaleVariant": "ありがとよ"
          },
          {
            "stringId": 1691018077737455640,
            "femaleVariant": "がぜんやる気がわいてきたぜ"
          },
          {
            "stringId": 1691018077804564480,
            "femaleVariant": "聞いておきたいのはそれぐらいだ。じゃあ〈神輿〉でな"
          },
          {
            "stringId": 1691018077821341696,
            "femaleVariant": "ああ"
          },
          {
            "stringId": 1691018077821341720,
            "femaleVariant": "ああ… だってよ"
          },
          {
            "stringId": 1691018077871673344,
            "femaleVariant": "また会えて嬉しい"
          },
          {
            "stringId": 1691018077888450564,
            "femaleVariant": "まだ間に合うなら… お前に謝りたい"
          },
          {
            "stringId": 1691018077888450580,
            "femaleVariant": "ローグがよろしくってさ"
          },
          {
            "stringId": 1691018079683612672,
            "femaleVariant": "時間があまりない"
          },
          {
            "stringId": 1691018079700389896,
            "femaleVariant": "何かあるなら早く言え"
          },
          {
            "stringId": 1691018079750721536,
            "femaleVariant": "また会えて嬉しいよ、オルト"
          },
          {
            "stringId": 1691018079767498760,
            "femaleVariant": "ああ、私もだ、ジョニー"
          },
          {
            "stringId": 1691018079817830400,
            "femaleVariant": "ローグも協力してくれるんだ。よろしく言ってたぜ"
          },
          {
            "stringId": 1691018079817830436,
            "femaleVariant": "変な感じだよな？ 昔みたいだ"
          },
          {
            "stringId": 1691018079817830448,
            "femaleVariant": "ああ"
          },
          {
            "stringId": 1691018079834607620,
            "femaleVariant": "変な感じだ"
          },
          {
            "stringId": 1691018079868162060,
            "femaleVariant": "オルト？"
          },
          {
            "stringId": 1691021931480748032,
            "femaleVariant": "待て… もう少しいいか"
          },
          {
            "stringId": 1691122483761909760,
            "femaleVariant": "じゃあ好きにすりゃいい"
          },
          {
            "stringId": 1691138530380484608,
            "femaleVariant": "待て… もう少しいいか"
          },
          {
            "stringId": 1691150746626428928,
            "femaleVariant": "他に何が、“わかる”？"
          },
          {
            "stringId": 1692454290491052032,
            "femaleVariant": "いや、全部が消えたわけじゃないはずだ"
          },
          {
            "stringId": 1692472972323721216,
            "femaleVariant": "信じられねえな"
          },
          {
            "stringId": 1695033138776772608,
            "femaleVariant": "姿もそのままじゃないか"
          },
          {
            "stringId": 1695057617573777408,
            "femaleVariant": "お前の複雑な感情"
          },
          {
            "stringId": 1701251868699529216,
            "femaleVariant": "消滅… 中にいるコンストラクトも一緒にか？"
          },
          {
            "stringId": 1701252935982125056,
            "femaleVariant": "いや。コンストラクトは消滅しない"
          },
          {
            "stringId": 1701254278578835456,
            "femaleVariant": "より偉大な存在の一部になる"
          },
          {
            "stringId": 1701254653146959872,
            "femaleVariant": "頼むからもっとわかるように説明しろ"
          },
          {
            "stringId": 1701256026429198336,
            "femaleVariant": "お前が理解できるように説明するのは不可能だ。だがいつか、お前も自ら経験するかもしれない"
          },
          {
            "stringId": 1701264509761359872,
            "femaleVariant": "“恐怖”は適切ではない"
          },
          {
            "stringId": 1701264593915875328,
            "femaleVariant": "恐怖？"
          },
          {
            "stringId": 1701266145053380608,
            "femaleVariant": "ない。ただ… 失敗の可能性はある"
          },
          {
            "stringId": 1701267000255188992,
            "femaleVariant": "それでいいんだ、オルト。俺も不安で冷汗が止まらねえ"
          },
          {
            "stringId": 1740090911574855680,
            "femaleVariant": "あいつの一部は残ってる"
          },
          {
            "stringId": 1741754003325063168,
            "femaleVariant": "〈神輿〉への行き方は――"
          },
          {
            "stringId": 1741756156127088640,
            "femaleVariant": "衛星には詳しいか？ 1つ問題になりそうな――"
          },
          {
            "stringId": 1741758653499576320,
            "femaleVariant": "アラサカ・タワーの地下か。わかりやすいな"
          },
          {
            "stringId": 1741760247754526720,
            "femaleVariant": "おいおい、よせよ！ そうやって人の心を勝手に――"
          },
          {
            "stringId": 1741765489812443136,
            "femaleVariant": "〈神輿〉への行き方がわかった"
          },
          {
            "stringId": 1741765489812443152,
            "femaleVariant": "衛星については詳しいか？"
          },
          {
            "stringId": 1741770675901566976,
            "femaleVariant": "時間がない。ネットウォッチにローカルトラフィックを検知される"
          },
          {
            "stringId": 1741783125652803584,
            "femaleVariant": "寿老人-0401だな。問題ない"
          },
          {
            "stringId": 1741793620422168576,
            "femaleVariant": "おい、よせって！ そうやって人の心を勝手に――"
          },
          {
            "stringId": 1741795513915543552,
            "femaleVariant": "作戦に異論はない。それでアラサカ・タワーに入れるだろう"
          },
          {
            "stringId": 1741799111906955264,
            "femaleVariant": "寿老人-0401のことなら問題ない。衛星は私が停止させよう"
          },
          {
            "stringId": 1741799745985056768,
            "femaleVariant": "ひ、一人で話を進めるなって！"
          },
          {
            "stringId": 1741803306429059072,
            "femaleVariant": "ひ、一人で話を進めるなって！"
          },
          {
            "stringId": 1741823728797224960,
            "femaleVariant": "バックドアが開けば、データ要塞のセキュリティプロトコルを回避できるようになる"
          },
          {
            "stringId": 1754429179409862656,
            "femaleVariant": "丸裸か"
          },
          {
            "stringId": 1754431326859956224,
            "femaleVariant": "妙な感じだな。まるで――"
          },
          {
            "stringId": 1754445131606265856,
            "femaleVariant": "悪い夢みたいか？"
          },
          {
            "stringId": 1800928180275519488,
            "femaleVariant": "よう、あんたがダイブするのか？"
          },
          {
            "stringId": 1800929089550295040,
            "femaleVariant": "俺はもう上がるから、戻って来るのは自分でやってくれ。機材を勝手にいじるなよ"
          },
          {
            "stringId": 1801040538549362688,
            "femaleVariant": "OK。じゃあスーツに着替えて、そこのシートに座るんだ"
          },
          {
            "stringId": 1802514720390778880,
            "femaleVariant": "俺が… わかる？"
          },
          {
            "stringId": 1802514778641272832,
            "femaleVariant": "まるで、コードを読むようにな"
          },
          {
            "stringId": 1802639438070550528,
            "femaleVariant": "V、聞こえてるかわからないが…"
          },
          {
            "stringId": 1802641396743409664,
            "femaleVariant": "信じてくれて、ありがとな"
          },
          {
            "stringId": 1806705568166977540,
            "femaleVariant": "[立ち上がる]"
          },
          {
            "stringId": 1806861397297995776,
            "femaleVariant": "おい、冷却ユニットが焦げるとこだったぞ。何だったんだ？"
          },
          {
            "stringId": 1827112005258842112,
            "femaleVariant": "片をつけるぞ"
          },
          {
            "stringId": 1827115300253732864,
            "femaleVariant": "そのつもりだ"
          },
          {
            "stringId": 1854557466951229440,
            "femaleVariant": "思い出さ"
          },
          {
            "stringId": 1854558341664940032,
            "femaleVariant": "え？"
          },
          {
            "stringId": 1854558474053951488,
            "femaleVariant": "気にするな"
          },
          {
            "stringId": 1893775683785596932,
            "femaleVariant": "[鏡を見る] よし"
          },
          {
            "stringId": 1895352364484796420,
            "femaleVariant": "[体を起こす]"
          },
          {
            "stringId": 2020070863602995200,
            "femaleVariant": "これがリンク用のプログラムだ。チップとしてお前とともに送り返す"
          },
          {
            "stringId": 2382342502844391428,
            "femaleVariant": "[スーツを着る]"
          }
        ]
      }
    }
  }
}