            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "グーッモーニン、ナイトシティ！",
                    "maleVariant": null,
                    "stringId": 2039066195769614336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "昨日の死亡者数クイズの結果は手堅く30人！ そのうち10人はギャング抗争の続くヘイウッドからだ！",
                    "maleVariant": null,
                    "stringId": 2039101856863416320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "警官も1人死んじまったから、取り締まりが厳しくなるぞ。NCPDは身内殺しを絶対に許さないからな！",
                    "maleVariant": null,
                    "stringId": 2039101945061240832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サントドミンゴはまたもや停電。今回もネットランナーどもが送電網に悪さをしたらしい",
                    "maleVariant": null,
                    "stringId": 2039102328538066944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ウエストブルックでは、トラウマ・チームが今もサイバーサイコ事件の犠牲者を掃除中だ",
                    "maleVariant": null,
                    "stringId": 2039102416819777536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "パシフィカはっていうと…",
                    "maleVariant": null,
                    "stringId": 2039102495404257280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあ… 相変わらずいつものパシフィカだな",
                    "maleVariant": null,
                    "stringId": 2039102603684409344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "以上、ナイトシティといえばこの俺、スタンがお届けしたぜ。それじゃあまた会おう、この夢の街で",
                    "maleVariant": null,
                    "stringId": 2039102697200611328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "シティ・センターがキマり顔のコーポで溢れてる一方――",
                    "maleVariant": null,
                    "stringId": 2150458461520850944
                  }
                }
              ]
            }
          }
        }
      }
    },
