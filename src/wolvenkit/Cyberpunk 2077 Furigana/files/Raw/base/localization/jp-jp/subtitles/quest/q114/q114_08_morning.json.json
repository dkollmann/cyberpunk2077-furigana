    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1551820382049157120,
            "femaleVariant": "[チップを挿す] ありがと",
            "maleVariant": "[チップを挿す] どうも"
          },
          {
            "stringId": 1551820623892725760,
            "femaleVariant": "谷は現場まで続いてる"
          },
          {
            "stringId": 1551820623993389056,
            "femaleVariant": "ドローン配備や早期探知システムに関する情報は？"
          },
          {
            "stringId": 1551820623993389080,
            "femaleVariant": "そこまではないと思うが… ナイト・コープは警備にミリテクを雇ってる。油断は禁物だ"
          },
          {
            "stringId": 1551820624094052352,
            "femaleVariant": "建設現場は囲われてるけど、パンツァーでゲートを破れるの？",
            "maleVariant": "建設現場は囲われてるが、パンツァーでゲートを破れるのか？"
          },
          {
            "stringId": 1551820624278601728,
            "femaleVariant": "ドリルを起動したら、あとはタワーの地下へ向かってまっしぐらだ"
          },
          {
            "stringId": 1551820624412819456,
            "femaleVariant": "トンネルを掘ってる間に邪魔が入る可能性は？"
          },
          {
            "stringId": 1551820624429596684,
            "femaleVariant": "作戦はこうだ"
          },
          {
            "stringId": 1551820624429596696,
            "femaleVariant": "偵察にやったドローンから、現場の脆弱なポイントをいくつか見つけることができた"
          },
          {
            "stringId": 1693793955935440896,
            "femaleVariant": "いい報せがあるといいけど"
          },
          {
            "stringId": 1693836644756246528,
            "femaleVariant": "わかった"
          },
          {
            "stringId": 1693841987410350080,
            "femaleVariant": "少しは寝たの？",
            "maleVariant": "少しは寝たのか？"
          },
          {
            "stringId": 1693842613603160064,
            "femaleVariant": "それに、どうせ一睡もできなかっただろうよ"
          },
          {
            "stringId": 1693858454918942720,
            "femaleVariant": "よし、いよいよだな。V、準備はいいか？"
          },
          {
            "stringId": 1696742466654064640,
            "femaleVariant": "ボブのジャマーの出番だ。通信アンテナに近づけるだけで、妨害電波を送信できる"
          },
          {
            "stringId": 1696743427233566720,
            "femaleVariant": "ミッチたちの出番だね"
          },
          {
            "stringId": 1696744506696421376,
            "femaleVariant": "問題なさそうね。あとは？",
            "maleVariant": "問題なさそうだな。あとは？"
          },
          {
            "stringId": 1696746092998971392,
            "femaleVariant": "中に入ったら、SERCを起動するんだ"
          },
          {
            "stringId": 1696747443011211264,
            "femaleVariant": "もし呼ばれたら？"
          },
          {
            "stringId": 1696761527819587584,
            "femaleVariant": "もう大丈夫",
            "maleVariant": "もう大丈夫だ"
          },
          {
            "stringId": 1705463066549571588,
            "femaleVariant": "[ドローンの接続を切る] もう十分",
            "maleVariant": "[ドローンの接続を切る] もう十分だ"
          },
          {
            "stringId": 1705464420974219264,
            "femaleVariant": "あとは自由に見てくれていい"
          },
          {
            "stringId": 1705464898655113216,
            "femaleVariant": "時間はまだある"
          },
          {
            "stringId": 1729978012331167744,
            "femaleVariant": "準備はできてるようね",
            "maleVariant": "準備はできてるようだな"
          },
          {
            "stringId": 1729978197836845056,
            "femaleVariant": "この空気――"
          },
          {
            "stringId": 1729979784525271040,
            "femaleVariant": "こんなハイリスクな襲撃の経験者は少ないはずなんだけど、きっとミッチがはっぱをかけたんだろうね"
          },
          {
            "stringId": 1729980632982638592,
            "femaleVariant": "重いね",
            "maleVariant": "重いな"
          },
          {
            "stringId": 1739172720815132672,
            "femaleVariant": "壮観だね。いつまでも見てたい"
          },
          {
            "stringId": 1751879639040282624,
            "femaleVariant": "〈神輿〉に到達するには、タワーの地下を掘り進むしかない"
          },
          {
            "stringId": 1751880279778938880,
            "femaleVariant": "まずはリニアトンネルの建設現場を襲撃し、ナイト・コープのSERCを奪取する"
          },
          {
            "stringId": 1795125220398125056,
            "femaleVariant": "ついに現実味を帯びてきたんだ"
          },
          {
            "stringId": 1795125425516367872,
            "femaleVariant": "でも待たせちゃ悪いか"
          },
          {
            "stringId": 1795202095430553600,
            "femaleVariant": "偵察を終えてきたんだが――"
          },
          {
            "stringId": 1795202208274108416,
            "femaleVariant": "こいつを見てみな"
          },
          {
            "stringId": 1795205416698564608,
            "femaleVariant": "見てみな"
          },
          {
            "stringId": 1795374990496681984,
            "femaleVariant": "警備は比較的薄いはずだ"
          },
          {
            "stringId": 1795375854691078144,
            "femaleVariant": "時間との勝負だ。増援を呼ばれる前に片付けたい"
          },
          {
            "stringId": 1795376233352843264,
            "femaleVariant": "気休め程度でしょ。長くは持たない"
          },
          {
            "stringId": 1795376890197626880,
            "femaleVariant": "少なくとも、時間は稼げる"
          },
          {
            "stringId": 1795378036551241728,
            "femaleVariant": "いずれにせよ、我々にもドローンがある。何かあればテディーが知らせる手筈だ"
          },
          {
            "stringId": 1795381225698676736,
            "femaleVariant": "あったり前でしょ"
          },
          {
            "stringId": 1795384231991234560,
            "femaleVariant": "計画通りに進めば、俺たちの存在すらバレないはずだ"
          },
          {
            "stringId": 1817127634554638336,
            "femaleVariant": "もちろんよ",
            "maleVariant": "もちろんだ"
          },
          {
            "stringId": 1817127634554638352,
            "femaleVariant": "帰りはどうするの？",
            "maleVariant": "帰りはどうするんだ？"
          },
          {
            "stringId": 1817127634588192772,
            "femaleVariant": "ええ、もちろんよ",
            "maleVariant": "ああ、もちろんだ"
          },
          {
            "stringId": 1817206557783150592,
            "femaleVariant": "寒っ。このままじゃ凍えちゃう"
          },
          {
            "stringId": 1819639319416852480,
            "femaleVariant": "睡眠より情報の方が役に立つ"
          },
          {
            "stringId": 1819648463351894016,
            "femaleVariant": "とはいえ、ちんたらしてる暇はない"
          },
          {
            "stringId": 1819665249426264064,
            "femaleVariant": "みんな覚悟はいい？"
          },
          {
            "stringId": 1819665253587013632,
            "femaleVariant": "じゃあエンジンをかけて"
          },
          {
            "stringId": 1841427597962825728,
            "femaleVariant": "ソウルが戻った。行こう"
          },
          {
            "stringId": 1913921585256460288,
            "femaleVariant": "年を取るほど寝なくてよくなる"
          },
          {
            "stringId": 1913929854209355800,
            "femaleVariant": "帰りはどうするの？",
            "maleVariant": "帰りはどうするんだ？"
          },
          {
            "stringId": 1913929854226132992,
            "femaleVariant": "行きと同じルートを使う。任せておけ"
          },
          {
            "stringId": 1913929854226133004,
            "femaleVariant": "私たちは〈神輿〉に集中すればいい"
          },
          {
            "stringId": 1913930601734991872,
            "femaleVariant": "で、準備はいいのか？"
          },
          {
            "stringId": 1934285081050329088,
            "femaleVariant": "少しは寝たの？",
            "maleVariant": "少しは寝たのか？"
          },
          {
            "stringId": 1936088090165243904,
            "femaleVariant": "そこから先はお前とパナムと俺の三人で〈神輿〉への道を探す"
          },
          {
            "stringId": 1937011016964108288,
            "femaleVariant": "目標は2つ。1つ目はアラサカ・タワーに侵入し、Vを〈神輿〉に到達させる"
          },
          {
            "stringId": 1937012121324351488,
            "femaleVariant": "2つ目は、混乱に乗じて奪えるだけの機材を奪う。上手くいけばクランの財政難を解決できる"
          },
          {
            "stringId": 2243458937288151040,
            "femaleVariant": "V、中身を確認して"
          },
          {
            "stringId": 2243460331911008256,
            "femaleVariant": "そろそろ行くよ"
          },
          {
            "stringId": 2243467449057693700,
            "femaleVariant": "[乗る]"
          },
          {
            "stringId": 2243469263446495232,
            "femaleVariant": "V、乗って。行くよ"
          },
          {
            "stringId": 2247466572614660096,
            "femaleVariant": "燃料は満タン、エンジンオイルも確認したし、装備も積んだ。いつでも行けるぞ"
          },
          {
            "stringId": 2247499830610030592,
            "femaleVariant": "どうやら、大した情報は集められなかったらしい。結局ミリテクの装備や規模もわからずじまいだ"
          },
          {
            "stringId": 2247500100270223360,
            "femaleVariant": "チャンスは一度きり… 失敗は許されない"
          },
          {
            "stringId": 2247506683750445056,
            "femaleVariant": "他の荷物は？ もし計画が失敗したら、急いでキャンプを畳まないといけないでしょ"
          },
          {
            "stringId": 2247508754209894400,
            "femaleVariant": "そっちも抜かりない。失敗するつもりもないがな"
          },
          {
            "stringId": 2247512276452175872,
            "femaleVariant": "すぐにわかるさ。嫌でもな"
          },
          {
            "stringId": 2247518045079793664,
            "femaleVariant": "だが、うまくいったとしてもだ… もうここには戻ってこれないんだろ？"
          },
          {
            "stringId": 2247518496605007872,
            "femaleVariant": "ええ… でも今は後のことを考えてる余裕なんてない"
          }
        ]
      }
    }
  }
}