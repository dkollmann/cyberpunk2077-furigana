    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 2176775298639482880,
            "femaleVariant": "ターゲットの女はこの建物のどこかにいる"
          },
          {
            "stringId": 2177740629902118912,
            "femaleVariant": "誘拐した連中と出くわすかもしれねえ。中に入ったら警戒を怠るなよ"
          },
          {
            "stringId": 2177740962896302080,
            "femaleVariant": "おっと、その前に… ミリテクの訓練用チップを手に入れたんだ。ほら、プロフェッショナルだって本番前にはリハーサルをするもんだろ？"
          },
          {
            "stringId": 2177742396207419392,
            "femaleVariant": "仮想訓練ってヤツだ。やってみるか？"
          },
          {
            "stringId": 2177742921837596676,
            "femaleVariant": "[チュートリアル] ウォームアップにちょうどいい"
          },
          {
            "stringId": 2177743165795094528,
            "femaleVariant": "[チュートリアルスキップ] そんな時間はない"
          },
          {
            "stringId": 2177747385013260288,
            "femaleVariant": "ええ、試してみる",
            "maleVariant": "ああ、試してみよう"
          },
          {
            "stringId": 2177747944868958212,
            "femaleVariant": "今はそんな時間ないでしょ。後でね",
            "maleVariant": "今はそんな時間ないだろ。後でな"
          },
          {
            "stringId": 2177749220306472960,
            "femaleVariant": "ま、それもそうだな、じゃまた今度ってことで"
          },
          {
            "stringId": 2177781685226065924,
            "femaleVariant": "[降りる] 行きましょ",
            "maleVariant": "[降りる] 行くぞ"
          },
          {
            "stringId": 2177970876203421700,
            "femaleVariant": "この訓練プログラムは諸君が戦闘の基礎を身につけ、反射能力を磨くために設計されている"
          },
          {
            "stringId": 2177970876220198916,
            "femaleVariant": "ここで学ぶスキルは、戦場で生き延び、さらには活躍するのに役立つだろう"
          },
          {
            "stringId": 2177970876220198924,
            "femaleVariant": "諸君には効果的な武器の使い方、ステルス戦術の活用法、敵ネットワークへのハッキングを学んでもらう"
          },
          {
            "stringId": 2177970876220198932,
            "femaleVariant": "なぜならミリテクでは――"
          },
          {
            "stringId": 2177982518987345924,
            "femaleVariant": "[訓練を開始する]"
          },
          {
            "stringId": 2178031462723375104,
            "femaleVariant": "[訓練を開始する]"
          },
          {
            "stringId": 2178031583066345472,
            "femaleVariant": "[訓練を開始する]"
          },
          {
            "stringId": 2178031673797529600,
            "femaleVariant": "[訓練を開始する]"
          },
          {
            "stringId": 2178045408817729536,
            "femaleVariant": "[訓練を終了する]"
          },
          {
            "stringId": 2178060801443647488,
            "femaleVariant": "どうだ、役に立ちそうだろ？"
          },
          {
            "stringId": 2178061842587340800,
            "femaleVariant": "じゃあいよいよ本番といくか"
          },
          {
            "stringId": 2178080907997499396,
            "femaleVariant": "よし、じゃあ今回は… ステルス技術の訓練ね"
          },
          {
            "stringId": 2178082332114702344,
            "femaleVariant": "射撃訓練プログラムを始めるわ。まずは、軽いウォームアップから"
          },
          {
            "stringId": 2178092310196146184,
            "femaleVariant": "この訓練モジュールでは、戦闘におけるテックツールの使い方を、実戦に近い形式で学んでもらうわ"
          },
          {
            "stringId": 2178103929609408520,
            "femaleVariant": "じゃあ次はいよいよ近接戦闘ね"
          },
          {
            "stringId": 2178136391022563328,
            "femaleVariant": "これで全ての訓練が終了よ。本当ならハイタッチしてるところね"
          },
          {
            "stringId": 2178137337509203972,
            "femaleVariant": "なかなか上出来よ"
          },
          {
            "stringId": 2178283216778055680,
            "femaleVariant": "エレベーター、こっちだ"
          },
          {
            "stringId": 2179250097248817152,
            "femaleVariant": "大丈夫。もう一度よ"
          },
          {
            "stringId": 2179250921513439236,
            "femaleVariant": "あー、惜しかったわね。もう一度初めから"
          },
          {
            "stringId": 2179251052811931652,
            "femaleVariant": "集中して"
          },
          {
            "stringId": 2179251161612177412,
            "femaleVariant": "あなたならできるって"
          },
          {
            "stringId": 2186400182759870464,
            "femaleVariant": "悪くないわ。でも実際の相手は撃ち返してくることを忘れずに"
          },
          {
            "stringId": 2186401634223935492,
            "femaleVariant": "今度はお友達を連れてきたみたいね。さあ、腕の見せどころよ"
          },
          {
            "stringId": 2186406688041488388,
            "femaleVariant": "今度はリアルタイムで敵をハックしてもらうわよ"
          },
          {
            "stringId": 2186417174657126404,
            "femaleVariant": "これまで学んだことを実践してみて"
          },
          {
            "stringId": 2186417341271658500,
            "femaleVariant": "次は近接武器よ。剣を取って"
          },
          {
            "stringId": 2186422589923020800,
            "femaleVariant": "[訓練を終了する]"
          },
          {
            "stringId": 2186422696458342400,
            "femaleVariant": "[訓練を終了する]"
          },
          {
            "stringId": 2186422749273018368,
            "femaleVariant": "[訓練を終了する]"
          },
          {
            "stringId": 2192518938878734336,
            "femaleVariant": "ミリテク？ それって極秘の軍用テックでしょ？ どうやって手に入れたの？",
            "maleVariant": "ミリテク？ それって極秘の軍用テックだろ？ どうやって手に入れたんだ？"
          },
          {
            "stringId": 2192523864266698752,
            "femaleVariant": "T-バグだよ。おまけに中身も調整してあるそうだ"
          },
          {
            "stringId": 2192525505967616000,
            "femaleVariant": "どこで手に入れたの？",
            "maleVariant": "どこで手に入れたんだ？"
          },
          {
            "stringId": 2192574062334504960,
            "femaleVariant": "調整って、具体的には？"
          },
          {
            "stringId": 2192574119410593792,
            "femaleVariant": "なんでも、“スタイル”が気に食わないんだと。まあ、実際にやってみりゃすぐにわかる"
          },
          {
            "stringId": 2198446477823139840,
            "femaleVariant": "V？"
          },
          {
            "stringId": 2201293108845498396,
            "femaleVariant": "まずは基本の戦闘訓練からだ。なお、このセッション内容は記録され、後の評価に使用される"
          },
          {
            "stringId": 2207017315545927680,
            "femaleVariant": "おまけに上級向けの戦闘技術よ。それが済んだら今回は終了"
          },
          {
            "stringId": 2207018368903434244,
            "femaleVariant": "お次はハッキング訓練。コース内容に少し手を加えてみたからお楽しみに"
          },
          {
            "stringId": 2207019732303892488,
            "femaleVariant": "その調子よ。そろそろ射撃訓練に移りましょうか"
          },
          {
            "stringId": 2207022374178541568,
            "femaleVariant": "これでそいつを見失うことはない。じゃあ訓練エリアに走って移動して。ほら！"
          },
          {
            "stringId": 2207024242808082440,
            "femaleVariant": "下の階の敵に、キロシでタグを付けて"
          },
          {
            "stringId": 2207026081859391488,
            "femaleVariant": "早く身を隠して！"
          },
          {
            "stringId": 2207027410044473352,
            "femaleVariant": "OK。次は出口に向かって。見つからないよう、ゆっくりと"
          },
          {
            "stringId": 2207029266711859208,
            "femaleVariant": "上出来よ。じゃあ次のエリアね"
          },
          {
            "stringId": 2207033273463361544,
            "femaleVariant": "もう一度、次はカメラにも気をつけて"
          },
          {
            "stringId": 2207072823199944704,
            "femaleVariant": "うん、さすがの腕前ね"
          },
          {
            "stringId": 2207078813119041544,
            "femaleVariant": "撃たれたら、リアニメーターを使って手当てして"
          },
          {
            "stringId": 2207080084026707968,
            "femaleVariant": "ぼーっとしてないで、身を隠して！"
          },
          {
            "stringId": 2207105771437576200,
            "femaleVariant": "見て、3体のターゲットを用意したわ"
          },
          {
            "stringId": 2207122486041006088,
            "femaleVariant": "ターゲットが見える？ 頑張ってね"
          },
          {
            "stringId": 2207130653424635904,
            "femaleVariant": "後ろにある画面をハックして、敵の注意を引いて"
          },
          {
            "stringId": 2207131731461427204,
            "femaleVariant": "OK！ じゃあそいつを静かに倒して"
          },
          {
            "stringId": 2207132875868885000,
            "femaleVariant": "上出来よ。あとはそいつを倒すだけ"
          },
          {
            "stringId": 2207134700676337672,
            "femaleVariant": "現場に痕跡を残さないのがプロの鉄則"
          },
          {
            "stringId": 2207136117327679488,
            "femaleVariant": "敵を運んで、どこかに隠すのよ"
          },
          {
            "stringId": 2207136437839613960,
            "femaleVariant": "いいじゃない。じゃあ次は、そいつを素早く倒して"
          },
          {
            "stringId": 2207137745858482184,
            "femaleVariant": "まだよ…"
          },
          {
            "stringId": 2207139201936281608,
            "femaleVariant": "よし。グレネードを起爆して"
          },
          {
            "stringId": 2207175498486796300,
            "femaleVariant": "1人をハッキングして、もう片方の注意を引いてみて"
          },
          {
            "stringId": 2207186615187558400,
            "femaleVariant": "よし、そこまで"
          },
          {
            "stringId": 2207187476831817728,
            "femaleVariant": "次はVの体格に見合った相手を用意してみようか"
          },
          {
            "stringId": 2208504118803189760,
            "femaleVariant": "ミリテクのプログラムは穴だらけね"
          },
          {
            "stringId": 2209917733972234240,
            "femaleVariant": "いい感じね"
          },
          {
            "stringId": 2209918395665633280,
            "femaleVariant": "プラットフォームの上に立って、終了して"
          },
          {
            "stringId": 2209959311889682444,
            "femaleVariant": "やるじゃない、V"
          },
          {
            "stringId": 2209959311889682452,
            "femaleVariant": "プラットフォームに立って。次へ行きましょ"
          },
          {
            "stringId": 2210014736832024588,
            "femaleVariant": "余計なことは考えず、命令に従えば結果が出る。ミリテク式も悪くないでしょ？"
          },
          {
            "stringId": 2210014736832024596,
            "femaleVariant": "まあ、私のおかげなのは事実だけどね。さあ、次に行きましょ"
          },
          {
            "stringId": 2210062262138327044,
            "femaleVariant": "いい調子よ。ずいぶん慣れてきたみたいね"
          },
          {
            "stringId": 2210062262138327052,
            "femaleVariant": "プラットフォームに乗って"
          },
          {
            "stringId": 2211420454714425344,
            "femaleVariant": "でもこの戦闘訓練自体は役に立ちそう"
          },
          {
            "stringId": 2211450293865496576,
            "femaleVariant": "じゃあ少しハードルを上げましょうか"
          },
          {
            "stringId": 2211456124686254080,
            "femaleVariant": "準備ができたら訓練エリアに進んで"
          },
          {
            "stringId": 2211489674823286784,
            "femaleVariant": "それが死体の場合は特にね"
          },
          {
            "stringId": 2211508992982204416,
            "femaleVariant": "敵を無力化して… 手強いから、気をつけるのよ"
          },
          {
            "stringId": 2211522364121366528,
            "femaleVariant": "大げさな言い方をしたけど、要はハッキングよ"
          },
          {
            "stringId": 2212888581692276736,
            "femaleVariant": "サプライズが好きだといいわね"
          },
          {
            "stringId": 2212901869985546240,
            "femaleVariant": "頑張ってね"
          },
          {
            "stringId": 2212909701120212992,
            "femaleVariant": "では… 新兵、1番ステーションへ向かえ！ どうした、さっさと進め！"
          },
          {
            "stringId": 2212910880642383872,
            "femaleVariant": "これならきっとジャッキーも文句を言わないでしょ"
          },
          {
            "stringId": 2220078820234612736,
            "femaleVariant": "よし、行きましょ",
            "maleVariant": "よし、行くぞ"
          },
          {
            "stringId": 2221229947252625408,
            "femaleVariant": "なにグズグズしてるの"
          },
          {
            "stringId": 2221230212164866052,
            "femaleVariant": "どうしたの、V？"
          },
          {
            "stringId": 2221231508875890688,
            "femaleVariant": "コーポなんかより… こっちの方がイケてるでしょ？"
          },
          {
            "stringId": 2221232700997750784,
            "femaleVariant": "まずまずね。ミリテク式の訓練はあなたに合ってるんじゃない？"
          },
          {
            "stringId": 2221233474528071680,
            "femaleVariant": "フィクサーからアドバイスは？"
          },
          {
            "stringId": 2221233657282285568,
            "femaleVariant": "ワカコから何かアドバイスはあった？",
            "maleVariant": "ワカコから何かアドバイスはあったか？"
          },
          {
            "stringId": 2221413786751168512,
            "femaleVariant": "「うちはあんたのおかんやない。簡単な仕事や。指示通りやったらええ」だとさ"
          },
          {
            "stringId": 2221433502462996480,
            "femaleVariant": "でも忘れないで。ストリートにはヒントもやり直しもないからね"
          },
          {
            "stringId": 2221434865829900288,
            "femaleVariant": "じゃあね、V"
          },
          {
            "stringId": 2231807161551646720,
            "femaleVariant": "スパルタ式に鍛えていくから、覚悟するのよ"
          },
          {
            "stringId": 2231811100489641984,
            "femaleVariant": "なに突っ立ってるの？ ミリテクの新米兵士じゃないんだから"
          },
          {
            "stringId": 2231958851626037248,
            "femaleVariant": "時間の無駄よ。ペースを上げて"
          },
          {
            "stringId": 2231960072906698752,
            "femaleVariant": "V、早く手当をして"
          },
          {
            "stringId": 2232102595943821312,
            "femaleVariant": "ステルスよ、ステルス。忍者のBDは見たことあるでしょ？ あんな感じ"
          },
          {
            "stringId": 2232105567339769856,
            "femaleVariant": "ねえ、どうしたの？ 瞑想シミュレーションをロードした覚えはないんだけど"
          },
          {
            "stringId": 2232108289124298752,
            "femaleVariant": "早く隠れて！"
          },
          {
            "stringId": 2232109598250463232,
            "femaleVariant": "V、散歩は済んだ？"
          },
          {
            "stringId": 2232111319894814720,
            "femaleVariant": "早く訓練に戻って"
          },
          {
            "stringId": 2239203123920916480,
            "femaleVariant": "次は防御。攻撃よりつまらないけど、重要な要素だから"
          },
          {
            "stringId": 2239207036518682632,
            "femaleVariant": "相手は攻撃の後に毎回バランスを崩してる。その隙を狙って、カウンターを食らわせて"
          },
          {
            "stringId": 2239208440788439048,
            "femaleVariant": "やるじゃない！ V、次は相手にパンチを空振りさせるの。攻撃を食らわないようにして。一発もね"
          },
          {
            "stringId": 2239213312086659072,
            "femaleVariant": "若き弟子よ… もう私から教えることは何もない… なんてね。さあ、次が最後の試練よ。ここまで来れたならやれるはず"
          },
          {
            "stringId": 2239243704617422856,
            "femaleVariant": "難易度を“イージー”から“ノーマル”に上げるわよ。今度の相手は攻撃をブロックするわ"
          },
          {
            "stringId": 2267775604361285632,
            "femaleVariant": "訓練を続けるなら、モジュールは他にもまだあるわ。もういいなら終了して"
          },
          {
            "stringId": 2267779382607106048,
            "femaleVariant": "訓練モジュールはあと2つ残ってるわ。今すぐやってもいいし、ひとまず終了してもいいわよ"
          },
          {
            "stringId": 2267781014812119048,
            "femaleVariant": "あとはステルス訓練ね。役に立つんじゃない？ それが終わればすべて完了よ"
          }
        ]
      }
    }
  }
}