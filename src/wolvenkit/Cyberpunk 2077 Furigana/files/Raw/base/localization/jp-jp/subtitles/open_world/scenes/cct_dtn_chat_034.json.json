              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいや、今回、委員会はやりすぎた。越えてはならない一線がある！",
                    "maleVariant": null,
                    "stringId": 2172287136265121792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しかし、一般大衆のいるところに出かけて、有権者に身近に感じてもらうことが…",
                    "maleVariant": null,
                    "stringId": 2180928470257483776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うっ… この臭い、わかるだろう！？ こんな連中の支持を得て何になる？",
                    "maleVariant": null,
                    "stringId": 2180928826974650368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そもそも馬鹿げたアイデアだったんだ。ホームレスにこびたりなどしないぞ！",
                    "maleVariant": null,
                    "stringId": 2180928982163898368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "とにかく試してみるだけでも… NCARTは多くの市民が利用しています",
                    "maleVariant": null,
                    "stringId": 2180929100258721792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お断りだ。あんな汚物まみれの列車になど二度と乗らないぞ。文明人が使うような乗り物じゃない",
                    "maleVariant": null,
                    "stringId": 2180929331113213952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しかし、委員会がなんと言うか…",
                    "maleVariant": null,
                    "stringId": 2180929468820602880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "知ったことか。それほど重要なことなら、連中が乗って大衆と交流すればいいんだ",
                    "maleVariant": null,
                    "stringId": 2180929675482349568
                  }
                }
              ]
            }
