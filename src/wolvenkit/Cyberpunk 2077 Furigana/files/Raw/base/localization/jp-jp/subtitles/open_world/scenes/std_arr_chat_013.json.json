              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これだけは確かだ。俺たちは屈しない",
                    "maleVariant": null,
                    "stringId": 2167867518284648448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その通り！ 戦って勝ち取ろう。団結するんだ！",
                    "maleVariant": null,
                    "stringId": 2214409772538892288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でもどうやって？ 一度、自動化したら元には戻せないだろう",
                    "maleVariant": null,
                    "stringId": 2214409820337180672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "会社はロボットの導入を決めたんだ。もうどうしようもない",
                    "maleVariant": null,
                    "stringId": 2221543300246376448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前、気は確かか？ 突然クビにされたってのに、あのクソ野郎どもの肩を持つのかよ？",
                    "maleVariant": null,
                    "stringId": 2221543529255374848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "肩を持つわけじゃない。でもこの社会は資本主義だろ？ 俺だって同じ立場なら、同じことをする",
                    "maleVariant": null,
                    "stringId": 2221545653922009088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何言ってやがんだ、こいつ。耳を疑うぜ！",
                    "maleVariant": null,
                    "stringId": 2221546298938855424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「同じことをする」だと！ ふざけやがって！",
                    "maleVariant": null,
                    "stringId": 2221546593680986112
                  }
                }
              ]
            }
          }
        }
