      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何かのリンクだ… ランディが書いてたやつか？",
              "maleVariant": "",
              "stringId": 1348817570413854720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リバー、あのアニメ… 例の件に使える可能性はある",
              "maleVariant": "",
              "stringId": 1348867615070797824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。すぐに送る",
              "maleVariant": "",
              "stringId": 1348868884887945216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、朝までに連絡をくれるそうだ",
              "maleVariant": "",
              "stringId": 1348869613052674048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんなの、これ。問題を抱えた子供たちばかりを狙ってたのね… ランディみたいな子を",
              "maleVariant": "なんなんだ、これ。問題を抱えた子供たちばかりを狙ってたのか… ランディみたいな子を",
              "stringId": 1558895322645839872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナタリーはランディが唯一信用できる相手だったのね",
              "maleVariant": "ナタリーはランディが唯一信用できる相手だったんだな",
              "stringId": 1558895820811714560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハリスは捕まった時、車に乗っていた",
              "maleVariant": "",
              "stringId": 1558914583711891456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リバー… これって… 一体どういうこと？",
              "maleVariant": "リバー… これって… 一体どういうことだ？",
              "stringId": 1558919609729597440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かに町の一部が封鎖された時期があった…",
              "maleVariant": "",
              "stringId": 1675140575782735872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昔通ってた学校の近くに住んでるのね",
              "maleVariant": "昔通ってた学校の近くに住んでるんだな",
              "stringId": 1675141198922092544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ランディの身元を調べて、ゲームをプレゼントしたのね",
              "maleVariant": "ランディの身元を調べて、ゲームをプレゼントしたのか",
              "stringId": 1675147513094443008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V。ネットランナーのワザで、他にも情報を絞り出せないか？",
              "maleVariant": "",
              "stringId": 1675167589466083328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まかせて",
              "maleVariant": "まかせろ",
              "stringId": 1699603167305502720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] そんな技術は持ってない",
              "maleVariant": "",
              "stringId": 1699603167305502740
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハリスのIPを特定してみる",
              "maleVariant": "",
              "stringId": 1699603168194695168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいにく無理ね",
              "maleVariant": "あいにく無理だ",
              "stringId": 1699603168228249600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕方ない。他の方法を考えよう",
              "maleVariant": "",
              "stringId": 1699603168228249616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人の心につけ入るのが上手ね",
              "maleVariant": "人の心につけ入るのが上手だな",
              "stringId": 1699805822401351680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ランディをいとも簡単にひっかけてる",
              "maleVariant": "",
              "stringId": 1699812400194097152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リンクでページを見れないか？",
              "maleVariant": "",
              "stringId": 1701028251194855424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺のせいだ。もっと注意を払ってれば… もっと構ってやってれば…",
              "maleVariant": "",
              "stringId": 1701030036290637824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハリスに夢を見させる刺激になればいいんだけど。ひとまずヤウェンに任せましょ",
              "maleVariant": "ハリスに夢を見させる刺激になればいいんだが。ひとまずヤウェンに任せよう",
              "stringId": 1784972033744048128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "出た！ でもこれ以上は無理よ",
              "maleVariant": "出た！ だがこれ以上は無理だ",
              "stringId": 1866495866245877760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よくやった！",
              "maleVariant": "",
              "stringId": 1866496097469468672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ランディの身元を調べて、ゲームをプレゼントしたのね",
              "maleVariant": "ランディの身元を調べて、ゲームをプレゼントしたのか",
              "stringId": 1888186168636313604
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "引き出しにあったヤツか？",
              "maleVariant": "",
              "stringId": 1888186168653090816
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それと、俺は最低だってさ",
              "maleVariant": "",
              "stringId": 1908584683541127168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あとは… いや、まあいい。何か食べようぜ",
              "maleVariant": "",
              "stringId": 1908584914110406656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それだ、V！ その情報をヤウェンに送ろう",
              "maleVariant": "",
              "stringId": 1918732189317435396
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンソニー・ハリスが作った隠しページ…",
              "maleVariant": "",
              "stringId": 1918760766603759620
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このアニメ、なんとも異様だぜ。寒気がする…",
              "maleVariant": "",
              "stringId": 1964708900869476356
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ファイルの形跡があるな…",
              "maleVariant": "",
              "stringId": 1964754778183405572
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待てよ… ランディのPCにあった動画ファイルもそんな名前じゃなかったか？",
              "maleVariant": "",
              "stringId": 1964804053487554560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ…！ ファイルが消えてなければ、手掛かりになったかもしれないのに…",
              "maleVariant": "",
              "stringId": 1964804513871138816
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。配達コードからすると、バッドランズから送ったみたい",
              "maleVariant": "ああ。配達コードからすると、バッドランズから送ったようだな",
              "stringId": 2009912275004166144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや… ありえない… ランディ、なぜ…？",
              "maleVariant": "",
              "stringId": 2009919627619078144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただ会う約束をしただけじゃない、ランディは遊園地にでも行くみたいに大喜びしてたのね",
              "maleVariant": "ただ会う約束をしただけじゃない、ランディは遊園地にでも行くみたいに大喜びしてたんだな",
              "stringId": 2009919701958922240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ野郎が… 子供の心をもてあそびやがって",
              "maleVariant": "",
              "stringId": 2009920041026457600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かめましょ",
              "maleVariant": "確かめてみよう",
              "stringId": 2014291340377067520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "くっ… 吐き気がする",
              "maleVariant": "",
              "stringId": 2014293071366328320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着いて。あんたのせいなんかじゃない",
              "maleVariant": "落ち着け、リバー。あんたのせいなんかじゃない",
              "stringId": 2014303078606573568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "削除されたようだけど、名前が残ってる",
              "maleVariant": "削除されたようだが、名前が残ってる",
              "stringId": 2014306618414600192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、これで… ハリスの心の中を覗けるかもしれないぞ",
              "maleVariant": "",
              "stringId": 2014325153698287616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だといいけど…",
              "maleVariant": "だといいがな…",
              "stringId": 2014325498486853632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このアニメのタイトル… ハリスのページから削除されたファイル名と同じよ",
              "maleVariant": "このアニメのタイトル… ハリスのページから削除されたファイル名と同じだ",
              "stringId": 2014401775545749504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ランディはこれを落としたのね…",
              "maleVariant": "ランディはこれを落としたのか…",
              "stringId": 2014415902532046848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうだ？ 何か見つかったか？",
              "maleVariant": "",
              "stringId": 2172337325158141952
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}