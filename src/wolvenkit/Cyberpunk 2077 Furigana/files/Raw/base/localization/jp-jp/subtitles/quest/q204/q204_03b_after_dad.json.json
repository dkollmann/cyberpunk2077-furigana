  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "親父の奴、あんたに完全にビビってた…",
                    "maleVariant": null,
                    "stringId": 1264746220086120448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "悪いことをしたな",
                    "maleVariant": null,
                    "stringId": 1264753264050589696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "乗って",
                    "maleVariant": null,
                    "stringId": 1308595607224471552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "え？ 逆だよ！",
                    "maleVariant": null,
                    "stringId": 1315470759160406016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺は… 親父が痛い目に遭えばいいって思ってる",
                    "maleVariant": null,
                    "stringId": 1316854673573634048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お金なんていらないよ",
                    "maleVariant": null,
                    "stringId": 1677951527939981312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかってる。だがお前の親父にゃ必要だろ",
                    "maleVariant": null,
                    "stringId": 1695447352097857536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "心配するな。向こうも忘れてる",
                    "maleVariant": null,
                    "stringId": 1794042101942517764
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こんな調子じゃ息子を失うぞ",
                    "maleVariant": null,
                    "stringId": 1794049458768510980
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "惨めな奴め",
                    "maleVariant": null,
                    "stringId": 1794054569813147648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんて惨めな野郎だ",
                    "maleVariant": null,
                    "stringId": 1794060208417341440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あいつは俺の子だ。お前にとやかく言われる覚えはねえんだよ！",
                    "maleVariant": null,
                    "stringId": 1794061687161159680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うるせえ！ てめえだって似たようなもんだろ！",
                    "maleVariant": null,
                    "stringId": 1794063439038054400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "心配するな。あんな様子じゃ、帰って来る頃には全部すっかり忘れてるさ",
                    "maleVariant": null,
                    "stringId": 1794064221409968128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "口答えしたのは失敗だったな",
                    "maleVariant": null,
                    "stringId": 1794065261412810752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今はあんまり口答えせずに、黙っていうとおり従っとけ。親の力は必要だ",
                    "maleVariant": null,
                    "stringId": 1794068307836469248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふう… だといいけどね",
                    "maleVariant": null,
                    "stringId": 1794076104527511552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言うのは簡単さ",
                    "maleVariant": null,
                    "stringId": 1794076676026597376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "親父に大口叩いたけど――",
                    "maleVariant": null,
                    "stringId": 1794080857143705600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うちはゴミ捨て場なんかじゃない。俺が掃除してるし",
                    "maleVariant": null,
                    "stringId": 1795385583214854144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言葉のアヤってやつだ、本気にすんじゃねえ",
                    "maleVariant": null,
                    "stringId": 1795392326263177216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいじゃないか、スティーブもそのうち出ていくんだ… そうすりゃ好きなだけ飲める。その調子で続けりゃいい",
                    "maleVariant": null,
                    "stringId": 1806762269081296896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今に心の底から嫌われるぞ。そしたら二度と息子には会えなくなる",
                    "maleVariant": null,
                    "stringId": 1806762680156643328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はあ… もう嫌われてるさ",
                    "maleVariant": null,
                    "stringId": 1806763542253887488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いずれ息子がいたらと悲しむ日がきっとくる。その時には手遅れだがな",
                    "maleVariant": null,
                    "stringId": 1806765141743329280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "きっと、また後で叱られる",
                    "maleVariant": null,
                    "stringId": 1806789170978054144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう少し大人になるまで待てばいい",
                    "maleVariant": null,
                    "stringId": 1806817998244896768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だとしても、俺から言えるのはそれだけだ",
                    "maleVariant": null,
                    "stringId": 1806822453317165056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だろうね。でも俺は自分で稼げてるんだ",
                    "maleVariant": null,
                    "stringId": 1806835620512489472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "負け犬が偉そうに",
                    "maleVariant": null,
                    "stringId": 2237622305589780480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "苦労して育ててやったってのに、得体の知れねえクズと遊び歩きやがって。恩知らずが",
                    "maleVariant": null,
                    "stringId": 2237623692360900608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "きつく叱ってやらねえとな。優しくしすぎたんだ",
                    "maleVariant": null,
                    "stringId": 2237624672049651712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クソ… 酒がいる。あいつのせいで肝臓がやられちまうよ",
                    "maleVariant": null,
                    "stringId": 2237626464594518016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[乗る]",
                    "maleVariant": null,
                    "stringId": 2512618004766851076
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}