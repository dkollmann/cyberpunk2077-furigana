      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1756116010216202240,
            "femaleVariant": "腹を立てないで"
          },
          {
            "stringId": 1756116010216202252,
            "femaleVariant": "彼… 今日はチャクラが全部閉じてるし、コーヒーを飲み過ぎなのよ"
          },
          {
            "stringId": 1756116010216202264,
            "femaleVariant": "それと、あたしのオーラが荒れてるんでしょ？",
            "maleVariant": "それと、俺のオーラが荒れてるんだろ？"
          },
          {
            "stringId": 1756116010216202276,
            "femaleVariant": "ええ、そのとおり"
          },
          {
            "stringId": 1756116010249756672,
            "femaleVariant": "V、さっき… 頭の声を聞いちゃったような…"
          },
          {
            "stringId": 1756116010249756688,
            "femaleVariant": "ごめん。変なのを聞かせちゃったね",
            "maleVariant": "すまん。変なのを聞かせちゃったな"
          },
          {
            "stringId": 1756116010249756700,
            "femaleVariant": "いいのよ。状況はわかってるから"
          },
          {
            "stringId": 1756116010283311104,
            "femaleVariant": "いいね、行きましょ",
            "maleVariant": "いいね、行こう"
          },
          {
            "stringId": 1756116010300088324,
            "femaleVariant": "ヴィクもあたしにウンザリだろうしね",
            "maleVariant": "ヴィクも俺にウンザリだろうしな"
          },
          {
            "stringId": 1756116010350419968,
            "femaleVariant": "行きましょ。特別な場所なの？",
            "maleVariant": "行こう。特別な場所なのか？"
          },
          {
            "stringId": 1756116010350419980,
            "femaleVariant": "前にジャッキーも連れてったの。すぐ近くよ"
          },
          {
            "stringId": 1756116010350419992,
            "femaleVariant": "ジャッキーにもそんな場所が必要だったのね",
            "maleVariant": "ジャッキーにもそんな場所が必要だったのか？"
          },
          {
            "stringId": 1756116010350420004,
            "femaleVariant": "いいから、来て"
          },
          {
            "stringId": 1756116010501414932,
            "femaleVariant": "彼もそういう暗い顔をしてた"
          },
          {
            "stringId": 1756116010501414944,
            "femaleVariant": "「もうダメだ、俺はおしまいだ」"
          },
          {
            "stringId": 1756116010518192128,
            "femaleVariant": "あいつらしくない台詞ね",
            "maleVariant": "あいつらしくない台詞だな"
          },
          {
            "stringId": 1756116010518192144,
            "femaleVariant": "ずっと昔の話。ヴァレンティーノズに加わったことがお母さんにバレてね"
          },
          {
            "stringId": 1756116010518192156,
            "femaleVariant": "一方にはお母さん、もう一方には強面のギャングたち。どうしていいかわからない板挟み"
          },
          {
            "stringId": 1756116010518192168,
            "femaleVariant": "それでも何とか乗り切ったわ"
          },
          {
            "stringId": 1756117114744217600,
            "femaleVariant": "見える？"
          },
          {
            "stringId": 1756117114744217612,
            "femaleVariant": "独りで考えごとをしたい時は、いつもここに来るの"
          },
          {
            "stringId": 1756117114760994828,
            "femaleVariant": "そいつは名案だ。いったん腰を下ろして頭を整理しようぜ"
          },
          {
            "stringId": 1756117114844880896,
            "femaleVariant": "[座る] 最近は独りの時間を作れなくてね",
            "maleVariant": "[座る] 最近は独りの時間を作れなくてな"
          },
          {
            "stringId": 1756117114945544192,
            "femaleVariant": "別れを言いたい奴がいたら、今のうちに連絡しとけ"
          },
          {
            "stringId": 1756117114945544216,
            "femaleVariant": "そうじゃないが、どう事が運んだって危険な賭けだ。もし上手くいかなかったら…"
          },
          {
            "stringId": 1756117114945544228,
            "femaleVariant": "とにかく連絡しとけ。話すんなら今しかない。幸い、薬は逃げやしねえからな"
          },
          {
            "stringId": 1756117115029430272,
            "femaleVariant": "独りの時間ね… それがどれだけ贅沢か最近わかった",
            "maleVariant": "独りの時間か… それがどれだけ贅沢か最近わかった"
          },
          {
            "stringId": 1756117847522680832,
            "maleVariant": "よう"
          },
          {
            "stringId": 1756117847522680852,
            "maleVariant": "おう… Vじゃないか"
          },
          {
            "stringId": 1756117847539458056,
            "maleVariant": "調子はどうだ？"
          },
          {
            "stringId": 1756117847539458072,
            "maleVariant": "良くも悪くもないな"
          },
          {
            "stringId": 1756117847606566912,
            "femaleVariant": "ジュデイ"
          },
          {
            "stringId": 1756117847606566932,
            "femaleVariant": "Vじゃない"
          },
          {
            "stringId": 1756117847606566948,
            "femaleVariant": "どうしたの？"
          },
          {
            "stringId": 1756117847774339072,
            "femaleVariant": "トラブルを起こしてないか？"
          },
          {
            "stringId": 1756117847774339088,
            "femaleVariant": "新曲はできそうか？"
          },
          {
            "stringId": 1756117847807893504,
            "maleVariant": "トラブルを起こさないって誓いは、守れてるか？"
          },
          {
            "stringId": 1756117847824670720,
            "maleVariant": "へっ、なかなか苦労してるよ"
          },
          {
            "stringId": 1756117847824670736,
            "maleVariant": "こっちもだ"
          },
          {
            "stringId": 1756117847875002368,
            "maleVariant": "新曲はできそうなのか？"
          },
          {
            "stringId": 1756117847875002384,
            "maleVariant": "ぼちぼちな。仮タイトルも決まってるんだ"
          },
          {
            "stringId": 1756117847875002400,
            "maleVariant": "『Seamurai in Smoke』"
          },
          {
            "stringId": 1756117847875002416,
            "maleVariant": "あの日のことを書いた曲さ"
          },
          {
            "stringId": 1756117847875002432,
            "maleVariant": "ケリー"
          },
          {
            "stringId": 1756117847891779592,
            "maleVariant": "なんだ？"
          },
          {
            "stringId": 1756117847891779608,
            "maleVariant": "あの夜は… すごく楽しかった"
          },
          {
            "stringId": 1756117847891779624,
            "maleVariant": "またやってもいいんだぜ。ボートはいくらでもあるからな"
          },
          {
            "stringId": 1756117847891779640,
            "maleVariant": "ああ。それもいいな"
          },
          {
            "stringId": 1756117847925334016,
            "maleVariant": "V、何かあったのか？ 様子が変だぞ"
          },
          {
            "stringId": 1756117847942111240,
            "maleVariant": "心配ない。声が聞きたくなっただけだ"
          },
          {
            "stringId": 1756117847942111256,
            "maleVariant": "そうか"
          },
          {
            "stringId": 1756117847942111272,
            "maleVariant": "すまん、そろそろ切らないと"
          },
          {
            "stringId": 1756117847958888456,
            "maleVariant": "ああ。きばれよ、ケリー"
          },
          {
            "stringId": 1756117847958888472,
            "maleVariant": "お前もな。じゃ"
          },
          {
            "stringId": 1756117848009220096,
            "femaleVariant": "すぐに済む"
          },
          {
            "stringId": 1756117848009220112,
            "femaleVariant": "起こしてごめん",
            "maleVariant": "起こして悪いな"
          },
          {
            "stringId": 1756117848059551744,
            "femaleVariant": "すぐに済む。ちょっと… 声を聞きたかっただけ",
            "maleVariant": "すぐに済む。ちょっと… 声を聞きたかっただけだ"
          },
          {
            "stringId": 1756117848059551768,
            "femaleVariant": "ん… わざわざこんな時間にかけなくても…"
          },
          {
            "stringId": 1756117848059551784,
            "femaleVariant": "V？"
          },
          {
            "stringId": 1756117848076328972,
            "femaleVariant": "いいよ。何があったの？"
          },
          {
            "stringId": 1756117848109883392,
            "femaleVariant": "こんな遅くに、ごめんね…",
            "maleVariant": "こんな遅くに、悪いな…"
          },
          {
            "stringId": 1756117848109883440,
            "femaleVariant": "何言ってんの… 謝るために寝てるとこ起こしたんじゃないでしょ"
          },
          {
            "stringId": 1756117848143437824,
            "femaleVariant": "これから… やることがあるの",
            "maleVariant": "これから… やることがあるんだ"
          },
          {
            "stringId": 1756117848143437840,
            "femaleVariant": "もう会えないかもしれない"
          },
          {
            "stringId": 1756117848176992256,
            "femaleVariant": "もう会えないかもしれない"
          },
          {
            "stringId": 1756117848176992272,
            "femaleVariant": "はあ！？"
          },
          {
            "stringId": 1756117848176992288,
            "femaleVariant": "何言って… あっそう… 勝手にしなよ"
          },
          {
            "stringId": 1756117848193769476,
            "femaleVariant": "待って。そういうことじゃない",
            "maleVariant": "待て。そういうことじゃない"
          },
          {
            "stringId": 1756117848193769492,
            "femaleVariant": "じゃ、なんなの？"
          },
          {
            "stringId": 1756117848227323904,
            "femaleVariant": "前に、泳ぎに行ったじゃない…",
            "maleVariant": "前に、泳ぎに行った時…"
          },
          {
            "stringId": 1756117848227323920,
            "femaleVariant": "ん？ 楽しかったよね"
          },
          {
            "stringId": 1756117848227323936,
            "femaleVariant": "あの時言ってたでしょ… あたしの中に何かを感じるって",
            "maleVariant": "あの時言ってただろ… 俺の中に何かを感じるって"
          },
          {
            "stringId": 1756117848227323952,
            "femaleVariant": "死を？"
          },
          {
            "stringId": 1756117848260878336,
            "femaleVariant": "ねえ。何をするつもりかわからないけど…"
          },
          {
            "stringId": 1756117848260878352,
            "femaleVariant": "とにかく無事でいて。私は待ってるから"
          },
          {
            "stringId": 1756117848260878368,
            "femaleVariant": "ありがとう。頑張るよ"
          },
          {
            "stringId": 1756117848294432768,
            "femaleVariant": "他の何かならよかった"
          },
          {
            "stringId": 1756117848294432784,
            "femaleVariant": "それと向き合う時が来た"
          },
          {
            "stringId": 1756117848311209984,
            "femaleVariant": "はあ… 何か違うモノだったらよかった"
          },
          {
            "stringId": 1756117848327987208,
            "femaleVariant": "別に、あなたのせいってわけじゃ…"
          },
          {
            "stringId": 1756117848327987220,
            "femaleVariant": "次は別の何かを感じれるといいな"
          },
          {
            "stringId": 1756117848344764416,
            "femaleVariant": "それと向き合う時が来た"
          },
          {
            "stringId": 1756117848361541632,
            "femaleVariant": "上手くいくかわからないけど、やるしかない",
            "maleVariant": "上手くいくかわからないが、やるしかない"
          },
          {
            "stringId": 1756117848378318848,
            "femaleVariant": "もしもし…"
          },
          {
            "stringId": 1756117848378318860,
            "femaleVariant": "パナム、あたしよ",
            "maleVariant": "パナム、俺だ"
          },
          {
            "stringId": 1756117848378318884,
            "femaleVariant": "いま何時だと思ってんの…？"
          },
          {
            "stringId": 1756117848428650496,
            "femaleVariant": "これから… やることがあるの",
            "maleVariant": "これから… やることがあるんだ"
          },
          {
            "stringId": 1756117848428650508,
            "femaleVariant": "かなり危険だけど、やらないといけない",
            "maleVariant": "かなり危険だが、やらないといけない"
          },
          {
            "stringId": 1756117848428650520,
            "femaleVariant": "何をするの？"
          },
          {
            "stringId": 1756117848428650532,
            "femaleVariant": "それは、ここで話せることじゃない"
          },
          {
            "stringId": 1756117848428650544,
            "femaleVariant": "なら言わなくていい。今どこ？ 街にいるの？"
          },
          {
            "stringId": 1756117848428650556,
            "femaleVariant": "ええ",
            "maleVariant": "ああ"
          },
          {
            "stringId": 1756117848445427712,
            "femaleVariant": "わかった。じゃあどっかで待ってなよ。すぐ行く"
          },
          {
            "stringId": 1756117848445427724,
            "femaleVariant": "そうじゃないの。もう時間がない",
            "maleVariant": "そうじゃないんだ。もう時間がない"
          },
          {
            "stringId": 1756117848445427736,
            "femaleVariant": "もしかして前に言ってたことと関係あるの？"
          },
          {
            "stringId": 1756117848445427760,
            "femaleVariant": "ゆっくり深呼吸して、そのことをよーく考えて。それからまたかけ直してよ。私はいつでも待ってるから"
          },
          {
            "stringId": 1756119732040245248,
            "femaleVariant": "[電話する] リバー"
          },
          {
            "stringId": 1756119732057022480,
            "femaleVariant": "[電話する] パナム"
          },
          {
            "stringId": 1756123225056616448,
            "femaleVariant": "やめとく。さよならを言うのは好きじゃない"
          },
          {
            "stringId": 1756123225056616468,
            "femaleVariant": "好きにしろ"
          },
          {
            "stringId": 1756123487250948096,
            "femaleVariant": "これでよし…"
          },
          {
            "stringId": 1756128344456306688,
            "femaleVariant": "お姫様はねんねしてな",
            "maleVariant": "王子様はねんねしてな"
          },
          {
            "stringId": 1756128344456306712,
            "femaleVariant": "向こう側でまた会おうぜ"
          },
          {
            "stringId": 1756128344624078848,
            "femaleVariant": "出番よ、ジョニー。あんたに任せる",
            "maleVariant": "出番だ、ジョニー。お前に任せる"
          },
          {
            "stringId": 1756128344640856068,
            "femaleVariant": "運転席は譲るけど、あまり… 無茶し過ぎないでよ",
            "maleVariant": "運転席は譲るが、あまり… 無茶し過ぎるなよ"
          },
          {
            "stringId": 1756128344640856088,
            "femaleVariant": "ありがとう、V"
          },
          {
            "stringId": 1756128344707964940,
            "femaleVariant": "やってやるさ"
          },
          {
            "stringId": 1756128344707964956,
            "femaleVariant": "たとえそれで街ごと焼き尽くすことになってもな！"
          },
          {
            "stringId": 1756147071067365376,
            "femaleVariant": "きっとこの決断を後悔することになるぞ"
          },
          {
            "stringId": 1756147071067365396,
            "femaleVariant": "気づいたときはもう遅い… 俺は助けてやれない"
          },
          {
            "stringId": 1756147071117697024,
            "femaleVariant": "ええ。でも、怒らないでね",
            "maleVariant": "ああ。でも、怒らないでくれよ"
          },
          {
            "stringId": 1756147606243778560,
            "femaleVariant": "パナムと一緒にやるよ"
          },
          {
            "stringId": 1756147933013614592,
            "femaleVariant": "パナムとは気が合う。あんたの危ないチューマより信頼できるし",
            "maleVariant": "パナムとは気が合う。お前の危ないチューマより信頼できる"
          },
          {
            "stringId": 1756147933013614604,
            "femaleVariant": "好きにしな"
          },
          {
            "stringId": 1756149335655981056,
            "femaleVariant": "〈神輿〉の中で会いましょ",
            "maleVariant": "〈神輿〉の中で会おう"
          },
          {
            "stringId": 1756191108524679168,
            "femaleVariant": "[薬を捨てる] それでいい"
          },
          {
            "stringId": 1756191108524679184,
            "femaleVariant": "[手を開く] 魔が差したみたい",
            "maleVariant": "[手を開く] 魔が差したみたいだ"
          },
          {
            "stringId": 1756191108558233600,
            "femaleVariant": "わかってる"
          },
          {
            "stringId": 1756191108575010820,
            "femaleVariant": "それで… いいの",
            "maleVariant": "それで… いいんだ"
          },
          {
            "stringId": 1756191108575010832,
            "femaleVariant": "おいおい、ちょっと待てよ。ここまで来るのにお前は散々苦労してきただろ？"
          },
          {
            "stringId": 1756191108575010852,
            "femaleVariant": "そうね",
            "maleVariant": "そうだな"
          },
          {
            "stringId": 1756191108575010872,
            "femaleVariant": "なぜ今になって諦める？ どうしてだ？"
          },
          {
            "stringId": 1756191108642119680,
            "femaleVariant": "犠牲が少なくて… 済むから",
            "maleVariant": "犠牲が少なくて… 済むからだ"
          },
          {
            "stringId": 1756191108642119692,
            "femaleVariant": "他の方法を選べば人が死ぬ"
          },
          {
            "stringId": 1756191108642119712,
            "femaleVariant": "人は死ぬもんだ。仕方ないだろ"
          },
          {
            "stringId": 1756191108642119732,
            "femaleVariant": "あたしたちのために死なせていいの？ そうは思えない",
            "maleVariant": "俺たちのために死なせていいのか？ そうは思えない"
          },
          {
            "stringId": 1760566670747635712,
            "femaleVariant": "辛いでしょうね。あなたたち、どちらにとっても"
          },
          {
            "stringId": 1760567420773076992,
            "femaleVariant": "もしここで決めたくなかったら、いい場所がある"
          },
          {
            "stringId": 1760571130416861184,
            "femaleVariant": "そうね。ヴィクターもあたしにウンザリだろうし",
            "maleVariant": "そうだな。ヴィクターも俺にウンザリだろうし"
          },
          {
            "stringId": 1760593517682233344,
            "femaleVariant": "あー、やっぱり外の空気はうまい"
          },
          {
            "stringId": 1760616367579336704,
            "femaleVariant": "ジャッキーのこと？",
            "maleVariant": "ジャッキーのことか？"
          },
          {
            "stringId": 1760624714781167616,
            "femaleVariant": "選択肢はもう1つある"
          },
          {
            "stringId": 1760624714781167636,
            "femaleVariant": "ああ？"
          },
          {
            "stringId": 1760624714797944844,
            "femaleVariant": "何もかも、この薬も忘れて、全て終わらせるの",
            "maleVariant": "何もかも、この薬も忘れて、全て終わらせるんだ"
          },
          {
            "stringId": 1760624714797944864,
            "femaleVariant": "このまま何もしなかったら、2人ともおしまいなんだぞ"
          },
          {
            "stringId": 1760626236558544896,
            "femaleVariant": "それに、2人とも自分でいられる"
          },
          {
            "stringId": 1760626236558544916,
            "femaleVariant": "死に際までってことか？"
          },
          {
            "stringId": 1760626236575322128,
            "femaleVariant": "ええ。命が尽きるまで",
            "maleVariant": "ああ。命が尽きるまで"
          },
          {
            "stringId": 1760626236575322148,
            "femaleVariant": "そんなこと考えもしなかった。お前って奴にはいまだに驚かされる"
          },
          {
            "stringId": 1760626236575322168,
            "femaleVariant": "あんたに影響されたのかも",
            "maleVariant": "お前に影響されたのかもな"
          },
          {
            "stringId": 1760626236592099332,
            "femaleVariant": "V、本当にいいのか？"
          },
          {
            "stringId": 1760626236592099356,
            "femaleVariant": "あんたが納得するなら",
            "maleVariant": "お前が納得してくれるなら"
          },
          {
            "stringId": 1760626236592099376,
            "femaleVariant": "俺なら選ばないが、まあいいさ"
          },
          {
            "stringId": 1760626236608876548,
            "femaleVariant": "本当に？ 反対しないの？",
            "maleVariant": "本当に？ 反対しないのか？"
          },
          {
            "stringId": 1760626236608876560,
            "femaleVariant": "俺もお前に影響されたのかもな"
          },
          {
            "stringId": 1760626236608876572,
            "femaleVariant": "やるか…"
          },
          {
            "stringId": 1760626236608876584,
            "femaleVariant": "ここは… 本当にきれいね",
            "maleVariant": "ここは… 本当にきれいだ"
          },
          {
            "stringId": 1760626236608876596,
            "femaleVariant": "お前といて楽しかったぜ、V"
          },
          {
            "stringId": 1760626236608876608,
            "femaleVariant": "ええ。あたしもよ、ジョニー…",
            "maleVariant": "ああ。俺もだよ、ジョニー…"
          },
          {
            "stringId": 1760631669742505984,
            "femaleVariant": "これが最後かもしれない。だからこそ…"
          },
          {
            "stringId": 1760631669759283216,
            "femaleVariant": "自分の目で終わりを見届けたいの",
            "maleVariant": "自分の目で終わりを見届けたいんだ"
          },
          {
            "stringId": 1761804424957530112,
            "femaleVariant": "回避する方法があるのに、あんたが拒んでるんでしょ",
            "maleVariant": "回避する方法があるのに、お前が拒んでるんだろ"
          },
          {
            "stringId": 1761804728256040960,
            "femaleVariant": "俺に任せてもいいし――"
          },
          {
            "stringId": 1761808930344669184,
            "femaleVariant": "最後まで見届けられないのが唯一の心残りだ"
          },
          {
            "stringId": 1761816944938192896,
            "femaleVariant": "あんな怪物どもと手を組むのは… 絶対にごめんだ"
          },
          {
            "stringId": 1761842852751347712,
            "femaleVariant": "だから、俺に任せて…"
          },
          {
            "stringId": 1761844366777651200,
            "femaleVariant": "パナムたちノーマッドを頼ってもいい。あいつらに何かあっても、お前が耐えられるならな"
          },
          {
            "stringId": 1761845463873359872,
            "femaleVariant": "あるいはアラサカとの“取引”に乗るか… そうすればお前は、魂を売ることになる"
          },
          {
            "stringId": 1761847709067513856,
            "femaleVariant": "ハナコの話に乗ってもいい"
          },
          {
            "stringId": 1761848934223712256,
            "femaleVariant": "裏目に出るぞ。お前の命だけの話じゃないんだ"
          },
          {
            "stringId": 1761858061280874496,
            "femaleVariant": "何よそれ。そんなにあたしが信用できない？",
            "maleVariant": "何だよそれ。そんなに俺が信用できないか？"
          },
          {
            "stringId": 1761859765829242880,
            "femaleVariant": "ごゆっくり"
          },
          {
            "stringId": 1761861668768190464,
            "femaleVariant": "[座る] ありがとう、ミスティ"
          },
          {
            "stringId": 1761862943853383680,
            "femaleVariant": "ありがとう、ミスティ。いい考えね",
            "maleVariant": "ありがとう、ミスティ。いい考えだ"
          },
          {
            "stringId": 1761863811453554688,
            "femaleVariant": "ごゆっくり"
          },
          {
            "stringId": 1761875955926568960,
            "femaleVariant": "まさにそれだ。今の俺は、180度変わってお前を助けようとしてる"
          },
          {
            "stringId": 1761876838039031808,
            "femaleVariant": "それがどうだ？ 今はこうして、ナイトシティの景色を二人で一緒に楽しんでる"
          },
          {
            "stringId": 1761881253852614656,
            "femaleVariant": "最後まで見届けられないのが唯一の心残りだ"
          },
          {
            "stringId": 1761886321310044160,
            "femaleVariant": "お前じゃない"
          },
          {
            "stringId": 1761892697927192576,
            "femaleVariant": "心配すんな。やり遂げてやるよ"
          },
          {
            "stringId": 1793866784782028800,
            "femaleVariant": "[電話する] ケリー"
          },
          {
            "stringId": 1793866902893629440,
            "femaleVariant": "[電話する] ジュディ"
          },
          {
            "stringId": 1793868418748649472,
            "femaleVariant": "さよならを言うのは好きじゃない"
          },
          {
            "stringId": 1793874710036877312,
            "femaleVariant": "あの石頭の警官に連絡してみる"
          },
          {
            "stringId": 1793875100929232896,
            "maleVariant": "ミュージシャンが一人いる"
          },
          {
            "stringId": 1793876428845879296,
            "femaleVariant": "ジュデイと…"
          },
          {
            "stringId": 1793876762578259968,
            "femaleVariant": "パナムと話す"
          },
          {
            "stringId": 1793937912896557056,
            "femaleVariant": "パナムか"
          },
          {
            "stringId": 1793938049513426944,
            "femaleVariant": "声を聞くだけよ",
            "maleVariant": "声を聞くだけだ"
          },
          {
            "stringId": 1793945859290697728,
            "femaleVariant": "ああ。あいつは俺も気に入ってる"
          },
          {
            "stringId": 1793945914856837120,
            "femaleVariant": "ちゃんと話したい"
          },
          {
            "stringId": 1793946378981740544,
            "maleVariant": "おい、まさか、ケリーか？"
          },
          {
            "stringId": 1793946475366846464,
            "maleVariant": "ま、別にいいけどよ"
          },
          {
            "stringId": 1793947617274499072,
            "maleVariant": "ああ、そうだよ"
          },
          {
            "stringId": 1804009669573611520,
            "femaleVariant": "じゃあ、ひとつだけ言うからよく聞いて。いい？ 解けない問題はないの"
          },
          {
            "stringId": 1827219047940231168,
            "femaleVariant": "いいか。パナムたちノーマッドを頼りたいならそうすりゃいい。あいつらに何かあっても、お前が耐えられるならな"
          },
          {
            "stringId": 1827219910104584192,
            "femaleVariant": "問題は他の奴らさ"
          },
          {
            "stringId": 1827220634343444480,
            "femaleVariant": "かといって、アラサカとの“取引”に乗れば… 連中に魂を売ることになる"
          },
          {
            "stringId": 1851598328034041856,
            "femaleVariant": "よう、V"
          },
          {
            "stringId": 1851613443567460352,
            "femaleVariant": "ごめん、寝てた？",
            "maleVariant": "すまん、寝てたか？"
          },
          {
            "stringId": 1851616640918953984,
            "femaleVariant": "いや、寝つけなくてな"
          },
          {
            "stringId": 1851616832397320192,
            "femaleVariant": "また例の倉庫の夢？",
            "maleVariant": "また例の倉庫の夢か？"
          },
          {
            "stringId": 1851621699501236224,
            "femaleVariant": "その方がマシだ"
          },
          {
            "stringId": 1851621873682292736,
            "femaleVariant": "話してみてよ",
            "maleVariant": "話してみろよ"
          },
          {
            "stringId": 1851622224258998272,
            "femaleVariant": "その… 俺じゃない誰かの、無茶苦茶な事件を追ってる夢だ。もうどうにもなんなくて…"
          },
          {
            "stringId": 1851623379353542656,
            "femaleVariant": "それはそうと、お前は何してるんだ？"
          },
          {
            "stringId": 1851624292101201924,
            "femaleVariant": "似たような悪夢を見てた"
          },
          {
            "stringId": 1851624678178496512,
            "femaleVariant": "ちょっと声が聞きたくなって"
          },
          {
            "stringId": 1851654238207025152,
            "femaleVariant": "あたしも、似たような悪夢を見てたみたい",
            "maleVariant": "俺も、似たような悪夢を見てたようだ"
          },
          {
            "stringId": 1851654481644429312,
            "femaleVariant": "ちょっと… 声が聞きたかっただけよ、お巡りさん",
            "maleVariant": "ちょっと… 声が聞きたかっただけさ、お巡りさん"
          },
          {
            "stringId": 1851656032144400384,
            "femaleVariant": "というと？"
          },
          {
            "stringId": 1851656139803795456,
            "femaleVariant": "他人の人生を生きる夢"
          },
          {
            "stringId": 1851656450652053504,
            "femaleVariant": "骨の髄までホットドッグが詰まってそうな、デブ警官が出てきたか？"
          },
          {
            "stringId": 1851657674096975872,
            "femaleVariant": "エゴの塊みたいな大昔のロッカーボーイね",
            "maleVariant": "エゴの塊みたいな大昔のロッカーボーイだ"
          },
          {
            "stringId": 1851661835702181888,
            "femaleVariant": "そりゃ悪夢だな"
          },
          {
            "stringId": 1851662182252355584,
            "femaleVariant": "ええ。試しに代わってみる？",
            "maleVariant": "ああ。試しに代わってみるか？"
          },
          {
            "stringId": 1851665059679117312,
            "femaleVariant": "なあ、お互い眠れないなら、どっかの店に飲みにでも行くか？"
          },
          {
            "stringId": 1851668444398559236,
            "femaleVariant": "今日は無理なの",
            "maleVariant": "今日は無理なんだ"
          },
          {
            "stringId": 1851668958335016960,
            "femaleVariant": "調子が良くないからまた今度ね",
            "maleVariant": "調子が良くないからまた今度な"
          },
          {
            "stringId": 1851669242574610432,
            "femaleVariant": "今日は… 無理なの",
            "maleVariant": "今日は… 無理なんだ"
          },
          {
            "stringId": 1851669482874675200,
            "femaleVariant": "ちょっと… 調子が良くないの。また今度にしましょ",
            "maleVariant": "ちょっと… 調子が良くないんだ。また今度にしよう"
          },
          {
            "stringId": 1851670007447887872,
            "femaleVariant": "どうかしたのか？"
          },
          {
            "stringId": 1855974703717228544,
            "femaleVariant": "さっきは本気でビビったぜ"
          },
          {
            "stringId": 1855979570653372416,
            "femaleVariant": "あたしはまだ生きてる",
            "maleVariant": "俺は、まだ生きてる"
          },
          {
            "stringId": 1856001054180450304,
            "femaleVariant": "今んとこな"
          },
          {
            "stringId": 1856013513964462080,
            "femaleVariant": "最悪の事態を考えとけ、ってこと？",
            "maleVariant": "最悪の事態を考えとけ、ってことか？"
          },
          {
            "stringId": 1856016948126359552,
            "femaleVariant": "あのまま死んじまうかと思った"
          },
          {
            "stringId": 1856020783347605504,
            "femaleVariant": "ここまで色んなことがあったな"
          },
          {
            "stringId": 1856021464016035840,
            "femaleVariant": "すべての始まりは、あのクソったれなゴミ捨て場だった"
          },
          {
            "stringId": 1856021831386734592,
            "femaleVariant": "その後、殺されかけたけどね",
            "maleVariant": "その後、殺されかけたけどな"
          },
          {
            "stringId": 1856023604017045504,
            "femaleVariant": "連絡できてよかったな。俺にはそれができなかった"
          },
          {
            "stringId": 1856028501756157952,
            "femaleVariant": "それだけで済めばいいがな"
          },
          {
            "stringId": 1856069501933395968,
            "femaleVariant": "フン"
          },
          {
            "stringId": 1856099445405196288,
            "femaleVariant": "仕方ないな、何か知らんが相談には乗ってやる"
          },
          {
            "stringId": 1856100872307417088,
            "femaleVariant": "今は違う"
          },
          {
            "stringId": 1856101109453365248,
            "femaleVariant": "そんなことはない"
          },
          {
            "stringId": 1856104176127123456,
            "femaleVariant": "さすがだな"
          },
          {
            "stringId": 1856104487478697984,
            "femaleVariant": "口を挟む暇もなかった"
          },
          {
            "stringId": 1856105367661780992,
            "femaleVariant": "いいじゃないか。余計なことを言わずに済んだ"
          },
          {
            "stringId": 1856251821553278980,
            "femaleVariant": "リスキーだけどアラサカの話に賭けてみる",
            "maleVariant": "リスキーだがアラサカの話に賭けてみる"
          },
          {
            "stringId": 1856253411647479808,
            "femaleVariant": "パナムに協力を頼む"
          },
          {
            "stringId": 1856253516421193728,
            "femaleVariant": "あんたとローグに任せる",
            "maleVariant": "お前とローグに任せる"
          },
          {
            "stringId": 1856253652467638272,
            "femaleVariant": "このまま全てを忘れるのもいい"
          },
          {
            "stringId": 1856262760113561600,
            "femaleVariant": "アラサカとの取引に賭けてみようと思う"
          },
          {
            "stringId": 1856263014590373892,
            "femaleVariant": "パナムに協力を頼んでみる"
          },
          {
            "stringId": 1856263160803811332,
            "femaleVariant": "あんたとローグに任せる。一番勝ち目があるのはそれだから",
            "maleVariant": "お前とローグに任せるよ。一番勝ち目があるのはそれだ"
          },
          {
            "stringId": 1856292412584550412,
            "femaleVariant": "[遮断薬を飲む] ええ。ノーマッドと一緒にやる",
            "maleVariant": "[遮断薬を飲む] ああ。ノーマッドと一緒にやる"
          },
          {
            "stringId": 1856292412584550428,
            "femaleVariant": "[疑似エンドトライジンを飲む] 後は任せたよ、ジョニー",
            "maleVariant": "[疑似エンドトライジンを飲む] 後は任せたぞ、ジョニー"
          },
          {
            "stringId": 1856292760141357056,
            "femaleVariant": "[遮断薬を飲む] ええ。怒らないでね",
            "maleVariant": "[遮断薬を飲む] ああ。怒るなよ"
          },
          {
            "stringId": 1856309111903936512,
            "femaleVariant": "待って。もう少し考えたい",
            "maleVariant": "待ってくれ。もう少し考えたい"
          },
          {
            "stringId": 1857521895422631936,
            "femaleVariant": "しばらく忙しくて"
          },
          {
            "stringId": 1857523815809884160,
            "femaleVariant": "ちょっと… 考え事があるだけ",
            "maleVariant": "ああ、ちょっと、考え事があるだけだ"
          },
          {
            "stringId": 1857524185177071616,
            "femaleVariant": "俺は決して優秀な警官じゃなかったかもしれないが、それでもわかるんだ。誰かが…"
          },
          {
            "stringId": 1857527637106040832,
            "femaleVariant": "んー、説明が難しいな"
          },
          {
            "stringId": 1857529439482355712,
            "femaleVariant": "嘘をついてる時？",
            "maleVariant": "嘘をついてる時か？"
          },
          {
            "stringId": 1857529654230720512,
            "femaleVariant": "何かを隠してる時だ"
          },
          {
            "stringId": 1863169183755083776,
            "femaleVariant": "トラブルを抱えてるんだろ。それも厄介な"
          },
          {
            "stringId": 1863169931280719872,
            "femaleVariant": "あたし自身が歩くトラブルだから",
            "maleVariant": "俺自身が歩くトラブルだからな"
          },
          {
            "stringId": 1863174171017420800,
            "femaleVariant": "ごめん、もう切るね",
            "maleVariant": "すまん、もう切るぞ"
          },
          {
            "stringId": 1863184690046640128,
            "femaleVariant": "ま、お前のそういうところが好きなんだ"
          },
          {
            "stringId": 1863189529149382656,
            "femaleVariant": "待て。事情はわからんが、いつでも俺を頼れよ"
          },
          {
            "stringId": 1863212886238502912,
            "femaleVariant": "ええ、わかってる。ありがとう、リバー",
            "maleVariant": "ああ、わかってる。ありがとう、リバー"
          },
          {
            "stringId": 1867884955840618496,
            "femaleVariant": "本当にいいんだな？ 引き返すことはできないぞ？"
          },
          {
            "stringId": 1871861611483701248,
            "femaleVariant": "ちょっと待って。少し… 考える時間が欲しい",
            "maleVariant": "待ってくれ。少し… 考える時間が欲しい"
          },
          {
            "stringId": 1871900218709843968,
            "femaleVariant": "パナムが言ってたでしょ。「解けない問題はない」って",
            "maleVariant": "パナムが言ってただろ。「解けない問題はない」って"
          },
          {
            "stringId": 1871910626170576896,
            "femaleVariant": "決めるのはお前だが、オルトがどうなったか、それを忘れるな"
          },
          {
            "stringId": 1871997094935207936,
            "femaleVariant": "ちょっと魔が差したみたい。今のは忘れて",
            "maleVariant": "ちょっと魔が差しただけだ。今のは忘れてくれ"
          },
          {
            "stringId": 1896718280267145216,
            "femaleVariant": "最上階よ"
          },
          {
            "stringId": 2012680418856280064,
            "femaleVariant": "死の危険に晒す相手は、そう簡単に選べないよな？"
          },
          {
            "stringId": 2012681269310136320,
            "femaleVariant": "だが幸い、俺はもう死んじまってるからその心配はない"
          },
          {
            "stringId": 2012681506204426240,
            "femaleVariant": "お前が無謀にも特攻作戦をやるってんなら、とことんつきあうぜ"
          },
          {
            "stringId": 2012681895637164032,
            "femaleVariant": "俺とお前とアラサカ・タワー。ケリーの歌の歌詞みたいだが、俺は本気だ。一緒にカッコよくキメてやろう"
          },
          {
            "stringId": 2012682058812366852,
            "femaleVariant": "特攻なんてご免よ",
            "maleVariant": "特攻なんてご免だ"
          },
          {
            "stringId": 2012683066133192704,
            "femaleVariant": "どんな作戦を考えてるの？",
            "maleVariant": "どんな作戦を考えてるんだ？"
          },
          {
            "stringId": 2012685325923524608,
            "femaleVariant": "具体的に、どんな作戦を考えてるの？",
            "maleVariant": "具体的に、どんな作戦を考えてる？"
          },
          {
            "stringId": 2012685666668781568,
            "femaleVariant": "ナシね。特攻なんて性に合わない",
            "maleVariant": "ナシだな。特攻なんて性に合わない"
          },
          {
            "stringId": 2012702736429096960,
            "femaleVariant": "名案だと思うがな"
          },
          {
            "stringId": 2012703605941870592,
            "femaleVariant": "“作戦”？ んー、ローグ式にやるなら――"
          },
          {
            "stringId": 2012703702779961344,
            "femaleVariant": "〈神輿〉がハナコの言うとおりタワーの地下深くにあるなら、エレベーターを探して降りりゃいい"
          },
          {
            "stringId": 2012728280243773440,
            "femaleVariant": "どうせ死ぬなら、銃を片手に、戦って死ぬ。誰も道連れにせずね",
            "maleVariant": "どうせ死ぬなら、銃を片手に、戦って死ぬよ。誰も道連れにせずにな"
          },
          {
            "stringId": 2012728567754924036,
            "femaleVariant": "[アラサカ・タワーを襲撃する] 真正面から攻め込むわけね。気に入った",
            "maleVariant": "[アラサカ・タワーを襲撃する] 真正面から攻め込むわけか。気に入った"
          },
          {
            "stringId": 2015363314335358976,
            "femaleVariant": "最強の武器を手に、真正面からタワーに乗り込んで、あとは力ずくで地下までまっしぐら、ってとこだ"
          },
          {
            "stringId": 2015365315337134080,
            "femaleVariant": "聞けば聞くほど、ありえない"
          },
          {
            "stringId": 2015382991627362304,
            "femaleVariant": "ジョニーとアラサカ・タワーに乗り込む"
          },
          {
            "stringId": 2028806703183372288,
            "femaleVariant": "そうよ",
            "maleVariant": "そうだ"
          },
          {
            "stringId": 2028837300396171264,
            "femaleVariant": "でも――"
          },
          {
            "stringId": 2028838837524701184,
            "femaleVariant": "話は終わり"
          },
          {
            "stringId": 2058080623858835456,
            "femaleVariant": "よく言った。それこそレジェンドになる奴の覚悟だよ。さあ、武器を取れ。行動開始だ"
          },
          {
            "stringId": 2228718799581192192,
            "femaleVariant": "正直、心底どうでもいい"
          },
          {
            "stringId": 2228751083759837184,
            "femaleVariant": "なに考えてんだ"
          },
          {
            "stringId": 2228752002480185344,
            "femaleVariant": "ったく、どこまでも強情な奴だな"
          },
          {
            "stringId": 2228752713985138688,
            "femaleVariant": "誰かさんの影響よ",
            "maleVariant": "誰かさんの影響だ"
          },
          {
            "stringId": 2260391424694788096,
            "femaleVariant": "ええ",
            "maleVariant": "ああ"
          },
          {
            "stringId": 2260393583083626496,
            "femaleVariant": "辛かったんでしょうね。でも… 彼は決断を下す勇気を手に入れた"
          },
          {
            "stringId": 2260405862210576384,
            "femaleVariant": "私がここに… 連れてきた後にね"
          },
          {
            "stringId": 2263217105908637696,
            "femaleVariant": "ジャッキーは何を決心したの？",
            "maleVariant": "ジャッキーは何を決心したんだ？"
          },
          {
