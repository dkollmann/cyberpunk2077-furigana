  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なあ、そこの人！ ダコタに頼まれて来たのか？",
                    "maleVariant": null,
                    "stringId": 1795243207413358592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうよ。例のものはある？",
                    "maleVariant": "そうだ。例のものはあるか？",
                    "stringId": 1795243702727106560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "両手を挙げろ！ 今すぐだ！",
                    "maleVariant": null,
                    "stringId": 1795308339233447936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここに。ダイナラー社のスーパーキドニー、鎌状赤血球症用だ",
                    "maleVariant": null,
                    "stringId": 1795311382200999936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "結局、こうなるのよね",
                    "maleVariant": "結局、こうなるのかよ",
                    "stringId": 1795313040326811648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "説明できるなら撃たないであげる。それもちゃんとした理由ならね",
                    "maleVariant": "説明できるなら撃たないでやる。それもちゃんとした理由ならな",
                    "stringId": 1795334517411438596
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "撃つな！ 撃たないで！",
                    "maleVariant": null,
                    "stringId": 1795338754933547008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ダコタとの電話が盗聴されてて… 警察にやらされたんだ！",
                    "maleVariant": null,
                    "stringId": 1795339108026834944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こっちも仕方ないの",
                    "maleVariant": "こっちも仕方ないんだ",
                    "stringId": 1795342351347900420
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今回は見逃してあげる",
                    "maleVariant": "今回は見逃してやる",
                    "stringId": 1795342827166523392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "インプラントに細工してないでしょうね？",
                    "maleVariant": "インプラントに細工してないだろうな？",
                    "stringId": 1795343016413519872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ラッキーね。サツと違って、あたしは理由なく人を殺さないの",
                    "maleVariant": "ラッキーだな。サツと違って、俺は理由なく人を殺さないんだ",
                    "stringId": 1795343840007053312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "神様… ありがとう… ありがとうございます…",
                    "maleVariant": null,
                    "stringId": 1795346978151751680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "仕方なく、ね… 確かにそういう時もある",
                    "maleVariant": null,
                    "stringId": 1795347310206410756
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "頼む、やめてくれ…",
                    "maleVariant": null,
                    "stringId": 1795347310223187968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと、待って…！",
                    "maleVariant": null,
                    "stringId": 1795347752437047296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "このインプラント、他に誰も触ってないでしょうね？",
                    "maleVariant": "このインプラント、他に誰も触ってないだろうな？",
                    "stringId": 1795348236560392196
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よーく思い出して。あんたにジャックインして、無理やり見させてもらってもいいのよ",
                    "maleVariant": "よーく思い出せ。あんたにジャックインして、無理やり見させてもらってもいいんだぞ",
                    "stringId": 1795348236560392204
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言い訳は無用よ",
                    "maleVariant": "言い訳は無用だ",
                    "stringId": 1795350143962079236
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "視界から消えて",
                    "maleVariant": "視界から消えろ",
                    "stringId": 1795350633705791488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしの前から消えて！",
                    "maleVariant": "俺の前から消えろ！",
                    "stringId": 1795351658290372608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "神様、ありがとう、ありがとうございます…",
                    "maleVariant": null,
                    "stringId": 1795351658290372616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "間に合うといいけど…",
                    "maleVariant": "間に合うといいが…",
                    "stringId": 1795352815180079104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人の命を守るはずの医者が、よくもまあこんなことを…",
                    "maleVariant": null,
                    "stringId": 1851635880394907652
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "聞いてくれ、違うんだ、進んでこんなことをしたわけじゃ…",
                    "maleVariant": null,
                    "stringId": 1851635880394907660
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お金は後で――",
                    "maleVariant": "金は後で――",
                    "stringId": 1853225931964207104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、後で、わかってる。もう行く。手術があるんだ",
                    "maleVariant": null,
                    "stringId": 1853226259338022912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もうちょっとマシな言い訳をして。サツは腎臓インプラント程度であそこまでしない",
                    "maleVariant": "もうちょっとマシな言い訳をしろ。サツは腎臓インプラント程度であそこまでしない",
                    "stringId": 1853232444342587392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本当だよ！ ナイトシティにいるラフェン・シヴの連中を、一人残らず捕まえる作戦って話だ！",
                    "maleVariant": null,
                    "stringId": 1853234480173862912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でもダコタは… いえ、説明するだけ無駄ね",
                    "maleVariant": "でもダコタは… いや、説明するだけ無駄だな",
                    "stringId": 1853234902137622528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お願いだ、俺は見逃してくれ！ 関わりたくなかったんだ！ 仕方なくだよ！",
                    "maleVariant": null,
                    "stringId": 1853235471908655104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "仕方なくどこを撃たれたい？ 頭？ それとも脚？",
                    "maleVariant": "仕方なくどこを撃たれたい？ 頭？ それとも脚か？",
                    "stringId": 1853238847601954816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ試してみましょ",
                    "maleVariant": "じゃあ試してみるか",
                    "stringId": 1853239934899769344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "神様じゃなくて、あたしに礼を言うのね。あんたたち医者だって普段そう思ってるんでしょ？",
                    "maleVariant": "神様じゃなくて、俺に礼を言うんだな。お前ら医者だって普段そう思ってるんだろ？",
                    "stringId": 1853248889772363776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "警察が… スーパーキドニーにウイルスを仕込んだんだ… このチップに入ってる元のファームウェアで上書きしてくれ…",
                    "maleVariant": null,
                    "stringId": 1853249532272627712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[無言でチップを受け取る]",
                    "maleVariant": null,
                    "stringId": 1853251674941214724
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[チップを取る] 正直に言ってくれて助かる",
                    "maleVariant": null,
                    "stringId": 1853252066051674112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "正直に言ってくれてどうも",
                    "maleVariant": null,
                    "stringId": 1853252931990900736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "せ… せめてもの償いだ",
                    "maleVariant": null,
                    "stringId": 1853253525266812928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "え、その、どうだったかな…",
                    "maleVariant": null,
                    "stringId": 1853255418944737280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "警察が… スーパーキドニーにウイルスを仕込んだんだ。このチップに入ってる元のファームウェアで、上書きしてくれ",
                    "maleVariant": null,
                    "stringId": 1853256554963587072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言い訳はいらない",
                    "maleVariant": null,
                    "stringId": 1853258551854944256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "消えてって言ってるでしょ！",
                    "maleVariant": "消えろって、言ってるだろ！",
                    "stringId": 1853265443532623872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[インプラントを取る]",
                    "maleVariant": null,
                    "stringId": 2111164456553570308
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうよ",
                    "maleVariant": "そうだ",
                    "stringId": 2128584841188237316
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "説明して。じゃないと撃つ",
                    "maleVariant": "説明しろ。じゃないと撃つ",
                    "stringId": 2167619436340469764
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}