      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウェイランドは何を手伝ったの？",
              "maleVariant": "ウェイランドは何を手伝ったんだ？",
              "stringId": 1566659289744629760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "グレイソンがちゃんと“家”にいることを確認してもらったのさ",
              "maleVariant": "",
              "stringId": 1566663534933925888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アポなしで行くわけだしね",
              "maleVariant": "アポなしで行くわけだしな",
              "stringId": 1566663860730683392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは本当にいいの？",
              "maleVariant": "あんたは本当にいいのか？",
              "stringId": 1566668552177147904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "幸運を！",
              "maleVariant": "",
              "stringId": 1728878062231838720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よう、V",
              "maleVariant": "",
              "stringId": 1728905734521577472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウェイランド？ あのウェイランド家の？",
              "maleVariant": "",
              "stringId": 1728907600416432132
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一緒に来てくれるの？",
              "maleVariant": "一緒に来てくれるのか？",
              "stringId": 1728910347064795136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウェイランド？ あのウェイランド家の？",
              "maleVariant": "",
              "stringId": 1728911906339250176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだよ。ならアンドリュー・ボア・ボアを知ってんだな？",
              "maleVariant": "",
              "stringId": 1728912362494976000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知らない人はいないでしょ",
              "maleVariant": "知らない人はいないだろ",
              "stringId": 1728913090273828864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一緒に来るんでしょ？",
              "maleVariant": "一緒に来るんだろ？",
              "stringId": 1728913679925862400
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、俺の出番は終わった",
              "maleVariant": "",
              "stringId": 1728913679925862408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スマッシャーが死ぬのを見たくないのかい？",
              "maleVariant": "",
              "stringId": 1728913679925862416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰かと思えば",
              "maleVariant": "",
              "stringId": 1750353986764627968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、協力者のクリスピン・ウェイランドだ",
              "maleVariant": "",
              "stringId": 1750367610015014912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スマッシャーを探してるらしいな",
              "maleVariant": "",
              "stringId": 1750370234978230272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "準備はいい？ 道中で話そう",
              "maleVariant": "",
              "stringId": 1750370823405527040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行こう",
              "maleVariant": "",
              "stringId": 1750372432676085760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "先にやることがある",
              "maleVariant": "",
              "stringId": 1750372601219997696
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと先にやることがあるの",
              "maleVariant": "ちょっと先にやることがある",
              "stringId": 1750373616476446720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行きましょ",
              "maleVariant": "行こうか",
              "stringId": 1750374823966236672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "乗って",
              "maleVariant": "",
              "stringId": 1750443672895971328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また後で戻るかい？",
              "maleVariant": "",
              "stringId": 1750589189592408064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行くんだろ？",
              "maleVariant": "",
              "stringId": 1778080573580599296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "持ってきたものがある",
              "maleVariant": "",
              "stringId": 1778102505277448192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "着てみろよ",
              "maleVariant": "",
              "stringId": 1778120545666818052
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっとマシな恰好になったな",
              "maleVariant": "",
              "stringId": 1778139018790920192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう",
              "maleVariant": "",
              "stringId": 1796725428324732932
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう",
              "maleVariant": "",
              "stringId": 1796725576601767936
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーの？",
              "maleVariant": "",
              "stringId": 1796726193785212928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーのジャケット？",
              "maleVariant": "",
              "stringId": 1796735138021052416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしが着てたら誰かさんと間違えない？",
              "maleVariant": "俺が着てたら誰かさんと間違えないか？",
              "stringId": 1796735572802605056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レプリカよ。特注のね",
              "maleVariant": "",
              "stringId": 1796736105311440896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは大丈夫",
              "maleVariant": "",
              "stringId": 1796740295907676160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本物は大昔に灰になっただろうから",
              "maleVariant": "",
              "stringId": 1796742607371833344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうそう、あの子にもあげたんだった",
              "maleVariant": "",
              "stringId": 1796774531981185024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なあ… ボスもさすがに歳なんじゃないか",
              "maleVariant": "",
              "stringId": 1796774993958604800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うちの婆ちゃんと同じだ。ものを捨てたがらないとことかな",
              "maleVariant": "",
              "stringId": 1796782328554786816
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "調子に乗るんじゃないよ、クリスピン",
              "maleVariant": "",
              "stringId": 1796782844504510464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつか役に立つかも、なんつってさ",
              "maleVariant": "",
              "stringId": 1796787953904979968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ん？ さっきは違うって言ってなかったっけ？",
              "maleVariant": "",
              "stringId": 1796788447255793664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ド忘れだよ",
              "maleVariant": "",
              "stringId": 1796788739867217920
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早くしな",
              "maleVariant": "",
              "stringId": 1796798327509291008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そりゃいい。でも俺は親父みたいな野心家じゃない",
              "maleVariant": "",
              "stringId": 1796826583730032640
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だからもっと長生きすると思うぜ",
              "maleVariant": "",
              "stringId": 1796828006303731712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見たいさ。でも遠くからでいい",
              "maleVariant": "",
              "stringId": 1796832611213316096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつは死んでも生き返る、厄介な奴だからな",
              "maleVariant": "",
              "stringId": 1796832937077182464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。本当の悪党ってのはなかなかしぶといからね",
              "maleVariant": "",
              "stringId": 1796833389256708096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わからない。待たなくていいよ",
              "maleVariant": "",
              "stringId": 1796837560726917120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何だい、怖じ気づいた？",
              "maleVariant": "",
              "stringId": 1796853820382580736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただ… ジョニーに乗せられたんじゃないかって",
              "maleVariant": "",
              "stringId": 1796854137975279616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アダム・スマッシャーは… あいつの問題でしょ",
              "maleVariant": "アダム・スマッシャーは… あいつの問題だろ",
              "stringId": 1796854637131010048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう思う？",
              "maleVariant": "",
              "stringId": 1796855263122493440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、いったい何のつもりだ？",
              "maleVariant": "",
              "stringId": 1796855510754201600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フェアにやりたいだけよ",
              "maleVariant": "フェアにやりたいだけだ",
              "stringId": 1796855812358213632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて。車はここまでだ",
              "maleVariant": "",
              "stringId": 1889577025730920452
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この辺はネズミが多いから、もう少し暗くなるまで待ったほうがいい",
              "maleVariant": "",
              "stringId": 1889577649910464512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スマッシャーかグレイソンが現れるかも",
              "maleVariant": "",
              "stringId": 1889587242183049220
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それまでどうするの？",
              "maleVariant": "それまでどうする？",
              "stringId": 1889588005579931648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その間… 何するの？",
              "maleVariant": "その間… 何するんだ？",
              "stringId": 1889588182780887040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろん見張るんだよ",
              "maleVariant": "",
              "stringId": 1889590394387054592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スマッシャーかグレイソンが現れるかもね",
              "maleVariant": "スマッシャーかグレイソンが現れるかもな",
              "stringId": 1889591150318075904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この時間なら、見つかりにくい",
              "maleVariant": "",
              "stringId": 1889596895122046976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは本当にいいの？",
              "maleVariant": "あんたは本当にいいのか？",
              "stringId": 1893837423736803332
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今までスマッシャーに出くわさなかったのは不思議ね",
              "maleVariant": "今までスマッシャーに出くわさなかったのは不思議だな",
              "stringId": 1893882399761911808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは今までスマッシャーに出くわさなかったのよね？",
              "maleVariant": "あんたは今までスマッシャーに出くわさなかったんだよな？",
              "stringId": 1893896093828927488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何度か話題には上ったけどね",
              "maleVariant": "",
              "stringId": 1893898741357498368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ならどうして探そうとは思わなかったの？",
              "maleVariant": "ならどうして探そうとは思わなかったんだ？",
              "stringId": 1893899547871825920
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーが戻って事情が変わったんだ。わかるだろ",
              "maleVariant": "",
              "stringId": 1893981305510318080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、そうね。そうだと思った",
              "maleVariant": "ああ、そうだよな。そうだと思った",
              "stringId": 1893982412538138624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話は後で",
              "maleVariant": "",
              "stringId": 1893990995946393600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“かも”ね。でも昼間に見つかるような真似はしないだろう",
              "maleVariant": "",
              "stringId": 1894008378786336768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし。もういいだろう",
              "maleVariant": "",
              "stringId": 1894009558862155776
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "前に来たことある",
              "maleVariant": "",
              "stringId": 1911478520118132736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メイルストロームとの仕事以来、引きこもってるらしい",
              "maleVariant": "",
              "stringId": 1912908436864782360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スマッシャーじゃなく、メイルストロームの仕事を？",
              "maleVariant": "",
              "stringId": 1912908436881559552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スマッシャーはいつも複数の手を仕込んでる",
              "maleVariant": "",
              "stringId": 1912908436881559564
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2人で動いてるんだよ。師匠と弟子としてね",
              "maleVariant": "",
              "stringId": 1912908436881559576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私のことは何て？",
              "maleVariant": "",
              "stringId": 1912908437066108932
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーとはよく話すんだろ",
              "maleVariant": "",
              "stringId": 1912908437082886144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーは自分のことばかり話してる",
              "maleVariant": "",
              "stringId": 1912908437116440576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグは最高だって",
              "maleVariant": "ローグは最高だってさ",
              "stringId": 1912908437116440592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "グレイソンの目的は何なの？",
              "maleVariant": "グレイソンの目的は何なんだ？",
              "stringId": 1912908437116440608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーがするのは自分の話ばかりよ",
              "maleVariant": "ジョニーがするのは自分の話ばかりだ",
              "stringId": 1912908437149995012
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん、だろうね",
              "maleVariant": "",
              "stringId": 1912908437166772232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはいつも… 最高だったって",
              "maleVariant": "",
              "stringId": 1912908437200326660
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと褒めすぎだが、まあいいか",
              "maleVariant": "",
              "stringId": 1912908437217103872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最高ね。だが最高ってのは高くつくよ",
              "maleVariant": "",
              "stringId": 1912908437217103884
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ジャケットを受け取る]",
              "maleVariant": "",
              "stringId": 2230518559926906880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "グレイソンの目的は何なの？ ウェイランドから何か聞いてる？",
              "maleVariant": "グレイソンの目的は何なんだ？ ウェイランドから何か聞いてるか？",
              "stringId": 2238819533832404992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見た目と違って、シャイな奴なんだな",
              "maleVariant": "",
              "stringId": 2250411038145593344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう行くのか、シスター？",
              "maleVariant": "もう行くのか、ブラザー？",
              "stringId": 2250413745535270912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[乗る]",
              "maleVariant": "",
              "stringId": 2511788698825912324
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
