        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こんにちは、WNSニュースへようこそ。アリフ・イクバルです",
                    "maleVariant": null,
                    "stringId": 1793567491194003456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本日は役所からのお知らせを中心にお届けます。まずは、アロヨで“労働組合”を名乗るテロ組織が一掃されました",
                    "maleVariant": null,
                    "stringId": 1796691793429643264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティ労働階級の代表を自称するこの組織は、数百万ユーロドルという経済損失に繋がりかねないストライキを画策していました",
                    "maleVariant": null,
                    "stringId": 1796691954490916864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "皆さん、労働組合員を騙る者を見かけたら、ただちに関連する企業の警備部門、もしくはNCPDに通報してください",
                    "maleVariant": null,
                    "stringId": 1796692438731702272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティのあらゆる病院で、患者が急増しています。違法サイバーウェアの不具合による怪我が多発しているためです",
                    "maleVariant": null,
                    "stringId": 1796693248987348992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "回路がショートし、使用者の生命に危険を及ぼすような事故も起きています",
                    "maleVariant": null,
                    "stringId": 1796693536381059072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "NCPDは、サイバーウェアは常に認可を受けた製造者からのみ購入するよう、強く呼びかけています",
                    "maleVariant": null,
                    "stringId": 1796693701854740480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ノース・オークとチャーター・ヒルに、ノーマッドのキャラバンが多数姿を現しました",
                    "maleVariant": null,
                    "stringId": 1796694007468507136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "報道によると、NCPDの努力の甲斐なく、一部がシティ内に入り込んだ可能性があるようです",
                    "maleVariant": null,
                    "stringId": 1796694229481406464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ノーマッド相手には自己防衛の権利を率先して行使しましょう。このような危険な犯罪者から自分の身を守るには、威嚇射撃だけでは不十分です",
                    "maleVariant": null,
                    "stringId": 1796694433106477056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本日最後のニュースです。NCARTの駅で恐ろしい大量殺人が起きました",
                    "maleVariant": null,
                    "stringId": 1796694740179861504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "医療センター駅の2番ホームで、身元不明の男性が電車を待っていた人々に次々と発砲。10人が死亡し、16人が負傷したとのことです",
                    "maleVariant": null,
                    "stringId": 1796695033831473152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティ市民はあらゆる交通手段の中から、最も安全なものを選ぶよう心がけてください",
                    "maleVariant": null,
                    "stringId": 1796695242674257920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "以上、WNSニュースをお届けしました。今日も安全な一日を",
                    "maleVariant": null,
                    "stringId": 1796695515387904000
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}