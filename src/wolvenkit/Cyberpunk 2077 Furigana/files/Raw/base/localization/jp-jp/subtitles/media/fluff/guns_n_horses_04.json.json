  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "手元に札束の山があるとしましょう",
                    "maleVariant": null,
                    "stringId": 1796397655766507520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それも部屋一杯、プール一杯、いや野球場に一杯の札束です！",
                    "maleVariant": null,
                    "stringId": 1831307891551031296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それの100倍を想像してください",
                    "maleVariant": null,
                    "stringId": 1831308102407081984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それだけあれば、レイフィールド・エアロンダイトはあなたのものです",
                    "maleVariant": null,
                    "stringId": 1831308277544439808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "完璧なスタイルと完璧な実用性を達成。車に求めうる贅沢の頂点といえるでしょう",
                    "maleVariant": null,
                    "stringId": 1831308479676338176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "エアロンダイトが誇る流線形のボディは、20世紀初頭のリムジンが得意としていたエレガントな雰囲気と、イタリア製スポーツカーのワイルドさをあわせ持ちます",
                    "maleVariant": null,
                    "stringId": 1831308724523028480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それでいて、あらゆる要素がお互いを引き立て合っている。まるで他の全ての車を淘汰すべく、遺伝子組み換えにより誕生した、まったく新種のスーパーカーともいえるのではないでしょうか",
                    "maleVariant": null,
                    "stringId": 1831308931167997952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "馬力はなんと1800",
                    "maleVariant": null,
                    "stringId": 1831309651397103616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "聞き間違いじゃありません、1800… 1800馬力です",
                    "maleVariant": null,
                    "stringId": 1831309884113866752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "エアロンダイトは合法的に車道を走れる中では世界最速の車です。翼をつければ、成層圏まで飛んでいくでしょうね",
                    "maleVariant": null,
                    "stringId": 1831310050946502656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お気づきの通り、窓は一つもありません。エアロンダイトは周囲の景色を車内に映し出すことができるのです。クリスタルドームのおかげでね",
                    "maleVariant": null,
                    "stringId": 1831310235948863488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "運転するあなたには周囲がはっきりと見えますが、外からは誰にも見られません",
                    "maleVariant": null,
                    "stringId": 1831310505156071424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この英国製最高級スポーツリムジンは、標準装甲仕様でも戦車より安全で、ご覧のとおり、比べ物にならない品格を提供します",
                    "maleVariant": null,
                    "stringId": 1831310718428041216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "また、インテリアについても高級志向の皆さんの期待を裏切らない作りになっています――ミニマルなデザイン、バックライト付パネル、本革のシート、チェリーウッド仕上げ…",
                    "maleVariant": null,
                    "stringId": 1831311133144043520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はあ。お金があったらな…",
                    "maleVariant": null,
                    "stringId": 1831311228170194944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "エアロンダイトを所有できる人は、世界でもおそらく千人以下。いま番組をご覧の方の中には… まずいないでしょう",
                    "maleVariant": null,
                    "stringId": 1831311403055894528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも、もし“あなた”がそうなら… 今すぐレイフィールドのショールームへ！ 絶対に損することはありませんよ！",
                    "maleVariant": null,
                    "stringId": 1831311555560787968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それでは、また次回。もしくは… どこかのハイウェイで！",
                    "maleVariant": null,
                    "stringId": 1831311956720799744
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}