  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちから連絡しようと思ってたの。ちゃんとお別れをするために、ジャッキーのオフレンダを準備してるところでね",
              "maleVariant": "",
              "stringId": 1825530058927558656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "式はコヨーテ・コホでやる。あの子の友達がたくさん来てくれるわ",
              "maleVariant": "",
              "stringId": 1825530720839061504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしも行くよ",
              "maleVariant": "俺も行くよ",
              "stringId": 1825530971842990084
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるべく行けるようにする",
              "maleVariant": "",
              "stringId": 1825531192731815936
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかったよ、ママ・ウェルズ。あたしも行く",
              "maleVariant": "わかったよ、ママ・ウェルズ。俺も行く",
              "stringId": 1825531854408437760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるべく行けるようにする",
              "maleVariant": "",
              "stringId": 1825531889103720452
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう。期待してるよ…",
              "maleVariant": "",
              "stringId": 1825533183029727232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Ahí te veo, V.\" t=\"じゃあね、V\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1825533377376997376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？ やっと連絡してくれたの。待ってたわ",
              "maleVariant": "",
              "stringId": 1864630254302973952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ずいぶん久しぶりね。ジャッキーが旅立った時以来",
              "maleVariant": "",
              "stringId": 1895149357484527616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お悔やみをと思って",
              "maleVariant": "",
              "stringId": 1895153995428450304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "生活のほうはどう？",
              "maleVariant": "生活のほうはどうだ？",
              "stringId": 1895153995428450324
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連絡が遅くなってごめん",
              "maleVariant": "連絡が遅くなってすまない",
              "stringId": 1895158468771106816
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ごめん… 連絡が遅くなって",
              "maleVariant": "すまない… 連絡が遅くなった",
              "stringId": 1895158821545627648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、ずいぶん遅かった。でも全くないよりマシよ",
              "maleVariant": "",
              "stringId": 1895162179589296128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "生活のほうは？",
              "maleVariant": "",
              "stringId": 1895162505453162496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お悔やみをと思って、連絡したの",
              "maleVariant": "お悔やみをと思って、連絡したんだ",
              "stringId": 1895162715772342272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろんそう言ってくれると思ってた",
              "maleVariant": "",
              "stringId": 1895169828607283200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フッ… “お悔やみ”ですって？",
              "maleVariant": "",
              "stringId": 1895171821824430080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは行く必要ないよね",
              "maleVariant": "俺は行く必要ないよな",
              "stringId": 1895181748601147392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V。私たちは一緒に暮らした仲でしょ？",
              "maleVariant": "",
              "stringId": 1895183899591233536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "息子のいない暮らしには慣れないものよ",
              "maleVariant": "",
              "stringId": 1895184080768389120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは参列する必要なさそうね",
              "maleVariant": "俺は参列する必要なさそうだな",
              "stringId": 1895185954330431488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あるに決まってるでしょ。きちんとお別れを言わないと。それが礼儀よ",
              "maleVariant": "",
              "stringId": 1895186516803375104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？ ずいぶん久しぶりね… ジャッキーが旅立って以来",
              "maleVariant": "",
              "stringId": 1896648400763265024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいの。やらないといけないことがあったんでしょ？",
              "maleVariant": "",
              "stringId": 1896670306790973440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもなんとか踏んばってる",
              "maleVariant": "",
              "stringId": 1923980998550155264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんな赤の他人みたいな態度を取るのはやめてちょうだい！",
              "maleVariant": "",
              "stringId": 1924055077659537408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただどうしてるかなと思って",
              "maleVariant": "",
              "stringId": 1924197459868708868
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その… 説明しようと思って… 今まで連絡しなかったのは…",
              "maleVariant": "",
              "stringId": 1924197464868319232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その… どうしてるかと思って。大丈夫か気になったの",
              "maleVariant": "その… どうしてるかと思って。大丈夫か気になってな",
              "stringId": 1924197469297504256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もっと早く連絡したかったのよ、ママ・ウェルズ… でも… 色々あって",
              "maleVariant": "もっと早く連絡したかったんだ、ママ・ウェルズ… でも… 色々あって",
              "stringId": 1924200201987223552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで？ 今日はどうしたの？",
              "maleVariant": "",
              "stringId": 1924202385877430272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "説明しようと思って",
              "maleVariant": "",
              "stringId": 1924202697430331392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたは… 前からやってた仕事を今も続けてるのよね",
              "maleVariant": "",
              "stringId": 1924208339574517760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫じゃないわ、V。自分の子に先立たれるなんて… 親にとって、一番酷なことよ",
              "maleVariant": "",
              "stringId": 1924210039072944128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいのよ、V。わかってるから、その必要はない",
              "maleVariant": "",
              "stringId": 1924315354019852288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたし、一緒だったんだ。ジャッキーが…",
              "maleVariant": "俺、一緒だったんだ。ジャッキーが…",
              "stringId": 1924320041959878656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、よく聞いて。あの子は自分で道を選んだの。覚悟はしていたはずよ",
              "maleVariant": "",
              "stringId": 1924320232129622016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "絶対に息子の後を追わないって約束して。どうか神のご加護がありますように",
              "maleVariant": "",
              "stringId": 1924321279984201728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも心配しないで。助けは必要ない",
              "maleVariant": "",
              "stringId": 1924322117670596608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたもできることは全てやったんでしょ。わかってる",
              "maleVariant": "",
              "stringId": 1924383116222197760
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}