        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんにちは、アリフ・イクバルです。WNSニュースをお届けします",
              "maleVariant": "",
              "stringId": "1793557986565595136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずはローカルニュースからお伝えします",
              "maleVariant": "",
              "stringId": "1796603504655843328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカ社がナイトシティ大学に協力支援を行うことで合意しました",
              "maleVariant": "",
              "stringId": "1796603830385491968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これによりナイトシティ大学の教授および学生は、アラサカ製オオヅクミSx認識インプラントのローンがなんと2割引きになります",
              "maleVariant": "",
              "stringId": "1796603946651598848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なお将来的に、全教員と全在学生に同インプラントの装着が義務付けられる見込みです",
              "maleVariant": "",
              "stringId": "1796604193981317120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "次のニュースです。ビスタ・デル・レイで12名が死亡した事件ですが、その死因はボツリヌス中毒であったことが判明しました",
              "maleVariant": "",
              "stringId": "1796604372641890304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "犠牲者は全員、同じ缶詰肉を食べていた疑いがあります",
              "maleVariant": "",
              "stringId": "1796604722832719872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自然環境で育った動物の肉には病原体が含まれている可能性があるため、食用肉は認可事業者のみから購入するようNCPDが呼び掛けています",
              "maleVariant": "",
              "stringId": "1796604914898288640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボツリヌス菌に続いてナイトシティをパニックに陥れるのは、リズィー・ウィズィーしかありません。ファン待望のライブツアーを発表しました",
              "maleVariant": "",
              "stringId": "1796605226417635328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの世界的アーティストが、もうすぐここナイトシティにやって来ます",
              "maleVariant": "",
              "stringId": "1796605406101618688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チケットを購入できるのは、クレジットレーティングが700以上の方のみです。それ以外のリジー・ウィジーファンは、是非ご家庭のフィーダーユニットでライブをお楽しみください",
              "maleVariant": "",
              "stringId": "1796605587161333760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "以上が、最新ニュースです！ それではまた！",
              "maleVariant": "",
              "stringId": "1796605803251875840"
            }
          ]
        }
      }
    },
