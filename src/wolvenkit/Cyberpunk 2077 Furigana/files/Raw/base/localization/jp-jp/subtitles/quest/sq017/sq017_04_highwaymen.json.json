    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1334369286923227152,
            "femaleVariant": "よし"
          },
          {
            "stringId": 1334374478800490496,
            "femaleVariant": "言ったろ？ サイバーサイコみたいな運転しやがって"
          },
          {
            "stringId": 1334406835389599744,
            "femaleVariant": "ひとまず待機だ"
          },
          {
            "stringId": 1334407236935487488,
            "femaleVariant": "来たぞ。時間通りだ"
          },
          {
            "stringId": 1334433943377678336,
            "femaleVariant": "お目当ての演奏機材よ",
            "maleVariant": "お目当ての演奏機材だ"
          },
          {
            "stringId": 1334435073323175936,
            "femaleVariant": "中はどうだ？"
          },
          {
            "stringId": 1334435406686457856,
            "femaleVariant": "大当たりよ。演奏機材が大量に積まれてる",
            "maleVariant": "大当たりだ。演奏機材が大量に積まれてる"
          },
          {
            "stringId": 1334435757296717824,
            "femaleVariant": "よーし。やったな"
          },
          {
            "stringId": 1337381437262794752,
            "femaleVariant": "いよいよグランドフィナーレだ。こいつを全部、クロームとプラスチックの燃えカスにしてやろう"
          },
          {
            "stringId": 1508094565279199236,
            "femaleVariant": "[スパイクを設置する]"
          },
          {
            "stringId": 1508108005624377344,
            "femaleVariant": "正直に言うとだな――"
          },
          {
            "stringId": 1508108718706388992,
            "femaleVariant": "まったくわからん。まあ、お前なら何とかしてくれるだろ？"
          },
          {
            "stringId": 1508198181264551940,
            "femaleVariant": "了解、ボス"
          },
          {
            "stringId": 1508198528385150976,
            "femaleVariant": "派手にやればいいのね？",
            "maleVariant": "派手にやればいいんだな？"
          },
          {
            "stringId": 1508199279920541696,
            "femaleVariant": "了解、ボス"
          },
          {
            "stringId": 1508201172289843200,
            "femaleVariant": "とりあえず、派手にやればいいのね？",
            "maleVariant": "とりあえず、派手にやればいいんだな？"
          },
          {
            "stringId": 1508201865641848832,
            "femaleVariant": "頼むぜ。ショボいのはナシだ"
          },
          {
            "stringId": 1508216598604410880,
            "femaleVariant": "ミキサー、スピーカー、ドラムに山ほどのケーブル…"
          },
          {
            "stringId": 1675125934088417284,
            "femaleVariant": "相手は武装してるの？",
            "maleVariant": "相手は武装してるのか？"
          },
          {
            "stringId": 1675139298432806912,
            "femaleVariant": "何のつもりだ！？ 頭イカれてんのか！？"
          },
          {
            "stringId": 1675213599454425088,
            "femaleVariant": "わかった"
          },
          {
            "stringId": 1802312133223510016,
            "femaleVariant": "すごい品揃えね",
            "maleVariant": "すごい品揃えだな"
          },
          {
            "stringId": 1802318628069699584,
            "femaleVariant": "警察用スパイクとグレネードをどうやって手に入れたの？",
            "maleVariant": "警察用スパイクとグレネードをどうやって手に入れたんだ？"
          },
          {
            "stringId": 1802361618309967876,
            "femaleVariant": "驚いた。すごい品揃えね",
            "maleVariant": "驚いた。すごい品揃えだな"
          },
          {
            "stringId": 1802361618309967900,
            "femaleVariant": "芸術のために戦ってんだ、ケチってる場合じゃねえ。ほら、早くしろ"
          },
          {
            "stringId": 1802361618326745092,
            "femaleVariant": "芸術のため？ こいつに限ってそれはない"
          },
          {
            "stringId": 1803760487245672448,
            "femaleVariant": "白状しなよ。これは売名行為なんでしょ？",
            "maleVariant": "白状しろよ。これは売名行為なんだろ？"
          },
          {
            "stringId": 1803761124830212096,
            "femaleVariant": "白状したら？ これは全部ヤラセで、そろそろメディアが押しかけてくるんでしょ？",
            "maleVariant": "白状したらどうだ？ これは全部ヤラセで、そろそろメディアが押しかけてくるんだろ？"
          },
          {
            "stringId": 1803761684434251776,
            "femaleVariant": "トラックの乗員は武装してる？ ハッスルが乗ってる可能性は？",
            "maleVariant": "トラックの乗員は武装してるのか？ ハッスルが乗ってる可能性は？"
          },
          {
            "stringId": 1803817188682493952,
            "femaleVariant": "このまま盗めば大儲けできそう",
            "maleVariant": "このまま盗めば大儲けできそうだ"
          },
          {
            "stringId": 1803822169569488896,
            "femaleVariant": "数十万エディーはくだらない機材よ。捨てるのはもったいないんじゃない？",
            "maleVariant": "数十万エディーはくだらない機材だ。捨てるのはもったいないんじゃないか？"
          },
          {
            "stringId": 1803849159043313664,
            "femaleVariant": "なに言ってんだ。俺は盗みなんてやらない"
          },
          {
            "stringId": 1803851578066202624,
            "femaleVariant": "おいおい、冗談だろ？ 俺の弦をくすねまくってたくせに。自分じゃ一度も買ったことないんだぜ"
          },
          {
            "stringId": 1803899576271892480,
            "femaleVariant": "派手にやれよ！"
          },
          {
            "stringId": 1803900684859994112,
            "femaleVariant": "やるじゃねえか、ケリー"
          },
          {
            "stringId": 1803901155511234560,
            "femaleVariant": "ま、しょせんは日本のアイドルのオモチャをぶっ壊しただけで――正直、シャボン玉を割るのと大差ない。とはいえ…"
          },
          {
            "stringId": 1803902921816535040,
            "femaleVariant": "それでも大きな前進だ。あのケリーがまるで別人だな"
          },
          {
            "stringId": 1805378097040928772,
            "femaleVariant": "[カードを取る] 動かないで",
            "maleVariant": "[カードを取る] 動くな"
          },
          {
            "stringId": 1805379210947407872,
            "femaleVariant": "動かないで。バカな真似はしないでよ",
            "maleVariant": "動くな。バカな真似をするんじゃないぞ"
          },
          {
            "stringId": 1805465746401472512,
            "femaleVariant": "ここに仕掛けろ"
          },
          {
            "stringId": 1805493101853487108,
            "femaleVariant": "[隠れる] 次は？"
          },
          {
            "stringId": 1806644304519966724,
            "femaleVariant": "[スパイクとグレネードを取る] 任せて",
            "maleVariant": "[スパイクとグレネードを取る] 任せろ"
          },
          {
            "stringId": 1806763155089862656,
            "femaleVariant": "次は？"
          },
          {
            "stringId": 1806794827084673024,
            "femaleVariant": "ちょ… 銃を下ろせよ。話し合おうぜ"
          },
          {
            "stringId": 1806794829802582016,
            "femaleVariant": "黙って従え。すぐに済む"
          },
          {
            "stringId": 1806794832503713792,
            "femaleVariant": "ったく、何なんだ…"
          },
          {
            "stringId": 1806794835221622784,
            "femaleVariant": "V、アクセスカードを奪え"
          },
          {
            "stringId": 1807063785805262848,
            "femaleVariant": "ここはDJのIVコーマが殺された場所なんだ。知ってたか？"
          },
          {
            "stringId": 1811002884727582720,
            "femaleVariant": "[銃を抜く] 降りて地面に伏せろ！"
          },
          {
            "stringId": 1811037494681350148,
            "femaleVariant": "おら、早くしろ！ お友達がどうなってもいいのか！"
          },
          {
            "stringId": 1811302890021924868,
            "femaleVariant": "おい、急げ！ 時間がないぞ！"
          },
          {
            "stringId": 1811362027594395648,
            "femaleVariant": "さてと… それじゃ、お前らはとっとと消えな。振り返るなよ"
          },
          {
            "stringId": 1812602101136961536,
            "femaleVariant": "運転を頼む。振り切ってくれ"
          },
          {
            "stringId": 1814423292914393088,
            "femaleVariant": "トランクの物を出してくれ"
          },
          {
            "stringId": 1814498268916965380,
            "femaleVariant": "早くスパイクを仕掛けるんだ。時間がない"
          },
          {
            "stringId": 1815303914956480512,
            "femaleVariant": "よし。運転手は任せろ。お前は横の奴をトラックから引きずり降ろせ"
          },
          {
            "stringId": 1815305051797413888,
            "femaleVariant": "本当にうまくいくんでしょうね",
            "maleVariant": "本当にうまくいくんだろうな"
          },
          {
            "stringId": 1815418889670713344,
            "femaleVariant": "さっさと降りろ。両手を広げて、地面に伏せるんだ"
          },
          {
            "stringId": 1815478078061371392,
            "femaleVariant": "降りろ！ 地面に伏せな！",
            "maleVariant": "降りろ！ 地面に伏せるんだ！"
          },
          {
            "stringId": 1815594361549447168,
            "femaleVariant": "後ろを開けろ"
          },
          {
            "stringId": 1815674506427719684,
            "femaleVariant": "[開ける]"
          },
          {
            "stringId": 1815756838232875012,
            "femaleVariant": "ハハハッ！ いいぞ！！"
          },
          {
            "stringId": 1815756838232875040,
            "femaleVariant": "助かったぜ、V！"
          },
          {
            "stringId": 1815756838450978820,
            "femaleVariant": "そういうのは弁護士に任せたほうが良かったんじゃない？",
            "maleVariant": "そういうのは弁護士に任せたほうが良かったんじゃないか？"
          },
          {
            "stringId": 1815756838450978832,
            "femaleVariant": "こいつはな、メッセージなんだよ。これがナイトシティの流儀だ、ってな"
          },
          {
            "stringId": 1815789471008514052,
            "femaleVariant": "こんなことをして何になるの？",
            "maleVariant": "こんなことをして何になるんだ？"
          },
          {
            "stringId": 1815791159970848768,
            "femaleVariant": "テロリストにでも転職する気？",
            "maleVariant": "テロリストにでも転職する気か？"
          },
          {
            "stringId": 1815792229988130840,
            "femaleVariant": "そこそこ楽しかったし、派手にやったのはいいんだけど… こんなことして何になるの？",
            "maleVariant": "そこそこ楽しかったし、派手にやったのはいいんだが… こんなことをして何になるんだ？"
          },
          {
            "stringId": 1815803989323374596,
            "femaleVariant": "弁護士に任せた方が良かったんじゃないの？",
            "maleVariant": "弁護士に任せた方が良かったんじゃないか？"
          },
          {
            "stringId": 1815805633876418560,
            "femaleVariant": "あたしでも同じことをした",
            "maleVariant": "俺でも同じことをした"
          },
          {
            "stringId": 1815813192314216448,
            "femaleVariant": "サツだ。ズラかるぞ"
          },
          {
            "stringId": 1823373038441947136,
            "femaleVariant": "降りて地面に伏せろ！"
          },
          {
            "stringId": 1823380448015507456,
            "femaleVariant": "運転手を脅してトラックを爆破… 第二のジョニー・シルヴァーハンドとして、テロリストにでもなる気？",
            "maleVariant": "運転手を脅してトラックを爆破… 第二のジョニー・シルヴァーハンドとして、テロリストにでもなる気か？"
          },
          {
            "stringId": 1823380448502046720,
            "femaleVariant": "そんな単純な見方しかできないのか？ ただの破壊行為と、芸術のための反抗を一緒にしないでくれ"
          },
          {
            "stringId": 1823383719304192004,
            "femaleVariant": "なるほど。ナイトシティの流儀を教えてやったってわけね",
            "maleVariant": "なるほど。ナイトシティの流儀を教えてやったってわけか"
          },
          {
            "stringId": 1823383719790731264,
            "femaleVariant": "その通りだ。ようやくわかってくれたか"
          },
          {
            "stringId": 1832688827151020032,
            "femaleVariant": "だが本当の標的は彼女じゃなく、バイオテクニカのコーポだった。見つかった死体の状態からすると、二人はよろしくやってた最中に“逝った”らしい"
          },
          {
            "stringId": 1832695331174346752,
            "femaleVariant": "よし。身を隠せ、早く"
          },
          {
            "stringId": 1832713424109957120,
            "femaleVariant": "降りて。言うことを聞けば危害は加えない",
            "maleVariant": "降りろ。言うことを聞けば危害は加えない"
          },
          {
            "stringId": 1832719338179260416,
            "femaleVariant": "降りな。いい？ 言う通りにすれば、危害は加えない",
            "maleVariant": "降りろ。いいか？ 言う通りにすれば、危害は加えない"
          },
          {
            "stringId": 1832720064733376512,
            "femaleVariant": "おい、待ってくれ…！"
          },
          {
            "stringId": 1832720474902753280,
            "femaleVariant": "黙って！ さっさと伏せな！",
            "maleVariant": "黙れ！ さっさと伏せろ！"
          },
          {
            "stringId": 1832736285432344576,
            "femaleVariant": "聞こえなかったか？ 行けっつってんだろ！"
          },
          {
            "stringId": 1834122335081852928,
            "femaleVariant": "出たとこ勝負ってわけね。それなら慣れてる",
            "maleVariant": "出たとこ勝負ってわけか。それなら慣れてる"
          },
          {
            "stringId": 1834150985047076864,
            "femaleVariant": "何かの間違いだ！ このトラックは――"
          },
          {
            "stringId": 1834189849316945920,
            "femaleVariant": "言う通りにしろって言ってるでしょ。地面に伏せて。3度目はないよ",
            "maleVariant": "言う通りにしろって言ってるだろ。地面に伏せろ。3度目はないぞ"
          },
          {
            "stringId": 1834206657839230976,
            "femaleVariant": "第二の俺だ？ ありえない。カッとして火がついても、こいつの魂には肝心の火薬がない"
          },
          {
            "stringId": 1834209712232067072,
            "femaleVariant": "ふうん。で、本当の理由は？"
          },
          {
            "stringId": 1837374321998819348,
            "femaleVariant": "あの小娘どもが俺の曲でひと儲けできると思ってんなら、そいつは大間違いだ"
          },
          {
            "stringId": 1837374322015596552,
            "femaleVariant": "俺がここまでたどり着くのに、どれだけ苦労したかわかるか？ どれだけの犠牲を払って成功を掴んだか"
          },
          {
            "stringId": 1837374322015596564,
            "femaleVariant": "あんなぽっと出が人の曲をパクろうとしてるのを、黙って見過ごせるとでも？ ずうずうしい奴らめ"
          },
          {
            "stringId": 1837374322032373764,
            "femaleVariant": "ハッ、まだ“成功”にこだわってるのか。ケリーは昔からそればっかりだ"
          },
          {
            "stringId": 1922735219663302656,
            "femaleVariant": "自動モジュールに、タイヤを確実に捕える中空スパイク、グレネードもそう。すべて警察の装備よ。どうやって手に入れたの？",
            "maleVariant": "自動モジュールに、タイヤを確実に捕える中空スパイク、グレネードもそう。すべて警察の装備だ。どうやって手に入れたんだ？"
          },
          {
            "stringId": 1922741441175089152,
            "femaleVariant": "どうやってって――デザイナードラッグや最新オーディオヴォックス、夕張メロンと一緒さ"
          },
          {
            "stringId": 1922743994868690944,
            "femaleVariant": "ブラックマーケット？"
          },
          {
            "stringId": 1922744103836708864,
            "femaleVariant": "ファンの贈り物だよ"
          },
          {
            "stringId": 1922767440205967360,
            "femaleVariant": "偉そうに。俺はもう、ニコニコしながら紙にサインするのにはウンザリなんだ"
          },
          {
            "stringId": 1928413355222626304,
            "femaleVariant": "クロークで姿を消したAVが彼女のリムジンをつけて… 車ごと木っ端みじんにしたのさ"
          },
          {
            "stringId": 1928416297812094976,
            "femaleVariant": "それも面白そうだな"
          },
          {
            "stringId": 1928417011833630720,
            "femaleVariant": "だが実際、どう転ぶかは俺にもわからない。その場のノリに任せるつもりだ"
          },
          {
            "stringId": 2099844757670146052,
            "femaleVariant": "[降りる]"
          },
          {
            "stringId": 2231409120504901632,
            "femaleVariant": "そのまま、動くんじゃねえぞ"
          },
          {
            "stringId": 2231409891669635072,
            "femaleVariant": "おとなしくしてりゃ、すぐに済む"
          },
          {
            "stringId": 2231411298506305536,
            "femaleVariant": "V、早く開けろ！"
          },
          {
            "stringId": 2231414076075077632,
            "femaleVariant": "おい、もったいつけずに教えろ。中身はどうだ？ お目当てのものか？"
          },
          {
            "stringId": 2237193681243402240,
            "femaleVariant": "早くしろ！"
          },
          {
            "stringId": 2237198347876364288,
            "femaleVariant": "小娘どもにアメリカ流の“おもてなし”をしてやるんだ。急げ、V！"
          },
          {
            "stringId": 2237199814070824960,
            "femaleVariant": "いつまで突っ立ってんだ？"
          },
          {
            "stringId": 2237200606861389824,
            "femaleVariant": "何してんだよ？"
          },
          {
            "stringId": 2237202292837380096,
            "femaleVariant": "静かにしてろ。おしゃべりは禁止だ"
          },
          {
            "stringId": 2413983319915163652,
            "femaleVariant": "[乗る]"
          }
        ]
      }
    }
  }
}