    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1428614983732842496,
            "femaleVariant": "滑走路に何人か残して、残りはヘルマンと行ったってことね",
            "maleVariant": "滑走路に何人か残して、残りはヘルマンと行ったってことか"
          },
          {
            "stringId": 1428644837865967616,
            "femaleVariant": "言ってたとおりね。壊れてる",
            "maleVariant": "言ってたとおりだな。壊れてる"
          },
          {
            "stringId": 1466116853736820736,
            "femaleVariant": "丘を越えて古いガソリンスタンドに向かったみたい"
          },
          {
            "stringId": 1531609420076687360,
            "femaleVariant": "ドローンにパトロール… ここで間違いなさそうね",
            "maleVariant": "ドローンにパトロール… ここで間違いなさそうだな"
          },
          {
            "stringId": 1550289155289305088,
            "femaleVariant": "ええ、どこかにヘルマンを監禁してるのかも"
          },
          {
            "stringId": 1550289639077105664,
            "femaleVariant": "すぐにわかる"
          },
          {
            "stringId": 1550326555981959168,
            "femaleVariant": "寂れた場所だ。応援を呼ぼうと思ってここへ来たなら、ずいぶんガッカリしただろうね"
          },
          {
            "stringId": 1550331757355126784,
            "femaleVariant": "私でもそうする。仲間を呼ぼうとしてるんだ。援護なしで砂漠を渡るのは無謀だからね"
          },
          {
            "stringId": 1641712814177144832,
            "femaleVariant": "直そうとしたみたいだ。でもオイルポンプがイカれて、ピストンが音を上げた"
          },
          {
            "stringId": 1641721302240481280,
            "femaleVariant": "うちの… ミッチたちの車だ。調べよう"
          },
          {
            "stringId": 1641734122919186432,
            "femaleVariant": "西に向かってる"
          },
          {
            "stringId": 1641736594991632384,
            "femaleVariant": "うん、でも今度は2台しかない"
          },
          {
            "stringId": 1662509107329650688,
            "femaleVariant": "他には何もなさそうね",
            "maleVariant": "他には何もなさそうだな"
          },
          {
            "stringId": 1662509222589124608,
            "femaleVariant": "クソ、誰もいない"
          },
          {
            "stringId": 1662509439803740160,
            "femaleVariant": "早くヘルマンの居場所を突き止めないと…"
          },
          {
            "stringId": 1730129179676635136,
            "femaleVariant": "多少は時間をロスしたはず。早く追い付かないと",
            "maleVariant": "多少は時間をロスしたはずだ。早く追い付かないと"
          },
          {
            "stringId": 1730167725399891976,
            "femaleVariant": "他の奴らがヘルマンを連れてったのね",
            "maleVariant": "他の奴らがヘルマンを連れてったのか"
          },
          {
            "stringId": 1730167725399891988,
            "femaleVariant": "連中が合流して応援を呼んだら、こっちに勝ち目はない"
          },
          {
            "stringId": 1730167725399892000,
            "femaleVariant": "じゃあ急がないと",
            "maleVariant": "急がないとな"
          },
          {
            "stringId": 1751809391932104704,
            "femaleVariant": "スコーピオンの言ってたとおりかも"
          },
          {
            "stringId": 1751826175624110080,
            "femaleVariant": "一掃してやれ、V！"
          },
          {
            "stringId": 1751826844397494272,
            "femaleVariant": "私の仲間を殺しやがって！"
          },
          {
            "stringId": 1751826865972994048,
            "femaleVariant": "気をつけて！ バンの裏にスナイパーがいる！"
          },
          {
            "stringId": 1754414960414945280,
            "femaleVariant": "どういうこと？",
            "maleVariant": "どういうことだ？"
          },
          {
            "stringId": 1754417549105819648,
            "femaleVariant": "ナイトシティは私に合わない"
          },
          {
            "stringId": 1754419058249953280,
            "femaleVariant": "ほら、何かのプロトタイプを盗んだ企業エージェント――確かミリテクよ。覚えてない？ 3つの州をまたがるカーチェイスの末、湖に突っ込んで死んだって",
            "maleVariant": "ほら、何かのプロトタイプを盗んだ企業エージェント――確かミリテクだ。覚えてないか？ 3つの州をまたがるカーチェイスの末、湖に突っ込んで死んだって"
          },
          {
            "stringId": 1754419297442721792,
            "femaleVariant": "街にうんざりしたら、キャンピングカーに食料を積んで、旅に出ようって言ってた…"
          },
          {
            "stringId": 1754438593556377600,
            "femaleVariant": "気をつけて、V"
          },
          {
            "stringId": 1754524686310043648,
            "femaleVariant": "ミッチはアルデカルドスと連絡ついたかな。干渉が酷いから…"
          },
          {
            "stringId": 1754527871061848064,
            "femaleVariant": "ヘルマンはきっとそこに拘束されてる"
          },
          {
            "stringId": 1754529525295345664,
            "femaleVariant": "大丈夫、きっとすぐに来る"
          },
          {
            "stringId": 1761796929532588032,
            "femaleVariant": "でも、あれを見て。潰れたスタンドにしては、ポンプの状態がいい"
          },
          {
            "stringId": 1761808718412509184,
            "femaleVariant": "連中は救助を呼んだのかもしれない"
          },
          {
            "stringId": 1766231412686348288,
            "femaleVariant": "スコーピオンの言ってたとおりかも"
          },
          {
            "stringId": 1772127462955282432,
            "femaleVariant": "きれいな場所らしいね。ライアン・ディーマーの車が底にあるし",
            "maleVariant": "きれいな場所らしいな。ライアン・ディーマーの車が底にあるし"
          },
          {
            "stringId": 1772134060847132672,
            "femaleVariant": "誰よそれ？"
          },
          {
            "stringId": 1772139469133705216,
            "femaleVariant": "自殺したの？"
          },
          {
            "stringId": 1772139560284319744,
            "femaleVariant": "いや、ウイルス攻撃だと思う。何なら湖にダイブする前に窒息死してたかも",
            "maleVariant": "いや、ウイルス攻撃だと思う。何なら湖にダイブする前に窒息死してたかもな"
          },
          {
            "stringId": 1772140715664076800,
            "femaleVariant": "悲惨だね"
          },
          {
            "stringId": 1772144707316084736,
            "femaleVariant": "でも人の不幸を聞いて、少しは気が紛れたでしょ？",
            "maleVariant": "だが人の不幸を聞いて、少しは気が紛れただろ？"
          },
          {
            "stringId": 1772146926270672896,
            "femaleVariant": "ハッ"
          },
          {
            "stringId": 1772151783861907456,
            "femaleVariant": "どういうこと？",
            "maleVariant": "どういうことだ？"
          },
          {
            "stringId": 1772152048019173376,
            "femaleVariant": "ナイトシティは私に合わない"
          },
          {
            "stringId": 1772152438592761856,
            "femaleVariant": "街にうんざりしたら、キブルの缶詰を買い込んで――"
          },
          {
            "stringId": 1772153296294375424,
            "femaleVariant": "ロマンがあるね",
            "maleVariant": "ロマンがあるな"
          },
          {
            "stringId": 1772154949319913472,
            "femaleVariant": "ええ… でもあいつは死んだ。V、どうして何もかもこうやって…"
          },
          {
            "stringId": 1772155658660605952,
            "femaleVariant": "気持ちはわかるよ… でもあんたはまだ生きてる。まだやるべきことがある",
            "maleVariant": "気持ちはわかる… だがあんたはまだ生きてる。まだやるべきことがある"
          },
          {
            "stringId": 1772162703581376512,
            "femaleVariant": "ん… そうだね"
          },
          {
            "stringId": 1772180750195093504,
            "femaleVariant": "なんで…"
          },
          {
            "stringId": 1772183431596863488,
            "femaleVariant": "ん？"
          },
          {
            "stringId": 1772183509006938112,
            "femaleVariant": "いつもこうだ。クソの山から抜け出したと思ったら、もっとデカいクソに足を突っ込んでる"
          },
          {
            "stringId": 1772184208331632640,
            "femaleVariant": "パナム…"
          },
          {
            "stringId": 1772184303458447360,
            "femaleVariant": "しかも周りの人間まで引きずりこんで"
          },
          {
            "stringId": 1772185986833969152,
            "femaleVariant": "でもそれももう終わり。私がまた何かやらかしたら、あんたが張り倒して"
          },
          {
            "stringId": 1772191239327313920,
            "femaleVariant": "あんたは別に何もーー"
          },
          {
            "stringId": 1772191415957843968,
            "femaleVariant": "約束して"
          },
          {
            "stringId": 1772191538767065088,
            "femaleVariant": "わかった。約束する"
          },
          {
            "stringId": 1954914388658774016,
            "femaleVariant": "ガソリンスタンドがあるだろ？"
          },
          {
            "stringId": 1955070312444612608,
            "femaleVariant": "賭けてもいいが、カン・タオの奴らはきっとあそこでくつろいでる"
          },
          {
            "stringId": 1955070819284307968,
            "femaleVariant": "ヘルマンを中に監禁してね",
            "maleVariant": "ヘルマンを中に監禁してな"
          },
          {
            "stringId": 1955071168921489408,
            "femaleVariant": "急ぐぞ。あの野郎、きっと死ぬほど退屈してるぜ"
          },
          {
            "stringId": 1979423555545096192,
            "femaleVariant": "ここを通ったようね",
            "maleVariant": "ここを通ったようだな"
          },
          {
            "stringId": 1979424050842066944,
            "femaleVariant": "タイヤの跡を追っていこう"
          },
          {
            "stringId": 1986354069071589376,
            "femaleVariant": "コーヒーとウィスキーの水筒を持って、一緒に旅に出ようって言ってたんだ… 何もかも捨てて"
          },
          {
            "stringId": 1986354651576528896,
            "femaleVariant": "クレーターレイクへ行くのがあいつの夢でさ"
          }
        ]
      }
    }
  }
}