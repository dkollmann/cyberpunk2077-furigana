        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ではさっそく、本日の一大ニュースから。アラサカ社は北九州および福岡の不採算施設を売却すると発表しました",
              "maleVariant": "",
              "stringId": "1752986579169239040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "先週の株主総会で新CEOのヨリノブ・アラサカ氏が宣言した大胆な再建計画ですが、この売却がその第一弾のようです",
              "maleVariant": "",
              "stringId": "1752986687667494912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一部アナリストは、これにより200億ユーロドル以上の費用削減が実現できると推測しています",
              "maleVariant": "",
              "stringId": "1752986873894592512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また関係筋の話によると、売却によって得られた資金は研究開発施設に投じられるとのこと",
              "maleVariant": "",
              "stringId": "1752987013128708096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカ家の異端児と呼ばれていたヨリノブ氏ですが、投資家たちの疑念は払拭されたようですね。新生アラサカ社は3%の上昇をマークしました",
              "maleVariant": "",
              "stringId": "1752987174458417152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "巨大日本企業アラサカの今後は？ 次回の“株価上昇・気分上々”も、どうぞお見逃しなく！",
              "maleVariant": "",
              "stringId": "1752987273175556096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "皆さん、こんにちは！ 今日も“株価上昇・気分上々”の時間です",
              "maleVariant": "",
              "stringId": "1753252642981212160"
            }
          ]
        }
      }
