    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "“あんしんシティライフ”へようこそ。本官はドブズ警部だ。今日はナイトシティのストリートを生き延びるすべをみんなに伝授するぞ",
                    "maleVariant": null,
                    "stringId": 1805354620293988352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "我らがミスター・パギンズは、これから友人のミスター・パグバーグに会いに行くところだ。出かける前に、気をつけるべきことは何だろうか？",
                    "maleVariant": null,
                    "stringId": 1805354830227292160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いちばん大切なのは、武器を持たずに家を出ないこと。武器さえあれば、何があってもひとまず安心だ",
                    "maleVariant": null,
                    "stringId": 1805355149279608832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "NCPD掲示板のチェックも忘れずに。街で銃撃事件が起きていないか、起きていたら場所はどこかを確認しておこう",
                    "maleVariant": null,
                    "stringId": 1805355529518432256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もっとも、レポートに何も出ていなくても、パシフィカや北工業地区には近づかないようにしよう。その2つの地域はNCPDの管轄外なんだ",
                    "maleVariant": null,
                    "stringId": 1805355749903941632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミスター・パギンズは友達と会うために、リトルチャイナのバーに向かった。それほど遠くないし、歩いても平気だろうと考えたんだ",
                    "maleVariant": null,
                    "stringId": 1805356081153294336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ところが、それは“猿の浅知恵”だった… 彼はイヌなのにねぇ",
                    "maleVariant": null,
                    "stringId": 1805356379317977088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしても必要でない限り、ナイトシティを徒歩で移動するのはもっての他。地下鉄NCARTや、コンバット・キャブといったプライベートサービスを利用しよう",
                    "maleVariant": null,
                    "stringId": 1805356588127207424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おやおや、ミスター・パギンズがまた問題に巻き込まれたぞ。ワトソン地区は、企業の治安部隊によって、一時的に封鎖されているようだ",
                    "maleVariant": null,
                    "stringId": 1805357327281012736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しかし、友達に会いに行くのに急いでいた彼は、バリケードを越えて進むことにした",
                    "maleVariant": null,
                    "stringId": 1805357545250603008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これも間違いだ！ 企業の治安部隊は何の警告なしに、侵入者を撃ってもいいことになってるんだ",
                    "maleVariant": null,
                    "stringId": 1805357792077004800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんな時はまずは友達に電話をして、新しい待ち合わせ場所を決めるといい",
                    "maleVariant": null,
                    "stringId": 1805358286434451456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "新しい場所を決める時は、ミスター・パギンズは、そこがギャングや危険な連中が集会に使っていない場所か、きちんと確かめてから決めないといけない",
                    "maleVariant": null,
                    "stringId": 1805358616643616768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おめでとう、ミスター・パギンズ。みんなのおかげで、やっと友人のミスター・パグバーグに会えたね。帰りもアドバイスを覚えていることを祈るばかりだ",
                    "maleVariant": null,
                    "stringId": 1805360105906749440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さて、今日の“あんしんシティライフ”はここまで。みんなしっかり生き延びろ！",
                    "maleVariant": null,
                    "stringId": 1805360259854483456
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}