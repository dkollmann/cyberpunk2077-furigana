    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1862099924888338432,
            "femaleVariant": "で、次は何？",
            "maleVariant": "で、次は何だ？"
          },
          {
            "stringId": 1862111411509936128,
            "femaleVariant": "ねえ、ちょっと話せない？ すぐ終わるから"
          },
          {
            "stringId": 1862112889364561924,
            "femaleVariant": "何の話？",
            "maleVariant": "何の話だ？"
          },
          {
            "stringId": 1862113452860915712,
            "femaleVariant": "何の話？",
            "maleVariant": "何の話だ？"
          },
          {
            "stringId": 1862113767769260032,
            "femaleVariant": "それが、その… レイザーのことなの"
          },
          {
            "stringId": 1862113948627648512,
            "femaleVariant": "前にお父さんが戦ったんだ。ずっと前、レイザーが新人の頃にね"
          },
          {
            "stringId": 1862114432952320000,
            "femaleVariant": "3ラウンド目に倒れて、審判が試合を止めたけど… レイザーはやめなくて、その後3分もお父さんを殴り続けた。命こそ助かったけど…"
          },
          {
            "stringId": 1862114628792762368,
            "femaleVariant": "気の毒に。それからどうなったの？",
            "maleVariant": "気の毒にな。それからどうなった？"
          },
          {
            "stringId": 1862115718087065604,
            "femaleVariant": "あたしに任せて",
            "maleVariant": "俺に任せろ"
          },
          {
            "stringId": 1862116886016180224,
            "femaleVariant": "子供は引っ込んでて",
            "maleVariant": "子供は引っ込んでろ"
          },
          {
            "stringId": 1862117668069326848,
            "femaleVariant": "ええ、あたしに任せて",
            "maleVariant": "ああ、俺に任せておけ"
          },
          {
            "stringId": 1862119924017676288,
            "femaleVariant": "子供は引っ込んでなさい",
            "maleVariant": "子供は引っ込んでるんだ"
          },
          {
            "stringId": 1862120581097340928,
            "femaleVariant": "バーカ！ バーーカ！"
          },
          {
            "stringId": 1862200459352096768,
            "femaleVariant": "こいつは時の人じゃないか！ V、よくやったな！"
          },
          {
            "stringId": 1862218017279066112,
            "femaleVariant": "で… 次は何？",
            "maleVariant": "で… 次は何だ？"
          },
          {
            "stringId": 1862219015120764928,
            "femaleVariant": "何って、そんなのタイトルマッチに決まってるだろ！ 最後の相手はレイザー・ヒューズ。聞いたことあるか？"
          },
          {
            "stringId": 1863215696171778052,
            "femaleVariant": "シーザーじゃない",
            "maleVariant": "シーザーじゃないか"
          },
          {
            "stringId": 1863218823293845504,
            "femaleVariant": "あら、ライノ",
            "maleVariant": "やあ、ライノ"
          },
          {
            "stringId": 1863222357766717440,
            "femaleVariant": "あら、ライノ。忙しそうね",
            "maleVariant": "よう、ライノ。忙しそうだな"
          },
          {
            "stringId": 1863454616763514880,
            "femaleVariant": "聞かせて",
            "maleVariant": "聞かせろ"
          },
          {
            "stringId": 1863455063289118720,
            "femaleVariant": "興味ない"
          },
          {
            "stringId": 1863455410510381056,
            "femaleVariant": "やめとく"
          },
          {
            "stringId": 1863455854972387328,
            "femaleVariant": "おいおい… まだ話を聞いてもいないだろ"
          },
          {
            "stringId": 1863460594435244032,
            "femaleVariant": "言ってみて",
            "maleVariant": "言ってみろ"
          },
          {
            "stringId": 1863460869564809216,
            "femaleVariant": "ここじゃまずい。人が多すぎる。向こうに行こう"
          },
          {
            "stringId": 1863462288783065088,
            "femaleVariant": "これから言うことは、2人だけの秘密だ、いいな？"
          },
          {
            "stringId": 1863462931736313856,
            "femaleVariant": "なあ、V、スポーツは公平であるべきだが、人生は必ずしもそうじゃない。そうだろ？"
          },
          {
            "stringId": 1863464990569152516,
            "femaleVariant": "やる"
          },
          {
            "stringId": 1863468401494605824,
            "femaleVariant": "わかった、やるよ"
          },
          {
            "stringId": 1863471531670126592,
            "femaleVariant": "あたしをカネで買おうだなんて… そういう話はお断りよ",
            "maleVariant": "俺をカネで買おうとはな… そういう話はお断りだ"
          },
          {
            "stringId": 1863473394947735552,
            "femaleVariant": "さあ、怪しまれる前にリングに戻ろう"
          },
          {
            "stringId": 1863476846457274368,
            "femaleVariant": "よし、二人とも戦う準備はいいか？"
          },
          {
            "stringId": 1863478438094979072,
            "femaleVariant": "1ラウンドだ"
          },
          {
            "stringId": 1863478634505846784,
            "femaleVariant": "え？"
          },
          {
            "stringId": 1863479397449744388,
            "femaleVariant": "やろう"
          },
          {
            "stringId": 1863481292251742212,
            "femaleVariant": "それでは、試合開始！"
          },
          {
            "stringId": 1863481295573630976,
            "femaleVariant": "[試合を開始する] 鳴らして",
            "maleVariant": "[試合を開始する] 鳴らしてくれ"
          },
          {
            "stringId": 1863481297133912064,
            "femaleVariant": "始めましょ",
            "maleVariant": "始めよう"
          },
          {
            "stringId": 1863481300439023616,
            "femaleVariant": "いつでも"
          },
          {
            "stringId": 1863481302217408512,
            "femaleVariant": "よし… ナイトシティ市民の皆さん！ 次は本日の目玉、ヘビー級タイトルマッチ！"
          },
          {
            "stringId": 1863481304096456704,
            "femaleVariant": "両者！ コーナーに！"
          },
          {
            "stringId": 1863481307988770820,
            "femaleVariant": "ゴングを鳴らすぞ？ いいな？"
          },
          {
            "stringId": 1863486634704519168,
            "femaleVariant": "お待ちかね、今夜の対戦カードは、レイザー・ヒューズ v.s. V！ 皆さん拍手を！"
          },
          {
            "stringId": 1863499231289950208,
            "femaleVariant": "そこまで！ 試合終了！"
          },
          {
            "stringId": 1863501759180193796,
            "femaleVariant": "勝利の栄誉と賞金はレイザー・ヒューズのものに！ チャンピオン誕生だ！"
          },
          {
            "stringId": 1863501759196971012,
            "femaleVariant": "おい、2人とも！ 試合は終わりだぞ！"
          },
          {
            "stringId": 1863502206813093888,
            "femaleVariant": "いいパンチの的だった。人間サンドバッグになって金でも稼いだらどうだ？ うちの天井からぶら下げて、動かなくなるまで続きをやってもいいんだぜ？"
          },
          {
            "stringId": 1863505636747132928,
            "femaleVariant": "V、ちょっと来てくれ。話がある"
          },
          {
            "stringId": 1863508708386947072,
            "femaleVariant": "わかってくれてよかった。ほら、約束どおりの金だ"
          },
          {
            "stringId": 1863509719650418688,
            "femaleVariant": "にしても、うまくやったな。誰も疑ってちゃいないようだ"
          },
          {
            "stringId": 1863510881556512768,
            "femaleVariant": "言われた通り、いい仕事をしたでしょ。でもあのレイザーって奴… 本当にクズね",
            "maleVariant": "言われた通り、いい仕事をしただろ。でもあのレイザーって奴… クズ野郎だな"
          },
          {
            "stringId": 1863511781486379008,
            "femaleVariant": "違いない。でもあいつにだって守らなきゃいけない立場があるんだよ"
          },
          {
            "stringId": 1863512769010089988,
            "femaleVariant": "勝利の栄誉と賞金はVのものに！ チャンピオン誕生だ！"
          },
          {
            "stringId": 1863512770369044480,
            "femaleVariant": "やめといたほうがいい。もう一回負けるのは耐えられないでしょ",
            "maleVariant": "やめといたほうがいい。もう一回負けるのは耐えられないだろ"
          },
          {
            "stringId": 1863513754201767936,
            "femaleVariant": "これで終わりだと思うか？ 俺から逃げられるとは思うなよ！"
          },
          {
            "stringId": 1863515405885128704,
            "femaleVariant": "やっぱりな。俺に勝てたんだ、誰にだって勝てるさ"
          },
          {
            "stringId": 1863515426537881600,
            "femaleVariant": "はっ！ これぞボクシングだね！"
          },
          {
            "stringId": 1863556704881360896,
            "femaleVariant": "うんうん。うまくいって、レイザーは負けた… いくらって？ 数千エディーはいったね！"
          },
          {
            "stringId": 1863557863935328260,
            "femaleVariant": "演技？ 何の話してるの？",
            "maleVariant": "演技？ 何の話をしてる？"
          },
          {
            "stringId": 1863558786782871552,
            "femaleVariant": "ハハ！ まあ、演技がうまいから！"
          },
          {
            "stringId": 1863558986347855872,
            "femaleVariant": "ずる賢いガキね",
            "maleVariant": "ずる賢いガキだな"
          },
          {
            "stringId": 1863559653158305792,
            "femaleVariant": "ずる賢い真似をするじゃない",
            "maleVariant": "ずる賢い真似をするじゃないか"
          },
          {
            "stringId": 1863560186942210048,
            "femaleVariant": "待って、違うの、説明させて…"
          },
          {
            "stringId": 1863560791827955712,
            "femaleVariant": "ちょっと… 「演技」って言った？ お父さんの話は全部作り話だったの？",
            "maleVariant": "おい… 「演技」って言ったか？ 親父の話は全部作り話だったのかよ？"
          },
          {
            "stringId": 1863561385372303360,
            "femaleVariant": "え… ごめん、何の話かわからない。もう行かなきゃ！"
          },
          {
            "stringId": 1863561809097670656,
            "femaleVariant": "はあ… まんまと騙された"
          },
          {
            "stringId": 1863562501493374980,
            "femaleVariant": "元気出せ。まだ生きてるだけ運がいい"
          },
          {
            "stringId": 1889653395212484608,
            "femaleVariant": "<kiroshi l=\"creo\" o=\"Ale nan lòt bò gwo sal la. Yap tann nou.\" t=\"みんな、中で待ってるよ\" b=\"\" a=\"\"/>"
          },
          {
            "stringId": 1889654496754155524,
            "femaleVariant": "2人とも元気？",
            "maleVariant": "2人とも元気か？"
          },
          {
            "stringId": 1889655029028110336,
            "femaleVariant": "双子じゃない！ 俺は一人だ！"
          },
          {
            "stringId": 1889655629853769728,
            "femaleVariant": "目の錯覚かと思ったら、双子たちじゃない。元気にしてた？",
            "maleVariant": "目の錯覚かと思ったら、双子たちじゃないか。元気にしてたか？"
          },
          {
            "stringId": 1889656485642780672,
            "femaleVariant": "双子じゃない！ 俺は一人だ！"
          },
          {
            "stringId": 1889657948381134848,
            "femaleVariant": "おめでとう！ よくやった"
          },
          {
            "stringId": 1889658579741327360,
            "femaleVariant": "おめでとう、やったな！"
          },
          {
            "stringId": 1889662309232558080,
            "femaleVariant": "約束と話が違うだろ、やってくれたな"
          },
          {
            "stringId": 1889734459616411652,
            "femaleVariant": "ここで何してるの、ヴィクター？",
            "maleVariant": "ここで何してるんだ、ヴィクター？"
          },
          {
            "stringId": 1889734706409259008,
            "femaleVariant": "ヴィクター？ ここで何してるの？",
            "maleVariant": "ヴィクター？ ここで何してる？"
          },
          {
            "stringId": 1889735032440897536,
            "femaleVariant": "決まってるだろ？ 今年最高の試合を見に来たんだ。みんな大盛り上がりしてるぞ"
          },
          {
            "stringId": 1889735393637580800,
            "femaleVariant": "いいか？ がっかりさせるなよ"
          },
          {
            "stringId": 1889736281823395840,
            "femaleVariant": "お前さん、わざと負けたな？"
          },
          {
            "stringId": 1889736713182396416,
            "femaleVariant": "まったく… 見損なったぞ、V"
          },
          {
            "stringId": 1889737425090002944,
            "femaleVariant": "よくやったぞ、V"
          },
          {
            "stringId": 1912393276410089472,
            "femaleVariant": "シーザーじゃない。揺りかごやらオムツの準備は平気なの？",
            "maleVariant": "よう、シーザー。揺りかごやらオムツの準備は平気なのか？"
          },
          {
            "stringId": 1912394355470290944,
            "femaleVariant": "予定日は明日だ。時間はある"
          },
          {
            "stringId": 1912396158551248896,
            "femaleVariant": "次は私が倒してやる番だからね。見てな"
          },
          {
            "stringId": 1912397787316932608,
            "femaleVariant": "まあ… あれからも、変わりなくやってるよ"
          },
          {
            "stringId": 1912500233460568064,
            "femaleVariant": "別に。それだけよ。私たち家族はもうおしまいってだけ…"
          },
          {
            "stringId": 1912500640173838336,
            "femaleVariant": "ただ… レイザーに罰を受けさせてほしいの。お父さんみたいな目に遭わせてほしい"
          },
          {
            "stringId": 1912502688487362560,
            "femaleVariant": "ありがとう… 幸運を祈ってる"
          },
          {
            "stringId": 1912763291701399552,
            "femaleVariant": "想像はつく。だからやめとく。勝つために戦うだけよ",
            "maleVariant": "想像はつく。だからやめとく。勝つために戦うだけだ"
          },
          {
            "stringId": 1912763783223496704,
            "femaleVariant": "わかった、好きにしろ"
          },
          {
            "stringId": 1912765601957597184,
            "femaleVariant": "大げさね。いいから何なの？",
            "maleVariant": "大げさだな。いいから何だよ？"
          },
          {
            "stringId": 1912769915446939648,
            "femaleVariant": "やるわけないでしょ",
            "maleVariant": "やるわけないだろ"
          },
          {
            "stringId": 1912770879314776064,
            "femaleVariant": "はっ！ 思ってたとおり、頭がさえるな"
          },
          {
            "stringId": 1912771968994955264,
            "femaleVariant": "残念だが、仕方ない。負けてもやられ損だぞ"
          },
          {
            "stringId": 1912775037715533824,
            "femaleVariant": "1ラウンドだけで、お前は俺の足にキスして、助けを乞うことになる"
          },
          {
            "stringId": 1912776335869399040,
            "femaleVariant": "とにかく… 始めようか？"
          },
          {
            "stringId": 1912789647986315264,
            "femaleVariant": "いいや、負けは認めねえ。明日でも、来年でもいい、絶対に再戦だ。いいな？"
          },
          {
            "stringId": 1912791709973270528,
            "femaleVariant": "じゃあそれまで、しっかりトレーニングするのね",
            "maleVariant": "じゃあそれまで、しっかりトレーニングするんだな"
          },
          {
            "stringId": 1912796014335807488,
            "femaleVariant": "どうでしょうね。ずっとこんな話を聞かされるんだったら、お断り",
            "maleVariant": "どうだろうな。ずっとこんな話を聞かされるんだったら、お断りだ"
          },
          {
            "stringId": 1912806905181687812,
            "femaleVariant": "どうも"
          },
          {
            "stringId": 1912807111205900288,
            "femaleVariant": "いらない"
          },
          {
            "stringId": 1912807312918368256,
            "femaleVariant": "そんな汚い金はいらない"
          },
          {
            "stringId": 1912807877572349952,
            "femaleVariant": "え？ でも… どうしたんだよ？"
          },
          {
            "stringId": 1912809813847625728,
            "femaleVariant": "あまり気にするな。結局のところ、大切なのは金だ。それならたっぷりあるだろ"
          },
          {
            "stringId": 1912813232742256640,
            "femaleVariant": "別にいいの。好きな方法で稼げばいい",
            "maleVariant": "別にいいんだ。好きな方法で稼げばいい"
          },
          {
            "stringId": 1941755192877793280,
            "femaleVariant": "サインか？ 試合の後にしてくれ"
          },
          {
            "stringId": 1941755759645704192,
            "femaleVariant": "なに、あたしのサインが欲しいってこと？",
            "maleVariant": "なんだ、俺のサインが欲しいってことか？"
          },
          {
            "stringId": 1941757002250207232,
            "femaleVariant": "おいおい、待てよ。お前がVか？ フン"
          },
          {
            "stringId": 1941757790477369348,
            "femaleVariant": "どうしてアマの試合を？"
          },
          {
            "stringId": 1941763214819737600,
            "femaleVariant": "そう簡単にはいかない"
          },
          {
            "stringId": 1941763446982852608,
            "femaleVariant": "そう簡単にいくといいけど",
            "maleVariant": "そう簡単にいくといいがな"
          },
          {
            "stringId": 1941763822507278336,
            "femaleVariant": "ヘビー級のチャンピオンが、素人向けの試合で何してるの？ カネに困ってるとか？",
            "maleVariant": "ヘビー級のチャンピオンが、素人向けの試合で何してる？ カネに困ってるのか？"
          },
          {
            "stringId": 1941764713075458048,
            "femaleVariant": "その逆だ。カネならありすぎて困ってる。なんせ使い道がないからな"
          },
          {
            "stringId": 1941765018672447488,
            "femaleVariant": "いいか、俺はこの拳一つで這い上がってきたんだ。だが頂点を極めちまった"
          },
          {
            "stringId": 1941766335449022464,
            "femaleVariant": "ファンが求めてんだ。ストリート魂を捨ててない俺の戦いざまをな。お前にゃ気の毒だが、せいぜい盛り上げてくれ"
          },
          {
            "stringId": 1941768652801982464,
            "femaleVariant": "VIPエリアの贅沢な椅子にふんぞり返って、何を言うかと思いきや“ストリート魂”ね？"
          },
          {
            "stringId": 1941770452745601024,
            "femaleVariant": "どんなサイバーウェアを付けてるの？",
            "maleVariant": "どんなサイバーウェアを付けてるんだ？"
          },
          {
            "stringId": 1941770777166626816,
            "femaleVariant": "クロームはどこ製？ ハウスシュトック？ アイコール？",
            "maleVariant": "クロームはどこ製だ？ ハウスシュトック？ アイコール？"
          },
          {
            "stringId": 1941771828007550976,
            "femaleVariant": "ふざけてんのか？ そんな一般向けのを使うわけねえだろ？"
          },
          {
            "stringId": 1941797730720432128,
            "femaleVariant": "おい、V。試合前の気分はどうだ？"
          },
          {
            "stringId": 1941800507718778880,
            "femaleVariant": "だが正直言って… 勝てる見込みは少ない。でもまあ、そのほうがいいのかもしれん"
          },
          {
            "stringId": 1942941120354861056,
            "femaleVariant": "レイザー… 実際に“カミソリ”を使うの？",
            "maleVariant": "レイザー… 実際に“カミソリ”を使うのか？"
          },
          {
            "stringId": 1942941499738046464,
            "femaleVariant": "ポスターは見たことある"
          },
          {
            "stringId": 1942941610283122688,
            "femaleVariant": "もう話をした"
          },
          {
            "stringId": 1942943386939965440,
            "femaleVariant": "ないね。シェイビングジェルがスポンサー？",
            "maleVariant": "ないな。シェイビングジェルがスポンサーか？"
          },
          {
            "stringId": 1942943754897866752,
            "femaleVariant": "ポスターを見たことある。プロボクサーでしょ？",
            "maleVariant": "ポスターを見たな。プロボクサーだろ？"
          },
          {
            "stringId": 1942944285208887296,
            "femaleVariant": "筋肉と自意識の塊みたいな男ね",
            "maleVariant": "筋肉と自意識の塊みたいな男だろ"
          },
          {
            "stringId": 1942945520146202624,
            "femaleVariant": "あいつは一流企業のスポンサーがついてるスターだから、こんなところに来るとは思わなかったが…"
          },
          {
            "stringId": 1942946100688207872,
            "femaleVariant": "事務所の話だと、いいPRになるんだそうだ。レイザーはまだストリートのルーツを忘れてないってことを、ファンに見せつけたいわけだな"
          },
          {
            "stringId": 1942949205328805888,
            "femaleVariant": "勝ち目なんかあるの？",
            "maleVariant": "勝ち目はあるのか？"
          },
          {
            "stringId": 1942949657021792256,
            "femaleVariant": "いい質問だ。V、お前も相当なやり手だが、あいつは… あいつはマシンさ"
          },
          {
            "stringId": 1942950222011318272,
            "femaleVariant": "炭素繊維、チタン製骨格、水圧式関節… どれも一流生物工学者による調整を受けてる"
          },
          {
            "stringId": 1942954096843571200,
            "femaleVariant": "どうせレイザーが勝つってんなら… エディーを稼ぐ方法を考えないか？"
          },
          {
            "stringId": 1942956781919551488,
            "femaleVariant": "レイザーのマネージャーは、とにかくあいつをリングで暴れ回させて、その活躍をファンに見せつけたがってる"
          },
          {
            "stringId": 1942961788828008448,
            "femaleVariant": "つまり… パンチを控えて、たまにはガードをし損なえ… 後は大の字でおネンネすりゃいい"
          },
          {
            "stringId": 1942962041056673792,
            "femaleVariant": "いくら？",
            "maleVariant": "いくらだ？"
          },
          {
            "stringId": 1942962787189800960,
            "femaleVariant": "たっぷりだ。勝つより稼げる"
          },
          {
            "stringId": 1942986757435834368,
            "femaleVariant": "腹だ！ 腹を狙え！"
          },
          {
            "stringId": 1942989068598001664,
            "femaleVariant": "ガードを崩すな！ でなきゃやられるぞ！"
          },
          {
            "stringId": 1942991140584177664,
            "femaleVariant": "いいぞ！ プレッシャーを与え続けろ！"
          },
          {
            "stringId": 1942992952590614528,
            "femaleVariant": "相手はふらついてる！ あと数発決めろ！"
          },
          {
            "stringId": 1943002296694620160,
            "femaleVariant": "そりゃ、いいコーチがいたからね",
            "maleVariant": "そりゃ、いいコーチがいたからな"
          },
          {
            "stringId": 1943024215221354496,
            "femaleVariant": "うるせえな、俺の負けだよ！"
          },
          {
            "stringId": 1943024831750488064,
            "femaleVariant": "「やめる」ってどういうことだ？ 全部打ち切るってのか？"
          },
          {
            "stringId": 1943025200413032448,
            "femaleVariant": "おいおい、ふざけんな！ お前は俺のマネージャーだろうが！ ちゃんと仕事しろ！"
          },
          {
            "stringId": 1943198304472940548,
            "femaleVariant": "[座る]"
          },
          {
            "stringId": 1943206317942390784,
            "femaleVariant": "ああ。いいぜ"
          },
          {
            "stringId": 1943252078772408320,
            "femaleVariant": "試合後にお前がサインしなきゃならんのは、入院同意書だろ。指が動けばの話だがな"
          },
          {
            "stringId": 1943271104772886528,
            "femaleVariant": "笑わせるな。あごに一発あてりゃ、すぐにリングに丸まっておネンネだろ"
          },
          {
            "stringId": 1943272702450388992,
            "femaleVariant": "底辺の出世話にしちゃ説得力に欠ける"
          },
          {
            "stringId": 1943283561151123456,
            "femaleVariant": "俺は市場に出回ってないプロトタイプしか使わない。いわばコーポの実験場だな… そしてお前は今日のモルモットってわけだ"
          },
          {
            "stringId": 1943288746116173824,
            "femaleVariant": "俺の生涯成績は47戦無敗なんだ。そのうち20回は、KO勝ちだぜ"
          },
          {
            "stringId": 1943289558905180160,
            "femaleVariant": "もっとも、お前で21回になりそうだがな"
          },
          {
            "stringId": 1943296402918789120,
            "femaleVariant": "最高のボクサーだ！ そんなことも知らないのか？"
          },
          {
            "stringId": 1943298372815605760,
            "femaleVariant": "何言ってんだ。あいつはヘビー級を3回も制してる王者だぞ！"
          },
          {
            "stringId": 1943300071089295360,
            "femaleVariant": "当然じゃないか！ ヘビー級を3回も制してるんだぜ！"
          },
          {
            "stringId": 1944323090507157504,
            "femaleVariant": "ヴィクターなら戦い方のコツを教えてくれるかもな…"
          },
          {
            "stringId": 1944323592984776704,
            "femaleVariant": "ヴィクターがいるの？",
            "maleVariant": "ヴィクターがいるのか？"
          },
          {
            "stringId": 1944326315054518272,
            "femaleVariant": "こんな大イベントを見逃すわけないだろ？"
          },
          {
            "stringId": 1944347055082491908,
            "femaleVariant": "ひどい"
          },
          {
            "stringId": 1944347663743111168,
            "femaleVariant": "準備万端"
          },
          {
            "stringId": 1944348326476697600,
            "femaleVariant": "正直、ひどい気分",
            "maleVariant": "正直、ひどい気分さ"
          },
          {
            "stringId": 1944348540067434496,
            "femaleVariant": "悪くない。武者震いしてる"
          },
          {
            "stringId": 1944348743625396224,
            "femaleVariant": "どういう意味？",
            "maleVariant": "どういう意味だ？"
          },
          {
            "stringId": 1944349223051120640,
            "femaleVariant": "確かにタフな相手だからな。でも、無敵な奴はいない。あいつだってそうだ"
          },
          {
            "stringId": 1944350259094540288,
            "femaleVariant": "はっ！ ボクシングってのは度胸が大事だ"
          },
          {
            "stringId": 1944355419296419840,
            "femaleVariant": "でもまあ、気持ちだけじゃ、レイザーみたいな相手に勝つのは難しい"
          },
          {
            "stringId": 1944366091166076928,
            "femaleVariant": "あいつはまさに戦車だ。ナノ繊維のリアルスキンに衝撃吸収材… ハンマーで頭をぶん殴ったとしても、微動だにしないだろう"
          },
          {
            "stringId": 1944369429882392576,
            "femaleVariant": "テンション上がってきた"
          },
          {
            "stringId": 1944370261008896000,
            "femaleVariant": "待てよ、まだ終わってない。奴には弱点がある。1週間前に腹筋を交換したんだが、まだ本調子じゃないんだ"
          },
          {
            "stringId": 1944372530714562560,
            "femaleVariant": "そこを狙えば、きっとダメージが入る"
          },
          {
            "stringId": 1944374131797839872,
            "femaleVariant": "ありがとう、ヴィク。覚えておく"
          },
          {
            "stringId": 2014421132593086464,
            "femaleVariant": "[座って待つ]"
          },
          {
