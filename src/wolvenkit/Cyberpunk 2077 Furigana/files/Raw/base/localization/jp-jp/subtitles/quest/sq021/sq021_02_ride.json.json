    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1338559622542442496,
            "femaleVariant": "ランディのことを教えてくれる？ どんな子なの？",
            "maleVariant": "ランディのことを教えてくれるか？ どんな子だ？"
          },
          {
            "stringId": 1396774671519178752,
            "femaleVariant": "手癖が悪く、しょっちゅう家出――そういう子だ"
          },
          {
            "stringId": 1401040175171756032,
            "femaleVariant": "何だ、言ってみろ…"
          },
          {
            "stringId": 1401042845500563456,
            "femaleVariant": "ランディについて詳しく教えて",
            "maleVariant": "ランディについて詳しく教えてくれ"
          },
          {
            "stringId": 1401045522640920576,
            "femaleVariant": "ランディの父親はどうしてるの？",
            "maleVariant": "ランディの父親はどうしてる？"
          },
          {
            "stringId": 1401046087731109888,
            "femaleVariant": "ランディのお父さんは？",
            "maleVariant": "ランディの父親は？"
          },
          {
            "stringId": 1401053490375680000,
            "femaleVariant": "伯父が警官なだけあるね",
            "maleVariant": "伯父が警官なだけあるな"
          },
          {
            "stringId": 1401053747838836736,
            "femaleVariant": "どうやっても人生がうまくいかない人間はいる。ランディみたいにな"
          },
          {
            "stringId": 1401057441661706240,
            "femaleVariant": "何年か前にギャングの抗争で撃たれた"
          },
          {
            "stringId": 1537312397961904128,
            "femaleVariant": "その、なんだ… 俺自身もまだ飲み込めてない話は、しにくいんだよ"
          },
          {
            "stringId": 1780929152491163648,
            "femaleVariant": "ヤウェンが規則を守って、上司に報告するとは思わないの？",
            "maleVariant": "ヤウェンが規則を守って、上司に報告するとは思わないのか？"
          },
          {
            "stringId": 1780930867542069248,
            "femaleVariant": "あれだけ口論した上でも手を貸してくれると？"
          },
          {
            "stringId": 1780931966516826112,
            "femaleVariant": "そうね。ランディを見つけて、打ち上げにパーっと飲みに行きましょ",
            "maleVariant": "そうだな。ランディを見つけて、打ち上げにパーっと飲みに行こうか"
          },
          {
            "stringId": 1780932008661192704,
            "femaleVariant": "今はランディ探しに集中しないと"
          },
          {
            "stringId": 1780932115615944708,
            "femaleVariant": "ランディ探しに集中しましょう",
            "maleVariant": "ランディ探しに集中しよう"
          },
          {
            "stringId": 1780932497180168192,
            "femaleVariant": "酒か、いいね"
          },
          {
            "stringId": 1782223670917337088,
            "femaleVariant": "ハリスの家を探すべきじゃない？",
            "maleVariant": "ハリスの家を探すべきじゃないか？"
          },
          {
            "stringId": 1782262268496580608,
            "femaleVariant": "立派なお手本だこと",
            "maleVariant": "立派なお手本だな"
          },
          {
            "stringId": 1782383506447536132,
            "femaleVariant": "先にハリスのアパートを探せばいいんじゃない？",
            "maleVariant": "先にハリスのアパートを探せばいいんじゃないか？"
          },
          {
            "stringId": 1782383506447536144,
            "femaleVariant": "ランディの部屋でハリスとの接点が見つかれば、次は奴の家を調べるさ"
          },
          {
            "stringId": 1782383506447536156,
            "femaleVariant": "望み薄ね",
            "maleVariant": "望み薄だな"
          },
          {
            "stringId": 1782383506464313344,
            "femaleVariant": "今はこれにかけるしかない"
          },
          {
            "stringId": 1782394969077714944,
            "femaleVariant": "警察から追い出されたのね",
            "maleVariant": "警察から追い出されたのか"
          },
          {
            "stringId": 1782400463632732160,
            "femaleVariant": "クビになったなんて聞いてない"
          },
          {
            "stringId": 1782487452558168064,
            "femaleVariant": "クビになったなんて聞いてないけど――なんで黙ってたの？",
            "maleVariant": "クビになったなんて聞いてないが――なんで黙ってたんだよ？"
          },
          {
            "stringId": 1782487534045106180,
            "femaleVariant": "警察から追い出されたのね",
            "maleVariant": "警察から追い出されたのか"
          },
          {
            "stringId": 1782489968704360448,
            "femaleVariant": "停職だよ。市長の死はどうも怪しかった。検死についてもうやむやにされてな… だから文句を言ったんだ"
          },
          {
            "stringId": 1782490296598269952,
            "femaleVariant": "当然納得してないわけね",
            "maleVariant": "当然納得してないわけだな"
          },
          {
            "stringId": 1782491664813789184,
            "femaleVariant": "ああ"
          },
          {
            "stringId": 1782493206992261120,
            "femaleVariant": "さあな"
          },
          {
            "stringId": 1782493312353177600,
            "femaleVariant": "子供みたいな返しをするのね",
            "maleVariant": "子供みたいな返しをするんだな"
          },
          {
            "stringId": 1783402570485731328,
            "femaleVariant": "腐っても友人だ、それはないだろう"
          },
          {
            "stringId": 1783446037299597312,
            "femaleVariant": "ああ。俺たちの喧嘩はいつもそうやって終わる"
          },
          {
            "stringId": 1783447658247098368,
            "femaleVariant": "酒のつまみ向けの話さ"
          },
          {
            "stringId": 1783452580413837312,
            "femaleVariant": "そうだな"
          },
          {
            "stringId": 1783458130853761024,
            "femaleVariant": "楽しみにしとく"
          },
          {
            "stringId": 1815737714720186368,
            "femaleVariant": "ジョスは子供を3人抱えて残されたが、へこたれずに頑張ってる"
          },
          {
            "stringId": 1841609618746380288,
            "femaleVariant": "ヤウェンは協力すると思う？",
            "maleVariant": "ヤウェンは協力すると思うか？"
          },
          {
            "stringId": 1841609835138912256,
            "femaleVariant": "ヤウェンに通報されるんじゃ？"
          },
          {
            "stringId": 1908178454259982336,
            "femaleVariant": "一軒家らしい。地下室に被害者を閉じ込めちゃいないだろう"
          },
          {
            "stringId": 1908216662825918464,
            "femaleVariant": "子供で結構"
          },
          {
            "stringId": 1908263265267900416,
            "femaleVariant": "それに、この手を使う気はないが… あいつが世間に知られたくないような秘密を知ってる"
          },
          {
            "stringId": 1912776659653758976,
            "femaleVariant": "よし、着いたぞ"
          },
          {
            "stringId": 1925708937564393472,
            "femaleVariant": "まあいいけど… せめて行き先くらいは教えてよ",
            "maleVariant": "まあいいが… せめて行き先くらいは教えてくれ"
          },
          {
            "stringId": 1925710024610549760,
            "femaleVariant": "ジョスの家"
          },
          {
            "stringId": 1925710242194264064,
            "femaleVariant": "俺の妹だ"
          },
          {
            "stringId": 1925710454023393280,
            "femaleVariant": "ジョスはただの家出だと思ってる。今はまだ、誘拐のことは伏せておきたい"
          }
        ]
      }
    }
  }
}