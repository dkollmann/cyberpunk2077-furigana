  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おいおい、V！ ちょっと待て！",
                    "maleVariant": null,
                    "stringId": 1854602074248847360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こいつのギター、お前はどう思う？",
                    "maleVariant": null,
                    "stringId": 1854606265348399104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もっと動きをつけたほうがいいな。指使いはメチャクチャだし、テンポもひどいもんだが、光るもんはある",
                    "maleVariant": null,
                    "stringId": 1854606776382398464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人を褒めるのがホント得意ね",
                    "maleVariant": "人を褒めるのがほんと得意だな",
                    "stringId": 1854606870284476416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたもこうやって、路上ライブから登りつめていったの？",
                    "maleVariant": "お前もこうやって、路上ライブから登りつめていったのか？",
                    "stringId": 1854670940425560064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "笑わせるな。別にトップを目指してたわけじゃない。世界の目を醒まさせるので忙しかったからな",
                    "maleVariant": null,
                    "stringId": 1854671024479412224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だが客は最初からいた。俺たちのことを知らずに録音してた奴もいたもんだ",
                    "maleVariant": null,
                    "stringId": 1854671132239470592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "冗談はともかく、そんなテープ、今でもまだ残ってると思う？",
                    "maleVariant": "冗談はともかく、そんなテープ、今でもまだ残ってると思うか？",
                    "stringId": 1854671279291768832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "んー… この辺にレインボー・カデンザがある。解散前によく演ってたクラブだ",
                    "maleVariant": null,
                    "stringId": 1854671662651154432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "下手。素人ね",
                    "maleVariant": "下手。素人だな",
                    "stringId": 1882034833207312388
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "悪くないかな",
                    "maleVariant": "悪くないんじゃないか",
                    "stringId": 1882034888236580864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ひどいね。持ち方さえ変だし。プロじゃないあたしでも、素人なのがわかる",
                    "maleVariant": "ひどいな。持ち方さえ変だし。プロじゃない俺でも、素人なのがわかる",
                    "stringId": 1882035145850732544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしは音楽には疎いけど、聴いてて悪い感じはしないかな。耳ざわりがいいって言うか",
                    "maleVariant": "俺は音楽には疎いが、聴いてて悪い感じはしないな。耳ざわりがいいって言うか…",
                    "stringId": 1882035192944377856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これはこれは、手厳しいな。よく聞けよ。そこまでひどかないだろ",
                    "maleVariant": null,
                    "stringId": 1882061255711899648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしてかわかるか？ 気持ちが入ってるからさ。技術は二の次だ",
                    "maleVariant": null,
                    "stringId": 1882061771544182784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "トップに立つのも悪かないでしょ。注目を集めることができれば、世界を動かすのも楽になるじゃない",
                    "maleVariant": "トップに立つのも悪かないだろ。注目を集めることができりゃ、世界を動かすのも楽になるんじゃないか",
                    "stringId": 1882109472625377280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はいはい。少なくともいくつかライブをやったのね。その記憶なら見たよ",
                    "maleVariant": "はいはい。少なくともいくつかライブをやったんだろ。その記憶なら見た",
                    "stringId": 1882109515289837568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ。場所には拘らなかった。公園、工場、アングラのクラブ…",
                    "maleVariant": null,
                    "stringId": 1882109700426416128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "成功すればそのための舞台が得られた",
                    "maleVariant": null,
                    "stringId": 1882113064543436804
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "負け犬が好きそうな話ね",
                    "maleVariant": "負け犬が好きそうな話だ",
                    "stringId": 1882113118817730560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふーん。それって、いかにも“アーティスト”くずれが言いそうなことね",
                    "maleVariant": "ふーん。それって、いかにも“アーティスト”くずれが言いそうなことだな",
                    "stringId": 1882113639616069632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "昔のテープは高く売れるかも",
                    "maleVariant": null,
                    "stringId": 1882123356946022404
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "テープを探してみる。ファンになれるかも",
                    "maleVariant": null,
                    "stringId": 1882123410784108544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "〈Samurai〉設立当初のライブ音源がどこかに眠ってるかもしれないってこと？ 金になりそうね",
                    "maleVariant": "〈Samurai〉設立当初のライブ音源がどこかに眠ってるかもしれないってことか？ 金になりそうだな",
                    "stringId": 1882123525489934336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "つまり、〈Samurai〉設立当初のライブ音源が手に入るかもしれないってこと？",
                    "maleVariant": "つまり、〈Samurai〉設立当初のライブ音源が手に入るかもしれないってことか？",
                    "stringId": 1882123569295245312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前がそんなにガメつい奴だったとはな。残念で、涙が出そうだよ",
                    "maleVariant": null,
                    "stringId": 1882136053473329152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なる気ねーだろ",
                    "maleVariant": null,
                    "stringId": 1882136404301692928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この男は心に傷を負ってる。音を聞きゃわかるんだ",
                    "maleVariant": null,
                    "stringId": 1898350552656101376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まやかしだ。レコード会社のために書き上げた歌詞が、共感を得られると思うか？",
                    "maleVariant": null,
                    "stringId": 1898370605640302592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "理想ってのは、一度捨てたら大枚はたいても買い戻せねえんだよ",
                    "maleVariant": null,
                    "stringId": 1898370709927477248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "確かに俺は失敗した。クソコーポどもは、50年前と同じように今もこの街を仕切ってる",
                    "maleVariant": null,
                    "stringId": 1902294355037372416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だがな、俺がアイドルか何かになりたがってると思ったら大間違いだ。あんな商売目当てのクズに興味ねえよ！",
                    "maleVariant": null,
                    "stringId": 1902296918864412672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ならそのテープを探してみる。ひょっとしたらあんたの大ファンになって、キャーキャー騒ぐようになるかもよ？",
                    "maleVariant": "ならそのテープを探してみる。ひょっとしたらお前の大ファンになって、サインをねだるようになるかもな？",
                    "stringId": 1902364303562620928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "音楽がわかる連中が集まるいい場所さ。あそこでなら何かわかるかもな",
                    "maleVariant": null,
                    "stringId": 1902393350761988096
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}