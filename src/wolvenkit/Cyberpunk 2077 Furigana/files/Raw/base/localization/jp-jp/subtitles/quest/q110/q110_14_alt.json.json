        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あるに決まってんだろ",
              "maleVariant": "",
              "stringId": "1567712226695254016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソウルキラーを作ったのはお前だ。それをアラサカが手にしたおかげで、俺とお前や、街のランナーの半分が犠牲になったんだぞ！",
              "maleVariant": "",
              "stringId": "1567848151756640256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前が死んだのは自業自得だ",
              "maleVariant": "",
              "stringId": "1567848513993510912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オルト、俺はお前を助けに行ったんだ。それでもまだ関係ないって言うのか？",
              "maleVariant": "",
              "stringId": "1567848840025149440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだ。お前がアラサカ・タワーから救い出そうとしたオルト・カニンガムはもう存在しない",
              "maleVariant": "",
              "stringId": "1567865230123667456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女を死なせたお前なら、それくらいのことは理解しているだろう",
              "maleVariant": "",
              "stringId": "1614242359262121984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴードゥー・ボーイズはどうすればいい？",
              "maleVariant": "",
              "stringId": "1695389307310723072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのまま通してくれる。そういう約束だった",
              "maleVariant": "",
              "stringId": "1695391416089333760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オルト、行く前に話せないか？ 二人だけで",
              "maleVariant": "",
              "stringId": "1699572875838640128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリジットとヴードゥー・ボーイズは？",
              "maleVariant": "",
              "stringId": "1699829769710145536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼らのBBSが侵入に利用されたため、やむなく排除した",
              "maleVariant": "",
              "stringId": "1699830939971293184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「排除」？ 全員、死んだってこと？",
              "maleVariant": "「排除」？ 全員、死んだってことか？",
              "stringId": "1699831363277230080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリジットは？ まだ生きてるの？",
              "maleVariant": "ブリジットは？ まだ生きてるのか？",
              "stringId": "1699851405641453568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。だが今ここには必要ない",
              "maleVariant": "",
              "stringId": "1699851405641453580"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何が望みだ？",
              "maleVariant": "",
              "stringId": "1699851405641453592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリジットはジョニーと引き換えに何を求めてたの？",
              "maleVariant": "ブリジットはジョニーと引き換えに何を求めてたんだ？",
              "stringId": "1699852544579538944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして向こう側へ行こうと？",
              "maleVariant": "",
              "stringId": "1699852544596316176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業に支配された今のネットは、ブラックウォール誕生以前と同じくらい不安定だといえる",
              "maleVariant": "",
              "stringId": "1699852544596316188"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴードゥー・ボーイズは… 時が来たら、こちら側で戦えるようにしておこうと目論んでいた",
              "maleVariant": "",
              "stringId": "1699852544596316200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまり自立AIたちと？ そんなことできるの？",
              "maleVariant": "つまり自立AIたちと？ そんなことできるのか？",
              "stringId": "1699852544613093380"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女と仲間たちをブラックウォールの先にかくまうことだ",
              "maleVariant": "",
              "stringId": "1699855083895066624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "適切な準備さえあれば可能だ",
              "maleVariant": "",
              "stringId": "1699855759698104320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダメだ。それはできない",
              "maleVariant": "",
              "stringId": "1699957172851773440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリジットはジョニーと引き換えに何を求めてたの？",
              "maleVariant": "ブリジットはジョニーと引き換えに何を求めてたんだ？",
              "stringId": "1730061768471154688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女と仲間たちをブラックウォールの先にかくまうことだ",
              "maleVariant": "",
              "stringId": "1730061768471154700"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何のためにそんなことを？",
              "maleVariant": "",
              "stringId": "1730061768487931908"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業に支配された今のネットは、ブラックウォール誕生以前と同じくらい不安定だといえる",
              "maleVariant": "",
              "stringId": "1730061768487931920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴードゥー・ボーイズは… 時が来たら、こちら側で戦えるようにしておこうと目論んでいた",
              "maleVariant": "",
              "stringId": "1730061768487931932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別に気の毒でもなんでもないだろ",
              "maleVariant": "",
              "stringId": "1730067878045356032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ネットウォッチのプロパガンダが効いているようだな。私が血の契約を求める悪魔だとでも？",
              "maleVariant": "",
              "stringId": "1953439749166092288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーに考えがあるの。あとはあんたの条件次第",
              "maleVariant": "ジョニーに考えがある。あとはあんたの条件次第だ",
              "stringId": "1953461074484023300"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈Relic〉のせいであたしは死にかけてる",
              "maleVariant": "〈Relic〉のせいで俺は死にかけてる",
              "stringId": "1953461530824298496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたについては何もわかってない",
              "maleVariant": "",
              "stringId": "1953471819368124416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはただ生き延びたいだけよ",
              "maleVariant": "俺はただ生き延びたいだけだ",
              "stringId": "1953472304313552896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昔の話は少し聞いてるけど、今のあんたは謎に包まれてる",
              "maleVariant": "昔の話は少し聞いてるが、今のあんたは謎に包まれてる",
              "stringId": "1953483168903090176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはただ生き延びたいだけ",
              "maleVariant": "俺はただ生き延びたいだけだ",
              "stringId": "1953484885178732544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しかもジョニーと違って、あたしはあんたにとって何の価値もない。だから条件を聞いてるの",
              "maleVariant": "しかもジョニーと違って、俺はあんたにとって何の価値もない。だから条件を聞いてるんだ",
              "stringId": "1953485898522578944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手を貸すことはできない",
              "maleVariant": "",
              "stringId": "1953522599907315712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫だ。俺に任せろ",
              "maleVariant": "",
              "stringId": "1954797616632713216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー",
              "maleVariant": "",
              "stringId": "1954797616716599296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブラックウォールを越えて俺に会いに来たってことは… お前は味方ってことでいいんだよな？",
              "maleVariant": "",
              "stringId": "1954797616817262592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前の記憶痕跡コードは認識できたが、ここにいる理由がわからない",
              "maleVariant": "",
              "stringId": "1954797616917925888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、こいつはオルト――ナイトシティ一のネットランナーだ",
              "maleVariant": "",
              "stringId": "1954797617236692992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしがネットウォッチを連れてきたのね",
              "maleVariant": "俺がネットウォッチを連れてきたのか",
              "stringId": "1954797617488351232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴードゥー・ボーイズはどうなったの？",
              "maleVariant": "ヴードゥー・ボーイズはどうなったんだ？",
              "stringId": "1954797617505128448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリジットの狙いは何だったの？",
              "maleVariant": "ブリジットの狙いは何だったんだ？",
              "stringId": "1954797617505128464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ、あたしのせいね。取引したあのエージェントにトラッカーを仕込まれたのよ",
              "maleVariant": "クソ、俺のせいだな。取引したあのエージェントにトラッカーを仕込まれたんだ",
              "stringId": "1954797617639346180"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "迷惑をかけたようね…",
              "maleVariant": "迷惑をかけたようだな…",
              "stringId": "1954797617740009472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "関係ない。サブネット全体が破壊されたからな",
              "maleVariant": "",
              "stringId": "1954797617823895552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オルト…",
              "maleVariant": "",
              "stringId": "1954797617907781632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わざわざ助けてくれたってことは… お前は味方ってことでいいんだよな？",
              "maleVariant": "",
              "stringId": "1954797618008444928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前の記憶痕跡コードは認識できたが、ここにいる理由がわからない",
              "maleVariant": "",
              "stringId": "1954797618075553792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何をすれば、あたしの命を救ってくれる？",
              "maleVariant": "何をすれば、俺の命を救ってくれる？",
              "stringId": "1954797619082186752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴードゥー・ボーイズはコンストラクトと引き換えに何を求めてたの？",
              "maleVariant": "ヴードゥー・ボーイズはコンストラクトと引き換えに何を求めてたんだ？",
              "stringId": "1954797619082186768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリジットをどうしたの？",
              "maleVariant": "ブリジットをどうしたんだ？",
              "stringId": "1954797619098963972"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここならネットウォッチも手が出せない",
              "maleVariant": "",
              "stringId": "1954797620508250112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今回は向こうも備えがあった",
              "maleVariant": "",
              "stringId": "1954797620692799488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカ・タワーで助けてやった貸しを返してもらおうと思ってな",
              "maleVariant": "",
              "stringId": "1954801787364724736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オルト、こいつの命を救ってやってくれ",
              "maleVariant": "",
              "stringId": "1954814379856515072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカ・タワーでの貸しを返してもらおうと思ってな",
              "maleVariant": "",
              "stringId": "1954817335314108416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、こいつはオルト――ナイトシティ一のネットランナーだ",
              "maleVariant": "",
              "stringId": "1954817420508811264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オルト、こいつの命を救ってやってくれ",
              "maleVariant": "",
              "stringId": "1954819219311579136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈Relic〉のせいであたしは死にかけてる",
              "maleVariant": "〈Relic〉のせいで俺は死にかけてる",
              "stringId": "1954822922848456704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちの要求は、ジョニーの言ったとおり",
              "maleVariant": "",
              "stringId": "1954828137307852800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あとは… あんたの条件次第よ",
              "maleVariant": "あとは… あんたの条件次第だ",
              "stringId": "1954830483534401536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何をすれば… あたしの命を救ってくれる？",
              "maleVariant": "何をすれば… 俺の命を救ってくれるんだ？",
              "stringId": "1954835782030872576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは〈Relic〉に殺されかけてる。コンストラクトが神経系を侵食してるの",
              "maleVariant": "俺は〈Relic〉に殺されかけてる。コンストラクトが神経系を侵食してるんだ",
              "stringId": "1954839529054294016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのようだな。だが私には関係がない",
              "maleVariant": "",
              "stringId": "1954847340878385152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーが気に食わないのはわかった。で、あたしの方は？",
              "maleVariant": "ジョニーが気に食わないのはわかった。で、俺の方は？",
              "stringId": "1954855277508186116"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手は貸せない",
              "maleVariant": "",
              "stringId": "1954855278380601344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーのせいじゃない",
              "maleVariant": "",
              "stringId": "1954855281115287552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーが気に食わないのはわかった。でもあたしは？",
              "maleVariant": "ジョニーが気に食わないのはわかった。だが俺のことは？",
              "stringId": "1954855281115287572"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あれはジョニーのせいじゃない",
              "maleVariant": "",
              "stringId": "1954855282021257220"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前に何がわかる？",
              "maleVariant": "",
              "stringId": "1954855282927226880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーの記憶を見た… あれは事故よ",
              "maleVariant": "ジョニーの記憶を見た… あれは事故だ",
              "stringId": "1954855283749310464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前が見たのはそいつの主観的記憶だ。潜在意識の奥にしまい込まれ、繰り返し再生され、次第にゆがんでいった記憶に過ぎない",
              "maleVariant": "",
              "stringId": "1954855284571394048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真実とは遠くかけ離れたものだ",
              "maleVariant": "",
              "stringId": "1954855285376700416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オルトじゃないなら、あんたは誰なの？",
              "maleVariant": "オルトじゃないなら、あんたは誰なんだ？",
              "stringId": "1954855287087976452"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女のデータを使っているだけだ",
              "maleVariant": "",
              "stringId": "1954855287977168896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ。その性格の悪さもデータにあったのかよ？",
              "maleVariant": "",
              "stringId": "1954855288799252480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "貸す気がないのか？",
              "maleVariant": "",
              "stringId": "1954863919770914816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんな話をしてる場合？",
              "maleVariant": "そんな話をしてる場合か？",
              "stringId": "1954869250546749440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ったく、こんなところまできて、まさかデジタル痴話げんかを見せられるとはね",
              "maleVariant": "ったく、こんなところまできて、まさかデジタル痴話げんかを見せられるとはな",
              "stringId": "1954869257425408000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まったくフォローになってないぞ",
              "maleVariant": "",
              "stringId": "1954869264505393152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "したつもりもないけど",
              "maleVariant": "したつもりもないが",
              "stringId": "1954869271316942848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "喧嘩は後にしてくれない？",
              "maleVariant": "喧嘩は後にしてくれないか？",
              "stringId": "1954869412195225600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソウルキラーを作った張本人でしょ？",
              "maleVariant": "ソウルキラーを作った張本人だろ？",
              "stringId": "1954875850820964356"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私の力では無理なのだ。それには、もっと高度なテクノロジーが必要になる",
              "maleVariant": "",
              "stringId": "1954877716145393664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリジットは可能だって言ってたけど",
              "maleVariant": "ブリジットは可能だって言ってたが",
              "stringId": "1954880173705547776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんでよ？ ソウルキラーはあんたが作ったモノでしょ",
              "maleVariant": "なんでだ？ ソウルキラーはあんたが作ったモノだろ",
              "stringId": "1954881962576203776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリジットはあたしを救えるって言ってたけど…？",
              "maleVariant": "ブリジットは俺を救えるって言ってたがな…",
              "stringId": "1954882340013232128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まんまと騙されたようだな",
              "maleVariant": "",
              "stringId": "1954886939403112448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ、やっぱりな",
              "maleVariant": "",
              "stringId": "1954887114020376576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカは自分たちの目的に合わせてプログラムを改変した。別物にな",
              "maleVariant": "",
              "stringId": "1954891346056335360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なら、〈神輿〉は使えないのか？ 十分高度なテクノロジーだろ？",
              "maleVariant": "",
              "stringId": "1954892883470077952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もし私が〈神輿〉にアクセスできていれば、すでに破壊している",
              "maleVariant": "",
              "stringId": "1954894758827282432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "助けてくれるにしても、具体的に何をするの？",
              "maleVariant": "助けてくれるにしても、具体的に何をするんだ？",
              "stringId": "1954897791392960516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、お前を〈神輿〉に連れていくから、Vを助けろ。それでどうだ？",
              "maleVariant": "",
              "stringId": "1954899034752438272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "街一番のネットランナーが、自力で〈神輿〉に侵入できないわけ？",
              "maleVariant": "街一番のネットランナーが、自力で〈神輿〉に侵入できないのか？",
              "stringId": "1954900887057747968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当に助かる方法はあるの？",
              "maleVariant": "本当に助かる方法はあるのか？",
              "stringId": "1954901708503797760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まず〈神輿〉内のソウルキラーを使ってお前のコンストラクトを作る。それからお前とジョニーの神経網を分離させる",
              "maleVariant": "",
              "stringId": "1954902534462918656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんなに手遅れだって言われてきたけど",
              "maleVariant": "みんなに手遅れだって言われてきたが",
              "stringId": "1954903097606950912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その「みんな」ってのは、サブロウの側近のヘルマンのことか？",
              "maleVariant": "",
              "stringId": "1954904187471679488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "街一番のネットランナーが、自力で〈神輿〉に入れないの？",
              "maleVariant": "街一番のネットランナーが、自力で〈神輿〉に入れないのか？",
              "stringId": "1954906108295139328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私は警戒されている。私を撃退する用のブラックICEまで用意されているほどだ",
              "maleVariant": "",
              "stringId": "1954906939891404800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またあそこに囚われるのはご免だからな",
              "maleVariant": "",
              "stringId": "1954908237659394048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "特別待遇とは、大物だな…",
              "maleVariant": "",
              "stringId": "1954910211448524800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまり… あたしを救う過程であたしを殺すってこと？",
              "maleVariant": "つまり… 俺を救う過程で俺を殺すってことか？",
              "stringId": "1954916956963098628"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前の意識、すなわち神経記憶痕跡はデータとして記録され、それ以外は消滅する",
              "maleVariant": "",
              "stringId": "1954916957114093568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それ以外って？",
              "maleVariant": "",
              "stringId": "1954916957231534080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "魂だ。私が名付けたわけではないが、“ソウルキラー”はその名の通りの働きをする",
              "maleVariant": "",
              "stringId": "1954916957332197376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おいおい、冗談キツいぜ",
              "maleVariant": "",
              "stringId": "1954916957466415104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vは自分の体に戻るだけで何も変わりゃあしないんだろ？",
              "maleVariant": "",
              "stringId": "1954916957650964480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何もかも変わる。お前はわかっているはずだ",
              "maleVariant": "",
              "stringId": "1954916957785182208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それしか方法がないのなら、やりましょ",
              "maleVariant": "それしか方法がないのなら、やろう",
              "stringId": "1954917137620160516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "救う前に一度殺すってわけ？",
              "maleVariant": "救う前に一度殺すってわけか？",
              "stringId": "1954917734587060224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それしか方法はないみたいね",
              "maleVariant": "それしか方法はないみたいだな",
              "stringId": "1954919692974706688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが、〈神輿〉は私が作り上げた軍隊をもってしても、突破できなかった",
              "maleVariant": "",
              "stringId": "1954920779131670528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そいつらは所詮ネットの産物――そこが問題だ",
              "maleVariant": "",
              "stringId": "1954921434231623680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちは人間の部分で勝負する",
              "maleVariant": "",
              "stringId": "1954921861933191168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vほどの人たらしは見たことねえ。傭兵やノーマッドにも頼れるダチがいる…",
              "maleVariant": "",
              "stringId": "1954926434127204352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そいつらと一緒に、入口を作ってやるよ",
              "maleVariant": "",
              "stringId": "1954926818241564672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつはアフターライフで気に入られてんだ。連中が協力してくれる",
              "maleVariant": "",
              "stringId": "1954932191832854528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんたって、Vにはノーマッドの大ファミリーがついてるからな",
              "maleVariant": "",
              "stringId": "1954932569387323392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "取引は成立ってことでいい？",
              "maleVariant": "取引は成立ってことでいいか？",
              "stringId": "1954939540639338500"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーの言うとおりよ",
              "maleVariant": "ジョニーの言うとおりだ",
              "stringId": "1954940866643382272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "取引は成立？",
              "maleVariant": "取引は成立か？",
              "stringId": "1954941253123330048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーの言うとおり、きっとうまくいく",
              "maleVariant": "",
              "stringId": "1954943069693169664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そいつを信用しているんだな",
              "maleVariant": "",
              "stringId": "1954943848306987008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。〈神輿〉への行き方は任せる。私はローカルネットでお前を導くプログラムを用意しよう",
              "maleVariant": "",
              "stringId": "1954946984908181504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうやって連絡すればいい？",
              "maleVariant": "どうやって連絡すればいいんだ？",
              "stringId": "1954949789387608064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このBBSアドレスを使え。安全な通信回線だ",
              "maleVariant": "",
              "stringId": "1954950537634664448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは？",
              "maleVariant": "",
              "stringId": "1954959234658226176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私が取引すべきは、“人間の部分”であるお前だ",
              "maleVariant": "",
              "stringId": "1954959590368759808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーは少し大げさに言いすぎ",
              "maleVariant": "ジョニーは少し大げさに言いすぎだ",
              "stringId": "1954961503625695232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーは… 少し話を盛るクセがあって",
              "maleVariant": "ジョニーは… 少し話を盛るクセがあってな",
              "stringId": "1954962726785404928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信用できないのはわかる。でもあたしの言葉は信じて。あんたを無事に〈神輿〉まで導いてみせる",
              "maleVariant": "信用できないのはわかる。だが俺の言葉は信じてくれ。あんたを無事に〈神輿〉まで導いてみせる",
              "stringId": "1954966219315236864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいだろう。“人間の部分”は信用に値するようだ",
              "maleVariant": "",
              "stringId": "1954967468395745280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その後、お前の記憶痕跡を肉体に戻せばいい",
              "maleVariant": "",
              "stringId": "1954975114930704384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何が起きたんだ？ なぜ戦わなかった？",
              "maleVariant": "",
              "stringId": "1967634147995045888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "モズリーの仕業か… あの野郎",
              "maleVariant": "",
              "stringId": "1967636299958210560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほどな。きっとヴードゥーのウイルスを除去したときにやられたんだろ",
              "maleVariant": "",
              "stringId": "1967642738969825280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここで話をしよう",
              "maleVariant": "",
              "stringId": "1967654082045562880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オルトじゃないなら、あんたは誰なの？",
              "maleVariant": "オルトじゃないなら、あんたは誰なんだ？",
              "stringId": "1967673521637969920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当にお前なのか",
              "maleVariant": "",
              "stringId": "2009946838818115584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私に何を求める？ 現状では推測もつかない",
              "maleVariant": "",
              "stringId": "2260811523331538944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それが、サイバー空間に投影されたお前の姿か…",
              "maleVariant": "",
              "stringId": "2260815330115403776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何故不要なリスクを冒してまで、ここに来た",
              "maleVariant": "",
              "stringId": "2260815409656184836"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}