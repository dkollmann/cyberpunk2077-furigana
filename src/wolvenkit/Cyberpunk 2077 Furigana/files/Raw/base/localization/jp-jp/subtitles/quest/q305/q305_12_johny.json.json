  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]... agents, presidents, lasers, gadgets ...",
                    "maleVariant": null,
                    "stringId": 3115694275560345600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I listened to So Mi and I feel good about it.",
                    "maleVariant": null,
                    "stringId": 3115707157979205636
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]It's good that Myers didn't get a So Mi alive.",
                    "maleVariant": null,
                    "stringId": 3115708640783110144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I couldn't let Reed down.",
                    "maleVariant": null,
                    "stringId": 3115711565471289344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I couldn't kill So Mi.",
                    "maleVariant": null,
                    "stringId": 3115711565471289356
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I did what So Mi asked me to do. After what I saw, what I found out about her ... I couldn't do otherwise.",
                    "maleVariant": null,
                    "stringId": 3115719512838508544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Most importantly, Myers didn't get So Mi alive.",
                    "maleVariant": null,
                    "stringId": 3115719581943861248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I know it got a lot more complicated in the end, but I couldn't let Reed down.",
                    "maleVariant": null,
                    "stringId": 3115719704736305152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I couldn't kill her. She asked me to, but I just couldn't.",
                    "maleVariant": null,
                    "stringId": 3115719803302449152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Well, but at least you got a medal.",
                    "maleVariant": null,
                    "stringId": 3115728336093843456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Eh ... A lesson for the future: stay away from secret agents and politicians. Maybe we'll survive it.",
                    "maleVariant": null,
                    "stringId": 3115728440011919360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT][get up] Let's go.",
                    "maleVariant": null,
                    "stringId": 3115729066674491396
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Okay, no more gossiping. You have to find a way to get you out of my head.",
                    "maleVariant": null,
                    "stringId": 3115729283821998080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]No, I support the decision itself.",
                    "maleVariant": null,
                    "stringId": 3115798679940632576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I know, I'm not making your life easier.",
                    "maleVariant": null,
                    "stringId": 3115800092833878016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Amen. You saw her at the border. Fake, as if the devil fucked her and then got into her.",
                    "maleVariant": null,
                    "stringId": 3115800939798073344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]So Mi is dead and it will be better for everyone.",
                    "maleVariant": null,
                    "stringId": 3115802781751840768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You know what, moral dilemmas aside ... I guess there was no other choice. Songbird has become too dangerous.",
                    "maleVariant": null,
                    "stringId": 3115803176586842112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]We don't know. Like we don't know what Myers is going to get this girl out of her head. Because it will pick - that's for sure.",
                    "maleVariant": null,
                    "stringId": 3115806553303552000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I do not judge. But you also have a ticking time bomb in your head.",
                    "maleVariant": null,
                    "stringId": 3115807675665747968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You couldn't because you trusted him? Couldn't you because you didn't have enough balls to trust the girl?",
                    "maleVariant": null,
                    "stringId": 3115810006457257984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Come on. I had to believe some version of the truth, Johnny. I bet on Reed's truth.",
                    "maleVariant": null,
                    "stringId": 3115810161109635072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I had a buddy in the war. Cole. He was hit by a burst of M-9. He was bleeding out on my knees.",
                    "maleVariant": null,
                    "stringId": 3115811828160925696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]He would probably live to surgery. He would land in a wheelchair and shit in a diaper for the rest of his life. He asked me not to let that happen.",
                    "maleVariant": null,
                    "stringId": 3115812195783282688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Did you kill him?",
                    "maleVariant": null,
                    "stringId": 3115814864551809024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Why don't you go and talk to someone from Arasaka's Intelligence? They probably miss the Relic as much as Myers miss Songbird.",
                    "maleVariant": null,
                    "stringId": 3115825010271195136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Very funny. We go.",
                    "maleVariant": null,
                    "stringId": 3115828530785423360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]And we're in deep shit as usual, huh?",
                    "maleVariant": null,
                    "stringId": 3120153301862248448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You always got into such trouble or was it my influence?",
                    "maleVariant": null,
                    "stringId": 3120154134700027904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Of course your influence.",
                    "maleVariant": null,
                    "stringId": 3120157996177616900
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You have nothing to do with it.",
                    "maleVariant": null,
                    "stringId": 3120158290198327296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Speak for yourself.",
                    "maleVariant": null,
                    "stringId": 3120158486709858304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Of course it's your fault.",
                    "maleVariant": null,
                    "stringId": 3120158702045425664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You would like to assign yourself all the merits. Even the most absurd ones.",
                    "maleVariant": null,
                    "stringId": 3120158794890539008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Really, Johnny? I assume this is a rhetorical question.",
                    "maleVariant": null,
                    "stringId": 3120158897801981952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Right. I am responsible for everything that matters in your life.",
                    "maleVariant": null,
                    "stringId": 3120159206519533568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Speaking of which, how's it going? Happy to play spy?",
                    "maleVariant": null,
                    "stringId": 3120160725109239808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]No, Johnny. This situation has nothing to do with you.",
                    "maleVariant": null,
                    "stringId": 3120162985604214784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]No, it's cool. It's good to know you got fucked by a foreign intelligence without my participation.",
                    "maleVariant": null,
                    "stringId": 3120163282577715200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]More like a question with a thesis. Which you just confirmed.",
                    "maleVariant": null,
                    "stringId": 3120164911192403968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Without me, you would still be hitting Night City without order and composition, like a louse on a baboon's ass.",
                    "maleVariant": null,
                    "stringId": 3120166092056752128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]But it is not me who will fall asleep knowing that maybe the girl could be saved somehow.",
                    "maleVariant": null,
                    "stringId": 3120178310433402880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Well, she wasn't thrilled.",
                    "maleVariant": null,
                    "stringId": 3120185134935109632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Worse: she couldn't hide it, though she tried. Which means it is good.",
                    "maleVariant": null,
                    "stringId": 3120185245698289664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Damn it knows, if she had survived, her connection to Blackwall wouldn't activate again ...",
                    "maleVariant": null,
                    "stringId": 3120187643229564928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]So in your opinion, I did wrong? How convenient it is for you to speak after the fact.",
                    "maleVariant": null,
                    "stringId": 3120189757712429056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I hope no one in a moment will decide that it would be safer to kill you right now.",
                    "maleVariant": null,
                    "stringId": 3120192203427868672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I have no idea.",
                    "maleVariant": null,
                    "stringId": 3120194952173383684
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]So let's not go too far with this experience ...",
                    "maleVariant": null,
                    "stringId": 3120195015339601920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]By the way. The shard from the girl. Do you know how to use it?",
                    "maleVariant": null,
                    "stringId": 3120196373941776384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Correction: You bet on a version of the lie. We will probably never know the whole truth.",
                    "maleVariant": null,
                    "stringId": 3120208213254008832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I believe So Mi will get a new chance.",
                    "maleVariant": null,
                    "stringId": 3120212899616976896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]So Mi did not have to die in this bunker. I did what seemed right to me.",
                    "maleVariant": null,
                    "stringId": 3120213548056375296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I held his stomach with one hand, because his guts were pouring out, and with the other I squeezed his arm and told him it would be fine.",
                    "maleVariant": null,
                    "stringId": 3120217386331074560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]It was a war, we were soldiers. Brothers. I did what I had to do.",
                    "maleVariant": null,
                    "stringId": 3120223201934897152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Mhm ...",
                    "maleVariant": null,
                    "stringId": 3120225616394383360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Right for you? Is it for her? Because, you know, it makes a difference.",
                    "maleVariant": null,
                    "stringId": 3120226945418326016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]So I will recover. He would get another chance. I belive that everything will be good.",
                    "maleVariant": null,
                    "stringId": 3120227151610310656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]"Everything will be fine". We've heard that somewhere before, huh? These are empty promises.",
                    "maleVariant": null,
                    "stringId": 3120227523796070400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Leave me alone, Johnny.",
                    "maleVariant": null,
                    "stringId": 3120227849089511424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I don't say anything anymore. Excuse me.",
                    "maleVariant": null,
                    "stringId": 3120228910198091776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]It was different with you. Don't torture yourself with this.",
                    "maleVariant": null,
                    "stringId": 3120232995701846016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I will write in my will for them to throw it into my coffin. Which will probably happen soon.",
                    "maleVariant": null,
                    "stringId": 3120235105185099776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Dark. I'll drink to that so that by then I am not in your head anymore.",
                    "maleVariant": null,
                    "stringId": 3120238121359437824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I'll just ... use it?",
                    "maleVariant": null,
                    "stringId": 3120239282594443264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I have no idea. So Mi did not include the instruction manual.",
                    "maleVariant": null,
                    "stringId": 3120239524555452416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I don't know, will I just use it somehow?",
                    "maleVariant": null,
                    "stringId": 3120239613994790912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Shit, you have to check it out.",
                    "maleVariant": null,
                    "stringId": 3120239745226174464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You must have been drowsy.",
                    "maleVariant": null,
                    "stringId": 3120242259291992064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You have no idea what's on that splinter, how to activate it, and what the fuck it will do to your head.",
                    "maleVariant": null,
                    "stringId": 3120242404800786432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You got a piece of code from a girl who was almost eaten by Blackwall.",
                    "maleVariant": null,
                    "stringId": 3120242925817229312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]What am I supposed to do?",
                    "maleVariant": null,
                    "stringId": 3120243966809927680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Of course. It would be too easy. We don't even know if you should put it up your head or your ass.",
                    "maleVariant": null,
                    "stringId": 3120244249472462848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Any ideas, Mr. Sarcastic?",
                    "maleVariant": null,
                    "stringId": 3120244481249701888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Ask Viktor. It's the only ripperdoc you can trust and won't scare you when it sees whose code it is.",
                    "maleVariant": null,
                    "stringId": 3120245721589280768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Okay, that's some kind of thought. We will do it this way.",
                    "maleVariant": null,
                    "stringId": 3120247024742756352
                  }
                }
              ]
            }
          }
        }
      }
    },
