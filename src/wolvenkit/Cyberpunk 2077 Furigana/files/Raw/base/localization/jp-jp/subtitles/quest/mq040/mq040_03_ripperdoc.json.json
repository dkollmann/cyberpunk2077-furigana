  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「今にも崩れそうな建物の中に姿を消す女。漂う空気は、スモッグと背信で重苦しかった」",
                    "maleVariant": null,
                    "stringId": 1805535941937475584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "気をつけて！ シャツが破れるじゃない！",
                    "maleVariant": null,
                    "stringId": 1805696370944913408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あらあら",
                    "maleVariant": "おやおや",
                    "stringId": 1805697185076097024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと！ あなた、ここで何してるの！？",
                    "maleVariant": null,
                    "stringId": 1805706982399815680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "シンシア！ 誰にも尾行されないようにと言ったろ！",
                    "maleVariant": null,
                    "stringId": 1805710631159083008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何してるのかって聞いてるの！ あなた誰よ！？",
                    "maleVariant": null,
                    "stringId": 1805711322950807552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "聞いてあげる",
                    "maleVariant": "聞いてやろう",
                    "stringId": 1805712598807752708
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "説明すべき相手はあたしじゃない",
                    "maleVariant": "説明すべき相手は俺じゃない",
                    "stringId": 1806586413578334208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言い分を聞きましょ",
                    "maleVariant": "言い分を聞いてやる",
                    "stringId": 1806589011949662208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "説明すべき相手はあたしじゃない",
                    "maleVariant": "説明すべき相手は俺じゃない",
                    "stringId": 1806589853595480064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんな、待って！ お願い、聞いて！",
                    "maleVariant": null,
                    "stringId": 1806600870757019648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "終わったよ。奥さんを尾行したら、汚いアパートに入ってって、中で男が待ってた",
                    "maleVariant": null,
                    "stringId": 1806609141186310144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リパードクに会って、スキャンを受けてたの。肌、髪、目… 彼女の全てがインプラントだったのよ",
                    "maleVariant": "リパードクに会って、スキャンを受けてたんだ。肌、髪、目… 彼女の全てがインプラントだったんだよ",
                    "stringId": 1806619943716925440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これ以上関わりたくないの。彼女に直接聞いて",
                    "maleVariant": "これ以上関わりたくないんだ。彼女に直接聞いてくれ",
                    "stringId": 1806620112176951296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼女はあんたを愛してるのよ",
                    "maleVariant": "彼女はあんたを愛してんだよ",
                    "stringId": 1806631233390784516
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "確かにそうね",
                    "maleVariant": "確かにそうだな",
                    "stringId": 1806631457635053568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "確かにシンシアは隠し事をしてた。ウソをついてた。でも…",
                    "maleVariant": "確かにシンシアは隠し事をしてた。ウソをついてた。だが…",
                    "stringId": 1806634352241684480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "形はどうあれ、裏切られたことに変わりはない",
                    "maleVariant": "形はどうあれ、裏切られたことに変わりはないな",
                    "stringId": 1806635012408356864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リパードクと会っていた",
                    "maleVariant": null,
                    "stringId": 1806661635568447492
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あとは彼女から直接聞いて",
                    "maleVariant": "あとは彼女から直接聞いてくれ",
                    "stringId": 1806661961616863232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もちろんペペには伝えるべきだったけど… タイミングがなかったの。それにまた新しい子を授かって… そんな予定じゃなかったんだけど…",
                    "maleVariant": null,
                    "stringId": 1816891964954652672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ねえ、シンシアよ。ペペの妻。覚えてる？",
                    "maleVariant": null,
                    "stringId": 1816909127090319360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "シンシアよ。ペペの妻。覚えてる？",
                    "maleVariant": null,
                    "stringId": 1816909362759872512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "覚えてる",
                    "maleVariant": null,
                    "stringId": 1816936212278161408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やっぱりな。それで？",
                    "maleVariant": null,
                    "stringId": 1816991053306277888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ息子は… つまり…",
                    "maleVariant": null,
                    "stringId": 1874927941530562560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はあ… もういっそ殺してよ",
                    "maleVariant": "はあ… もういっそ殺してくれ",
                    "stringId": 1914116135583010816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "旦那に頼まれた",
                    "maleVariant": null,
                    "stringId": 1914123870533898244
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "関係ない。証拠はもう十分よ",
                    "maleVariant": "関係ない。証拠はもう十分だ",
                    "stringId": 1914124093586984960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "旦那に頼まれたの。あんたを疑ってたけど、正しかったみたいね",
                    "maleVariant": "旦那に頼まれた。あんたを疑ってたが、正しかったみたいだな",
                    "stringId": 1914124605191409664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "え？ それ、どういう意味よ！？",
                    "maleVariant": null,
                    "stringId": 1914127063858860032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「ウソは脚が短いと言う諺があるが、ウソつきの脚は至りて棒のように長いのである」",
                    "maleVariant": null,
                    "stringId": 1914127462200299520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "表現が古いよ、ジョニー",
                    "maleVariant": "表現が古いぞ、ジョニー",
                    "stringId": 1914128895779852288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "え？ 誤解よ！ これは、そういうことじゃないの。説明させて！",
                    "maleVariant": null,
                    "stringId": 1914129924575514624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "関係ない。もう十分よ",
                    "maleVariant": "関係ない。もう十分だ",
                    "stringId": 1914137819362938880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「私は女の頼みに耳を傾けず、その場を後にした。ネオンに染まった空が、目を明るく照らしつけた…」",
                    "maleVariant": null,
                    "stringId": 1914141799455444992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いい加減やめてくれる？ お願いだから",
                    "maleVariant": "いい加減やめてくれるか？ お願いだ",
                    "stringId": 1914143372336222208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。まず… 彼はインプットじゃなくて、リパードクよ。美容整形専門のね",
                    "maleVariant": null,
                    "stringId": 1914144586587230208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こんなみすぼらしい場所で？",
                    "maleVariant": null,
                    "stringId": 1914145588119916544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その… 数年前にちょっとした事故があって… 被害者が… 患者の家族がまだ許してくれないんだ。だから、ここでひっそりとやってる",
                    "maleVariant": null,
                    "stringId": 1914145931079766016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なら、子供の顔が違うのは？",
                    "maleVariant": null,
                    "stringId": 1914146928132935680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その… 私、昔はこんな顔じゃなかったの。体を完全に作り替えたのよ。肌も髪も目も、全部ね… でも、もちろん遺伝子は変えられなくて",
                    "maleVariant": null,
                    "stringId": 1914147280186036224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも… どうして？ 何のために？",
                    "maleVariant": null,
                    "stringId": 1914150648816128000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうして外見を変えたの？",
                    "maleVariant": "どうして外見を変えたんだ？",
                    "stringId": 1914150965553188868
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "話が変わってきた",
                    "maleVariant": null,
                    "stringId": 1914151225365155840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしてだと思う？ 魔が差して？ それとも家が金持ちだったから？",
                    "maleVariant": null,
                    "stringId": 1914154306903527424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "誰にだって、忘れたいことや、ぬぐい去りたい秘密はあるでしょ",
                    "maleVariant": null,
                    "stringId": 1914154948715925504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "えっと… 思ってたより、複雑な話になってきた",
                    "maleVariant": "えっと… 思ってたより、複雑な話になってきたな",
                    "stringId": 1914160173057101824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一方的に決めつけず、話を聞いてくれてありがとう",
                    "maleVariant": null,
                    "stringId": 1914161084697464832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お願い… 彼に… 彼に伝えて…",
                    "maleVariant": null,
                    "stringId": 1914164961727533056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自分でも何て伝えたらいいのかわからない。本当に申し訳ないと思ってる。人生って不条理なドラマよね。とにかく… お願い",
                    "maleVariant": null,
                    "stringId": 1914165432580100096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何を？",
                    "maleVariant": null,
                    "stringId": 1914165754803310592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最後の台詞はどんぴしゃに的を射てる",
                    "maleVariant": null,
                    "stringId": 1914167710875701248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあね。あたしには関係ない話よ",
                    "maleVariant": "さあな。俺には関係ない話だ",
                    "stringId": 1914175260186693632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "とにかく、あたしの仕事は終わりよ。支払いはよろしく",
                    "maleVariant": "とにかく、俺の仕事は終わりだ。支払いはよろしく",
                    "stringId": 1914176153623777280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "所詮、俺はお前にとって、ただの仕事の依頼主の一人か。もっと情に厚い奴だと思ってたんだがな",
                    "maleVariant": null,
                    "stringId": 1914177789050015744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あばよ、V",
                    "maleVariant": null,
                    "stringId": 1914178281092206592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何がやっぱりよ。浮気じゃなかったの。少なくともそういう意味では、あんたを裏切ってなかった",
                    "maleVariant": "何がやっぱりだ。浮気じゃなかったんだよ。少なくともそういう意味では、あんたを裏切ってなかった",
                    "stringId": 1914180527360102400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "え？ じゃあどういう意味でだ？",
                    "maleVariant": null,
                    "stringId": 1914181188365635584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そういうこと",
                    "maleVariant": "そういうことだ",
                    "stringId": 1914192292215050240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まったく… どうしてシンシアは隠してたんだ？ 自分の夫に… 丸10年もだぞ！",
                    "maleVariant": null,
                    "stringId": 1914195247672643584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "外見なんてどうでもいい。ずっと俺にウソをついてたなんて。これからどうやって妻を信じればいいんだ？",
                    "maleVariant": null,
                    "stringId": 1914197369134829568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしには関係ない",
                    "maleVariant": "俺には関係ない",
                    "stringId": 1914198878278963200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあね",
                    "maleVariant": "さあな",
                    "stringId": 1914199097087414272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしだったら、すべてを水に流して一緒に暮らす",
                    "maleVariant": "俺だったら、すべてを水に流して一緒に暮らすね",
                    "stringId": 1914202097340174336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ… まあ、確かにそうだな",
                    "maleVariant": null,
                    "stringId": 1914202691270397952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいのよ、ペペ。それじゃあね",
                    "maleVariant": "いいんだ、ペペ。それじゃあな",
                    "stringId": 1914203021714444288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「街は水を打ったかのように静寂と沈黙に包まれた。少し休もう、と私は思った。酒の時間だと」",
                    "maleVariant": null,
                    "stringId": 1914203420726972416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺は… どうすりゃいい？",
                    "maleVariant": null,
                    "stringId": 1914206824622772224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ペペ、あたしには答えはわからない。自分の気持ちは自分で決めないと",
                    "maleVariant": "ペペ、俺には答えはわからない。自分の気持ちは自分で決めないと",
                    "stringId": 1914207011336409088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、そうだな。じゃあ… またな",
                    "maleVariant": null,
                    "stringId": 1914207773273673728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうね、その終わり方だけは悪くない",
                    "maleVariant": "そうだな、その終わり方だけは悪くない",
                    "stringId": 1914209576455294976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼に何を言ったか知らないけど… 本当に… ありがとう",
                    "maleVariant": null,
                    "stringId": 1914213430383136768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あなたにとってはいつもの一日だったのかもしれないけど、私にとっては人生の終わりになったわ",
                    "maleVariant": null,
                    "stringId": 1914215357867151360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ペペに放り出されたの。家無しよ。あなたは、さぞかしうれしいでしょうね",
                    "maleVariant": null,
                    "stringId": 1914215951612825600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "息子の外見がペペのイメージと違ったのはそのせいよ。彼は間違いなく父親。ただ私が… 昔の自分とは大違いなの",
                    "maleVariant": null,
                    "stringId": 1935781619185274880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私には… たくさんあった。それは誰にも話せない。夫にさえね",
                    "maleVariant": null,
                    "stringId": 1935791495043141632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼女はあんたを愛してるのよ。それがこの街でどれほど貴重なことかわかる？",
                    "maleVariant": "彼女はあんたを愛してるんだ。それがこの街でどれほど貴重なことかわかるか？",
                    "stringId": 1935801503759781888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ありがとよ。受け取ってくれ… 世話になったな",
                    "maleVariant": null,
                    "stringId": 2237427012068950016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、待て。礼をしなくちゃな。手間を… かけたんだし",
                    "maleVariant": null,
                    "stringId": 2237429669881954304
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}