  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "よう",
              "stringId": "1177494386146975744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "おっと… 聞こえるか？",
              "stringId": "1177533483452375040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ふう… さっぱりしたな",
              "stringId": "1252860399381815296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "いいサンプルが取れそうだ。パン、パン… ヘヘ",
              "stringId": "1316819088611864576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "そうだな",
              "stringId": "1316819856521818112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "クソ、いい加減にしろ",
              "stringId": "1316821737079959552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大事な用だったかもしれない",
              "maleVariant": "",
              "stringId": "1316822502171340800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "元カレ？",
              "maleVariant": "元カレか？",
              "stringId": "1316822791846752256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "いいのか？",
              "stringId": "1316823597136343040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "誰だったんだ？ 元カレがよりを戻そうとしてきたか？",
              "stringId": "1316823717932298256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "大事な用なら、なんとか連絡してくるだろ",
              "stringId": "1316824169793056768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "新しいマネージャーだ。前の奴と同じくらい使えない。これじゃ雇った意味がなかったな",
              "stringId": "1316828875030392832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "それはそうと… お前と一緒にいて、ひとつ学んだことがある",
              "stringId": "1316830823771140096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "へえ？ なんだ",
              "stringId": "1316831952626118656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "過去は過去として、割り切らなきゃやっていけないってことさ",
              "stringId": "1316832020825501696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Agonizing about the future's a waste of time, too.",
              "maleVariant": "未来を憂うのも時間の無駄さ",
              "stringId": "1316834078903365632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]True, that.",
              "maleVariant": "言いえて妙だな",
              "stringId": "1316834368260009984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "だからといって、先のことを憂うのも時間の無駄に過ぎない",
              "stringId": "1316834659076272144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "そうだ。過去は変えられない。だからこそ、未来に後悔しないようにするべきだ",
              "stringId": "1316834742979129344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "いや、そうじゃないんだ。これまで俺は… 何かがうまくいかなかったとき、常に誰かのせいにしてきた",
              "stringId": "1316835078255013888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "それで…？",
              "stringId": "1316836317269192704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "あいつの影から出ようとすればするほど、どんどん闇に染まっていったんだ。わかるか？",
              "stringId": "1316836381022613504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "で、この先は？",
              "stringId": "1316837583982555136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "新しいアルバムを完成させて、クリスタル・パレスでライブ――カジノにも顔を出してみるかもな",
              "stringId": "1316837991366914048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "金持ちのブタどもがどれだけ来るかわからんが、来た奴らには俺の叫びを聴かせてやる",
              "stringId": "1316838561825812480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ジョニーのためにも、俺なりに戦うつもりだ",
              "stringId": "1316839426439639040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "同じとこをグルグル回って、ずいぶんと時間を無駄にしちまった。一体、何を求めてたのかもわからない",
              "stringId": "1316847847612215296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "V… 気をつけろよ",
              "stringId": "1316853945694023680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "努力する",
              "stringId": "1316854948854099968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "じゃあ… また… 近いうちにな",
              "stringId": "1316855066294611968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "はあ…  新しいデモを聴かないと",
              "stringId": "1490834190305456128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "本当は昨日のうちにやるはずだったんだが、まあ… わかるだろ",
              "stringId": "1490834803965685760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ああ…",
              "stringId": "1490835436768718848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "おい、お前大丈夫か？ 昨日もなんか変だったぞ",
              "stringId": "1490835588334088192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Don't worry about me. I'll be fine.",
              "maleVariant": "平気だ。気にするな",
              "stringId": "1490837026477043712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Not feeling well. Gets worse every day.",
              "maleVariant": "疲れてるだけだ。日に日にひどくなってる",
              "stringId": "1490837288386162688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Just noticed yesterday?",
              "maleVariant": "昨日になってやっと気づいたのか？",
              "stringId": "1490837552107220992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "平気だ、何も問題ない。心配するな",
              "stringId": "1490839706519859200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "その… 俺にできることがあれば、遠慮なく言えよ。「ミコシ」のことでも何でも相談に乗ってやる",
              "stringId": "1490841818502918144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "何なら、ピロートークでもいいぞ。特別に腕枕もサービスしてやる",
              "stringId": "1490842541282160640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "覚えておく",
              "stringId": "1490842956384038912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "調子が悪いんだ",
              "stringId": "1490846312649322496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "なんだ… 言ってくれればよかったのに。昨日飲みすぎたのもまずかったかな",
              "stringId": "1490846804389523456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "そうじゃない",
              "stringId": "1490847833722056704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "じゃあまた例の「ミコシ」の件かよ？ クソ… 何がどうなってるのか、俺にはさっぱりだ",
              "stringId": "1490848236962443264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "クリスタル・パレスが見えるかもしれん。この俺が、まだあそこでライブしたことがねぇとはな",
              "stringId": "1490848839147057152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "外に行ってるぜ",
              "stringId": "1490850965155868672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "なんだ、昨日になってやっと気づいたのか？",
              "stringId": "1490853562604449792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "おい、俺だってお前の置かれた状況は…",
              "stringId": "1490854128936153088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "わかってるつもりか？ なら教えてくれよ、ケリー",
              "stringId": "1490854579739947008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "こういう時のお前は、あいつそっくりだ",
              "stringId": "1490854817305325568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "おいおい…",
              "stringId": "1490858522436038656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "その頑固なところも、人の神経を逆なでするところもな",
              "stringId": "1490858741093494784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Got no idea what you're talking about.",
              "maleVariant": "その話はやめておけ",
              "stringId": "1490860710268891136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I don't wanna argue.",
              "maleVariant": "ケンカはしたくない",
              "stringId": "1490861072203771904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "適当なこと言うな",
              "stringId": "1490863575314067456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "話題を変えよう。お前のためにも",
              "stringId": "1490865173058678784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ずいぶんな上から目線だな",
              "stringId": "1490865276272111616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ケンカはしたくない",
              "stringId": "1490867688164683776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ケンカじゃない",
              "stringId": "1490867997536546816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ケリー、頼むよ。やりあう気力も時間もないんだ",
              "stringId": "1490868157708627968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "わかったよ。これ以上は言わない",
              "stringId": "1490869023765295104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ジョニーに執心しすぎるあまり、自分のことが見えてなかったんだろ",
              "stringId": "1490887405705228288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]What now?",
              "maleVariant": "これからどうするんだ？",
              "stringId": "1490897339964583936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Think about him often?",
              "maleVariant": "よくジョニーのことを考えるのか？",
              "stringId": "1490897644890484736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "あいつが恋しいか？",
              "stringId": "1490910391850102784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "さあな",
              "stringId": "1490910720800976896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "昔は恨んでた… たまに、言葉にできないくらい恋しくなって… でも、なんやかんやでムカついてることが多かったな。ただ最近は… 何も感じないんだ",
              "stringId": "1490912386526867456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "そうか",
              "stringId": "1490914551257493504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "あいつはもういない――それでいいと思ってる。かつて心の中にあいつがいた場所は… 別の誰かで埋めたいんだ",
              "stringId": "1490914924416331776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "同じ気持ちだよ",
              "stringId": "1490915948564705280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us][Make coffee]",
              "maleVariant": "[コーヒーを作る]",
              "stringId": "1708453217933316100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us][Drink coffee]",
              "maleVariant": "[マグを取る]",
              "stringId": "1708454553835917316"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "良くなるさ。新しい薬のせいか？",
              "stringId": "1792090361995849728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "実をいうと、徐々にひどくなってる",
              "stringId": "1792090449270927360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ああ…",
              "stringId": "1792107872845950976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "だが、V… お前はもう十分やった。それを否定できる奴なんて、一人もいない",
              "stringId": "1792108109975121920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "もう休んでいいだろ… これ以上、何を成し遂げようって言うんだ？",
              "stringId": "1792109394036125696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]It's not enough. I don't know any other way.",
              "maleVariant": "これ以外の生き方を知らない",
              "stringId": "1792110799832608772"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Easy for you to say. You're not dying.",
              "maleVariant": "お前は死にかけてないだろ",
              "stringId": "1792255553065054208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Can't believe my ears. You're worried about me?",
              "maleVariant": "お前が俺の心配をするなんてな",
              "stringId": "1792256646268448768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "大切な誰かで",
              "stringId": "1792269424400134144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "十分なもんか。それに俺は、これ以外の生き方を知らない",
              "stringId": "1792287708394680320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "お前はアフターライフのトップに上り詰めた。それでもまだ満足してないのか？",
              "stringId": "1792290272037171200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "それをお前が言うか、ケリー？ スポットライトを浴び続けるために、自分の信念を捨てたくせに",
              "stringId": "1792291605909729280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "まず、その「捨てた」信念とやらは、俺のじゃない。そして次は… 「くたばれ」だ",
              "stringId": "1792292633598095360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ケリー、俺は降りない。ここまで色々あったからこそ… 他に選択肢はないんだ",
              "stringId": "1792293831373877248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "お前は死にかけてない",
              "stringId": "1792299710479462400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "本気で言ってるのかよ",
              "stringId": "1792300323149836288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "事実を言っただけだ。お前と違って、俺はすでに受け入れてる",
              "stringId": "1792300823647744000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "言われなくても、お前の… 俺たちの時間が残り少ないのは、わかってる",
              "stringId": "1792301323508117504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "でも今、俺はお前と一緒にいるだろ。これまでも、これからも、いずれ訪れる最後…",
              "stringId": "1792302618994405376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "わかってる、もういいんだ…",
              "stringId": "1792303743218872320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Don't wanna lose you, either.",
              "maleVariant": "俺もだ",
              "stringId": "1792306404353765380"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ、俺は… お前を失いたくないんだ。ほら、これで満足か？",
              "maleVariant": "",
              "stringId": "1792307617782689792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "お前を失いたくない",
              "stringId": "1792310581461118976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]We both know how this ends.",
              "maleVariant": "結末はすでにわかってるはずだ",
              "stringId": "1792311826615758848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ハッ… こんな日が来るとはな",
              "stringId": "1792393619721285632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "何が言いたいんだよ？",
              "stringId": "1792394761880596480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ユーロダインが俺の心配をしてる",
              "stringId": "1792394889404215296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "お前がイカレちまったんじゃないかって心配してるだけだ。それに…",
              "stringId": "1792395984889311232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "時間はまだ残ってる",
              "stringId": "1792404881461080064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "無事に帰ってこなかったら、ぶっ飛ばしてやるからな。わかったか？",
              "stringId": "1792406104050364416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ああ。約束する",
              "stringId": "1792407878811381760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "よし、ならいい",
              "stringId": "1792408120151633920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "結末はすでにわかってるはずだ。好き好んでこうなったわけじゃない",
              "stringId": "1792409356414349312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ライブ後に、屋根の上で話したことを覚えてるか？",
              "stringId": "1792410173598986240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ああ…",
              "stringId": "1792411340169146368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "あのときは、自分と街が俺のすべてだった",
              "stringId": "1792411609560903680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "それは今もそうだろ",
              "stringId": "1792412078903521280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "そりゃ、その2つは今でも大事だがな",
              "stringId": "1792412258621059072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "だからこそ、俺はこの仕事をやり遂げなきゃいけないんだ。だろ？",
              "stringId": "1792413237370286080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "それはお前がいるからだ。お前のいないナイトシティなんて、何の魅力もない",
              "stringId": "1792413920689516544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "そうかもな。でも、絶対に帰ってこいよ。俺には… まだお前が必要なんだ",
              "stringId": "1792416786472890368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "それじゃあな",
              "stringId": "1792420149247733760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "困ったらいつもこれだ",
              "stringId": "1792420538428813312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "まあいい。今回はデラマンに救われたな",
              "stringId": "1792423023721705472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us][Take shower]",
              "maleVariant": "[シャワーを浴びる]",
              "stringId": "1872155769232281600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us][Stand]",
              "maleVariant": "[立ち上がる]",
              "stringId": "1872156345210884096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[キスする] 朝よ",
              "maleVariant": "[キスする] 朝だぞ",
              "stringId": "1872156345244438528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[抱きしめる] じゃあね",
              "maleVariant": "[抱きしめる] じゃあな",
              "stringId": "1873250571879313412"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "お迎えだ",
              "stringId": "1873252330299322368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us][Step away] I gotta go.",
              "maleVariant": "[去る] デラマンが待ってる",
              "stringId": "1873253522001752064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "わかってるよ。俺もすぐ出る。この後スタジオで収録だからな",
              "stringId": "1873272679200059396"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "頭はいてえわ、クスリは抜けきってねえわ、それになんだか無性にイライラする――いいセッションができそうだ",
              "stringId": "1873272679216836624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "メーターが回ってるから",
              "stringId": "1873272986055340032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "もう行くよ。アフターライフでみんなが待ってる",
              "stringId": "1873275436451622912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us][Turn]",
              "maleVariant": "[寝返りを打つ]",
              "stringId": "1926912778227773444"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us][Wash off blood] Mhmmm...",
              "maleVariant": "[血を洗い流す] ああ…",
              "stringId": "1926953308106092544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[寄りかかる]",
              "maleVariant": "",
              "stringId": "1927156767048691712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "朝だぞ",
              "stringId": "2008265613816492032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "うーん",
              "stringId": "2008267526721105920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "待てよ、俺も浴びる",
              "stringId": "2008268081409421312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "クソ…",
              "stringId": "2008268726694703104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ああ…",
              "stringId": "2008269350421262336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "実はまだ頭ん中にいるんじゃねえのか？",
              "stringId": "2008275712442232832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "わかってる",
              "stringId": "2008276967981969408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "大抵はシルヴァーハンドさ。前にも話しただろ",
              "stringId": "2008280322737303552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us][pl_pl]Zmykaj, czekają na ciebie.",
              "maleVariant": "行けよ。待ってる奴がいるんだろ",
              "stringId": "2237243144670474240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us][pl_pl]Leć. I uważaj na siebie.",
              "maleVariant": "ほら、早く行けって。達者でな",
              "stringId": "2237243919526195200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us][pl_pl]No heeej. Jak tam, rześki i rozbudzony?",
              "maleVariant": "V、気分はどうだ。ゆっくり休めたか？",
              "stringId": "2237269870607790080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[アミュレットを取る]",
              "maleVariant": "",
              "stringId": "2405519763042988032"
            }
          ]
        }
      }
