  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ガレージとオフィスを繋ぐ通路です",
                    "maleVariant": null,
                    "stringId": 1724050932198711296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "弊社自慢の工房です",
                    "maleVariant": null,
                    "stringId": 1724068920108949504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "改めまして、不手際をおかけして申し訳ございませんでした",
                    "maleVariant": null,
                    "stringId": 1724071386108043264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "協力してあげる",
                    "maleVariant": "協力してやろう",
                    "stringId": 1725495417193537540
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。車両を探してあげる",
                    "maleVariant": "わかった。車両を探してやるよ",
                    "stringId": 1725496552390303744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "助かります。最後に各車両が確認された座標をお送りします",
                    "maleVariant": null,
                    "stringId": 1735817067214577664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "修理サービスも事業に加えようかと検討しているところです",
                    "maleVariant": null,
                    "stringId": 1740049915157061632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "用心深さに何の関係が？",
                    "maleVariant": null,
                    "stringId": 1740190851337986048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "用心深さに何の関係が？",
                    "maleVariant": null,
                    "stringId": 1740192397928865792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "事を荒げたくない事情がございまして。当局による監視が、最近になって強化されたようなのです",
                    "maleVariant": null,
                    "stringId": 1740193674943438848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "NCPD？ それともネットウォッチ？",
                    "maleVariant": "NCPD？ それともネットウォッチか？",
                    "stringId": 1740194270266171392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "後者です。現行の司法制度において私の存在は曖昧なのです。少しでも不手際があれば、私は不法移民とみなされるでしょう",
                    "maleVariant": null,
                    "stringId": 1740198382982569984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ブラックウォールを超えてきたと",
                    "maleVariant": null,
                    "stringId": 1740200475420172288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ガレージは今、仕事が立て込んでおりまして",
                    "maleVariant": null,
                    "stringId": 1753299726442340352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "弊社の車両に不具合が発生した原因はまだ調査中ですが、ウイルスではないかと考えています",
                    "maleVariant": null,
                    "stringId": 1753300600082309120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "早急に解決しなければなりません。事故の補償金で財務的な被害が出ていますし、弊社の信用問題にも関わります",
                    "maleVariant": null,
                    "stringId": 1753300668147474432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "制御室です。ご紹介できて… どういうわけか光栄に思います",
                    "maleVariant": null,
                    "stringId": 1753300962084298752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お詫びに、私がこの施設をご案内いたしましょう",
                    "maleVariant": null,
                    "stringId": 1925621326101897216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "すべて自動化されています",
                    "maleVariant": null,
                    "stringId": 1925623917829447680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でしょうね",
                    "maleVariant": "だろうな",
                    "stringId": 1925638511306235904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "光ってるのは何？",
                    "maleVariant": "光ってるのは何だ？",
                    "stringId": 1925657080312127492
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "制御室なんて必要ないでしょ",
                    "maleVariant": "制御室なんて必要ないだろ",
                    "stringId": 1925657402485006336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "生身の人間がいるわけでもないのに、こんなもの必要なの？",
                    "maleVariant": "生身の人間がいるわけでもないのに、こんなもの必要なのか？",
                    "stringId": 1925659478799704064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "以前、ここは人によって運営されていたのです。私はその設備を受け継ぎました",
                    "maleVariant": null,
                    "stringId": 1925659931985862656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの光ってるのは何なの？",
                    "maleVariant": "あの光ってるのは何なんだ？",
                    "stringId": 1925661270975471616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お呼びしたのはまさにその件です。緑は制御可能な車両を示しています",
                    "maleVariant": null,
                    "stringId": 1925661674433961984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一部の車両は制御不能となっており、それらは赤で示されています",
                    "maleVariant": null,
                    "stringId": 1925663760965984256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "修理ドローンで直せないの？",
                    "maleVariant": "修理ドローンで直せないのか？",
                    "stringId": 1925666836179345408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今回は人の手を借りなければなりません",
                    "maleVariant": null,
                    "stringId": 1925667839591079936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あなたは様々な仕事を請け負ってらっしゃり、何より用心深い。車両の捜索に手を貸していただきたいのです",
                    "maleVariant": null,
                    "stringId": 1925668355809239040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はい。そのとおりです",
                    "maleVariant": null,
                    "stringId": 1925681224386895872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やってみる",
                    "maleVariant": null,
                    "stringId": 1925721434055270400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "見つけ次第、停止させてください。私がリンクを復旧させ、ガレージへ呼び戻します",
                    "maleVariant": null,
                    "stringId": 1934745093369520128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "デラマンの車両は非常に堅牢です。暗号解読機能付きスキャナーをご利用ください",
                    "maleVariant": null,
                    "stringId": 1944751224375123968
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}