    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なあ、クロームだらけで筋トレなんか必要ない奴が、どうしてここに来るのかわかるか？",
                    "maleVariant": null,
                    "stringId": 2179631184448446464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあな。どんだけ持ち上げられるか試したいとか？",
                    "maleVariant": null,
                    "stringId": 2212564219017228288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや、違う。本能だ",
                    "maleVariant": null,
                    "stringId": 2212564933542080512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本能？",
                    "maleVariant": null,
                    "stringId": 2212565089788293120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そう。読んだことがあるんだ。リパーにワイヤーやボルトをどれだけ埋め込まれても、人間は身体を鍛えたくなるんだと",
                    "maleVariant": null,
                    "stringId": 2212565145606090752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その本能は何世代もしてからでないと消えない",
                    "maleVariant": null,
                    "stringId": 2212568570842509312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "へえ。幻肢痛みたいな感じか？ ゼロ世代の頃は、腕を切断された後でもかゆくなったりしたらしい",
                    "maleVariant": null,
                    "stringId": 2212568830134382592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "新しいクロームをつけても、そいつをかかずにはいられなかったって",
                    "maleVariant": null,
                    "stringId": 2212570605298053120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そう、それと同じことさ。人間の体ってのは不思議だよな",
                    "maleVariant": null,
                    "stringId": 2212571100007821312
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
