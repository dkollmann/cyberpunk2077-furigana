  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼は報いを受けたのよ",
              "maleVariant": "奴は報いを受けたんだ",
              "stringId": "1803877978846699524"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リズィー、いったい何が… なるほど。慌てるわけね",
              "maleVariant": "リズィー、いったい何が… なるほど。慌てるわけだ",
              "stringId": "1803878046173667328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、私… 怒って彼のことを押しただけなのに… そしたらこの腕が、この手が… ああ、そんな…",
              "maleVariant": "",
              "stringId": "1803878836330209280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまりあたしを呼んだ用件は、死体の処理ね？",
              "maleVariant": "つまり俺を呼んだ用件は、死体の処理か？",
              "stringId": "1803879317265883136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ… メディアにバレたらまずい。もしそんなことになったら… ああ…",
              "maleVariant": "",
              "stringId": "1803879697756364800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リズィー、しっかりして。死体をどうするの？",
              "maleVariant": "リズィー、しっかりしろ。死体はどうするんだ？",
              "stringId": "1803880416374857728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。任せて",
              "maleVariant": "わかった。任せろ",
              "stringId": "1803880682276954116"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしにはできない",
              "maleVariant": "俺にはできない",
              "stringId": "1803880931368280064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "安心して眠れるように処理しとく",
              "maleVariant": "安心して眠れるように処理しとくよ",
              "stringId": "1803881331337109504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "眠れるように？ 私は眠らない。必要がないの",
              "maleVariant": "",
              "stringId": "1803881603144785920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう、リアム。あなたは気づいてないけど、私に素晴らしいものを与えてくれた",
              "maleVariant": "",
              "stringId": "1803884412489605120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うわ… ひどい有様ね…",
              "maleVariant": "うわ… ひどい有様だな…",
              "stringId": "1803885789680934912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これは世界中が興奮する、いい詩が書けそう。他人の記憶を操作したいって誘惑にかられるストーリーよ。“人格”が、コピーと再生を繰り返すだけのデータの集積に過ぎないってことを、改めてメッセージにして…",
              "maleVariant": "",
              "stringId": "1803887153232388096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この跡… 絞め殺したのね",
              "maleVariant": "この跡… 絞め殺したのか",
              "stringId": "1803891823908659200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時間をかけて絞め殺したのね。脊椎が砕けてる",
              "maleVariant": "時間をかけて絞め殺したんだな。脊椎が砕けてる",
              "stringId": "1803902012946817024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっと… 怒っていて我を忘れたのよ",
              "maleVariant": "",
              "stringId": "1803902489822404608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リズィー… 何か異常がないか、診てもらった方がいい。信用できる誰かに",
              "maleVariant": "",
              "stringId": "1803904713088737280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いったい何の話？",
              "maleVariant": "いったい何の話だ？",
              "stringId": "1803907031783874560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、彼を連れて出てって",
              "maleVariant": "",
              "stringId": "1803907501009051648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この状態に満足してるの",
              "maleVariant": "",
              "stringId": "1803912807189487616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたは役目を終えた。もう行って",
              "maleVariant": "",
              "stringId": "1804183420361678848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう言うことは何もない",
              "maleVariant": "",
              "stringId": "1804186394760749056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いえ、お断り。何か… 様子が変よ、リズィー",
              "maleVariant": "いや、お断りだ。何か… 様子が変だぞ、リズィー",
              "stringId": "1804208150733037572"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいえ、その逆よ。ようやく全てが、あるべき形に収まった",
              "maleVariant": "",
              "stringId": "1804208150733037580"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "夜中に目を覚まして… 一瞬自分が誰だかわからなくなることはない？ それか… 自分が自分じゃないと感じることは？",
              "maleVariant": "",
              "stringId": "1946082666097201152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私は… いつもそう感じてる。どの瞬間にも。この無力さと、虚しさが… 曲作りも手につかなくなって… それで私は…",
              "maleVariant": "",
              "stringId": "1946087276492935168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "起きたことは仕方ない",
              "maleVariant": "",
              "stringId": "1946090382794477568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リアムはこれを恐れてたのね…",
              "maleVariant": "リアムはこれを恐れてたんだな…",
              "stringId": "1946090628295479296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼は報いを受けたのよ",
              "maleVariant": "奴は報いを受けたんだ",
              "stringId": "1946091999463788544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まぁ、起きたことは仕方ない",
              "maleVariant": "",
              "stringId": "1946092346785714176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リアムはこうなるのを恐れてたわけね…",
              "maleVariant": "リアムはこうなるのを恐れてたわけか…",
              "stringId": "1946092555712385024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "殺すつもりなんてなかったの！ ほんとよ！？ 嘘じゃない！",
              "maleVariant": "",
              "stringId": "1946094520055943168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だとしても、リアムにとっちゃ何も変わらないでしょ",
              "maleVariant": "だとしても、リアムにとっちゃ何も変わらないだろ",
              "stringId": "1946095626798551040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なに？",
              "maleVariant": "なんだ？",
              "stringId": "1946101833596719104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "金は払うから、どこかに運んで燃やして",
              "maleVariant": "",
              "stringId": "1946105079535030272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう行って、V",
              "maleVariant": "",
              "stringId": "1946107579071778816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "会えて良かったわ、V。さあ、もう行って",
              "maleVariant": "",
              "stringId": "1946116410447286272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼と別れようとしたのよ。もう関わりたくないと伝えようとして… でも… それがどういうわけか、こんな…",
              "maleVariant": "",
              "stringId": "1946137041456910336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死体はどうするつもり？",
              "maleVariant": "死体はどうするつもりだ？",
              "stringId": "2079267954155757572"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}