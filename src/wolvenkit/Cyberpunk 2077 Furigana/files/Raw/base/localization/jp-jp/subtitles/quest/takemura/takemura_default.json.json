        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "調子はどう？",
                    "maleVariant": "調子はどうだ？",
                    "stringId": 1951904610263048196
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どこにいるの？",
                    "maleVariant": "どこにいるんだ？",
                    "stringId": 1951904927167881216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカの様子は？",
                    "maleVariant": null,
                    "stringId": 1951905708180840448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカの動きはつかめてる？",
                    "maleVariant": "アラサカの動きはつかめてるか？",
                    "stringId": 1951915942400040960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "テレビから推測できること以外には、何も",
                    "maleVariant": null,
                    "stringId": 1951921956964868096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それが真っ赤な嘘であるのは、火を見るより明らかだ",
                    "maleVariant": null,
                    "stringId": 1951922478451073024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヨリノブと奴の傀儡どもが微笑みながら、事態は終息に向かっていると訴えている",
                    "maleVariant": null,
                    "stringId": 1951923714294358016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今どこにいるの？",
                    "maleVariant": "今どこにいるんだ？",
                    "stringId": 1951924816289013760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティだ。ほかにどこがある",
                    "maleVariant": null,
                    "stringId": 1951925106954280960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "答えになってない",
                    "maleVariant": null,
                    "stringId": 1951925834414366720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前が敵に捕まった時のためにも、俺の正確な所在は知らない方がいいのではないか",
                    "maleVariant": null,
                    "stringId": 1951926438394142720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここにきてナイトシティを気に入ってきた。この街の地下は潜りやすい",
                    "maleVariant": null,
                    "stringId": 1951927198586576896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なるほど",
                    "maleVariant": null,
                    "stringId": 1951932843415117824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "虫どもとカビ、あとは飲み水がないことにさえ耐えられれば、文句はない",
                    "maleVariant": null,
                    "stringId": 1951933518647730176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "細かく説明しなくていい",
                    "maleVariant": null,
                    "stringId": 1951934810342367232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうか",
                    "maleVariant": null,
                    "stringId": 1951935805852033024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "元気にしてるの？",
                    "maleVariant": "元気にしてるのか？",
                    "stringId": 1951938283729395712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ。だが急にどうした？",
                    "maleVariant": null,
                    "stringId": 1951938625145741312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ただ聞いただけ。いけなかった？",
                    "maleVariant": "ただ聞いただけだ。いけなかったか？",
                    "stringId": 1951938928544915460
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたに死なれちゃ困るからよ",
                    "maleVariant": "あんたに死なれちゃ困るからな",
                    "stringId": 1951939359417376768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ただ聞いただけよ。いつも何か目的がないと話せないの？",
                    "maleVariant": "ただ聞いただけだ。いつも何か目的がないと話せないのか？",
                    "stringId": 1951941210145628160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたに死なれちゃ困るからよ。まさか、忘れたの？",
                    "maleVariant": "あんたに死なれちゃ困るからだ。まさか、忘れたのか？",
                    "stringId": 1951941270912704512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これはすまない、誤解させてしまったようだ。なんというか… そのような質問には慣れていないものでな",
                    "maleVariant": null,
                    "stringId": 1951941526631030784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もちろん覚えている",
                    "maleVariant": null,
                    "stringId": 1951942467514081280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ならいい。ナイトシティは風邪をひくより鉛玉をもらうことの方が多い街だからね",
                    "maleVariant": "ならいい。ナイトシティは風邪をひくより鉛玉をもらうことの方が多い街だからな",
                    "stringId": 1951943057652654080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この通り、生きてるし達者にしている。自分のことを心配した方がいい",
                    "maleVariant": null,
                    "stringId": 1951950331330764800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺みたいな輩は… 順調にやってるか、墓の中にいるかのどちらかしかない",
                    "maleVariant": null,
                    "stringId": 1951952431318114304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "パレードの件の準備は？",
                    "maleVariant": null,
                    "stringId": 1952009240347222016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほぼ完了している。後は機を待つだけだが、それが一番難しい",
                    "maleVariant": null,
                    "stringId": 1952009483365195776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これ以上はやめておこう。電話でするような話ではないからな",
                    "maleVariant": null,
                    "stringId": 1952011266967805952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ、話題を変えましょ",
                    "maleVariant": "じゃあ、話題を変えよう",
                    "stringId": 1952012209847345156
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "考えすぎじゃない？",
                    "maleVariant": "考えすぎじゃないか？",
                    "stringId": 1952012432011239424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "OK。話題を変えましょ",
                    "maleVariant": "OK。話題を変えよう",
                    "stringId": 1952016335750635520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "被害妄想がすぎるんじゃない？",
                    "maleVariant": "被害妄想がすぎるんじゃないか？",
                    "stringId": 1952016628982816768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "愚か者として死ぬより、被害妄想でも生きてる方がよかろう",
                    "maleVariant": null,
                    "stringId": 1953301660296880128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "“彼女”と話をした",
                    "maleVariant": null,
                    "stringId": 1953303342045011968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "直接じゃないけど、“彼女”と話をした。誰のことかはわかるよね",
                    "maleVariant": "直接じゃないが、“彼女”と話をした。誰のことかはわかるよな",
                    "stringId": 1953306251080048640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "実は、俺も連絡を頂いた",
                    "maleVariant": null,
                    "stringId": 1953307112522981376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "少々手荒な真似にはなったが、その甲斐はあったな",
                    "maleVariant": null,
                    "stringId": 1953307344300220416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まだ油断はできない",
                    "maleVariant": null,
                    "stringId": 1953308051224993796
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今どこにいるの？",
                    "maleVariant": "今どこにいるんだ？",
                    "stringId": 1953310512694239232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたの隠れ家は、今頃跡形もないでしょ。どこにいるの？",
                    "maleVariant": "あんたの隠れ家は、今頃跡形もないだろ。どこにいるんだ？",
                    "stringId": 1953311043189809152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "前よりはマシなところである、とだけ言っておこう。それ以上は勘弁してくれ",
                    "maleVariant": null,
                    "stringId": 1953314581924093952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "信じがたいけど、その通りね",
                    "maleVariant": "信じがたいが、その通りだな",
                    "stringId": 1953316565896994816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まだただの口約束よ。なんとも言えない",
                    "maleVariant": "まだただの口約束だ。なんとも言えない",
                    "stringId": 1953322292044701696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言葉の重みとは、発する人間によって異なる。ハナコ様の言葉には真実の重さがあると信じている",
                    "maleVariant": null,
                    "stringId": 1953322542243323904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まさかうまくいくとはね",
                    "maleVariant": "まさかうまくいくとはな",
                    "stringId": 1953361299256451072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "プロキシ経由で連絡してくる人間は信用しない",
                    "maleVariant": null,
                    "stringId": 1954522890559627264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "プロキシ経由で話す相手を信用していいものか",
                    "maleVariant": null,
                    "stringId": 1954523647111405568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オダはどうしてる？",
                    "maleVariant": null,
                    "stringId": 1954633385270648832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オダはどうしてる？ まだ治療中？",
                    "maleVariant": "オダはどうしてる？ まだ治療中か？",
                    "stringId": 1954638883114000384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカが最高の医療設備を用意したが、それで十分だという保証はない。なんせ、傷ついたプライドは骨よりも治りが遅いからな",
                    "maleVariant": null,
                    "stringId": 1954639672314241024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "また話しましょ",
                    "maleVariant": "また話そう",
                    "stringId": 1954645338936385536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "また話しましょ",
                    "maleVariant": "また後で話そう",
                    "stringId": 1954645992123736064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあな、V",
                    "maleVariant": null,
                    "stringId": 1954646769848360960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言っただろう",
                    "maleVariant": null,
                    "stringId": 1975314368131047424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "提案をなさっただろう",
                    "maleVariant": null,
                    "stringId": 1975327764469587968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "直接会う予定よ",
                    "maleVariant": "直接会う予定だ",
                    "stringId": 1975328997947285508
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ソウルキラーを探さないと",
                    "maleVariant": null,
                    "stringId": 1975329820366413824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よく知ってるのね",
                    "maleVariant": "よく知ってるな",
                    "stringId": 1975330905499967488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何でも知ってるのね。話を続ける意味ある？",
                    "maleVariant": "何でも知ってるんだな。話を続ける意味あるか？",
                    "stringId": 1975334357462491136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハナコ様が共有してもよいと判断された情報しか把握していない",
                    "maleVariant": null,
                    "stringId": 1975335006187102208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だから続けてくれ",
                    "maleVariant": null,
                    "stringId": 1975336246996443136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "近々、直接会う",
                    "maleVariant": null,
                    "stringId": 1975336674211471360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "素晴らしい。あの方の信頼を得たという何よりの証拠だな",
                    "maleVariant": null,
                    "stringId": 1975337225997328384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハナコの力を借りる前に、ソウルキラーを探さないといけない。彼女本人は動けないそうよ",
                    "maleVariant": "ハナコの力を借りる前に、ソウルキラーを探さないといけない。彼女本人は動けないそうだ",
                    "stringId": 1975339193008463872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたも来るの？",
                    "maleVariant": "あんたも来るのか？",
                    "stringId": 1975340735941910528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや。アラサカが今も俺を探してる以上、不用意な真似はできない",
                    "maleVariant": null,
                    "stringId": 1975341343562342400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なら早急に見つけださねばな",
                    "maleVariant": null,
                    "stringId": 1975343705727246336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ。カブキの露店で売ってるようなものじゃないけどね",
                    "maleVariant": "ああ。カブキの露店で売ってるようなものじゃないけどな",
                    "stringId": 1975344863288041472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "力にはなれんが、見つけ出すしかなかろう。お前の命がかかっているのだからな",
                    "maleVariant": null,
                    "stringId": 1975345636868694016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だが… V、それももうすぐ終わるだろう",
                    "maleVariant": null,
                    "stringId": 1975350485450563584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "他に何か？",
                    "maleVariant": null,
                    "stringId": 1975351673831104512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "準備は進んでる？",
                    "maleVariant": "準備は進んでるか？",
                    "stringId": 1975353812439605248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかってる",
                    "maleVariant": "思い出させるなよ",
                    "stringId": 1975361318012395520
                  }
                }
              ]
            }
          }
        }
      }
    },
