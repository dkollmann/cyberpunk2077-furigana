    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1541544623217430528,
            "femaleVariant": "着いたよ"
          },
          {
            "stringId": 1679604612049297408,
            "femaleVariant": "その逆よ",
            "maleVariant": "その逆だ"
          },
          {
            "stringId": 1679604612737163264,
            "femaleVariant": "まったくもって、その逆よ。今までにないくらい、生き生きしてる",
            "maleVariant": "まったくもって、その逆だ。今までにないくらい、生き生きしてる"
          },
          {
            "stringId": 1679604738817941504,
            "femaleVariant": "フン、ホントに来るとはね"
          },
          {
            "stringId": 1679604860637306880,
            "femaleVariant": "州境を越えた後はどうするの？",
            "maleVariant": "州境を越えた後はどうする？"
          },
          {
            "stringId": 1679604860670861312,
            "femaleVariant": "わかった。で、州境を越えた後はどうするの？",
            "maleVariant": "わかった。で、州境を越えた後はどうするんだ？"
          },
          {
            "stringId": 1695483831905996800,
            "femaleVariant": "ジュディ。来たのね",
            "maleVariant": "ジュディ。来たな"
          },
          {
            "stringId": 1695494872673079296,
            "femaleVariant": "まだやることが残ってる。すぐに済むから"
          },
          {
            "stringId": 1695494911076126720,
            "femaleVariant": "そうね、行きましょ",
            "maleVariant": "そうだな、行こう"
          },
          {
            "stringId": 1695499359219404800,
            "femaleVariant": "ええ、キャロルに乗せてもらった。こんなとこ、一人じゃ見つけられないよ"
          },
          {
            "stringId": 1695506785234636800,
            "femaleVariant": "あたしにもあんたが必要なの",
            "maleVariant": "俺にもあんたが必要なんだ"
          },
          {
            "stringId": 1695548721681190912,
            "femaleVariant": "わかった。ここで待ってる"
          },
          {
            "stringId": 1695655639925968896,
            "femaleVariant": "リバー！"
          },
          {
            "stringId": 1695659043352006656,
            "femaleVariant": "よう。元気そうだな、V"
          },
          {
            "stringId": 1695667537572581376,
            "femaleVariant": "必要なのは本当に俺か？ いや、そうは思わない"
          },
          {
            "stringId": 1695667706720473088,
            "femaleVariant": "お前に必要なのは、家族… それに新たなスタートだ"
          },
          {
            "stringId": 1699732726282506240,
            "femaleVariant": "よし、嵐が来る前にさっさと出発だ"
          },
          {
            "stringId": 1699795518956032000,
            "femaleVariant": "じゃあ改めて計画を確認させて",
            "maleVariant": "じゃあ改めて計画を確認させてくれ"
          },
          {
            "stringId": 1699797694122086400,
            "femaleVariant": "アルデカルドスが秘密裏に掘り進めたっていう、あの？"
          },
          {
            "stringId": 1699798311926288384,
            "femaleVariant": "誰かに気づかれる可能性はないの？",
            "maleVariant": "誰かに気づかれる可能性はないのか？"
          },
          {
            "stringId": 1699811868990558208,
            "femaleVariant": "前提として、検問所は通らない。それはいいな？"
          },
          {
            "stringId": 1699812104039354368,
            "femaleVariant": "密輸用の古いトンネルを使う。場所は掴んでる"
          },
          {
            "stringId": 1699813396505743360,
            "femaleVariant": "合流地点で仲間が待ってる。まずはそこまで行ってから、全員でツーソンに向かう"
          },
          {
            "stringId": 1699814172317765632,
            "femaleVariant": "そう。第一次企業戦争直後に掘られたヤツらしいよ"
          },
          {
            "stringId": 1708131366101372928,
            "femaleVariant": "ありがとう"
          },
          {
            "stringId": 1708131366118150156,
            "femaleVariant": "感謝してもしきれない"
          },
          {
            "stringId": 1708131366235590656,
            "femaleVariant": "あんたたちには、世話になりっぱなしね",
            "maleVariant": "あんたらには、世話になりっぱなしだな"
          },
          {
            "stringId": 1708131366235590668,
            "femaleVariant": "水くさい。旅は道連れ世は情けよ"
          },
          {
            "stringId": 1708150680418308096,
            "femaleVariant": "そこまで行けばこっちのもの、か"
          },
          {
            "stringId": 1708152511651434496,
            "femaleVariant": "地の利はあるし、何より、アリゾナには昔の貸しがある奴らがいっぱいいるから"
          },
          {
            "stringId": 1708153420322230272,
            "femaleVariant": "州境警備隊はいずれ気づくだろうね"
          },
          {
            "stringId": 1708162867371233280,
            "femaleVariant": "それに、おかげでアラサカから空母が作れるくらいのテックをごっそり盗めたんだ"
          },
          {
            "stringId": 1708163142500798464,
            "femaleVariant": "今じゃ私たちも、あんたに負けないほどの賞金首よ"
          },
          {
            "stringId": 1714234858293481472,
            "femaleVariant": "[立つ] あたしは逃げてるとでも？",
            "maleVariant": "[立つ] 俺は逃げてるとでも？"
          },
          {
            "stringId": 1714234858293481488,
            "femaleVariant": "[立つ] あの街に身を捧げる意味がある？",
            "maleVariant": "[立つ] あの街に身を捧げる意味があるか？"
          },
          {
            "stringId": 1714234858343813120,
            "femaleVariant": "あたしは逃げてるって、そう言いたいの？",
            "maleVariant": "俺は逃げてるって、そう言いたいのか？"
          },
          {
            "stringId": 1714234858360590344,
            "femaleVariant": "違う。そうじゃない"
          },
          {
            "stringId": 1714234858410921984,
            "femaleVariant": "あの街に身を捧げたって、意味なんかないでしょ？",
            "maleVariant": "あの街に身を捧げたって、意味なんかないだろ？"
          },
          {
            "stringId": 1714281276739735552,
            "femaleVariant": "ああ、俺は残る。残らないといけない"
          },
          {
            "stringId": 1714281724188086272,
            "femaleVariant": "妹のことがある。あいつには…"
          },
          {
            "stringId": 1714281724188086284,
            "femaleVariant": "もう大人でしょ",
            "maleVariant": "もう大人だろ"
          },
          {
            "stringId": 1714281724188086296,
            "femaleVariant": "ああ。でもあいつは独りじゃダメなんだ。それに…"
          },
          {
            "stringId": 1714281724204863488,
            "femaleVariant": "それに？"
          },
          {
            "stringId": 1714281724204863500,
            "femaleVariant": "ナイトシティは… 俺の街だ"
          },
          {
            "stringId": 1714281724238417920,
            "femaleVariant": "だと思った…"
          },
          {
            "stringId": 1714281724255195148,
            "femaleVariant": "ナイトシティを選ぶのね",
            "maleVariant": "ナイトシティを選ぶのか"
          },
          {
            "stringId": 1714281724305526784,
            "femaleVariant": "だと思った。正直言うと、きっとそうだろうと思いつつも、それを信じたくない自分がいたの",
            "maleVariant": "だと思った。正直言うと、きっとそうだろうと思いつつも、それを信じたくない自分がいたんだ"
          },
          {
            "stringId": 1714281724305526796,
            "femaleVariant": "ホントか？"
          },
          {
            "stringId": 1714281724355858432,
            "femaleVariant": "ナイトシティを選ぶのね",
            "maleVariant": "ナイトシティを選ぶのか"
          },
          {
            "stringId": 1714283694386892800,
            "femaleVariant": "ホントよ",
            "maleVariant": "ホントさ"
          },
          {
            "stringId": 1714283825903489024,
            "femaleVariant": "なら、わかってくれるのか"
          },
          {
            "stringId": 1714283873081020416,
            "femaleVariant": "そうは言ってない"
          },
          {
            "stringId": 1714284392319078400,
            "femaleVariant": "一緒には行けない。無理なんだ"
          },
          {
            "stringId": 1714290669027905552,
            "femaleVariant": "ハッピーエンドにならないのはわかってたろ。俺も、お前もな"
          },
          {
            "stringId": 1714290669027905564,
            "femaleVariant": "そうかも… しれない"
          },
          {
            "stringId": 1715481961439219712,
            "femaleVariant": "誰かに気付かれる心配は？"
          },
          {
            "stringId": 1715493531946897408,
            "femaleVariant": "連中が動く頃には、俺たちは遥か彼方さ。それに、運よく例の嵐も来てる。痕跡は残らない"
          },
          {
            "stringId": 1715493908041748480,
            "femaleVariant": "嵐だけでコーポの追跡を振り切れればいいけど"
          },
          {
            "stringId": 1715494030431539200,
            "femaleVariant": "あたしたちなんかに構ってる暇なんてないはず。ま、どう転んでも捕まりっこないけどね",
            "maleVariant": "俺たちなんかに構ってる暇なんてないはずだ。ま、どう転んでも捕まりっこないがな"
          },
          {
            "stringId": 1715494590807330816,
            "femaleVariant": "どこも塞がってないよ。私たちが通るまで持ちこたえてくれれば、後は崩れても構わない。もう使うことはないからね"
          },
          {
            "stringId": 1715497624681631744,
            "femaleVariant": "ありがとう"
          },
          {
            "stringId": 1715501117798666240,
            "femaleVariant": "よし"
          },
          {
            "stringId": 1721406334169931776,
            "maleVariant": "よう、来たな"
          },
          {
            "stringId": 1721407699030007808,
            "maleVariant": "当たり前だろ？"
          },
          {
            "stringId": 1731412655457751040,
            "femaleVariant": "中も調査済みなの？ 途中で塞がってたりしない保証は？",
            "maleVariant": "中も調査済みなのか？ 途中で塞がってたりしない保証は？"
          },
          {
            "stringId": 1731427100238073860,
            "femaleVariant": "[en_us][pl_pl]Mogłem się tego spodziewać.",
            "maleVariant": "なんとなく察してた"
          },
          {
            "stringId": 1731427184526807040,
            "maleVariant": "なんとなく気づいてた"
          },
          {
            "stringId": 1731550074513444864,
            "femaleVariant": "[en_us][pl_pl]O czym ty mówisz?",
            "maleVariant": "ふざけんな！"
          },
          {
            "stringId": 1731550136891133952,
            "maleVariant": "なんでだよ？ 何言ってんだ！"
          },
          {
            "stringId": 1732690442671935488,
            "maleVariant": "お前は新たなスタートを切ることを選んだ。それは承知の上だし、時間がないことも理解してる… ほんとさ。でも…"
          },
          {
            "stringId": 1732690610058219520,
            "femaleVariant": "[en_us]So all that chat about the loops of life, what was that?",
            "maleVariant": "“生の円環”がどうとか言ってたのは何だったんだ？"
          },
          {
            "stringId": 1732690610058219536,
            "femaleVariant": "[en_us]I do. Just... Night City's a closed book to me.",
            "maleVariant": "わかるよ。ナイトシティにまだ用があるんだろ"
          },
          {
            "stringId": 1732690610108551168,
            "maleVariant": "帰って来る必要はないだろ？ “生の円環”ってのは何だったんだ？"
          },
          {
            "stringId": 1732691275576823808,
            "femaleVariant": "[en_us]Just go.",
            "maleVariant": "もう行ってくれ"
          },
          {
            "stringId": 1732691275576823824,
            "femaleVariant": "[en_us]I understand it all.",
            "maleVariant": "よくわかったよ"
          },
          {
            "stringId": 1732691275610378240,
            "maleVariant": "じゃあ… もう行けよ"
          },
          {
            "stringId": 1732696322113064960,
            "maleVariant": "会えて嬉しいよ"
          },
          {
            "stringId": 1732701206933721088,
            "maleVariant": "俺はそんなに意外性がないか？"
          },
          {
            "stringId": 1732701514980184064,
            "maleVariant": "それだけ… あんたのことを理解してるってだけだ"
          },
          {
            "stringId": 1732701643527213056,
            "maleVariant": "何もかもを捨てるなんて、俺には無理だ。わかるだろ"
          },
          {
            "stringId": 1732701834552594432,
            "maleVariant": "今はまだ、街を離れられない。離れるわけにはいかないんだ"
          },
          {
            "stringId": 1732703315729108992,
            "maleVariant": "楽しむだけ楽しんで、後片付けは他人任せ――ケリー・ユーロダインの人間性がよくわかったよ"
          },
          {
            "stringId": 1732703457496584192,
            "maleVariant": "仕方ないだろ！"
          },
          {
            "stringId": 1732708863769890816,
            "maleVariant": "俺にはまだあそこでやることがある"
          },
          {
            "stringId": 1732712917128499200,
            "maleVariant": "円環は、ひとりひとりにあるものだ。それぞれが、異なる輪を持っている"
          },
          {
            "stringId": 1732713112968941568,
            "maleVariant": "俺たちの輪は、少しの間絡み合っただけだ"
          },
          {
            "stringId": 1732713251347419136,
            "maleVariant": "少しかよ"
          },
          {
            "stringId": 1732714604664770560,
            "maleVariant": "お前の気持ちはわかる。俺だって2人でやってけると思ってた。でもそれは、ナイトシティが見せる幻、掴むことのできない夢に過ぎない"
          },
          {
            "stringId": 1732714706385031168,
            "maleVariant": "掴んだと思ったんだがな"
          },
          {
            "stringId": 1732714822735024128,
            "maleVariant": "俺もだよ"
          },
          {
            "stringId": 1732717371496124416,
            "maleVariant": "もし… ナイトシティに来ることがあったら、連絡を寄こせよ？ いいな？"
          },
          {
            "stringId": 1744630061600657408,
            "femaleVariant": "ミッチ"
          },
          {
            "stringId": 1744630061600657424,
            "femaleVariant": "よう。さっさとここを出発しようぜ"
          },
          {
            "stringId": 1744632579508457472,
            "femaleVariant": "準備はどう？ 全部整ってる？"
          },
          {
            "stringId": 1744634288989327360,
            "femaleVariant": "あと少しで完了だ"
          },
          {
            "stringId": 1744634417200812032,
            "femaleVariant": "じゃあ見つかったんだね？ あの場所にあったの？"
          },
          {
            "stringId": 1744635066948833280,
            "femaleVariant": "うん、そういうこと"
          },
          {
            "stringId": 1751748719812595712,
            "femaleVariant": "[立つ] あたしにもあんたが必要なの",
            "maleVariant": "[立つ] 俺にもあんたが必要なんだ"
          },
          {
            "stringId": 1751889133987221504,
            "femaleVariant": "俺もよく考えたんだ。あれだけのことがあれば、離れたくなる気持ちもよくわかる"
          },
          {
            "stringId": 1751889302396915712,
            "femaleVariant": "お前には人生をやり直す資格がある。他の誰よりも"
          },
          {
            "stringId": 1751889739124625408,
            "femaleVariant": "だが周りを見てみろ。お前は家族を手に入れた。俺も家族といて、支えてやりたいんだ"
          },
          {
            "stringId": 1751890289132097536,
            "femaleVariant": "俺が自信を持ってできることは… それしかない"
          },
          {
            "stringId": 1755969689435820032,
            "femaleVariant": "元気でな"
          },
          {
            "stringId": 1755969884303183872,
            "femaleVariant": "あんたもね",
            "maleVariant": "あんたもな"
          },
          {
            "stringId": 1763490187304554496,
            "femaleVariant": "リバー、何が言いたいの？ ナイトシティがあんたに何をしてくれたっていうのよ",
            "maleVariant": "リバー、何が言いたいんだ？ ナイトシティがあんたに何をしてくれたっていうんだよ"
          },
          {
            "stringId": 1763493646313508864,
            "femaleVariant": "街のことじゃない。そこに住む人々のことだ。俺には彼らへの責任がある。務めを果たすと誓ったんだ… 破るわけには… 逃げ出すわけにはいかない"
          },
          {
            "stringId": 1800812826630971392,
            "femaleVariant": "改めて計画を確認させて",
            "maleVariant": "改めて計画を確認させてくれ"
          },
          {
            "stringId": 1825519533875130368,
            "femaleVariant": "州境沿いの防衛システムはどうする？",
            "maleVariant": "州境沿いの防衛システムはどうするんだ？"
          },
          {
            "stringId": 1825521502010339328,
            "femaleVariant": "トンネルはいいとして、州境付近は監視が厳しい。そもそもどうやって辿り着くの？",
            "maleVariant": "トンネルはいいとして、州境付近は監視が厳しい。そもそもどうやって辿り着く？"
          },
          {
            "stringId": 1825544315752542208,
            "femaleVariant": "大丈夫だよ。対策は考えてるし、キャロルとキャシディがコーポの連中を追い払ってくれる"
          },
          {
            "stringId": 1825547876699860992,
            "femaleVariant": "じゃあ、心配は何もいらないってことね",
            "maleVariant": "じゃあ、心配は何もいらないってことだな"
          },
          {
            "stringId": 1825549250820960256,
            "femaleVariant": "今日からは一人で全部背負わなくていいんだよ。仲間がいる。Vも、きっとすぐ慣れる"
          },
          {
            "stringId": 1825579918397403136,
            "maleVariant": "いよいよ、お前の大冒険が始まるわけだな。しかし何だ… この風景は、なんつうか、世界の終わりみたいに見えないか"
          },
          {
            "stringId": 1825587999630028800,
            "maleVariant": "俺のためでもか"
          },
          {
            "stringId": 1825591582756827140,
            "femaleVariant": "[en_us][pl_pl]I zrobisz to beze mnie?",
            "maleVariant": "俺抜きでやるのかよ？"
          },
          {
            "stringId": 1825591891709259776,
            "femaleVariant": "[en_us][pl_pl]Rozumiem. Ja już pożegnałem Night City.",
            "maleVariant": "わかった。まだやり残したことがあるんだな"
          },
          {
            "stringId": 1825592671547813888,
            "maleVariant": "俺抜きでやるってんだな？ わかった。好きにしろ"
          },
          {
            "stringId": 1825592759158435840,
            "maleVariant": "わかったよ、ケリー。よくわかった"
          },
          {
            "stringId": 1825594136400097280,
            "maleVariant": "そういう言い方はないだろ！ 俺はただ…"
          },
          {
            "stringId": 1825595192542621696,
            "maleVariant": "ケリー、たった数ヶ月の話だろ。それすら待てないって言うのか？ お前のやるツアーの方が長いんだぞ"
          },
          {
            "stringId": 1825595727400267776,
            "maleVariant": "ああ、もう、クッソ… んなこたわかってる！ V、怖いんだ！ この先どうなるかも、こんな状況で街を離れることも！"
          },
          {
            "stringId": 1825596442260664320,
            "maleVariant": "今お前と旅立ったら、俺は独りで帰って来るんだぞ…？ そんなの… 俺には耐えられない… 無理だ"
          },
          {
            "stringId": 1825597275870195712,
            "maleVariant": "頼む。頼むから… わかってくれ"
          },
          {
            "stringId": 1825603207941451776,
            "maleVariant": "本当か？"
          },
          {
            "stringId": 1825603443359346688,
            "maleVariant": "俺はもうナイトシティに別れを告げた。だから次に向かえる。でもお前の物語はまだ終わってないんだろ"
          },
          {
            "stringId": 1825604777248681984,
            "maleVariant": "あのアルバムを仕上げなくちゃいけないんだ。完成したら… 聴いてくれるよな？ 世界中に響かせる前に… 一番にさ"
          },
          {
            "stringId": 1825607372549779456,
            "maleVariant": "ああ、もちろん。ただ… 急いでくれよ"
          },
          {
            "stringId": 1825608676491784192,
            "maleVariant": "ああ、約束する"
          },
          {
            "stringId": 1825609996539916288,
            "maleVariant": "ああ、そうする"
          },
          {
            "stringId": 1825640702804619264,
            "femaleVariant": "そして俺には俺の家族がいる"
          },
          {
            "stringId": 1825641730006446080,
            "femaleVariant": "元気でね、リバー",
            "maleVariant": "元気でな、リバー"
          },
          {
            "stringId": 1825698198541955072,
            "femaleVariant": "今にして思えば、私はナイトシティに馴染めたことなんて一度もなかったのかも"
          },
          {
            "stringId": 1825698198541955092,
            "femaleVariant": "いつか、あそこが自分の居場所になる日が来るって思ってた… 何の危険もない、居心地のいい場所にね"
          },
          {
            "stringId": 1825698198827167744,
            "femaleVariant": "逃げてなんかない。これは新しいチャンスよ"
          },
          {
            "stringId": 1825698198843944976,
            "femaleVariant": "その気持ちはわかる"
          },
          {
            "stringId": 1825698198877499396,
            "femaleVariant": "逃げてなんかない。またここから、人生の新たなステージが始まるの",
            "maleVariant": "逃げてなんかない。またここから、人生の新たなステージが始まるんだ"
          },
          {
            "stringId": 1825698198894276620,
            "femaleVariant": "あなたがそんなこと言うなんてね"
          },
          {
            "stringId": 1825698198978162692,
            "femaleVariant": "後ろめたさでしょ。街を裏切ってるような気が、そして街もそれに気付いてる気がしてならない",
            "maleVariant": "後ろめたさだろ。街を裏切ってるような気が、そして街もそれに気付いてる気がしてならない"
          },
          {
            "stringId": 1825698198994939908,
            "femaleVariant": "そう。離れたいはずなのに、引き戻されそうになる。まるで私のためだけの、甘い約束を囁かれてるみたい"
          },
          {
            "stringId": 1825698199045271556,
            "femaleVariant": "前はよく、自分の人生を色んなステージに区切って考えてた"
          },
          {
            "stringId": 1825698199062048776,
            "femaleVariant": "メガビルディングのボロ部屋、グループホーム暮らし、それからモックスへ…"
          },
          {
            "stringId": 1825698199078825984,
            "femaleVariant": "毎回、居場所を見つけたって思った。でもいつも、あっけなく裏切られる"
          },
          {
            "stringId": 1825698199112380436,
            "femaleVariant": "今回はそうならない"
          },
          {
            "stringId": 1825698199112380456,
            "femaleVariant": "だからこそ旅立つのよ"
          },
          {
            "stringId": 1825698199213043716,
            "femaleVariant": "心配いらないよ、居場所はきっとすぐに見つかる",
            "maleVariant": "心配いらないさ、居場所はきっとすぐに見つかる"
          },
          {
            "stringId": 1825698199263375364,
            "femaleVariant": "今回はそうならない。きっと",
            "maleVariant": "今回はそうならない。きっとな"
          },
          {
            "stringId": 1825698199263375384,
            "femaleVariant": "約束よ？"
          },
          {
            "stringId": 1825698199313707008,
            "femaleVariant": "まさに、ナイトシティの縮図って感じね",
            "maleVariant": "まさに、ナイトシティの縮図って感じだな"
          },
          {
            "stringId": 1825698199313707028,
            "femaleVariant": "ええ、身に染みてわかった。だから… 私は街を出る。もう何の迷いもない"
          },
          {
            "stringId": 1825698199632474112,
            "femaleVariant": "もうすぐ出発よ",
            "maleVariant": "もうすぐ出発だぞ"
          },
          {
            "stringId": 1825698199666028548,
            "femaleVariant": "嵐が来る前に出発しましょ。パナムの機嫌も損ねたくないしね",
            "maleVariant": "嵐が来る前に出発しよう。パナムの機嫌も損ねたくないしな"
          },
          {
            "stringId": 1825698199699582976,
            "femaleVariant": "ええ、キャロルからもいくつか忠告された"
          },
          {
            "stringId": 1825698199699582988,
            "femaleVariant": "きっと仲良くなれるよ",
            "maleVariant": "きっと仲良くなれるさ"
          },
          {
            "stringId": 1825729754237104128,
            "femaleVariant": "約束はしないよ。その必要がないから",
            "maleVariant": "約束はしない。その必要がないからな"
          },
          {
            "stringId": 1825733944028033024,
            "femaleVariant": "もっと確かなものを見つけなきゃ――自分の… あたしたちのために",
            "maleVariant": "もっと確かなものを見つけなきゃ――自分の… 俺たちのために"
          },
          {
            "stringId": 1825737857967976448,
            "femaleVariant": "Vが言うなら… きっとそうだね"
          },
          {
            "stringId": 1825762974383075328,
            "femaleVariant": "でしょ？ 二人なら大丈夫よ",
            "maleVariant": "だろ？ 二人なら大丈夫さ"
          },
          {
            "stringId": 1825767797446799360,
            "femaleVariant": "じゃ… 行こうか"
          },
          {
            "stringId": 1825896226129309700,
            "femaleVariant": "ナイトシティを出ないと思った？",
            "maleVariant": "ナイトシティを出ないと思ったか？"
          },
          {
            "stringId": 1825898571097567232,
            "femaleVariant": "なに、がっかりした？",
            "maleVariant": "なんだ、がっかりしたか？"
          },
          {
            "stringId": 1825899131574022144,
            "femaleVariant": "ナイトシティから一生出ないと思ってた？",
            "maleVariant": "ナイトシティから一生出ないと思ってたか？"
          },
          {
            "stringId": 1825899279867834368,
            "femaleVariant": "なによ、がっかりした？",
            "maleVariant": "なんだ、がっかりしたか？"
          },
          {
            "stringId": 1825900011018907648,
            "femaleVariant": "新しくやりなおそうとか、そういうこと考えそうにないから"
          },
          {
            "stringId": 1825906005434298368,
            "femaleVariant": "残念ながら、勉強不足ね",
            "maleVariant": "残念ながら、勉強不足だな"
          },
          {
            "stringId": 1825915177555062784,
            "femaleVariant": "それは今後次第ね"
          },
          {
            "stringId": 1825918104843710464,
            "femaleVariant": "「その子供は、青白く痩せこけていた…」"
          },
          {
            "stringId": 1825926060884750336,
            "femaleVariant": "どうぞ"
          },
          {
            "stringId": 1825926629833699328,
            "femaleVariant": "なんて顔をしてんだ。この雄大な自然のどこに不満がある？"
          },
          {
            "stringId": 1825929546586255360,
            "femaleVariant": "死にかけてるから、かな"
          },
          {
            "stringId": 1825931593725374464,
            "femaleVariant": "あんたは雄大な自然に元気をもらったようね",
            "maleVariant": "あんたは雄大な自然に元気をもらったようだな"
          },
          {
            "stringId": 1825933195563626496,
            "femaleVariant": "もうすぐ死ぬから。そのせいかもね",
            "maleVariant": "もうすぐ死ぬから、そのせいかもな"
          },
          {
            "stringId": 1825933340787208192,
            "femaleVariant": "あんたに会うといつも褒められてばっかりね",
            "maleVariant": "あんたに会うといつも褒められてばっかりだな"
          },
          {
            "stringId": 1825933913712357376,
            "femaleVariant": "ハハハ、若い時はのんびり構えてるのが一番だ"
          },
          {
            "stringId": 1825935099928637440,
            "femaleVariant": "人はすぐに不安になるが、不安ってのは真実を覆っちまうんだ。ガス欠になるのを心配してたら本当になるし、もうすぐ死ぬんじゃないかと思ってたら、その日は近い"
          },
          {
            "stringId": 1825937092072042496,
            "femaleVariant": "さあ、早く支度をしろ。州境を越えなきゃならないうえに、嵐が迫って来てる。忙しい一日になるぞ"
          },
          {
            "stringId": 1825938166434623488,
            "femaleVariant": "いつ会っても元気だけど、それも雄大な自然と新鮮な空気のおかげ？",
            "maleVariant": "いつ会っても元気だが、それも雄大な自然と新鮮な空気のおかげか？"
          },
          {
            "stringId": 1825939420665737216,
            "femaleVariant": "空気は関係ない。ある信念を貫いてるだけだ。おかげで俺は、夜になれば赤子のようにぐっすり眠り、朝が来れば清々しい気持ちで目覚められる"
          },
          {
            "stringId": 1825941386519244800,
            "femaleVariant": "で、その信念は何なの",
            "maleVariant": "で、その信念は何なんだ"
          },
          {
            "stringId": 1825941785313669120,
            "femaleVariant": "俺たちノーマッドは自ら選んだ相手と家族を作る。そうして、選ぶことで固い絆が生まれるんだ。古い樫の木のように、固く揺るぎない絆がな"
          },
          {
            "stringId": 1825952146318290944,
            "femaleVariant": "ええ。それが家族よね",
            "maleVariant": "ああ。それが家族だろ"
          },
          {
            "stringId": 1825953282102259712,
            "femaleVariant": "この先どんな苦難が待ち受けようと、何が起ころうと、少なくともこの家族だけは、決してお前を見捨てたりしない。わかるか？"
          },
          {
            "stringId": 1826901574122336256,
            "femaleVariant": "V、正直に言ってもいいか"
          },
          {
            "stringId": 1826907205478772736,
            "femaleVariant": "ふむ。だがいいか、V。ここで生き死にを語ってるお前は、紛れもなく生きてる"
          },
          {
            "stringId": 1826908379766452224,
            "femaleVariant": "そいつを忘れるな"
          },
          {
            "stringId": 1826909146015461376,
            "femaleVariant": "ええ、覚えとく",
            "maleVariant": "ああ、覚えとく"
          },
          {
            "stringId": 1826915385042661376,
            "femaleVariant": "何か手伝えることは？"
          },
          {
            "stringId": 1826916443181674496,
            "femaleVariant": "何か手伝える？",
            "maleVariant": "何か手伝えるか？"
          },
          {
            "stringId": 1826917465149009920,
            "femaleVariant": "大丈夫、準備は終わった。むしろ、あんたを待ってたところだよ"
          },
          {
            "stringId": 1845795462312488960,
            "femaleVariant": "今のところ計画通りってこと？",
            "maleVariant": "今のところ計画通りってことか？"
          },
          {
            "stringId": 1845799787881426944,
            "femaleVariant": "[抱きしめる] でもそれは幻想に過ぎなかった"
          },
          {
            "stringId": 1845799902385926144,
            "femaleVariant": "でもそれは幻想に過ぎなかった",
            "maleVariant": "だがそれは幻想に過ぎなかった"
          },
          {
            "stringId": 1845800515794497536,
            "femaleVariant": "[抱きしめる] 居場所は見つかる"
          },
          {
            "stringId": 1845801512646340608,
            "femaleVariant": "ええ。ここから眺めると、何も悪意のない街に見えるのに… なんでだろ… まるで何かから逃げてるみたいな気がする"
          },
          {
            "stringId": 1845802885391708160,
            "femaleVariant": "本当の居場所はこれから見つければいい"
          },
          {
            "stringId": 1845803243803373568,
            "femaleVariant": "自分はもう見つけたからって"
          },
          {
            "stringId": 1845803613288001536,
            "femaleVariant": "あんたにだって、きっと見つかる"
          },
          {
            "stringId": 1845803923565834240,
            "femaleVariant": "なら、逃げてるみたいなこの気持ちは、いったい何？"
          },
          {
            "stringId": 1845807971522179076,
            "femaleVariant": "その約束の先には何もない"
          },
          {
            "stringId": 1845808161809362944,
            "femaleVariant": "忘れるのよ"
          },
          {
            "stringId": 1845808932168790016,
            "femaleVariant": "ジュディ、その約束の先には何もないのよ",
            "maleVariant": "ジュディ、その約束の先には何もないんだ"
          },
          {
            "stringId": 1845809839027646464,
            "femaleVariant": "もう忘れるのよ",
            "maleVariant": "もう忘れるんだ"
          },
          {
            "stringId": 1845810747128016896,
            "femaleVariant": "でもどうやって？"
          },
          {
            "stringId": 1845818455101140996,
            "femaleVariant": "準備はいい？"
          },
          {
            "stringId": 1845818566317305856,
            "femaleVariant": "準備はできてる？",
            "maleVariant": "準備はできてるか？"
          },
          {
            "stringId": 1845819589945585664,
            "femaleVariant": "もちろん。いざまとめてみると、案外持ってく物は少なかった…"
          },
          {
            "stringId": 1845820875365543936,
            "femaleVariant": "どうかした？",
            "maleVariant": "どうかしたか？"
          },
          {
            "stringId": 1845821464296157184,
            "femaleVariant": "気が変わらなくてよかった"
          },
          {
            "stringId": 1845833987246387200,
            "femaleVariant": "来てくれて嬉しいよ。気が変わったらどうしようかと"
          },
          {
            "stringId": 1845834455079055360,
            "femaleVariant": "先に決心してたのは私のほうなんだけどね"
          },
          {
            "stringId": 1845836216334413824,
            "femaleVariant": "ただ…"
          },
          {
            "stringId": 1845841723623337984,
            "femaleVariant": "あんたの嘘はすぐわかる"
          },
          {
            "stringId": 1845842244891439104,
            "femaleVariant": "ようやく落ち着いたからね",
            "maleVariant": "ようやく落ち着いたからな"
          },
          {
            "stringId": 1845843314489290752,
            "femaleVariant": "あんたの嘘はすぐわかる。警官にしては、感情を隠すのが下手なんじゃない？",
            "maleVariant": "あんたの嘘はすぐわかる。警官にしては、感情を隠すのが下手なんじゃないか？"
          },
          {
            "stringId": 1845844488357539840,
            "femaleVariant": "お前には隠さないだけさ。必要ないからな"
          },
          {
            "stringId": 1845845865968300032,
            "femaleVariant": "ようやく落ち着いたから。全てを置いて旅立てる",
            "maleVariant": "ようやく落ち着いたからな。全てを置いて旅立てる"
          },
          {
            "stringId": 1845846741856415744,
            "femaleVariant": "誰もかもを、な"
          },
          {
            "stringId": 1845847204102270976,
            "femaleVariant": "あんたも心を決めたみたいね",
            "maleVariant": "あんたも心を決めたようだな"
          },
          {
            "stringId": 1845848453149224960,
            "femaleVariant": "だから、言わなくてもわかる。考えは… 変わらないんでしょ",
            "maleVariant": "だから、言わなくてもわかる。考えは変わらないんだろ"
          },
          {
            "stringId": 1845857512594206720,
            "femaleVariant": "ナイトシティが人をどんな目に遭わせるかは、十分わかってる"
          },
          {
            "stringId": 1845859641136377860,
            "femaleVariant": "がんばってね、リバー",
            "maleVariant": "がんばれよ、リバー"
          },
          {
            "stringId": 1845859887425908736,
            "femaleVariant": "[抱きしめる] いつまでも忘れない"
          },
          {
            "stringId": 1845861434268446720,
            "femaleVariant": "じゃあ、さよならね",
            "maleVariant": "さよならだな"
          },
          {
            "stringId": 1845862814647447552,
            "femaleVariant": "これで最後ね。いつまでも忘れない",
            "maleVariant": "これで最後だな。いつまでも忘れない"
          },
          {
            "stringId": 1845869136688312320,
            "femaleVariant": "[en_us]\"Your\" adventure? Not \"ours\"?",
            "maleVariant": "“俺たちの”冒険じゃないのか？"
          },
          {
            "stringId": 1845869636146032640,
            "femaleVariant": "[en_us]Kerry, I know you. Something's wrong.",
            "maleVariant": "どうしたんだ。様子がおかしいぞ"
          },
          {
            "stringId": 1845870147918229504,
            "maleVariant": "“俺たちの”冒険、じゃないのかよ？"
          },
          {
            "stringId": 1845870272623276032,
            "maleVariant": "何かあるなら言えよ、ケリー"
          },
          {
            "stringId": 1845871858137296896,
            "maleVariant": "別に、何でもない。何があるってんだ… ただ、その… クソ…"
          },
          {
            "stringId": 1845872875390234624,
            "maleVariant": "相変わらずわかりやすい奴だな。何か隠してるのがバレバレだぞ。言えよ、出発の時間も迫ってる"
          },
          {
            "stringId": 1845873767720022016,
            "maleVariant": "V… 俺は残る。すまない"
          },
          {
            "stringId": 1845875650224320512,
            "maleVariant": "ああ、いや、まあ… その…"
          },
          {
            "stringId": 1845876687458922496,
            "maleVariant": "ケリー？"
          },
          {
            "stringId": 1845877873037668352,
            "maleVariant": "てっきり一緒に来てくれるもんだと… 俺の勘違いか？"
          },
          {
            "stringId": 1845878301846532096,
            "maleVariant": "クソ。こんなに言いにくいとはな"
          },
          {
            "stringId": 1845879738831859712,
            "maleVariant": "やっとあの街の首根っこを掴んだんだ。今はまだ、手を離したくないんだよ"
          },
          {
            "stringId": 1845881629808652288,
            "maleVariant": "V。ナイトシティは確かにろくでもない掃き溜めだ。でも俺が一番輝ける掃き溜めなんだ。わかってくれ"
          },
          {
            "stringId": 1845895649521430532,
            "femaleVariant": "有名な“アルデカルドスのトンネル”？",
            "maleVariant": "有名な“アルデカルドスのトンネル”か？"
          },
          {
            "stringId": 1845896613204717568,
            "femaleVariant": "トンネルのことは初耳",
            "maleVariant": "トンネルのことは初耳だ"
          },
          {
            "stringId": 1845897014549278720,
            "femaleVariant": "そんな裏ルートを持ってたなんて",
            "maleVariant": "そんな裏ルートを持ってたとは"
          },
          {
            "stringId": 1845906173214715904,
            "femaleVariant": "トンネル？ そんなの初耳よ",
            "maleVariant": "トンネル？ そんなの初耳だ"
          },
          {
            "stringId": 1845906461430509568,
            "femaleVariant": "聞かなかったから"
          },
          {
            "stringId": 1845906622508560384,
            "femaleVariant": "古すぎて探すのには苦労したけど、幸い崩落もしてない。あとは突き進むのみ"
          },
          {
            "stringId": 1845910559164854272,
            "femaleVariant": "アルデカルドスだってもう何年も使ってない。ナイト・コープが発見して、周辺を監視してるって情報をつかんでな"
          },
          {
            "stringId": 1845915549983961088,
            "femaleVariant": "まさか、そんな裏ルートを持ってるとはね",
            "maleVariant": "まさか、そんな裏ルートを持ってるとはな"
          },
          {
            "stringId": 1845916351431569408,
            "femaleVariant": "正しくは“見つけた”"
          },
          {
            "stringId": 1845916693586112512,
            "femaleVariant": "大昔からあるトンネルでな。幸い崩落もしてない"
          },
          {
            "stringId": 1845917856129740800,
            "femaleVariant": "古すぎて探すのには苦労したけど、もはや、あとは突き進むのみ"
          },
          {
            "stringId": 1864591451356028928,
            "femaleVariant": "乗りな、V"
          },
          {
            "stringId": 1864593174929760260,
            "femaleVariant": "さっさとしなよ"
          },
          {
            "stringId": 1876353179668905988,
            "femaleVariant": "[寄りかかる] 来たのね",
            "maleVariant": "[寄りかかる] よう、来たな"
          },
          {
            "stringId": 1876353223558103044,
            "femaleVariant": "[寄りかかる] リバー"
          },
          {
            "stringId": 1876353324523388928,
            "femaleVariant": "ジュディ"
          },
          {
            "stringId": 1885107080006139904,
            "femaleVariant": "シカゴのブレインダンスクラブ。大昔よ。でも編集はしないで、既製品を使ってたかな。とにかく片っ端から客にドーパミンウェアをぶちこんでた。新しい法律ができた頃よ"
          },
          {
            "stringId": 1885107258398277632,
            "femaleVariant": "どうやら込み入った話があるみたいだし、じゃあ私はこれで"
          },
          {
            "stringId": 1885178397733810176,
            "femaleVariant": "随分昔ね…"
          },
          {
            "stringId": 2005345267698233344,
            "femaleVariant": "あらゆるテックが目白押しよ。巡回ドローンや、もっと手ごわい兵器もあるかも",
            "maleVariant": "あらゆるテックが目白押しだろ。巡回ドローンや、もっと手ごわい兵器もあるかも"
          },
          {
            "stringId": 2005350660935643144,
            "femaleVariant": "今のところはね"
          },
          {
            "stringId": 2005353615738925056,
            "femaleVariant": "でもその前に、お客が来てるみたいだよ… また後で声をかけて。詳細を教えるから"
          },
          {
            "stringId": 2005357034818105344,
            "femaleVariant": "準備ができたら声をかけて。詳細を教えるから"
          },
          {
            "stringId": 2006616646150123520,
            "femaleVariant": "それだけは、いま確信を持って言える"
