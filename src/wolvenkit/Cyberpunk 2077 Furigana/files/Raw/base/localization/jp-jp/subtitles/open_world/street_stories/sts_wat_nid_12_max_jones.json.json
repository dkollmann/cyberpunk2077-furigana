    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1170634428159336448,
            "femaleVariant": "グレネードの破片が太ももを貫通したんだ"
          },
          {
            "stringId": 1170637917551828992,
            "femaleVariant": "俺たちがICUを出たとこで連中が現れたんだ。企業のためにカメラを回してさ"
          },
          {
            "stringId": 1170639822722146304,
            "femaleVariant": "ところが撮影が済むと、あなたたちにはもうお構いなし…"
          },
          {
            "stringId": 1330104204480843776,
            "femaleVariant": "ああ、そうだ…"
          },
          {
            "stringId": 1330106366812327936,
            "femaleVariant": "ああ、そうさ。最上級のインプラントや医療を提供された。役員会のお偉いさんまで出て来て…"
          },
          {
            "stringId": 1330114895124205568,
            "femaleVariant": "俺は吹き飛ばされた"
          },
          {
            "stringId": 1608286073385226240,
            "femaleVariant": "無理やりテレビ番組に連れてかれて、台本を読まされた。退役兵を思いやってるってアピールするためにな"
          },
          {
            "stringId": 1952200580606038016,
            "femaleVariant": "お前、コーポか？"
          },
          {
            "stringId": 1952201060736405504,
            "femaleVariant": "言わなきゃ撃つぞ！"
          },
          {
            "stringId": 1952201385509752836,
            "femaleVariant": "違う"
          },
          {
            "stringId": 1952201487817216000,
            "femaleVariant": "マックス、銃を捨てて！",
            "maleVariant": "マックス、銃を捨てろ！"
          },
          {
            "stringId": 1952201957528932352,
            "femaleVariant": "マックス！ 銃を捨てて！ 話し合おう",
            "maleVariant": "マックス！ 銃を捨てろ！ 話し合おう"
          },
          {
            "stringId": 1952202174793879552,
            "femaleVariant": "断る…！ 生け捕りになってたまるか！"
          },
          {
            "stringId": 1952205060642803712,
            "femaleVariant": "コーポじゃない。あたしはV。レジーナに頼まれて来た",
            "maleVariant": "コーポじゃない。俺はV。レジーナに頼まれて来た"
          },
          {
            "stringId": 1952206397015166976,
            "femaleVariant": "レジーナだと？ はっ！"
          },
          {
            "stringId": 1952206610404577280,
            "femaleVariant": "“クソ食らえ”と伝えろ"
          },
          {
            "stringId": 1952207119693746176,
            "femaleVariant": "あんたを助けようとしてるのよ",
            "maleVariant": "あんたを助けようとしてるんだ"
          },
          {
            "stringId": 1952207294092906496,
            "femaleVariant": "その銃、弾は入ってるの？",
            "maleVariant": "その銃、弾は入ってるのか？"
          },
          {
            "stringId": 1952253621908639744,
            "femaleVariant": "どうして退役兵の取材にこだわったの？ 誰も興味を持たないのに",
            "maleVariant": "どうして退役兵の取材にこだわったんだ？ 誰も興味を持たないのに"
          },
          {
            "stringId": 1952259327571365888,
            "femaleVariant": "どうしてそんな脅しをかけるわけ？",
            "maleVariant": "どうしてそんな脅しをかけたんだ？"
          },
          {
            "stringId": 1952259453165604864,
            "femaleVariant": "コーポが口封じに来たのかと思ったからだ"
          },
          {
            "stringId": 1952259579615481856,
            "femaleVariant": "頭を撃ったら同じことじゃない",
            "maleVariant": "頭を撃ったら同じことじゃないか"
          },
          {
            "stringId": 1952260060098170880,
            "femaleVariant": "自分で手を下したかったんだ！ 何もかも企業に操られてたまるか！ 俺の死に方は俺が決める！"
          },
          {
            "stringId": 1952260876192288768,
            "femaleVariant": "今のもレジーナに伝えといてくれ。俺は彼女とは違う"
          },
          {
            "stringId": 1952261851032424448,
            "femaleVariant": "メディアはいつから爆薬のエキスパートになったの？",
            "maleVariant": "メディアはいつから爆薬のエキスパートになったんだ？"
          },
          {
            "stringId": 1952262651574063104,
            "femaleVariant": "地雷のことか？ さっき放送で話してた退役兵の力を借りたんだ"
          },
          {
            "stringId": 1952263017736802304,
            "femaleVariant": "話を聞き、行動に移してくれた感謝の印としてな。俺を守ろうとしてくれたのさ"
          },
          {
            "stringId": 1952293368878174208,
            "femaleVariant": "助ける？ 助けが必要なのは向こうだろ"
          },
          {
            "stringId": 1952294355361697796,
            "femaleVariant": "レジーナに憧れてたの？",
            "maleVariant": "レジーナに憧れてたのか？"
          },
          {
            "stringId": 1952296250801229824,
            "femaleVariant": "彼女は俺が知る最高のジャーナリストだった！ 憧れの的さ！ それが今やあのザマだ"
          },
          {
            "stringId": 1952303153803530240,
            "femaleVariant": "レジーナに憧れてたの？",
            "maleVariant": "レジーナに憧れてたのか？"
          },
          {
            "stringId": 1952306174323167232,
            "femaleVariant": "そうだな"
          },
          {
            "stringId": 1952312376390160384,
            "femaleVariant": "昔は違った… メディアには何かを変える力があったんだ"
          },
          {
            "stringId": 1952312565301612548,
            "femaleVariant": "あんたが死んでも何も変わらない"
          },
          {
            "stringId": 1952320354157694976,
            "femaleVariant": "俺だけじゃない。彼女はナイトシティで一番の戦場リポーターだった"
          },
          {
            "stringId": 1952320688041070592,
            "femaleVariant": "ああ。魂を売っちまったのさ"
          },
          {
            "stringId": 1952342518252539904,
            "femaleVariant": "話はもういい。さっさと行くよ",
            "maleVariant": "話はもういい。さっさと行くぞ"
          },
          {
            "stringId": 1952343382346272768,
            "femaleVariant": "俺はどこにも行かない"
          },
          {
            "stringId": 1952350121418395648,
            "femaleVariant": "なら力尽くね",
            "maleVariant": "なら力尽くだ"
          },
          {
            "stringId": 1952350953031438336,
            "femaleVariant": "メディアはフィクサーに取って代わられた"
          },
          {
            "stringId": 1952351608836034560,
            "femaleVariant": "あんたの墓にそう刻んでおこうか？"
          },
          {
            "stringId": 1952351918593773568,
            "femaleVariant": "なんなら墓に刻もうか？ “信念を曲げなかった男”って"
          },
          {
            "stringId": 1952352552923533312,
            "femaleVariant": "ノーマッドの所で身を潜めた方がいい"
          },
          {
            "stringId": 1952369122873806848,
            "femaleVariant": "ふん、やってみろ"
          },
          {
            "stringId": 1952369929874673664,
            "femaleVariant": "なかなか面白い画だな、V。この場面にタイトルをつけるなら… “真実への一撃”だ"
          },
          {
            "stringId": 1952370405592633344,
            "femaleVariant": "相当な頑固者ね。イライラしてきた",
            "maleVariant": "相当な頑固者だな。イライラしてきた"
          },
          {
            "stringId": 1952371911112888320,
            "femaleVariant": "うるさい"
          },
          {
            "stringId": 1952373122260111360,
            "femaleVariant": "俺もメディアは苦手だと思ってたが…"
          },
          {
            "stringId": 1952373358248431616,
            "femaleVariant": "黙ってて",
            "maleVariant": "黙ってろ"
          },
          {
            "stringId": 1952373597407645696,
            "femaleVariant": "どうだ？ “言論の自由”の重みを感じるか？"
          },
          {
            "stringId": 1952373755482574848,
            "femaleVariant": "ジョニー、勘弁してよ",
            "maleVariant": "ジョニー、勘弁してくれ"
          },
          {
            "stringId": 1952386382149554176,
            "femaleVariant": "消えりゃいいんだろ。お前の良心みたいにな"
          },
          {
            "stringId": 1952388055844622336,
            "femaleVariant": "俺は信念を曲げない！ 絶対にな！"
          },
          {
            "stringId": 1952394911937826816,
            "femaleVariant": "“魂を売ったクソアマ”よりマシさ"
          },
          {
            "stringId": 1952395145778663424,
            "femaleVariant": "そんなにレジーナが憎いわけ？",
            "maleVariant": "そんなにレジーナが憎いのかよ？"
          },
          {
            "stringId": 1952395428441198592,
            "femaleVariant": "なんでわからないんだ… お前は何とも思わないのか？ 捏造記事に、偏向報道… 隠ぺい工作まで何でもありだ"
          },
          {
            "stringId": 1952403382401863680,
            "femaleVariant": "でも今はフィクサーよ",
            "maleVariant": "でも今はフィクサーだ"
          },
          {
            "stringId": 1952411800000892928,
            "femaleVariant": "なのに今じゃ、ホルトとアラサカの関係を暴いても誰ひとり気にしない"
          },
          {
            "stringId": 1953257480722419712,
            "femaleVariant": "ノーマッドを紹介するよ。彼らと旅をするといい。少し休息が必要でしょ",
            "maleVariant": "ノーマッドを紹介するよ。彼らと旅をするといい。少し休息が必要だろ"
          },
          {
            "stringId": 1953260355431272448,
            "femaleVariant": "休息？ 責任から逃げろってことか？"
          },
          {
            "stringId": 1953260630544060416,
            "femaleVariant": "悪いがそれはできないな"
          },
          {
            "stringId": 1953294291159060480,
            "femaleVariant": "ああ。レジーナのせいでヨリノブは夜も眠れないだろうよ"
          },
          {
            "stringId": 1953297781541408768,
            "femaleVariant": "あんたがどれだけ訴えても、企業にしてみれば蚊が飛んでるようなものよ",
            "maleVariant": "あんたがどれだけ訴えても、企業にしてみれば蚊が飛んでるに過ぎない"
          },
          {
            "stringId": 1953412943472594944,
            "femaleVariant": "レジーナの使いだ。そいつを乗せろ！"
          },
          {
            "stringId": 1953430423217737728,
            "femaleVariant": "ふん…"
          },
          {
            "stringId": 1953446875677167616,
            "femaleVariant": "少なくとも、彼女はそれを求めてる"
          },
          {
            "stringId": 1953450268198014976,
            "femaleVariant": "真実を知りたいんでしょ？ だったら今のレジーナがどんな奴で、何のために戦ってるか、直接会って確かめたらどう？",
            "maleVariant": "真実を知りたいんだろ？ だったら今のレジーナがどんな奴で、何のために戦ってるか、直接会って確かめたらどうだ？"
          },
          {
            "stringId": 1953474476932173824,
            "femaleVariant": "誰かが疑問を抱けばいいんだ。お前だって今、なぜだと気になっただろう"
          },
          {
            "stringId": 1953480058544164864,
            "femaleVariant": "小さな違和感が、いずれ人の考えを変えるかもしれない。やってみなきゃわからないだろ？"
          },
          {
            "stringId": 1953612079815823360,
            "femaleVariant": "なぜそんな脅しを？"
          },
          {
            "stringId": 1953613159630999552,
            "femaleVariant": "弾は入ってるの？",
            "maleVariant": "弾は入ってるのか？"
          },
          {
            "stringId": 1953613370705154048,
            "femaleVariant": "..."
          },
          {
            "stringId": 1953621040107458560,
            "femaleVariant": "頑固？ この仕事ではな、粘り強いのはいいことなんだ"
          },
          {
            "stringId": 1963712647469043712,
            "femaleVariant": "というと？"
          },
          {
            "stringId": 1963712701927886848,
            "femaleVariant": "いわゆる即席爆弾ってやつでだよ"
          },
          {
            "stringId": 1963729016579264512,
            "femaleVariant": "「企業の務め」という番組ですね"
          },
          {
            "stringId": 1963735229266235392,
            "femaleVariant": "しかも、もらったインプラントまで返却させられた"
          },
          {
            "stringId": 1963735843429781504,
            "femaleVariant": "こんな、第一世代のポンコツと交換しろってな…"
          },
          {
            "stringId": 1963742072189325312,
            "femaleVariant": "汚い連中だ。何もかもを奪われた。これじゃ俺たちは… まるで…"
          },
          {
            "stringId": 1963743265703694336,
            "femaleVariant": "ええ… わかりますよ"
          },
          {
            "stringId": 1963786090201845760,
            "femaleVariant": "運転手付きか。次は何だ？ シャンパンと合成葉巻が出てくるのか？"
          },
          {
            "stringId": 1963810503634817024,
            "femaleVariant": "インタビューを聞いた"
          },
          {
            "stringId": 1963811513438670848,
            "femaleVariant": "来る途中であんたのインタビューを聞いた"
          },
          {
            "stringId": 1963827277579259904,
            "femaleVariant": "監視映像でお前を見かけた時に流し始めたんだ"
          },
          {
            "stringId": 1963831985651945472,
            "femaleVariant": "理想通りには仕上がらなかったが… それでも真実を公にできた"
          },
          {
            "stringId": 1963853008828993536,
            "femaleVariant": "俺たちは悪党どもの動きを細かく追ってた"
          },
          {
            "stringId": 1986659118371672064,
            "femaleVariant": "地雷はどこで手に入れたの？",
            "maleVariant": "地雷はどこで手に入れたんだ？"
          },
          {
            "stringId": 1986673396185141248,
            "femaleVariant": "もう知ってるでしょうけど、あんたに死刑執行令が出てる",
            "maleVariant": "もう知ってるだろうが、あんたに死刑執行令が出てる"
          },
          {
            "stringId": 1986695655574573056,
            "femaleVariant": "近頃の連中は… それをわかってない…"
          },
          {
            "stringId": 1986707576658956292,
            "femaleVariant": "レジーナにチャンスをあげて。話をするだけでいい",
            "maleVariant": "レジーナにチャンスをやれよ。話をするだけでいい"
          },
          {
            "stringId": 1986719975659114496,
            "femaleVariant": "悪く思わないで",
            "maleVariant": "悪く思うなよ"
          },
          {
            "stringId": 1986730506482716672,
            "femaleVariant": "話をしよう。だがそれだけだ"
          },
          {
            "stringId": 1986731078283792384,
            "femaleVariant": "それで十分",
            "maleVariant": "それで十分さ"
          },
          {
            "stringId": 1986731114757459968,
            "femaleVariant": "わかった。いくつかダウンロードしておくものがある"
          },
          {
            "stringId": 1986731279073513472,
            "femaleVariant": "お好きにどうぞ"
          },
          {
            "stringId": 1986870730487599104,
            "femaleVariant": "今や、企業が警戒するのはメディアじゃなくてフィクサー… 切れ味抜群の情報を掴んだ彼らこそが脅威なの",
            "maleVariant": "今や、企業が警戒するのはメディアじゃなくてフィクサー… 切れ味抜群の情報を掴んだ彼らこそが脅威なんだ"
          },
          {
            "stringId": 1986871861641056256,
            "femaleVariant": "ならどうして取材を続けるの？",
            "maleVariant": "ならどうして取材を続けてるんだ？"
          },
          {
            "stringId": 1986876088710959104,
            "femaleVariant": "ほら行くよ。バカな真似を続けたって何も変わらない",
            "maleVariant": "ほら行くぞ。バカな真似を続けたって何も変わらない"
          },
          {
            "stringId": 1986877642734145536,
            "femaleVariant": "逃げたって変わらないだろ。お前の助けはいらない"
          },
          {
            "stringId": 1986878108419330048,
            "femaleVariant": "お前にはわからないさ"
          },
          {
            "stringId": 1986881435123159040,
            "femaleVariant": "なら飛び続けてやる"
          },
          {
            "stringId": 1986933115340558336,
            "femaleVariant": "モタモタするな、行くぞ"
          },
          {
            "stringId": 1988069148299988992,
            "femaleVariant": "それを知ったレジーナが、あんたを助けようとしてるのよ",
            "maleVariant": "それを知ったレジーナが、あんたを助けようとしてるんだ"
          },
          {
            "stringId": 1988089649185525760,
            "femaleVariant": "そんな態度をしてるのは、レジーナの鼻を明かしたいから？",
            "maleVariant": "そんな態度をしてるのは、レジーナの鼻を明かしたいからか？"
          },
          {
            "stringId": 1988246906242174976,
            "femaleVariant": "マックス？"
          },
          {
            "stringId": 2283885679787479040,
            "femaleVariant": "話はもういいでしょ。さっさと行くよ",
            "maleVariant": "話はもういいだろ。さっさと行くぞ"
          }
        ]
      }
    }
  }
}