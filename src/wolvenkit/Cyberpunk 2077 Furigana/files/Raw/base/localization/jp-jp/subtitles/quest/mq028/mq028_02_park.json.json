    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1816784476100009984,
            "femaleVariant": "ほら、言ったろ？ 本物だ！ ブルー・ムーンだよ！"
          },
          {
            "stringId": 1816794588449513472,
            "femaleVariant": "あ、あの！ 一緒に写真撮ってくれませんか？ 大っっファンなんです…"
          },
          {
            "stringId": 1816830239278538756,
            "femaleVariant": "何をしてるの？",
            "maleVariant": "何をしてる？"
          },
          {
            "stringId": 1816830513586020352,
            "femaleVariant": "ここで何を？"
          },
          {
            "stringId": 1816832978662367232,
            "femaleVariant": "ああ… 完璧だ…"
          },
          {
            "stringId": 1816833074393161728,
            "femaleVariant": "またあの女か？ ふざけんなよ…"
          },
          {
            "stringId": 1816902114381832192,
            "femaleVariant": "本物はこんな太ってねえだろ"
          },
          {
            "stringId": 1816902349296410624,
            "femaleVariant": "太ってんのはあんたよ、クズ野郎"
          },
          {
            "stringId": 1816902534147776512,
            "femaleVariant": "ブハハ！ 言うじゃねえか！"
          },
          {
            "stringId": 1816903874429231104,
            "femaleVariant": "知ってるよ！"
          },
          {
            "stringId": 1816904872908464128,
            "femaleVariant": "なあ！ 「Pillow Fight」はいい曲だったぜ！"
          },
          {
            "stringId": 1816909236477796352,
            "femaleVariant": "いいよ。お名前は？"
          },
          {
            "stringId": 1816909491223044096,
            "femaleVariant": "グリーン・クラウド！"
          },
          {
            "stringId": 1816910463932473344,
            "femaleVariant": "いいね！ 3… 2… 1… はい、チーズ。ほら"
          },
          {
            "stringId": 1816939166997585920,
            "femaleVariant": "邪魔されるまでは眺めを堪能してた"
          },
          {
            "stringId": 1816989011502342144,
            "femaleVariant": "何でわかってくれないのさ！ あんたに何もかも捧げたってのに！"
          },
          {
            "stringId": 1816989724164923392,
            "femaleVariant": "こっちはあなたが誰か知らないの！ V、助けて！"
          },
          {
            "stringId": 1817192367567052800,
            "femaleVariant": "なあ、あれって… 本物だぞ！"
          },
          {
            "stringId": 1817193248572219392,
            "femaleVariant": "ああ、退屈だ。クスリでもくれよ"
          },
          {
            "stringId": 1817193477296005120,
            "femaleVariant": "なにが「くれよ」だ。タダでやるかよ"
          },
          {
            "stringId": 1817239793904558080,
            "femaleVariant": "酒さえ、酒さえありゃいい… そうだ… 1杯だけ…"
          },
          {
            "stringId": 1817240458617856000,
            "femaleVariant": "あっ… 唇が… 焼けるみたいだ… 熱い… 熱い…"
          },
          {
            "stringId": 1817264994004410368,
            "femaleVariant": "ったく、実物に会えるなんてな！"
          },
          {
            "stringId": 1821393765695397888,
            "femaleVariant": "死を迎えても、私たちは1つ"
          },
          {
            "stringId": 1822683487969386496,
            "femaleVariant": "おい！ あの女、撃ちやがったぞ！"
          },
          {
            "stringId": 1822684390667493376,
            "femaleVariant": "何がどうなってんだ！？"
          },
          {
            "stringId": 1864881884746653696,
            "femaleVariant": "V、来てくれてありがとう。じゃあ、これから私はこの辺を適当に歩き回ってみる。いい？"
          },
          {
            "stringId": 1864947485808697344,
            "femaleVariant": "かわいこちゃん、こんなとこで何してんだい？"
          },
          {
            "stringId": 1864949292245098496,
            "femaleVariant": "目を引く男はいたかな？"
          },
          {
            "stringId": 1864949556251369472,
            "femaleVariant": "うっせえな。あんたの目玉をくり抜いてやってもいいんだよ！ 何か文句ある？ ないならその口閉じてて"
          },
          {
            "stringId": 1864951654074458112,
            "femaleVariant": "これだからじじいは…"
          },
          {
            "stringId": 1865016212549967872,
            "femaleVariant": "なんか違う曲にしてくれよ"
          },
          {
            "stringId": 1865064865486512128,
            "femaleVariant": "厄介そうな連中よ、気をつけて",
            "maleVariant": "厄介そうな連中だ、気をつけろ"
          },
          {
            "stringId": 1865065471966093312,
            "femaleVariant": "気をつけて。厄介そうな連中よ",
            "maleVariant": "気をつけろ。厄介そうな連中だ"
          },
          {
            "stringId": 1865065878813581312,
            "femaleVariant": "んー、ただのチンピラでしょ。わざわざ脅迫なんてするガラじゃないって"
          },
          {
            "stringId": 1866125689474949120,
            "femaleVariant": "ブルー・ムーンよ！"
          },
          {
            "stringId": 1866133680009945088,
            "femaleVariant": "ホントに、ありがとうございます！"
          },
          {
            "stringId": 1867495788739993600,
            "femaleVariant": "カメラをしまって、今すぐよ",
            "maleVariant": "カメラをしまえ、今すぐだ"
          },
          {
            "stringId": 1867496011742748676,
            "femaleVariant": "写真を撮ってどうするの？",
            "maleVariant": "写真を撮ってどうするんだ？"
          },
          {
            "stringId": 1867498766460952576,
            "femaleVariant": "質問の仕方を変えるから、次はしっかり答えてちょうだい。写真を撮ってどうするの？",
            "maleVariant": "質問の仕方を変えるから、次はしっかり答えてくれよ。写真を撮ってどうするんだ？"
          },
          {
            "stringId": 1867499997254295552,
            "femaleVariant": "さあ、売って金にでもするかな。おい、ありゃ（Us Cracks〉のブルー・ムーンか…？"
          },
          {
            "stringId": 1867503052636987396,
            "femaleVariant": "カメラをしまって",
            "maleVariant": "カメラをしまえ"
          },
          {
            "stringId": 1867512219691032576,
            "femaleVariant": "何か変わったことはなかった？",
            "maleVariant": "何か変わったことはなかったか？"
          },
          {
            "stringId": 1867514536943329280,
            "femaleVariant": "何様だ。勝手に言ってな"
          },
          {
            "stringId": 1867518002377957376,
            "femaleVariant": "話は終わりね",
            "maleVariant": "話は終わりだ"
          },
          {
            "stringId": 1867522197269159936,
            "femaleVariant": "ねえ… 彼女の写真はよく撮るの？",
            "maleVariant": "なあ… 彼女の写真はよく撮るのか？"
          },
          {
            "stringId": 1867528116757504000,
            "femaleVariant": "ああ。それが何か法に触れるか？"
          },
          {
            "stringId": 1867531733170298880,
            "femaleVariant": "彼女につきまとってる奴に覚えはない？ あんたならわかるでしょ",
            "maleVariant": "彼女につきまとってる奴に覚えはないか？ あんたならわかるだろ"
          },
          {
            "stringId": 1867531929899933696,
            "femaleVariant": "なんで言わなきゃならないんだよ"
          },
          {
            "stringId": 1867540376255111168,
            "femaleVariant": "[武器を抜く]"
          },
          {
            "stringId": 1867573180678447104,
            "femaleVariant": "なぜブルー・ムーンにつきまとってるの？",
            "maleVariant": "なぜブルー・ムーンにつきまとってるんだ？"
          },
          {
            "stringId": 1867573390712414208,
            "femaleVariant": "いいニックネームね",
            "maleVariant": "いいニックネームだな"
          },
          {
            "stringId": 1867774440597790720,
            "femaleVariant": "ブルー・ムーンにつきまとってるそうね",
            "maleVariant": "ブルー・ムーンにつきまとってるそうだな"
          },
          {
            "stringId": 1867781750481133568,
            "femaleVariant": "何のこと？ 実物を見たのはこれが初めてよ！"
          },
          {
            "stringId": 1867783058097348608,
            "femaleVariant": "グリーン・クラウド、いいニックネームね",
            "maleVariant": "グリーン・クラウド、いいニックネームだな"
          },
          {
            "stringId": 1867783857296809984,
            "femaleVariant": "ここで何してるの？",
            "maleVariant": "ここで何してる？"
          },
          {
            "stringId": 1867784509443973120,
            "femaleVariant": "へえ？ あるカメラマンが違うこと言ってたけど。証拠の写真もある",
            "maleVariant": "へえ？ あるカメラマンが違うこと言ってたがな。証拠の写真もある"
          },
          {
            "stringId": 1867786327959969792,
            "femaleVariant": "それは… どうも…"
          },
          {
            "stringId": 1867786535125032960,
            "femaleVariant": "略して“GC”。手紙にはそうサインしてるんでしょ？",
            "maleVariant": "略して“GC”。手紙にはそうサインしてるんだろ？"
          },
          {
            "stringId": 1867786739924508672,
            "femaleVariant": "クソ…"
          },
          {
            "stringId": 1867787343015092224,
            "femaleVariant": "ここで何を？",
            "maleVariant": "ここで何してる？"
          },
          {
            "stringId": 1867787562729512960,
            "femaleVariant": "散歩してるだけ… 何か問題ある？"
          },
          {
            "stringId": 1867843632789364736,
            "femaleVariant": "いい子じゃない"
          },
          {
            "stringId": 1871943042253246464,
            "femaleVariant": "[武器を抜く]"
          },
          {
            "stringId": 1871943294582575104,
            "femaleVariant": "へえ、そう",
            "maleVariant": "へえ、そうか"
          },
          {
            "stringId": 1871958054959108096,
            "femaleVariant": "どっかのサイコ野郎がブルー・ムーンを殺したりしたら、あんたは稼ぎ頭がいなくなるでしょ",
            "maleVariant": "どっかのサイコ野郎がブルー・ムーンを殺したりしたら、お前は稼ぎ頭がいなくなるだろ"
          },
          {
            "stringId": 1871958536549093376,
            "femaleVariant": "あそこにいる緑の服の女。しょっちゅう写真に写り込んでる。妙だろ？"
          },
          {
            "stringId": 1871959597691228160,
            "femaleVariant": "なるほどね",
            "maleVariant": "なるほどな"
          },
          {
            "stringId": 1896814549259165696,
            "femaleVariant": "V、近寄りすぎだって。少し距離を取って"
          },
          {
            "stringId": 1896823816473190400,
            "femaleVariant": "V、近づきすぎ！"
          },
          {
            "stringId": 1896826019858522112,
            "femaleVariant": "あー、もう！ 無茶苦茶じゃない。私帰るね"
          },
          {
            "stringId": 1915386260258725888,
            "femaleVariant": "ブルー・ムーンから目を離さないで"
          },
          {
            "stringId": 1915463148159279104,
            "femaleVariant": "ストーカーを殺した"
          },
          {
            "stringId": 1915464299529281536,
            "femaleVariant": "ストーカーを殺した。一件落着ね",
            "maleVariant": "ストーカーを殺した。一件落着だ"
          },
          {
            "stringId": 1915464624302628864,
            "femaleVariant": "えっ！？ 忘れたの？ 殺さないって約束でしょ！"
          },
          {
            "stringId": 1915465368422490112,
            "femaleVariant": "自分が殺されるよりマシでしょ？",
            "maleVariant": "自分が殺されるよりマシだろ？"
          },
          {
            "stringId": 1915498437791621120,
            "femaleVariant": "時間の無駄ね。ここまでにしましょ、V"
          },
          {
            "stringId": 1915541770421313536,
            "femaleVariant": "V、真面目にやってくれないなら、私もう帰るから"
          },
          {
            "stringId": 1922679128618868736,
            "femaleVariant": "よし。もう行って平気だよ"
          },
          {
            "stringId": 1924010139200049152,
            "femaleVariant": "合図をしたらついて来て… でも近づき過ぎないでね。犯人が逃げると元も子もないから"
          },
          {
            "stringId": 1924011067735068672,
            "femaleVariant": "わかった"
          },
          {
            "stringId": 1924011425022660608,
            "femaleVariant": "ふう… よし、やるか。レッド・メナスも高所から見張ってくれてる。じゃあ、行ってくるね！"
          },
          {
            "stringId": 1924043929016389632,
            "femaleVariant": "はあ… わかった、後始末はこっちで何とかする… メディアに漏れないようにしなきゃ。V、一応、手を貸してくれてありがと…"
          },
          {
            "stringId": 1957638791374217216,
            "femaleVariant": "すぐに新しいフェイスプレートがいるの！"
          },
          {
            "stringId": 1957746802470068224,
            "femaleVariant": "銃の店で何してたの？",
            "maleVariant": "銃の店で何してたんだ？"
          },
          {
            "stringId": 1957747721190416384,
            "femaleVariant": "武器屋なんかで何を探してたの？",
            "maleVariant": "武器屋なんかで何を探してた？"
          },
          {
            "stringId": 1957749148428181504,
            "femaleVariant": "え？ 何の話？ どこにも行ってないけど"
          },
          {
            "stringId": 1957749658472325120,
            "femaleVariant": "嘘つくのが下手ね",
            "maleVariant": "嘘つくのが下手だな"
          },
          {
            "stringId": 1957752263520985088,
            "femaleVariant": "ちぇっ… 炸薬弾はおいてないか…"
          },
          {
            "stringId": 1957755496993824768,
            "femaleVariant": "普通のでやるしかないな"
          },
          {
            "stringId": 1958827136381001728,
            "femaleVariant": "襲ってくるとしたらこの路地かも… 注意してて！"
          },
          {
            "stringId": 1959041641912377344,
            "femaleVariant": "ふう… 何もなかったね"
          },
          {
            "stringId": 1959043155687022592,
            "femaleVariant": "ねえ、あちこち行ったけど… もしかしたらここにはいないのかも"
          },
          {
            "stringId": 1963248519896944640,
            "femaleVariant": "もうやめよう、V。レッド・メナスに来てもらうよ"
          },
          {
            "stringId": 1963252548995698688,
            "femaleVariant": "そ、そうだけどさ…"
          },
          {
            "stringId": 1967544093053722624,
            "femaleVariant": "ストーカーを見つけた。その場にいて",
            "maleVariant": "ストーカーを見つけた。その場にいろ"
          },
          {
            "stringId": 1967545886370340864,
            "femaleVariant": "う、うん… 気をつけて。あと、絶対殺さないでね！"
          },
          {
            "stringId": 1967588025485664256,
            "femaleVariant": "大丈夫になったら教えてね"
          },
          {
            "stringId": 1974823601985200128,
            "femaleVariant": "じゃあ… これで最後だね。奴が現れなかったら、もうやめよう"
          },
          {
            "stringId": 1975313822573043712,
            "femaleVariant": "[何も言わない]"
          },
          {
            "stringId": 1979120590027804672,
            "femaleVariant": "コソコソしてる人とか、上から私の様子ばかり伺ってる人とか… とにかくストーカーが姿を現さないか見張ってて"
          },
          {
            "stringId": 2015453437229178880,
            "femaleVariant": "こいつじゃないみたいね…",
            "maleVariant": "こいつじゃないみたいだな…"
          },
          {
            "stringId": 2070921218581766148,
            "femaleVariant": "わかった"
          },
          {
            "stringId": 2231705026017607680,
            "femaleVariant": "フンフン… 落ち着いて、自然に振る舞うの… きっと大丈夫…"
          },
          {
            "stringId": 2231705233753096192,
            "femaleVariant": "まだこの雑誌には特集されてないんだよね。これいつの号…？"
          },
          {
            "stringId": 2231705351747256320,
            "femaleVariant": "何なのよ、もう… 変な奴ばっかで嫌になるんだけど"
          },
          {
            "stringId": 2233147213842464768,
            "femaleVariant": "V、どこにいるの？ 姿が見えないんだけど。ついてくる約束でしょ"
          }
        ]
      }
    }
  }
}