    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "“インフォフラッシュ”へようこそ。ロザリンド・マイヤーズが4期目に立候補するって聞きました？",
                    "maleVariant": null,
                    "stringId": 1793521051205804032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そう、マイヤーズはこれを前回のミリテク株主総会… じゃなかった、えーと、連邦党議会で発表しました",
                    "maleVariant": null,
                    "stringId": 1811017756384673792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "中には、違憲じゃないの、と言う人もいるかもしれません…",
                    "maleVariant": null,
                    "stringId": 1811018041597345792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うん、その通り！",
                    "maleVariant": null,
                    "stringId": 1811018726661406720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マイヤーズ氏は、ルーズベルトと自身を重ねています。4度の任期を全うしましたから",
                    "maleVariant": null,
                    "stringId": 1811018945687961600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うん、確かに、似てるかもしれませんね",
                    "maleVariant": null,
                    "stringId": 1811019236554555392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マイヤーズ氏のスローガンは「ふたたび一つの国へ」としていますが、他にも候補はあったそうです… 「アラサカ、東京へ帰れ！」とか、「テキサスなんてクソ喰らえ」とか",
                    "maleVariant": null,
                    "stringId": 1811019752856600576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "有権者には「合衆国の再統一に向け引き続き努力する」と約束していますが、これ、実際は「また戦争するぞ！」って意味ですよね",
                    "maleVariant": null,
                    "stringId": 1811020065214808064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[沈黙]",
                    "maleVariant": null,
                    "stringId": 1811020216746622976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ん？ オチはって？ 本気ですよ。みんな死にます",
                    "maleVariant": null,
                    "stringId": 1811020924191490048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ウソウソ！ みんなじゃありません。私にはシェルターがあるから大丈夫",
                    "maleVariant": null,
                    "stringId": 1811021322532929536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "では、本日の“インフォフラッシュ”はここまで。おやすみなさい",
                    "maleVariant": null,
                    "stringId": 1811022084637966336
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
