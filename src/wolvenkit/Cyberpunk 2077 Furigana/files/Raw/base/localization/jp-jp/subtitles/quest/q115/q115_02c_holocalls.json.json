    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "着信6件… ハナコの個人回線ね",
              "maleVariant": "着信6件… ハナコの個人回線だな",
              "stringId": 1711123159506231296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー？",
              "maleVariant": "",
              "stringId": 1715509145311920128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたし一人か",
              "maleVariant": "俺一人か",
              "stringId": 1715509476242505728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よっぽどの緊急事態ってことかしら？",
              "maleVariant": "よっぽどの緊急事態ってことか？",
              "stringId": 1794017355161800704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしもし",
              "maleVariant": "",
              "stringId": 1794018203669499904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ずいぶんかかったわね",
              "maleVariant": "",
              "stringId": 1794018976797167616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "決めた、あんたの話に乗る",
              "maleVariant": "",
              "stringId": 1794020068507389952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうこと？",
              "maleVariant": "どういうことだ？",
              "stringId": 1794023199521771520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ノース・オークの彼の邸宅に招待されたの。それも長期滞在でね",
              "maleVariant": "",
              "stringId": 1794023822023593984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何言ってんの？",
              "maleVariant": "",
              "stringId": 1804020160031232000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本気よ。例のジョニーの件で… 今すぐ手を打たないと、あたしは死ぬ",
              "maleVariant": "本気だ。例のジョニーの件で… 今すぐ手を打たないと、俺は死ぬ",
              "stringId": 1804020409407770624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今すぐアラサカ・タワーに侵入したいの",
              "maleVariant": "今すぐアラサカ・タワーに侵入したい",
              "stringId": 1804020959968890880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今どこにいるの？",
              "maleVariant": "",
              "stringId": 1804023497472479232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴィクの診療所",
              "maleVariant": "",
              "stringId": 1804023597062033408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リパーのとこだね。それならわかる",
              "maleVariant": "",
              "stringId": 1804023965623914496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと死ぬの待っててくれる？",
              "maleVariant": "",
              "stringId": 1804024147639930880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やってみる",
              "maleVariant": "",
              "stringId": 1804026140806746112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこにも行かないでよ。すぐ迎えに行くから",
              "maleVariant": "",
              "stringId": 1804027276255170560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう。彼の部下が私の“安全”を守ってるわ",
              "maleVariant": "",
              "stringId": 1830212541107273728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その間に、ヨリノブが手を打ってきたわ",
              "maleVariant": "",
              "stringId": 1830215298929262592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？",
              "maleVariant": "",
              "stringId": 1856112250162323456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナム",
              "maleVariant": "",
              "stringId": 1856113477382119424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いま何時だと思ってんの？",
              "maleVariant": "",
              "stringId": 1856113549054386176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってる。でも… 力を借りたくて。貸しもあるしね",
              "maleVariant": "わかってる。でも… 力を借りたくてな。貸しもあるだろ",
              "stringId": 1856113910100074496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着いた？",
              "maleVariant": "",
              "stringId": 1856119058188804096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ",
              "maleVariant": "ああ",
              "stringId": 1856120240344997888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはよかった",
              "maleVariant": "",
              "stringId": 1856120289737121792
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "朝まで待てないくらい緊急の案件ってこと？",
              "maleVariant": "",
              "stringId": 1856158338634293248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "解放するよう“説得”してみる",
              "maleVariant": "",
              "stringId": 1856159077620326404
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブに勘付かれたと思う？",
              "maleVariant": "ヨリノブに勘付かれたと思うか？",
              "stringId": 1856159768875180032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずい状況ね。どうするの？",
              "maleVariant": "まずい状況だな。どうする？",
              "stringId": 1856161749794627584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど。なら解放するよう、かけ合ってみる",
              "maleVariant": "",
              "stringId": 1856162306496208896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずい状況ね。どうするの？",
              "maleVariant": "まずい状況だな。どうする？",
              "stringId": 1856162407478272000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブに勘付かれたの？",
              "maleVariant": "ヨリノブに勘付かれたのか？",
              "stringId": 1856163581967278080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあそこに迎えをやるわ。邸宅まで送ってくれる",
              "maleVariant": "",
              "stringId": 1856168053917425664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのあと私と一緒に、アラサカ・タワーにいる兄を訪ねるとしましょう",
              "maleVariant": "",
              "stringId": 1856168632647491584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "疑いを持つきっかけは色々あったでしょうね",
              "maleVariant": "",
              "stringId": 1856169252498513920
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "先延ばしにはできない",
              "maleVariant": "",
              "stringId": 1856171994617028608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今どこにいるの？",
              "maleVariant": "",
              "stringId": 1856173198214504448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴィクターってリパードクのとこよ。「エソテリカ」っていう店の裏にある",
              "maleVariant": "ヴィクターってリパードクのとこだ。「エソテリカ」っていう店の裏にある",
              "stringId": 1856173285288255488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でなきゃ私を拘束する理由が存在しないもの",
              "maleVariant": "",
              "stringId": 1856175520734846976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "力を貸して。なんとしてもここを脱出しないと",
              "maleVariant": "",
              "stringId": 1856180724691705856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ聞かせて",
              "maleVariant": "",
              "stringId": 1856189740683698176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "監禁か…",
              "maleVariant": "",
              "stringId": 1856192621130358784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お願いするわ",
              "maleVariant": "",
              "stringId": 1856193848434040832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ハナコに連絡し、提案に乗る]",
              "maleVariant": "",
              "stringId": 2025547093456723972
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がり、パナムに連絡する]",
              "maleVariant": "",
              "stringId": 2025547107432144900
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待ってよ。大体なんでアラサカ・タワー？",
              "maleVariant": "",
              "stringId": 2028846800964493312
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "テックよ。タワーの地下にアクセスポイントがある。冗談みたいな名前だけど、そこが“魂の牢獄”に通じてるの",
              "maleVariant": "テックさ。タワーの地下にアクセスポイントがある。冗談みたいな名前だが、そこが“魂の牢獄”に通じてるんだ",
              "stringId": 2028855090939240448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ… やっぱりホロだと説明しにくいな",
              "maleVariant": "",
              "stringId": 2028855918072766464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこは〈神輿〉と呼ばれていて、あたしとジョニーを切り離せる唯一の場所なの。もうそこに賭けるしかない",
              "maleVariant": "そこは〈神輿〉と呼ばれていて、俺とジョニーを切り離せる唯一の場所なんだ。もうそこに賭けるしかない",
              "stringId": 2028856702407614464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確認するけど… こんな夜中にかけてきたのは、私にアラサカ・タワーへの侵入を手伝ってほしい、でないとあんたは死ぬ。そういうこと？",
              "maleVariant": "",
              "stringId": 2028862286116757504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "簡単に言うとね",
              "maleVariant": "簡単に言えばな",
              "stringId": 2028864758977732608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "魂の牢獄…？",
              "maleVariant": "",
              "stringId": 2037334078170931200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やる… って？",
              "maleVariant": "",
              "stringId": 2037335773206618112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やるよ",
              "maleVariant": "",
              "stringId": 2037337089379213312
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[パナムに連絡し、助けを求める]",
              "maleVariant": "",
              "stringId": 2186499465946341376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がり、ハナコに連絡する]",
              "maleVariant": "",
              "stringId": 2186500501637439488
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}