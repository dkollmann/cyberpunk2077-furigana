  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、お前。どこに座ってる",
              "maleVariant": "",
              "stringId": "1797024417207955456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ん… ここ… バーだろ？",
              "maleVariant": "",
              "stringId": "1797025516115603456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“俺ら”のバーだ",
              "maleVariant": "",
              "stringId": "1797026057113710592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "立て。ちょっと出よう",
              "maleVariant": "",
              "stringId": "1797026714277261312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、離れろ。お前は関係ねえだろ",
              "maleVariant": "",
              "stringId": "1802787291180183552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんだ、耳が聞こえないのか？ 殴られたいんなら、いつでも相手してやる",
              "maleVariant": "",
              "stringId": "1802804976328781824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "面白い、やってみなよ",
              "maleVariant": "面白い、やってみろよ",
              "stringId": "1802815930827419652"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やめといたほうがいい",
              "maleVariant": "",
              "stringId": "1802816295127887872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ママ・ウェルズはケンカが嫌いよ",
              "maleVariant": "ママ・ウェルズはケンカが嫌いだ",
              "stringId": "1802816360257040384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。好きにして",
              "maleVariant": "わかった。好きにしろ",
              "stringId": "1802816477680775168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us][pl_pl][INVISIBLE LINE, IGNORE]",
              "maleVariant": "",
              "stringId": "1802819032313901056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "面白いじゃない。やってみなよ。相手になったげる",
              "maleVariant": "面白いじゃないか。やってみろよ。相手になってやる",
              "stringId": "1802820962264166400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "随分自信があるようだけど、ほんとに使いこなせるの？",
              "maleVariant": "随分自信があるようだが、ほんとに使いこなせるのか？",
              "stringId": "1802821275226353664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ホントにいいの？ あんたたちバカ2人が自分の店でふざけてるのを見たら、ママ・ウェルズが何て言うかしら",
              "maleVariant": "ホントにいいのか？ あんたらバカ2人が自分の店でふざけてるのを見たら、ママ・ウェルズが何て言うかねぇ",
              "stringId": "1802821423100735488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。知り合いのコーポでもないし、あたしには関係ない",
              "maleVariant": "わかった。知り合いのコーポでもないし、俺には関係ない",
              "stringId": "1802821625232633856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待て！ こいつ知ってるぞ。うちの連中とやりあった――ほら、グスタボがやられた時だ",
              "maleVariant": "",
              "stringId": "1802821867613073408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あれ… 何… 何が起きたんだ？",
              "maleVariant": "",
              "stringId": "1802839757141716992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フン… お望み通り、叩きのめしてやるよ",
              "maleVariant": "",
              "stringId": "1803779314272055296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん、行くぞ、ペドロ。ダメだ。やめとこう",
              "maleVariant": "",
              "stringId": "1803780257218703360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お金を持ってそうだから",
              "maleVariant": "",
              "stringId": "1803789537082748932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "危ないと思ったから",
              "maleVariant": "",
              "stringId": "1803790113547890688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お金を持ってそうに見えたから",
              "maleVariant": "カネを持ってそうに見えたからな",
              "stringId": "1803802761656152064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "放っておいたら、最悪死んでたでしょ",
              "maleVariant": "放っておいたら、最悪死んでただろ",
              "stringId": "1803802936038535172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや。帰りたくねえだけだ",
              "maleVariant": "",
              "stringId": "1803948385340895232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "詳しく聞かせてよ",
              "maleVariant": "詳しく聞かせてくれ",
              "stringId": "1803948831447068676"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "夫婦仲をなんとかしてくれって？ パスよ",
              "maleVariant": "夫婦仲をなんとかしてくれって？ パスだよ",
              "stringId": "1803950634326700032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "詳しく聞かせて",
              "maleVariant": "詳しく聞かせろ",
              "stringId": "1803952405816160256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "夫婦のもめ事を仲裁しろって？ 悪いけどパス",
              "maleVariant": "夫婦のもめ事を仲裁しろって？ 悪いけどパスだ",
              "stringId": "1803952497855967236"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうか… まあ、仕方ない。気が変わったら言ってくれ",
              "maleVariant": "",
              "stringId": "1803977739345211392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺の女房、シンシアのことだ。あいつ、浮気してるんだ、間違いない。ただ、相手がわからないんだよ",
              "maleVariant": "",
              "stringId": "1803978363759636480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、やる",
              "maleVariant": "",
              "stringId": "1803979515834281988"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奥さんはどこにいるの？",
              "maleVariant": "奥さんはどこにいる？",
              "stringId": "1803979968265465856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "浮気をしてる確証があるの？",
              "maleVariant": "浮気をしてる確証があるのか？",
              "stringId": "1803980456163684352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "相手を知ってどうするの？",
              "maleVariant": "相手を知ってどうするんだ？",
              "stringId": "1803980630948720640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奥さんを尾行して、誰と会ってるか確かめたい。今はどこに？",
              "maleVariant": "",
              "stringId": "1803986762400411648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "浮気の確証があるの？",
              "maleVariant": "浮気の確証があるのか？",
              "stringId": "1803987272712990724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "近くで働いてるから住所を教える。あいつのことだから、タバコ休憩してるだろうな。ヘビースモーカーなんだ",
              "maleVariant": "",
              "stringId": "1803992328040493056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "えーと… 今日はピンクのジャケットを着てたかな？ それと… んー… 青いバッグを持ってたはずだ",
              "maleVariant": "",
              "stringId": "1803993258253234176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まず、仕事から帰ってくるのがいつも遅いんだ。ウソをついてるのはわかってる。この前、夜オフィスに電話したら出勤日なのにいなかったしな",
              "maleVariant": "",
              "stringId": "1803995158340055040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昔はこういうのも簡単に調べられたけど、今は最新テックでなんでも誤魔化せちまうだろ？",
              "maleVariant": "",
              "stringId": "1803996257163816960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わざわざ相手が誰かを知ってどうするの？",
              "maleVariant": "わざわざ相手が誰かを知ってどうすんだよ？",
              "stringId": "1804005183582253056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうするかなんて、そんなの決まってるだろ！",
              "maleVariant": "",
              "stringId": "1804005524730163200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミエルダ。金をたっぷり持ってそうだったのに",
              "maleVariant": "",
              "stringId": "1804095019299254272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、まぁ、他をあたろうぜ",
              "maleVariant": "",
              "stringId": "1804095712970027008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "近寄るな！ そのほうが身のためだぞ！",
              "maleVariant": "",
              "stringId": "1804107404038778880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2人がかりで酔っ払いからカネを巻き上げようっての？ 情けないんじゃない？",
              "maleVariant": "2人がかりで酔っ払いからカネを巻き上げようってのか？ 情けないな、おい",
              "stringId": "1913896741858758656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "てめえ、黙らねえと、その脇腹をこいつでぶっ刺すぞ",
              "maleVariant": "",
              "stringId": "1913899597861015552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "許可をくれるのか、お嬢ちゃん？ そりゃどうも。じゃあ失せな",
              "maleVariant": "許可をくれるのか、お坊ちゃん？ そりゃどうも。じゃあ失せな",
              "stringId": "1913902141958049792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ、来いよ、チューマ。外に新鮮な空気を吸いに行こうぜ",
              "maleVariant": "",
              "stringId": "1913902880944082944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっとショットガンであんたたちの頭をぶっ飛ばすんじゃない？",
              "maleVariant": "きっとショットガンであんたらの頭をぶっ飛ばすんじゃないか？",
              "stringId": "1913908770099220480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたのニューロンに信号が伝わる前に、風穴が開くことになると思うけどね",
              "maleVariant": "あんたのニューロンに信号が伝わる前に、風穴が開くことになると思うけどな",
              "stringId": "1913910408075595776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもまあ、せっかくだから少し実験してみようか？",
              "maleVariant": "",
              "stringId": "1913913390947160064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソッタレ！ カラホ！ 次は容赦しねえ！",
              "maleVariant": "",
              "stringId": "1913915422600908800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっとした問題。もう大丈夫",
              "maleVariant": "ちょっとした問題さ。もう大丈夫だ",
              "stringId": "1913926363610664960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まったく… 悪いな――ちょっと飲み過ぎたらしい… クソ… なんて夜だ…",
              "maleVariant": "",
              "stringId": "1913926610588061696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして助けてくれたんだ？",
              "maleVariant": "",
              "stringId": "1913927466662285312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど… この街じゃ、この人生じゃ、なんでもカネ次第ってわけか",
              "maleVariant": "",
              "stringId": "1913933411031240704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら。これで全部だ。残りは… バーテンダーに",
              "maleVariant": "",
              "stringId": "1913933928373473280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この街は、ここの連中は… 俺は一人で飲んでただけなのに… あんたのおかげで助かったよ…",
              "maleVariant": "",
              "stringId": "1913935992038150144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これを。面倒かけたな。残りは… バーテンダーに",
              "maleVariant": "",
              "stringId": "1913939284382240768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "結婚して10年。10年もたって、急に俺じゃ駄目なんだとよ。あの女、きっと外で別の男と浮気してんだ",
              "maleVariant": "",
              "stringId": "1913945340890107904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだ、カネの話が出たことだし… この件で力を貸してくれないか？ 報酬は払う",
              "maleVariant": "",
              "stringId": "1913947174035836928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あれこれ一人で思い悩んでるより、お前みたいな奴を雇ったほうがいいだろ。一晩で片付く簡単な仕事だ",
              "maleVariant": "",
              "stringId": "1913954756045623296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっぱりやめておく",
              "maleVariant": "",
              "stringId": "1913958093855969280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪いけど、興味ないね",
              "maleVariant": "悪いが、興味ないね",
              "stringId": "1913959470493650944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それだったら、答えなよ",
              "maleVariant": "それだったら、答えろよ",
              "stringId": "1913961456882470912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そいつの目を見て、他の男の女房と寝て他人の結婚生活を壊すのはどんな気分か、問い詰めるだけさ",
              "maleVariant": "",
              "stringId": "1913961881966792704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "街の半分がね",
              "maleVariant": "街の半分がな",
              "stringId": "1913969914998476800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "派手だね",
              "maleVariant": "派手だな",
              "stringId": "1913970857894793216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おしゃれ好きなんだ。誕生日や記念日に、女にいいものを買ってやって… そのお返しがこれだもんな",
              "maleVariant": "",
              "stringId": "1913971004091453440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おいおい、そんなこと言うなよ――ああ、もういい",
              "maleVariant": "",
              "stringId": "1913972066475102208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうこなくっちゃ、V。本当に助かるよ",
              "maleVariant": "",
              "stringId": "1913973264116666368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。やるよ",
              "maleVariant": "",
              "stringId": "1913976634139267072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、お前ら、何やってる！",
              "maleVariant": "",
              "stringId": "1933310411455737856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V！ こいつらに俺のバーでの礼儀ってものを教えてくれ！",
              "maleVariant": "",
              "stringId": "1933312496276484096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつら、まるでコーポの血を嗅ぎつけたサメだな… V、もしお前が偶然居合わせなかったら危なかった",
              "maleVariant": "",
              "stringId": "1933320580008914948"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあお前に乾杯といくか？ タダでいいぞ",
              "maleVariant": "",
              "stringId": "1933320581468532736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいね",
              "maleVariant": "",
              "stringId": "1933323141856243716"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やめとく",
              "maleVariant": "",
              "stringId": "1933323317631135744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありたがいけど、やめとく",
              "maleVariant": "ありたがいが、やめとく",
              "stringId": "1933324834056581120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "タダ酒なら喜んで",
              "maleVariant": "",
              "stringId": "1933324908312539136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕事中は飲まないって？ わかったよ。さすがだな",
              "maleVariant": "",
              "stringId": "1933328040249667584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "幸運と繁栄に、乾杯！",
              "maleVariant": "",
              "stringId": "1933328384920793088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとな、V。お見事だよ",
              "maleVariant": "",
              "stringId": "1934651021072674816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ぺぺの言うことが聞こえたでしょ。さっさと出て行きな",
              "maleVariant": "ペペの言うことが聞こえただろ。さっさと出て行け",
              "stringId": "1935673400622370816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よかったね、あたしがヒーローで",
              "maleVariant": "よかったな、俺がヒーローで",
              "stringId": "1935688948135546880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お疲れね？",
              "maleVariant": "お疲れだな？",
              "stringId": "1935731572565397504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあな。日勤夜勤の両方で、おまけに休憩なし",
              "maleVariant": "",
              "stringId": "1935731945741012992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "目的は… 残業代？",
              "maleVariant": "目的は… 残業代か？",
              "stringId": "1935735375138181120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それに… まあ、子供がいるんだけどさ。俺と全然似てないんだ。髪も、目も、全部違う！",
              "maleVariant": "",
              "stringId": "1935755973683892224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
