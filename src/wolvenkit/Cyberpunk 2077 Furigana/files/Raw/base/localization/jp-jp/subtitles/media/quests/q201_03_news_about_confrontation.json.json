        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "過去数週間に渡り、アラサカ、ミリテク両社の間の緊張は、かつてないほどまでに高まっていました",
                    "maleVariant": null,
                    "stringId": 1743128075853680640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「継続は力なり」――アラサカ社の基本理念です",
                    "maleVariant": null,
                    "stringId": 1743128857470619648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サブロウ・アラサカ氏とロザリンド・マイヤーズ新合衆国大統領は、恒久的平和の実現に向けて会談を行うことで合意しました",
                    "maleVariant": null,
                    "stringId": 1743129221250994176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "多くの評論家や専門家は、両社間で武力衝突が起こる可能性を警告――",
                    "maleVariant": null,
                    "stringId": 1743131549039382528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最も緊張が高まったのは、亡き父の後を継いだヨリノブ・アラサカ氏が、父親殺害の背景にはミリテク社がいたとして、同社を糾弾した時でした",
                    "maleVariant": null,
                    "stringId": 1743131827675385856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しかし事態は急転直下、サブロウ・アラサカ氏が代表の座に戻ったことで、二大企業間の緊張も和らいだものと考えられます",
                    "maleVariant": null,
                    "stringId": 1743132092067532800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "息子のヨリノブはこの言葉を忘れ、焦って変化を求めすぎました",
                    "maleVariant": null,
                    "stringId": 1743133623089156096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私が敵味方両者に説き続けてきた教訓――完璧な物を変える必要はない。これが真理なのです",
                    "maleVariant": null,
                    "stringId": 1743134039717761024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "以上となります",
                    "maleVariant": null,
                    "stringId": 1743134328688529408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "二社間の協力体制の最初の一歩として、アラサカ社CEOは今日、アカアシ型武装車両15台をナイトシティから全面的に撤退させることを決定しました",
                    "maleVariant": null,
                    "stringId": 1743140047974354944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "N54ニュースでは、続報が入り次第お伝えします",
                    "maleVariant": null,
                    "stringId": 1743140692890537984
                  }
                }
              ]
            }
          }
        }
      }
    },
