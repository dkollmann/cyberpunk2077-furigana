            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "すみません、御社の発電所が付近の住民に健康被害を及ぼしているとの疑いがありますが、コメントをいただけないでしょうか？",
                    "maleVariant": null,
                    "stringId": 1963261700214923264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ペトロケムは市民のみなさんのことを最優先に考えていますのでご安心ください",
                    "maleVariant": null,
                    "stringId": 1966348923576238080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "記憶喪失や奇妙な火傷痕などを訴えているアロヨの人々は納得しないと思いますが",
                    "maleVariant": null,
                    "stringId": 1966349165285588992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あなた… 半年くらい前に似たような質問をしてきた別の記者と似ていますね",
                    "maleVariant": null,
                    "stringId": 1966349418286006272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "確か、まだ昏睡状態だとか。容疑者はまだ捕まってないようですよ",
                    "maleVariant": null,
                    "stringId": 1966349634007449600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それは… 脅迫ですか？",
                    "maleVariant": null,
                    "stringId": 1966349818389053440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "記事にするならもっといいネタがあるでしょう。ジギーQの新しいネクタイとか",
                    "maleVariant": null,
                    "stringId": 1966350112845971456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その方が読者も喜ぶし、あなたも長い人生を楽しめますよ",
                    "maleVariant": null,
                    "stringId": 1967630927592669184
                  }
                }
              ]
            }
          }
        }
      }
