    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しばらくここにいるの？",
                    "maleVariant": "しばらくここにいるのか？",
                    "stringId": 1950308564545957892
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "パナムとあんたとカウボーイの話、教えてよ",
                    "maleVariant": "パナムとあんたとカウボーイの話、教えてくれよ",
                    "stringId": 1950309647062265856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの建設現場からよく生きて出てこれたね",
                    "maleVariant": "あの建設現場からよく生きて出てこれたな",
                    "stringId": 1950309927275327488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しばらくカリフォルニアに残るつもり？",
                    "maleVariant": "しばらくカリフォルニアに残るつもりか？",
                    "stringId": 1950310958889562112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたとパナムとカウボーイの話、詳しく聞きたいんだけど",
                    "maleVariant": "あんたとパナムとカウボーイの話、詳しく聞きたいんだが",
                    "stringId": 1950311296027717632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "建設現場でのミリテク戦、いったいどうやって…",
                    "maleVariant": null,
                    "stringId": 1950311555940347904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺たちのクランには仕事が必要なんだ。儲かる類のな。そうなるとナイトシティが一番だ",
                    "maleVariant": null,
                    "stringId": 1952915333122224128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ただここで仕事すると、いずれはどこかの誰かを怒らせることになる",
                    "maleVariant": null,
                    "stringId": 1952918321647710208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だから様子を見るつもりだ",
                    "maleVariant": null,
                    "stringId": 1952919833107099648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "別に大したもんじゃねえ。道で暮らす連中にとっちゃ、誰もが一度は経験するようなよくある話さ",
                    "maleVariant": null,
                    "stringId": 1952922791265824768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "話してくれないわけ？",
                    "maleVariant": "話してくれないのかよ？",
                    "stringId": 1952924925327699968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺とパナムは“カウボーイ”に頼まれて、シスコからカンザスに荷物を運んでたんだ",
                    "maleVariant": null,
                    "stringId": 1952924988946903040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人工肉ファームのオーナーをそう呼んでてな。カンザスじゃありきたりの光景だが、パナムにとっては初めてだった",
                    "maleVariant": null,
                    "stringId": 1952929950036783104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で、そこへ行ってボイラー用のバルブだのなんだのを荷降ろししてたら、とにかく臭いがすごくてよ。パナムの奴、吐きそうになったんだ",
                    "maleVariant": null,
                    "stringId": 1952932035025301504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "んでオーナーが、カウボーイハットを手に颯爽と歩いてきて、あいさつしようとした瞬間…",
                    "maleVariant": null,
                    "stringId": 1952933939524530176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あー…",
                    "maleVariant": null,
                    "stringId": 1952935177280417792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "パナムはその日の昼飯をそいつのブーツにぶちまけた",
                    "maleVariant": null,
                    "stringId": 1952935488665546752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あーこの仕事は終わったな、って思ったよ。でもそのカウボーイは「お辞儀はいらない。握手でいい」って言ったのさ",
                    "maleVariant": null,
                    "stringId": 1952937497133182976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "AVでの一件で、パナムが即座に動けたのはそういうことか",
                    "maleVariant": null,
                    "stringId": 1952943977651630080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうやって生き延びたか知りたいのか？",
                    "maleVariant": null,
                    "stringId": 1952984593295077376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺たちの訓練では、相棒がやられた時のために、一人でも操縦できるようにさせられたんだ。それが役に立った",
                    "maleVariant": null,
                    "stringId": 1952985555116093440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺は昔ながらの傭兵だ。数多くの死線を潜り抜けてきた。当時は戦士になるように鍛えられたもんだ",
                    "maleVariant": null,
                    "stringId": 1952989813341286400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "企業が正面切っての撃ち合いを止めてから、ミリテクは経費削減で訓練を外注しててな。俺みたいな奴にゃ動きが読めるわけさ",
                    "maleVariant": null,
                    "stringId": 1952994756882198528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でもパンツァーを2人目なしで操縦するなんて、不可能じゃないの？",
                    "maleVariant": "だがパンツァーを2人目なしで操縦するなんて、不可能じゃないのか？",
                    "stringId": 1956207632048582656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その代わり、この先何ヶ月かは頭痛に悩まされるだろうな",
                    "maleVariant": null,
                    "stringId": 1956212948429565952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの建設現場からよく無事で…",
                    "maleVariant": null,
                    "stringId": 2140700139260235808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあね",
                    "maleVariant": "じゃあな",
                    "stringId": 2524599062293049344
                  }
                }
              ]
            }
          }
        }
      }
    },
