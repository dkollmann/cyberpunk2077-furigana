  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "資本は圧制システムにおける血液に等しい！",
              "maleVariant": "",
              "stringId": 1828746088348295168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "金とは、思考を操る植民地的圧制の道具でしかない！",
              "maleVariant": "",
              "stringId": 1828751991713734656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ネットから切断させる]",
              "maleVariant": "",
              "stringId": 1831301583219105796
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまり… こういうことよね… 誰かがこの占い師ロボットをハックして、アルゴリズムをいじり、占いの代わりに反体制的な格言を言うようにさせた…",
              "maleVariant": "つまり… こういうことか… 誰かがこの占い師ロボットをハックして、アルゴリズムをいじり、占いの代わりに反体制的な格言を言うようにした…",
              "stringId": 1831301872340869120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[意味不明なことを発信するようにいじる]",
              "maleVariant": "",
              "stringId": 1831305666961252352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "思考を力強いルンバと喜びに満ちたアコーディオンに乗せて踊らせよ",
              "maleVariant": "",
              "stringId": 1831309163903492100
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "成功とは魂の希釈物である！",
              "maleVariant": "",
              "stringId": 1831450512652935168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "生産手段は集合的無意識に属すべきものである！",
              "maleVariant": "",
              "stringId": 1831450679452016640
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "格差社会こそが、無秩序の温床になっている！",
              "maleVariant": "",
              "stringId": 1831733552675463168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと… 何これ？",
              "maleVariant": "はあ？ 何だこれ？",
              "stringId": 1831733968616202240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[小銭を入れて予言を聞く]",
              "maleVariant": "",
              "stringId": 1831735198352580612
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "中身を見てみないと",
              "maleVariant": "",
              "stringId": 1831735855717457920
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "激しい階級闘争が勃発するとは思えないような時こそ、魅力的なロビイストが生まれるものだ！",
              "maleVariant": "",
              "stringId": 1831739612119343104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…それで、発信元を隠すルーターを複数かましてネットにつないだってわけだ。こりゃ天才的だな！",
              "maleVariant": "",
              "stringId": 1831743177277743104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "焼けるような氷に囲まれた、発情期のイッカクのようになれ",
              "maleVariant": "",
              "stringId": 1831750117642903552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "めまぐるしく動く経済のたまり場で、キツネの子たちがデータを集めている！",
              "maleVariant": "",
              "stringId": 1831750776551284736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見て。誰かがこいつをネットに繋げたのよ",
              "maleVariant": "見ろ。誰かがこいつをネットに繋げてる",
              "stringId": 1857482422529204224
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ならさっさと調べろよ？ 何かわかるかもしれねえだろ",
              "maleVariant": "",
              "stringId": 1857482984817598464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "調べてみましょ。きっと誰かが中身をいじったのよ",
              "maleVariant": "調べてみよう。きっと誰かが中身をいじったんだ",
              "stringId": 1857636336708788224
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふむ。ここで途切れてる…",
              "maleVariant": "",
              "stringId": 1857819177694437376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ… この辺に隠れてるってことか？",
              "maleVariant": "",
              "stringId": 1857819824674217984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "かもね。もしくは、別のターミナルがあるのかも",
              "maleVariant": "かもな。もしくは、別のターミナルがあるのかも",
              "stringId": 1857820287171731456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "にしても、もし探してる男がここに隠れてるとしたら、納得だな",
              "maleVariant": "",
              "stringId": 1857820581897084928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本気かよ？ まさかこん中に、人間になりたがってる人形が隠れてるってんじゃないだろうな！",
              "maleVariant": "",
              "stringId": 1914405990948839424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰の仕業か調べられないか？ そいつと握手したい",
              "maleVariant": "",
              "stringId": 1914415543291650048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "難しいでしょうね。手掛かりを残してないし、それに… あんたもう手がないでしょ",
              "maleVariant": "難しいだろうな。手掛かりを残してないし、それに… お前もう手がないだろ",
              "stringId": 1914416629884821504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、そうだった。ついつい忘れちまうんだよな",
              "maleVariant": "",
              "stringId": 1914417060488847360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだ、もっといい考えがある",
              "maleVariant": "",
              "stringId": 1914418701669670912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭が空っぽの連中にぴったりの娯楽だからだよ",
              "maleVariant": "",
              "stringId": 1915560034029588480
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんでよ？ 遊園地は暴走した資本主義の象徴？",
              "maleVariant": "なんでだよ？ 遊園地は暴走した資本主義の象徴か？",
              "stringId": 1915562539723575296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見てのとおり、スウェーデンボルグ＝リビエラだ！ ハッ！ こいつがナイトシティの予言者だったってわけだよ！ ハッ！",
              "maleVariant": "",
              "stringId": 1915567931635585024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ… 傑作だな… 箱に入った過激な哲学者かよ… 小銭をくれ。次は何て言うか試してみようぜ！",
              "maleVariant": "",
              "stringId": 1915569275926794240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いったいどうなってるの？",
              "maleVariant": "いったいどうなってんだ？",
              "stringId": 1915574603850502144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これは… いったいどういうこと？",
              "maleVariant": "これは… いったいどういうことだ？",
              "stringId": 1915576473889329152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "脳内物質がとめどなく流れるように生産手段が回り出すと、次第に運も向いてくるだろう！",
              "maleVariant": "",
              "stringId": 1915577663712710656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これ、録画してるか？ 後世のために残しておくべき教材だぞ！ ハハ！",
              "maleVariant": "",
              "stringId": 1915578147852832768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあな。でもよ、V… こいつは見事じゃないか？ ネットの半分が、ガラス箱に入った機械人形の言葉について思い悩んでるんだ。無駄な詮索をしたり、解釈を巡って議論したりな…",
              "maleVariant": "",
              "stringId": 1915582177857556480
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マジで、魂の監獄に50年もぶち込まれたかいがあった。こんな面白いもんを拝めたんだからな",
              "maleVariant": "",
              "stringId": 1915584646104797184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V… そいつはそのままにしてやれよ",
              "maleVariant": "",
              "stringId": 1915599416748986368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心配しないで、ジョニー。ナイトシティの人気哲学者は、このままにしておきましょ",
              "maleVariant": "心配するな、ジョニー。ナイトシティの人気哲学者は、このままにしておこう",
              "stringId": 1915601423488569344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "冗談ってのは、何回も言うと面白くなくなるものよ",
              "maleVariant": "冗談ってのは、何回も言うと面白くなくなるもんだ",
              "stringId": 1915601801043038208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、よせって、V！ あー、だからお前はつまんねえんだ",
              "maleVariant": "",
              "stringId": 1915602701526552576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうこなくちゃ！ スウェーデンボルグ＝リビエラに乾杯！ 長生きしろよ！",
              "maleVariant": "",
              "stringId": 1915616937984184320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ？ ハッ！ お前もたまにはやるじゃねえか",
              "maleVariant": "",
              "stringId": 1923104831264956432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まったく… 俺たちは滅多にウマが合わねえが、こうやってハマるときがあるから面白いんだよな",
              "maleVariant": "",
              "stringId": 1923104831264956444
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "思考を力強いルンバと喜びに満ちたアコーディオンに乗せて踊らせよ",
              "maleVariant": "",
              "stringId": 1927427021103865856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "焼けるような氷に囲まれた、発情期のイッカクのようになれ",
              "maleVariant": "",
              "stringId": 1927427021103865868
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レッサーパンダによるこの傷をよく見よ――大好きなジュースが近くに落ちている！",
              "maleVariant": "",
              "stringId": 1927427021103865880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "小銭は持ってない",
              "maleVariant": "",
              "stringId": 1945897659946672128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー… 小銭がないな…",
              "maleVariant": "",
              "stringId": 1945898069696618496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "金は、社会的エントロピーの根源である！",
              "maleVariant": "",
              "stringId": 1945898391215185920
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心配しないで。野暮なことはしない",
              "maleVariant": "心配するな。野暮なことはしない",
              "stringId": 2086802285006213124
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}