    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1668242618758946816,
            "femaleVariant": "ジャック。デクスと話したよ",
            "maleVariant": "ジャック。デクスと話したぞ"
          },
          {
            "stringId": 1668242853623193600,
            "femaleVariant": "なかなかの“大物”だったろ？ 色んな意味でな"
          },
          {
            "stringId": 1669642123433697280,
            "femaleVariant": "で、ミリテクの件は… 結局どうしたんだ"
          },
          {
            "stringId": 1669642123433697288,
            "femaleVariant": "会って話をした。丁重に扱われたよ。裏があるかどうかは何とも言えない"
          },
          {
            "stringId": 1741806682925625344,
            "femaleVariant": "慎重な奴ね。いきなりデカい仕事を任せたりしない。まずは下準備に、いくつか雑用をこなすことになった",
            "maleVariant": "慎重な奴だ。いきなりデカい仕事を任せたりしない。まずは下準備に、いくつか雑用をこなすことになった"
          },
          {
            "stringId": 1741806701917433860,
            "femaleVariant": "色々と仕事を頼まれた"
          },
          {
            "stringId": 1741806709752393728,
            "femaleVariant": "まずはあたしたちの力量を見極めたいんだって。メイルストロームとミリテクのいざこざに巻き込まれた感じがしなくもないけど",
            "maleVariant": "まずは俺たちの力量を見極めたいんだとさ。メイルストロームとミリテクのいざこざに巻き込まれた感じがしなくもないが"
          },
          {
            "stringId": 1741806935707938816,
            "femaleVariant": "デクスはあたしたちの力量を試そうとしてる",
            "maleVariant": "デクスは俺たちの力量を試そうとしてる"
          },
          {
            "stringId": 1741828265119510528,
            "femaleVariant": "ほう、面白そうだな。詳しく聞かせてくれ"
          },
          {
            "stringId": 1741832357434814464,
            "femaleVariant": "なんだ、気に入らねえのか？"
          },
          {
            "stringId": 1762006311520628736,
            "femaleVariant": "試されてるとしか思えない。信用するなんて言っときながら、実力を証明してみせろって？ なんだかね",
            "maleVariant": "試されてるとしか思えない。信用するなんて言っときながら、実力を証明してみせろだ？ なんだかな"
          },
          {
            "stringId": 1767692545108434944,
            "femaleVariant": "メイルストロームが軍の戦闘用ボットを盗んだの。デクスはそれを買い取ろうと金を払ったんだけど、直後に組織のトップが入れ替わった",
            "maleVariant": "メイルストロームが軍の戦闘用ボットを盗んだんだ。デクスはそれを買い取ろうと金を払ったんだが、直後に組織のトップが入れ替わった"
          },
          {
            "stringId": 1767694057188581376,
            "femaleVariant": "ふん、なるほどな"
          },
          {
            "stringId": 1767694736883933188,
            "femaleVariant": "まずメイルストロームと会う"
          },
          {
            "stringId": 1767695253873844224,
            "femaleVariant": "先にパーカーと会う"
          },
          {
            "stringId": 1767695934122844160,
            "femaleVariant": "まずはメイルストロームね。すんなり行くとは思えないけど",
            "maleVariant": "まずはメイルストロームだ。すんなり行くとは思えないがな"
          },
          {
            "stringId": 1767695962291789824,
            "femaleVariant": "まずは依頼主に会う。どんな奴なのか、彼女の狙いを確かめたい"
          },
          {
            "stringId": 1767704537043001344,
            "femaleVariant": "あたしも後で向かう",
            "maleVariant": "俺も後で向かう"
          },
          {
            "stringId": 1767705106931474432,
            "femaleVariant": "V、ひとついいか…"
          },
          {
            "stringId": 1767742234440720384,
            "femaleVariant": "V、待ってたぜ"
          },
          {
            "stringId": 1767753786124361728,
            "femaleVariant": "依頼主と会った。エヴリン・パーカーって女よ",
            "maleVariant": "依頼主と会った。エヴリン・パーカーって女だ"
          },
          {
            "stringId": 1767764707588661248,
            "femaleVariant": "会って話をすることになった。何とかなるでしょ",
            "maleVariant": "会って話をすることになった。何とかなるだろ"
          },
          {
            "stringId": 1767765385102974976,
            "femaleVariant": "V… くれぐれも慎重にな"
          },
          {
            "stringId": 1767771762995191808,
            "femaleVariant": "俺はオールフーズにいるからよ。急いでくれ"
          },
          {
            "stringId": 1767780701962985472,
            "femaleVariant": "まだ何も"
          },
          {
            "stringId": 1767837707151302656,
            "femaleVariant": "ミリテクを信用するのはやめとけ。握手しようとした手を切り落とすような、とにかく危険な連中だ… 気をつけろよ"
          },
          {
            "stringId": 1772005558462640128,
            "femaleVariant": "なに？",
            "maleVariant": "なんだ？"
          },
          {
            "stringId": 1772009407323762688,
            "femaleVariant": "まあそんなとこ。デクスはロイスと話せと言ってる。あとミリテクのエージェントの情報もくれたけど… 彼女が役に立つかは未知数ね",
            "maleVariant": "まあそんなとこだ。デクスはロイスと話せと言ってる。あとミリテクのエージェントの情報もくれたが… 彼女が役に立つかは未知数だな"
          },
          {
            "stringId": 1772062093133934592,
            "femaleVariant": "わかった。俺は一足先にオールフーズに行くか。現地の周辺を探っておくぜ"
          },
          {
            "stringId": 1772155716710100996,
            "femaleVariant": "エヴリンは大物と繋がってる"
          },
          {
            "stringId": 1772155730920402944,
            "femaleVariant": "エヴリンは大物と繋がってる… とんでもない大物とね",
            "maleVariant": "エヴリンは大物と繋がってる… とんでもない大物とな"
          },
          {
            "stringId": 1772155733755752448,
            "femaleVariant": "どうも妙な感じがした"
          },
          {
            "stringId": 1772156519768322048,
            "femaleVariant": "どうも妙な感じがした"
          },
          {
            "stringId": 1772161870609707008,
            "femaleVariant": "へえ。どんな風にだ？"
          },
          {
            "stringId": 1772162427059630080,
            "femaleVariant": "上手く言えないけど、信用できない。何か大事なことを隠してる",
            "maleVariant": "上手く言えないが、信用できない。何か大事なことを隠してる"
          },
          {
            "stringId": 1772164133705150464,
            "femaleVariant": "なんだ、ずいぶん神経質じゃねえか。そうやって誰も彼も疑ってたら、ストレスで死んじまうぜ？"
          },
          {
            "stringId": 1772208092343103488,
            "femaleVariant": "俺たちだって、あのデクスと組んでるぜ"
          },
          {
            "stringId": 1772208536519897088,
            "femaleVariant": "向こうはヨリノブ・アラサカよ。夜の相手をしてる",
            "maleVariant": "向こうはヨリノブ・アラサカだ。夜の相手をしてる"
          },
          {
            "stringId": 1772208911104798720,
            "femaleVariant": "ミエルダ！ おい、マジかよ！"
          },
          {
            "stringId": 1776466802917699584,
            "femaleVariant": "V。俺はこういうのに鼻が利く、知ってるだろ。企業の連中とは関わらないほうが吉だ"
          },
          {
            "stringId": 1776469522219200512,
            "femaleVariant": "で、ミリテクの件はどうすんだ？"
          },
          {
            "stringId": 1776473013373300736,
            "femaleVariant": "ミリテクの件は進展あったのか？"
          },
          {
            "stringId": 1776487301370540032,
            "femaleVariant": "そうか。このまま関わらないほうが吉だぞ"
          },
          {
            "stringId": 1784859163293052928,
            "femaleVariant": "また後でな"
          },
          {
            "stringId": 1784861443971018752,
            "femaleVariant": "へえ、何者だ？ どんな奴だった？"
          },
          {
            "stringId": 1995093596603084800,
            "femaleVariant": "ああ、その件なら聞いてる。ロイスとブリックがやり合ったんだろ"
          },
          {
            "stringId": 1995101727496269828,
            "femaleVariant": "それともうひとつ"
          },
          {
            "stringId": 1995101794538024960,
            "femaleVariant": "それともうひとつ。この件の依頼主、エヴリン・パーカーに会わないといけない"
          },
          {
            "stringId": 1995101798648442880,
            "femaleVariant": "気に入らなさそうね。ロイスについて何か知ってるの？",
            "maleVariant": "気に入らなさそうだな。ロイスについて何か知ってるのか？"
          },
          {
            "stringId": 1995102950790197248,
            "femaleVariant": "ロイスについて何か知ってることは？"
          },
          {
            "stringId": 1995110400914288640,
            "femaleVariant": "サイコパスにも色々いる。ヤバさ度合いはピンキリだ"
          },
          {
            "stringId": 1995110640862031872,
            "femaleVariant": "ロイスは文句なし、トップクラスのイカレ野郎さ。クロームにとり憑かれてるようなもんだ"
          },
          {
            "stringId": 1995150344378576896,
            "femaleVariant": "そりゃデクスの仕事だろ。あいつは何してんだ？ リムジンで姉ちゃんをはべらすだけか？"
          },
          {
            "stringId": 1995150734683729920,
            "femaleVariant": "クルーと直接会いたがってるから、あたしに行けって",
            "maleVariant": "クルーと直接会いたがってるから、俺に行けって"
          },
          {
            "stringId": 1995151249157058560,
            "femaleVariant": "まあ考えがあってのことなんだろ… で？ メイルストロームとパーカー、どっちから片づける？"
          }
        ]
      }
    }
  }
}