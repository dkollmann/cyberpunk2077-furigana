    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1699884261689643008,
            "femaleVariant": "V。どうぞ、入って"
          },
          {
            "stringId": 1699885739208724480,
            "femaleVariant": "よく来てくれた"
          },
          {
            "stringId": 1699885852018724868,
            "femaleVariant": "何があったのか、だいたいわかった"
          },
          {
            "stringId": 1699885967445970944,
            "femaleVariant": "いかんせん謎の多い事件だった"
          },
          {
            "stringId": 1699891785969029120,
            "femaleVariant": "そうでしょうね。それで――？"
          },
          {
            "stringId": 1750453383196168192,
            "femaleVariant": "ラインの死は事故よ――それも恥ずかしいヤツ。セックスクラブで死んだの。張り切りすぎてね",
            "maleVariant": "ラインの死は事故だ――それも恥ずかしいヤツ。セックスクラブで死んだんだ。張り切りすぎてな"
          },
          {
            "stringId": 1750454344597753856,
            "femaleVariant": "つまり――彼が死んで得する者が関与した可能性はないと？"
          },
          {
            "stringId": 1750454902037532672,
            "femaleVariant": "ご苦労だった、V"
          },
          {
            "stringId": 1750455526804279296,
            "femaleVariant": "とにかく複雑な事件で、不可解な点が多いの。まだ結論は出したくない",
            "maleVariant": "とにかく複雑な事件で、不可解な点が多いんだ。まだ結論は出したくない"
          },
          {
            "stringId": 1750456241647898624,
            "femaleVariant": "別に推測でも構わないさ"
          },
          {
            "stringId": 1750456772680339456,
            "femaleVariant": "ラインの死にはウェルドン・ホルトが関与してた"
          },
          {
            "stringId": 1750457338877825024,
            "femaleVariant": "ある程度はわかった"
          },
          {
            "stringId": 1750459893947158528,
            "femaleVariant": "ラインはセックスクラブで殺された。ウイルス入りBDを渡されてね",
            "maleVariant": "ラインはセックスクラブで殺された。ウイルス入りBDを渡されてな"
          },
          {
            "stringId": 1750460447360405504,
            "femaleVariant": "確かなのか？"
          },
          {
            "stringId": 1941764551241064448,
            "femaleVariant": "どうぞ、座って"
          },
          {
            "stringId": 1941767194743103488,
            "femaleVariant": "報告があるようだな。ぜひ聞かせてくれ"
          },
          {
            "stringId": 1941780506256039940,
            "femaleVariant": "ラインの死は事故で、NCPDがもみ消したの",
            "maleVariant": "ラインの死は事故で、NCPDがもみ消したんだ"
          },
          {
            "stringId": 1941781073577598976,
            "femaleVariant": "ホルトが関与してた"
          },
          {
            "stringId": 1941785909559779328,
            "femaleVariant": "ラインはBDによって殺された"
          },
          {
            "stringId": 1941811175224991744,
            "femaleVariant": "あたしの見解ではね",
            "maleVariant": "俺の見解ではな"
          },
          {
            "stringId": 1941811552443916288,
            "femaleVariant": "よかった。一安心ね…"
          },
          {
            "stringId": 1941811789573087232,
            "femaleVariant": "だとしてらウソをつく必要なんて…？ いや、まあ冷静に考えれば当然か"
          },
          {
            "stringId": 1941814770347499520,
            "femaleVariant": "明確な結論は出せない"
          },
          {
            "stringId": 1941817073339162624,
            "femaleVariant": "ジェフ、真意を汲んであげて"
          },
          {
            "stringId": 1941817731878445056,
            "femaleVariant": "「複雑な事件」ってVは言ったけど、心不全で亡くなるなんて、ちっとも“複雑”じゃないわ"
          },
          {
            "stringId": 1941818565974515712,
            "femaleVariant": "ではやはり…？"
          },
          {
            "stringId": 1941818783239462912,
            "femaleVariant": "ええ。この情報だけで十分よ。ジェフ、これからはさらに注意していかないと"
          },
          {
            "stringId": 1941819401496649728,
            "femaleVariant": "Vはそれを伝えようとしたのよ"
          },
          {
            "stringId": 1941820337413644288,
            "femaleVariant": "妻の言うとおり、これで安心して眠れるな"
          },
          {
            "stringId": 1941830039878983680,
            "femaleVariant": "報酬だ"
          },
          {
            "stringId": 1941831168717185024,
            "femaleVariant": "ええ。あたしも同じBDにやられそうになったから",
            "maleVariant": "ああ。俺も同じBDにやられそうになったからな"
          },
          {
            "stringId": 1941831691042250752,
            "femaleVariant": "犯人の正体は？"
          },
          {
            "stringId": 1941832675680280580,
            "femaleVariant": "ウェルドン・ホルト"
          },
          {
            "stringId": 1941833239075971072,
            "femaleVariant": "わからない"
          },
          {
            "stringId": 1941833391194988544,
            "femaleVariant": "警察が調べてる"
          },
          {
            "stringId": 1941833855152119808,
            "femaleVariant": "確かなことは言えないけど、ウェルドン・ホルトが怪しい",
            "maleVariant": "確かなことは言えないが… ウェルドン・ホルトが怪しい"
          },
          {
            "stringId": 1941833873338621952,
            "femaleVariant": "それはわからなかった"
          },
          {
            "stringId": 1941833891223134208,
            "femaleVariant": "警察が調べてる。ラインの死に関する捜査を再開するみたいよ",
            "maleVariant": "警察が調べてる。ラインの死に関する捜査を再開するみたいだ"
          },
          {
            "stringId": 1941839358045413392,
            "femaleVariant": "やはりな！ 証拠は？"
          },
          {
            "stringId": 1941839358062190592,
            "femaleVariant": "ないけど… そうとしか思えないの",
            "maleVariant": "ないが… そうとしか思えないんだ"
          },
          {
            "stringId": 1941839358062190604,
            "femaleVariant": "十分よ、ジェフ。私たちはホルトを警戒しつつ、警備を強化すればいい"
          },
          {
            "stringId": 1941839358062190616,
            "femaleVariant": "確かに… 君の言う通りだな"
          },
          {
            "stringId": 1941843848953815040,
            "femaleVariant": "そうか。危ない橋を渡らせたな"
          },
          {
            "stringId": 1941844188692439040,
            "femaleVariant": "その情報で十分よ。ありがとう"
          },
          {
            "stringId": 1941846015513157632,
            "femaleVariant": "よかったわ。これで… 一息つける"
          },
          {
            "stringId": 1944687764098998272,
            "femaleVariant": "1つだけ言っておく"
          },
          {
            "stringId": 1944698410148519940,
            "femaleVariant": "何？",
            "maleVariant": "何だ？"
          },
          {
            "stringId": 1944698956498558976,
            "femaleVariant": "1つ以上になりそうな気がする",
            "maleVariant": "1つ以上になりそうな気がするな"
          },
          {
            "stringId": 1944700637978255360,
            "femaleVariant": "1つで済む気がしないけど、どうぞ",
            "maleVariant": "1つで済む気がしないが、どうぞ"
          },
          {
            "stringId": 1944700753170620416,
            "femaleVariant": "何よ",
            "maleVariant": "何だよ"
          },
          {
            "stringId": 1944710015552806912,
            "femaleVariant": "奴らには何も教えるな。この問題はお前が思ってるより深い"
          },
          {
            "stringId": 1944713295045550080,
            "femaleVariant": "油断すると飲み込まれるぞ"
          },
          {
            "stringId": 1944714823298932740,
            "femaleVariant": "大げさね",
            "maleVariant": "大げさだな"
          },
          {
            "stringId": 1944717809727266816,
            "femaleVariant": "油断なんてしない"
          },
          {
            "stringId": 1944717950102233088,
            "femaleVariant": "報告しないと、報酬がもらえないでしょ",
            "maleVariant": "報告しないと、報酬がもらえないだろ"
          },
          {
            "stringId": 1944719948688089088,
            "femaleVariant": "飲み込まれるも何も、報告したら終わりよ",
            "maleVariant": "飲み込まれるも何も、報告したら終わりだ"
          },
          {
            "stringId": 1944719969089183744,
            "femaleVariant": "油断なんかしない"
          },
          {
            "stringId": 1944720005999058944,
            "femaleVariant": "報告しないと、報酬をもらえない"
          },
          {
            "stringId": 1944720801289428992,
            "femaleVariant": "ふん、好きにしやがれ。どうなっても知らねえからな"
          },
          {
            "stringId": 1944726725777383424,
            "femaleVariant": "別に困りはしないだろ"
          },
          {
            "stringId": 1944726967956496384,
            "femaleVariant": "あんたは食べなくていいからね",
            "maleVariant": "お前は食べなくていいからな"
          },
          {
            "stringId": 1949139127415169024,
            "femaleVariant": "ゆっくりしてて。ジェファーソンもすぐ来るわ"
          },
          {
            "stringId": 1949158709580357636,
            "femaleVariant": "[座る] ありがとう"
          },
          {
            "stringId": 1949161780951736320,
            "femaleVariant": "ありがとう"
          },
          {
            "stringId": 1950183727063982084,
            "femaleVariant": "[立つ] 助けになれてよかった"
          },
          {
            "stringId": 1950184281903292416,
            "femaleVariant": "どういたしまして"
          },
          {
            "stringId": 1950229978224615424,
            "femaleVariant": "[立つ] 2人とも気をつけて"
          },
          {
            "stringId": 1950230711305068548,
            "femaleVariant": "2人とも気をつけてね",
            "maleVariant": "2人とも気をつけてくれ"
          },
          {
            "stringId": 2251736540213108736,
            "femaleVariant": "それで会見のほうは？ 市庁舎は使えるの？"
          },
          {
            "stringId": 2251746604428779520,
            "femaleVariant": "それじゃダメよ！ ジェフはつい最近まで議員だったのよ。ラインと同じようにあの場所を使う権利がある"
          },
          {
            "stringId": 2251747697615396864,
            "femaleVariant": "なんとかして。私たちを追い払おうったってそうはいかない。会見は市庁舎で開くのよ"
          },
          {
            "stringId": 2251752335827755008,
            "femaleVariant": "V、力になってくれてありがとう。また何かあったら連絡するから"
          }
        ]
      }
    }
  }
}