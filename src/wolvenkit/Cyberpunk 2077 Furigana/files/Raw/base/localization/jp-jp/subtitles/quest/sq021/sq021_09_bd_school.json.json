    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "古いトロフィー、2042年、学校対抗のレスリングトーナメントで優勝",
                    "maleVariant": null,
                    "stringId": 1354559229814554624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どういうことだ、アンソニー？ これは報告しないといけない。わかってるな？",
                    "maleVariant": null,
                    "stringId": 1560404525063520256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待て、どの学校が優勝したか調べてみる…",
                    "maleVariant": null,
                    "stringId": 1679272597438566400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "カメは病気だったから… 食欲はないし、甲羅にこもって… 助けてやりたかったんだ",
                    "maleVariant": null,
                    "stringId": 1775204307735547904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リアムのカメを殺すなんて――アンソニー、なぜこんなことをやったんだね",
                    "maleVariant": null,
                    "stringId": 1785030042780307456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヒト成長ホルモンを注射してか？ 何を考えてる？ こんなこと、お父さんに教わったのか？",
                    "maleVariant": null,
                    "stringId": 1785040636971454464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ラグーナ・ベンド。50年代に洪水で沈んだ町か",
                    "maleVariant": null,
                    "stringId": 1785055621575479296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いい人たちに引き取ってもらったのに、これが君の恩返しの仕方かね？",
                    "maleVariant": null,
                    "stringId": 1838903295546101760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "友達が死んで辛いのはわかるが… 言い訳にはならない",
                    "maleVariant": null,
                    "stringId": 1840351874584285184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "死ねよ",
                    "maleVariant": null,
                    "stringId": 1840353692429193216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今、何と言った？",
                    "maleVariant": null,
                    "stringId": 1840360976492400640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よし、よくわかった。校長に報告する。里親のお二人にもだ",
                    "maleVariant": null,
                    "stringId": 1840361541230268416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "蛙の子は蛙だな",
                    "maleVariant": null,
                    "stringId": 1840362048187404288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "父さんのことを口にするな…",
                    "maleVariant": null,
                    "stringId": 1840362845423931392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふん、全部知ってるんだからな、ワルガキが。農場が破綻した後、頭を撃って死んだんだろ",
                    "maleVariant": null,
                    "stringId": 1840363526461460480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "黙れ、黙れ、黙れ！！ 父さんの話をするな、クソ野郎！",
                    "maleVariant": null,
                    "stringId": 1840364786497490944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "違う… 違う… 病気で弱ってたんだ… 助けようとしただけだ",
                    "maleVariant": null,
                    "stringId": 1840372731767558144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これでハリスが住んでたおおよその場所はわかった",
                    "maleVariant": null,
                    "stringId": 1927255638418788352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これは、ハリスの子供時代か…",
                    "maleVariant": null,
                    "stringId": 1995084720032669696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この事件… アカデミーのケーススタディに使われてたヤツだ",
                    "maleVariant": null,
                    "stringId": 1997001401079828480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どの学校だったか調べてみる…",
                    "maleVariant": null,
                    "stringId": 1997004874903064576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[調べる]",
                    "maleVariant": null,
                    "stringId": 2101252810638069764
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[調べる]",
                    "maleVariant": null,
                    "stringId": 2101265162796240900
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これで全部のようね…",
                    "maleVariant": "これで全部のようだな…",
                    "stringId": 2243147507819319296
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
