        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Así es, culera.\" t=\"よう、クソアマ\" b=\"\" a=\"\"/>",
              "maleVariant": "<kiroshi l=\"mex\" o=\"Así es, culero.\" t=\"よう、クソ野郎\" b=\"\" a=\"\"/>",
              "stringId": 1805407380061151236
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"A ver si ya dejas de andar con tus mamadas.\" t=\"何メンチ切ってんだ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380077928452
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Este es territorio Valentino.\" t=\"ここはヴァレンティーノズのシマだぞ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380111482884
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¿Por qué chingados metes las pinches narices donde no te llaman?\" t=\"よそ者が何しにきた。消えろ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380128260100
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Ya, guarda esa mierda.\" t=\"おい、武器をしまえ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380178591748
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"No estoy de humor para amenazas. Nomás guarda eso, ¿sí?\" t=\"そんなものを見て、ビビると思ったか？\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380195368964
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"No me provoques.\" t=\"俺を試すなよ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380228923396
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¿Te creíste que encontrabas a una pinche pendeja?\" t=\"てめえ、俺をナメてんのか？\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380245700612
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡A huevo!\" t=\"ふん、それでいい\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380279255044
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Mucho mejor.\" t=\"バカが\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380312809476
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Ehhh... Será mejor que cheque.\" t=\"クソ… 調べたほうがよさそうだな\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380346363908
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¿Qué? Otra vez no...\" t=\"あ？ またかよ…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380363141124
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Todo tranquilón. Por suerte.\" t=\"何もない\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380413472772
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Me vale madres.\" t=\"もうどうでもいい\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380430249988
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Aquí estoy, esperándote.\" t=\"諦めて出て来いよ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380463804420
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¿Y luego? ¿Dónde estás?\" t=\"おい！ どこにいやがるんだ！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380480581636
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Tenemos un problema!\" t=\"問題発生だ！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380514136068
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Bueno, pues que así sea.\" t=\"へいへい…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380547690496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Estás muerta.\" t=\"殺してやる\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380581244932
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Ni pedo, ahora tengo que matarte.\" t=\"てめえ、死んだぞ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380614799364
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡A punto de estallar!\" t=\"グレネードを投げる！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380631576580
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Solo Valentinos!\" t=\"ヴァレンティーノズの力を見せてやれ！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380681908228
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Valentinos por siempre!\" t=\"ヴァレンティーノズ、万歳！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380698685444
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Putísima madre!\" t=\"クソったれ！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380732239876
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Mierda!\" t=\"クソ！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380749017092
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Bájale de huevos.\" t=\"んだよ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380799348740
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Órale.\" t=\"おい！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380816125956
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Oye.\" t=\"よう\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380832903172
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Sipi... seguro... No hay bronca... Mmm, mmm... Ahí estaré...\" t=\"ああ… 問題ない… ああ… 顔を出すよ…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380883234820
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Qué bueno que llamaste... Sí, ayudaré... Sipi, ya está... Sí, va conmigo...\" t=\"ああ、よく連絡くれたな… わかった、手を貸す… ああ、持ってく、持ってく…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380900012036
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Por un lado! ¡El lado!\" t=\"横から攻めろ！ 横だ！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380933566468
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Sí! ¡Allá voy!\" t=\"よし！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380967120900
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Sácate mucho a la chingada!\" t=\"逃げろ！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407380983898116
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Cuídate.\" t=\"じゃあな\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407381000675332
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¿Qué chingados es eso?\" t=\"今のは？\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407381017452548
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Mmm. Sale pues, vamos.\" t=\"ん？ 調べに行くぞ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407381067784196
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Me suena a un problema de dos.\" t=\"おい、一緒に来い\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407381084561412
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Buena suerte. Pongo chonguitos por ti.\" t=\"行ってこい。頑張れよ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407381118115844
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Santa Muerte!\" t=\"死の聖母よ！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407381118115856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Agh... grave... error...\" t=\"くっ… ただじゃ… 済まないぞ…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 1805407381151670276
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¿Eh?\" t=\"ん？\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 2234992076528951300
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Cúbrete las espaldas! ¡Nos están masacrando!\" t=\"背後に気を付けろ！ 狙われてるぞ！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 2234992076797386756
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Carajo... No veo ni mierda...\" t=\"クソ… 見えねえ…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 2234992076814163972
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¿Sí? ¿Qué? No llamé a nadie. Ya déjalo.\" t=\"ん？ なんだ？ 応援は呼んでない。行け\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 2234992076931604484
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Carajo, carajo, carajo! ¡Qué pinche mierda!\" t=\"クソッ！ やってられっか！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 2234992077015490564
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Ay, carajo! ¡¿Por qué?!\" t=\"クソお！ 何でだよお！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 2234992077065822212
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Ponte de rodillas! ¡Empieza a rezar, carajo!\" t=\"ひざまいて、最後のお祈りでもするんだな！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 2234992077132931072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡No vas a ir a ningún lado, perra!\" t=\"どこにも行かせねえぞ！\" b=\"\" a=\"\"/>",
              "maleVariant": "<kiroshi l=\"mex\" o=\"¡No vas a ir a ningún lado, pinche cabrón!\" t=\"どこにも行かせねえぞ！\" b=\"\" a=\"\"/>",
              "stringId": 2234992077183262724
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Te vamos a matar, pendeja!\" t=\"潰してやるからな！\" b=\"\" a=\"\"/>",
              "maleVariant": "<kiroshi l=\"mex\" o=\"¡Te vamos a matar, hijo de puta!\" t=\"潰してやるからな！\" b=\"\" a=\"\"/>",
              "stringId": 2234992077233594372
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Punto para mí, perra!\" t=\"1ポイント獲得だな！\" b=\"\" a=\"\"/>",
              "maleVariant": "<kiroshi l=\"mex\" o=\"¡Punto para mí, culero!\" t=\"1ポイント獲得だな！\" b=\"\" a=\"\"/>",
              "stringId": 2234992077401366532
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Está a punto de caer! ¡Último esfuerzo!\" t=\"もう少しで倒れる！ 最後の一押しだ！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 2234992077585915908
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Yo me encargo de ella!\" t=\"片付けてやるよ！\" b=\"\" a=\"\"/>",
              "maleVariant": "<kiroshi l=\"mex\" o=\"¡Yo me encargo de él!\" t=\"片付けてやるよ！\" b=\"\" a=\"\"/>",
              "stringId": 2234992077636247556
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Ja! ¡¿Ahora te vas a esconder?!\" t=\"今度は隠れるってのかよ！？\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 2234992077686579204
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡No tengo munición! ¡Un segundo!\" t=\"弾がない！ ちょっと待て！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 2234992077753688068
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Qué pedo! ¡Perdieron la pinche cabeza!\" t=\"うわ、とうとうイカれやがったな！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 2234992077837574144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Carajo! ¡Estoy pinche ciego!\" t=\"うお！ 何も見えんぞ！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 2234992078022123524
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡La muy zorra está hackeando!\" t=\"ハックしてやがるぞ！\" b=\"\" a=\"\"/>",
              "maleVariant": "<kiroshi l=\"mex\" o=\"¡El muy cabrón está hackeando!\" t=\"ハックしてやがるぞ！\" b=\"\" a=\"\"/>",
              "stringId": 2234992078089232384
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Uuuf! ¿Cómo es que no te gusta esta mierda?\" t=\"フーッ！ これが嫌いな奴なんかいないだろ！？\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 2234992078139564036
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Esto parece una pinche guerra! ¡Pide refuerzos!\" t=\"まるで戦場じゃねえか！ 応援を呼べ！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 2234992078189895684
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Je, je, je. ¡Me aseguraré de que ardas en el infierno!\" t=\"地獄に落としてやるからな！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 2234992078240227332
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Esperé un chingo para usar esto!\" t=\"早く使いたくてウズウズしてたぜ！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 2234992078307336196
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Atentos! ¡Voy con todo!\" t=\"撃つぞ！ お前ら気をつけろよ！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 2234992078357667844
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"¡Chingada madre! ¿Viste eso? ¡Carajo!\" t=\"おい！ こいつはまずいんじゃないか！？\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": 2234992078424776708
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}