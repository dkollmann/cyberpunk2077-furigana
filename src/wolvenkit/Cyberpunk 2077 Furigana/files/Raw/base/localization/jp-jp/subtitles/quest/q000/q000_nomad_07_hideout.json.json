  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チンガーダ・マドレ！ クソッたれが！",
              "maleVariant": "",
              "stringId": "1660727260740603904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしのせいだって言いたいの？",
              "maleVariant": "俺のせいだって言いたいのか？",
              "stringId": "1660729509290201092"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着いて。こういうこともある",
              "maleVariant": "落ち着け。こういうこともある",
              "stringId": "1660729930616426496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着いて。こういうこともある",
              "maleVariant": "落ち着け。こういうこともある",
              "stringId": "1660731196541259776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[イグアナに触る] 売ろう",
              "maleVariant": "[イグアナに触る] 売るべきだ",
              "stringId": "1660740068031090688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "イグアナに詳しいの？",
              "maleVariant": "イグアナに詳しいのか？",
              "stringId": "1660740068031090704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うお！ 本物のイグアナだ！ たしか、レッサーアンティルとかいうやつじゃねえか？",
              "maleVariant": "",
              "stringId": "1660740835823603712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "売れば金になると思う？",
              "maleVariant": "売れば金になると思うか？",
              "stringId": "1660741391786987520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "相棒だろ。金は山分けだ",
              "maleVariant": "",
              "stringId": "1660742685327118336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レッサーアンティル？",
              "maleVariant": "",
              "stringId": "1660743425755992064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。テレビで見た。小アンティル諸島に生息してて、30年前に絶滅したヤツだ",
              "maleVariant": "",
              "stringId": "1660743425755992072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ホントは飼いたいけどな… ペットが欲しくてよ",
              "maleVariant": "",
              "stringId": "1660747702989770752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マニーって名前まで考えてたのに",
              "maleVariant": "",
              "stringId": "1660749381583785984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ところで… ナイトシティで何するかはもう決めてんのか？",
              "maleVariant": "",
              "stringId": "1660749876914311168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "特に決めてない",
              "maleVariant": "",
              "stringId": "1660750834725572608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして聞くの？",
              "maleVariant": "どうして聞く？",
              "stringId": "1660750834725572624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして聞くの？",
              "maleVariant": "なぜ聞くんだ？",
              "stringId": "1660751711167336448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしのせいだと思ってるなら、そう言えばいいでしょ",
              "maleVariant": "俺のせいだと思ってるなら、そう言えばいいだろ",
              "stringId": "1731663961628012544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんだそりゃ？  フォローになってねえぞ！",
              "maleVariant": "",
              "stringId": "1731681828289310720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前自身はどう思ってんだ？",
              "maleVariant": "",
              "stringId": "1731687851662176256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、2人で一儲けしてやろう",
              "maleVariant": "",
              "stringId": "1731711088458657792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「2人」？",
              "maleVariant": "",
              "stringId": "1731711236182044672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "特に決めてない。ここ数年は、ノーマッドのファミリーと州を行き来して過ごしてたけど",
              "maleVariant": "特に決めてない。ここ数年は、ノーマッドのファミリーと州を行き来して過ごしてたが",
              "stringId": "1732774790444281856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よう、お前、よく生きてたな",
              "maleVariant": "",
              "stringId": "1732872998780596224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこかで帰りを待っててくれてんのか",
              "maleVariant": "",
              "stringId": "1732906860319535104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いえ。別の道を歩むことにしたの",
              "maleVariant": "いや。別の道を歩むことにしたんだ",
              "stringId": "1732906958499803136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "帰る先がないってのは、つれえよな",
              "maleVariant": "",
              "stringId": "1732907265774514176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんつうか、行くだけ行って、ダラダラすんのももったいないと思ってな",
              "maleVariant": "",
              "stringId": "1732910034719797248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それにナイトシティは1人で生きていける場所じゃねえ",
              "maleVariant": "",
              "stringId": "1732910686766297088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気にすんな。これも何かの縁だ。それに、お前とは気が合いそうだしな… 相棒",
              "maleVariant": "",
              "stringId": "1732935380848537600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "助けを頼んだ覚えはない",
              "maleVariant": "",
              "stringId": "1740238259133280256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう",
              "maleVariant": "",
              "stringId": "1740238272840265732"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手助けはいらない",
              "maleVariant": "",
              "stringId": "1740238689737306112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいじゃねえか。これも何かの縁だ。それに、お前とは気が合いそうだしな… 相棒",
              "maleVariant": "",
              "stringId": "1740239352202457088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう、助かる",
              "maleVariant": "",
              "stringId": "1740240449650487296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、心配すんな。俺が住む場所探すのを手伝ってやるからよ",
              "maleVariant": "",
              "stringId": "1740241310388776960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしたちが荷物を持ってるって、税関が企業にたれ込んだのよ",
              "maleVariant": "俺たちが荷物を持ってるって、税関が企業にたれ込んだんだ",
              "stringId": "1812670217100128256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ野郎どもが、賄賂を受け取ったうえで、俺たちを売りやがったのか！ 信じられねえ！",
              "maleVariant": "",
              "stringId": "1812683710880739328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "賭けに出たのよ。あたしたちにクランがついてないと見破られたのね",
              "maleVariant": "賭けに出たんだ。俺たちにクランがついてないと見破られたんだろ",
              "stringId": "1812686329955151872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウソでしょ",
              "maleVariant": "ウソだろ",
              "stringId": "1812693221314064384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケッ… 納得いかねえ。で、どうすんだ？",
              "maleVariant": "",
              "stringId": "1812693583769038848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "開封済みの荷物を受け取って喜ぶ客がいる？ トラブルの元よ",
              "maleVariant": "開封済みの荷物を受け取って喜ぶ客がいるか？ トラブルの元だ",
              "stringId": "1812700626626211840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[開ける] わかった",
              "maleVariant": "",
              "stringId": "1812701465638006784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "客なんか知るか。これからは自分の好きなようにやる。手数料も仲介人もクソ食らえだ",
              "maleVariant": "",
              "stringId": "1812723075665293312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカって書いてある… まさか相手がこんな大物だとは",
              "maleVariant": "",
              "stringId": "1812804648804741120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その分、手に入る金もデカいはずだ",
              "maleVariant": "",
              "stringId": "1812805170458718208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが、売り物が何かも知らずに、新しい買い手は探せねぇ",
              "maleVariant": "",
              "stringId": "1815449295838441472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それなりのフィクサーなら、こいつを欲しがる奴を見つけてくれる",
              "maleVariant": "",
              "stringId": "1815639482442903552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "捕まるところだったじゃねえか！ これのどこが密輸だ！",
              "maleVariant": "",
              "stringId": "1834150743188197376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チンガード！",
              "maleVariant": "",
              "stringId": "1834150743624404992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ、すべてスムーズにいくはずだったのによ",
              "maleVariant": "",
              "stringId": "1834150744127721472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "報酬を払って",
              "maleVariant": "報酬を払ってくれ",
              "stringId": "1885092012469710852"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "州境は越えたでしょ。約束の報酬をもらい次第、解散よ",
              "maleVariant": "州境は越えただろ。約束の報酬をもらい次第、解散だ",
              "stringId": "1885092071508733952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはこっちが聞きたい。報酬も払わないまま、ずっと文句を言い続ける気？",
              "maleVariant": "それはこっちが聞きたい。報酬も払わないまま、ずっと文句を言い続ける気か？",
              "stringId": "1885092082615250944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはこっちが聞きたい",
              "maleVariant": "",
              "stringId": "1885092317815042048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実を言うとよ――いま金欠でな。すぐには払えない",
              "maleVariant": "",
              "stringId": "1885122683250601984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしが黙って待つと思ったわけ？",
              "maleVariant": "俺が黙って待つと思ったのか？",
              "stringId": "1885141424592056320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業のオモチャを届けて、このヤマを片付けりゃ、お前に渡す分ができる",
              "maleVariant": "",
              "stringId": "1885141691282681856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、本当は一切払わずに、州境を越えたらトンズラするつもりだったんだが…",
              "maleVariant": "",
              "stringId": "1885144037844774912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしをだましたのね",
              "maleVariant": "俺をだましたのか",
              "stringId": "1885144826608807936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だましたのね",
              "maleVariant": "だましたのか",
              "stringId": "1885144847731322880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直なのはいいことね",
              "maleVariant": "正直なのはいいことだ",
              "stringId": "1885144997837074432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直なのはいいことね",
              "maleVariant": "正直なのはいいことだ",
              "stringId": "1885145593092698112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう怒んなって。密輸犯のピュアなハートを傷つけちまったか",
              "maleVariant": "",
              "stringId": "1885162312863510528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おうよ",
              "maleVariant": "",
              "stringId": "1885171940603129856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて、どうしようか",
              "maleVariant": "さて、どうするか",
              "stringId": "1885176987525246976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "中身を拝んでみようぜ",
              "maleVariant": "",
              "stringId": "1885178090643976192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前を気に入った",
              "maleVariant": "",
              "stringId": "1885197235896905728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "開けてくれ",
              "maleVariant": "",
              "stringId": "1898158607917420544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしもあんたが気に入った",
              "maleVariant": "俺もお前が気に入った",
              "stringId": "1908362814473670656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃ、このトカゲを連れて出発しましょ、相棒",
              "maleVariant": "じゃ、このトカゲを連れて出発しようか、相棒",
              "stringId": "1908363086516228096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "偶然ね… ちょうど、あたしも同じことを考えてた",
              "maleVariant": "偶然だな… ちょうど、俺も同じことを考えてた",
              "stringId": "1908364155661094912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ、よせよ。照れるじゃねえか",
              "maleVariant": "",
              "stringId": "1908365988269953024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ジャッキーと握手] 行きましょ、相棒",
              "maleVariant": "[ジャッキーと握手] 行こう、相棒",
              "stringId": "1912879289522384896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "次は？",
              "maleVariant": "",
              "stringId": "1944598918306500608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、箱の中身を見てみよう",
              "maleVariant": "",
              "stringId": "1944615282920427520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "商品には触らない方がいい",
              "maleVariant": "",
              "stringId": "1959162683922161668"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ、かわいい奴じゃねえか",
              "maleVariant": "",
              "stringId": "2024420271072583680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと！",
              "maleVariant": "おい！",
              "stringId": "2024444949095706624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[降りる]",
              "maleVariant": "",
              "stringId": "2116916449292341252"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼れる仲間… 家族は誰にも必要だ。お前もナイトシティでそういう奴が見つかるといいな",
              "maleVariant": "",
              "stringId": "2308345229633306624"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}