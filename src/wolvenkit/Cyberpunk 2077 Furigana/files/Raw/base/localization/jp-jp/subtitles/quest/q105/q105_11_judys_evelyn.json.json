          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る]",
              "maleVariant": "",
              "stringId": "1540279308002295812"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ううん、こちらこそ。あなたって… いい人ね。本当に",
              "maleVariant": "",
              "stringId": "1541549002944446464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そろそろ行くか？",
              "maleVariant": "",
              "stringId": "1541694966116155392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[行く] そうね。待つ意味もないし",
              "maleVariant": "[行く] そうだな。待つ意味もないし",
              "stringId": "1541695732549713924"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少し時間をあげましょ",
              "maleVariant": "少し時間をやろう",
              "stringId": "1541696072556773376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、そうね",
              "maleVariant": "ああ、そうだな",
              "stringId": "1541696828471017472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドアの前でボーっとするために苦労してきたわけじゃない",
              "maleVariant": "",
              "stringId": "1541697302326706176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少し待って。彼女の様子を見たでしょ",
              "maleVariant": "少し待とう。彼女の様子を見ただろ",
              "stringId": "1541698254014283776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待ったところで、状態が良くなるってわけでもないだろ",
              "maleVariant": "",
              "stringId": "1541698717484875776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エヴリンは他人と深く関わらないようにしてたけど…",
              "maleVariant": "",
              "stringId": "1547297193846620160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女の様子は？",
              "maleVariant": "",
              "stringId": "1560738181961203712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪いに決まってるでしょ。あの場所はエヴリンから人間としての全てを奪い取ったのよ",
              "maleVariant": "",
              "stringId": "1560755531750031360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エヴリン、気分はどう？",
              "maleVariant": "エヴリン、気分はどうだ？",
              "stringId": "1561737531076100096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気分はどう？",
              "maleVariant": "気分はどうだ？",
              "stringId": "1561738046304403456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こりゃ完全に時間の無駄だ",
              "maleVariant": "",
              "stringId": "1561738845487087616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おいおい、何やってんだ",
              "maleVariant": "",
              "stringId": "1561748322802188288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直、回復の見込みがあるようには思えん",
              "maleVariant": "",
              "stringId": "1561771239640383488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "眠ってるよ",
              "maleVariant": "",
              "stringId": "1630000944581914624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "1630452612704448512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのビッチが目を覚まして、自分のしでかしたことに気づくのをここに突っ立って待つつもりはないぞ",
              "maleVariant": "",
              "stringId": "1641559720368758784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺ならあのスタジオで顔に水でもぶっかけて、その場で細かく問い詰めてた",
              "maleVariant": "",
              "stringId": "1641560843771142144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいにく、決めるのはあたしよ",
              "maleVariant": "あいにく、決めるのは俺だ",
              "stringId": "1641566859124498432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はある種のトランス状態。殻にこもっているみたい。周囲のことには一切反応しない",
              "maleVariant": "",
              "stringId": "1641580791696416768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう差し出せるものは何も残っていなかったのに、奴らは奪い続けたんだ",
              "maleVariant": "",
              "stringId": "1641585137213464576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少なくともそう見える。目を閉じてるし、震えも止まったけど",
              "maleVariant": "",
              "stringId": "1641589358495227904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "周りに聞いてみる",
              "maleVariant": "",
              "stringId": "1643022604986703872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてそんなこと聞くの？",
              "maleVariant": "どうしてそんなこと聞くんだ？",
              "stringId": "1643022605003481092"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんで？ あたしがエヴリンを売ろうとしてるとでも？",
              "maleVariant": "なぜだ？ 俺がエヴリンを売ろうとしてるとでも？",
              "stringId": "1643022612888772608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "違う。最初は信用してなかったけど… エヴリンへの報復が目的なら、とっくにやってるでしょ",
              "maleVariant": "",
              "stringId": "1643051562142752768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ何を心配してるの？",
              "maleVariant": "じゃあ何を心配してるんだ？",
              "stringId": "1643052620516646912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただ… あなたが大丈夫かなって。策はあるの？",
              "maleVariant": "",
              "stringId": "1643053062864723968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何人かツテを当たってみることにする",
              "maleVariant": "",
              "stringId": "1643150851200212992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いきなり連中のシマに乗り込んだって追い返されるのは目に見えてる。でも誰かしら接触方法を知ってるはず",
              "maleVariant": "いきなり連中のシマに乗り込んだって追い返されるのは目に見えてる。だが誰かしら接触方法を知ってるはず",
              "stringId": "1643245490502590464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう、ジュディ",
              "maleVariant": "",
              "stringId": "1643281569419059200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞きたいことがある",
              "maleVariant": "",
              "stringId": "1643350099464249344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何か要る？",
              "maleVariant": "何か要るか？",
              "stringId": "1643359632228048896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "生体チップを取り除く方法を知ってる？",
              "maleVariant": "生体チップを取り除く方法を知ってるか？",
              "stringId": "1643360688353796096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それか、知ってそうな人は？",
              "maleVariant": "",
              "stringId": "1643360688370573320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話を… しにきただけよ",
              "maleVariant": "話を… しにきただけだ",
              "stringId": "1643362377752338432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us][db_db][PL][Milczy]",
              "maleVariant": "",
              "stringId": "1643363180105916416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1643387891602907136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "力を借りたい",
              "maleVariant": "",
              "stringId": "1643395341425008640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エヴリン、力を貸して。大事なことなの",
              "maleVariant": "エヴリン、力を貸してくれ。大事なことなんだ",
              "stringId": "1643398697623183360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早く手を打たないと、あたしは死んでしまう",
              "maleVariant": "早く手を打たないと、俺は死んでしまう",
              "stringId": "1643398697639960584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1643398697639960596"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう",
              "maleVariant": "",
              "stringId": "1646411170239426564"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[触る]",
              "maleVariant": "",
              "stringId": "1646489301012918276"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "座って",
              "maleVariant": "",
              "stringId": "1711010636074999808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ファイルはひどい状態で… あまり編集できなかった",
              "maleVariant": "",
              "stringId": "1711011486713405440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや… 回収できただけでも、良しとしましょ",
              "maleVariant": "いや、回収できただけでも、良しとしよう",
              "stringId": "1711012052390797312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] 始めよう",
              "maleVariant": "",
              "stringId": "1711012573507903492"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どう思う？",
              "maleVariant": "",
              "stringId": "1711014950436757504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK、見せて",
              "maleVariant": "OK、見せてくれ",
              "stringId": "1711183856955363332"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう見たの？",
              "maleVariant": "もう見たのか？",
              "stringId": "1711184484238692352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK。じゃあ見せて",
              "maleVariant": "OK。見せてくれ",
              "stringId": "1711184882580131840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジュディはもう見たのね？",
              "maleVariant": "ジュディはもう見たんだな？",
              "stringId": "1711185632135811072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。意味がわかるまで苦労したけど",
              "maleVariant": "",
              "stringId": "1711186529213550592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あえて何も言わないでおく。あなたの目で見て確かめて",
              "maleVariant": "",
              "stringId": "1711187063148449792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ… ロードするまで待って",
              "maleVariant": "",
              "stringId": "1711190308549890048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "状態はひどいけど、できる限りのことはした",
              "maleVariant": "",
              "stringId": "1711190681389961216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BDはいくつあるの？",
              "maleVariant": "BDはいくつある？",
              "stringId": "1711191784693239808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いくつリッピングできたの？",
              "maleVariant": "いくつリッピングできたんだ？",
              "stringId": "1711192244086968320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2つ。完全な状態なのは1つだけ",
              "maleVariant": "",
              "stringId": "1711193073334423552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残りは… まあ、今のエヴリンと同じ状態よ",
              "maleVariant": "",
              "stringId": "1711193708435935232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エヴリンは全てを話してはいなかった",
              "maleVariant": "",
              "stringId": "1711195558996414468"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エヴリンを雇った女…",
              "maleVariant": "",
              "stringId": "1711196290550140928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エヴリンが狙われた理由がわかった",
              "maleVariant": "",
              "stringId": "1711197003263053824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エヴリンは全てを話してはいなかったようね",
              "maleVariant": "エヴリンは全てを話してはいなかったようだな",
              "stringId": "1711201161747697664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女を雇った女… 誰だか見当はつく？",
              "maleVariant": "彼女を雇った女… 誰だか見当はつくか？",
              "stringId": "1711202371720515584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クラウドで何が起きたのか、これでわかった。奴らが彼女を殺そうとしたのよ",
              "maleVariant": "クラウドで何が起きたのか、これでわかった。奴らが彼女を殺そうとしたんだ",
              "stringId": "1711203259369459712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "罰だったってこと？ 彼女にだまされたから？",
              "maleVariant": "",
              "stringId": "1711212936199213056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エヴリンは知りすぎてたのよ。口封じの必要があった",
              "maleVariant": "エヴリンは知りすぎてたんだ。口封じの必要があった",
              "stringId": "1711214768120205312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まったくわからない",
              "maleVariant": "",
              "stringId": "1711226674524524544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ごめんね",
              "maleVariant": "",
              "stringId": "1711266474543616000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふざけてる！ こんなことに首を突っ込んでるって知ってたら…",
              "maleVariant": "",
              "stringId": "1711268950860697600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "依頼はバーチャの撮影だけだった",
              "maleVariant": "",
              "stringId": "1711270311543246848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しかし、エヴリンもよく1人であれだけの計画を立てたものね",
              "maleVariant": "しかし、エヴリンもよく1人であれだけの計画を立てたもんだ",
              "stringId": "1711274020331393024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それであなたの力が必要だったのね",
              "maleVariant": "",
              "stringId": "1711274900162154496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "結局、こんな目に遭って。あなたまで",
              "maleVariant": "",
              "stringId": "1711278079058710528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しかもボスから生体チップを横取りしようとするなんて",
              "maleVariant": "しかもボスから生体チップを横取りしようとするとは",
              "stringId": "1711281279782424576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと… 時間をくれる？ 頭の中を整理したいの",
              "maleVariant": "ちょっと… 時間をくれるか？ 頭の中を整理したいんだ",
              "stringId": "1711290346710044672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え？ うん… わかった",
              "maleVariant": "",
              "stringId": "1711292264731709440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中やあの女が何者かわかる？ どうやって探せばいいの？",
              "maleVariant": "連中やあの女が何者かわかるか？ どうやって探せばいい？",
              "stringId": "1711305433168547840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、V…",
              "maleVariant": "",
              "stringId": "1711306557644673024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺はこの半世紀死んでたんだぞ？",
              "maleVariant": "",
              "stringId": "1711313213602467840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "指を鳴らせばどんな答えも差し出してくれるような、クロームの魔法使いじゃなくて悪かったな",
              "maleVariant": "",
              "stringId": "1711314716438368256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中はオルトに何の用があるの？",
              "maleVariant": "連中はオルトに何の用があるんだ？",
              "stringId": "1711319596343300096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなこと俺が知るかよ！",
              "maleVariant": "",
              "stringId": "1711322550878146560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここまで得た情報はすべて、例のネットランナーにつながってる。その女を探すのが最優先だ",
              "maleVariant": "",
              "stringId": "1711322994283188224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうやら相当チップについて詳しいようだからな。助けになってくれるだろ",
              "maleVariant": "",
              "stringId": "1711323713690210304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいよ、始めて",
              "maleVariant": "よし、始めてくれ",
              "stringId": "1714408778483277824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女に話があるの",
              "maleVariant": "彼女に話がある",
              "stringId": "1719942935009890304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、あなたに山ほど質問があるのも、すぐに話を聞きたいのもわかってる",
              "maleVariant": "",
              "stringId": "1719942935009890316"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも今は無理。完全に放心状態だから",
              "maleVariant": "",
              "stringId": "1719942935026667528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おいおい、冗談じゃねえぞ",
              "maleVariant": "",
              "stringId": "1719944987500941312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう行かなきゃ",
              "maleVariant": "俺はもう行く",
              "stringId": "1734450173769900032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って… ヴードゥー・ボーイズと連絡を取る方法は知ってるの？",
              "maleVariant": "",
              "stringId": "1734450173786677248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女もあなたともっと仲良くなれてたら… こうはならなかったかもしれないのに",
              "maleVariant": "",
              "stringId": "1734454260078960640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パラメーターを設定するから待って",
              "maleVariant": "",
              "stringId": "1734477703738515456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早くしろ、V",
              "maleVariant": "",
              "stringId": "1734480208375537664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BDはもう1つある",
              "maleVariant": "",
              "stringId": "1744544162657583104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見る",
              "maleVariant": "",
              "stringId": "1744545354678779908"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。ここまで来たらもう何にも驚かない",
              "maleVariant": "ああ。ここまで来たらもう何にも驚かない",
              "stringId": "1744545817696387072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お願い、質問に答えて",
              "maleVariant": "頼む、質問に答えてくれ",
              "stringId": "1744654416615268352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんな状態じゃなければ、私が殺してるところね",
              "maleVariant": "",
              "stringId": "1744703747703660544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え、どうして？",
              "maleVariant": "え、なぜだ？",
              "stringId": "1744704263921819648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全部見せてあげる――すぐにわかるよ",
              "maleVariant": "",
              "stringId": "1744704433371701248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これであの女の身に何があったのか、すべてがわかるな",
              "maleVariant": "",
              "stringId": "1744721330729345024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チップを丸焦げにしてね",
              "maleVariant": "チップを丸焦げにしてな",
              "stringId": "1744723268665565184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから必死に逃げてたのね…",
              "maleVariant": "",
              "stringId": "1744724761904898048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見たい？",
              "maleVariant": "",
              "stringId": "1744731473193951232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見当もつかない",
              "maleVariant": "",
              "stringId": "1759268548823965700"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いろいろあってね",
              "maleVariant": "いろいろあってな",
              "stringId": "1759269042292219904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうこと… ジョニー・シルヴァーハンドと何の関係があるの？ ずっと前に死んだ人よ",
              "maleVariant": "",
              "stringId": "1759270076221710336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、何か知ってるの？",
              "maleVariant": "",
              "stringId": "1759270076221710348"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ… 見当もつかない",
              "maleVariant": "",
              "stringId": "1759273777309114368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ややこしい話なの",
              "maleVariant": "ややこしい話なんだ",
              "stringId": "1759278421125062656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話してよ。ちゃんと聞く",
              "maleVariant": "",
              "stringId": "1759278960462225408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はやめておく",
              "maleVariant": "",
              "stringId": "1759279146135674880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "生体チップに彼のコンストラクトが入ってる",
              "maleVariant": "",
              "stringId": "1759280545959469056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの生体チップには、シルヴァーハンドの… 記憶痕跡が入ってたの",
              "maleVariant": "あの生体チップには、シルヴァーハンドの… 記憶痕跡が入ってたんだ",
              "stringId": "1759282484432560128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はい… ？",
              "maleVariant": "",
              "stringId": "1759283677259063296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "デジタル化された人格コンストラクトよ",
              "maleVariant": "デジタル化された人格コンストラクトだ",
              "stringId": "1759283809530634240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まじで…",
              "maleVariant": "",
              "stringId": "1759284908169846784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあね",
              "maleVariant": "じゃあな",
              "stringId": "1759293153567531008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあね、ジュディ",
              "maleVariant": "じゃあな、ジュディ",
              "stringId": "1759293610813775872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うん、その…",
              "maleVariant": "",
              "stringId": "1759294089769738240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう、V",
              "maleVariant": "",
              "stringId": "1759294208300769280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "幸運を祈ってる。あなたなら大丈夫よ",
              "maleVariant": "",
              "stringId": "1759295581868220416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんな人はいないんじゃなかったの？",
              "maleVariant": "そんな人はいないんじゃなかったのか？",
              "stringId": "1759299860729389056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭の中をかき回すような真似はしたくなかったけど――",
              "maleVariant": "",
              "stringId": "1759306393391423488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、あなたのためだからやったのよ",
              "maleVariant": "",
              "stringId": "1759306854211215360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう、あったま来る！",
              "maleVariant": "",
              "stringId": "1759321120700063744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいから、まずは例のシャーマン女を探せ、な？",
              "maleVariant": "",
              "stringId": "1759325126159720448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつから吸い始めたんだ？",
              "maleVariant": "",
              "stringId": "1788008717957971968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[煙草を吸う] 時々、どうしてもね",
              "maleVariant": "[煙草を吸う] 時々、どうしてもな",
              "stringId": "1788010001045897220"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[煙草を吸う] 頭がすっきりする",
              "maleVariant": "",
              "stringId": "1788010430542626816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずいし、頭がクラクラする",
              "maleVariant": "",
              "stringId": "1788010809573490688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "同じ中毒者として、よくわかるぜ",
              "maleVariant": "",
              "stringId": "1788011903179538432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭がすっきりするし、集中できるの",
              "maleVariant": "頭がすっきりするし、集中できるんだ",
              "stringId": "1788012143144058884"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前が吸えば、俺の欲求も満たされる",
              "maleVariant": "",
              "stringId": "1788012143160836100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行きましょ",
              "maleVariant": "行こうか",
              "stringId": "1788012731755905024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも、一度始めたらやめない性分なの",
              "maleVariant": "だが、一度始めたらやめない性分でね",
              "stringId": "1791339721954910208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やめないでくれよ？",
              "maleVariant": "",
              "stringId": "1791345460064772096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてエヴリンに腹を立ててるの？",
              "maleVariant": "どうしてエヴリンに腹を立ててるんだ？",
              "stringId": "1792430641802600452"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女に話がある",
              "maleVariant": "",
              "stringId": "1792431641942777856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女の容体は？",
              "maleVariant": "",
              "stringId": "1792442218870169600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてエヴリンに怒ってるの？",
              "maleVariant": "どうしてエヴリンに怒ってるんだ？",
              "stringId": "1792444930236047360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたに行動チップを調べるように言われた時、最初は無視してやろうと思ってた",
              "maleVariant": "",
              "stringId": "1792446866913976320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジュディ、あたしにはそれしか…",
              "maleVariant": "ジュディ、俺にはそれしか…",
              "stringId": "1792449669380136960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫、わかってる",
              "maleVariant": "",
              "stringId": "1792449909864751104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあいったいなんで…",
              "maleVariant": "",
              "stringId": "1792450436132462592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今にわかるって。見つけたバーチャを見せてあげる",
              "maleVariant": "",
              "stringId": "1792450709886296064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、頼むよ",
              "maleVariant": "",
              "stringId": "1826945117708836864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "絶対に嫌",
              "maleVariant": "絶対に嫌だね",
              "stringId": "1826948728685813764"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなに大したことじゃないだろ",
              "maleVariant": "",
              "stringId": "1826961413687173120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんなに肺がキレイなつもりでも、どうせこのナイトシティの空気を吸ってんだ。何も変わらねえんだから早くしろって",
              "maleVariant": "",
              "stringId": "1826966953725222912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼む、V。1本だけ",
              "maleVariant": "",
              "stringId": "1826974153013157888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[煙草を吸う] わかった",
              "maleVariant": "",
              "stringId": "1826980437305286656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "タバコの何がいいのよ。臭いだけでしょ",
              "maleVariant": "タバコの何がいいんだ。臭いだけだろ",
              "stringId": "1826982532964114432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう、わかった。吸ってあげる。満足するまでね",
              "maleVariant": "ったく、わかった。吸ってやるよ。満足するまでな",
              "stringId": "1826982556385107968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何とでも言ってくれ。とにかく火をつけろ",
              "maleVariant": "",
              "stringId": "1826985737378816000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "嫌よ",
              "maleVariant": "断る",
              "stringId": "1826985894245785600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あああ… やっぱヤニだな。この街の悪臭と排気ガスにはうんざりしてたんだ。お前だってそうだろ",
              "maleVariant": "",
              "stringId": "1826987057779269632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "吸わない奴はこれだからな… ツイてねえ",
              "maleVariant": "",
              "stringId": "1826994275253706752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何か思い当たることは？",
              "maleVariant": "",
              "stringId": "1975283242570047488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行こう",
              "maleVariant": "",
              "stringId": "1999447134790811652"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中はオルトに何の用があるの？",
              "maleVariant": "連中はオルトに何の用があるんだ？",
              "stringId": "2005289570126225412"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうやって連中を探したらいいと思う？",
              "maleVariant": "",
              "stringId": "2005292866345852928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう行かなきゃ",
              "maleVariant": "もう行かないと",
              "stringId": "2243348469338812416"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}