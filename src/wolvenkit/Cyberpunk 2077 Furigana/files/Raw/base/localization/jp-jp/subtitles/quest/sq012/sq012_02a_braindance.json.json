  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今回は女の尻ばかり撮るんじゃないぞ。市長から目を離すな",
                    "maleVariant": null,
                    "stringId": 1873252197612216320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいか、これは市の記録なんだ。市にふさわしいものを撮ってくれ",
                    "maleVariant": null,
                    "stringId": 1873606620576600064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "3日間で2度目の会見です。こんなに頻繁にやる意味がわかりません",
                    "maleVariant": null,
                    "stringId": 1873618686918119424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だからお前は市長になれないんだ。いま必要なのは透明性だろう",
                    "maleVariant": null,
                    "stringId": 1873619209528397824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "色々と手を広げ過ぎでは",
                    "maleVariant": null,
                    "stringId": 1873701282511441920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今夜はゆっくり休むさ。レッド・クイーンズ・レースの部屋は取ってあるな？",
                    "maleVariant": null,
                    "stringId": 1873722826084175872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、もちろん",
                    "maleVariant": null,
                    "stringId": 1873724386633699328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おっと！ ここでお待ちを",
                    "maleVariant": null,
                    "stringId": 1876346172579893248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そこで止まれ",
                    "maleVariant": null,
                    "stringId": 1876526123790749696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "NCPDのリバー・ウォードだ",
                    "maleVariant": null,
                    "stringId": 1876527194445565952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "警察か。どうします？",
                    "maleVariant": null,
                    "stringId": 1876528300215095296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待たせておけ",
                    "maleVariant": null,
                    "stringId": 1876531296021893120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "緊急の要件なんだ。市長の警護責任者に話がある",
                    "maleVariant": null,
                    "stringId": 1876531815578718208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふん、ふふん…",
                    "maleVariant": null,
                    "stringId": 1876548255321018368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "...",
                    "maleVariant": null,
                    "stringId": 1876548971171270656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "...",
                    "maleVariant": null,
                    "stringId": 1876549381558751232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ねえ、レッド・クイーンズ・レースって聞いたことある？",
                    "maleVariant": "なあ、レッド・クイーンズ・レースって聞いたことあるか？",
                    "stringId": 1882269497621598208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "とんでもない量のインプラントね",
                    "maleVariant": "とんでもない量のインプラントだな",
                    "stringId": 1882272235310927872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "きっと、こいつの死体はモルグから“うっかり”消えたんじゃないか",
                    "maleVariant": null,
                    "stringId": 1882273130895495168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あいつが通った時、ターミナルがクラッシュした。グリッチは偶然じゃない",
                    "maleVariant": null,
                    "stringId": 1882274040254156800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "たとえば？",
                    "maleVariant": null,
                    "stringId": 1882275663097483264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この曲… 聞いたことない",
                    "maleVariant": "この曲… 聞いたことないな",
                    "stringId": 1882276350728458240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "曲？",
                    "maleVariant": null,
                    "stringId": 1882277547162062848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "みんな、襲撃の直前に立ち去ってる",
                    "maleVariant": null,
                    "stringId": 1882277981725511680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まるで、あらかじめ計画されてたみたい…",
                    "maleVariant": "まるで、あらかじめ計画されてたみたいだ…",
                    "stringId": 1882289406858612736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "警官は警備に何か伝えようとしてた",
                    "maleVariant": null,
                    "stringId": 1882289792784912384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうせラインにゴマすりしたかっただけだろ",
                    "maleVariant": null,
                    "stringId": 1882290314472443904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ホルトって誰？",
                    "maleVariant": "ホルトって誰だ？",
                    "stringId": 1882291019702718464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあな。ゴミに違いなんてねえだろ",
                    "maleVariant": null,
                    "stringId": 1882291310468648960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "かつらを着けたイギリス人が叫ぶヤツだ。「首を落とせ」ってな",
                    "maleVariant": null,
                    "stringId": 1882293696171986944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よし。行っていいぞ。練習をする",
                    "maleVariant": null,
                    "stringId": 1925782813180387328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "会見の後にしてくれ",
                    "maleVariant": null,
                    "stringId": 1925943109178056704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そして次の日にゃ、財布が厚くなった警官がいたはずだ",
                    "maleVariant": null,
                    "stringId": 1927253452898656256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もっとすごい偶然なんていくらでもある",
                    "maleVariant": null,
                    "stringId": 1927257874651373568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あいつの鼻歌",
                    "maleVariant": null,
                    "stringId": 1927261590100074496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "かなり入れ込んでる",
                    "maleVariant": null,
                    "stringId": 1927262519893385216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "考えすぎだろ。単にあのサイコ野郎が機会をものにしただけだ",
                    "maleVariant": null,
                    "stringId": 1927266324982751232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "知らないなら知らないって言えばいいでしょ",
                    "maleVariant": "知らないなら知らないって言えばいいだろ",
                    "stringId": 1930116732222660608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハッ、また適当にでまかせ言ってるの？",
                    "maleVariant": "フッ、また適当にでまかせ言ってるのか？",
                    "stringId": 1930122220486221824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺ぐらい色々経験してると、わかるんだよ。人なんてどこに行っても変わらねえ",
                    "maleVariant": null,
                    "stringId": 1930122421812813824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なるほど、でまかせなのね",
                    "maleVariant": "なるほど、でまかせなんだな",
                    "stringId": 1930123298204246016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ある女とヤった後、別の女とヤったら、実は一人目の妹だった、とかな",
                    "maleVariant": null,
                    "stringId": 1930126305167892480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本当にあった話？",
                    "maleVariant": "本当にあった話か？",
                    "stringId": 1930140925286899712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや。可能性の話だ",
                    "maleVariant": null,
                    "stringId": 1930141153792581632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "曲とは呼べないだろ",
                    "maleVariant": null,
                    "stringId": 1930142031710740480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よう、元気か？",
                    "maleVariant": null,
                    "stringId": 1944642883873787904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いやいや、なんであんたがいるのよ？",
                    "maleVariant": "いやいや、なんでお前がいるんだよ？",
                    "stringId": 1944644023801729024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺の脳でもあるんだぞ",
                    "maleVariant": null,
                    "stringId": 1944731342179360768
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
