  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "埃を被ったツナミ製ライフルを引っ張り出して、またあんたとアラサカの戦いに加われって？",
              "maleVariant": "",
              "stringId": "1660540860888911872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ビンゴ",
              "maleVariant": "",
              "stringId": "1660541839034159104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつ？",
              "maleVariant": "",
              "stringId": "1660542178873446400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今すぐは、どうだ？",
              "maleVariant": "",
              "stringId": "1660542248247234560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさか私がそんな話に乗るとでも？",
              "maleVariant": "",
              "stringId": "1660543268067086336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺に借りがあるだろ",
              "maleVariant": "",
              "stringId": "1660543500465082372"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前は俺に借りがあるからな",
              "maleVariant": "",
              "stringId": "1660543963499466752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこでお前の出番だ",
              "maleVariant": "",
              "stringId": "1660629041969762308"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人と装備が必要だ",
              "maleVariant": "",
              "stringId": "1660629334446968832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前は街一番のフィクサーだろ",
              "maleVariant": "",
              "stringId": "1660629524885147648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちの動きはお前に任せる",
              "maleVariant": "",
              "stringId": "1660630001341304832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人が要る。集められるだろ",
              "maleVariant": "",
              "stringId": "1660634278273093632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それと装備もだ。いくらかかってもいい",
              "maleVariant": "",
              "stringId": "1660635055376961536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あとは… 俺たちに運があることを願うだけさ…",
              "maleVariant": "",
              "stringId": "1660641711015989248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "作戦？ そんなもんが役に立った試しがあるか？",
              "maleVariant": "",
              "stringId": "1661627262682664960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おいおい、最近のガキみてえなこと言うなよ。どいつもこいつも作戦、作戦って喚きやがって",
              "maleVariant": "",
              "stringId": "1661627905048711168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前自身のためだ",
              "maleVariant": "",
              "stringId": "1661631564176297984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「俺たち」だって？",
              "maleVariant": "",
              "stringId": "1661785639970803712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前なしじゃできっこない",
              "maleVariant": "",
              "stringId": "1661786214942773252"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんなチャンスを逃していいのか？",
              "maleVariant": "",
              "stringId": "1661786686265102336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。お前なしじゃこんなことできっこない",
              "maleVariant": "",
              "stringId": "1661787606193410048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チャンスを逃す気か？",
              "maleVariant": "",
              "stringId": "1661787740562132992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昔からそうだろ？",
              "maleVariant": "",
              "stringId": "1668117293056266240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昔のお前は一発キメたら銃を引っつかんで飛び出してた。だからこそ上手くいったのさ",
              "maleVariant": "",
              "stringId": "1668118010466799616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちのやり方でやろうぜ",
              "maleVariant": "",
              "stringId": "1668119140177416192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vが死にかけてる。助けたいんだ",
              "maleVariant": "",
              "stringId": "1669430932694503428"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この体を… 俺はただ… ああ、 クソ！",
              "maleVariant": "",
              "stringId": "1669431864148758528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vが死にかけてる。原因はこの俺なんだ",
              "maleVariant": "",
              "stringId": "1669432155166347264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "進行を止める方法はある。ただ――",
              "maleVariant": "",
              "stringId": "1669435492892807168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やめな、ジョニー。もういい",
              "maleVariant": "",
              "stringId": "1669436366532775936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰かを助けたいだなんて、所詮は思い込みだよ。結局、自分が楽になりたいだけさ",
              "maleVariant": "",
              "stringId": "1669437745284386816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言ってくれるじゃねえか",
              "maleVariant": "",
              "stringId": "1669440153251090436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言ってくれるじゃねえか",
              "maleVariant": "",
              "stringId": "1669442119104598016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカ・タワーに乗り込む",
              "maleVariant": "",
              "stringId": "1669465027487076352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は変わったんだ",
              "maleVariant": "",
              "stringId": "1669469960575782912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "陰気くせえなあ",
              "maleVariant": "",
              "stringId": "1670574995363729408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少しは嬉しそうにしろよ",
              "maleVariant": "",
              "stringId": "1670575310188187648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "喜ぶべきならそうしてるさ",
              "maleVariant": "",
              "stringId": "1670575482624413696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうした？ 映画ん時は一緒に楽しんだろ？",
              "maleVariant": "",
              "stringId": "1670576483955773440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何か必要なんだろ？ すでに嫌な予感がしてるよ",
              "maleVariant": "",
              "stringId": "1670577501376483328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それに、何しに来たのか見当もつく",
              "maleVariant": "",
              "stringId": "1670580960871976960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時間がないんだ、ローグ",
              "maleVariant": "",
              "stringId": "1670595528075644928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "タイムリミットが来たら、もうおしまいだ。ジョニーは消えて、Vも死ぬ",
              "maleVariant": "",
              "stringId": "1670604672497225728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その前に俺は、あいつの頭から出てかないといけない",
              "maleVariant": "",
              "stringId": "1670606504703430656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オルトと接触できたんだ。あいつが協力してくれる",
              "maleVariant": "",
              "stringId": "1670607675971211264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも？",
              "maleVariant": "",
              "stringId": "1670609347669790720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのためには、〈神輿〉ってデータ要塞に乗り込まなきゃならない",
              "maleVariant": "",
              "stringId": "1670609460077137920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今度こそはって思いながら、同じことを何度も繰り返すのはバカのやることだ",
              "maleVariant": "",
              "stringId": "1670614956947410992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうせロクな作戦もないんだろ？",
              "maleVariant": "",
              "stringId": "1670614956947411004"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "変わらないものも、あるもんだ",
              "maleVariant": "",
              "stringId": "1670757502046572544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうだろうね",
              "maleVariant": "",
              "stringId": "1670763857339879424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつかこういう日が来ると思ってた",
              "maleVariant": "",
              "stringId": "1673748556882513920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なら確かめようぜ",
              "maleVariant": "",
              "stringId": "1673752168950009856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺に手を貸してくれるか？",
              "maleVariant": "",
              "stringId": "1673754324067291136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう黙って、ジョニー…",
              "maleVariant": "",
              "stringId": "1673754574400131072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのとおりだ",
              "maleVariant": "",
              "stringId": "1699564292481617924"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい身分じゃないか、ローグ",
              "maleVariant": "",
              "stringId": "1699565063612796928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいご身分だな、ローグ",
              "maleVariant": "",
              "stringId": "1699570563335196672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここまで来るのは大変だったろ？ みんな消えてったもんな",
              "maleVariant": "",
              "stringId": "1699573530637504512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] Vは休暇中さ。俺だよ",
              "maleVariant": "",
              "stringId": "1699576040173473796"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アトランティスの連中のうち、この時代まで生き残ったのは、どうしたもんかお前だけだからな",
              "maleVariant": "",
              "stringId": "1699606138532532224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつの話をしてんだい",
              "maleVariant": "",
              "stringId": "1699615164859289600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんな、今日はもう閉店だよ！ 出ていきな",
              "maleVariant": "",
              "stringId": "1699648392286572544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、何だよ？ まだ飲んでる途中だ！",
              "maleVariant": "",
              "stringId": "1699648592522645504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいから出な！",
              "maleVariant": "",
              "stringId": "1699650205266087936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、何なのよ",
              "maleVariant": "",
              "stringId": "1699652861938241536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何でもないよ。さあ、あんたもさっさと家に帰んな",
              "maleVariant": "",
              "stringId": "1699654601416773632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんだ、貸し切りにしてくれるのか。VIP待遇だな",
              "maleVariant": "",
              "stringId": "1699688044649385984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これでいいだろ。ほら、さっさと本題にはいりな",
              "maleVariant": "",
              "stringId": "1699689959449825280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昔のことを思い出したりはしないか？",
              "maleVariant": "",
              "stringId": "1699691521005981700"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの頃を思い出さないか、ローグ？",
              "maleVariant": "",
              "stringId": "1699695824579989508"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "思い出は思い出、私は今に満足してる",
              "maleVariant": "",
              "stringId": "1699695824579989520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アトランティスの他の奴らに“今”がないのが残念だ",
              "maleVariant": "",
              "stringId": "1699695824579989532"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "色々変わってて驚いてるのさ",
              "maleVariant": "",
              "stringId": "1699695979635019776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんせ50年も寝過ごしちまったもんでな。その間に何があったか確認してるんだよ",
              "maleVariant": "",
              "stringId": "1699698629613064192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで？ 何かわかったの？",
              "maleVariant": "",
              "stringId": "1699701853808103424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昔の仲間はロクな結末を迎えられなかったらしい",
              "maleVariant": "",
              "stringId": "1699701979435896832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前を除いてな",
              "maleVariant": "",
              "stringId": "1699702509595922432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "懐かしいだけだ",
              "maleVariant": "",
              "stringId": "1699703173000597504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] いい暮らしをしてるようだが、どんな代償を払ったんだ？",
              "maleVariant": "",
              "stringId": "1699707978951561216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] 白状しろよ。企業の側に寝返ったんだろ？",
              "maleVariant": "",
              "stringId": "1699709755977502720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグ、正直に白状しろ。みんなを裏切ったんだろ？",
              "maleVariant": "",
              "stringId": "1699723860046123008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よくもそんなことを――このロクデナシ！",
              "maleVariant": "",
              "stringId": "1699724234211594240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私はアトランティスを救ったんだ。あんたが殺し損ねた連中をね！",
              "maleVariant": "",
              "stringId": "1699725980401012736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話すってよりは… ただ事実を知りたくてな",
              "maleVariant": "",
              "stringId": "1699758810392825856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "代償を払うこともなく、これだけの暮らしを手に入れたか",
              "maleVariant": "",
              "stringId": "1699760383491706880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほどね",
              "maleVariant": "",
              "stringId": "1699761439802003456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何が言いたいんだい？",
              "maleVariant": "",
              "stringId": "1699869857611124736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだね。でも目的は別にあるんだろ？",
              "maleVariant": "",
              "stringId": "1699938048907546624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またアラサカ？ あんた、まだ懲りてないのかい？",
              "maleVariant": "",
              "stringId": "1699953650695016448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー… あんた病気だよ！",
              "maleVariant": "",
              "stringId": "1699953650711793672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、そうカッカすんなって… 昔の話だろ",
              "maleVariant": "",
              "stringId": "1699956076160356352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でだな… 〈神輿〉へのアクセスポイントはわかってる。アラサカ・タワーの地下だ",
              "maleVariant": "",
              "stringId": "1699958654499688448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずはそこにたどり着き――",
              "maleVariant": "",
              "stringId": "1699958654516465672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それからオルトをシステムに入り込ませる",
              "maleVariant": "",
              "stringId": "1699958654516465684"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺とVが… 別の道を行くには、ほかに方法がない",
              "maleVariant": "",
              "stringId": "1699958654516465696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その体に居続けられないの？",
              "maleVariant": "",
              "stringId": "1699958654516465708"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "狭苦しいんでな",
              "maleVariant": "",
              "stringId": "1699958654533242888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうじゃない！ Vを救うためだ",
              "maleVariant": "",
              "stringId": "1699958654566797312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "趣味みたいなもんさ",
              "maleVariant": "",
              "stringId": "1699958654566797328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカは関係ない！ Vを救うためだ",
              "maleVariant": "",
              "stringId": "1699958654600351744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "趣味みたいなもんさ",
              "maleVariant": "",
              "stringId": "1699958654617128960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は変わったかもしれないだろ",
              "maleVariant": "",
              "stringId": "1699967783503007744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] ハズレだ",
              "maleVariant": "",
              "stringId": "1699971134164701184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前自身のためだ",
              "maleVariant": "",
              "stringId": "1701300107926704128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ワカコ、はっきり言わせてもらえば――邪魔なのよ",
              "maleVariant": "",
              "stringId": "1703772053469794304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたが商売できてるのは、私がそれを許してるからだ。気分一つでどうとでもなるんだよ",
              "maleVariant": "",
              "stringId": "1703773135264681984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、ちょっと待って",
              "maleVariant": "",
              "stringId": "1703774694337810432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？",
              "maleVariant": "",
              "stringId": "1703797697343787008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vは休暇中さ。俺だよ",
              "maleVariant": "",
              "stringId": "1703799695879311360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハズレだ。回答権は残り1回だぞ",
              "maleVariant": "",
              "stringId": "1703800676121710592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー",
              "maleVariant": "",
              "stringId": "1703801148886880256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ワカコ、またかける",
              "maleVariant": "",
              "stringId": "1703802080878006272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、何の用だいジョニー",
              "maleVariant": "",
              "stringId": "1703805871723515904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "笑顔ぐらい見せたらどうだ",
              "maleVariant": "",
              "stringId": "1703819325675016196"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大事な仕事の話を邪魔したか？",
              "maleVariant": "",
              "stringId": "1703821625344790528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また面倒な仕事でも請け負ったのか？",
              "maleVariant": "",
              "stringId": "1703822076702232576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの話の方がクソ面倒なんじゃないかって気がしてるよ",
              "maleVariant": "",
              "stringId": "1703822407280496640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうしなきゃ、こっちまで道連れにされてた",
              "maleVariant": "",
              "stringId": "1703841166506463232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全ての選択には代償がある。私だって払ってきたよ",
              "maleVariant": "",
              "stringId": "1703843555565244416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今のはただの前振り。またわたしを何かに巻き込むつもりかい？",
              "maleVariant": "",
              "stringId": "1703854699797417984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカ・タワーに乗り込む。お前の力を貸してくれ",
              "maleVariant": "",
              "stringId": "1703856413237710848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この体は死にかけてる。原因は俺なんだ",
              "maleVariant": "",
              "stringId": "1703859271722995712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺やみんなの犠牲のおかげで、今ここにいるんだろ",
              "maleVariant": "",
              "stringId": "1703862320076034048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何のことやら…",
              "maleVariant": "",
              "stringId": "1703863392425349120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このままナイトシティの女王を気取ったっていいが、そいつは本当のローグじゃない",
              "maleVariant": "",
              "stringId": "1703863625175666688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心のどこかに、まだそいつが残ってるはずだ",
              "maleVariant": "",
              "stringId": "1703863928239296512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "結局、またアラサカ？ ホントに変わらないね",
              "maleVariant": "",
              "stringId": "1703878841103945728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る]",
              "maleVariant": "",
              "stringId": "1725774310776954884"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "1725831765628215300"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待ちな。「俺たち」…？",
              "maleVariant": "",
              "stringId": "1740078522171265024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちは？ 変わっちまったのか？",
              "maleVariant": "",
              "stringId": "1742849177505632256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話は… もういいから",
              "maleVariant": "",
              "stringId": "1742856747955916800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこへのアクセスポイントの中で辿り着けるのは、アラサカ・タワーの地下にあるヤツだけだ",
              "maleVariant": "",
              "stringId": "1742859043364278272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前は自分を押し殺して、ここまで登りつめた",
              "maleVariant": "",
              "stringId": "1743059864458760192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何か必要なんだろ？ すでに嫌な予感がする",
              "maleVariant": "",
              "stringId": "1754321626315231232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのとおりだ",
              "maleVariant": "",
              "stringId": "1754326215370346496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前に頼るしか方法がないだろ？",
              "maleVariant": "",
              "stringId": "1754326559336828928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティの女王か",
              "maleVariant": "",
              "stringId": "1754331088396619776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何かの奇跡で、ある夜突然あんたが現れて、何もかもぶち壊してく日がね",
              "maleVariant": "",
              "stringId": "1754335400543784960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前がコーポに差し出したのは、俺だけじゃなくみんなか？",
              "maleVariant": "",
              "stringId": "1760762643159584768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんただけさ",
              "maleVariant": "",
              "stringId": "1760763060610273280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "派手にやってくれちゃって",
              "maleVariant": "",
              "stringId": "1760764207618199552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう思うのはコーポの金を勘定しながらか？",
              "maleVariant": "",
              "stringId": "1760767221896712192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "違う。アトランティスの仲間たちの墓の前で祈りながらだ",
              "maleVariant": "",
              "stringId": "1760771558706384896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それと今のあんたを見てね",
              "maleVariant": "",
              "stringId": "1760775022329073664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、それは… 昔の話だ",
              "maleVariant": "",
              "stringId": "1760775718398988288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、それも… 昔の話だ… 今となっちゃな",
              "maleVariant": "",
              "stringId": "1760777038296125440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あら、V。お休み前の1杯でも飲みに来たの？",
              "maleVariant": "",
              "stringId": "1760802764160372736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグを探してる",
              "maleVariant": "",
              "stringId": "1760804037936943108"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグを探してる",
              "maleVariant": "",
              "stringId": "1760806126213795840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お休み前？ 夜はこれからだ",
              "maleVariant": "",
              "stringId": "1760808775587860480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "向こうのブースにいるけど、邪魔しない方がいいよ",
              "maleVariant": "",
              "stringId": "1760809851208732672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、邪魔してやる",
              "maleVariant": "",
              "stringId": "1760811304769949696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お休み前？ 夜はこれからだ",
              "maleVariant": "",
              "stringId": "1760814486250196992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って、本当だって！ ぶっ飛ばされるよ！",
              "maleVariant": "",
              "stringId": "1760816487637848064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ？ そう思うんなら、どうなるかよく見てな",
              "maleVariant": "",
              "stringId": "1760827970467794944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見なくたって、結果は想像つく",
              "maleVariant": "",
              "stringId": "1761747391667376128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残念。もう閉めるとこよ",
              "maleVariant": "",
              "stringId": "1761752268955062272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サルバドールで見たんだ。殺人マシンどもをよ",
              "maleVariant": "",
              "stringId": "1761753126673453056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこにあるのは俺のボトルか？",
              "maleVariant": "",
              "stringId": "1761762770133323776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこにあるのは俺のボトルか？",
              "maleVariant": "",
              "stringId": "1761763579013238784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。ずっと残してあった… 何のためだか知らないけどね",
              "maleVariant": "",
              "stringId": "1761764123333234688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもここにきて、ようやくいい使い道が見つかりそうだよ",
              "maleVariant": "",
              "stringId": "1761764753351249920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当にそう思ってるのか？",
              "maleVariant": "",
              "stringId": "1761775991284174848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当にそう思ってるのか？",
              "maleVariant": "",
              "stringId": "1761776433128935424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。オルトの時と同じだろ",
              "maleVariant": "",
              "stringId": "1761776883832066048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "罪の意識からですらない。そのままじゃ腹の虫の居所が悪いってだけ",
              "maleVariant": "",
              "stringId": "1761777848303882240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも一番奇妙だったのは…",
              "maleVariant": "",
              "stringId": "1803733596390690816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ、バカじゃないの",
              "maleVariant": "",
              "stringId": "1803747880629833728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺が嘘つきだってのか！？",
              "maleVariant": "",
              "stringId": "1803748161966968832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サルバドールはクソ暑いから… 熱で色々とおかしなものが見えたんじゃない",
              "maleVariant": "",
              "stringId": "1803748740176941056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どの部隊にも属さない極秘の存在だ… どこからともなく現れ、ジャングルに消えてった。まるで… 亡霊だ",
              "maleVariant": "",
              "stringId": "1803758210059735040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なに？",
              "maleVariant": "",
              "stringId": "1803761287387025408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴らの動きだ… まるで1人の人間みたいにシンクロしてた…",
              "maleVariant": "",
              "stringId": "1803761340822458368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃ、仕事に戻るとするか",
              "maleVariant": "",
              "stringId": "1872007248774766592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私たちにはサービスを拒否する権利がある… ええ、いいルールよね",
              "maleVariant": "",
              "stringId": "1905303808960897024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんた、本気で私が… まったく",
              "maleVariant": "",
              "stringId": "2262044250013540352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そもそも、いつから私の決断に口を出せる立場になったんだい！？",
              "maleVariant": "",
              "stringId": "2262044250349084672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいだろう。あんたが言いたいことはわかってる――アダム・スマッシャーだろ",
              "maleVariant": "",
              "stringId": "2262048084882464768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言っておくが、元々そうするつもりじゃなかった。あんたの死を乗り越えた後に起きた話だ",
              "maleVariant": "",
              "stringId": "2262049075526737920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この業界に感傷はいらない。私はスマッシャーの雇い主に頭を垂れた",
              "maleVariant": "",
              "stringId": "2262049929017270272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、そうさ、ひねり出したばかりのクソみたいに臭う話だろ。ほかの選択肢だってあった。でもね――",
              "maleVariant": "",
              "stringId": "2262050861964697600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これでいいかい？",
              "maleVariant": "",
              "stringId": "2262051621284716544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "後悔はしてる",
              "maleVariant": "",
              "stringId": "2262065612576890880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、何だい？ 未来の話がしたいってわけ？ それだったら、さっさと本題に入りな",
              "maleVariant": "",
              "stringId": "2262213751619297280"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}