  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "失礼。部屋を間違えたみたい",
                    "maleVariant": "失礼。部屋を間違えたみたいだ",
                    "stringId": 1944419146780286980
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "失礼。部屋を間違えたみたい",
                    "maleVariant": "失礼。部屋を間違えたみたいだ",
                    "stringId": 1944419871019147264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そのことなんだが… バックアップを作る時に、その… 修正を加えることはできないか？",
                    "maleVariant": null,
                    "stringId": 1944420030234927104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "〈Relic〉の使用許可が下りることは珍しいんだろ… ましてや、生きてる人間の記憶痕跡を作るなんてのは",
                    "maleVariant": null,
                    "stringId": 1944428403206561792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リズィーはどうも… 体の改造をして以来… 少し様子がおかしくて",
                    "maleVariant": null,
                    "stringId": 1944468764339642368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちなみに… どの程度の修正が必要なの？",
                    "maleVariant": null,
                    "stringId": 1944473740545794052
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大きな修正さ。彼女は時々… まるで別人みたいになるんだ。それに…",
                    "maleVariant": null,
                    "stringId": 1944473740545794060
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おい！ 何のつもりだ！",
                    "maleVariant": null,
                    "stringId": 1944476554605010944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや、忘れてくれ。まずは乾杯しよう。俺たちの協力関係に",
                    "maleVariant": null,
                    "stringId": 1944485314845122560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "シャンパン？ 驚いた",
                    "maleVariant": null,
                    "stringId": 1944486753524948992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さっさと出てかないとセキュリティを呼ぶぞ",
                    "maleVariant": null,
                    "stringId": 1944490181043068928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたに探りを入れてたのよ、リアム",
                    "maleVariant": "あんたに探りを入れてたんだよ、リアム",
                    "stringId": 1944490583830470656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リズィーよ",
                    "maleVariant": "リズィーだ",
                    "stringId": 1944491022537891844
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まったく、プライバシーはどうなってるんだ…",
                    "maleVariant": null,
                    "stringId": 1944491606485671936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "出ていけ。セキュリティを呼ぶぞ",
                    "maleVariant": null,
                    "stringId": 1944492200566890496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わからない？ あんたたちの会話を聞いてたの",
                    "maleVariant": "わからないか？ あんたらの会話を聞いてたんだ",
                    "stringId": 1944493226661421056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "セキュリティ！ 侵入者だ！ 来てくれ！",
                    "maleVariant": null,
                    "stringId": 1944495284487626752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リズィーよ。あんたのアウトプットのね",
                    "maleVariant": "リズィーだ。あんたのアウトプットのな",
                    "stringId": 1944501871222628352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "チップにコピーしようとしてるんでしょ？ それも無断で",
                    "maleVariant": "チップにコピーしようとしてるんだろ？ それも無断で",
                    "stringId": 1944502047450505220
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それで問題が消えると思ってるわけ？ 金さえばらまけば、あっという間に解決？",
                    "maleVariant": "それで問題が消えると思ってるわけか？ 金さえばらまけば、あっという間に解決だって？",
                    "stringId": 1944508089932619776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "断る",
                    "maleVariant": null,
                    "stringId": 1944508147159703556
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "シャンパンをご注文されました？",
                    "maleVariant": null,
                    "stringId": 1944524667197759488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "シャンパンをご注文されました？",
                    "maleVariant": null,
                    "stringId": 1944525214101446656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ。できれば静かに味わいたいんだが",
                    "maleVariant": null,
                    "stringId": 1944526277323956224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そう。じゃあ払って",
                    "maleVariant": "そうか。じゃあ払え",
                    "stringId": 1944532327741693952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ払って。それなりの額なんでしょうね",
                    "maleVariant": "じゃあ払え。それなりの額なんだろうな",
                    "stringId": 1944532428925083648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、もちろんだ… 今、送金する",
                    "maleVariant": null,
                    "stringId": 1944584332027797504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺からの感謝の印と思ってくれ",
                    "maleVariant": null,
                    "stringId": 1945911819109715968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいのよ… アラサカ社はリズィーのキャリアに大きな関心を寄せているんだから",
                    "maleVariant": null,
                    "stringId": 1945914958646923264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "申し訳ありません。システム上でオーダーが通ってなくて…",
                    "maleVariant": null,
                    "stringId": 1945929555613372416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そっちのシステムのことなんか知るか。出てけ",
                    "maleVariant": null,
                    "stringId": 1945930212072280064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はあ？ 誰に雇われた！？ 言え！",
                    "maleVariant": null,
                    "stringId": 1945987248248115200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "話す気はない",
                    "maleVariant": null,
                    "stringId": 1945994613093617664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "誰でしょうね？ まぁ焦らないで。じきにわかるから",
                    "maleVariant": "誰だろうな？ まぁ焦るなって。じきにわかるから",
                    "stringId": 1945995187981701120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今聞いたことを依頼主に話したらどうなるか、楽しみね",
                    "maleVariant": "今聞いたことを依頼主に話したらどうなるか、楽しみだ",
                    "stringId": 1945996179179622400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼女、今のあんたたちの会話に、大きな興味を示すでしょうね",
                    "maleVariant": "彼女、今のあんたたちの会話に、大きな興味を示すだろうな",
                    "stringId": 1945999597956812800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お、おい！ 待て！ 待ってくれ！",
                    "maleVariant": null,
                    "stringId": 1946000548704866304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "金だ… 金を払う！ どうだ？ いくらでも払うから… 今聞いたことは忘れてくれ！",
                    "maleVariant": null,
                    "stringId": 1946003903661527040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おい… 今の話、お前も聞いたよな？",
                    "maleVariant": null,
                    "stringId": 1946005083754123264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "浮気は確定ね。賭けてもいい",
                    "maleVariant": "浮気は確定だな。賭けてもいい",
                    "stringId": 1946007858336550912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やましいところがあったのは事実だが… 今のはどうも… そういうのとは話が違う",
                    "maleVariant": null,
                    "stringId": 1946008284276510720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大した違いじゃない。とにかく、彼女はあいつと距離を置くべきね",
                    "maleVariant": "大した違いじゃない。とにかく、彼女はあいつと距離を置くべきだ",
                    "stringId": 1946008939108028416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "〈Relic〉… あの女を俺と同じ目に遭わせる気だ…",
                    "maleVariant": null,
                    "stringId": 1946009301227458560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "残念だけどそれは無理。“魂の救済”プログラムに保存された記憶痕跡は書き換えられないの",
                    "maleVariant": null,
                    "stringId": 1946018749115322368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたの目的は何なの？",
                    "maleVariant": "あんたの目的は何なんだ？",
                    "stringId": 1946021117370355712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうしてもいいけど、物事には順番がある。まずはあんたの目的が知りたい",
                    "maleVariant": "そうしてもいいが、物事には順番がある。まずはあんたの目的が知りたい",
                    "stringId": 1946021547991158784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リアム、ダメよ。アラサカ社は――",
                    "maleVariant": null,
                    "stringId": 1946025992124682240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカなんて知るか",
                    "maleVariant": null,
                    "stringId": 1946026216285065216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺はリズィーが心配なんだ… 彼女は全身をクローム化した。そしたら、彼女の中で何かが狂ってしまったんだよ",
                    "maleVariant": null,
                    "stringId": 1946026503645220864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "初めは記憶が乱れる程度だったが… やがて変わってしまったんだ",
                    "maleVariant": null,
                    "stringId": 1946027161345642496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人は誰だって変化するものよ。大抵は悪い方向にね",
                    "maleVariant": "人は誰だって変化するものだ。大抵は悪い方向にな",
                    "stringId": 1946027928114745344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そういうことじゃない。彼女と話してると… 怖くなるんだよ。彼女のなかに… 何かがいる… 上手く言えないが…",
                    "maleVariant": null,
                    "stringId": 1946028906058665984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大丈夫よ。説明すべき相手はあたしじゃないから",
                    "maleVariant": "大丈夫だ。説明すべき相手は俺じゃないからな",
                    "stringId": 1946030217047101440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待ってくれ！ 金はいくらでも払う！ だからどうか、誰にも話さないでくれ",
                    "maleVariant": null,
                    "stringId": 1946032626137878528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それじゃ。シャンパンを楽しんで。冷たいうちにね",
                    "maleVariant": "それじゃ。シャンパンを楽しめよ。冷たいうちにな",
                    "stringId": 1946035058012114944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今回は通用しない",
                    "maleVariant": null,
                    "stringId": 1946035909002842112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "万が一事故死でもしたら大変。バックアップを用意しておくのは現実的な対策よ",
                    "maleVariant": null,
                    "stringId": 1963322144276664320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "同じじゃない。あんたは死んだも同然だけど、今回はバックアップよ",
                    "maleVariant": "同じじゃない。お前は死んだも同然だが、今回はバックアップだ",
                    "stringId": 1963370484083249152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バックアップね！ 旅行の写真や何かとは違うんだぞ。あの男は本人の許可も取らず、裏で話を進める気だ…",
                    "maleVariant": null,
                    "stringId": 1963376035898904576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなことする権利は奴にはない。いや、誰にもな",
                    "maleVariant": null,
                    "stringId": 1963377114707447808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "素直に信じるかわからないぞ",
                    "maleVariant": null,
                    "stringId": 1963384484653223936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ。だから監視カメラの映像を盗まないと",
                    "maleVariant": "ああ。だから監視カメラの映像を盗まないと",
                    "stringId": 1963385265380970496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本人に一切ばれずにことを進めてる。こんな話、信じてくれるかしら",
                    "maleVariant": "本人に一切ばれずにことを進めてる。こんな話、信じてくれるのか",
                    "stringId": 1963385719456321536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "心配はいらない。あそこにはカメラがある… ってことは映像もあるはずだ",
                    "maleVariant": null,
                    "stringId": 1963386166535573504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ねえ、誰だか知らないけど… 間違いなく後悔することになるわよ",
                    "maleVariant": null,
                    "stringId": 1963390791208718336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、だからこそあたしが雇われた",
                    "maleVariant": "ああ、だからこそ俺が雇われた",
                    "stringId": 1963391102107308032
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}