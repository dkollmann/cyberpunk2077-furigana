    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1695131715807199236,
            "femaleVariant": "[タバコを渡す] ほら"
          },
          {
            "stringId": 1695133188511219712,
            "femaleVariant": "ほら"
          },
          {
            "stringId": 1695137325219037184,
            "femaleVariant": "タバコある？"
          },
          {
            "stringId": 1695146952824438788,
            "femaleVariant": "生きるのを諦めるなんて"
          },
          {
            "stringId": 1695147307108909056,
            "femaleVariant": "生きるのを諦めるような、そんなタイプには思えなかった…"
          },
          {
            "stringId": 1695147849247866880,
            "femaleVariant": "あんなひどい目に遭ったのよ…？ 諦めたなんて言い方は間違ってる"
          },
          {
            "stringId": 1770497282046672896,
            "femaleVariant": "気を強く持って",
            "maleVariant": "気を強く持つんだ"
          },
          {
            "stringId": 1770513705431187456,
            "femaleVariant": "ごめん、こういう時にうまく言葉を見つけられなくて",
            "maleVariant": "すまない、こういう時にうまく言葉を見つけられなくて"
          },
          {
            "stringId": 1770515874926542848,
            "femaleVariant": "何も言わなくていい。あなたは誰よりも力になってくれた"
          },
          {
            "stringId": 1770516721035100160,
            "femaleVariant": "それはあたしの都合もあったからよ。知ってるでしょ",
            "maleVariant": "それは俺の都合もあったからだ。知ってるだろ"
          },
          {
            "stringId": 1770516995527131136,
            "femaleVariant": "そうかもね。でもこんな結末を選ぶなんて",
            "maleVariant": "そうかもな。だがこんな結末を選ぶなんて"
          },
          {
            "stringId": 1831506301992787968,
            "femaleVariant": "へえ、吸うんだ",
            "maleVariant": "へえ、吸うのか"
          },
          {
            "stringId": 1831506485132877824,
            "femaleVariant": "何年も前にやめたんだけど、今は、空気以外のものを肺に入れたい気分。お願い、1本だけ"
          },
          {
            "stringId": 1831519728446431232,
            "femaleVariant": "ごめん、切らしてるの",
            "maleVariant": "ごめん、切らしてる"
          },
          {
            "stringId": 1831520835776241664,
            "femaleVariant": "ダメよ",
            "maleVariant": "ダメだ"
          },
          {
            "stringId": 1831587983697600512,
            "femaleVariant": "悪いけど、ダメ",
            "maleVariant": "悪いが、ダメだ"
          },
          {
            "stringId": 1831588006011297792,
            "femaleVariant": "今は切らしてる"
          },
          {
            "stringId": 1831594158753275904,
            "femaleVariant": "エヴリンのケースね"
          },
          {
            "stringId": 1831608042604158976,
            "femaleVariant": "あんたが持っとく？",
            "maleVariant": "あんたが持っとくか？"
          },
          {
            "stringId": 1831608751139545088,
            "femaleVariant": "ううん、気に入ってるんでしょ？"
          },
          {
            "stringId": 1831609765087371268,
            "femaleVariant": "返そうか？"
          },
          {
            "stringId": 1831611705405956096,
            "femaleVariant": "変わった銘柄を吸ってた"
          },
          {
            "stringId": 1831616692550852608,
            "femaleVariant": "ウソ。本当は持ってるくせに"
          },
          {
            "stringId": 1831620604276203520,
            "femaleVariant": "ええ？"
          },
          {
            "stringId": 1831620695175159808,
            "femaleVariant": "いま私を怒らせないで"
          },
          {
            "stringId": 1831643514772611072,
            "femaleVariant": "洒落た感じの変わった銘柄を吸ってたのよね",
            "maleVariant": "洒落た感じの変わった銘柄を吸ってたんだよな"
          },
          {
            "stringId": 1831658304039510016,
            "femaleVariant": "エヴリンらしい。新しい顔をまとう時は、細部まで徹底してたの"
          },
          {
            "stringId": 1831658955582693376,
            "femaleVariant": "あなたが持っててくれるのは、嬉しいし"
          },
          {
            "stringId": 1831664955350016000,
            "femaleVariant": "いない、1人も"
          },
          {
            "stringId": 1831686266138877952,
            "femaleVariant": "ケチ"
          },
          {
            "stringId": 1831686617067905024,
            "femaleVariant": "また吸い始めたら、あたしのせいになるでしょ",
            "maleVariant": "また吸い始めたら、俺のせいになるだろ"
          },
          {
            "stringId": 1831687368435523584,
            "femaleVariant": "V、お願い。1本だけ"
          },
          {
            "stringId": 1831696308711940096,
            "femaleVariant": "言葉が見つからない"
          },
          {
            "stringId": 1831724151122649088,
            "femaleVariant": "あなたは彼女に選択の自由を与えてくれたじゃない"
          },
          {
            "stringId": 1832715479537115136,
            "femaleVariant": "ジュディ、気を強く持って。乗り越えないと",
            "maleVariant": "ジュディ、気を強く持て。乗り越えるんだ"
          },
          {
            "stringId": 1832731391870853120,
            "femaleVariant": "ねえ。少しの間、何も言わずにいましょ。お願い"
          },
          {
            "stringId": 1832761840185860096,
            "femaleVariant": "ああ、クソ"
          },
          {
            "stringId": 1832771838869725184,
            "femaleVariant": "私のせいよ。しばらくそっとしておけば… 立ち直ってくれると思ってた…"
          },
          {
            "stringId": 1832807642136535040,
            "femaleVariant": "覚えてる？ エヴリンはフィンガーズのとこに4日いた"
          },
          {
            "stringId": 1833090277056405504,
            "femaleVariant": "あいつ、そう言ってたね",
            "maleVariant": "あいつ、そう言ってたな"
          },
          {
            "stringId": 1833090507189477376,
            "femaleVariant": "トムから聞いたんだけど、彼女がクラウドからいなくなったのは、あなたが現れる1週間前だったの。それを知った日は夜も眠れなかった"
          },
          {
            "stringId": 1833091409820475392,
            "femaleVariant": "空白の3日間に何があったのか… そのことが頭を離れなくて、彼女のバーチャをもう少し探ってみたの。そしたらウッドマンにたどり着いた"
          },
          {
            "stringId": 1833108935048650752,
            "femaleVariant": "エヴリンはあの男のもとで好き勝手に弄ばれた。散々酷いことをして… 飽きたらよそに売り飛ばしたってわけ"
          },
          {
            "stringId": 1833118183086317572,
            "femaleVariant": "なら殺しておいて良かった"
          },
          {
            "stringId": 1833120464502480896,
            "femaleVariant": "他にもバーチャが見つかったのね？",
            "maleVariant": "他にもバーチャが見つかったんだな？"
          },
          {
            "stringId": 1833125265470611456,
            "femaleVariant": "ウッドマンは何をしたの？",
            "maleVariant": "ウッドマンは何をしたんだ？"
          },
          {
            "stringId": 1833134536912494592,
            "femaleVariant": "ウッドマンが何をしたか、それも見たの？",
            "maleVariant": "ウッドマンが何をしたか、それも見たのか？"
          },
          {
            "stringId": 1833134569309298688,
            "femaleVariant": "他にもバーチャがあったってこと？",
            "maleVariant": "他にもバーチャがあったってことか？"
          },
          {
            "stringId": 1833140817580744704,
            "femaleVariant": "私にできることがあるはず"
          },
          {
            "stringId": 1833141339654152192,
            "femaleVariant": "たとえば？"
          },
          {
            "stringId": 1833141445350612992,
            "femaleVariant": "まだわからない。スージーに相談してみるよ… ごめん、ちょっとひとりにさせてくれる？ また必ず連絡するから"
          },
          {
            "stringId": 1833146717171310592,
            "femaleVariant": "ええ… 見なかったことにしたい"
          },
          {
            "stringId": 1833159437203496960,
            "femaleVariant": "破損してた分を一から見直したのよ。ノイズが酷くて、ほとんど何もわからなかったけど"
          },
          {
            "stringId": 1833160420902002688,
            "femaleVariant": "でもそのなかで、私と口論した時の様子が映ったシーンに出くわしたの。私と彼女の最後のやりとりだけど、その時彼女は記録してなかった"
          },
          {
            "stringId": 1833162635007975424,
            "femaleVariant": "あるとしたら、後から思い出して、追体験したとか？"
          },
          {
            "stringId": 1833162908208160768,
            "femaleVariant": "ならどうして保存されてたのよ？",
            "maleVariant": "ならどうして保存されてたんだよ？"
          },
          {
            "stringId": 1833163233954586624,
            "femaleVariant": "それがわからないの"
          },
          {
            "stringId": 1833166286166384640,
            "femaleVariant": "彼女が… ひどい仕打ちを受けてた時に、インプラントが起動したのかもしれない、防衛反応として、過去への逃避が行われたとかね"
          },
          {
            "stringId": 1846071701002739712,
            "femaleVariant": "本当の顔を知ってた人は、どれぐらいいるの？",
            "maleVariant": "本当の顔を知ってた人は、どれぐらいいるんだ？"
          },
          {
            "stringId": 1876373005134708736,
            "femaleVariant": "何かあったら、ホロコールして",
            "maleVariant": "何かあったら、ホロコールしてくれ"
          },
          {
            "stringId": 1877717801103245316,
            "femaleVariant": "[立つ] いつでも連絡して",
            "maleVariant": "[立つ] いつでも連絡しろよ"
          },
          {
            "stringId": 1877771287723491328,
            "femaleVariant": "遠慮はいらないからね",
            "maleVariant": "遠慮はいらないからな"
          },
          {
            "stringId": 1877777428486537216,
            "femaleVariant": "ありがとう。でも大丈夫"
          },
          {
            "stringId": 1906876133319622656,
            "femaleVariant": "あいつがそこまでの外道だったなんて"
          },
          {
            "stringId": 1906877212564373504,
            "femaleVariant": "あいつがそこまでの外道だったなんてね。せいぜい、どこにでもいるクズかと思ってた",
            "maleVariant": "あいつがそこまでの外道だったなんてな。せいぜい、どこにでもいるクズかと思ってた"
          },
          {
            "stringId": 1906881546740023296,
            "femaleVariant": "表じゃ猫を被ってるのよ。あいつのことは知ってたけど、ここまでとは思いもしなかった"
          },
          {
            "stringId": 1912507583725002756,
            "femaleVariant": "ウッドマンはあたしが殺した。成り行きでそうなったに過ぎないけど…",
            "maleVariant": "ウッドマンは俺が殺した。成り行きでそうなったに過ぎないが…"
          },
          {
            "stringId": 1912507583758557184,
            "femaleVariant": "正解だったね",
            "maleVariant": "正解だったな"
          },
          {
            "stringId": 1912507583808888832,
            "femaleVariant": "そうね。その場に居合わせたかったくらい"
          },
          {
            "stringId": 1912507583842443264,
            "femaleVariant": "でも、それで終わりじゃない… ああいう奴は他にも大勢いるんだから"
          },
          {
            "stringId": 1934309781035180032,
            "femaleVariant": "遠慮はいらないからね",
            "maleVariant": "遠慮はいらないからな"
          },
          {
            "stringId": 2257755116858466304,
            "femaleVariant": "エヴリンのことが頭から離れなくて… テックは何も問題なかった。ドールチップも不具合なし…"
          },
          {
            "stringId": 2257758153652002816,
            "femaleVariant": "精神的なトラウマが原因だと思ってバーチャをもう少し探ってみたら、ウッドマンにたどり着いた"
          }
        ]
      }
    }
  }
}