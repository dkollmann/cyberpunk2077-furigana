    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "血が見たいだろ！？ そのために来たんだよな！？",
              "maleVariant": "",
              "stringId": 1854553219312480260
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたと戦うの？",
              "maleVariant": "あんたと戦うのか？",
              "stringId": 1854553219362811904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "生き生きしてるね",
              "maleVariant": "生き生きしてるな",
              "stringId": 1854553219379589128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "戦う相手はあんた？",
              "maleVariant": "戦う相手はあんたか？",
              "stringId": 1854553219429920772
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "場合による",
              "maleVariant": "",
              "stringId": 1854553219429920780
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまり？",
              "maleVariant": "",
              "stringId": 1854553219429920788
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "吹き飛ぶ覚悟があるなら、ってことさ。構わねえんなら、賭け金を出して試合開始だ",
              "maleVariant": "",
              "stringId": 1854553219446697996
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やりましょ",
              "maleVariant": "やろう",
              "stringId": 1854553219497029632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "賭け金はマックスで",
              "maleVariant": "",
              "stringId": 1854553219497029648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また後で",
              "maleVariant": "",
              "stringId": 1854553219513806856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと待ってくれる？",
              "maleVariant": "ちょっと待ってくれるか？",
              "stringId": 1854553219547361284
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっとだけだぞ。ファンが待ってる",
              "maleVariant": "",
              "stringId": 1854553219547361292
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK。やりましょ",
              "maleVariant": "OK。やってやる",
              "stringId": 1854553219631247360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やるなら、大金を賭けないと。マックスまで上げましょ",
              "maleVariant": "やるなら、大金を賭けないと。マックスまで上げるぞ",
              "stringId": 1854553219648024580
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ、本気みたいだな。いいだろう。こっちも乗ってやる",
              "maleVariant": "",
              "stringId": 1854553219664801792
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2人とも準備はいい？",
              "maleVariant": "",
              "stringId": 1854553219681579012
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V対オゾブ、試合開始！",
              "maleVariant": "",
              "stringId": 1854553219782242308
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょちょちょ、おま、やばいって！",
              "maleVariant": "",
              "stringId": 1854553220050677764
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこまで、試合終了！ オゾブの勝利！",
              "maleVariant": "",
              "stringId": 1854553220084232196
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "当然の結果だな",
              "maleVariant": "",
              "stringId": 1854553220084232204
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも。別に、顔にグレネードを入れたのはあたしじゃないからね",
              "maleVariant": "どうも。別に、顔にグレネードを入れたのは俺じゃないからな",
              "stringId": 1854553220302336004
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいから消えてよ",
              "maleVariant": "",
              "stringId": 1854553220302336012
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[試合を開始する] 始めよう",
              "maleVariant": "",
              "stringId": 1854553220335890432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、いつでもどうぞ",
              "maleVariant": "ああ、いつでもいいぞ",
              "stringId": 1854553220369444868
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ついてこい",
              "maleVariant": "",
              "stringId": 1854553220470108164
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "生き生きしてるじゃない",
              "maleVariant": "生き生きしてるじゃないか",
              "stringId": 1854553220503662592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、まあ、人を楽しませるのは好きだからな。生粋のピエロさ",
              "maleVariant": "",
              "stringId": 1854553220503662600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？ はは！ こりゃ偶然だな！",
              "maleVariant": "",
              "stringId": 1854593747580186624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その鼻はどうしたの？",
              "maleVariant": "その鼻はどうしたんだ？",
              "stringId": 1854622118271873024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのグレネードの鼻は何？",
              "maleVariant": "そのグレネードの鼻は何だ？",
              "stringId": 1854622609408094208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何って、ここについてるだけだが",
              "maleVariant": "",
              "stringId": 1854623061335961600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふーん。で、誰かがジャブでも当てたらどうなるの？",
              "maleVariant": "ふーん。で、誰かがジャブでも当てたらどうなるんだ？",
              "stringId": 1854623256756973568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バカだな、グレネードなんだからどうなるかは決まってるだろ。せいぜい、気をつけろよ",
              "maleVariant": "",
              "stringId": 1854623718348517376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ… ウッソ…",
              "maleVariant": "おい… ウソだろ…",
              "stringId": 1854646630606532608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V… VがTKOにより勝利…",
              "maleVariant": "",
              "stringId": 1854647237992083456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お金を受け取って、さっさと出てって",
              "maleVariant": "",
              "stringId": 1854647573234413568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うっ… ああ！ 終わりだ！ 降参だ！",
              "maleVariant": "",
              "stringId": 1854700942212820992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これにて試合終了！ 勝者はV！",
              "maleVariant": "",
              "stringId": 1854701657526202368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも。鼻っ柱をへし折られたんじゃない？ オゾブ",
              "maleVariant": "どうも。鼻っ柱をへし折られたんじゃないか？ オゾブ",
              "stringId": 1854702519774441472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この俺を倒したんだ。あんただって鼻を高くして構わないぜ？ ハハハ！",
              "maleVariant": "",
              "stringId": 1854702966451040256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "子供のお誕生会には近づかせないようにしないとね",
              "maleVariant": "子供のお誕生会には近づかせないようにしないとな",
              "stringId": 1908228604463206400
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はは！ そいつは違いねえや",
              "maleVariant": "",
              "stringId": 1908229325766053888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふう… 今のは… いいパンチだったぞ、V。思わず笑っちまったよ",
              "maleVariant": "",
              "stringId": 1908255414118830080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、お前の金だ。持ってけ",
              "maleVariant": "",
              "stringId": 1908256128710791168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう一戦やるか、V？ こっちは何度でもやってやる。俺の鼻を明かしてみろよ！ ハハハ！",
              "maleVariant": "",
              "stringId": 1908264011955490816
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうした？ 会いたかったか？",
              "maleVariant": "",
              "stringId": 1908273523093458944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのグレネード、いつか爆発するよ",
              "maleVariant": "そのグレネード、いつか爆発するぞ",
              "stringId": 1908304384379637764
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ… こんなことしてたら、いつかそのグレネードが爆発するよ？ みんなあたしみたいに器用とは…",
              "maleVariant": "なあ… こんなことしてたら、いつかそのグレネードが爆発するぞ？ みんな俺みたいに器用とは…",
              "stringId": 1908304603473301504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、わかってる",
              "maleVariant": "",
              "stringId": 1908307553511854080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんないつかは死ぬんだ。だったら派手に散ろうや。なあ？",
              "maleVariant": "",
              "stringId": 1908307705697980416
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}