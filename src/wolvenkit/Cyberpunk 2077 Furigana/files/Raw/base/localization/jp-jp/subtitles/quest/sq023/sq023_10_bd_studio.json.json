          {
            "stringId": 1383584950234824704,
            "femaleVariant": "[くぎを打つ]"
          },
          {
            "stringId": 1383585947992637440,
            "femaleVariant": "[十字架を立たせる]"
          },
          {
            "stringId": 1383668566302310400,
            "femaleVariant": "〈父よ… 父よ、我が霊を御手に委ねん〉"
          },
          {
            "stringId": 1569013786454417408,
            "femaleVariant": "やっと来た！"
          },
          {
            "stringId": 1569034971196055552,
            "femaleVariant": "あたしに会いたいって…？",
            "maleVariant": "俺に会いたいって…？"
          },
          {
            "stringId": 1569037632163176448,
            "femaleVariant": "どうやら君が唯一の友人らしい。私はどうしたらいいんだ？"
          },
          {
            "stringId": 1569040810959069184,
            "femaleVariant": "何かあったの？",
            "maleVariant": "何かあったのか？"
          },
          {
            "stringId": 1569170987290615808,
            "femaleVariant": "私は本当にここにいるべきなのか…？ やっぱり狂ってるのか？"
          },
          {
            "stringId": 1569171317650776068,
            "femaleVariant": "わかった。やる"
          },
          {
            "stringId": 1569171648564584448,
            "femaleVariant": "やりたくない"
          },
          {
            "stringId": 1569221569690886144,
            "femaleVariant": "アーメン"
          },
          {
            "stringId": 1772017241380851712,
            "femaleVariant": "ヨシュアは朝からずっとあなたはどこだって。楽屋にいるから話してきて"
          },
          {
            "stringId": 1812694604579364864,
            "femaleVariant": "あたしに会いたいって…？",
            "maleVariant": "俺に会いたいって…？"
          },
          {
            "stringId": 1812699008648896512,
            "femaleVariant": "[手を合わせる] 祈りなよ。そばにいるから",
            "maleVariant": "[手を合わせる] 祈れよ。そばにいるから"
          },
          {
            "stringId": 1812699457607196672,
            "femaleVariant": "お祈りはしない"
          },
          {
            "stringId": 1812700336112558084,
            "femaleVariant": "[座る] どうかしてる。考え直して",
            "maleVariant": "[座る] どうかしてる。考え直せ"
          },
          {
            "stringId": 1812700561447346176,
            "femaleVariant": "[座る] 応援してる"
          },
          {
            "stringId": 1812700888854716416,
            "femaleVariant": "こんなこと、どうかしてる"
          },
          {
            "stringId": 1812700956248793088,
            "femaleVariant": "ヨシュア――大丈夫、あなたならできる",
            "maleVariant": "ヨシュア――大丈夫だ、あんたならできる"
          },
          {
            "stringId": 1812701758652702720,
            "femaleVariant": "あなたの死を利用して、スタジオは今後一生儲け続けるのよ",
            "maleVariant": "あんたの死を利用して、スタジオは今後一生儲け続けるんだぞ"
          },
          {
            "stringId": 1812702410950860800,
            "femaleVariant": "[座る] したい事はわかるけど、やり方が間違ってる"
          },
          {
            "stringId": 1812702742032441344,
            "femaleVariant": "何がしたいかはわかる。でもこんなのは正しいやり方じゃない"
          },
          {
            "stringId": 1812703152352813056,
            "femaleVariant": "ここまで来たら、あとは最後の一歩を踏み出すだけ",
            "maleVariant": "ここまで来たら、あとは最後の一歩を踏み出すだけだ"
          },
          {
            "stringId": 1812714935109050368,
            "femaleVariant": "死をBDにするなんて… 絶対に間違ってる"
          },
          {
            "stringId": 1812715956237524992,
            "femaleVariant": "[座る] まさに反逆者、真の革命家ね",
            "maleVariant": "[座る] まさに反逆者、真の革命家だな"
          },
          {
            "stringId": 1812717334284492800,
            "femaleVariant": "間違いない。あなたは真の反逆者、革命家ね",
            "maleVariant": "間違いない。あんたは真の反逆者、革命家だな"
          },
          {
            "stringId": 1812717678032871424,
            "femaleVariant": "…どういう意味だ？"
          },
          {
            "stringId": 1812717759335260160,
            "femaleVariant": "人々に“真実”と“現実”を見せつけようとしてる"
          },
          {
            "stringId": 1812719507386949632,
            "femaleVariant": "スタジオはあなたでひと稼ぎするつもりでいるけど、本当に利用してるのはあなたの方。これで世界は変わる",
            "maleVariant": "スタジオはあんたでひと稼ぎするつもりでいるが、本当に利用してるのはあんたの方だ。これで世界は変わる"
          },
          {
            "stringId": 1812719923478683648,
            "femaleVariant": "なかなか面白い発想だな…"
          },
          {
            "stringId": 1812720548631306240,
            "femaleVariant": "手遅れだ。もう後には引けないんだよ"
          },
          {
            "stringId": 1812721131421458432,
            "femaleVariant": "ああ。やってみる"
          },
          {
            "stringId": 1812721292482732032,
            "femaleVariant": "手遅れだ。後戻りはできない"
          },
          {
            "stringId": 1812721609890881536,
            "femaleVariant": "こ… 怖いんだ。一緒に祈ってくれるか？ どの祈りでもいい"
          },
          {
            "stringId": 1812723267848921088,
            "femaleVariant": "お祈りはしたくないけど、そばにいてあげる",
            "maleVariant": "お祈りはしたくないが、そばにはいてやるよ"
          },
          {
            "stringId": 1812723331837222912,
            "femaleVariant": "あたしはやめとく",
            "maleVariant": "俺はやめとく"
          },
          {
            "stringId": 1812723500649570304,
            "femaleVariant": "そうか。じゃあ少し時間をくれ"
          },
          {
            "stringId": 1812724648748015616,
            "femaleVariant": "[手を合わせる] わかった。祈りましょ",
            "maleVariant": "[手を合わせる] わかった。祈ろう"
          },
          {
            "stringId": 1812724969394167808,
            "femaleVariant": "ええ、わかった",
            "maleVariant": "ああ、わかった"
          },
          {
            "stringId": 1812730029318627328,
            "femaleVariant": "[主の祈りを唱える]"
          },
          {
            "stringId": 1812730704215695360,
            "femaleVariant": "天にまします我らの父よ…"
          },
