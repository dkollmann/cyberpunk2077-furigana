        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で… どう思う？",
                    "maleVariant": null,
                    "stringId": 1698275538001055744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ラインを救った警官は誰なの？",
                    "maleVariant": "ラインを救った警官は誰なんだ？",
                    "stringId": 1928826001470218244
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "レッド・クイーンズ・レースって何？",
                    "maleVariant": "レッド・クイーンズ・レースって何だ？",
                    "stringId": 1928826839810924544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "レッド・クイーンズ・レースって何なの？",
                    "maleVariant": "レッド・クイーンズ・レースって何なんだ？",
                    "stringId": 1928830979001769988
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "悪いが… 聞いたことがないな",
                    "maleVariant": null,
                    "stringId": 1928830979068878848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ラインの護衛以外に、ターミナルにアクセスできる人は？",
                    "maleVariant": null,
                    "stringId": 1928830979974848512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふむ… NCPDだけだ",
                    "maleVariant": null,
                    "stringId": 1928830980041957376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ラインを助けた警官について、何か知ってる？",
                    "maleVariant": "ラインを助けた警官について、何か知ってるか？",
                    "stringId": 1928830980847263748
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リバー・ウォード刑事。いい奴だ。一緒に働いたことがある",
                    "maleVariant": null,
                    "stringId": 1928830980914372608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ホルトって誰？",
                    "maleVariant": "ホルトって誰だ？",
                    "stringId": 1928830981048590340
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ラインの補佐役というか、番犬だな。今はラインの代理をしている",
                    "maleVariant": null,
                    "stringId": 1928830981098921984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "市長の護衛はNCPDが？",
                    "maleVariant": null,
                    "stringId": 1928830981266694144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや。ホルトが直々に選んだ、企業の連中だ",
                    "maleVariant": null,
                    "stringId": 1928830981317025792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "中には元警官もいるが",
                    "maleVariant": null,
                    "stringId": 1928830981367357440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの会見はなんのためだったの？",
                    "maleVariant": "あの会見はなんのためだったんだ？",
                    "stringId": 1928830981501575172
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "特に目的はないさ。ラインは自分の声も、自分の姿をテレビで見るのも好きだった",
                    "maleVariant": null,
                    "stringId": 1928830981551906816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "確かあの時の建前は、ナイトシティの暴力犯罪率が下がったことを報告する、といったものだったな",
                    "maleVariant": null,
                    "stringId": 1928830981619015680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "下がったって、ほんとに？",
                    "maleVariant": "下がったって、マジかよ",
                    "stringId": 1928830981669347328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ラインはパシフィカを“独立地区”に分類し直して、全体の統計対象から外したのさ",
                    "maleVariant": null,
                    "stringId": 1928830981736456192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なるほど、賢いね",
                    "maleVariant": "なるほど、賢いな",
                    "stringId": 1928830981770010624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ウォードと連絡を取ってみる",
                    "maleVariant": null,
                    "stringId": 1928845965333848068
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "市長の護衛はNCPDの警官なの？",
                    "maleVariant": "市長の護衛はNCPDの警官なのか？",
                    "stringId": 1928847199566520320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ホルトって誰？",
                    "maleVariant": "ホルトって誰だ？",
                    "stringId": 1928851463865065472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "セキュリティターミナルにアクセスできる人物は？",
                    "maleVariant": null,
                    "stringId": 1928855228236128256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何の会見だったの？",
                    "maleVariant": "何の会見だったんだ？",
                    "stringId": 1928860810435321856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "必要なら、紹介してもいい",
                    "maleVariant": null,
                    "stringId": 1929998123244675072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ウォードは、よくあの場に居合わせた…",
                    "maleVariant": null,
                    "stringId": 1930017986277236736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よほど運が良かったのか、誰かから情報を入手してたのか… 直接聞いてみないと",
                    "maleVariant": "よほど運が良かったのか、誰かから情報を入手してたのか… 直接聞いてみないとな",
                    "stringId": 1930019268828291072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうね",
                    "maleVariant": null,
                    "stringId": 1930020090106568704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "他にも何か必要なら、遠慮せずに言ってくれ",
                    "maleVariant": null,
                    "stringId": 1930020546883051520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼の情報を送る",
                    "maleVariant": null,
                    "stringId": 1930026374012821504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[チップを返す] まだわからない",
                    "maleVariant": null,
                    "stringId": 1930027661748035584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[チップを返す] 確かに何かあるかもしれない",
                    "maleVariant": null,
                    "stringId": 1930028216771895296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まだ何とも言えない。結論に至るには、手掛かりが少なすぎる",
                    "maleVariant": null,
                    "stringId": 1930029164164505600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "夏のコロナド湾に負けないほど臭う",
                    "maleVariant": null,
                    "stringId": 1930029211140710400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私たちに聞きたいことは？",
                    "maleVariant": null,
                    "stringId": 1930029290849263616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[チップを返す] ただの偶然みたい",
                    "maleVariant": "[チップを返す] ただの偶然のようだ",
                    "stringId": 1930050031296667648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もっとすごい偶然なんていくらでもある、って感じね",
                    "maleVariant": "もっとすごい偶然なんていくらでもある、って感じだ",
                    "stringId": 1930057031455825920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何か怪しいことは？ それとも私たちの考えすぎなのか？",
                    "maleVariant": null,
                    "stringId": 1930057708500377600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふむ、なるほど",
                    "maleVariant": null,
                    "stringId": 1930063033068642304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ではやはり、何かあるのか",
                    "maleVariant": null,
                    "stringId": 1930063841780785152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なるほどね。私たちは、まったくそうは思わなかったけど",
                    "maleVariant": null,
                    "stringId": 1930068211406692352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "第一印象だけではまだわからないさ",
                    "maleVariant": null,
                    "stringId": 1930070246197780480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[降りる]",
                    "maleVariant": null,
                    "stringId": 2065249417394741252
                  }
                }
              ]
            }
          }
        }
      }
    },
