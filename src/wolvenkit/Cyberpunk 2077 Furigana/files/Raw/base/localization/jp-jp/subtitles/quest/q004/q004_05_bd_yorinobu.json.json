  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい加減、しつこいぞ",
              "maleVariant": "",
              "stringId": 1646342469183037440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これでは私の立場が…",
              "maleVariant": "",
              "stringId": 1646366208037707776
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すべては大義のためだ",
              "maleVariant": "",
              "stringId": 1646367615327363072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "プログラムはまだ試験段階です。市場に出すわけには…！",
              "maleVariant": "",
              "stringId": 1646383359368847360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いちいちうるさいぞ",
              "maleVariant": "",
              "stringId": 1646383589954904064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待たせてすまない。阿呆の相手は… 疲れる",
              "maleVariant": "",
              "stringId": 1650358263638351872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って。それもいいけど… 1つ条件があるの",
              "maleVariant": "",
              "stringId": 1650359651785203712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "寝室のナイトスタンドだ",
              "maleVariant": "",
              "stringId": 1650361173076041728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど… ほら、もういいだろ",
              "maleVariant": "",
              "stringId": 1650454585695494144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "焦らしやがって… ずっと会いたかったんだぞ",
              "maleVariant": "",
              "stringId": 1650486272622026752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "警報のファームウェアは最新版、だけど… このシステムは穴だらけだって、某BBSで話題になってた",
              "maleVariant": "",
              "stringId": 1650559518658748416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自動タレット。最新のスキャナーモデル… システムは全てサーバーに繋がってる…",
              "maleVariant": "",
              "stringId": 1650559860880400384
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずい、ネットウォッチのカメラだ… モーションセンサーに熱感知、IFFも搭載されてる",
              "maleVariant": "",
              "stringId": 1650560025028681728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "セキュリティシステムは全てホテルのサブネットに繋がってるみたい",
              "maleVariant": "",
              "stringId": 1650585724082937856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "作戦は決まりね。フラットヘッドで中央サーバーに穴を開ける――",
              "maleVariant": "",
              "stringId": 1650587205947318272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フラットヘッドがないと、システムをシャットダウンできない",
              "maleVariant": "",
              "stringId": 1650587291746000896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しかし――",
              "maleVariant": "",
              "stringId": 1653700796166660096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "くつろいでてくれ。こっちはすぐに終わる",
              "maleVariant": "",
              "stringId": 1653701130570129408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい考えがあるの。ちょっと待ってて",
              "maleVariant": "",
              "stringId": 1653708332492087296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もういい。わかった",
              "maleVariant": "",
              "stringId": 1654542740130230272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気にしないで。そんなに待ってないわ",
              "maleVariant": "",
              "stringId": 1654552079117185024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少しな。気にするな",
              "maleVariant": "",
              "stringId": 1654553945397915648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってる",
              "maleVariant": "",
              "stringId": 1654621623160532992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "装填済み、セーフティはオン。実際にこの部屋に行った時、役に立ってくれそうね",
              "maleVariant": "",
              "stringId": 1654624110047260672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待ってってば。いいのを探してるの… フフ… うん、これでよし",
              "maleVariant": "",
              "stringId": 1654790608564908032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バグならハックできるでしょ？",
              "maleVariant": "お前ならハックできるだろ？",
              "stringId": 1654858051966595072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "余裕よ。連中のサブネットにアクセスする必要があるけど",
              "maleVariant": "",
              "stringId": 1654858195042693120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無力化できそう？",
              "maleVariant": "無力化できそうか？",
              "stringId": 1654863053523120128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "外部からは無理。中に入ってシステムとどう繋がってるか確認しないと",
              "maleVariant": "",
              "stringId": 1654863217000312832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シグネチャーを偽造してキーを生成しておくわ",
              "maleVariant": "",
              "stringId": 1654864902758199296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そしたら私が接続して、混乱を引き起こす",
              "maleVariant": "",
              "stringId": 1654869027336089600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自力じゃ無理なの？",
              "maleVariant": "自力じゃ無理なのか？",
              "stringId": 1654869522062635008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無理。少なくとも思いつかない",
              "maleVariant": "",
              "stringId": 1654869962867208192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたのお父様は、このプロジェクトに強い関心を示されています。リスクについて、きちんとご説明を受けて――",
              "maleVariant": "",
              "stringId": 1654880736289353728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこまでにしておけ。あの干からびた老いぼれの話を俺にするな。あいつから聞くことなど何一つない。何のビジョンもないお山の大将が…！",
              "maleVariant": "",
              "stringId": 1654883020389871616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈Relic〉の保管方法についてですが… 非常に特別な環境が必要になります。必ず資料をお読みください",
              "maleVariant": "",
              "stringId": 1654885991064399872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この20年、あいつとは目を合わせたこともない。そして、この先も… ないだろう",
              "maleVariant": "",
              "stringId": 1654892623685087232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブのPDA。重要な情報が入ってるかも",
              "maleVariant": "",
              "stringId": 1655204614574399488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "特殊な温度調節コンテナのマニュアルね",
              "maleVariant": "",
              "stringId": 1655211393643405312
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今の聞いた？ 〈Relic〉の資料がどこかにあるはず。探してみて",
              "maleVariant": "",
              "stringId": 1655219164463980544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "かなりの低温ね。冷却器か何かがあるみたい",
              "maleVariant": "",
              "stringId": 1656209557884198912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "熱パターンを確認。資料のスペックと一致してる",
              "maleVariant": "",
              "stringId": 1656219332961329152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "間違いないはず。よくやったわ",
              "maleVariant": "",
              "stringId": 1656220471446429696
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK、エディターのサーマルレイヤーをつけて",
              "maleVariant": "",
              "stringId": 1656224332454256640
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと待って。一致する熱パターンがさっきあったはず",
              "maleVariant": "",
              "stringId": 1656226543053791232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "情報は揃ったようね",
              "maleVariant": "",
              "stringId": 1656236222400126976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クラウド？ クラブの名前かしら？",
              "maleVariant": "クラウド？ クラブの名前か？",
              "stringId": 1656306459527045120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最初のシーンで、ヨリノブが話してた相手ね",
              "maleVariant": "",
              "stringId": 1656316077737205760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確執があるのは明らかね。ヨリノブが社長の座を狙ってるのか、サブロウが彼を切り捨てようとしてるのか、あるいはその両方か",
              "maleVariant": "",
              "stringId": 1656316180296327168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って… 新情報よ。ヨリノブはアラサカの役職に就いてないどころか、従業員ですらない",
              "maleVariant": "",
              "stringId": 1656322277321170944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘルマンはチップを取り戻したがってる。それもサブロウを恐れてのことだけじゃないみたい",
              "maleVariant": "ヘルマンはチップを取り戻したがってる。それもサブロウを恐れてのことだけじゃないようだ",
              "stringId": 1658858749527977984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チップの生みの親だし… 何か個人的な目的とか？",
              "maleVariant": "",
              "stringId": 1658866246041071616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "冷凍庫に入れとく必要があるってこと？",
              "maleVariant": "冷凍庫に入れとく必要があるってことか？",
              "stringId": 1658890053598441472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。そうしないと損傷する",
              "maleVariant": "",
              "stringId": 1658890991562256384
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "温度がわかれば、ヨリノブのチップを見つけやすくなる",
              "maleVariant": "",
              "stringId": 1658907042475798528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう一度調べてみましょ",
              "maleVariant": "もう一度調べてみるか",
              "stringId": 1658917149758582784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブはここにケースを隠してるってことね",
              "maleVariant": "ヨリノブはここにケースを隠してるってことだな",
              "stringId": 1658921750691966976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "終わりにしましょ",
              "maleVariant": "",
              "stringId": 1658924156444078080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、暇を持て余した金持ち用のね。会員制の超高級かつディープなエスコートサービスを提供してる",
              "maleVariant": "",
              "stringId": 1658936412821123072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何か、あったの？ 少し… 気が立ってるみたいだけど…",
              "maleVariant": "",
              "stringId": 1658955560607412224
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "君が私に要求するのか？",
              "maleVariant": "",
              "stringId": 1658970419147436032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1つだけ。集中して。私に",
              "maleVariant": "",
              "stringId": 1658971083206422528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、キャンディはまだある？ あれ、すごく気に入っちゃったの",
              "maleVariant": "",
              "stringId": 1658971771508486144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その音は… 換気と空調？ ってことはサーバーね",
              "maleVariant": "",
              "stringId": 1659500246734692352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "機材を冷却して管理してる",
              "maleVariant": "",
              "stringId": 1659501338595909632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "照明の熱ね。特に異常なし",
              "maleVariant": "",
              "stringId": 1659529689993797632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "普通のものは無視してかないと",
              "maleVariant": "",
              "stringId": 1659534584964669440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今のは聞かなかったことにします…",
              "maleVariant": "",
              "stringId": 1660316905839915008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ならばこれを聞け。サブロウ・アラサカは時代遅れの理想に囚われた暴君だ！ 現実を直視することを諦め、未来に目を向けることを拒み、そもそも存在したかも怪しい過去の世界を夢見てやがる",
              "maleVariant": "",
              "stringId": 1660317068713127936
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ、楽しそうね",
              "maleVariant": "へえ、楽しそうだな",
              "stringId": 1660328008447815680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ、ドナギー。何かお祝い事でもあったようね",
              "maleVariant": "",
              "stringId": 1660800326037102592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなとこにあるわけないでしょ",
              "maleVariant": "",
              "stringId": 1660804996595933184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただの冷蔵庫ね",
              "maleVariant": "",
              "stringId": 1660828525114646528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "温度がガイドラインと一致しない。ここじゃない",
              "maleVariant": "",
              "stringId": 1660828525164978176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "空調ね。ホテルによくある標準的モデル",
              "maleVariant": "",
              "stringId": 1660899619758628864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "温度は資料と一致するけど、ここにチップがあるとは思えない",
              "maleVariant": "",
              "stringId": 1660900974267162624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この中じゃ温度が低すぎて、チップが損傷する",
              "maleVariant": "",
              "stringId": 1709886265579139072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈Relic〉は低温で保管しろって",
              "maleVariant": "",
              "stringId": 1915814643698888704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "客層はセレブや企業の重役、成金傭兵とかビジネス界隈の連中…",
              "maleVariant": "",
              "stringId": 1915815103260389376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまり、チップの件は間違いなく奴の謀反ね",
              "maleVariant": "つまり、チップの件は間違いなく奴の謀反だな",
              "stringId": 1915817320050716672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "温度が変わって損傷する可能性がある",
              "maleVariant": "",
              "stringId": 1915818186040274944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはもう見た。他を探して",
              "maleVariant": "",
              "stringId": 1928675921689980928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ、ヨリノブが〈Relic〉をどこに保管してるか突き止めるのよ",
              "maleVariant": "",
              "stringId": 1928739524468158464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "結構いけるシャンパンね。味がブーストされてるの？",
              "maleVariant": "結構いけるシャンパンだ。味がブーストされてるのか？",
              "stringId": 1957616205530279936
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブーストされてるのは値段だけ。現実でもすぐ飲めるようになるよ",
              "maleVariant": "",
              "stringId": 1957617154231513088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この匂いは…",
              "maleVariant": "",
              "stringId": 1957618018375577600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エヴリンの… 彼女の恐怖を感じた",
              "maleVariant": "",
              "stringId": 1957619926515462144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ずいぶんアダルトなBDだったのね",
              "maleVariant": "ずいぶんアダルトなBDだったんだな",
              "stringId": 1957622654826328064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うわ… これじゃまるであたしが…",
              "maleVariant": "おい… これじゃまるで俺が…",
              "stringId": 1957629576216457216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "通話はここまでね。話しはじめたところまで戻してくれる？",
              "maleVariant": "",
              "stringId": 1957682279290384384
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "巻き戻して。聞き取れなかった",
              "maleVariant": "",
              "stringId": 1957688044327677952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "会話をよく聞いて。重要な情報があるかも",
              "maleVariant": "",
              "stringId": 1957704010533056512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。スギ、黒コショウ、ベンゾイン、ベチバー…",
              "maleVariant": "",
              "stringId": 1964965070985883648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう、それよ。そうか、あいつのコロンね…！",
              "maleVariant": "そう、それだ。なるほど、あいつのコロンか…！",
              "stringId": 1964966082987552768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、仕事よ。集中して",
              "maleVariant": "",
              "stringId": 1964969308289888256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すぐ平静を取り戻したけど",
              "maleVariant": "すぐに平静を取り戻したが",
              "stringId": 1964972312334299136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あ、ちょっと… なんだ、期待外れね",
