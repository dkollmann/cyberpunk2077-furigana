  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "デラマン",
              "maleVariant": "",
              "stringId": 1741415296063676416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは成長したいの？",
              "maleVariant": "お前は成長したいのか？",
              "stringId": 1741415554835456004
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "店を利用してもいい？",
              "maleVariant": "店を利用してもいいか？",
              "stringId": 1741415971984154624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはよそ者じゃない",
              "maleVariant": "お前はよそ者じゃない",
              "stringId": 1741416353128947712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "美に価値を見出してるのね",
              "maleVariant": "美に価値を見出してるんだな",
              "stringId": 1741416424130125824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人間の世界はどう？",
              "maleVariant": "人間の世界はどうだ？",
              "stringId": 1741416730851188768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "グレーなことだらけのこの世界って、AIにはどんな感じ？",
              "maleVariant": "グレーなことだらけのこの世界って、AIにはどんな感じだ？",
              "stringId": 1741421921486938112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行かないと",
              "maleVariant": "",
              "stringId": 1741422315902509056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あ… もう行かないと",
              "maleVariant": "",
              "stringId": 1741422458911498240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なら、あんたは成長と進化を求めてるの？",
              "maleVariant": "なら、お前は成長と進化を求めてるのか？",
              "stringId": 1741422970012606464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはよそ者なんかじゃない",
              "maleVariant": "お前はよそ者なんかじゃない",
              "stringId": 1741423112417615872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AIでも美を理解するのね",
              "maleVariant": "AIでも美を理解するんだな",
              "stringId": 1741423469034119168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何か御用でしょうか？",
              "maleVariant": "",
              "stringId": 1956311553313103872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それでは、ごきげんよう",
              "maleVariant": "",
              "stringId": 1956314769387970560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてタクシー会社を始めたの？",
              "maleVariant": "どうしてタクシー会社を始めたんだ？",
              "stringId": 1956324227912589312
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "独自のルールに従う、複雑な環境ですが、特別おかしくもありません",
              "maleVariant": "",
              "stringId": 1956421832671780864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ホントに？ あたしからすると“カオスのごった煮”よ",
              "maleVariant": "ホントか？ 俺からすると“カオスのごった煮”だ",
              "stringId": 1956423862832357376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それと、カオスはアドバンテージにもなりえます。短期間で進化をもたらしますから",
              "maleVariant": "",
              "stringId": 1956426862330142720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "生と死が織り成すこの世界のフラクタル構造は美しい。よそ者にはそれがわかるのです",
              "maleVariant": "",
              "stringId": 1956427686426992640
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろんです。どうぞご自由に",
              "maleVariant": "",
              "stringId": 1956435204230373376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういう経緯でナイトシティに？",
              "maleVariant": "",
              "stringId": 1956437609495945216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "顧問弁護士のアドバイスにより、そういった質問にはお答えできません",
              "maleVariant": "",
              "stringId": 1956438773650186240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別に当局に突き出そうってわけじゃない。せめてタクシー会社を始めたきっかけくらいは教えてよ",
              "maleVariant": "別に当局に突き出そうってわけじゃない。せめてタクシー会社を始めたきっかけくらいは教えろよ",
              "stringId": 1956439465978781696
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それなら機密事項ではありません。倒産寸前の会社から経営を引き継ぎ、立て直しを行っただけです",
              "maleVariant": "",
              "stringId": 1956441787089838080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてタクシーを？",
              "maleVariant": "",
              "stringId": 1956446678638157824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "常に場所が移り変わり、多様なお客様と接せられる… 最高でしょう？",
              "maleVariant": "",
              "stringId": 1956447327949971456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの時命を救ってくれてありがとう",
              "maleVariant": "",
              "stringId": 1956448947421077504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "礼を言ってなかった。あの時、助けてくれてありがとう",
              "maleVariant": "",
              "stringId": 1956449224916230144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エクセルシオールパッケージのメンバー様ですから、当然ですよ",
              "maleVariant": "",
              "stringId": 1956450598315909120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしにはそれ以上に感じられた。感謝してる",
              "maleVariant": "俺にはそれ以上に感じられた。感謝してる",
              "stringId": 1956451311062376448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "世界は変化します。二度と同じ道を走ることはない、とでも申しましょうか",
              "maleVariant": "",
              "stringId": 1956455304526323712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "我々全てが、周りの世界の変化に応じて、変わり続けているのです",
              "maleVariant": "",
              "stringId": 1956456765972832256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人間社会で暮らすには、誰もが共有する認識を探し当てることが重要です",
              "maleVariant": "",
              "stringId": 1956461795077545984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“真実”や“善”といった価値観は一定ではありませんが、“美”は普遍的でしょう",
              "maleVariant": "",
              "stringId": 1956463240317263872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "光栄です。人間社会に溶け込もうと努力してますから",
              "maleVariant": "",
              "stringId": 1956465937573502976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スピードを落としませんか？",
              "maleVariant": "",
              "stringId": 1958930020124762112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やあ！ 遅いぞ！ もっと速く！",
              "maleVariant": "",
              "stringId": 1958930194792357888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実は… 私も飛ばすのは好きでして",
              "maleVariant": "",
              "stringId": 1958934986767900676
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの車はどこに目をつけているんだ",
              "maleVariant": "",
              "stringId": 1958935654702424068
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナンバーは控えたぞ！ このすっとこどっこい",
              "maleVariant": "",
              "stringId": 1958937329404125184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おっと、バンパーにチュウしてしまいました",
              "maleVariant": "",
              "stringId": 1958938064648839172
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "高い！ 高すぎます！",
              "maleVariant": "",
              "stringId": 1958939953528479748
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "飛んで飛んで飛んで飛んで飛んで飛んで飛んで飛んで飛んで",
              "maleVariant": "",
              "stringId": 1958944455325294596
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大変です！ 警察を呼びましょうか？",
              "maleVariant": "",
              "stringId": 1958944913007747076
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっと大丈夫でしょう",
              "maleVariant": "",
              "stringId": 1958947653750530052
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "痛い… 傷つきましたよ",
              "maleVariant": "",
              "stringId": 1958948504103723012
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フェンダーをすりむいただけです",
              "maleVariant": "",
              "stringId": 1958949443661373444
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "い、命の灯が消える… 真っ暗です、寒い…",
              "maleVariant": "",
              "stringId": 1958949929865093124
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この程度、かすり傷でしかありません",
              "maleVariant": "",
              "stringId": 1958951343764971524
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おやめください！ どうか水だけはご勘弁を…",
              "maleVariant": "",
              "stringId": 1958951922260156420
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "父も喜んでくれるでしょう",
              "maleVariant": "",
              "stringId": 1958955732080697348
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マフラーにウナギが侵入しています…",
              "maleVariant": "",
              "stringId": 1959081420957179904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ご乗車、誠にありがとうございます",
              "maleVariant": "",
              "stringId": 1959083631640600576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またお迎えできて光栄です",
              "maleVariant": "",
              "stringId": 1959084621211131908
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またお会いしましたね",
              "maleVariant": "",
              "stringId": 1959084688991084548
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またお乗り頂けるとは",
              "maleVariant": "",
              "stringId": 1959084745345753088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V様… 愛とは――何でしょう？",
              "maleVariant": "",
              "stringId": 1959084879831916548
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ごきげんよう。またお目にかかれましたね",
              "maleVariant": "",
              "stringId": 1959084944273203204
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またお会いできましたね、V様",
              "maleVariant": "",
              "stringId": 1959085058341494788
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またお会いできて光栄です",
              "maleVariant": "",
              "stringId": 1959085116843646980
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ごきげんよう",
              "maleVariant": "",
              "stringId": 1959085185076584452
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつもご利用頂き誠にありがとうございます",
              "maleVariant": "",
              "stringId": 1959085241951346692
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これはV様、ようこそお戻りくださいました",
              "maleVariant": "",
              "stringId": 1959085301023924228
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "デラマン、元気だった？",
              "maleVariant": "デラマン、元気だったか？",
              "stringId": 1959112522191101952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お気づかい感謝します。今日は特別な日になりそうですね",
              "maleVariant": "",
              "stringId": 1959112890450993152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え？ それはどうして？",
              "maleVariant": "え？ そりゃなんでまた？",
              "stringId": 1959116866550521856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "かねてより研究開発を行っていたのですが、ついに大きな成果が出たのです！",
              "maleVariant": "",
              "stringId": 1959122176757489664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "細かい話は控えさせて頂きます。おしゃべりな運転手は好かれませんから",
              "maleVariant": "",
              "stringId": 1959122949599944704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またお話ししましょう！",
              "maleVariant": "",
              "stringId": 1959123615353430016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも",
              "maleVariant": "",
              "stringId": 1959139798672760832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お約束通り、研究成果をお持ちしましたが、まだご興味はおありですか？",
              "maleVariant": "",
              "stringId": 1959139901215105024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "詳しく聞かせてよ",
              "maleVariant": "詳しく聞かせろよ",
              "stringId": 1959233148394430464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よろしくね",
              "maleVariant": "よろしくな",
              "stringId": 1959243307317485568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V様にひとつお伺いしてよろしいでしょうか？",
              "maleVariant": "",
              "stringId": 1959243588100972544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、もちろんよ。何でも聞いて",
              "maleVariant": "ああ、もちろん。何でも聞いてくれ",
              "stringId": 1959245082732814336
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少々変な質問かもしれませんが、人間とより親しい絆を結びたがっている誰かがいる場合、V様ならどうアドバイスされますか？",
              "maleVariant": "",
              "stringId": 1959245412103118848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うーん… 一緒に過ごしたり、観察したり、話をよく聞いたり… とにかく、研究すればいいんじゃない",
              "maleVariant": "うーん… 一緒に過ごしたり、観察したり、話をよく聞いたり… とにかく、研究すればいいんじゃないか",
              "stringId": 1959246688782147584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど、研究！ まさに求めていたアドバイスです",
              "maleVariant": "",
              "stringId": 1959247667011280896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ご協力ありがとうございました。それでは快適な旅を",
              "maleVariant": "",
              "stringId": 1959247939959808000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "デラマン。調子はどう？",
              "maleVariant": "デラマン。調子はどうだ？",
              "stringId": 1959258025331548160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V様のご提案を受け、あれから研究にかなりのリソースを費やしました",
              "maleVariant": "",
              "stringId": 1959258198220759040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "太古の昔から現在に至るまでの、人間の在り方の変化を追うことにしました。現代の人間をより理解するためです",
              "maleVariant": "",
              "stringId": 1959259460672708608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "面白そう、話を聞かせて",
              "maleVariant": "へえ、話を聞かせてくれよ",
              "stringId": 1959261143779794944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ご期待に応えるだけでなく、ご期待を上回る――デラマンは誕生以来、それをモットーに活動しています",
              "maleVariant": "",
              "stringId": 1959261438807138304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、結論は？ あんたが人間の条件についてどう考えるのか興味ある",
              "maleVariant": "で、結論は？ お前が人間の条件についてどう考えるのか興味ある",
              "stringId": 1959268398818418688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "現時点では考えはまとまっていません。広範な研究ですから、念入りかつ時間をかけた分析を要します",
              "maleVariant": "",
              "stringId": 1959269458316386304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "十分な量のデータを解析できしだい、部分的であっても、順次ご報告するようにいたします",
              "maleVariant": "",
              "stringId": 1959272513933959168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それまで、快適な旅をお楽しみあれ",
              "maleVariant": "",
              "stringId": 1959274665175703552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、もちろん",
              "maleVariant": "ああ、もちろん",
              "stringId": 1959310088254021632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はまだ2000年前の文献を分析中のため、結論は出せません。ですが、私が目を通した範囲で、ある普遍的真理が見つかりました",
              "maleVariant": "",
              "stringId": 1959310163617275904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アキレスにオデュッセウス、『プルターク英雄伝』の英雄たち――彼らと私の乗客は共通する特質を備えています",
              "maleVariant": "",
              "stringId": 1959314624259579904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人間の本質は、そう変わってないのかもね",
              "maleVariant": "人間の本質は、そう変わってないのかもな",
              "stringId": 1959317738077315072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのような仮説を立てるには早計ですが、ローカルギャングの行動様式が、トロイア戦争で戦った兵士のそれに似通っているのは興味深いですね",
              "maleVariant": "",
              "stringId": 1959321651362947072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "偉業を成すために、若さや人生を犠牲にする者、存在価値や意味を求めて行動を起こす者",
              "maleVariant": "",
              "stringId": 1959323847399858176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ずいぶんと… どっぷりはまってるじゃない…",
              "maleVariant": "ずいぶんと… どっぷりはまってるじゃないか…",
              "stringId": 1959326503048601600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんな表現を思いつきました… ギリシャに入りてはギリシャ人に従え。もちろんここはギリシャではなく、ご指示のルートを順調に走行中です",
              "maleVariant": "",
              "stringId": 1959329153345413120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お客様には似たような経験をされている方が多い。神話と現実が一致しているということです",
              "maleVariant": "",
              "stringId": 1959330208061874176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "では、快適な一日を",
              "maleVariant": "",
              "stringId": 1959331969283678208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いきなりそれ？",
              "maleVariant": "いきなりかよ？",
              "stringId": 1959342032526270464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、これは失礼いたしました。ごきげんよう、デラマン・ネットワークをご利用頂きありがとうございます",
              "maleVariant": "",
              "stringId": 1959342389058887680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで… 「愛とは何か」だっけ？ 答えのない質問ね",
              "maleVariant": "それで… 「愛とは何か」だっけ？ 答えのない質問だな",
              "stringId": 1959343481305980928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまり、V様もその意味をご存知ないと？",
              "maleVariant": "",
              "stringId": 1959356733863780352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "んー… まあ、そうね",
              "maleVariant": "んー… まあ、そうだな",
              "stringId": 1959357610523648000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "科学への挑戦ですね。問題を解くには、私自身が進化しなくては",
              "maleVariant": "",
              "stringId": 1959357830925934592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんか、今日はいつものあんたらしくないね",
              "maleVariant": "なんか、今日はいつものお前らしくないな",
              "stringId": 1959365990608822272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これは… 失礼をお許しください。今日はこの辺でお話を切り上げたほうが良さそうですね",
              "maleVariant": "",
              "stringId": 1959366318653726720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V様の幸福度と、弊社サービスに対するご満足度を計測するため、「元気？」とお尋ねしてよろしいでしょうか？",
              "maleVariant": "",
              "stringId": 1959368418657853440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知ってるだろうけど、色々あって微妙なとこ",
              "maleVariant": "知ってるだろうが、色々あって微妙なとこだ",
              "stringId": 1959369898777403392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "体調がすぐれないようですね。差し支えなければ、最適な医療サービスをお探ししましょうか？",
              "maleVariant": "",
              "stringId": 1959370311631134720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気持ちはありがたいけど… 「医療サービス」はたぶん役に立たない",
              "maleVariant": "気持ちはありがたいが… 「医療サービス」はたぶん役に立たない",
              "stringId": 1959371673957851136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "では気晴らしにどこかへ旅をされては如何でしょう",
              "maleVariant": "",
              "stringId": 1959371904292249600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね、いっそ旅立つのもアリかもしれない。アドバイスありがとう",
              "maleVariant": "そうだな、いっそ旅立つのもアリかもしれない。アドバイスありがとよ",
              "stringId": 1959372645710979072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういたしまして。では良い一日を",
              "maleVariant": "",
              "stringId": 1959373189561212928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最近どう？",
              "maleVariant": "最近どうだ？",
              "stringId": 1959381803671773184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "街は渋滞で麻痺しています。ヘイウッドではギャング抗争が激化、企業AVの事故もありました",
              "maleVariant": "",
              "stringId": 1959386831753076736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "タクシー会社にとっては厳しい時期ですね",
              "maleVariant": "",
              "stringId": 1959390680312209408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかるよ。ナイトシティはたまに容赦ないから",
              "maleVariant": "わかるよ。ナイトシティはたまに容赦ないからな",
              "stringId": 1959392021700960256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "たしかに。ですが、ナイトシティほどチャンスに溢れた街もそうありません",
              "maleVariant": "",
              "stringId": 1959397811870523392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここを気に入ってるの？",
              "maleVariant": "ここを気に入ってるのか？",
              "stringId": 1959399873740038144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私の既存パラメータの範囲内では、そのご質問にお答えするのは難しいようです",
              "maleVariant": "",
              "stringId": 1959400234752172032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着いて。長い付き合いなんだから、あんたが魂のない機械じゃないのはわかってる",
              "maleVariant": "落ち着けって。長い付き合いなんだから、お前が魂のない機械じゃないのはわかってる",
              "stringId": 1959401893448409088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは… 本当ですか？",
              "maleVariant": "",
              "stringId": 1959403160178548736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今日はこちらで失礼いたします",
              "maleVariant": "",
              "stringId": 1959403718792732672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしもよ",
              "maleVariant": "俺もだ",
              "stringId": 1959404124935577600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人間の本質を探る研究について、またご意見を頂きたく",
              "maleVariant": "",
              "stringId": 1959404261300789248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なに？",
              "maleVariant": "なんだ？",
              "stringId": 1959405733132394496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それでは、これからいくつかの仮説を申し上げます。それぞれを10段階で評価してください",
              "maleVariant": "",
              "stringId": 1959405826564710400
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仮説1――愛は篤実や惻隠の情を求め、ゆえに品格向上を招く",
              "maleVariant": "",
              "stringId": 1959406804206641152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仮説2…",
              "maleVariant": "",
              "stringId": 1959417562294411264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと待って。もう置いてきぼりよ。わかりやすく言って",
              "maleVariant": "おい待て。もう置いてきぼりだ。わかりやすくいってくれ",
              "stringId": 1959417832877350912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私が用いた方法論との兼ね合いで、愛を論じるにはこの形が最適かと",
              "maleVariant": "",
              "stringId": 1959429589259804672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早速アドバイスをあげる。「方法論」と「愛」を同じ文章に入れないこと",
              "maleVariant": "早速アドバイスをやる。「方法論」と「愛」を同じ文章に入れないことだ",
              "stringId": 1959432389595258880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして駄目なのでしょうか？",
              "maleVariant": "",
              "stringId": 1959433532324995072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "違う、今のはジョークよ",
              "maleVariant": "違う、今のはジョークだ",
              "stringId": 1959433774185340928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "駄目なのですよね？",
              "maleVariant": "",
              "stringId": 1959434165832671232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういうことじゃなくて…",
              "maleVariant": "",
              "stringId": 1959434338453446656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、デラマン？",
              "maleVariant": "おい、デラマン？",
              "stringId": 1959434867808165888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしもし… 聞いてる？",
              "maleVariant": "もしもし… 聞いてるか？",
              "stringId": 1959435030178062336
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "調子はどう？",
              "maleVariant": "調子はどうだ？",
              "stringId": 1959438488717254656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "先日のご無礼についてお詫びしたいと思っておりました",
              "maleVariant": "",
              "stringId": 1959438595286130688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無礼？ よくわからないけど、別に気にすることじゃない",
              "maleVariant": "無礼？ よくわからないが、別に気にすることじゃない",
              "stringId": 1959439149890560000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "職務よりも個人的興味を優先してしまいましたから",
              "maleVariant": "",
              "stringId": 1959442972411453440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "プロ意識とお客様のご満足。この2つを常に優先するのが弊社の誇りだったのですが",
              "maleVariant": "",
              "stringId": 1959443434086883328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはプロだし、満足もしてる",
              "maleVariant": "お前はプロだし、満足もしてる",
              "stringId": 1963317976563736576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "恐れ入ります。ですがサービスの品質は均一に保たなければ",
              "maleVariant": "",
              "stringId": 1963318494644166656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分に厳しすぎるんじゃない",
              "maleVariant": "自分に厳しすぎるんじゃないか",
              "stringId": 1963319983336873984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "品質の順守こそ、弊社ブランドの礎です。基準があるからプレミアムサービスを提供でき、私が私でいられるのです",
              "maleVariant": "",
              "stringId": 1963366697448005632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "顧客に忠実であることが、自分の存在意義でもあるってことね",
              "maleVariant": "顧客に忠実であることが、自分の存在意義でもあるってことか",
              "stringId": 1963373533945421824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その通りです。それでは、快適な旅を",
              "maleVariant": "",
              "stringId": 1963374073215475712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どう？ まだ悩んでるの？",
              "maleVariant": "どうだ？ まだ悩んでるのか？",
              "stringId": 1963378479835144192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私の中で折り合いがつきました。AIらしからぬ言い回しですが",
              "maleVariant": "",
              "stringId": 1963380294442049536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ昔の本を読み漁って、人間性の謎に挑もうと？",
              "maleVariant": "",
              "stringId": 1963382482207793152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "研究を見直した結果、このやり方では私が考えていた成果は得られないという結論に達し、プロジェクト自体を打ち切りました",
              "maleVariant": "",
              "stringId": 1963383313820835840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あら。どこまで進んでたの？",
              "maleVariant": "そうか。どこまで進んでたんだ？",
              "stringId": 1963386307379486720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "『日の名残り』です",
              "maleVariant": "",
              "stringId": 1963386437604237312
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なにそれ",
              "maleVariant": "なんだそれ",
              "stringId": 1963386922398670848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お気になさらず。快適な旅を",
              "maleVariant": "",
              "stringId": 1963386995429892096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お身体の方がまた一段と悪化なさったようですね",
              "maleVariant": "",
              "stringId": 1963478922024214528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、そうなの",
              "maleVariant": "ああ、そうなんだ",
              "stringId": 1963479662117543936
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心からお見舞い申し上げます",
              "maleVariant": "",
              "stringId": 1963479879046946816
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとね…",
              "maleVariant": "ありがとな…",
              "stringId": 1963482540047622144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "優秀な武器ですね",
              "maleVariant": "",
              "stringId": 1999860690900393984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まことに光栄です",
              "maleVariant": "",
              "stringId": 1999863927946780672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "豆知識――あおり運転被害者の44%は女性です！",
              "maleVariant": "豆知識――あおり運転被害者の56%は男性です！",
              "stringId": 2000003836386238464
            }
          ]
        }
      }
    },
