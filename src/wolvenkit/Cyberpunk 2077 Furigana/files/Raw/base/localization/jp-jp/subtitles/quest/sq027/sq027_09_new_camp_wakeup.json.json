    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1947404463386521600,
            "femaleVariant": "V… 何が起きてるのか話して"
          },
          {
            "stringId": 1947406641522143232,
            "femaleVariant": "血も出てたし… 何時間も気絶してた。大丈夫なの？"
          },
          {
            "stringId": 1947407692413399044,
            "femaleVariant": "なんでもない"
          },
          {
            "stringId": 1947408811587923968,
            "femaleVariant": "どんどん悪化してる"
          },
          {
            "stringId": 1947412378944139264,
            "femaleVariant": "なんでもない。もう平気よ",
            "maleVariant": "なんでもない。もう平気だ"
          },
          {
            "stringId": 1947412847313678336,
            "femaleVariant": "ごまかせると思ってるの？"
          },
          {
            "stringId": 1947413144505282560,
            "femaleVariant": "ちゃんと話してよ。それともそんな芝居をいつまでも続ける気？"
          },
          {
            "stringId": 1947414091914670080,
            "femaleVariant": "パナム、あたし… 日に日に悪化してて…",
            "maleVariant": "パナム、俺… 日に日に悪化してて…"
          },
          {
            "stringId": 1947414952367747072,
            "femaleVariant": "落ち着いて、V。ゆっくりでいいから――いったい何が起きてるのか、ちゃんと説明してよ"
          },
          {
            "stringId": 1947415432951099396,
            "femaleVariant": "ジョニー・シルヴァーハンドを知ってる？",
            "maleVariant": "ジョニー・シルヴァーハンドを知ってるか？"
          },
          {
            "stringId": 1947416848981684224,
            "femaleVariant": "あたしの頭に入ってるチップのこと",
            "maleVariant": "俺の頭に入ってるチップのことだ"
          },
          {
            "stringId": 1947418956921434112,
            "femaleVariant": "本当のことを話すのが怖い"
          },
          {
            "stringId": 1947419833950400512,
            "femaleVariant": "本当のことを話したら… あたしたちはこのままの関係じゃいられなくなる",
            "maleVariant": "本当のことを話したら… 俺たちはこのままの関係じゃいられなくなる"
          },
          {
            "stringId": 1947486841144471552,
            "femaleVariant": "V、よく聞いて"
          },
          {
            "stringId": 1947486935616974848,
            "femaleVariant": "いいから本当のことを話して！ 心配なんだよ！"
          },
          {
            "stringId": 1947490954582290432,
            "femaleVariant": "それがどんなことだとしても、私の気持ちは…"
          },
          {
            "stringId": 1947493282739777536,
            "femaleVariant": "ああもう、さっさと本当のことを話して！"
          },
          {
            "stringId": 1947494844782473216,
            "femaleVariant": "ジョニー・シルヴァーハンドを知ってる？",
            "maleVariant": "ジョニー・シルヴァーハンドを知ってるか？"
          },
          {
            "stringId": 1947494915531993088,
            "femaleVariant": "そうね… 〈Relic〉って聞いたことある？",
            "maleVariant": "そうだな… 〈Relic〉って聞いたことあるか？"
          },
          {
            "stringId": 1947496595300413440,
            "femaleVariant": "魂をチップに詰め込むとかいう、アラサカのテックだっけ？ 金持ち向けの胡散臭い商品だと思ってたけど"
          },
          {
            "stringId": 1947498317783625728,
            "femaleVariant": "誇大広告なんかじゃない。あたしの頭にはそれが入ってる。他人の人格コンストラクト入りでね",
            "maleVariant": "誇大広告なんかじゃない。俺の頭にはそれが入ってる。他人の人格コンストラクト入りでな"
          },
          {
            "stringId": 1947499797416636416,
            "femaleVariant": "じゃあ取り出しなよ！"
          },
          {
            "stringId": 1947503021360017408,
            "femaleVariant": "曲なら聞いたことがあるけど… でもなんで？"
          },
          {
            "stringId": 1947504691598979072,
            "femaleVariant": "それが原因。ジョニーが… あたしの頭の中で生きてるの",
            "maleVariant": "それが原因さ。ジョニーが… 俺の頭の中で生きてるんだ"
          },
          {
            "stringId": 1947505641373954048,
            "femaleVariant": "はい？ 頭の中って… それは… えっと、何かの例えで言ってるわけ？"
          },
          {
            "stringId": 1947511706555641856,
            "femaleVariant": "V… 話を戻すけどさ…"
          },
          {
            "stringId": 1947512811486310404,
            "femaleVariant": "あたしにもわからない",
            "maleVariant": "俺にもわからない"
          },
          {
            "stringId": 1947516061451153408,
            "femaleVariant": "残念だけどできることはない",
            "maleVariant": "残念だができることはない"
          },
          {
            "stringId": 1947516316330618880,
            "femaleVariant": "正直、あたしにもわからない",
            "maleVariant": "正直、俺にもわからない"
          },
          {
            "stringId": 1947520915368177664,
            "femaleVariant": "でも何か手があるはずだ。誰かに…"
          },
          {
            "stringId": 1947523448643907584,
            "femaleVariant": "ヘルマンを覚えてる？ あいつはコンストラクトの開発者だったの",
            "maleVariant": "ヘルマンを覚えてるか？ あいつはコンストラクトの開発者だったんだ"
          },
          {
            "stringId": 1947524044000194560,
            "femaleVariant": "それで奴を追ってたのか"
          },
          {
            "stringId": 1947525687949258752,
            "femaleVariant": "でも無駄だった。解決策はないのかも",
            "maleVariant": "でも無駄だった。解決策はないのかもな"
          },
          {
            "stringId": 1947526241429614592,
            "femaleVariant": "そんなことない"
          },
          {
            "stringId": 1947527040360640512,
            "femaleVariant": "でも解決には繋がらなかった。だから次の手を考えないと",
            "maleVariant": "だが解決には繋がらなかった。だから次の手を考えないと"
          },
          {
            "stringId": 1947553441960816640,
            "femaleVariant": "きっと方法は見つかる。そしたら知らせて。力になるから"
          },
          {
            "stringId": 1947562000052252676,
            "femaleVariant": "ありがとう、パナム"
          },
          {
            "stringId": 1947562560612593664,
            "femaleVariant": "[パナムにキスする] そばにいてくれてありがとう"
          },
          {
            "stringId": 1947562781836963840,
            "femaleVariant": "あたしは大丈夫。そっちも元気でね！",
            "maleVariant": "俺は大丈夫だ。そっちも元気でな！"
          },
          {
            "stringId": 1947563333673152512,
            "femaleVariant": "ありがとう、パナム"
          },
          {
            "stringId": 1947563407811670016,
            "femaleVariant": "ありがとね",
            "maleVariant": "ありがとな"
          },
          {
            "stringId": 1947563488677851136,
            "femaleVariant": "あたしなら大丈夫。新しいキャンプで頑張って",
            "maleVariant": "俺なら大丈夫だ。新しいキャンプで頑張れよ"
          },
          {
            "stringId": 1947564437865623552,
            "femaleVariant": "そばにいてくれて"
          },
          {
            "stringId": 1947564698231238656,
            "femaleVariant": "気をつけてね。何かあったら… なくてもいいから、とにかくまた連絡して"
          },
          {
            "stringId": 1947566217273929728,
            "femaleVariant": "一緒に歩こうか"
          },
          {
            "stringId": 1947567810186702848,
            "femaleVariant": "ありがとう…"
          },
          {
            "stringId": 1947569196957822976,
            "femaleVariant": "じゃあね、パナム！",
            "maleVariant": "じゃあな、パナム！"
          },
          {
            "stringId": 1947620009591828484,
            "femaleVariant": "本当のことなの。話せば長くなる",
            "maleVariant": "本当のことだ。話せば長くなる"
          },
          {
            "stringId": 1947620976966103040,
            "femaleVariant": "シルヴァーハンドがあたしの頭を侵食してる",
            "maleVariant": "シルヴァーハンドが俺の頭を侵食してる"
          },
          {
            "stringId": 1947621501103108096,
            "femaleVariant": "長い話よ。仕事でヘマをして、気づいたらあいつの人格コンストラクトがあたしの頭に入ってた",
            "maleVariant": "長い話だ。仕事でヘマをして、気づいたらあいつの人格コンストラクトが俺の頭に入ってた"
          },
          {
            "stringId": 1947621619147599872,
            "femaleVariant": "あたしの頭はあいつのコンストラクトに侵食されてるの",
            "maleVariant": "俺の頭はあいつのコンストラクトに侵食されてるんだ"
          },
          {
            "stringId": 1947622932015083520,
            "femaleVariant": "嘘でしょ"
          },
          {
            "stringId": 1947623142149713920,
            "femaleVariant": "ま、向こうも不満らしいけど。共存はどうも無理みたいで、症状がどんどん悪化してる",
            "maleVariant": "ま、向こうも不満らしいがな。共存はどうも無理なようで、症状がどんどん悪化してる"
          },
          {
            "stringId": 1947625918846070784,
            "femaleVariant": "本気で言ってるんだね"
          },
          {
            "stringId": 1947626740828991488,
            "femaleVariant": "いまは、なんとか生き延びようとしてる"
          },
          {
            "stringId": 1947627029111894016,
            "femaleVariant": "体には悪影響が出てるみたいだけど"
          },
          {
            "stringId": 1947631101562974208,
            "femaleVariant": "ありえない。そんなわけないでしょ？ ジョニー・シルヴァーハンド？"
          },
          {
            "stringId": 1947631731614543872,
            "femaleVariant": "ねえ。そのことって… 誰かに… その、相談はしたの？"
          },
          {
            "stringId": 1947632163678187520,
            "femaleVariant": "セラピーで治るならいいんだけど… そうもいかないの。コンストラクトは… その、癌みたいなもの。日に日に悪化してる",
            "maleVariant": "セラピーで治るならいいんだが… そうもいかないんだ。コンストラクトは… その、癌みたいなもんで、日に日に悪化してる"
          },
          {
            "stringId": 1947634931398733828,
            "femaleVariant": "チップは破損してて、取り出せばあたしは死ぬ",
            "maleVariant": "チップは破損してて、取り出せば俺は死ぬ"
          },
          {
            "stringId": 1947635269845512192,
            "femaleVariant": "人格コンストラクトが脳を侵食してるの",
            "maleVariant": "人格コンストラクトが脳を侵食してるんだ"
          },
          {
            "stringId": 1947636668092248064,
            "femaleVariant": "チップは破損してて、取り出せばあたしは死ぬの。でも…",
            "maleVariant": "チップは破損してて、取り出せば俺は死ぬんだ。だが…"
          },
          {
            "stringId": 1947636772580749312,
            "femaleVariant": "チップ内の人格コンストラクトが、あたしの脳を侵食してる",
            "maleVariant": "チップ内の人格コンストラクトが、俺の脳を侵食してる"
          },
          {
            "stringId": 1947637830702985216,
            "femaleVariant": "サイバーサイコシスってこと？ それだったら頼れる相手が――"
          },
          {
            "stringId": 1947638232584417280,
            "femaleVariant": "そうじゃない。チップがあたしの神経接続を侵食してるの。サイバーサイコシスは関係ない",
            "maleVariant": "そうじゃない。チップが俺の神経接続を侵食してるんだ。サイバーサイコシスは関係ない"
          },
          {
            "stringId": 1947638889177542656,
            "femaleVariant": "取り出したら、あたしは死ぬ。放置しても、状態が悪化して… 死ぬ",
            "maleVariant": "取り出したら、俺は死ぬ。放置しても、状態が悪化して… 死ぬ"
          },
          {
            "stringId": 1947643814229962752,
            "femaleVariant": "そんな…"
          },
          {
            "stringId": 1947647502919004160,
            "femaleVariant": "どうにかして、進行を止められないの？ 取り出す方法が何かあるはずだ！"
          },
          {
            "stringId": 1947648959969882112,
            "femaleVariant": "その方法を探してるけど、時間がない。症状は日に日に悪化してる",
            "maleVariant": "その方法を探してるが、時間がない。症状は日に日に悪化してる"
          },
          {
            "stringId": 1947651317470720000,
            "femaleVariant": "そんな…"
          },
          {
            "stringId": 1989299053052841988,
            "femaleVariant": "ここはどこ？",
            "maleVariant": "ここはどこだ？"
          },
          {
            "stringId": 1989299684866019328,
            "femaleVariant": "何があったの？",
            "maleVariant": "何があったんだ？"
          },
          {
            "stringId": 1989300029218377728,
            "femaleVariant": "ここは… ここはどこ？",
            "maleVariant": "ここは… ここはどこだ？"
          },
          {
            "stringId": 1989300160030330884,
            "femaleVariant": "いったい… 何が起きたの？ 確か… 二人で話してたら、急に…",
            "maleVariant": "いったい… 何があったんだ？ 確か… 二人で話してたら、急に…"
          },
          {
            "stringId": 1989300574360457216,
            "femaleVariant": "もう行くよ。だいぶ… マシになったから",
            "maleVariant": "もう行くよ。だいぶ… マシになったからさ"
          },
          {
            "stringId": 1989304274005020672,
            "femaleVariant": "V！ 生者の世界によくぞ戻った！"
          },
          {
            "stringId": 1989305535668441088,
            "femaleVariant": "この近辺は昔使ってたから知ってるんだ"
          },
          {
            "stringId": 1989306481014231040,
            "femaleVariant": "声がしたと思ったんだ！"
          },
          {
            "stringId": 1989394591764992004,
            "femaleVariant": "もう少しここに居るよ。ありがとう"
          },
          {
            "stringId": 1989395837305507840,
            "femaleVariant": "忘れないよう、もう一度言っておく"
          },
          {
            "stringId": 1989396850448027648,
            "femaleVariant": "さっそく新拠点を構えたのね",
            "maleVariant": "さっそく新拠点を構えたのか"
          },
          {
            "stringId": 1989401209235853312,
            "femaleVariant": "だいぶ良くなった。助かったよ"
          },
          {
            "stringId": 1989401577093091328,
            "femaleVariant": "ありがとう。もう少しここに居る"
          },
          {
            "stringId": 1989401769645199364,
            "femaleVariant": "良くなったよ、ありがとう"
          },
          {
            "stringId": 1989402808322641920,
            "femaleVariant": "新たな拠点を構えたのね",
            "maleVariant": "新たな拠点を構えたんだな"
          },
          {
            "stringId": 1989403539020730368,
            "femaleVariant": "引っ越しには慣れてるからな"
          },
          {
            "stringId": 1989403768281387008,
            "femaleVariant": "ここはいい場所だ。一帯を掌握しやすい。残りの奴らもすぐに合流する"
          },
          {
            "stringId": 1989432291394080768,
            "femaleVariant": "突然気を失ったから、一緒に連れて来たんだよ"
          },
          {
            "stringId": 1989434433525796864,
            "femaleVariant": "キャンプを移動させた。大丈夫。ここなら安全だから"
          },
          {
            "stringId": 1989437188864757760,
            "femaleVariant": "突然気を失ったから、一緒に連れて来たんだよ"
          },
          {
            "stringId": 1989437286843699200,
            "femaleVariant": "新天地さ。キャンプを移動させた。ここなら安全だよ。ひとまずはね"
          },
          {
            "stringId": 1989438692220751872,
            "femaleVariant": "本当に…？"
          },
          {
            "stringId": 1989438880846991360,
            "femaleVariant": "待って。一緒に行く"
          },
          {
            "stringId": 1989442034560946176,
            "femaleVariant": "本当に安全なの？",
            "maleVariant": "本当に安全なのか？"
          },
          {
            "stringId": 1989442812252016640,
            "femaleVariant": "よう、眠れる美女がお目覚めか！"
          },
          {
            "stringId": 1989443584557600768,
            "femaleVariant": "ミッチ"
          },
          {
            "stringId": 1989443758403112960,
            "femaleVariant": "車は俺が見といたからな。あそこにある"
          },
          {
            "stringId": 1989444960037654528,
            "femaleVariant": "のんびりしてってくれ"
          },
          {
            "stringId": 1989445834885582848,
            "femaleVariant": "それは良かった"
          },
          {
            "stringId": 1989450121413939200,
            "femaleVariant": "困った時はアルデカルドスが力になるからな"
          },
          {
            "stringId": 1989455230277537792,
            "femaleVariant": "助けが必要な時はいつでも来てくれ"
          },
          {
            "stringId": 1999485574543175680,
            "femaleVariant": "V？"
          },
          {
            "stringId": 1999491326695567360,
            "femaleVariant": "パナム… あたし…",
            "maleVariant": "パナム… 俺…"
          },
          {
            "stringId": 1999491880998006784,
            "femaleVariant": "V、大丈夫よ。落ち着いて。気分はどう？"
          },
          {
            "stringId": 1999500089217380352,
            "femaleVariant": "どこに…？"
          },
          {
            "stringId": 1999504706575659008,
            "femaleVariant": "どこに？"
          },
          {
            "stringId": 1999530197072343040,
            "femaleVariant": "やっと起きたか。心配したぞ"
          },
          {
            "stringId": 1999536111594631168,
            "femaleVariant": "大丈夫なのか？"
          },
          {
            "stringId": 1999540554755076096,
            "femaleVariant": "それはそうと大丈夫なのか？ 心配したぞ"
          },
          {
            "stringId": 1999547341189279744,
            "femaleVariant": "ありがとう。だけどもう行かないと",
            "maleVariant": "ありがとう。だがもう行かないと"
          },
          {
            "stringId": 1999547835395731456,
            "femaleVariant": "ありがとう。だけどもう行かないと",
            "maleVariant": "ありがとう。だが、もう行かないと"
          },
          {
            "stringId": 1999552354842177536,
            "femaleVariant": "まだフラフラしてるじゃねえか。気をつけるんだぞ？"
          },
          {
            "stringId": 1999554930211942400,
            "femaleVariant": "珍しく、意見が一致したね"
          },
          {
            "stringId": 1999558769963036672,
            "femaleVariant": "正直まだ事態が呑み込めてないし、わかったふりをするつもりもない。でも… 私に何かできることはないの？"
          },
          {
            "stringId": 1999573728327495680,
            "femaleVariant": "ファミリーみんながあんたの味方だよ。ソウルも言ってたでしょ？"
          },
          {
            "stringId": 1999577078670422016,
            "femaleVariant": "ひとまずはね。ミリテクもここまでは来ない。見晴らしが利く点も申し分ないよ"
          },
          {
            "stringId": 1999578352665096192,
            "femaleVariant": "とはいえ、ここはあくまで仮の拠点。全部その場しのぎさ。次の動きを早く決めないといけない"
          },
          {
            "stringId": 2006859935632523264,
            "femaleVariant": "だがすぐに出てくわけじゃないんだろ？"
          },
          {
            "stringId": 2077967784476962816,
            "femaleVariant": "もう行くよ"
          },
          {
            "stringId": 2227325631560499200,
            "femaleVariant": "V、ちゃんと話してよ"
          },
          {
            "stringId": 2227358561578074112,
            "femaleVariant": "V、どうかしたか？"
          }
        ]
      }
    }
  }
}