  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "上の階の騒ぎだが、あれは何だ？ さっき通りかかって… 鼓膜が破れるかと思ったぜ… 何かのパーティーか？",
                    "maleVariant": null,
                    "stringId": 1246154759014154240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "パーティーなもんか。あれはスレイトンの部屋さ。どっかのくだらんバンドが大好きで、いつもノンストップでズンズンやってるんだ。家にいないときでもな",
                    "maleVariant": null,
                    "stringId": 1246155319373168640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だが最悪なのは、そいつが降りてきて、ベラベラとバンドのことを語り出す時だ。シルヴァーハンドがどうの、ユーロダインがこうのって。4時間もぶっ通しだぞ",
                    "maleVariant": null,
                    "stringId": 1246155786937401344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あいつは絶対イカれてる。曲の歌詞どころか、アルバム全部の発売日、コンサートの内容まで暗記してる。まともじゃない。1日も早くくたばってほしいね",
                    "maleVariant": null,
                    "stringId": 1246157139197788160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "武器を捨てて、両手を上げろ！ 今すぐにだ！",
                    "maleVariant": null,
                    "stringId": 1246157905480351744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こん時はひどかった。めちゃくちゃに酔ったケリーが、ナンシーのギターケースにゲロッてな",
                    "maleVariant": null,
                    "stringId": 1272023330562736128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おい、これを見ろ。バンドの祭壇を作ってやがる。というか… 祭壇兼金庫って感じだな",
                    "maleVariant": null,
                    "stringId": 1610994235385597952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こん時のことは忘れねえ。ここから東海岸まで伸びるほど長い行列ができたんだ。打ち上げは二晩ぶっ通しで…",
                    "maleVariant": null,
                    "stringId": 1722720825722556416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "妙だな… こいつは覚えがない",
                    "maleVariant": null,
                    "stringId": 1722721085668741120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここはとにかく音のひどいハコで、照明もグダグダ。おまけに食い物まで最悪だった",
                    "maleVariant": null,
                    "stringId": 1722721376535334912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クソ、ケリーの奴も歳取ったな。いつの写真だ、これ？",
                    "maleVariant": null,
                    "stringId": 1722721423830306816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "見ろよ… 金庫室と入力用のテンキー… わかったぞ、あのギターの写真の日付だ！",
                    "maleVariant": null,
                    "stringId": 1722732650052734976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クソ、ケリーの奴も歳取ったな。いつの写真だ、これ？",
                    "maleVariant": null,
                    "stringId": 1722732746253291520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "目覚めたら留置所？",
                    "maleVariant": "目覚めたら留置所か？",
                    "stringId": 1903743171993849856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ぜひこの目で見たかった",
                    "maleVariant": null,
                    "stringId": 1903744703468457984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ウォッカの飲み過ぎで脳みそをやられたんでしょ",
                    "maleVariant": "ウォッカの飲み過ぎで脳みそをやられたんだろ",
                    "stringId": 1903747525513629696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いい機会だから言っとくが、俺はウォッカはやらない。バーボン、ウイスキー、テキーラ。それが俺の酒だ",
                    "maleVariant": null,
                    "stringId": 1903748084949897216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ま、その分、かわいこちゃんたちが埋め合わせてくれたがな",
                    "maleVariant": null,
                    "stringId": 1903752148861374464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "見てみろ、キーパッドがある… 何らかの数字を入れるってわけか、そうだな… 69、69、69と入れてみてくれ",
                    "maleVariant": null,
                    "stringId": 1903756025790779392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハッ、絶対ない",
                    "maleVariant": "ハッ、絶対ないね",
                    "stringId": 1903756878291456000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "スレイトンって奴はそんなに間抜け？",
                    "maleVariant": "スレイトンって奴はそんなに間抜けか？",
                    "stringId": 1903759997964439552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あのケリーのファンらしいからな。間違いない",
                    "maleVariant": null,
                    "stringId": 1903760365452578816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなに前じゃない。日付を見て。それに、お目当てのギターも写ってる",
                    "maleVariant": "そんなに前じゃない。日付を見ろ。それに、お目当てのギターも写ってるな",
                    "stringId": 1903765170111696896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなに前じゃない。日付は…",
                    "maleVariant": null,
                    "stringId": 1903766508698652672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "日付… そうだ、あの金庫室！ きっとこいつが暗証番号だ！ まだ冴えてんな、俺",
                    "maleVariant": null,
                    "stringId": 1903766748780613632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おいおいなんだよ。音楽と酒はなかなかいい趣味してんじゃねえか",
                    "maleVariant": null,
                    "stringId": 2008323675251298316
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "教えてやるよ。ここのは全部違う",
                    "maleVariant": null,
                    "stringId": 2008323700366790668
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしてわかるの？",
                    "maleVariant": "どうしてわかるんだ？",
                    "stringId": 2014024351686643712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何だかんだ言ってもケリーはギターを熟知してる。こんなガラクタは選ばないさ",
                    "maleVariant": null,
                    "stringId": 2014024616380780544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だがそれにしても… なんでまたケリーにこだわるんだ？",
                    "maleVariant": null,
                    "stringId": 2014027867939459072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ビンゴ",
                    "maleVariant": null,
                    "stringId": 2228769065565040640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こんなにあっさり見つかるなんて",
                    "maleVariant": "こんなにあっさり見つかるとは",
                    "stringId": 2471959741353873412
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バーね…",
                    "maleVariant": "バーか…",
                    "stringId": 2472023661858967552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ポスター…",
                    "maleVariant": null,
                    "stringId": 2472025434287951872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "随分入れ込んでるのね…",
                    "maleVariant": "随分入れ込んでるんだな…",
                    "stringId": 2472026535074648064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "またポスター…",
                    "maleVariant": null,
                    "stringId": 2472027245405196288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どんだけ好きなのよ…",
                    "maleVariant": "どんだけ好きなんだ…",
                    "stringId": 2472028437141180416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この写真…",
                    "maleVariant": null,
                    "stringId": 2472030888409878528
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
