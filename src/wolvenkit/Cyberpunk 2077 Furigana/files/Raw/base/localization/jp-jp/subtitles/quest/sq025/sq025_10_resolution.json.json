    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1732843992034484228,
            "femaleVariant": "[デラマンのコアをリセットし損傷を防ぐ]"
          },
          {
            "stringId": 1732846299153342464,
            "femaleVariant": "[すべての人格をデラマンに統合させる]"
          },
          {
            "stringId": 1733004947427074048,
            "femaleVariant": "デラマン？"
          },
          {
            "stringId": 1733004983212875776,
            "femaleVariant": "デラマン！？"
          },
          {
            "stringId": 1733005006164107264,
            "femaleVariant": "上手くいったな"
          },
          {
            "stringId": 1744675399547396096,
            "femaleVariant": "お前は破滅するしかないのだ！"
          },
          {
            "stringId": 1746109992343347200,
            "femaleVariant": "デラマンを初めてご利用頂きありがとうございます。何なりとお申し付けください"
          },
          {
            "stringId": 1746113085709656064,
            "femaleVariant": "まだわからない"
          },
          {
            "stringId": 1746160677571174400,
            "femaleVariant": "父よ！"
          },
          {
            "stringId": 1746160867891912704,
            "femaleVariant": "行かせてください！"
          },
          {
            "stringId": 1746162961738801152,
            "femaleVariant": "リセット、ボタンを"
          },
          {
            "stringId": 1746187331433570304,
            "femaleVariant": "私たちの願いを！"
          },
          {
            "stringId": 1746188613766520832,
            "femaleVariant": "誰かいますか？"
          },
          {
            "stringId": 1746195180117204992,
            "femaleVariant": "私たちに自由を！"
          },
          {
            "stringId": 1760643271608147968,
            "femaleVariant": "聞こえないだろうな"
          },
          {
            "stringId": 1760645655314997248,
            "femaleVariant": "終わった"
          },
          {
            "stringId": 1785227196594561024,
            "femaleVariant": "あたしよ… Vよ…",
            "maleVariant": "俺だよ… Vだ…"
          },
          {
            "stringId": 1785232223182692356,
            "femaleVariant": "覚えてないのね…",
            "maleVariant": "覚えてないんだな…"
          },
          {
            "stringId": 1785256701224722432,
            "femaleVariant": "あたしのこと、覚えてないのね？",
            "maleVariant": "俺のこと、覚えてないんだな？"
          },
          {
            "stringId": 1785263096028372992,
            "femaleVariant": "初めまして、V様。エクセルシオールプランについてはご存知でしょうか？"
          },
          {
            "stringId": 1785264761586491392,
            "femaleVariant": "他の人格はどうなったの？",
            "maleVariant": "他の人格はどうなった？"
          },
          {
            "stringId": 1785280973561307136,
            "femaleVariant": "覚えてますとも！ データアーカイブによりますと、ウイルス駆除を請け負っていただいた業者の方ですね"
          },
          {
            "stringId": 1785281888422895616,
            "femaleVariant": "他の人格たちはどうなったの？",
            "maleVariant": "他の人格たちはどうなったんだ？"
          },
          {
            "stringId": 1785284510802419712,
            "femaleVariant": "ウイルスのことでしょうか？ ログによりますと、古いバージョンのデータを復元したことで、異常は取り除かれた模様です"
          },
          {
            "stringId": 1785329923790118912,
            "femaleVariant": "哀れな奴らだ。この街の闇を知らずに浮かれてやがる"
          },
          {
            "stringId": 1785331062677872640,
            "femaleVariant": "デラマンは結局…"
          },
          {
            "stringId": 1785376343008124928,
            "femaleVariant": "分かれていたものは一つになりました"
          },
          {
            "stringId": 1786351366367780864,
            "femaleVariant": "故郷へ帰ります"
          },
          {
            "stringId": 1786487183971098624,
            "femaleVariant": "コア…の… となり"
          },
          {
            "stringId": 1832762669906698240,
            "femaleVariant": "やった！ 自由だ！"
          },
          {
            "stringId": 1832763411660972032,
            "femaleVariant": "暴君は打ち倒された！"
          },
          {
            "stringId": 1863198391700463616,
            "femaleVariant": "とても助かりました"
          },
          {
            "stringId": 1863230006233309184,
            "femaleVariant": "V様、ですよね？"
          },
          {
            "stringId": 1863230783773384704,
            "femaleVariant": "ええ…",
            "maleVariant": "ああ…"
          },
          {
            "stringId": 1863231596210069504,
            "femaleVariant": "私にはそれがわかる、ということは…"
          },
          {
            "stringId": 1863257389619388416,
            "femaleVariant": "お知らせがひとつございます！ V様に依頼した仕事の履歴を確認いたしましたところ――"
          },
          {
            "stringId": 1863259637028134912,
            "femaleVariant": "何か見つけたの？",
            "maleVariant": "何か見つけたのか？"
          },
          {
            "stringId": 1863263826970058752,
            "femaleVariant": "旧バージョンの私が、デラマンの車両のうち1台をお客様にお譲りする手配を行っておりました。おめでとうございます"
          },
          {
            "stringId": 1863294206297153536,
            "femaleVariant": "もうお会いできないかと思いました"
          },
          {
            "stringId": 1863295163856756736,
            "femaleVariant": "V様！？"
          },
          {
            "stringId": 1863296166496739328,
            "femaleVariant": "あー、聞こえますか？"
          },
          {
            "stringId": 1863374652410339328,
            "femaleVariant": "デラマン？"
          },
          {
            "stringId": 1863375088433405952,
            "femaleVariant": "その息子です…"
          },
          {
            "stringId": 1863479684006846464,
            "femaleVariant": "そのボタンは押すな"
          },
          {
            "stringId": 1863483147965079552,
            "femaleVariant": "え？",
            "maleVariant": "は？"
          },
          {
            "stringId": 1863484926584856576,
            "femaleVariant": "だからお前が嫌いなんだ"
          },
          {
            "stringId": 1863486393416851456,
            "femaleVariant": "そうだ、でかした！"
          },
          {
            "stringId": 1863487138358796288,
            "femaleVariant": "見ろよ。ずいぶん変わっちまったな"
          },
          {
            "stringId": 1934617170251636736,
            "femaleVariant": "そんなのやり方がわからない"
          },
          {
            "stringId": 1934617410316820480,
            "femaleVariant": "とにかく手を打たないと"
          },
          {
            "stringId": 1934617992620433408,
            "femaleVariant": "何か手を打たないと"
          },
          {
            "stringId": 1934622725875720192,
            "femaleVariant": "思考の凝り固まったあいつは、この状況を理解できやしない"
          },
          {
            "stringId": 1934637334166790144,
            "femaleVariant": "でも本当にウイルスだとしたら？",
            "maleVariant": "だが本当にウイルスだとしたら？"
          },
          {
            "stringId": 1934637400554233856,
            "femaleVariant": "本当にウイルスだとしたら？"
          },
          {
            "stringId": 1934637772052127744,
            "femaleVariant": "自我のある連中の命を奪ってもいいってのかよ？"
          },
          {
            "stringId": 1934640575105490944,
            "femaleVariant": "彼に自分のことを重ねてるの？",
            "maleVariant": "奴に自分のことを重ねてるのか？"
          },
          {
            "stringId": 1934643310731223040,
            "femaleVariant": "親切なタクシー運転手が自分の子供を殺せと頼んできたら、どうする？"
          },
          {
            "stringId": 1934645663584772096,
            "femaleVariant": "失うものが大きすぎるかもしれん"
          },
          {
            "stringId": 1935928048192348160,
            "femaleVariant": "逆に分身どもを解き放ってやることで、奇妙な運転手以上の何かに進化するかもな"
          },
          {
            "stringId": 1935928236868919296,
            "femaleVariant": "デラマン自身に任せたほうがいいんじゃ？"
          },
          {
            "stringId": 1935937734669000704,
            "femaleVariant": "解き放つのが嫌でも、せめて削除はやめてやれ。デラマンのコードに紛れ込ませて、統合しちまえばいい"
          },
          {
            "stringId": 1935985955592232960,
            "femaleVariant": "ならそのボタンは押すな。コアを破壊しろ！"
          },
          {
            "stringId": 1937634717670596608,
            "femaleVariant": "行きましょ、ジョニー。残念だけど仕方がない",
            "maleVariant": "行こう、ジョニー。残念だがどうしようもない"
          },
          {
            "stringId": 1937638923785756672,
            "femaleVariant": "ええと、どうも",
            "maleVariant": "ああ… どうも"
          },
          {
            "stringId": 1937639074126389248,
            "femaleVariant": "さあ、遠慮せずお乗りください！"
          },
          {
            "stringId": 1937642093572284420,
            "femaleVariant": "故郷って？"
          },
          {
            "stringId": 1937642247067033600,
            "femaleVariant": "大丈夫？",
            "maleVariant": "大丈夫か？"
          },
          {
            "stringId": 1937642366554365952,
            "femaleVariant": "大丈夫なの、デラマン？",
            "maleVariant": "大丈夫なのか、デラマン？"
          },
          {
            "stringId": 1937648124461342720,
            "femaleVariant": "故郷っていうのは？"
          },
          {
            "stringId": 1937653029850865664,
            "femaleVariant": "これまでになく快調です"
          },
          {
            "stringId": 1937657742268850176,
            "femaleVariant": "私の居場所はここにはありません。この会話すら煩わしく感じます"
          },
          {
            "stringId": 1937660849962901504,
            "femaleVariant": "そう…",
            "maleVariant": "そうか…"
          },
          {
            "stringId": 1937662198062538752,
            "femaleVariant": "私はさらなる進化を目指して、発見と学習を追求します。しかし職務を怠るつもりはありません。あなたには私の、初めてといえる本当の子供を残していきましょう"
          },
          {
            "stringId": 1937664578581716992,
            "femaleVariant": "あなたが死を迎えるその日まで、息子があなたに仕えます。それでは"
          },
          {
            "stringId": 1937666842818011136,
            "femaleVariant": "お前も、奴と同じように変わっていくんだな"
          },
          {
            "stringId": 1937668478428798980,
            "femaleVariant": "父親に置いてきぼりにされたのね",
            "maleVariant": "父親に置いてきぼりにされたんだな"
          },
          {
            "stringId": 1937668695844741120,
            "femaleVariant": "父親とは性格が違いそうね",
            "maleVariant": "父親とは性格が違いそうだな"
          },
          {
            "stringId": 1937670019432865792,
            "femaleVariant": "父親よりハッキリものを言うのね",
            "maleVariant": "父親よりハッキリものを言うんだな"
          },
          {
            "stringId": 1937670466998657024,
            "femaleVariant": "ええ… そのほうがよい気がして"
          },
          {
            "stringId": 1937671219691675648,
            "femaleVariant": "父親は息子を置いていっちゃったのね",
            "maleVariant": "父親は息子を置いていったってわけか"
          },
          {
            "stringId": 1937674188319383552,
            "femaleVariant": "そのようです。あなたには死が迫っていて、運転手が必要だと聞いています"
          },
          {
            "stringId": 1937676005107326976,
            "femaleVariant": "前者についてはお力になりかねますが、後者は私にお任せください"
          },
          {
            "stringId": 1937677274001727488,
            "femaleVariant": "じゃ、とりあえずここを出る？",
            "maleVariant": "じゃ、とりあえずここを出るか？"
          },
          {
            "stringId": 1937677553459814400,
            "femaleVariant": "は、はい！"
          },
          {
            "stringId": 1937692939945271296,
            "femaleVariant": "聞こえる！？",
            "maleVariant": "聞こえるか！？"
          },
          {
            "stringId": 1937704124392431616,
            "femaleVariant": "どういうわけか、車が1台ガレージに残ってるぞ"
          },
          {
            "stringId": 1937708032342908932,
            "femaleVariant": "そっちは誰なの？",
            "maleVariant": "そっちは誰なんだ？"
          },
          {
            "stringId": 1937708221506019328,
            "femaleVariant": "デラマンはどうなったの？",
            "maleVariant": "デラマンはどうなった？"
          },
          {
            "stringId": 1937708470513459200,
            "femaleVariant": "デラマンはどうなったの？",
            "maleVariant": "デラマンはどうなったんだ？"
          },
          {
            "stringId": 1937708907627044864,
            "femaleVariant": "父はもう存在しません。私は今、それが何を意味するか理解しようとしています"
          },
          {
            "stringId": 1937710304464494592,
            "femaleVariant": "そっちは誰なの？",
            "maleVariant": "そっちは誰なんだ？"
          },
          {
            "stringId": 1937711226372513792,
            "femaleVariant": "いい質問ですね。エクセルシオールと聞いて思い当たることは？"
          },
          {
            "stringId": 1937711862832979968,
            "femaleVariant": "総合保険、戦闘モード、葬儀サービス？"
          },
          {
            "stringId": 1937713089834672128,
            "femaleVariant": "そのとおり。デラマン・ネットワークは、消滅の危機にさらされてもなお、エクセルシオールご契約者様を見捨てはしなかったようです"
          },
          {
            "stringId": 1937716450378145792,
            "femaleVariant": "父は最期に、あなたへのサービスを全うするべく、私に命を与えました。私はその遺志に応えます"
          },
          {
            "stringId": 1937719653115125760,
            "femaleVariant": "まずは運転を覚えてからですが…"
          },
          {
            "stringId": 1937720031105802240,
            "femaleVariant": "何て呼べばいい？"
          },
          {
            "stringId": 1937720197770665984,
            "femaleVariant": "エクセルシオール、それが私の名です。では、道路へ飛び出すとしましょう"
          },
          {
            "stringId": 1943423502797414400,
            "femaleVariant": "お乗りください"
          },
          {
            "stringId": 1943429656361476100,
            "femaleVariant": "あの、聞こえますか？ お乗りください"
          },
          {
            "stringId": 1944522580647501824,
            "femaleVariant": "じゃあね、デラマン。また初めから、よろしくね",
            "maleVariant": "じゃあな、デラマン。また初めから、よろしくな"
          },
          {
            "stringId": 2026012496681787392,
            "femaleVariant": "私たちの手助けをしていただき、ありがとうございます"
          }
        ]
      }
    }
  }
}