            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "みんな、我先に逃げ出すのね",
                    "maleVariant": null,
                    "stringId": 2179412543517519872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "母さん、よして。大げさよ",
                    "maleVariant": null,
                    "stringId": 2189631659328651264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "事実でしょ。街から人が消えていってる。特に若い人たちがね",
                    "maleVariant": null,
                    "stringId": 2189631853424263168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここにいても、未来がないからよ。街に行けば仕事があるし、まともな暮らしができるかもしれない",
                    "maleVariant": null,
                    "stringId": 2189631944692318208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待ってて。私がヘイウッドで稼ぐから。お金が貯まったら母さんも一緒に住もう。きっとその方がいいって",
                    "maleVariant": null,
                    "stringId": 2189632076460572672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人もその根から切り離されると、枯れてしまう。私の故郷はここだけよ",
                    "maleVariant": null,
                    "stringId": 2189632295520681984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも… はあ、もういい。また今度話そう",
                    "maleVariant": null,
                    "stringId": 2189632479130533888
                  }
                }
              ]
            }
          }
        }
      }
