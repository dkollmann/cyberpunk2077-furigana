          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんにちは、アリフ・イクバルがWNSニュースをお届けします",
              "maleVariant": "",
              "stringId": "1792240161548836864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サイバーサイコシス発症者による、ライン市長襲撃というショッキングな事件の続報です",
              "maleVariant": "",
              "stringId": "1796858381269188608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "市長は記者会見の最中に襲われましたが、犯人はNCPDのリバー・ウォード刑事によって実行直前に取り押さえられました",
              "maleVariant": "",
              "stringId": "1796859723714904064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“スーパーコップ”として称賛されたウォード刑事は、市長より勲章を授与されました",
              "maleVariant": "",
              "stringId": "1796860063570968576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あわや惨劇という恐ろしい事件でしたが、次の市長選で再選を狙いたい市長には、思わぬ幸運をもたらすかもしれません",
              "maleVariant": "",
              "stringId": "1796865065446260736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "襲撃後の世論調査では、現市長はライバルのジェファーソン・ペラレス氏に対するリードを12パーセントから19パーセントにまで広げました",
              "maleVariant": "",
              "stringId": "1796865310376837120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CMの後もローカルニュースです",
              "maleVariant": "",
              "stringId": "1796865549821263872"
            }
          ]
        }
      }
