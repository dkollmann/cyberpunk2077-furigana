          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんな… なんて罪深いことを！",
              "maleVariant": "",
              "stringId": "1831681692086689792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おいおい、ただのインターフェースプラグじゃないか",
              "maleVariant": "",
              "stringId": "1831683317849251840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなこと言うなんて信じられない… 出会った頃はそんな人じゃなかったのに",
              "maleVariant": "",
              "stringId": "1831683486074396672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その頃は二人きりで、子供がいなかったからな",
              "maleVariant": "",
              "stringId": "1831683633881669632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私たちは神の創造物なのよ！ その御業に手を加えるなんて、畏れ多いわ！",
              "maleVariant": "",
              "stringId": "1831683771287068672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちの息子に、より楽で、恵まれた人生を送らせたいだけだ",
              "maleVariant": "",
              "stringId": "1831683920419741696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "楽？ あの子にとって？ それともあなた？ あなた、友達のマイケルやピーターの真似がしたいだけでしょ",
              "maleVariant": "",
              "stringId": "1831684098744770560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やめないか",
              "maleVariant": "",
              "stringId": "1831684277220794368"
            }
          ]
        }
