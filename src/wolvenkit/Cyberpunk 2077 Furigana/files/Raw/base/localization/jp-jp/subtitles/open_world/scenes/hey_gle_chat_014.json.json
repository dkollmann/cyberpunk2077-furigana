            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はあ、チューマ… 俺ももうジジイだな",
                    "maleVariant": null,
                    "stringId": 2161735494471331840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そう卑屈になるなよ。痛みを感じるなら、まだ元気な証拠だ",
                    "maleVariant": null,
                    "stringId": 2176554386006458368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そういうことじゃない。若い奴らのことさ。昔のティーノズには規律や礼儀ってもんがあった",
                    "maleVariant": null,
                    "stringId": 2176554719420071936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サンタ・ムエルテを敬い、その加護を受けていた。家族や仲間を大切にしてた",
                    "maleVariant": null,
                    "stringId": 2176555158060384256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今は変わっちまったって？ ったく、それこそジジイのたわごとだぜ",
                    "maleVariant": null,
                    "stringId": 2176555328231686144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だが事実だ。最近の奴らときたら、金とサプリのことしか考えてない",
                    "maleVariant": null,
                    "stringId": 2176555598009319424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺たちは、こうやって少しずつ… 大企業に染まってくんだ",
                    "maleVariant": null,
                    "stringId": 2176555929023791104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なに言ってんだよ",
                    "maleVariant": null,
                    "stringId": 2176556126021861376
                  }
                }
              ]
            }
          }
        }
      }
    },
