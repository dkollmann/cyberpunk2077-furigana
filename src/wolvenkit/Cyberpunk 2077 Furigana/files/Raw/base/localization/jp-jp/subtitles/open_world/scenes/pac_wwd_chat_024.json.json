    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本当だって。俺も、お前も、実験の道具にされてるんだ。パシフィカは連中のサンドボックスなのさ",
                    "maleVariant": null,
                    "stringId": 2179626301892714496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バカなこと言うのもほどほどにな",
                    "maleVariant": null,
                    "stringId": 2211501515057852416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺は真剣だ！ パシフィカといえば海と砂… サンドボックス！ 実験場だ！",
                    "maleVariant": null,
                    "stringId": 2212502627542036480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なぜコーポの連中がここをさっさと解体しないと思う？ 昔のノース・オークみたいにさ",
                    "maleVariant": null,
                    "stringId": 2212503036033691648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何の得もないからだ",
                    "maleVariant": null,
                    "stringId": 2212516406820532224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "違う！ 隠れて何かやってるからだ！ 放射線とかいろいろ！ おかしな言葉で話すハイチ人は、みんな連中のスパイだ！",
                    "maleVariant": null,
                    "stringId": 2212516566287970304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はいはい。じゃあその話をヴードゥー・ボーイズにしてこいよ。そうすりゃ俺はお前のくだらん話を聞かずに済む",
                    "maleVariant": null,
                    "stringId": 2212517614561980416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちゃんと聞けって。パシフィカは奴らが用意した檻で、俺たちはネズミなんだ！",
                    "maleVariant": null,
                    "stringId": 2212518150661140480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうか、わかった。そしてどうでもいい",
                    "maleVariant": null,
                    "stringId": 2212520598641504256
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
