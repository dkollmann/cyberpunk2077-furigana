    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "車を止めろ！",
              "maleVariant": "",
              "stringId": 1559111705497067520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よせ！ やめろ！ 撃つな！",
              "maleVariant": "",
              "stringId": 1560251716485406720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "向こうは悪くない！ 彼女を撃ったら取引はなしだ！",
              "maleVariant": "向こうは悪くない！ 彼を撃ったら取引はなしだ！",
              "stringId": 1560264606554677248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一緒に来てくれ。知りたいことは途中で教える",
              "maleVariant": "",
              "stringId": 1560285676439494656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残念だが、君に意見する権利はない",
              "maleVariant": "",
              "stringId": 1560286823581638656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいねえ。こいつらが何をするつもりか知らねえが、行けよ。こんな面白そうな機会、滅多にないぞ",
              "maleVariant": "",
              "stringId": 1560288335946997760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっとか。ようやく面白いもんが見れそうだ！ ポップコーンでもあれば暇つぶしに最高だぜ…",
              "maleVariant": "",
              "stringId": 1563169581303750656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レイチェルの言うとおり… ここに長居はできない",
              "maleVariant": "",
              "stringId": 1563172895642771456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前を殺しに来たんだぞ！",
              "maleVariant": "",
              "stringId": 1755039294645886976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女を連れて行く",
              "maleVariant": "彼を連れて行く",
              "stringId": 1755041991331700736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "遠慮しとく。じゃあさよなら",
              "maleVariant": "遠慮しとく。俺はもう行くよ",
              "stringId": 1755045418463944704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時間の無駄よ！",
              "maleVariant": "",
              "stringId": 1755045722852974592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼む、君が必要なんだ！",
              "maleVariant": "",
              "stringId": 1755045913458925568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いえ、お断りよ",
              "maleVariant": "いや、お断りだ",
              "stringId": 1755048913460027392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてそんなにあたしが必要なの？",
              "maleVariant": "どうしてそんなに俺が必要なんだ？",
              "stringId": 1755049082003939328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ、もういい？",
              "maleVariant": "",
              "stringId": 1755050212234649600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしが…？ あんたたちと？",
              "maleVariant": "俺が…？ あんたたちと？",
              "stringId": 1755052034408079360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "君が必要だ",
              "maleVariant": "",
              "stringId": 1755052977908375552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰も君には指一本触れないと約束する。ほんの短い間… 来てもらうだけだ",
              "maleVariant": "",
              "stringId": 1755053834771128320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一体どういうこと？",
              "maleVariant": "一体どういうことだ？",
              "stringId": 1755055933751844864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vよ",
              "maleVariant": "Vだ",
              "stringId": 1755087536876183552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨシュア・スティーブンソンだ。もう知ってるよな…",
              "maleVariant": "",
              "stringId": 1755087879114612736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一緒にいた男… 彼の名は？",
              "maleVariant": "",
              "stringId": 1755089781483134976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言いたくなければいい。私はヨシュア・スティーブンソンだ",
              "maleVariant": "",
              "stringId": 1755096294062841856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一緒にいた男… 彼の名は？",
              "maleVariant": "",
              "stringId": 1755097256101961728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ビル・ジャブロンスキー、知ってるでしょ？",
              "maleVariant": "ビル・ジャブロンスキー、知ってるだろ？",
              "stringId": 1755101055252193280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、知ってる",
              "maleVariant": "",
              "stringId": 1755101818967842816
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈我らに日の数え方を教え、知恵の心を得させたまえ！〉",
              "maleVariant": "",
              "stringId": 1755101954242535424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "特定済みよ。ビル・ジャブロンスキー",
              "maleVariant": "",
              "stringId": 1755102971864571904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待てよ、お前Vだろ？ 知ってるぞ！",
              "maleVariant": "",
              "stringId": 1755105826189107200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女に頼みたいことがある",
              "maleVariant": "彼に頼みたいことがある",
              "stringId": 1755106202200072192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "勘弁してくれよ…",
              "maleVariant": "",
              "stringId": 1755106404382302208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は絶対反対だ",
              "maleVariant": "",
              "stringId": 1772116061515124736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずい！ ブレーキだ！ ブレーキ！",
              "maleVariant": "",
              "stringId": 1785261803226238976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "離れるなよ、今からあいつをバラバラにしてやる！",
              "maleVariant": "",
              "stringId": 1785275500145381376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここまでだ、行くぞ！",
              "maleVariant": "",
              "stringId": 1790938824107331584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まず、どういうことなのか説明して",
              "maleVariant": "まず、どういうことなのか説明しろ",
              "stringId": 1801135282559356928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、いったい何してんだ！？",
              "maleVariant": "",
              "stringId": 1802552528787705856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "NCPDだ！ いいからさっさとそのトラックを動かせ！ 早く！",
              "maleVariant": "",
              "stringId": 1802555133047836672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは巻き添えになる覚悟があって言ってるのか？",
              "maleVariant": "",
              "stringId": 1802612284567031808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあどうするのよ、クソ！ ここには置いていけないでしょ！ メディアが来ちゃう！",
              "maleVariant": "",
              "stringId": 1802613003755950080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こちらバスケス、ヨシュアの暗殺を阻止",
              "maleVariant": "",
              "stringId": 1802618336310169600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バスケス！ 撃って！",
              "maleVariant": "",
              "stringId": 1804412503094484992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ！？",
              "maleVariant": "",
              "stringId": 1804425734043230208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭おかしいんじゃないの？",
              "maleVariant": "",
              "stringId": 1804426452644945920
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それ以上何か言えば会社に連絡する。キャリアが台無しになるぞ",
              "maleVariant": "",
              "stringId": 1804427281019985920
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "君の名前は？",
              "maleVariant": "",
              "stringId": 1804441986451136512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつは連れてけない。論外だ。この女は危険すぎる",
              "maleVariant": "こいつは連れてけない。論外だ。この男は危険すぎる",
              "stringId": 1804474842179198976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしを巻き込まないで",
              "maleVariant": "俺を巻き込まないでくれ",
              "stringId": 1804477268030414848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… 残念だな",
              "maleVariant": "",
              "stringId": 1804477986900566016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、行ってくれ。誰も止めない",
              "maleVariant": "",
              "stringId": 1804478108736708608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしに何をさせるつもりなの？",
              "maleVariant": "俺に何をさせるつもりなんだ？",
              "stringId": 1804479647425519616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちは知らないけど",
              "maleVariant": "こっちは知らないがな",
              "stringId": 1804480122556276736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。一緒に行く",
              "maleVariant": "",
              "stringId": 1804487988772102144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "関わる気はない",
              "maleVariant": "",
              "stringId": 1804489174870941696
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早くしてよ。押してるんだから",
              "maleVariant": "",
              "stringId": 1804491309335470080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、1名死亡",
              "maleVariant": "",
              "stringId": 1804505357384966144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "説明する… だから、頼む…！ 一緒に居てくれ！",
              "maleVariant": "",
              "stringId": 1812994947364184064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんな手に乗ると思ったら大間違いよ",
              "maleVariant": "そんな手に乗ると思ったら大間違いだ",
              "stringId": 1827265802063601664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "君じゃなきゃダメなんだ。来てくれ… 頼むよ…",
              "maleVariant": "",
              "stringId": 1827266797254500352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "諦めて。もう行きましょ",
              "maleVariant": "",
              "stringId": 1827266955933409280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "絶対に来てもらう。これは運命だ",
              "maleVariant": "",
              "stringId": 1827267238830825472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… 残念だ…",
              "maleVariant": "",
              "stringId": 1827277033335971840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "NCPDのバスケス警部補だ！ 動くな！",
              "maleVariant": "",
              "stringId": 1902535604594249728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人殺しを守る気か？",
              "maleVariant": "",
              "stringId": 1902545920149393408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待て！",
              "maleVariant": "",
              "stringId": 1902566478379454468
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "武器を捨てろ！ 撃つぞ！",
              "maleVariant": "",
              "stringId": 1938886351573192704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしを連れてくですって？",
              "maleVariant": "俺を連れてくだって？",
              "stringId": 1959291298781028352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "関わる気はない",
              "maleVariant": "",
              "stringId": 1959291298781028368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしに何をさせる気？",
              "maleVariant": "俺に何をさせる気だ？",
              "stringId": 1959291298864914432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お断りよ",
              "maleVariant": "お断りだ",
              "stringId": 1959291298864914448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてあたしなの？",
              "maleVariant": "どうして俺なんだ？",
              "stringId": 1959291299083018240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたとはどこにも行かない",
              "maleVariant": "",
              "stringId": 1959291299083018256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "事態が飲み込めない",
              "maleVariant": "",
              "stringId": 1959291299200458752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭がどうかしてる",
              "maleVariant": "",
              "stringId": 1959291299200458768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、やめとく",
              "maleVariant": "",
              "stringId": 1959291299200458784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vよ",
              "maleVariant": "Vだ",
              "stringId": 1959291299284344832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうことか説明して",
              "maleVariant": "どういうことか説明してくれ",
              "stringId": 1959291299284344864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ビル・ジャブロンスキー",
              "maleVariant": "",
              "stringId": 1959291299385008128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[乗る] わかった、行く",
              "maleVariant": "",
              "stringId": 1959291300928512000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "NCPDだ！ そこを動くな！",
              "maleVariant": "",
              "stringId": 1960500864785014784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このバカ野郎が！",
              "maleVariant": "",
              "stringId": 1996611625131782144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[降りる]",
              "maleVariant": "",
              "stringId": 2086613906903871492
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ビル・ジャブロンスキー。前科はないが、念のためよく調べろ",
              "maleVariant": "",
              "stringId": 2218695280691605504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ… なんでだ… なんでこんなことに…",
              "maleVariant": "",
              "stringId": 2218695432105979904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんで武器を捨てなかったんだ…",
              "maleVariant": "",
              "stringId": 2218716369232687104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "警告を無視したあいつが悪い… そうだ…",
              "maleVariant": "",
              "stringId": 2218716736485945344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もういいだろう。後は管轄の連中に任せればいい",
              "maleVariant": "",
              "stringId": 2218727630821486592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やってやる… この手であいつを殺してやる！",
              "maleVariant": "",
              "stringId": 2218728539861381120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オルガ？ まずいことになった…",
              "maleVariant": "",
              "stringId": 2218738687191379968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰かが主役を殺そうとしたのよ",
              "maleVariant": "",
              "stringId": 2218740669838880768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "相手はプロを雇ったの！ こうなることは予想できたでしょ",
              "maleVariant": "",
              "stringId": 2218741137453445120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭がどうかしてるからよ！",
              "maleVariant": "",
              "stringId": 2218741241606402048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何なの！",
              "maleVariant": "",
              "stringId": 2218741738211995648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すまないな。パトカーも、無線も使うなよ。後で面倒なことになると困る",
              "maleVariant": "",
              "stringId": 2219983097342291968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "降りろ。早く行け",
              "maleVariant": "",
              "stringId": 2221547689587601408
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}