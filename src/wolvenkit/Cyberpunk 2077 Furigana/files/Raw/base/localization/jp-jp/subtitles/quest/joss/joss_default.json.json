  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一人で大変でしょう",
                    "maleVariant": "一人で大変だろ",
                    "stringId": 1921436089602510848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リバーとは仲直りした？",
                    "maleVariant": "リバーとは仲直りしたか？",
                    "stringId": 1921436089602510864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "子供3人を1人で育てるなんて大変でしょ",
                    "maleVariant": "子供3人を一人で育てるなんて大変だろ",
                    "stringId": 1921436089619288068
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "夫がいた時だって大差なかった",
                    "maleVariant": null,
                    "stringId": 1921436089619288084
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いない日のほうが平和だったくらい",
                    "maleVariant": null,
                    "stringId": 1921436089619288100
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そばにいてほしくなくて、しばらく消えてほしいと思ったこともあったのよ",
                    "maleVariant": null,
                    "stringId": 1921436089619288116
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リバーとの関係は問題ない？",
                    "maleVariant": "リバーとの関係は問題ないか？",
                    "stringId": 1921436089652842500
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ。今さらだけど、やっとお互い理解し合えるようになった",
                    "maleVariant": null,
                    "stringId": 1921436089652842516
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よかった。それならこっちも安心だから…",
                    "maleVariant": "よかった。それならこっちも安心だよ…",
                    "stringId": 1921436089652842532
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ランディには明るい未来が待ってるといいね",
                    "maleVariant": "ランディには明るい未来が待ってるといいな",
                    "stringId": 1921436089669619716
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だといいんだけど… 旦那が亡くなって、いろいろ変わったから",
                    "maleVariant": null,
                    "stringId": 1921436089669619732
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "父親を失うのはつらい",
                    "maleVariant": "父親を失うのはつらいな",
                    "stringId": 1921436089686396932
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でもあの殺人鬼の元から生きて帰れたのなら… あとはよくなる一方なはず",
                    "maleVariant": null,
                    "stringId": 1921436089686396948
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "間違いない",
                    "maleVariant": null,
                    "stringId": 1921436089686396964
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなにひどい旦那だったの？",
                    "maleVariant": "そんなにひどい旦那だったのか？",
                    "stringId": 1937433101388091396
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなにひどかったの？",
                    "maleVariant": "そんなにひどかったのか？",
                    "stringId": 1937433238692827136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ、旦那がいなくなって良かったんじゃ？",
                    "maleVariant": null,
                    "stringId": 1937433257734967296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "旦那さんに何が？",
                    "maleVariant": null,
                    "stringId": 1937433275300712448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "えっと、話題を変えましょ",
                    "maleVariant": "えっと、話題を変えよう",
                    "stringId": 1937433292514136064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ旦那が亡くなってせいせいしたと？",
                    "maleVariant": null,
                    "stringId": 1937433429902757888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうして亡くなったの？",
                    "maleVariant": "どうして亡くなったんだ？",
                    "stringId": 1937433466930073600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "話題を変えましょ",
                    "maleVariant": "話題を変えよう",
                    "stringId": 1937433499209437184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これでランディは安全ね",
                    "maleVariant": "これでランディは安全だな",
                    "stringId": 1937457110540488704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バイクで高速を走るのが生きがいの男だった。でも家じゃ… ひとつ話をしてあげる",
                    "maleVariant": null,
                    "stringId": 1937487895792324608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ある時、彼はドリアンを街に連れてったの。何か用事があって、二人でね",
                    "maleVariant": null,
                    "stringId": 1937488756446728192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "夜に戻った彼は酔っ払って、よくも警察を呼んだなって怒鳴り散らしたの",
                    "maleVariant": null,
                    "stringId": 1937489534070689792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "数週間、泣き続けたわ",
                    "maleVariant": null,
                    "stringId": 1937496987298566144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "夫が、子供たちの父親が殺されたんだから",
                    "maleVariant": null,
                    "stringId": 1937498132075122688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ごめん…",
                    "maleVariant": "すまない…",
                    "stringId": 1937498589472362496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ランディはそれから引きこもるようになった。もちろん下の子たちもいる",
                    "maleVariant": null,
                    "stringId": 1937498863393968128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼がいてもいなくても、人生が大変であることに変わりはないわ",
                    "maleVariant": null,
                    "stringId": 1937500056757342208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アルデカルドスには、立派な最期を遂げたって言われた。どういう意味か知らないけど",
                    "maleVariant": null,
                    "stringId": 1937504728759013376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "顔のない遺体を見せられた時、それが立派な死にざまになんか見えなかった",
                    "maleVariant": null,
                    "stringId": 1937506112678006784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一時はどうなることかと思ったけど…",
                    "maleVariant": null,
                    "stringId": 1937558700526014464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "終わりよければ全てよし、よ。ランディと… 兄さんを取り戻した",
                    "maleVariant": null,
                    "stringId": 1937559117121064960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあね、ジョス",
                    "maleVariant": "じゃあな、ジョス",
                    "stringId": 1937577636801941504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあね、ジョス",
                    "maleVariant": "じゃあな、ジョス",
                    "stringId": 1937577789340389376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "呼んだの？",
                    "maleVariant": "呼んだのか？",
                    "stringId": 1963542117637820416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リバーがたまり場で2人を見つけて連れ帰ったのよ",
                    "maleVariant": null,
                    "stringId": 1963542326044397568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リバーからご両親のことを聞いた",
                    "maleVariant": null,
                    "stringId": 1963568882162733056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リバーからご両親のことを聞いた…",
                    "maleVariant": null,
                    "stringId": 1963569226095661056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "信頼されてるのね。親のことは話さないのに",
                    "maleVariant": null,
                    "stringId": 1963573508765257728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ご両親のあんな最期を目の当たりにするなんて… 何が心の支えに？",
                    "maleVariant": null,
                    "stringId": 1963576553309868032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "孤児院で少し気持ちの整理ができるようになったけど、一番助けになったのはお互いの存在ね",
                    "maleVariant": null,
                    "stringId": 1963577292597891072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも、やがて兄は強迫観念に駆られるようになった。あらゆる殺人犯を自分で捕まえようとするくらいにね",
                    "maleVariant": null,
                    "stringId": 1963581037087952896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいことじゃない。いまどきそんな警官はいないよ",
                    "maleVariant": "いいことじゃないか。いまどきそんな警官はいない",
                    "stringId": 1963585992322584576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "かもね。でも私は今の兄さんがいい。やっと自分らしい生き方を受け入れたみたい",
                    "maleVariant": null,
                    "stringId": 1963586493525135360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョス、何て言ったらいいか…",
                    "maleVariant": null,
                    "stringId": 2005702499139457024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その… 何て言ったらいいか…",
                    "maleVariant": null,
                    "stringId": 2005702799820722176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よくも… よくも2人を見捨てたわね… 約束したのに…",
                    "maleVariant": null,
                    "stringId": 2005704389596155904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョス、あたしは…",
                    "maleVariant": "ジョス、俺は…",
                    "stringId": 2005704971295789056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "兄さんとあの子は死んだ。死んだのよ！ あんたのせいで！",
                    "maleVariant": null,
                    "stringId": 2005705450302083072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう二度と顔を見せないで… 二度とね",
                    "maleVariant": null,
                    "stringId": 2005705631898669056
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}