  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ニュース見ました？ ムーア・テクノロジーのCTOロイ・レバックが、リムジンでスクールバスに突っ込んで、犠牲者がでました",
                    "maleVariant": null,
                    "stringId": 1793522033008484352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "レバックはその場で逮捕、拘束され、裁判を待ってるところ。最長で40年の懲役となります",
                    "maleVariant": null,
                    "stringId": 1811026189687177216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ウソよ。だって彼、大金持ちだもの",
                    "maleVariant": null,
                    "stringId": 1811026636078563328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ロイは、全くおとがめなし。その日の夕方には市庁舎で、被害に遭った子たちのための募金イベントに参加していました",
                    "maleVariant": null,
                    "stringId": 1811027331242508288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "てのもウッソ。真に受けちゃって。私の番組は初めて？",
                    "maleVariant": null,
                    "stringId": 1811027732268302336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ガキのことなんて気にするわけないじゃない。募金はゴンザレス市会議員が働きかけたみたい",
                    "maleVariant": null,
                    "stringId": 1811028451406888960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも、でもですよ！ ロイはちゃんと、遺族にお悔やみの言葉を送ったんです",
                    "maleVariant": null,
                    "stringId": 1811028599415488512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あー、さすがに今度は騙されないかー",
                    "maleVariant": null,
                    "stringId": 1811030428685680640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなわけないですよね。何ならレバックは、公の場で事故の話をした者は必ず訴えてやると――",
                    "maleVariant": null,
                    "stringId": 1811030660731355136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あ、ヤバ",
                    "maleVariant": null,
                    "stringId": 1811030879942459392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ、ロイ・レバック… 裁判所で会いましょうか",
                    "maleVariant": null,
                    "stringId": 1811031130191413248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私だって、相当なお金持ちよ。あんたみたいなクズ、私が吠え面かかせてやるから",
                    "maleVariant": null,
                    "stringId": 1811031409934712832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほら、また引っかかった",
                    "maleVariant": null,
                    "stringId": 1811031850571513856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私にお金があったとしても、あの億万長者に敵うわけないでしょ？",
                    "maleVariant": null,
                    "stringId": 1811032155094761472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "訴状が送られてきたら、頭を丸めて謝るしかありません。もしくは消されるかです",
                    "maleVariant": null,
                    "stringId": 1811032276041711616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティに… ヒーローはいないから。今日はここまでね。おやすみ",
                    "maleVariant": null,
                    "stringId": 1811032577326956544
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}