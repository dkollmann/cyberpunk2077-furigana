    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1706920521373437952,
            "femaleVariant": "おはようございます"
          },
          {
            "stringId": 1706921423366901760,
            "femaleVariant": "おはよう",
            "maleVariant": "ああ、おはよう"
          },
          {
            "stringId": 1706923119711211524,
            "femaleVariant": "[乗る] アフターライフへ"
          },
          {
            "stringId": 1706923469734268928,
            "femaleVariant": "ちょっと待って",
            "maleVariant": "ちょっと待ってくれ"
          },
          {
            "stringId": 1712536438191017984,
            "femaleVariant": "少し待ってもらっていい？",
            "maleVariant": "少し待ってもらっていいか？"
          },
          {
            "stringId": 1730042504923435008,
            "femaleVariant": "これは… お疲れのようですね。昨晩、大変なことでも？"
          },
          {
            "stringId": 1730044571087917056,
            "femaleVariant": "このところ大変でね",
            "maleVariant": "このところ大変でな"
          },
          {
            "stringId": 1730083021631639552,
            "femaleVariant": "よう、ボス"
          },
          {
            "stringId": 1730084364916215808,
            "femaleVariant": "奴はもう来てる？",
            "maleVariant": "奴はもう来てるか？"
          },
          {
            "stringId": 1730087222730412032,
            "femaleVariant": "まだだが、遅れはしないだろう"
          },
          {
            "stringId": 1730088040233816064,
            "femaleVariant": "機材の方は？"
          },
          {
            "stringId": 1730088935231180800,
            "femaleVariant": "到着です。このままお待ちしていましょうか？"
          },
          {
            "stringId": 1730089517148917760,
            "femaleVariant": "大丈夫、その必要はない",
            "maleVariant": "大丈夫だ、その必要はない"
          },
          {
            "stringId": 1795186050455957504,
            "femaleVariant": "では、V様のご成功をお祈りしております"
          },
          {
            "stringId": 1795191990295064576,
            "femaleVariant": "武器は準備万端。他も問題ない。いつでも実行できる"
          },
          {
            "stringId": 1795196387133616128,
            "femaleVariant": "アフターライフへ向かってくれる？",
            "maleVariant": "アフターライフへ向かってくれ"
          },
          {
            "stringId": 1795200313539031040,
            "femaleVariant": "ご出発されますか？"
          },
          {
            "stringId": 1795216023573098496,
            "femaleVariant": "何かお役に立てることはございますか？"
          },
          {
            "stringId": 1795216832503345156,
            "femaleVariant": "街の様子は？ アラサカに関して何かある？",
            "maleVariant": "街の様子は？ アラサカに関して何かあるか？"
          },
          {
            "stringId": 1795232853721214976,
            "femaleVariant": "もう手遅れよ",
            "maleVariant": "もう手遅れだ"
          },
          {
            "stringId": 1795232930124656640,
            "femaleVariant": "いいからアフターライフへ。話をする気分じゃない"
          },
          {
            "stringId": 1795238055899688960,
            "femaleVariant": "街の様子を教えて",
            "maleVariant": "街の様子はどうだ"
          },
          {
            "stringId": 1795240682121195520,
            "femaleVariant": "ストリートは？"
          },
          {
            "stringId": 1795240792330727424,
            "femaleVariant": "V様が大きな仕事に取り掛かろうとしているとの噂が広まっております。そのバックには大物がいるとも"
          },
          {
            "stringId": 1795241838121709568,
            "femaleVariant": "ならよかった"
          },
          {
            "stringId": 1795284810141921280,
            "femaleVariant": "正直言って、もう手遅れね。手の施しようがない",
            "maleVariant": "正直言って、もう手遅れだ。手の施しようがない"
          },
          {
            "stringId": 1795285540722569216,
            "femaleVariant": "〈神輿〉襲撃を成功され、今やアフターライフの頂点に立つV様の潤沢な資産状況を鑑みれば、きっと何らかの解決策が…"
          },
          {
            "stringId": 1795286626292330496,
            "femaleVariant": "大丈夫よ、デル",
            "maleVariant": "大丈夫だ、デル"
          },
          {
            "stringId": 1795291220766486528,
            "femaleVariant": "そうおっしゃるなら"
          },
          {
            "stringId": 1795291470075916288,
            "femaleVariant": "いいから、アフターライフに向かって。おしゃべりはなしよ",
            "maleVariant": "いいから、アフターライフに向かってくれ。おしゃべりはなしだ"
          },
          {
            "stringId": 1795292030300712960,
            "femaleVariant": "もちろん、最短ルートを選択しております。すぐに到着いたします"
          },
          {
            "stringId": 1795292484594167808,
            "femaleVariant": "口を閉じる前にもうひとつだけ。V様、大丈夫ですよ"
          },
          {
            "stringId": 1795294027108184064,
            "femaleVariant": "ひとつよろしいですか？"
          },
          {
            "stringId": 1795296022623809536,
            "femaleVariant": "何？",
            "maleVariant": "何だ？"
          },
          {
            "stringId": 1795296175497801728,
            "femaleVariant": "以前、「頂点からナイトシティを見下ろせればそれでいい」とおっしゃっていました"
          },
          {
            "stringId": 1795300277376118784,
            "femaleVariant": "あんたって比喩も学習してるの？ 窓から落ちないように気をつけろとか言うんじゃないでしょうね",
            "maleVariant": "お前って比喩も学習してるのか？ 窓から落ちないように気をつけろとか言うんじゃないだろうな"
          },
          {
            "stringId": 1795301146838560768,
            "femaleVariant": "実に愉快な皮肉です"
          },
          {
            "stringId": 1795302229975625728,
            "femaleVariant": "私がお伺いしたいのは――目標を果たされた後のご予定です"
          },
          {
            "stringId": 1795303012230098948,
            "femaleVariant": "満足して終わりよ",
            "maleVariant": "満足して終わりだ"
          },
          {
            "stringId": 1795304267014860800,
            "femaleVariant": "あたしは死ぬ",
            "maleVariant": "俺は死ぬ"
          },
          {
            "stringId": 1795304364188495872,
            "femaleVariant": "その時になったら聞いて",
            "maleVariant": "その時になったら聞いてくれ"
          },
          {
            "stringId": 1795306057580015616,
            "femaleVariant": "満足して終わりよ",
            "maleVariant": "満足して終わりだ"
          },
          {
            "stringId": 1795306685635096576,
            "femaleVariant": "本当に？"
          },
          {
            "stringId": 1795306762709626880,
            "femaleVariant": "じゃないと困る"
          },
          {
            "stringId": 1795307635275853824,
            "femaleVariant": "あとは死ぬだけ",
            "maleVariant": "あとは死ぬだけだ"
          },
          {
            "stringId": 1795312689898713088,
            "femaleVariant": "おっしゃる意味はわかりますが、どうにも想像がつきかねます"
          },
          {
            "stringId": 1795313464603439104,
            "femaleVariant": "誰にもどうにもできない。でもそれでいいの",
            "maleVariant": "誰にもどうにもできない。だがそれでいいんだ"
          },
          {
            "stringId": 1795313731696717824,
            "femaleVariant": "十分すぎる"
          },
          {
            "stringId": 1795314242109960192,
            "femaleVariant": "全部終わった時に聞いて",
            "maleVariant": "全部終わった時に聞いてくれ"
          },
          {
            "stringId": 1795314486553997312,
            "femaleVariant": "はい。そうさせていただきます"
          },
          {
            "stringId": 1796410891880230912,
            "femaleVariant": "制限航路に沿って航行いたします"
          },
          {
            "stringId": 1796413052517216256,
            "femaleVariant": "渋滞情報はありません"
          },
          {
            "stringId": 1796413975784189952,
            "femaleVariant": "わかった"
          },
          {
            "stringId": 1796414966814339072,
            "femaleVariant": "目的地に到着しました"
          },
          {
            "stringId": 1796415631376642048,
            "femaleVariant": "ご利用ありがとうございました。どうぞ良い一日を"
          },
          {
            "stringId": 1796416780213284864,
            "femaleVariant": "デラマンをご利用いただき、ありがとうございます"
          },
          {
            "stringId": 1796417595636314112,
            "femaleVariant": "本日はどちらへ？"
          },
          {
            "stringId": 2008307402489327632,
            "femaleVariant": "恐れ入りますが、少々お疲れのようですね。昨晩は眠れましたか？"
          },
          {
            "stringId": 2008307402506104840,
            "femaleVariant": "このところ大変でね",
            "maleVariant": "このところ大変でな"
          },
          {
            "stringId": 2008307402506104856,
            "femaleVariant": "それはお気の毒です"
          },
          {
            "stringId": 2008307402556436496,
            "femaleVariant": "いいからアフターライフに向かって",
            "maleVariant": "いいからアフターライフに向かってくれ"
          },
          {
            "stringId": 2008307402573213708,
            "femaleVariant": "“安全な旅”はあたし向けじゃない",
            "maleVariant": "“安全な旅”は俺向けじゃない"
          },
          {
            "stringId": 2008307402724208660,
            "femaleVariant": "せっかくだけど、“安全な旅”はあたしには似合わない",
            "maleVariant": "せっかくだが、“安全な旅”は俺には似合わない"
          },
          {
            "stringId": 2008307402740985864,
            "femaleVariant": "我が社のモットーは、安全で快適なドライブをお約束すること。V様の資産状況を鑑みれば、ご加入いただいて損はないかと"
          },
          {
            "stringId": 2008307402740985884,
            "femaleVariant": "大丈夫よ、デル",
            "maleVariant": "大丈夫だ、デル"
          },
          {
            "stringId": 2008307402757763072,
            "femaleVariant": "承知しました"
          },
          {
            "stringId": 2008307402824871952,
            "femaleVariant": "いいから、急いでアフターライフに向かって",
            "maleVariant": "いいから急いでアフターライフに向かってくれ"
          },
          {
            "stringId": 2008307402824871968,
            "femaleVariant": "かしこまりました"
          },
          {
            "stringId": 2008319234855792640,
            "femaleVariant": "しかしその程度です。アラサカが事態の収拾に躍起になっている今、V様やアフターライフは安全かと… 当面の間は"
          },
          {
            "stringId": 2008321694798311424,
            "femaleVariant": "あなたならこの仕事をやり遂げられます。その、私の知っているV様であれば…"
          },
          {
            "stringId": 2008323158325194752,
            "femaleVariant": "私が言いたいこと、わかっていただけますよね？"
          },
          {
            "stringId": 2008323323379445760,
            "femaleVariant": "ええ、ありがとう、デル",
            "maleVariant": "ああ、ありがとう、デル"
          },
          {
            "stringId": 2008326669460959232,
            "femaleVariant": "私でお力になれることがあれば、どうかお気軽におっしゃってください"
          },
          {
            "stringId": 2008327804691279872,
            "femaleVariant": "でも安心しました"
          },
          {
            "stringId": 2008328295944941568,
            "femaleVariant": "どうして？"
          },
          {
            "stringId": 2008328457777967104,
            "femaleVariant": "そういう風におっしゃるということは、V様は戻られるご予定… ということですよね？"
          },
          {
            "stringId": 2008328842160762880,
            "femaleVariant": "ええ…",
            "maleVariant": "ああ…"
          },
          {
            "stringId": 2008329785979826176,
            "femaleVariant": "部下がさっき、ここをスキャンしに来たんだぜ。奴の到着前に、安全を確認したいんだろうよ"
          },
          {
            "stringId": 2008332457751474176,
            "femaleVariant": "わかった。すぐに着く"
          },
          {
            "stringId": 2008345642260885504,
            "femaleVariant": "最適な航路を設定。すぐに到着いたします"
          },
          {
            "stringId": 2008372946676609024,
            "femaleVariant": "デラマン・ネットワークでは、そんなお客様の体調改善に最適なサービスを、数多く取り揃えさせていただいております"
          },
          {
            "stringId": 2008373588790996992,
            "femaleVariant": "また先ごろ、サービス向上のため、トラウマ・チームとの業務提携も開始いたしました"
          },
          {
            "stringId": 2008375038610894848,
            "femaleVariant": "期間限定の“セーフトラベル”パッケージをお選びいただければ、様々な医療サービスをお気軽にご利用いただけます"
          },
          {
            "stringId": 2008383319341395968,
            "femaleVariant": "ふん。バレバレだっての"
          },
          {
            "stringId": 2008390008518078464,
            "femaleVariant": "その間、どうぞ素晴らしいナイトシティの眺めをご堪能ください"
          },
          {
            "stringId": 2008390581493559300,
            "femaleVariant": "これが最後になるかも"
          },
          {
            "stringId": 2008391107257954304,
            "femaleVariant": "ありがとう"
          },
          {
            "stringId": 2008391878624014336,
            "femaleVariant": "この景色を眺めるのも、これが最後かも",
            "maleVariant": "この景色を眺めるのも、これが最後かもな"
          },
          {
            "stringId": 2008391979773849600,
            "femaleVariant": "ありがとう",
            "maleVariant": "ああ、ありがとう"
          },
          {
            "stringId": 2008393018249965568,
            "femaleVariant": "お客様の言葉をお借りすれば、この街は“息を呑む美しさ”"
          },
          {
            "stringId": 2008393782485708800,
            "femaleVariant": "まったくね",
            "maleVariant": "まったくだ"
          },
          {
            "stringId": 2008395672506200064,
            "femaleVariant": "ナイトシティを離れるご予定でも？"
          },
          {
            "stringId": 2008396263114534912,
            "femaleVariant": "そんなところ",
            "maleVariant": "そんなところだ"
          },
          {
            "stringId": 2008396558678749184,
            "femaleVariant": "では、またのお戻りをお待ちしております"
          },
          {
            "stringId": 2008397776268423168,
            "femaleVariant": "または“心しびれるメトロポリス”とも呼ばれています"
          },
          {
            "stringId": 2008398163872444416,
            "femaleVariant": "よくわかる",
            "maleVariant": "よくわかるよ"
          },
          {
            "stringId": 2008402470399242240,
            "femaleVariant": "“夢のような場所”で――"
          },
          {
            "stringId": 2008402713098448896,
            "femaleVariant": "確かに"
          },
          {
            "stringId": 2008402897496829952,
            "femaleVariant": "“明るい未来が約束された、希望の街”"
          },
          {
            "stringId": 2008405353664475136,
            "femaleVariant": "そうね…",
            "maleVariant": "そうだな…"
          },
          {
            "stringId": 2008405723585310720,
            "femaleVariant": "この街は多くのお客様がおっしゃるとおり、“西海岸の至宝”です"
          },
          {
            "stringId": 2008406071226003456,
            "femaleVariant": "そうね",
            "maleVariant": "そうだな"
          },
          {
            "stringId": 2015641922133684224,
            "femaleVariant": "到着です"
          },
          {
            "stringId": 2015645060580372480,
            "femaleVariant": "それでは良い一日を！"
          },
          {
            "stringId": 2015653006672523272,
            "femaleVariant": "はい、かしこまりました"
          },
          {
            "stringId": 2015653006722854924,
            "femaleVariant": "ただちに向かいます"
          },
          {
            "stringId": 2017213844831203340,
            "femaleVariant": "おはようございます"
          },
          {
            "stringId": 2017213844847980544,
            "femaleVariant": "V様、今日はどちらまで？"
          },
          {
            "stringId": 2017215004505927688,
            "femaleVariant": "お任せを"
          },
          {
            "stringId": 2017216898318069768,
            "femaleVariant": "はい、V様"
          },
          {
            "stringId": 2017216898385178632,
            "femaleVariant": "もちろんです"
          },
          {
            "stringId": 2017216898519396360,
            "femaleVariant": "ええ、もちろん"
          },
          {
            "stringId": 2017221175132418064,
            "femaleVariant": "特別航路を航行いたします。ワクワクしますね！"
          },
          {
            "stringId": 2017221175132418080,
            "femaleVariant": "楽しい旅になりますよ"
          },
          {
            "stringId": 2017221175149195264,
            "femaleVariant": "わかったよ"
          },
          {
            "stringId": 2017221175199526940,
            "femaleVariant": "お疲れですね。二日酔いですか？"
          },
          {
            "stringId": 2017221175216304140,
            "femaleVariant": "このところ大変でね",
            "maleVariant": "このところ大変でな"
          },
          {
            "stringId": 2017221175216304156,
            "femaleVariant": "失礼しました。アラサカ・タワー襲撃後、数多の賞賛とリソースを手に入れられたことと存じますが、体調のご不安については――"
          },
          {
            "stringId": 2017221175233081348,
            "femaleVariant": "よろしければ、追加でスパのサービスなどもご利用いただけますよ。お疲れの体にはよく効くことでしょう"
          },
          {
            "stringId": 2017221175233081376,
            "femaleVariant": "トラウマ・チームと提携して、“セーフトラベル”という医療ケアパッケージの提供も開始いたしました。皆さんにお勧めしているのですが――"
          },
          {
            "stringId": 2017221175300190208,
            "femaleVariant": "いいからアフターライフに向かって",
            "maleVariant": "いいからアフターライフに向かってくれ"
          },
          {
            "stringId": 2017221175300190224,
            "femaleVariant": "“安全な旅”はあたし向けじゃない",
            "maleVariant": "“安全な旅”は俺向けじゃない"
          },
          {
            "stringId": 2017221175350521876,
            "femaleVariant": "いや、“安全な旅”はあたしには似合わない",
            "maleVariant": "せっかくだが、“安全な旅”は俺には似合わない"
          },
          {
            "stringId": 2017221175350521892,
            "femaleVariant": "ですが、デラマンのモットーは安全で快適なドライブをお約束すること。V様の資産状況を鑑みれば、ご加入いただいて損はありませんよ"
          },
          {
            "stringId": 2017221175367299072,
            "femaleVariant": "大丈夫よ",
            "maleVariant": "大丈夫だ"
          },
          {
            "stringId": 2017221175367299088,
            "femaleVariant": "そうおっしゃるなら"
          },
          {
            "stringId": 2017221175417630740,
            "femaleVariant": "いいから、急いでアフターライフに向かって",
            "maleVariant": "いいから、急いでアフターライフに向かってくれ"
          },
          {
            "stringId": 2017221175434407936,
            "femaleVariant": "承知しました"
          },
          {
            "stringId": 2017221175434407952,
            "femaleVariant": "最適な航路を選択いたしました。すぐに到着いたします"
          },
          {
            "stringId": 2017221175434407964,
            "femaleVariant": "おくつろぎになり、素晴らしい街の眺めをご堪能ください"
          },
          {
            "stringId": 2017221175467962368,
            "femaleVariant": "これが最後になるかも"
          },
          {
            "stringId": 2017221175484739596,
            "femaleVariant": "ありがとう"
          },
          {
            "stringId": 2017221175535071248,
            "femaleVariant": "この景色を眺めるのも、これが最後かも",
            "maleVariant": "この景色を眺めるのも、これが最後かもな"
          },
          {
            "stringId": 2017221175535071260,
            "femaleVariant": "どこかへ移られるのですか？"
          },
          {
            "stringId": 2017221175535071276,
            "femaleVariant": "そんなところ",
            "maleVariant": "そんなところだ"
          },
          {
            "stringId": 2017221175535071288,
            "femaleVariant": "では、またお会いするのを楽しみにしています！"
          },
          {
            "stringId": 2017221175551848460,
            "femaleVariant": "多くのお客様がおっしゃるには、ナイトシティは“西海岸の至宝”"
          },
          {
            "stringId": 2017221175551848472,
            "femaleVariant": "そうね",
            "maleVariant": "そうだな"
          },
          {
            "stringId": 2017221175551848484,
            "femaleVariant": "または“心しびれるメトロポリス”とも呼ばれていますね"
          },
          {
            "stringId": 2017221175551848496,
            "femaleVariant": "よくわかる",
            "maleVariant": "よくわかるよ"
          },
          {
            "stringId": 2017221175602180128,
            "femaleVariant": "ありがとう"
          },
          {
