    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "簡単にはいかないだろうね",
                    "maleVariant": "簡単にはいかないだろうな",
                    "stringId": 1074855074073272340
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミリテクの金でもう一度支払う",
                    "maleVariant": null,
                    "stringId": 1074855531504066560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "相手はメイルストローム。簡単にはいかないでしょうね",
                    "maleVariant": "相手はメイルストローム。簡単にはいかないだろうな",
                    "stringId": 1163304933608067072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミリテクと協力することになった。確実にボットを買い取るため金を預かってる",
                    "maleVariant": null,
                    "stringId": 1163304980013846544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ギャングの世界はシンプルだ。力が正義、弱肉強食がすべて――",
                    "maleVariant": null,
                    "stringId": 1163306048202412032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "連中から買い取る",
                    "maleVariant": null,
                    "stringId": 1176760969979973632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あ？ 誰だ、お前ら",
                    "maleVariant": null,
                    "stringId": 1196551541142093836
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "デクスに言われて来た。ロイスは？",
                    "maleVariant": null,
                    "stringId": 1196555736771493892
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "メインルームに来い。待ってたぞ",
                    "maleVariant": null,
                    "stringId": 1196555736771493900
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "デクスに言われて来た",
                    "maleVariant": null,
                    "stringId": 1196557869625409536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ドアを開けて",
                    "maleVariant": "ドアを開けな",
                    "stringId": 1214205208048635904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待ちくたびれたぜ！",
                    "maleVariant": null,
                    "stringId": 1671071062173638656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあいい、いずれにせよ手に入れなきゃ始まんねえ。それで、作戦はあるのか？",
                    "maleVariant": null,
                    "stringId": 1671091734673317888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "勘弁してくれ。バレたとたん、俺らが殺されちまう。連中に教えてやったほうが、いいんじゃねえか",
                    "maleVariant": null,
                    "stringId": 1671098738758344704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ボットは用意した金で買い取る",
                    "maleVariant": null,
                    "stringId": 1671100363480719360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "金はもうデクスが払ったんじゃないのか？",
                    "maleVariant": null,
                    "stringId": 1671101238043435008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うし。じゃあ、ちょいとお邪魔させてもらうとするか",
                    "maleVariant": null,
                    "stringId": 1671101797781692416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しかしよ、なんでよりによってこのサイボーグどもと…",
                    "maleVariant": null,
                    "stringId": 1671130383003639808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ギャングなんてどれも一緒でしょ？",
                    "maleVariant": "ギャングなんてどれも一緒だろ？",
                    "stringId": 1671130383003639816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、行けよ。挨拶しようぜ",
                    "maleVariant": null,
                    "stringId": 1673677367128317952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいからドアを開けて",
                    "maleVariant": "いいからドアを開けな",
                    "stringId": 1711554970904125440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前ら、アポは取ってんのか？ そんな話聞いてねえけどな",
                    "maleVariant": null,
                    "stringId": 1711554970904125456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "メイルストロームの連中と会う",
                    "maleVariant": null,
                    "stringId": 1718753419206549508
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "デクスはもうメイルストロームにボットの代金を支払ってる。問題は、連中がおとなしく商品を渡してくれるかどうかよ",
                    "maleVariant": "デクスはもうメイルストロームにボットの代金を支払ってる。問題は、連中がおとなしく商品を渡してくれるかどうかだ",
                    "stringId": 1718754883521961984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そのバイクってアーチ？",
                    "maleVariant": "そのバイクってアーチか？",
                    "stringId": 1718755993250914304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それってアーチ？ いいバイクね",
                    "maleVariant": "それはアーチか？ いいバイクだ",
                    "stringId": 1718756840030560256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こないだの分け前で買った。かなりお得だったぜ",
                    "maleVariant": null,
                    "stringId": 1718757295431311360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いくらお得と言っても、アーチなら中古でも相当値が張るでしょ",
                    "maleVariant": "いくらお得と言っても、アーチなら中古でも相当値が張るだろ",
                    "stringId": 1718759043214565376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マフラーを付け替えた方がいいんじゃない？",
                    "maleVariant": "マフラーを付け替えた方がいいんじゃないか？",
                    "stringId": 1718760428895166464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マフラーを付け替えた方がいいんじゃない？",
                    "maleVariant": "マフラーを付け替えた方がいいんじゃないか？",
                    "stringId": 1718764508023578624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何でだよ？ ブンブンいわすだけで女が寄ってくんだぜ",
                    "maleVariant": null,
                    "stringId": 1718764508023578632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その「ブンブン」のせいで、あんたが走ってるのが丸わかりでしょ",
                    "maleVariant": "その「ブンブン」のせいで、お前が走ってるのが丸わかりだろ",
                    "stringId": 1718764508023578640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あとはインジェクションもいじりたいとこね。マップの更新も。排気まわりを直すには金がかかるから、ひとまずバンテージを巻けばいい",
                    "maleVariant": "あとはインジェクションもいじりたいとこだな。マップの更新も。排気まわりを直すには金がかかるから、ひとまずバンテージを巻けばいい",
                    "stringId": 1718769445088485376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待たせてごめん",
                    "maleVariant": "待たせてすまん",
                    "stringId": 1732001242542727168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待たせてごめんね",
                    "maleVariant": "待たせてすまない",
                    "stringId": 1732001868248997888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいさ。「短気は損気」だってオフクロも言ってたからな",
                    "maleVariant": null,
                    "stringId": 1732002465803100160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうなったか聞かせてくれ",
                    "maleVariant": null,
                    "stringId": 1754699002255699968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあ… ローンなんだけどな。だがよ、間違いなくそれだけの価値はあるぜ",
                    "maleVariant": null,
                    "stringId": 1754757041625165824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お、おお…",
                    "maleVariant": null,
                    "stringId": 1754768650921766912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう一度払うの。なるべく穏便に済ませたい",
                    "maleVariant": "もう一度払うんだ。なるべく穏便に済ませたい",
                    "stringId": 1754807286971166720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "使っちまうには惜しい大金だな",
                    "maleVariant": null,
                    "stringId": 1754981399408865280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "変なこと考えないで。この金は預かっただけ。とはいえ、取引がどう転ぶかわからないけどね",
                    "maleVariant": "変なこと考えるなよ。この金は預かっただけだ。とはいえ、取引がどう転ぶかわからないけどな",
                    "stringId": 1754981503310163968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それに、このチップは仕掛け付きよ",
                    "maleVariant": "それに、このチップは仕掛け付きだ",
                    "stringId": 1754982063702732800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今から渡す金は罠ですよって？ それこそ怒りを買うだけでしょ",
                    "maleVariant": "今から渡す金は罠ですよってか？ それこそ怒りを買うだけだろ",
                    "stringId": 1754983330265100288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうは言ってもよ… 変にコソコソすると、逆に痛い目を見るかもしれないぜ",
                    "maleVariant": null,
                    "stringId": 1754983570967818240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヴァレンティーノズを見ろ。神と聖母の教えに従い、何よりも敬う心ってもんがある",
                    "maleVariant": null,
                    "stringId": 1755949900726464512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "目的と流儀、何が許せて何が許せないかが、はっきりしてる",
                    "maleVariant": null,
                    "stringId": 1755950171158409216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だがメイルストロームは、まるで得体が知れねえ",
                    "maleVariant": null,
                    "stringId": 1755950405552893952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "前払いしたのか？ バカだな…",
                    "maleVariant": null,
                    "stringId": 1762140551830761472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やるか、やられるか。それしかねえ",
                    "maleVariant": null,
                    "stringId": 1762145704667443200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おい、もう行くのかよぉ？",
                    "maleVariant": null,
                    "stringId": 1777946122915868672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ったく、せっかちな奴だぜ",
                    "maleVariant": null,
                    "stringId": 1777953524587360256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "T-バグの方はどうなってる？",
                    "maleVariant": null,
                    "stringId": 1780625118329761792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "T-バグの方はどうなってる？ 話はしたの？",
                    "maleVariant": "T-バグの方はどうなってる？ 話はしたのか？",
                    "stringId": 1780625474711384064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "デクスのヤマだって伝えたらやる気満々だったぜ。もう準備を始めてる",
                    "maleVariant": null,
                    "stringId": 1780625784183910400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんてったって、バグはデクスが“休業”する前からの仲だからな",
                    "maleVariant": null,
                    "stringId": 1780639719071084544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃ、さっそく虎穴に入るか？",
                    "maleVariant": null,
                    "stringId": 1995370154412666880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "行きましょ",
                    "maleVariant": "行こう",
                    "stringId": 1995370620718608388
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、行きましょ",
                    "maleVariant": "ああ、行くぞ",
                    "stringId": 1995370634173935616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと待って",
                    "maleVariant": "ちょっと待ってくれ",
                    "stringId": 1995370637009285120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと待って",
                    "maleVariant": "ちょっと待ってくれ",
                    "stringId": 1995371702681268224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。準備ができたら言ってくれ",
                    "maleVariant": null,
                    "stringId": 1995379199278247936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうした、チューマ。さっさと片付けようぜ",
                    "maleVariant": null,
                    "stringId": 2240093139929055232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、なにグズグズしてる",
                    "maleVariant": null,
                    "stringId": 2240094617834012676
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}