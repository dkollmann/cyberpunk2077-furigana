        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "罪深き者にはトランクがお似合いだ。グスタボが悔い改めるのにうってつけの苦行だろう",
                    "maleVariant": null,
                    "stringId": 2328600522248187904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "グスタボ・オルタは死んだ。自ら命を絶ってな",
                    "maleVariant": null,
                    "stringId": 2328600728725385216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "グスタボ・オルタの罪を清める機会を得ながら、その場に置いてくるとは",
                    "maleVariant": null,
                    "stringId": 2328600948322365440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "主の御手に代わり、よくぞ裁きを下してくれた。見事だったぞ、V",
                    "maleVariant": null,
                    "stringId": 2334427041593278464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これで奴は報いを受けずに生きることになる。無論、審判の時までだが、それまで野放しでは甘すぎる",
                    "maleVariant": null,
                    "stringId": 2340613223802572800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "忘れるな。主はお前の過ちもすべて覚えている。そして、この私もな",
                    "maleVariant": null,
                    "stringId": 2340621234906435584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうやって仕向けたんだ？ いや… 聞かないでおこう。商売の秘密を明かす者は、信頼と評判を失うことになる",
                    "maleVariant": null,
                    "stringId": 2340625981214396416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "肝心なのは、望んだ通りの結果をもたらしたことだ。よくやってくれた",
                    "maleVariant": null,
                    "stringId": 2340626711425945600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自らの良心と向き合ういい機会だ。トランクが開くまでに答えが見つかるといいのだがな",
                    "maleVariant": null,
                    "stringId": 2340636568191340544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "苦しみを伴う道をあえて選んだことに喝采を送ろう。仕事は完了だ",
                    "maleVariant": null,
                    "stringId": 2340638112114642944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "仕事はこれで完了だ",
                    "maleVariant": null,
                    "stringId": 2340641842612506624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "グスタボ・オルタは死んだ。自ら命を絶ってな",
                    "maleVariant": null,
                    "stringId": 2362219600920547328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前の良心も痛まずに済んだ、そうだろう？ ご苦労だったな",
                    "maleVariant": null,
                    "stringId": 2362219783355994112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前が何を言ったのかは知らない。だが奴は己の罪の重さを理解し、然るべき報いを受け入れたのだろう",
                    "maleVariant": null,
                    "stringId": 2362221353569193984
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}