            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティで警官をやるってどんな感じ？",
                    "maleVariant": "ナイトシティで警官をやるってどんな感じだ？",
                    "stringId": 1938771702101184512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ、犯罪者はどんな感じだ？",
                    "maleVariant": null,
                    "stringId": 1938771744094556160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと、それってどういう意味？",
                    "maleVariant": "おい、それってどういう意味だ？",
                    "stringId": 1938771774008332288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "真っ当な警官っているの？",
                    "maleVariant": "真っ当な警官っているのか？",
                    "stringId": 1938772639041589248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしに聞いてるの？",
                    "maleVariant": "俺に聞いてるのか？",
                    "stringId": 1938840326736318468
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "見当もつかない",
                    "maleVariant": null,
                    "stringId": 1938841931292479488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうね… 今のは忘れて",
                    "maleVariant": "そうだな… 今のは忘れてくれ",
                    "stringId": 1938841975315894272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんだ、まさか自覚してなかったのか？",
                    "maleVariant": null,
                    "stringId": 1938842775538765824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしはルールには従う、それが合法かは知らないけど",
                    "maleVariant": "俺はルールには従う、それが合法かは知らないがな",
                    "stringId": 1938842906803703808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあ、犯罪者じゃないから",
                    "maleVariant": "さあ、犯罪者じゃないからな",
                    "stringId": 1938843197804515328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "法を犯すこと、それが犯罪の定義だぞ、V",
                    "maleVariant": null,
                    "stringId": 1938843571567333376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "法が肝心の現実に追いついてないのは、残念ね",
                    "maleVariant": "法が肝心の現実に追いついてないのは、残念だ",
                    "stringId": 1938843686877138944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クズを1人引っこ抜けば2人生えてくる、それがこの街だ",
                    "maleVariant": null,
                    "stringId": 1938845444542173184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "賄賂も蔓延してる。まれに見る理想主義者でもない限り抗えないさ",
                    "maleVariant": null,
                    "stringId": 1938845751732998144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あとは帳簿の操作だな。現実から目を背けるにはたっぷりの酒がいる。それを賄賂で賄うんだよ",
                    "maleVariant": null,
                    "stringId": 1938845910462238720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それは… 楽しそうね",
                    "maleVariant": "それは… 楽しそうだ",
                    "stringId": 1938846876662108164
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "気が滅入るね",
                    "maleVariant": "気が滅入るな",
                    "stringId": 1938847007323066368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "楽しそうな毎日ね",
                    "maleVariant": "楽しそうな毎日だ",
                    "stringId": 1938847395531067392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "警察がそこまで汚れてるなんて",
                    "maleVariant": "警察がそこまで汚れてるとはな",
                    "stringId": 1938850570401091584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "毎年2万人が殺されるか行方不明になってる。必要な仕事だよ",
                    "maleVariant": null,
                    "stringId": 1938853750253948928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この街で警官をやるってどんな感じ？",
                    "maleVariant": "この街で警官をやるってどんな感じだ？",
                    "stringId": 1938891075012923392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一番の札付きギャングはどこ？",
                    "maleVariant": "一番の札付きギャングはどこだ？",
                    "stringId": 1938899768345497600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この街に真っ当な警官はいるの？",
                    "maleVariant": "この街に真っ当な警官はいるのか？",
                    "stringId": 1938900123686932480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺以外にか？",
                    "maleVariant": null,
                    "stringId": 1938902314489352192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クリーンな警官なんざ、ユニコーンみたいなもんだ",
                    "maleVariant": null,
                    "stringId": 1938902450837786624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたもどこかに角が生えてるの？",
                    "maleVariant": "あんたもどこかに角が生えてるのか？",
                    "stringId": 1938903723758718976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "実際、まったくいないことはない。たとえばアナ・ハミルとか",
                    "maleVariant": null,
                    "stringId": 1938905522561486848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しばらく会ってないが、何をしてるやら",
                    "maleVariant": null,
                    "stringId": 1938906703107067904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アナは賄賂を受け取らなかったの？",
                    "maleVariant": "アナは賄賂を受け取らなかったのか？",
                    "stringId": 1938907497944453120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アナは他の警官とどう違うの？",
                    "maleVariant": "アナは他の警官とどう違うんだ？",
                    "stringId": 1938907600218361856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アナは賄賂を断ってたの？",
                    "maleVariant": "アナは賄賂を断ってたのか？",
                    "stringId": 1938909696246288384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どう違ったの？",
                    "maleVariant": "どう違ったんだ？",
                    "stringId": 1938909743843250176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あいつは自分のやり方で犯罪者を追ってた。スーパーヒーロー気取りでな",
                    "maleVariant": null,
                    "stringId": 1938911016244088832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "法律よりも正義を優先する奴だった。あと、権力に屈することもなかったな",
                    "maleVariant": null,
                    "stringId": 1938911398613618688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "誰かさんにそっくりね",
                    "maleVariant": "誰かさんにそっくりだ",
                    "stringId": 1938916291520892928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "NCPDの見解だと、ナイトシティ一凶悪なギャングはどこなの？",
                    "maleVariant": "NCPDの見解だと、ナイトシティ一凶悪なギャングはどこなんだ？",
                    "stringId": 1938948860761214976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "死体の数でいくとスカベンジャーだ。ま、季節によってはメイルストロームだな",
                    "maleVariant": null,
                    "stringId": 1938949990455054336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハンとの関係はどうなったの？",
                    "maleVariant": "ハンとの関係はどうなったんだ？",
                    "stringId": 1938995287830958080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "別の話をしましょ",
                    "maleVariant": "別の話をしよう",
                    "stringId": 1938998681425547264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "話題を変えましょ",
                    "maleVariant": "話題を変えよう",
                    "stringId": 1938999883580182528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あれからハンとの関係はどうなったの？",
                    "maleVariant": "あれからハンとの関係はどうなったんだ？",
                    "stringId": 1939026581583433728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ピーター・パン事件に関わったことで、おとがめはなかったの？",
                    "maleVariant": "ピーター・パン事件に関わったことで、おとがめはなかったのか？",
                    "stringId": 1939026716405141504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "新しいキャリアは順調？ 探偵事務所はいつ立ち上げる？",
                    "maleVariant": "新しいキャリアは順調か？ 探偵事務所はいつ立ち上げる？",
                    "stringId": 1939027027790270464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ピーター・パン事件の後、問題はない？",
                    "maleVariant": "ピーター・パン事件の後、問題はないか？",
                    "stringId": 1939027458494959616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私立探偵の仕事はどう？",
                    "maleVariant": "私立探偵の仕事はどうだ？",
                    "stringId": 1939027497317437440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたのために時間を割けなくて怒ってない？",
                    "maleVariant": "お前のために時間を割けなくて怒ってないか？",
                    "stringId": 1939028270243778560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "二人のための時間を割けなくてごめん",
                    "maleVariant": "二人のための時間を割けなくてすまん",
                    "stringId": 1939029448977420288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや。ゴナガルが事件を解決できたのは俺たちのおかげだ",
                    "maleVariant": null,
                    "stringId": 1940237512195837952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "連中は成績が上がり、俺は目的を果たせた",
                    "maleVariant": null,
                    "stringId": 1940237986940719104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの事件は… 大変だったね",
                    "maleVariant": "あの事件は… 大変だったな",
                    "stringId": 1940238341476847616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうだな。さしあたっては、ジョスの身の回りの世話をしてる。久々の休暇だ…",
                    "maleVariant": null,
                    "stringId": 1940238472842448896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "時間をかけて、ゆっくりやるさ",
                    "maleVariant": null,
                    "stringId": 1940239336550305792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まだやり方を模索してるとこだが、最大のハードルはなんとか乗り越えられた",
                    "maleVariant": null,
                    "stringId": 1940244212026052608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "免許に設備にソフトウェア… 必要なものはそろってる。あとは自分をどう売り込むかだ…",
                    "maleVariant": null,
                    "stringId": 1940245564756201472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何かの専門になるつもり？",
                    "maleVariant": "何かの専門になるつもりか？",
                    "stringId": 1940249345552158720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "誘拐だ。ランディの一件でいろいろ勉強になった",
                    "maleVariant": null,
                    "stringId": 1940249846704377856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なるほど… 活躍できそうね",
                    "maleVariant": "なるほど… 活躍できそうだな",
                    "stringId": 1940250219947102208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いまいちだな",
                    "maleVariant": null,
                    "stringId": 1940264474440241152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まるで市長の死の隠蔽なんてなかったかのように振る舞ってる",
                    "maleVariant": null,
                    "stringId": 1940264909154684928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "辛いところね…",
                    "maleVariant": "辛いとこだな…",
                    "stringId": 1940266000479031296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "相棒を超えた… 友達だと思ってたからな",
                    "maleVariant": null,
                    "stringId": 1940266265038950400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そりゃまあ… 辛いさ",
                    "maleVariant": null,
                    "stringId": 1940266525018689536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。話題を変えましょ",
                    "maleVariant": "わかった。話題を変えよう",
                    "stringId": 1940268513219448832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前にもやることがあるのはわかってるさ。そのうち埋めあわせよう",
                    "maleVariant": null,
                    "stringId": 1940298667278864384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、約束する。仕事が片付いたらすぐにね",
                    "maleVariant": "ああ、約束する。仕事が片付いたらすぐにな",
                    "stringId": 1940303157784612864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待ってるぜ",
                    "maleVariant": null,
                    "stringId": 1940304534355185664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしたち、上手くいってない気がする",
                    "maleVariant": "俺たち、上手くいってない気がする",
                    "stringId": 1940306564146663424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最近よく思うんだけどさ…",
                    "maleVariant": "最近よく思うんだが…",
                    "stringId": 1940306967940698112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんだ？",
                    "maleVariant": null,
                    "stringId": 1940313276408795136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その、あたしたちのこと",
                    "maleVariant": "その、俺たちのことを",
                    "stringId": 1940313682568417280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "上手くいきそうにないってことか？",
                    "maleVariant": null,
                    "stringId": 1940313993517338624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしてわかったの？",
                    "maleVariant": "どうしてわかったんだ？",
                    "stringId": 1940314167228633088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあな。お前のことがわかってきたのかもしれん",
                    "maleVariant": null,
                    "stringId": 1940314307872034816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その… ごめん、何言ってんだろ。今言ったことは忘れて",
                    "maleVariant": "その… すまん、何言ってんだか。今言ったことは忘れてくれ",
                    "stringId": 1940314679923576832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう忘れたさ",
                    "maleVariant": null,
                    "stringId": 1940315222448410624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヴァレンティーノズには特別賞をやる。あそこの“罰”はバリエーション豊かだ",
                    "maleVariant": null,
                    "stringId": 1963623968138203136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "たとえば、カミソリを持って鏡の前に座らされ、それから骨が見えるまで肉をそぎ落とせと言われるんだ",
                    "maleVariant": null,
                    "stringId": 1963624844160536576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ある女性を探してる",
                    "maleVariant": null,
                    "stringId": 1976315387115692032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人を探してる。20代の女性、ドールよ。クラウドって店から消えた",
                    "maleVariant": "人を探してる。20代の女性、ドールだ。クラウドって店から消えた",
                    "stringId": 1976316005406433280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "システムで調べてもいいが… この街で毎日どれだけの女性が消えてるか、知ってるか？",
                    "maleVariant": null,
                    "stringId": 1976318338462867456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大半が水商売の女だ。あまり期待しすぎるな",
                    "maleVariant": null,
                    "stringId": 1976325284867825664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、どうした？",
                    "maleVariant": null,
                    "stringId": 2257692897095847936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": " じゃあまた",
                    "maleVariant": null,
                    "stringId": 2430427304218996744
                  }
                }
              ]
            }
          }
        }
      }
