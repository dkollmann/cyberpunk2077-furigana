        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なぜ家じゃダメだったの？",
                    "maleVariant": "なぜ家じゃダメだったんだ？",
                    "stringId": 1247535407760527360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ぶっ飛んだ話だな。まったく、哀れな連中だぜ",
                    "maleVariant": null,
                    "stringId": 1247539819933454336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうして家じゃなくて、わざわざここで？",
                    "maleVariant": "なぜ家じゃなく、わざわざここで？",
                    "stringId": 1283710187983196160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたには似合わない場所ね",
                    "maleVariant": "あんたには似合わない場所だな",
                    "stringId": 1287692705029812224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いい店だけど… あんたにはあまり似合わないね",
                    "maleVariant": "いい店だが… あんたにはあまり似合わないな",
                    "stringId": 1287698826012520448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも密会にはぴったりでしょ？",
                    "maleVariant": null,
                    "stringId": 1287700502190616576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "来てくれてありがとう",
                    "maleVariant": null,
                    "stringId": 1287710955453149184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あなたに話しておきたいことがあって。ジェフのいないところでね",
                    "maleVariant": null,
                    "stringId": 1287719774749061120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふーん… まあいいけど",
                    "maleVariant": "へえ… まあいいが",
                    "stringId": 1287723625640226816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最初からこうなることはわかってたのね",
                    "maleVariant": "最初からこうなることはわかってたのか",
                    "stringId": 1287726403041226752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたにも何かしらの変化が？",
                    "maleVariant": null,
                    "stringId": 1287726788933971968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたにも何かしらの変化が？",
                    "maleVariant": null,
                    "stringId": 1287727646098714624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おかしな行動をとるようになったって、何度か友人に言われたの",
                    "maleVariant": null,
                    "stringId": 1287733877290954752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "てことは、最初からこうなることはわかってたのね",
                    "maleVariant": "てことは、最初からこうなることはわかってたのか",
                    "stringId": 1287735294999261184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "誰が、なぜ、どうやってやってるのかはわからない。でも、私たちは操られてる",
                    "maleVariant": null,
                    "stringId": 1287737648708448256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "家にあった、あの隠し部屋…",
                    "maleVariant": null,
                    "stringId": 1287755286696292352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジェフには何も話してないの？",
                    "maleVariant": "ジェフには何も話してないのか？",
                    "stringId": 1287785680837668864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "話そうとはしたわ。でも聞く耳を持たなかった。考えすぎだって笑い飛ばされたの",
                    "maleVariant": null,
                    "stringId": 1287786179892736000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "以前、ジェフが行ったことのない夫婦旅行の話をし始めたの",
                    "maleVariant": null,
                    "stringId": 1287787148156203008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼ったらすごく詳細に話すから、つい信じそうになったくらい…",
                    "maleVariant": null,
                    "stringId": 1287788098199613440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "偽りの生活を送り続ける気？ ジェフに知らせるべきよ",
                    "maleVariant": "偽りの生活を送り続ける気か？ ジェフに知らせるべきだ",
                    "stringId": 1287789011668692992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしをだましてたのね",
                    "maleVariant": "俺をだましてたのか",
                    "stringId": 1287791032903790592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "侵入者を送り込んだ相手を知ってたのに、あたしをだましてたのね",
                    "maleVariant": "侵入者を送り込んだ相手を知ってたのに、俺をだましてたのか",
                    "stringId": 1287791620089573376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "仕方なかったの。どうしていいかわからなくて…",
                    "maleVariant": null,
                    "stringId": 1287799222651449344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "実はね… 少し前から、ジェフの言動がおかしいことに気づいてはいたの",
                    "maleVariant": null,
                    "stringId": 1287801261787836416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "読書をやめたり、好きな映画の題名を忘れたり… 一晩で音楽の趣味まで変わったのよ",
                    "maleVariant": null,
                    "stringId": 1287833424281333760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "性格が… 私の目の前で変わっていった… まるで別人みたいに",
                    "maleVariant": null,
                    "stringId": 1287835703298355200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大丈夫よ、焦らなくていい",
                    "maleVariant": "大丈夫だ、焦らなくていい",
                    "stringId": 1287842183078604800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何か取引したの？",
                    "maleVariant": "何か取引したのか？",
                    "stringId": 1287845370296659968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "脅迫されたの？",
                    "maleVariant": "脅迫されたのか？",
                    "stringId": 1287846648418529280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "内容は？ 脅迫されたの？",
                    "maleVariant": "内容は？ 脅迫されたのか？",
                    "stringId": 1287847779840421888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その男と何か取引したの？",
                    "maleVariant": "その男と何か取引したのか？",
                    "stringId": 1287854952737796096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いえ。幸せな暮らしはいつ消え去るかわからない、夫のことを考えろって言われたわ",
                    "maleVariant": null,
                    "stringId": 1287855400387473408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だからお願いよ、V… 何があっても絶対にジェフに知らせるわけにはいかないの",
                    "maleVariant": null,
                    "stringId": 1287867296037597184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前は危ない橋を渡ってる、余計な真似をしたら、ジェフに“事故”が起こるぞって…",
                    "maleVariant": null,
                    "stringId": 1287868978523926528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ウソでしょ？ 誰かに自分の頭をいじられてるのに、このまま放っておくつもり？",
                    "maleVariant": "ウソだろ？ 誰かに自分の頭をいじられてるのに、このまま放っておくつもりか？",
                    "stringId": 1287892776988045312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だからあたしにもウソをつけと？",
                    "maleVariant": "だから俺にもウソをつけと？",
                    "stringId": 1287896037941964800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "他にどうしろって言うの？ 殺されるかもしれない、夫が奪われるかもしれないのに！",
                    "maleVariant": null,
                    "stringId": 1287898335262928896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジェフにはSSIがホルトとグルだったって言う。あなたは口裏を合わせてくれるだけでいい",
                    "maleVariant": null,
                    "stringId": 1287907477553803264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "協力しないと言ったら？",
                    "maleVariant": null,
                    "stringId": 1287915457032613888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "脅迫されてるのに、SSIをクビにする気？",
                    "maleVariant": "脅迫されてるのに、SSIをクビにする気か？",
                    "stringId": 1287916955271557120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "協力しないと言ったら？",
                    "maleVariant": null,
                    "stringId": 1287917731821776896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "強制はしない。ただしあなたの選択が、人の人生を左右することを忘れないで",
                    "maleVariant": null,
                    "stringId": 1287918161872154624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "脅迫されてるんでしょ？ なのにSSIをクビにする気？ 勇気は認めるけど、リスクが大きすぎる…",
                    "maleVariant": "脅迫されてるんだろ？ なのにSSIをクビにする気か？ 勇気は認めるが、リスクが大きすぎる…",
                    "stringId": 1287922531347066880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私の家にあの部屋がある限り、安心して夜も眠れないわ… そうでしょ？",
                    "maleVariant": null,
                    "stringId": 1287925323663011840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ん…",
                    "maleVariant": null,
                    "stringId": 1287926949694009344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジェフと会えるように手配しておく。場所はレコンシリエーション・パークで",
                    "maleVariant": null,
                    "stringId": 1287928663855722496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "明日になったら、私はこの会話を覚えてないかもしれないから… くれぐれもお願いしたことを忘れないでね",
                    "maleVariant": null,
                    "stringId": 1287929485352103936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "にしても… 記憶が重なるとか、人格が変わるとか、どっかで聞いたような話だな",
                    "maleVariant": null,
                    "stringId": 1287993972071460864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたならどうした？",
                    "maleVariant": "お前ならどうした？",
                    "stringId": 1287997052770971648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの二人とあたしたちじゃ、まったく違う",
                    "maleVariant": "あの二人と俺たちじゃ、まったく違う",
                    "stringId": 1287997371789733888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これは話が別よ。見たでしょ。二人の人格や考えを、誰かが意図的に操作してるのよ",
                    "maleVariant": "これは話が別だ。見ただろ。二人の人格や考えを、誰かが意図的に操作してるんだ",
                    "stringId": 1287998573390721024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "確かに。最先端仕様のマリオネットだ。まあ、こういう類のことは、昔から噂されてたがな",
                    "maleVariant": null,
                    "stringId": 1288002562777804800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で… あんたならどうした？",
                    "maleVariant": "で… お前ならどうした？",
                    "stringId": 1288006846974128128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "陰で糸を引いてるクズどもを、誰かがまとめて始末すべきだ",
                    "maleVariant": null,
                    "stringId": 1288010564033441792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "誰かの操り人形と化した市長なんて、何をしでかすかわからねえ",
                    "maleVariant": null,
                    "stringId": 1288013014396170240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だが確かに、二人を危険にさらす可能性もある。V、これは難しい選択だな",
                    "maleVariant": null,
                    "stringId": 1288016290533470208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "都合がいい？",
                    "maleVariant": null,
                    "stringId": 1644785539914797056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なるほど。それで何？",
                    "maleVariant": "なるほど。それで何だ？",
                    "stringId": 1664877952788205568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "切った後、履歴は残ってなかった――まるで何もなかったかのように",
                    "maleVariant": null,
                    "stringId": 1664946768566161408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "切った後、通話の記録は消去されてた。まるで何もなかったかのように",
                    "maleVariant": null,
                    "stringId": 1664947729565093888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この先SSIに何をされても全部私が責任を持つ。だからお願い、ジェフを巻き込まないで",
                    "maleVariant": null,
                    "stringId": 1664953057472024576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺たちは相手がわかってるだけマシか",
                    "maleVariant": null,
                    "stringId": 1664955961641668608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "すぐにではないけど――",
                    "maleVariant": null,
                    "stringId": 1751766830148620288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたには、このままの方が色々と都合がよかったんでしょ",
                    "maleVariant": "あんたには、このままの方が色々と都合がよかったんだろ",
                    "stringId": 1751811004373798912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私がこんな生活を望んでるとでも？ 私の何がわかるっていうの？ ふざけないで！",
                    "maleVariant": null,
                    "stringId": 1751812112475361280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お義兄さんのお墓参りに行こうって言ったときは、自分は一人っ子で兄弟なんていないって言いだして…！",
                    "maleVariant": null,
                    "stringId": 1751820687377567744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何も見つからなかったんでしょ？",
                    "maleVariant": "何も見つからなかったんだろ？",
                    "stringId": 1751825035176316928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしてジェファーソンに話さなかったの？",
                    "maleVariant": "なぜジェファーソンに話さなかったんだ？",
                    "stringId": 1751825035176316944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "話はこれで全部？ 他には？",
                    "maleVariant": "話はこれで全部か？ 他には？",
                    "stringId": 1751830634723151872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジェファーソンには話したの？",
                    "maleVariant": "ジェファーソンには話したのか？",
                    "stringId": 1751830634739929096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたが忘れてたってことは？ 旅行の記憶が消されてたのかも",
                    "maleVariant": null,
                    "stringId": 1751837893318545408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やめて！ そう言われると何が何だかわからなくなる！",
                    "maleVariant": null,
                    "stringId": 1751838794171158528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まだ隠してることがあるんじゃない？",
                    "maleVariant": "まだ隠してることがあるんじゃないか？",
                    "stringId": 1751847778940751872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ある夜、家で知らない人がモニターをいじってるのを見た",
                    "maleVariant": null,
                    "stringId": 1751847778957529100
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも、私はもう自分の目さえ信用できない状態になってたから",
                    "maleVariant": null,
                    "stringId": 1751847778957529160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で、何もしなかったの？",
                    "maleVariant": "で、何もしなかったのか？",
                    "stringId": 1751873582835830784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうなったのか覚えてない。翌朝、警備スタッフには話したけど",
                    "maleVariant": null,
                    "stringId": 1751873749551026176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "普通なら、警備より先に旦那に話すと思うんだけど",
                    "maleVariant": "普通なら、警備より先に旦那に話すと思うんだが",
                    "stringId": 1751880320431558656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自分がおかしくなったのかもしれないと思って――先に映像を確認したかったのよ…",
                    "maleVariant": null,
                    "stringId": 1751880320431558672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もちろん何も映ってなかった。こんな話、ジェフにできないでしょ？",
                    "maleVariant": null,
                    "stringId": 1751880320431558688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうせカメラには何も映ってなかったんでしょ？",
                    "maleVariant": "どうせカメラには何も映ってなかったんだろ？",
                    "stringId": 1751880320465113088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ。きっと夢でも見たんだろうって",
                    "maleVariant": null,
                    "stringId": 1751880320465113104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "スタッフに映像を見せられた時は、頭がおかしくなったんだと思った",
                    "maleVariant": null,
                    "stringId": 1751880320465113120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そしたら次の日、連絡があったの",
                    "maleVariant": null,
                    "stringId": 1751887134497837056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "誰から？",
                    "maleVariant": "誰からだ？",
                    "stringId": 1751887134497837072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "奴らよ。そのうちの一人",
                    "maleVariant": null,
                    "stringId": 1751887134497837088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "加工された男の声だった。何者かは名乗らなかったわ",
                    "maleVariant": null,
                    "stringId": 1751887134514614276
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼の命が懸かってる",
                    "maleVariant": null,
                    "stringId": 1751898429271969792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ペラレス夫妻を危険にさらしても？",
                    "maleVariant": "ペラレス夫妻を危険にさらしてもか？",
                    "stringId": 1751953208727703552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私たちにあんなことをしてる上に、隠し部屋まで作ってたのよ…",
                    "maleVariant": null,
                    "stringId": 1751964396832296960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "例えば？",
                    "maleVariant": null,
                    "stringId": 1806542800348647424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "前に見てるはずなの。でもなぜか思い出せなくて…",
                    "maleVariant": null,
                    "stringId": 1806558494545022976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ。もう何が現実で何がそうじゃないのか、わからなくなった",
                    "maleVariant": null,
                    "stringId": 1806559956075417600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それ以来、何か悪いことが起きてると思い始めたの。何か… 恐ろしいことが。それもジェフだけにじゃなく",
                    "maleVariant": null,
                    "stringId": 1806565017811816448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたはプログラムされたことを言ってるだけかもしれない",
                    "maleVariant": null,
                    "stringId": 1806649853817786368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "理想的な暮らし、有力者の夫。困ることは何もない",
                    "maleVariant": null,
                    "stringId": 1806655371273703424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたはプログラムされたことを言ってるだけかもしれない",
                    "maleVariant": null,
                    "stringId": 1806662097242820608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "他の誰かが書いた台本を読んでるだけで",
                    "maleVariant": null,
                    "stringId": 1806662931523440640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私はただ、愛する夫に無事で… そして幸せでいてほしいだけよ",
                    "maleVariant": null,
                    "stringId": 1806664422413963264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それもプログラムかも",
                    "maleVariant": null,
                    "stringId": 1806664880079638528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジェフに本当のことを言ったら――彼のことだもの、自分で調べ始めるに決まってる",
                    "maleVariant": null,
                    "stringId": 1806665649163997184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "絶対に言っちゃダメよ。きっと… 壊れてしまうから…",
                    "maleVariant": null,
                    "stringId": 1806666322802774016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "忘れないけど、それ以外は約束できない",
                    "maleVariant": "忘れないが、それ以外は約束できないな",
                    "stringId": 1806718248571506688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さよなら、V",
                    "maleVariant": null,
                    "stringId": 1806719497618460672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その… ごめんなさい、ちょっとまだ気持ちの収まりがついてなくて",
                    "maleVariant": null,
                    "stringId": 1825880475211796480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "焦らなくていい",
                    "maleVariant": null,
                    "stringId": 1825882957686116356
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "前回会ってから、何かあったの？",
                    "maleVariant": "前回会ってから、何かあったのか？",
                    "stringId": 1825883035750502400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最後に会ってからそんな経ってないけど、あれから何かあったの？",
                    "maleVariant": "最後に会ってからそんな経ってないが、あれから何かあったのか？",
                    "stringId": 1825885633685622784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いえ… あるにはあったけど、最近のことじゃないの",
                    "maleVariant": null,
                    "stringId": 1825888194828308480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "長い話よ。正直、どこから話したらいいのかもわからない…",
                    "maleVariant": null,
                    "stringId": 1825889090312212480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "つらかったでしょうね…",
                    "maleVariant": "つらかっただろうな…",
                    "stringId": 1825920897950814208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ",
                    "maleVariant": "ああ",
                    "stringId": 1825927567145832448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それが怖いの",
                    "maleVariant": null,
                    "stringId": 1825927922403381248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "家じゃなくても嫌だけどね…",
                    "maleVariant": "家じゃなくても嫌だけどな…",
                    "stringId": 1825986511478329344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もし全てを話したら、ジェフが無茶をするって言いたいの？",
                    "maleVariant": "もし全てを話したら、ジェフが無茶をするって言いたいのか？",
                    "stringId": 2007009273591238656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そのとおりよ。ジェフの正義感の強さだけは、変わってないから",
                    "maleVariant": null,
                    "stringId": 2007011551433854976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "勝ち目はないのに… で、あんたはどうしたいの？",
                    "maleVariant": "勝ち目はないのに… で、あんたはどうしたいんだ？",
                    "stringId": 2007012879535050752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジェフが知ったら、相手と戦うと思うの？",
                    "maleVariant": "ジェフが知ったら、相手と戦うと思うのか？",
                    "stringId": 2007014498855161856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "認知パターン変動機、遠隔操作の神経回路再編成… 自分の頭がおかしいと思わせるわけだ",
                    "maleVariant": null,
                    "stringId": 2007037463692972032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こんなのがそこら中にあふれてる。テレビにはサブリミナル信号、電動歯ブラシには高密度エミッターって具合にな",
                    "maleVariant": null,
                    "stringId": 2007039860989366272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[立ち上がる]",
                    "maleVariant": null,
                    "stringId": 2015911712252489732
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[座る]",
                    "maleVariant": null,
                    "stringId": 2015914022626127872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうぞ、早く座って…",
                    "maleVariant": null,
                    "stringId": 2228800174800637952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V？ 話を聞いてる？",
                    "maleVariant": null,
                    "stringId": 2228811841945079808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "気になることでも？",
                    "maleVariant": null,
                    "stringId": 2228812318334128128
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
