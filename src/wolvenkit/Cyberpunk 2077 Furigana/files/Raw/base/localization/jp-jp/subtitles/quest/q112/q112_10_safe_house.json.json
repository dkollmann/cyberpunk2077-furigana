    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1670537923168059396,
            "femaleVariant": "[1回ノックする]"
          },
          {
            "stringId": 1670539041772158976,
            "femaleVariant": "よし、入れ。早く"
          },
          {
            "stringId": 1670552548320231424,
            "femaleVariant": "ハナコ様！ Vは兄君の罪を白日の下にさらけ出す、生き証人です！"
          },
          {
            "stringId": 1670553047593402368,
            "femaleVariant": "〈Relic〉についてお力添えを頂ければ、今の話を証明してみせましょう"
          },
          {
            "stringId": 1670553432932499456,
            "femaleVariant": "ハナコ様？"
          },
          {
            "stringId": 1670783052671893508,
            "femaleVariant": "[座る] サブロウが死んだ現場にいた"
          },
          {
            "stringId": 1711081985600786432,
            "femaleVariant": "ハナコ様… 彼女が例の人物です",
            "maleVariant": "ハナコ様… 彼が例の人物です"
          },
          {
            "stringId": 1711126618515050496,
            "femaleVariant": "そんなデタラメを信じると思うの？ どうかしてるわね"
          },
          {
            "stringId": 1711127514468716548,
            "femaleVariant": "あたしに失うものはない。もうすぐ死ぬから",
            "maleVariant": "俺に失うものはない。もうすぐ死ぬからな"
          },
          {
            "stringId": 1711128146499997696,
            "femaleVariant": "どうかしてるのよ。〈Relic〉のせいでね！",
            "maleVariant": "どうかしてるんだ。〈Relic〉のせいでな！"
          },
          {
            "stringId": 1711129711461928960,
            "femaleVariant": "信じがたいのはわかる。でも提案があるの",
            "maleVariant": "信じがたいのはわかる。でも提案がある"
          },
          {
            "stringId": 1711131304357924864,
            "femaleVariant": "言っておくけど、あたしにはもう失うものは何もない",
            "maleVariant": "言っておくが、俺にはもう失うものは何もない"
          },
          {
            "stringId": 1711131385140219904,
            "femaleVariant": "どうかしてる？ それもそのはずよ",
            "maleVariant": "どうかしてる？ それもそのはずだ"
          },
          {
            "stringId": 1711131482766839808,
            "femaleVariant": "これが突拍子もない話なのはわかってる"
          },
          {
            "stringId": 1714504976757178368,
            "femaleVariant": "いったい何考えてるの！？",
            "maleVariant": "いったい何考えてんだ！？"
          },
          {
            "stringId": 1714505163051384832,
            "femaleVariant": "説明してる時間はない。落ち合おう"
          },
          {
            "stringId": 1714506181881380864,
            "femaleVariant": "逃げ切れたようで良かった"
          },
          {
            "stringId": 1724285845483950080,
            "femaleVariant": "今のところはね",
            "maleVariant": "今のところはな"
          },
          {
            "stringId": 1724288361797914624,
            "femaleVariant": "ヴァイン通りにある廃虚になったマンション。部屋は303号室だ"
          },
          {
            "stringId": 1724297593293246464,
            "femaleVariant": "V？ 無事か？"
          },
          {
            "stringId": 1724299457560711168,
            "femaleVariant": "急げ！"
          },
          {
            "stringId": 1724303905972424708,
            "femaleVariant": "いったい何のつもりだったの！？",
            "maleVariant": "いったい何のつもりだったんだ！"
          },
          {
            "stringId": 1731480264486293504,
            "femaleVariant": "なによ？ あたしの話はクソ同然？",
            "maleVariant": "なんだ？ 俺の話はクソ同然か？"
          },
          {
            "stringId": 1731481746602332160,
            "femaleVariant": "ハナコ様、非礼をおわび申し上げます…"
          },
          {
            "stringId": 1731482286476365824,
            "femaleVariant": "黙ってて！ 正直に話すって言ってたでしょ。そうしただけよ！",
            "maleVariant": "黙ってろ！ 正直に話すって言ってただろ。そうしただけだ！"
          },
          {
            "stringId": 1731482803734712320,
            "femaleVariant": "確かにあたしはちんけな泥棒かもしれない… でも、あんたたちから〈Relic〉を盗んでみせた",
            "maleVariant": "確かに俺はちんけな泥棒かもしれない… だが、あんたらから〈Relic〉を盗んでみせた"
          },
          {
            "stringId": 1731484323599486976,
            "femaleVariant": "それを頭に入れたあと、一度死んだのよ",
            "maleVariant": "それを頭に入れたあと、一度死んだんだ"
          },
          {
            "stringId": 1731485027705688064,
            "femaleVariant": "あたしに失うものはない。あんたと違ってね",
            "maleVariant": "俺に失うものはない。あんたと違ってな"
          },
          {
            "stringId": 1731486828655939584,
            "femaleVariant": "そうよ。で、ジョニー・シルヴァーハンドと脳みそをシェアする羽目になった",
            "maleVariant": "ああ。で、ジョニー・シルヴァーハンドと脳みそをシェアする羽目になった"
          },
          {
            "stringId": 1731486970926731264,
            "femaleVariant": "死んだ… ですって？"
          },
          {
            "stringId": 1731495317390364672,
            "femaleVariant": "どうか話を聞いてあげてください"
          },
          {
            "stringId": 1731498839699755008,
            "femaleVariant": "おたくの〈Relic〉試作品が頭に入っててね",
            "maleVariant": "おたくの〈Relic〉試作品が頭に入っててな"
          },
          {
            "stringId": 1731499450658852864,
            "femaleVariant": "妙な巡りあわせなんだけど、このチップは一度停止したあと、撃たれたあたしを生き返らせた。そして今はじわじわと殺しにかかってる",
            "maleVariant": "妙な巡りあわせなんだが、このチップは一度停止したあと、撃たれた俺を生き返らせた。そして今はじわじわと殺しにかかってる"
          },
          {
            "stringId": 1731500272574664704,
            "femaleVariant": "あたしの精神はチップの記憶痕跡に上書きされてる最中で、どうにか止める方法を見つけないと… 一巻の終わりってわけ",
            "maleVariant": "俺の精神はチップの記憶痕跡に上書きされてる最中で、どうにか止める方法を見つけないと… 一巻の終わりってわけだ"
          },
          {
            "stringId": 1731526782471770112,
            "femaleVariant": "[座る] 真犯人はお兄さんよ",
            "maleVariant": "[座る] 真犯人はお兄さんだ"
          },
          {
            "stringId": 1731526904324689920,
            "femaleVariant": "こんな形で会うことになるなんて",
            "maleVariant": "こんな形で会うことになるとは"
          },
          {
            "stringId": 1731528127736057856,
            "femaleVariant": "あの夜、あたしは紺碧プラザにいたの。サブロウ・アラサカが死ぬのを見た",
            "maleVariant": "あの夜、俺は紺碧プラザにいた。サブロウ・アラサカが死ぬのを見たんだ"
          },
          {
            "stringId": 1731528788221497344,
            "femaleVariant": "こんな形で会うことになるとは… 残念ね",
            "maleVariant": "こんな形で会うことになるとは… 残念だ"
          },
          {
            "stringId": 1731528858736136192,
            "femaleVariant": "殺したのはヨリノブよ",
            "maleVariant": "殺したのはヨリノブだ"
          },
          {
            "stringId": 1731535225572167680,
            "femaleVariant": "いったい何考えてたのか、説明してくれる！？",
            "maleVariant": "いったい何考えてたのか、説明してくれよ！？"
          },
          {
            "stringId": 1731535308216733696,
            "femaleVariant": "アラサカが総力を挙げて探してる"
          },
          {
            "stringId": 1731536118136836096,
            "femaleVariant": "ハナコはいまこの瞬間、世界で一番有名な行方不明者でしょうね",
            "maleVariant": "ハナコはいまこの瞬間、世界で一番有名な行方不明者だろうな"
          },
          {
            "stringId": 1731536476464615424,
            "femaleVariant": "わかっている"
          },
          {
            "stringId": 1731536556978475008,
            "femaleVariant": "アラサカがその総力を挙げてあたしたちを追ってくる",
            "maleVariant": "アラサカがその総力を挙げて俺たちを追ってくる"
          },
          {
            "stringId": 1731536987850936320,
            "femaleVariant": "そうだろうな。一刻も無駄にはできまい"
          },
          {
            "stringId": 1731538055049646080,
            "femaleVariant": "来い"
          },
          {
            "stringId": 1731538519778529280,
            "femaleVariant": "ヨリノブが何をしたかを話すんだ。誇張したりせず、はっきりと事実だけを伝えてくれ"
          },
          {
            "stringId": 1731545725391474688,
            "femaleVariant": "追跡器を作動されそうになって――"
          },
          {
            "stringId": 1731545950373941248,
            "femaleVariant": "ああするしかなかった"
          },
          {
            "stringId": 1731562679003123712,
            "femaleVariant": "詫び言ならすでに聞いているわ"
          },
          {
            "stringId": 1731602204832174080,
            "femaleVariant": "でもあんたは誠実で、信頼に値する人物だってことは知ってる。だから提案をしたい",
            "maleVariant": "だがあんたは誠実で、信頼に値する人物だってことは知ってる。だから提案をしたい"
          },
          {
            "stringId": 1731602376899301376,
            "femaleVariant": "あなたが？ この私に？"
          },
          {
            "stringId": 1731602439931301888,
            "femaleVariant": "あんたのお兄さんがネットウォッチに売ろうとしてた〈Relic〉が、あたしの頭に入ってる",
            "maleVariant": "あんたのお兄さんがネットウォッチに売ろうとしてた〈Relic〉が、俺の頭に入ってる"
          },
          {
            "stringId": 1731607036267728896,
            "femaleVariant": "前に弾を頭に受けたとき、チップのおかげで助かった。でも今はあたしをむしばんでる",
            "maleVariant": "前に弾を頭に受けたとき、チップのおかげで助かった。でも今は俺をむしばんでる"
          },
          {
            "stringId": 1731608809820786688,
            "femaleVariant": "チップに保存された記憶痕跡――人格コンストラクトが、あたしの精神を上書きしてるの",
            "maleVariant": "チップに保存された記憶痕跡――人格コンストラクトが、俺の精神を上書きしてるんだ"
          },
          {
            "stringId": 1731608989924200448,
            "femaleVariant": "手遅れになる前になんとかしたい。あんたならきっと方法がわかるはず"
          },
          {
            "stringId": 1731614900839387136,
            "femaleVariant": "頭の中で声がするのよ。記憶痕跡、つまり人格コンストラクトのね",
            "maleVariant": "頭の中で声がするんだよ。記憶痕跡、つまり人格コンストラクトのな"
          },
          {
            "stringId": 1731617727749951488,
            "femaleVariant": "記憶痕跡？"
          },
          {
            "stringId": 1731617964241588224,
            "femaleVariant": "そうよ。ヨリノブがネットウォッチに売ろうとしてた〈Relic〉を盗んだの",
            "maleVariant": "そうだ。ヨリノブがネットウォッチに売ろうとしてた〈Relic〉を盗んだんだ"
          },
          {
            "stringId": 1731620602693685248,
            "femaleVariant": "お父さんの首を絞めるのを、あたしはこの目で見た",
            "maleVariant": "お父さんの首を絞めるのを、俺はこの目で見た"
          },
          {
            "stringId": 1731632438516256768,
            "femaleVariant": "耳を貸してくださるといいのだが"
          },
          {
            "stringId": 1731650922847539200,
            "femaleVariant": "毒を盛られたんじゃない。それはあんたのお兄さんがでっち上げたウソ――"
          },
          {
            "stringId": 1731652182883569664,
            "femaleVariant": "そのとき頭に弾を受けて… 一度は〈Relic〉に救われた"
          },
          {
            "stringId": 1731653101738135552,
            "femaleVariant": "なのに今度は殺されかけてる。なんとかして止めたいの",
            "maleVariant": "なのに今度は殺されかけてる。なんとかして止めたいんだ"
          },
          {
            "stringId": 1772156934268260352,
            "femaleVariant": "お茶をおすすめしたんだが…"
          },
          {
            "stringId": 1772157094272569344,
            "femaleVariant": "ハナコ・アラサカを拉致しておいて、よりによってお茶を出そうとしたの？",
            "maleVariant": "ハナコ・アラサカを拉致しておいて、よりによってお茶を出そうとしたのか？"
          },
          {
            "stringId": 1772157332911689728,
            "femaleVariant": "ああ… 丁重に断られた"
          },
          {
            "stringId": 1772186550869561344,
            "femaleVariant": "そのヨリノブが殺したの",
            "maleVariant": "そのヨリノブが殺したんだ"
          },
          {
            "stringId": 1772206455006302208,
            "femaleVariant": "あたしの話は聞くに値しない！？",
            "maleVariant": "俺の話は聞くに値しないか！？"
          },
          {
            "stringId": 1774843468238045184,
            "femaleVariant": "クソ… 早すぎる！"
          },
          {
            "stringId": 1774902623493701632,
            "femaleVariant": "逃げるならこれが最後のチャンスだぞ！"
          },
          {
            "stringId": 1774902980512858112,
            "femaleVariant": "もう死んじまったに決まってる！ お前までやられちまう気か！？"
          },
          {
            "stringId": 1774907798509862912,
            "femaleVariant": "身を低くして警戒を怠るな… 廊下にいるぞ"
          },
          {
            "stringId": 1775139918641643524,
            "femaleVariant": "マジにイカレたか！？ 助かるはずがねえだろ！"
          },
          {
            "stringId": 1775141232750641152,
            "femaleVariant": "タケムラが！ 置いてはいけない！"
          },
          {
            "stringId": 1775148051548094464,
            "femaleVariant": "うおおおおお！"
          },
          {
            "stringId": 1775173472251559936,
            "femaleVariant": "なぜ戻った！ 俺と一緒に死ぬつもりか！"
          },
          {
            "stringId": 1775173889081491456,
            "femaleVariant": "御礼は後！ とにかく出口を見つけるのが先よ！",
            "maleVariant": "御礼は後にしろ！ とにかく出口を見つけるのが先だ！"
          },
          {
            "stringId": 1780703689993752576,
            "femaleVariant": "アラサカだ！ 見つかった！"
          },
          {
            "stringId": 1780705812227690496,
            "femaleVariant": "聞こえたか？"
          },
          {
            "stringId": 1780708445327855616,
            "femaleVariant": "見てきてくれ"
          },
          {
            "stringId": 1780724354675249152,
            "femaleVariant": "逃げろ！"
          },
          {
            "stringId": 1783641647877275648,
            "femaleVariant": "[立ち上がる]"
          },
          {
            "stringId": 1783643088234823684,
            "femaleVariant": "おい"
          },
          {
            "stringId": 1783643091355385860,
            "femaleVariant": "行け"
          },
          {
            "stringId": 1783720280742510592,
            "femaleVariant": "何してる！？ 待て！"
          },
          {
            "stringId": 1803755666667552772,
            "femaleVariant": "[廊下を調べる]"
          },
          {
            "stringId": 1803770686319181824,
            "femaleVariant": "嫌な予感しかしない…"
          },
          {
            "stringId": 1803781561360924672,
            "femaleVariant": "クソ！"
          },
          {
            "stringId": 1861773876471341056,
            "femaleVariant": "どこにいるの？",
            "maleVariant": "どこにいるんだ？"
          },
          {
            "stringId": 1861905001671892992,
            "femaleVariant": "右に空き部屋がある！ そこに賭けるしかない！"
          },
          {
            "stringId": 1879401451202035712,
            "femaleVariant": "[3回ノックする]"
          },
          {
            "stringId": 1879402549874802688,
            "femaleVariant": "[4回ノックする]"
          },
          {
            "stringId": 1882090646910345216,
            "femaleVariant": "4回ノックしろ！"
          },
          {
            "stringId": 1885463230638211072,
            "femaleVariant": "クソ…"
          },
          {
            "stringId": 1885464458914971648,
            "femaleVariant": "気をつけろ！"
          },
          {
            "stringId": 1885464717904855040,
            "femaleVariant": "V、しっかりしろ！"
          },
          {
            "stringId": 1885465158071894016,
            "femaleVariant": "ああ… まずい…"
          },
          {
            "stringId": 1885466455504338944,
            "femaleVariant": "しっかりしろ！ よりによって今かよ！"
          },
          {
            "stringId": 1885466634819223552,
            "femaleVariant": "おい！ 生きてここから出るんだ！"
          },
          {
            "stringId": 1902548247936569344,
            "femaleVariant": "部屋を制圧しろ！"
          },
          {
            "stringId": 1902552545185566720,
            "femaleVariant": "イエッサー！"
          },
          {
            "stringId": 1903750080499617792,
            "femaleVariant": "セクタークリア！"
          },
          {
            "stringId": 1903751055507525632,
            "femaleVariant": "クソ！ もう一度捜索だ！"
          },
          {
            "stringId": 1975070597751848964,
            "femaleVariant": "[立ち上がる]"
          },
          {
            "stringId": 1984825670769917952,
            "femaleVariant": "[もう一度ノックする]"
          },
          {
            "stringId": 2003866031066259456,
            "femaleVariant": "鎮静剤を使った"
          },
          {
            "stringId": 2218783478496206848,
            "femaleVariant": "動くな！ 投降しろ！"
          },
          {
            "stringId": 2257598929787265024,
            "femaleVariant": "二手に分かれよう！ 一人の方が身動きが取りやすい！"
          },
          {
            "stringId": 2257604549919744000,
            "femaleVariant": "足手まといだって言いたいの？",
            "maleVariant": "足手まといだって言いたいのか？"
          },
          {
            "stringId": 2258996731255824384,
            "femaleVariant": "いいから行け！ 早く！"
          }
        ]
      }
    }
  }
}