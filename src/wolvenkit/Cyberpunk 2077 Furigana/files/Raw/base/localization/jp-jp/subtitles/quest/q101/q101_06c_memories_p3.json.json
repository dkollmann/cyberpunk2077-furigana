                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "直接話をする",
                    "maleVariant": null,
                    "stringId": 1732959274342182912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人は嘘をつき、自分自身を欺く",
                    "maleVariant": null,
                    "stringId": 1732967691588890624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "相棒にも喋らせてやれよ",
                    "maleVariant": null,
                    "stringId": 1760540066848661508
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よし、もう一度だ",
                    "maleVariant": null,
                    "stringId": 1760540109395681280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そこに立ってる奴は置物か？ 話をしたそうだぞ？",
                    "maleVariant": null,
                    "stringId": 1760544752641204232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "核分裂性物質はどこで手に入れた？",
                    "maleVariant": null,
                    "stringId": 1760544752641204240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大物の登場だな",
                    "maleVariant": null,
                    "stringId": 1760545803733786624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前が所属するテロ組織の名前を言え",
                    "maleVariant": null,
                    "stringId": 1760661241281077248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前の仲間たちは何者だ？",
                    "maleVariant": null,
                    "stringId": 1764773934742921216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "核分裂性物質の入手元は？",
                    "maleVariant": null,
                    "stringId": 1764774215509630976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ボスがお前じゃ力不足だってよ",
                    "maleVariant": null,
                    "stringId": 1764788388985593856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ボスがお前じゃ力不足だってよ",
                    "maleVariant": null,
                    "stringId": 1764797708997517312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でもこの世には、死より残酷な運命がある…",
                    "maleVariant": null,
                    "stringId": 1764803828285726720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "殺すつもりはなかったんだ",
