        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰にも尾行されてないし、見られてもいない。保証する",
              "maleVariant": "",
              "stringId": 1653315916396859392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ど、どうして言い切れる？",
              "maleVariant": "",
              "stringId": 1653315916396859404
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゼータテクに疑われてたら、あんたは今頃死んでるはず",
              "maleVariant": "",
              "stringId": 1653315916396859416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大方、警戒するほどの相手だと思われてないんでしょ",
              "maleVariant": "大方、警戒するほどの相手だと思われてないんだろ",
              "stringId": 1653315916396859428
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "トラックを盗む場所は？",
              "maleVariant": "",
              "stringId": 1653315916430413824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このチップには何が？",
              "maleVariant": "",
              "stringId": 1653315916430413840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてスラスターが必要なの？",
              "maleVariant": "どうしてスラスターが必要なんだ？",
              "stringId": 1653315917923586048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "小さくて買い手が見つかりやすい… それに、いい値段も付く",
              "maleVariant": "",
              "stringId": 1653315917923586060
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "金持ちにとっちゃはした金でも、俺には… 人生が変わるような額だ",
              "maleVariant": "",
              "stringId": 1653315917923586072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はっきり言わせてもらうけど、これはあんたの身に余る仕事よ",
              "maleVariant": "はっきり言わせてもらうが、これはあんたの身に余る仕事だ",
              "stringId": 1653315917957140480
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "目的は？",
              "maleVariant": "",
              "stringId": 1653315917957140492
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その話は… したくない",
              "maleVariant": "",
              "stringId": 1653315917957140504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう、じゃあ聞かない",
              "maleVariant": "そうか、じゃあ聞かない",
              "stringId": 1653315917990694912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいから話して",
              "maleVariant": "いいから話せ",
              "stringId": 1653315917990694928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう、まあいい",
              "maleVariant": "そうか、まあいい",
              "stringId": 1653315919752302592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よかった、ありがとう",
              "maleVariant": "",
              "stringId": 1653315919752302604
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話して。はっきりしないのは嫌いなの",
              "maleVariant": "話せよ。はっきりしないのは嫌いだ",
              "stringId": 1653315919802634240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…俺、ALSなんだ",
              "maleVariant": "",
              "stringId": 1653315919802634264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それとトラック強奪に何の関係が？",
              "maleVariant": "",
              "stringId": 1653315919836188672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何それ？ 深刻そうな話だけど",
              "maleVariant": "何だそれ？ 深刻そうな話だが",
              "stringId": 1653315919852965896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "警備はどうなってる？",
              "maleVariant": "",
              "stringId": 1653315921631350784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "トラックは1ヶ所に30秒以上停まると、自動的に信号を発信する",
              "maleVariant": "",
              "stringId": 1653315921631350796
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最寄りのゼータテク緊急対応チームが出動してくる。装甲車が2、3台来るだろう",
              "maleVariant": "",
              "stringId": 1653315921631350808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スピードが鍵ね",
              "maleVariant": "スピードが鍵か",
              "stringId": 1653315921631350820
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ",
              "maleVariant": "",
              "stringId": 1653315921631350832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "トラックはどこで待てばいい？",
              "maleVariant": "",
              "stringId": 1653315921664905216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケンモア・カフェっていう店の前だ",
              "maleVariant": "",
              "stringId": 1653315921664905228
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "君が着いたら連絡するよ",
              "maleVariant": "",
              "stringId": 1653315921664905240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "トラックの強奪と何の関係が？",
              "maleVariant": "",
              "stringId": 1653315921698459648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "体を替えるのに、金が必要なんだ",
              "maleVariant": "",
              "stringId": 1653315921715236864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "体を変える？",
              "maleVariant": "",
              "stringId": 1653315921715236876
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう、身体交換だ。シベリアのアバカンにいるリパー、ドクター・フェドロフと話したんだ",
              "maleVariant": "",
              "stringId": 1653315921715236888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やってくれるって… しかもナイトシティや東京よりもずっと安く",
              "maleVariant": "",
              "stringId": 1653315921715236900
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ALS？ 初めて聞くけど、何のこと？",
              "maleVariant": "ALS？ 初めて聞くが、何のことだ？",
              "stringId": 1653315921732014080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3ヶ月前までは歩けてた… 3ヶ月後には、寝たきりになる",
              "maleVariant": "",
              "stringId": 1653315921748791300
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "指も動かせず、言葉も話せなくなって…",
              "maleVariant": "",
              "stringId": 1653315921748791324
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一人でクソもできない",
              "maleVariant": "",
              "stringId": 1653315921748791336
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから失うものは何もない、危険を冒してもいいだろうって… 思ったんだ",
              "maleVariant": "",
              "stringId": 1653315921748791348
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのリパーとはどこで知り合ったの？",
              "maleVariant": "そのリパーとはどこで知り合ったんだ？",
              "stringId": 1653315921782345728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "USSRなんて、どうやって行くの？",
              "maleVariant": "USSRなんて、どうやって行くんだよ？",
              "stringId": 1653315921782345744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてそこなら安いの？",
              "maleVariant": "どうしてそこなら安いんだ？",
              "stringId": 1653315921782345760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういうタイプには見えないのに、どうしてこんなことを？",
              "maleVariant": "",
              "stringId": 1653315923376181248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言ったろ、その話はやめてくれ",
              "maleVariant": "",
              "stringId": 1653315923376181260
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、任せて。じゃあね",
              "maleVariant": "ああ、任せろ。じゃあな",
              "stringId": 1653315923527176192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…ありがとう",
              "maleVariant": "",
              "stringId": 1653315923527176204
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "簡単そうね。じゃあ後で",
              "maleVariant": "簡単そうだな。じゃあ後で",
              "stringId": 1653315923577507840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてこんなことを？",
              "maleVariant": "",
              "stringId": 1653315923577507856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "警備はどうなってる？",
              "maleVariant": "",
              "stringId": 1653315923577507872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう一度計画を説明して",
              "maleVariant": "もう一度計画を説明してくれ",
              "stringId": 1653315923594285056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう一度お願い",
              "maleVariant": "もう一度頼む",
              "stringId": 1653315925272006656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "君が待機する交差点に、トラックを停める。俺はシステムに侵入し…",
              "maleVariant": "",
              "stringId": 1653315925272006668
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはトラックに乗って、盗む",
              "maleVariant": "俺はトラックに乗って、盗む",
              "stringId": 1653315925272006680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": 1653315925272006696
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと待ってくれ",
              "maleVariant": "",
              "stringId": 1653315925305561088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ご、ごめん。すぐに済む",
              "maleVariant": "",
              "stringId": 1653315925305561100
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何してるの？",
              "maleVariant": "何してるんだ？",
              "stringId": 1653315925339115520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして待たせるの？",
              "maleVariant": "どうして待たせるんだ？",
              "stringId": 1653315925339115536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "監視カメラをハッキングしてるの？",
              "maleVariant": "監視カメラをハッキングしてるのか？",
              "stringId": 1653315925339115552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何してるの？",
              "maleVariant": "何してるんだ？",
              "stringId": 1653315927083945984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…ハゲは売人、金髪はモーテル暮らし…",
              "maleVariant": "",
              "stringId": 1653315927083945996
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは… ひとり言？",
              "maleVariant": "それは、ひとり言か？",
              "stringId": 1653315927083946008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え？ ああ、ごめん。君が尾行されてないか確認してたんだ。大丈夫らしい",
              "maleVariant": "",
              "stringId": 1653315927100723200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして待たせるわけ？",
              "maleVariant": "どうして待たせるんだよ？",
              "stringId": 1653315927151054848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "た、確かめたいからさ…",
              "maleVariant": "",
              "stringId": 1653315927151054860
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すごいの使ってるね",
              "maleVariant": "すごいの使ってるな",
              "stringId": 1653315927201386496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え、何…？",
              "maleVariant": "",
              "stringId": 1653315927201386508
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その冷却装置につながってるケンダチのマイクロ76",
              "maleVariant": "",
              "stringId": 1653315927201386520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、それ。プロセッサを増設して、システムバスも改良してある",
              "maleVariant": "",
              "stringId": 1653315927218163716
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ご、ごめん。自己紹介してなかった。ペドロだ",
              "maleVariant": "",
              "stringId": 1653315927251718144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゼータテクの輸送車を襲撃するんだって？",
              "maleVariant": "",
              "stringId": 1653315927285272576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "尾行はされてない。保証する",
              "maleVariant": "",
              "stringId": 1653315927285272608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ターゲットは、AVスラスターを運ぶゼータテクの無人輸送車だ",
              "maleVariant": "",
              "stringId": 1653315931009814528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ルートは毎日ランダムに変更される",
              "maleVariant": "",
              "stringId": 1653315931026591748
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうやって待ち伏せるの？",
              "maleVariant": "どうやって待ち伏せる？",
              "stringId": 1653315931026591760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ルート設定のコードを書き換えた。どう変わるかわかってる",
              "maleVariant": "",
              "stringId": 1653315931026591772
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど、ルートが予測できると。それから？",
              "maleVariant": "",
              "stringId": 1653315931026591784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まず俺がトラックを止めて、システムに侵入する",
              "maleVariant": "",
              "stringId": 1653315931026591796
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは何をすれば？",
              "maleVariant": "俺は何をすれば？",
              "stringId": 1653315931026591808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "現場で待機して、トラックに乗り込む。あとは俺の指示に従って盗んでくれ",
              "maleVariant": "",
              "stringId": 1653315931026591820
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうやってフェドロフを見つけたの？ 国際データベースか何か？",
              "maleVariant": "どうやってフェドロフを見つけた？ 国際データベースか何かか？",
              "stringId": 1653315931110477824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、そうさ。当然だろ？",
              "maleVariant": "",
              "stringId": 1653315931110477836
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなの誰でもデータを改ざんできる。詐欺じゃない確証は？",
              "maleVariant": "",
              "stringId": 1653315931110477848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや… 絶対に詐欺じゃない",
              "maleVariant": "",
              "stringId": 1653315931110477860
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして絶対なんて言えるわけ？",
              "maleVariant": "どうして絶対なんて言えるんだ？",
              "stringId": 1653315931110477872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく、スラスターが必要なんだ。頼む… 手を貸してくれ",
              "maleVariant": "",
              "stringId": 1653315931127255048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんた、こういうことするの初めてでしょ？",
              "maleVariant": "あんた、こういうことするの初めてだろ？",
              "stringId": 1653315931144032256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー…",
              "maleVariant": "",
              "stringId": 1653315931144032268
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっぱり…",
              "maleVariant": "やっぱりな…",
              "stringId": 1653315931144032280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも、準備は完璧だ！",
              "maleVariant": "",
              "stringId": 1653315931144032292
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうやってUSSRまで？",
              "maleVariant": "",
              "stringId": 1653315931211141120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゼータテクは空港の警備を強化するはず",
              "maleVariant": "",
              "stringId": 1653315931227918340
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だからまず船でウラジオストクに行って、列車でアバカンへ行く",
              "maleVariant": "",
              "stringId": 1653315931227918352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本気？ 2、3週間… もっとかかるかもしれない",
              "maleVariant": "本気か？ 2、3週間… もっとかかるかもしれない",
              "stringId": 1653315931227918364
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "準備してある",
              "maleVariant": "",
              "stringId": 1653315931244695556
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ご、ごめん。君が尾行されてないか確認してたんだ。大丈夫らしい",
              "maleVariant": "",
              "stringId": 1653359015722586112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、ちゃんと説明して",
              "maleVariant": "おい、ちゃんと説明しろ",
              "stringId": 1653436261950918656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "モーテルのカメラを、いや、なんならシステム全体をハッキングしてるでしょ？",
              "maleVariant": "モーテルのカメラを、いや、なんならシステム全体をハッキングしてるのか？",
              "stringId": 1653474078617866240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ… よし、尾行されてないな",
              "maleVariant": "",
              "stringId": 1653474777539907584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。Vよ",
              "maleVariant": "ああ。Vだ",
              "stringId": 1654632543251959808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エル・キャピタンに言われて来たのか？",
              "maleVariant": "",
              "stringId": 1654632592744747008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エル・キャピタンから仕事があるって聞いたけど",
              "maleVariant": "エル・キャピタンから仕事があるって聞いたんだが",
              "stringId": 1654650200583933952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ… 会社のトラックをちょっと… 借りたいんだ",
              "maleVariant": "",
              "stringId": 1654650200583933964
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分の雇い主から盗むってわけ？",
              "maleVariant": "自分の雇い主から盗むってのか？",
              "stringId": 1654650200583933976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ… そんなとこ。大声で言わないでくれよ",
              "maleVariant": "",
              "stringId": 1654650200583933988
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こういうのは初めて？",
              "maleVariant": "こういうのは初めてか？",
              "stringId": 1654700475206176768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "冷静になった方がいい",
              "maleVariant": "",
              "stringId": 1654700802412220416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大企業にそんな真似をしたらただじゃすまない。相応の覚悟はできてるの？",
              "maleVariant": "大企業にそんな真似をしたらただじゃすまない。相応の覚悟はできてるのか？",
              "stringId": 1654732695195193344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": 1654743888366505984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "命を狙われるよ",
              "maleVariant": "命を狙われるぞ",
              "stringId": 1654744024547168256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わ、わかってる…",
              "maleVariant": "",
              "stringId": 1654744375039987712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手を借りたいなら、言うとおりにして",
              "maleVariant": "手を借りたいなら、言うとおりにしろ",
              "stringId": 1654748323205451776
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで… どんな計画なの？",
              "maleVariant": "それで… どんな計画なんだ？",
              "stringId": 1654771479991390212
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゼータテクにあんたの仕業だとばれる",
              "maleVariant": "",
              "stringId": 1654772890384830464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで… どんな計画なの？",
              "maleVariant": "それで… どんな計画なんだ？",
              "stringId": 1654773626300633088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "怖気づいてない？ やめるなら今のうちよ",
              "maleVariant": "怖気づいちゃいないか？ やめるなら今のうちだ",
              "stringId": 1654804137228210176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今ならまだやめられるけど？",
              "maleVariant": "今ならまだやめられるぞ？",
              "stringId": 1654804616838483968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうすればいいか言ってくれ",
              "maleVariant": "",
              "stringId": 1654805166896287744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "震えてるじゃない。こういう仕事は向かないんじゃないの？",
              "maleVariant": "震えてるじゃないか。こういう仕事は向かないんじゃないのか？",
              "stringId": 1654806852268298240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、大丈夫だ。やれるよ。計画は万全なんだ",
              "maleVariant": "",
              "stringId": 1654807370415837184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕事を終えるまで、冷静でいるの。頭に火がついても慌てないで。いい？",
              "maleVariant": "仕事を終えるまで、冷静でいろ。頭に火がついても慌てるな。いいか？",
              "stringId": 1654809451243606016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだった。今のところ、こんな感じだ",
              "maleVariant": "",
              "stringId": 1654810510439583744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": 1654815985465921536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "簡単そうね。じゃあまた",
              "maleVariant": "簡単そうだな。じゃあまた",
              "stringId": 1654855890007068672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだよな… とにかく忘れないでくれ。ケンモア・カフェの交差点、午後5時だぞ",
              "maleVariant": "",
              "stringId": 1654855890007068684
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え… それだけ？ 他に何も知りたくないのか？",
              "maleVariant": "",
              "stringId": 1654856728213557248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ペドロ、こっちは初めての仕事じゃないのよ",
              "maleVariant": "ペドロ、こっちは初めての仕事じゃないんだ",
              "stringId": 1654857396500402176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゼータテクの捜索プロトコルはわかってる。港が盲点なんだ",
              "maleVariant": "",
              "stringId": 1654942024301121536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心配しないでいい",
              "maleVariant": "心配しなくていい",
              "stringId": 1662212621896880128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして安くすむの？",
              "maleVariant": "どうして安くすむ？",
              "stringId": 1662214816474173440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "契約するつもり？",
              "maleVariant": "契約するつもりか？",
              "stringId": 1662214956765253636
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まともな仕組みとは思えない",
              "maleVariant": "",
              "stringId": 1662215152387592192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなのにサインするつもり？",
              "maleVariant": "そんなのにサインするつもりか？",
              "stringId": 1662215232012259328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直、そんなのまともな手術になるとは思えない",
              "maleVariant": "",
              "stringId": 1662215324354056192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "身体交換がかなり一般的になってきてるらしいんだ",
              "maleVariant": "",
              "stringId": 1662270462238232576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "重工業地帯で、気候も厳しい… 普通の人間の体じゃ… 長く持たないんだ",
              "maleVariant": "",
              "stringId": 1662275127847784448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "裏はないの？",
              "maleVariant": "裏はないのか？",
              "stringId": 1662281689416962048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待機リストに載る為に、「労働力同意書」を提出する義務がある",
              "maleVariant": "",
              "stringId": 1662282052945678336
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうこと？",
              "maleVariant": "どういうことだ？",
              "stringId": 1662282371377238016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "労働契約さ。数年間、地元の鉱山か工場で働くと約束するんだ",
              "maleVariant": "",
              "stringId": 1662282598272307200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうかな。志願者は多いぞ",
              "maleVariant": "",
              "stringId": 1662290667727327232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "代わりに全額前払いする",
              "maleVariant": "",
              "stringId": 1662293555857952768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、そのリパー、 国営のクリニックなのに外国人から裏金を受け取ってるわけ？",
              "maleVariant": "おいおい、そのリパー、 国営のクリニックなのに外国人から裏金を受け取ってるのかよ？",
              "stringId": 1662294101956333568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "国の給付金だけじゃやってけないんじゃないかな",
              "maleVariant": "",
              "stringId": 1662294272932941824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK、任せて",
              "maleVariant": "OK、任せておけ",
              "stringId": 1663176225341149184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜスラスターを盗むの？",
              "maleVariant": "なぜスラスターを盗むんだ？",
              "stringId": 1709858558647308288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "中身は？",
              "maleVariant": "",
              "stringId": 1709860567333048320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "中国製ICEブレイカー。ゼータテクのセキュリティを破れる",
              "maleVariant": "",
              "stringId": 1709862466799112192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うちの… 連中のトラックをハックするのに要る",
              "maleVariant": "",
              "stringId": 1709865734984011776
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ターゲットはこれから数日間、午後5時頃にそこを通り過ぎる",
              "maleVariant": "",
              "stringId": 1709874605198774272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、手伝う",
              "maleVariant": "",
              "stringId": 1745974627052916736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。手伝う",
              "maleVariant": "",
              "stringId": 1745987719405080576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "忘れないでくれ。ケンモア・カフェの交差点、午後5時だ。頼むぞ",
              "maleVariant": "",
              "stringId": 1746020147347689472
            }
          ]
        }
      }
    },
