    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1715518415144230912,
            "femaleVariant": "でも… さらわれた直後はまだ疑っていた"
          },
          {
            "stringId": 1760692181418393600,
            "femaleVariant": "済んだわ。行きましょう"
          },
          {
            "stringId": 1760734771220070400,
            "femaleVariant": "ジョニー… ダメ、今は… やめて！",
            "maleVariant": "ジョニー… ダメだ、今は… やめろ！"
          },
          {
            "stringId": 1761764088704155648,
            "femaleVariant": "お父様"
          },
          {
            "stringId": 1761764752712810496,
            "femaleVariant": "「くたばれ、じじい」――シルヴァーハンドからの伝言よ",
            "maleVariant": "「くたばれ、じじい」――シルヴァーハンドからの伝言だ"
          },
          {
            "stringId": 1761765033244639232,
            "femaleVariant": "あたしに何の用？",
            "maleVariant": "俺に何の用だ？"
          },
          {
            "stringId": 1761765192963735552,
            "femaleVariant": "あんたと話す気はない"
          },
          {
            "stringId": 1761769029325832192,
            "femaleVariant": "お会いできて光栄よ",
            "maleVariant": "お会いできて光栄だ"
          },
          {
            "stringId": 1761770557881204736,
            "femaleVariant": "あたしに何の用？",
            "maleVariant": "俺に何の用だ？"
          },
          {
            "stringId": 1761771222292512768,
            "femaleVariant": "そのとおりよ。クソジジイ！",
            "maleVariant": "そのとおりだ、クソジジイ！"
          },
          {
            "stringId": 1761771565218807808,
            "femaleVariant": "ハッ"
          },
          {
            "stringId": 1761772564369129472,
            "femaleVariant": "誰があんたなんかと話をするか"
          },
          {
            "stringId": 1761773175395336192,
            "femaleVariant": "こんな風にあなたとお会いできるなんて…",
            "maleVariant": "こんな風にあなたとお会いできるとは…"
          },
          {
            "stringId": 1761773726761762816,
            "femaleVariant": "あたしは以前、アラサカ・ナイトシティ支社の防諜に…",
            "maleVariant": "俺は以前、アラサカ・ナイトシティ支社の防諜に…"
          },
          {
            "stringId": 1761783469928628224,
            "femaleVariant": "シルヴァーハンドがよろしくってさ",
            "maleVariant": "シルヴァーハンドがよろしくだとよ"
          },
          {
            "stringId": 1824104557170266112,
            "femaleVariant": "お前は我が社から盗みを働いた"
          },
          {
            "stringId": 1824105142477000704,
            "femaleVariant": "さらには娘の誘拐に加担し――"
          },
          {
            "stringId": 1824105491141103616,
            "femaleVariant": "今度は息子を葬り去ろうとしている"
          },
          {
            "stringId": 1824117826253623296,
            "femaleVariant": "確かにただ者ではない"
          },
          {
            "stringId": 1824123159579594752,
            "femaleVariant": "私のテクノロジーと己の愚行によって身を滅ぼす前のことか"
          },
          {
            "stringId": 1824172674714046464,
            "femaleVariant": "生と死…"
          },
          {
            "stringId": 1824176913259565056,
            "femaleVariant": "私たちの使命は――"
          },
          {
            "stringId": 1824177960426278912,
            "femaleVariant": "その境界を限りなく薄くすることだ"
          },
          {
            "stringId": 1824178288722841600,
            "femaleVariant": "お前はそのプロセスの一部となった。感謝しなくてはな"
          },
          {
            "stringId": 1824212573248950272,
            "femaleVariant": "ハナコの言うとおりだな"
          },
          {
            "stringId": 1824218436953051136,
            "femaleVariant": "この人よ"
          },
          {
            "stringId": 1824245662985621504,
            "femaleVariant": "こんなの聞いてない！",
            "maleVariant": "こんなの聞いてないぞ！"
          },
          {
            "stringId": 1824246559107059712,
            "femaleVariant": "なぜ言ってくれなかったの？",
            "maleVariant": "なぜ言ってくれなかった？"
          },
          {
            "stringId": 1824249633246679040,
            "femaleVariant": "こんなの聞いてない！",
            "maleVariant": "こんなの聞いてないぞ！"
          },
          {
            "stringId": 1824249670257217536,
            "femaleVariant": "せめて先に言ってくれれば…"
          },
          {
            "stringId": 1825862985198735360,
            "femaleVariant": "役員室は最上階よね…",
            "maleVariant": "役員室は最上階だろ…"
          },
          {
            "stringId": 1825863062692696064,
            "femaleVariant": "下に向かってる…"
          },
          {
            "stringId": 1825865485138137088,
            "femaleVariant": "言ったでしょ… 〈神輿〉にはアラサカ・タワーの地下からしかアクセスできない"
          },
          {
            "stringId": 1825865869789368320,
            "femaleVariant": "役員会議に押し入る前に、もう1つやることがあるの"
          },
          {
            "stringId": 1825866592149180416,
            "femaleVariant": "え？"
          },
          {
            "stringId": 1825867694076727296,
            "femaleVariant": "じゃあどうして下に降りてるの？",
            "maleVariant": "じゃあなぜ下に降りてるんだ？"
          },
          {
            "stringId": 1825867824788017152,
            "femaleVariant": "ええ"
          },
          {
            "stringId": 1825868995686699008,
            "femaleVariant": "ったく。なんで最初からすべてを教えてくれないのよ",
            "maleVariant": "ったく。なぜ最初からすべてを教えてくれないんだ"
          },
          {
            "stringId": 1825870677183172608,
            "femaleVariant": "もちろん信頼はしてるわ。だからあなたはここに居るのよ"
          },
          {
            "stringId": 1825874072505815040,
            "femaleVariant": "元々すべてを教えるつもりはないわ"
          },
          {
            "stringId": 1825874324063391744,
            "femaleVariant": "用心深いから"
          },
          {
            "stringId": 1825877092404695040,
            "femaleVariant": "てっきり信頼されてると思ってたけど",
            "maleVariant": "てっきり信頼されてると思ってたが"
          },
          {
            "stringId": 1825899115017277440,
            "femaleVariant": "父が精巧なコピーを作ったのは、オフィスだけじゃない"
          },
          {
            "stringId": 1825901745399533568,
            "femaleVariant": "え？"
          },
          {
            "stringId": 1825906453237338112,
            "femaleVariant": "それで助言を求めたの"
          },
          {
            "stringId": 1825925022780100608,
            "femaleVariant": "V！"
          },
          {
            "stringId": 1825927783135449088,
            "femaleVariant": "行きましょう… 彼女がバカなことをしでかす前に",
            "maleVariant": "行きましょう… 彼がバカなことをしでかす前に"
          },
          {
            "stringId": 1841697679718879232,
            "femaleVariant": "アラサカにずいぶんと恨みがあるようだな"
          },
          {
            "stringId": 1841703054048366592,
            "femaleVariant": "そのはざまに…"
          },
          {
            "stringId": 1841705595041615872,
            "femaleVariant": "ガラスがある"
          },
          {
            "stringId": 1841711470842089476,
            "femaleVariant": "あたしは自分のために行動してる",
            "maleVariant": "俺は自分のために行動してる"
          },
          {
            "stringId": 1841711580330201088,
            "femaleVariant": "感謝されても何にもならない"
          },
          {
            "stringId": 1841712794312773632,
            "femaleVariant": "あたしは自分のためにやってる",
            "maleVariant": "俺は自分のために行動してる"
          },
          {
            "stringId": 1841712875212509184,
            "femaleVariant": "感謝なんてどうでもいい"
          },
          {
            "stringId": 1841713039008468992,
            "femaleVariant": "ガラスの向こう側に行く気なんてない"
          },
          {
            "stringId": 1841723310456311808,
            "femaleVariant": "さて、時間を無駄にするのはよそう"
          },
          {
            "stringId": 1841749823373316096,
            "femaleVariant": "準備はいいぞ、ハナコ"
          },
          {
            "stringId": 1841752511217537024,
            "femaleVariant": "お父様"
          },
          {
            "stringId": 1841755617804292096,
            "femaleVariant": "心配するな。しかるべき代償は払う"
          },
          {
            "stringId": 1841769802772647936,
            "femaleVariant": "ウソでしょ…",
            "maleVariant": "ウソだろ…"
          },
          {
            "stringId": 1841781785127424000,
            "femaleVariant": "誰に？"
          },
          {
            "stringId": 1841781826332266496,
            "femaleVariant": "父よ"
          },
          {
            "stringId": 1843279128821633024,
            "femaleVariant": "大胆さと傲慢さの境界もまた薄いものだ"
          },
          {
            "stringId": 1844422167308931072,
            "femaleVariant": "始めろ"
          },
          {
            "stringId": 1844439463662325760,
            "femaleVariant": "今まで隠していたのには、理由があるの"
          },
          {
            "stringId": 1844440694170456064,
            "femaleVariant": "いいえ… 言えなかったのよ"
          },
          {
            "stringId": 1844450271377100800,
            "femaleVariant": "なんで？",
            "maleVariant": "なぜだ？"
          },
          {
            "stringId": 1844450332328726528,
            "femaleVariant": "私が口外を禁じたからだ"
          },
          {
            "stringId": 1844451479621865472,
            "femaleVariant": "ふざけないで！",
            "maleVariant": "ふざけるな！"
          },
          {
            "stringId": 1844452084172066816,
            "femaleVariant": "V… 無礼な真似はよして"
          },
          {
            "stringId": 1844485967118733312,
            "femaleVariant": "お前を自らの目で見てみたかった"
          },
          {
            "stringId": 1844489113719148544,
            "femaleVariant": "お前の意見は関係ない"
          },
          {
            "stringId": 1844498747179798528,
            "femaleVariant": "あたしはハナコと取引したのよ",
            "maleVariant": "俺はハナコと取引したんだ"
          },
          {
            "stringId": 1844499144732708864,
            "femaleVariant": "ほう。それはどうかな"
          },
          {
            "stringId": 1844499507103797248,
            "femaleVariant": "どうして？",
            "maleVariant": "どうしてだ？"
          },
          {
            "stringId": 1844513504922308608,
            "femaleVariant": "その過ちを… 正しに来たの",
            "maleVariant": "その過ちを… 正しに来たんだ"
          },
          {
            "stringId": 1844516918330789888,
            "femaleVariant": "ごくありふれた人間が、時に重要な役割を担う――運命のいたずらとでも言うべきか"
          },
          {
            "stringId": 1846017255449432064,
            "femaleVariant": "ならば道は誤っていない"
          },
          {
            "stringId": 1896453856763658244,
            "femaleVariant": "[座る]"
          },
          {
            "stringId": 1896458874057211908,
            "femaleVariant": "[立ち上がる] 行こう"
          },
          {
            "stringId": 1912733334420418560,
            "femaleVariant": "待って。ジャッキー・ウェルズと話がしたい",
            "maleVariant": "待て。ジャッキー・ウェルズと話がしたい"
          },
          {
            "stringId": 1912733705096228864,
            "femaleVariant": "誰のこと？"
          },
          {
            "stringId": 1912733891256217600,
            "femaleVariant": "ゴロウの話だと、尋問するためにジャッキーの遺体をアラサカ社に持っていったって。今のを見て、ようやくその意味がわかった"
          },
          {
            "stringId": 1912734187122421760,
            "femaleVariant": "残念だけどそれはまた話が別よ。でもちょっと待って…"
          },
          {
            "stringId": 1912734340751388672,
            "femaleVariant": "ジャッキー・ウェルズ…"
          },
          {
            "stringId": 1951816865171709952,
            "femaleVariant": "待って。ジャッキー・ウェルズと話したい",
            "maleVariant": "待て。ジャッキー・ウェルズと話したい"
          },
          {
            "stringId": 2008157582990843904,
            "femaleVariant": "早くここを出よう"
          },
          {
            "stringId": 2008158247939022848,
            "femaleVariant": "早く行こう"
          },
          {
            "stringId": 2008322358689853440,
            "femaleVariant": "チーカ！ やっと会えたな！",
            "maleVariant": "ブロダー！ やっと会えたな！"
          },
          {
            "stringId": 2008322397562662912,
            "femaleVariant": "ずっと… あんたにお礼を言いたかった",
            "maleVariant": "ずっと… お前に礼を言いたかった"
          },
          {
            "stringId": 2008322403585683456,
            "femaleVariant": "ジャッキー！ 気分はどう？",
            "maleVariant": "ジャッキー！ 気分はどうだ？"
          },
          {
            "stringId": 2008322408534962176,
            "femaleVariant": "ミスティが… ずいぶん寂しがってるよ",
            "maleVariant": "ミスティが… ずいぶん寂しがってるぞ"
          },
          {
            "stringId": 2008322412796375040,
            "femaleVariant": "落ち着きなって、アミーゴ",
            "maleVariant": "落ち着けって、アミーゴ"
          },
          {
            "stringId": 2008322552768688132,
            "femaleVariant": "いいえ",
            "maleVariant": "いや"
          },
          {
            "stringId": 2008322581474504704,
            "femaleVariant": "わかってて見せたのね",
            "maleVariant": "わかってて見せたのか"
          },
          {
            "stringId": 2008322587933732864,
            "femaleVariant": "ハナコ、もう黙ってて",
            "maleVariant": "ハナコ、もう黙っててくれ"
          },
          {
            "stringId": 2008322777835040768,
            "femaleVariant": "黙れ"
          },
          {
            "stringId": 2008323120207687680,
            "femaleVariant": "あんたにお礼を言いたかった",
            "maleVariant": "お前に礼を言いたかった"
          },
          {
            "stringId": 2008323201392635904,
            "femaleVariant": "ミスティが寂しがってる"
          },
          {
            "stringId": 2008323233252569088,
            "femaleVariant": "ジャッキー、落ち着きなって",
            "maleVariant": "ジャッキー、落ち着けって"
          },
          {
            "stringId": 2008325810165837824,
            "femaleVariant": "アフターライフで… 待ってるぜ！"
          },
          {
            "stringId": 2008326252916568064,
            "femaleVariant": "ああ、ミスティ…"
          },
          {
            "stringId": 2008326330108538880,
            "femaleVariant": "なあ、V。その… 悪いんだが。ちょっと、車を借りてもいいか？"
          },
          {
            "stringId": 2008329878103846912,
            "femaleVariant": "ミスティとデートなんだよ… メトロなんかで行ったら、カッコがつかないだろ？"
          },
          {
            "stringId": 2008332149470457856,
            "femaleVariant": "ウェルズ家の男は、道端でのたれ死ぬクセがあるから、無理もねえ"
          },
          {
            "stringId": 2008342351712284672,
            "femaleVariant": "おふくろの言ってたとおり、待ってりゃいつかは、良いことが起こるもんだ！"
          },
          {
            "stringId": 2008349190340407296,
            "femaleVariant": "チップを… あとは、任せたぞ…"
          },
          {
            "stringId": 2008351852079280128,
            "femaleVariant": "あんたのこと、頼りにしてたんだ",
            "maleVariant": "お前のこと、頼りにしてたんだ"
          },
          {
            "stringId": 2008352407992332288,
            "femaleVariant": "ウォッカのロックに、ライムジュースとジンジャービア。そして忘れちゃいけない… 愛情だ"
          },
          {
            "stringId": 2008369670422196224,
            "femaleVariant": "しかし… 大丈夫だ。問題ない、って言えば言うほど、おふくろをダマしてる気がしてな"
          },
          {
            "stringId": 2008377331100348416,
            "femaleVariant": "望み通りの結果は得られた？"
          },
          {
            "stringId": 2008378069683728384,
            "femaleVariant": "だからさっき言ったでしょ。話が別なんだって"
          },
          {
            "stringId": 2012906519069007872,
            "femaleVariant": "信じられない…"
          },
          {
            "stringId": 2012906519069007888,
            "femaleVariant": "ジャッキー！ 気分はどう？",
            "maleVariant": "ジャッキー！ 気分はどうだ？"
          },
          {
            "stringId": 2012910597224341504,
            "femaleVariant": "前にあったこと、何か思い出せる？",
            "maleVariant": "前にあったこと、何か思い出せるか？"
          },
          {
            "stringId": 2012911267876773888,
            "femaleVariant": "いよいよメジャーリーグだ、チーカ！ あのアフターライフの太っちょブラックジーザス、正真正銘の大物、デクスター・デショーンと仕事だぞ！",
            "maleVariant": "いよいよメジャーリーグだ、ブロダー！ あのアフターライフの太っちょブラックジーザス、正真正銘の大物、デクスター・デショーンと仕事だぞ！"
          },
          {
            "stringId": 2012913148921454608,
            "femaleVariant": "ジャッキー… 信じられない！"
          },
          {
            "stringId": 2012915225840132096,
            "femaleVariant": "確かに… 別物だった"
          },
          {
            "stringId": 2012916045272915968,
            "femaleVariant": "では先へ進みましょう"
          },
          {
            "stringId": 2012918795679375360,
            "femaleVariant": "よくわかった。本物じゃない"
          },
          {
            "stringId": 2012927704783724544,
            "femaleVariant": "へっ… 流石は、帝王の息子だな…"
          },
          {
            "stringId": 2066592079971172352,
            "femaleVariant": "これは本物のジャッキーじゃない"
          },
          {
            "stringId": 2252097794813042688,
            "femaleVariant": "座ってて。少し時間をちょうだい"
          }
        ]
      }
    }
  }
}