  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この辺でいいだろ",
                    "maleVariant": null,
                    "stringId": 1504245706121764864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここで何するつもり？",
                    "maleVariant": "ここで何するつもりだ？",
                    "stringId": 1827289967998291968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で、ここで何するつもり？",
                    "maleVariant": "で、ここで何するつもりだ？",
                    "stringId": 1827289968031846404
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ケリーが豪邸に住んでると聞いたんでな。大したもんだ",
                    "maleVariant": null,
                    "stringId": 1827289968048623632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大の仲良しでもなかったんでしょ？",
                    "maleVariant": "大の仲良しでもなかったんだろ？",
                    "stringId": 1827289968786821120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしに彼の豪邸に向かえって？",
                    "maleVariant": "俺にあいつの豪邸に向かえってのか？",
                    "stringId": 1827289968786821136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼には不満があったんじゃないの？",
                    "maleVariant": "あいつには不満があったんじゃないのか？",
                    "stringId": 1827289968820375556
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺が？ まさか。向こうは俺のケツにぶち込むか、バンドから追い出すか迷ってたようだがな",
                    "maleVariant": null,
                    "stringId": 1827289968837152768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で、あたしは家に押しかけて、晩御飯の内容でも聞けばいいわけ？",
                    "maleVariant": "で、俺は家に押しかけて、晩御飯の内容でも聞けばいいのか？",
                    "stringId": 1827289968870707204
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最近の記事によると、あいつは鬱で自殺未遂をしたらしい",
                    "maleVariant": null,
                    "stringId": 1827289968870707216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本気で心配してるの？",
                    "maleVariant": "本気で心配してるのか？",
                    "stringId": 1827289968887484416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここはひとつ、経験者が助言してやろうと思ってな",
                    "maleVariant": null,
                    "stringId": 1827289968887484428
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どっちがよかった？",
                    "maleVariant": null,
                    "stringId": 1827289968921038848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "方向性の違い？",
                    "maleVariant": "方向性の違いか？",
                    "stringId": 1827289968921038864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これ以上は聞かないでおく",
                    "maleVariant": null,
                    "stringId": 1827289968937816064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どっちがよかったの？",
                    "maleVariant": "どっちがよかったんだ？",
                    "stringId": 1827289968971370500
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "方向性の違いってやつ？",
                    "maleVariant": "方向性の違いってやつか？",
                    "stringId": 1827289969004924932
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これ以上は、聞かないでおく",
                    "maleVariant": null,
                    "stringId": 1827289969038479364
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ケリーのヤツ、こっちは竿には興味ねえってのに、おまけに玉までついてねえんだからな",
                    "maleVariant": null,
                    "stringId": 1827289969072033792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あいつは反逆者を気取りつつ、いつも誰かの足を踏んづけないよう必死だった",
                    "maleVariant": null,
                    "stringId": 1827289969072033804
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "特にアラサカのな",
                    "maleVariant": null,
                    "stringId": 1827289969072033816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で、彼の豪邸に向かえって？",
                    "maleVariant": "で、あいつの豪邸に向かえって？",
                    "stringId": 1827289969105588224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "他のメンバーとはうまくやってたの？",
                    "maleVariant": "他のメンバーとはうまくやってたのか？",
                    "stringId": 1827289969105588240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "他のメンバーとはうまくやってたの？",
                    "maleVariant": "他のメンバーとはうまくやってたのか？",
                    "stringId": 1827289969155919876
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺のことを気にしてた奴なんていねえよ",
                    "maleVariant": null,
                    "stringId": 1827289969155919888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナンシーは仕切りたがりで、俺たちをセックスに飢えた間抜けとしか思ってなかった",
                    "maleVariant": null,
                    "stringId": 1827289969155919900
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ま、ヘンリーは実際そうだったしな",
                    "maleVariant": null,
                    "stringId": 1827289969172697096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "デニーを口説こうとしてたが… あいつら2人が同時にシラフでいる瞬間がなくてよ",
                    "maleVariant": null,
                    "stringId": 1827289969172697108
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「ジョニーとして」行きたいってこと？ また体を貸せって？",
                    "maleVariant": "「ジョニーとして」行きたいってことか？ また体を貸せと？",
                    "stringId": 1827314219883450368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうだ。前回もお行儀よくしてただろ？ ご褒美にもう1回、大目に見てくれよ",
                    "maleVariant": null,
                    "stringId": 1827314795862052864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お断りよ。ローグの時は特別だった",
                    "maleVariant": "お断りだ。ローグの時は特別だった",
                    "stringId": 1827321360988000260
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今度はケリーのため。その次は誰なの？",
                    "maleVariant": "今度はケリーのため。その次は誰なんだ？",
                    "stringId": 1831309575914803200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。でも何をする気か教えて",
                    "maleVariant": "わかった。だが何をする気か教えろ",
                    "stringId": 1831310214019436544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お断りよ、絶対ダメ。ローグの時が例外だっただけ",
                    "maleVariant": "絶対に断る。ローグの時が例外だっただけだ",
                    "stringId": 1831315708289241088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。でも何をする気か教えて",
                    "maleVariant": "わかった。だが何をする気か教えろ",
                    "stringId": 1831315981288099840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はあ… 最初はローグ、次はケリー。先に全員分のリストをもらってもいい？",
                    "maleVariant": "最初はローグ、次はケリー。先に全員分のリストをもらってもいいか？",
                    "stringId": 1831321140349128704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "たしかに、ローグの時とは話が別だ。わかってくれると思ったがな",
                    "maleVariant": null,
                    "stringId": 1831322199813541888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "仕方ない。もし気が変わったら教えてくれ",
                    "maleVariant": null,
                    "stringId": 1831339334149361664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バカな真似はしないって。奴の様子を知りたいだけだ",
                    "maleVariant": null,
                    "stringId": 1831362348916166656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その2人だけだ。俺がダチだと思ってて、今でも生きてる奴なんて、そうたくさんいねえからな",
                    "maleVariant": null,
                    "stringId": 1831365468958248960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だからこそケリーと話したいんだ",
                    "maleVariant": null,
                    "stringId": 1831371414635827200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうやって自分だと伝えるの？",
                    "maleVariant": "どうやって自分だと伝えるんだ？",
                    "stringId": 1831380068558721024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、あいつは常に何かしら悩んでた。だから俺たちは、いろいろあっても何とかやれてたのさ",
                    "maleVariant": null,
                    "stringId": 1831380596269273088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "心配するな。ちゃんと様子を見てうまくやる。だがまずは、あの家に潜り込まなきゃな",
                    "maleVariant": null,
                    "stringId": 1831384784667693056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。代わりたくなったら教えて",
                    "maleVariant": "わかった。代わりたくなったら教えろ",
                    "stringId": 1831385400894836736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もし本当に苦しんでるんだとしたら、下手に混乱させない方がいいんじゃない",
                    "maleVariant": "もし本当に苦しんでるんだとしたら、下手に混乱させない方がいいんじゃないか",
                    "stringId": 1831392864189042688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自殺未遂ってどういうこと？",
                    "maleVariant": "自殺未遂ってどういうことだ？",
                    "stringId": 1876197229319704576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自殺未遂ってどういうこと？",
                    "maleVariant": "自殺未遂ってどういうことだ？",
                    "stringId": 1876197961980727296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "素人くせえ話だよ。やっつけにもほどがある",
                    "maleVariant": null,
                    "stringId": 1876218224227143680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの野郎、自分の部屋で頭を撃とうとして、ボディガードにあっさり銃を取り上げられやがったんだ",
                    "maleVariant": null,
                    "stringId": 1876222194353983488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ただの噂かもしれない",
                    "maleVariant": null,
                    "stringId": 1876223930275745792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "かもな。だが確かめたいんだ",
                    "maleVariant": null,
                    "stringId": 1876224983650029568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんな話、どこで聞いたの？",
                    "maleVariant": "そんな話、どこで聞いた？",
                    "stringId": 1908077626076712960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "街のあちこちさ。スクリームシートはあいつの話題で持ち切りだ… そういや、お前はいなかったな",
                    "maleVariant": null,
                    "stringId": 1908077851428278272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大騒ぎした夜のことね。それで、ケリーを訪ねる気？",
                    "maleVariant": "大騒ぎした夜のことだな。それで、ケリーを訪ねる気か？",
                    "stringId": 1908086178312900608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ。またとない機会だからな",
                    "maleVariant": null,
                    "stringId": 1908088208523808768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何があってもお断り。信用できない",
                    "maleVariant": "何があっても断る。信用できない",
                    "stringId": 1913853474725711872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本気で体を渡すと思ったの？",
                    "maleVariant": "本気で体を渡すと思ったのか？",
                    "stringId": 1913858853652156416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ダメよ。信用できない",
                    "maleVariant": "ダメだ。信用できない",
                    "stringId": 1913859278048612352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ダメ元だ。予想通りのリアクションだな、ったく",
                    "maleVariant": null,
                    "stringId": 1913864768946196480
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}