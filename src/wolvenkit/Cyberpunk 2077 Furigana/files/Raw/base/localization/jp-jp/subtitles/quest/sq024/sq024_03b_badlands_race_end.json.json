            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やったね、V！ 勝ったよ！",
                    "maleVariant": null,
                    "stringId": 1782456050122760196
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いい調子よ",
                    "maleVariant": null,
                    "stringId": 1782456050189869060
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ゴールラインまで行って",
                    "maleVariant": null,
                    "stringId": 1782456050240200708
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "運転はどう？",
                    "maleVariant": "運転はどうだ？",
                    "stringId": 1782456050374418436
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "前のドライバーの方がよかった？",
                    "maleVariant": "前のドライバーの方がよかったか？",
                    "stringId": 1782456050458304516
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "...",
                    "maleVariant": null,
                    "stringId": 1782456050458304524
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ…",
                    "maleVariant": null,
                    "stringId": 1782456050458304532
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼はいい夫だったし、最高のドライバーだった",
                    "maleVariant": null,
                    "stringId": 1782456050458304540
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ガレージまで乗ってく？",
                    "maleVariant": null,
                    "stringId": 1782456050575745028
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、お願い",
                    "maleVariant": "ああ、頼むよ",
                    "stringId": 1782456050609299456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "遠慮しとく",
                    "maleVariant": null,
                    "stringId": 1782456050609299472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、ありがと",
                    "maleVariant": "ああ、助かるよ",
                    "stringId": 1782456050626076676
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "せっかくだけど、遠慮しとく",
                    "maleVariant": "せっかくだが、遠慮しとく",
                    "stringId": 1782456050659631108
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしは前のドライバーと比べてどう？",
                    "maleVariant": "俺は前のドライバーと比べてどうだ？",
                    "stringId": 1782456050693185536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、クソ！",
                    "maleVariant": null,
                    "stringId": 1824282514711126016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんとかね",
                    "maleVariant": "なんとかな",
                    "stringId": 1857417701380722688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "決勝に進めそうってこと？",
                    "maleVariant": "決勝に進めそうってことか？",
                    "stringId": 1857418579449876480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そのとおり",
                    "maleVariant": null,
                    "stringId": 1857419021160419328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しょうがないでしょ",
                    "maleVariant": "しょうがないだろ",
                    "stringId": 1857419711609966592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しょうがない？ 負けるレースじゃなかったのに、気休めはよして",
                    "maleVariant": null,
                    "stringId": 1857419806334128128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった",
                    "maleVariant": null,
                    "stringId": 1857428512115396608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "行きましょ",
                    "maleVariant": null,
                    "stringId": 1857428904685473792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしの運転はどう？",
                    "maleVariant": "俺の運転はどうだ？",
                    "stringId": 1857451799813951488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まだまだ甘いところがあるけど、まあ、いいんじゃないかな",
                    "maleVariant": null,
                    "stringId": 1857454113190711296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ、あたしを選んで後悔してない？",
                    "maleVariant": "じゃあ、俺を選んで後悔してないか？",
                    "stringId": 1857474085828825088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今のところは",
                    "maleVariant": null,
                    "stringId": 1857475150561284096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "夫は… どうして亡くなったの？",
                    "maleVariant": "夫は… どうして亡くなったんだ？",
                    "stringId": 1857651099735035908
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "夫が亡くなった理由、聞いてもいい？",
                    "maleVariant": "夫が亡くなった理由、聞いてもいいか？",
                    "stringId": 1857651142533713920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼のためにレースを？",
                    "maleVariant": null,
                    "stringId": 1857651165199732736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "レースは亡くなった夫のために？",
                    "maleVariant": null,
                    "stringId": 1857651246535675904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大体想像はつくでしょ？",
                    "maleVariant": null,
                    "stringId": 1857652459176071168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私たちは去年、決勝まで進んだの。トップ争いに食い込んで…",
                    "maleVariant": null,
                    "stringId": 1857653003428958208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "...",
                    "maleVariant": null,
                    "stringId": 1857653710521503744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "話題を変えようか",
                    "maleVariant": null,
                    "stringId": 1857654124583194624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ",
                    "maleVariant": null,
                    "stringId": 1857655344773337088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "夫はレースを愛してた。マシンの声を聞き、自由自在に操ることができたの",
                    "maleVariant": null,
                    "stringId": 1857656614221385728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハンドルを握れば、車と一体化するみたいだった。インプラントも使わず、生身でね",
                    "maleVariant": null,
                    "stringId": 1857658895285227520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼のためにも勝たないと",
                    "maleVariant": "彼のためにも勝たないとな",
                    "stringId": 1859055615294443520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "頼りにしてる",
                    "maleVariant": null,
                    "stringId": 1859055784123568128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今回の勝者は… Vとクレアだ！",
                    "maleVariant": null,
                    "stringId": 1936238282875736064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "勝者はルイージとマーゴットだ！",
                    "maleVariant": null,
                    "stringId": 1936239383326887936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クソ… ウソだろ！ まさか負けるなんて！",
                    "maleVariant": null,
                    "stringId": 2237461226216099848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいレースだったぞ！ お疲れさん！",
                    "maleVariant": null,
                    "stringId": 2237461226283208712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やったぜ、賭けが当たった！ これで奥さんも喜ぶ",
                    "maleVariant": null,
                    "stringId": 2237461226367094792
                  }
                }
              ]
            }
          }
        }
      }
