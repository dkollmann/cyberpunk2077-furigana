  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Kitè zo li rete poze avek lapè.\" t=\"亡骸に安らかな眠りを\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1502285492643713024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Li te dènye nan nou ki te sonje lavi sou zile a.\" t=\"彼は島の暮らしを知る最後の一人でした\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1502287006602907648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Anvan lanmè yo leve nan kòlè, li te lakay nou.\" t=\"海の怒りに沈んだ、我らが故郷\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1502287489132417024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vだな？ 待っていたぞ",
              "maleVariant": "",
              "stringId": "1502288818693230592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"E nou pa mete konfyans anko sou bagay vini soti nan deyò,\" t=\"そして、これからも外の者、我らの岸を襲う者を…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1531719281211150336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Men kounye a san Michel, yon lòt chanjman ap vini.\" t=\"しかしミシェルを失った今、我々は再び旅に出なければなりません\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1531720130457047040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Nou pase nan lòt bò a, nan Guinee.\" t=\"そう、遥かギニーの地へと赴くのです\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1531721225321385984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Menm jan ak Agwe Arroyo, Michel mennen nou pase tout dlo mal\" t=\"アグウェ・アロヨのように、ミシェルは荒波の向うへと我らを導いてくれました\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1531721288219168768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Bondye avèk ou.\" t=\"皆さんに神のご加護を\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1531722164224724992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたが仲介役？",
              "maleVariant": "あんたが仲介役か？",
              "stringId": "1531724662536736768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんでこの場所を？ 葬式は苦手なの",
              "maleVariant": "なぜこの場所を？ 葬式は苦手だ",
              "stringId": "1531725210044403712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたが仲介役？ いい加減、仕事の詳細を教えてよ",
              "maleVariant": "あんたが仲介役か？ 仕事の詳細を教えてくれ",
              "stringId": "1531727349642760192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰の葬式？ この仕事と関係あるの？",
              "maleVariant": "誰の葬式だ？ 仕事と関係あるのか？",
              "stringId": "1531728127400939520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Bagay vin chanje, fason lavi nou pa ta janm menm bagay la.\" t=\"時代が変わり、かつての暮らしは失われましたが…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1562149150843539456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"E nou pral pran memwa Michel avèk nou.\" t=\"ミシェルとの思い出を携えて\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1562151243079815168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前のためにわざわざ葬式を開くと思うか？",
              "maleVariant": "",
              "stringId": "1562154694102814720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私から話すことできない。詳しいこと… すぐわかる",
              "maleVariant": "",
              "stringId": "1562159252455624704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリジットと話がしたい",
              "maleVariant": "",
              "stringId": "1562161572694265860"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "品定めのつもり？",
              "maleVariant": "品定めのつもりか？",
              "stringId": "1562163129619910656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずは品定め？ 見てからじゃないと話せないってわけ？",
              "maleVariant": "まずは品定めか？ 一度俺を見てからじゃないと話せないと？",
              "stringId": "1562177416862175232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうなのよ？",
              "maleVariant": "どうなんだ？",
              "stringId": "1562184528187166720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリジットは？ ここにいるの？",
              "maleVariant": "ブリジットは？ ここにいるのか？",
              "stringId": "1562191907058421760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "交差点そばのショッピングセンター。そこの肉屋に行け",
              "maleVariant": "",
              "stringId": "1570470761750368256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "プラシドに会えば、すべて教えてくれる",
              "maleVariant": "",
              "stringId": "1570472083509776384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰の葬式なの？",
              "maleVariant": "誰の葬式なんだ？",
              "stringId": "1590783941461479424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんでこの場所を？ 葬式は嫌いなの",
              "maleVariant": "なぜこの場所を？ 葬式は嫌いなんだ",
              "stringId": "1590791270235967488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "恐れるな、エトランジュ。これお前の葬式じゃない",
              "maleVariant": "",
              "stringId": "1590792754314940416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつもの仕事相手、昔からの馴染みだけだ。知らないこと何もない",
              "maleVariant": "",
              "stringId": "1590804374063747072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもお前、違う",
              "maleVariant": "",
              "stringId": "1590846366378430464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いない",
              "maleVariant": "",
              "stringId": "1705653243120783360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Pacifica vin nouvo zile nou an.\" t=\"パシフィカは我らの新たな島となりました\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1770768207023771648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Tout moun isit la te konnen Michel.\" t=\"ミシェルのことはご存じでしょう\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1770792101923328000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Men nou siviv.\" t=\"我々はまだここにいます\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1770793582814629888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これがパシフィカ流のやり方ってわけ？",
              "maleVariant": "これがパシフィカ流のやり方ってわけか",
              "stringId": "1792160735320936448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてわかったの？",
              "maleVariant": "なぜわかった？",
              "stringId": "1792284736328372224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Lakay pèp nou an.\" t=\"我が同胞たちの故郷\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1793624784764211200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"ekraze sou tè nou an. Non.\" t=\"決して信じてはいけません\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1793626122529083392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Nou grandi pi fò andedan.\" t=\"同胞よ、団結するのです\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1793626356705464320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"pou nou rive kay nèf.\" t=\"新たな故郷へと\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1793630464036376576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これがパシフィカ流のやり方なの？",
              "maleVariant": "これがパシフィカ流のやり方なのか？",
              "stringId": "1966405021384593408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何の情報も明かさず仕事をしろって？",
              "maleVariant": "何の情報も明かさず仕事をしろと？",
              "stringId": "1966406189716361216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前の名前わかってる。それで十分だ",
              "maleVariant": "",
              "stringId": "1966406868388302848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてわかったの？ そうか、髪型ね",
              "maleVariant": "なんでわかったんだ？ そうか、髪型だな",
              "stringId": "1966410153853734912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前パシフィカに入ってから、ずっと見張ってた",
              "maleVariant": "",
              "stringId": "1966412078938607616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "迷子にならないように？",
              "maleVariant": "俺が迷子にならないようにか？",
              "stringId": "1966415117762187264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"creo\" o=\"Ou ka di sa.\" t=\"そんなところだ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1966415746270253056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "開ける",
              "maleVariant": "",
              "stringId": "2328546985262555140"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "開ける",
              "maleVariant": "",
              "stringId": "2338568230725066752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[待つ]",
              "maleVariant": "",
              "stringId": "2379698887104417796"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}