              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本当だって。二日酔いにはこれが一番効くぞ",
                    "maleVariant": null,
                    "stringId": 2172190711699189760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ウマミ？",
                    "maleVariant": null,
                    "stringId": 2188357755538345984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そう、ウマミだ！ ったく、記憶が消えたのかな。リパーに見てもらわないと",
                    "maleVariant": null,
                    "stringId": 2188387646128361472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうだな。この前なんか、空港から電話がかかってきて、俺のヘレラ・アウトローが1ヶ月も駐車されたままだって言われたんだ",
                    "maleVariant": null,
                    "stringId": 2188387801787371520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "外付けのを買えよ。俺はそうしたぞ",
                    "maleVariant": null,
                    "stringId": 2188387947363274752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アウトローを持ってるのか？ 待てよ… 誰かがお前のデータを盗んで、買ったとか…",
                    "maleVariant": null,
                    "stringId": 2188388555285696512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハッ！ 違う、出張の時に自分で買ったのを忘れてたんだよ！",
                    "maleVariant": null,
                    "stringId": 2188392561433219072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほら、あれが入ってるんだ。甘味じゃなくて… 酸味でもなくて…",
                    "maleVariant": null,
                    "stringId": 2188406524623556608
                  }
                }
              ]
            }
          }
        }
