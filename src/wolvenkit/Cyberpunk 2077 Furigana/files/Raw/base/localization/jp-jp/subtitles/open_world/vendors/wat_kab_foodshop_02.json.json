    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1824255085739126784,
            "femaleVariant": "[座る] ねえ",
            "maleVariant": "[座る] なあ"
          },
          {
            "stringId": 1824260336521195520,
            "femaleVariant": "ねえ",
            "maleVariant": "なあ"
          },
          {
            "stringId": 1824260336521195532,
            "femaleVariant": "何か出そうか？"
          },
          {
            "stringId": 1824261735841673232,
            "femaleVariant": "エヴリンを探してる"
          },
          {
            "stringId": 1824261735841673248,
            "femaleVariant": "ここは“リジー”って人のお店なの？",
            "maleVariant": "ここは“リジー”って人の店なのか？"
          },
          {
            "stringId": 1824264933881032708,
            "femaleVariant": "気になってたんだけど、ここは“リジー”って人がオーナーの店なの？",
            "maleVariant": "気になってたんだが、ここは“リジー”って人がオーナーの店なのか？"
          },
          {
            "stringId": 1824264933881032720,
            "femaleVariant": "今はもう違う。あまり心温まる話じゃないんだ"
          },
          {
            "stringId": 1824264933897809928,
            "femaleVariant": "よかったら聞かせてよ",
            "maleVariant": "よかったら聞かせてくれ"
          },
          {
            "stringId": 1824264933897809940,
            "femaleVariant": "リジーは実在の人物で、ここをストリップバーとして運営してたんだ"
          },
          {
            "stringId": 1824264933897809952,
            "femaleVariant": "女の子の給料はちゃんと出てたし、身辺警護までついてた。いい店だったんだぜ？"
          },
          {
            "stringId": 1824266017118760960,
            "femaleVariant": "でも？",
            "maleVariant": "だが？"
          },
          {
            "stringId": 1824266017118760976,
            "femaleVariant": "古き良き時代の話… 嫌いじゃない"
          },
          {
            "stringId": 1824266597945978884,
            "femaleVariant": "でも、何かあったのね？",
            "maleVariant": "だが、何かあったんだろ？"
          },
          {
            "stringId": 1824266597962756104,
            "femaleVariant": "ああ。タイガークロウズの連中だよ"
          },
          {
            "stringId": 1824266990264397828,
            "femaleVariant": "古き良き時代の話ってわけか… そういうのは、たいていお酒なしには語れないのよね",
            "maleVariant": "古き良き時代の話ってわけか… そういうのは大抵、酒なしには語れないんだよな"
          },
          {
            "stringId": 1824266990264397840,
            "femaleVariant": "酒がありゃ、ホラ話だって鵜呑みにできる"
          },
          {
            "stringId": 1824266990264397852,
            "femaleVariant": "それで、何があったの？",
            "maleVariant": "それで、何があったんだ？"
          },
          {
            "stringId": 1824266990281175040,
            "femaleVariant": "タイガークロウズがリジーを撃ったんだ"
          },
          {
            "stringId": 1824267643988619268,
            "femaleVariant": "ある時、クロウズの一人が店の女の子を殴ってな。リジーは一切の迷いなくそいつのタマを撃ち抜いた。明くる日、連中が押しかけてきて…"
          },
          {
            "stringId": 1824267643988619280,
            "femaleVariant": "リズは殺された"
          },
          {
            "stringId": 1824267643988619292,
            "femaleVariant": "でも、クロウズは結局この店をモックスに渡したんでしょ？",
            "maleVariant": "だが、クロウズは結局この店をモックスに渡したんだろ？"
          },
          {
            "stringId": 1824267643988619304,
            "femaleVariant": "まあな。モックスは連中と交渉したのさ。幸い混乱は最小限ですんだがな"
          },
          {
            "stringId": 1824267643988619316,
            "femaleVariant": "今のボスはスージー・Q。でも看板は当時のままにしてある"
          },
          {
            "stringId": 1824267644005396484,
            "femaleVariant": "そして今日の繁栄に至るわけだ"
          },
          {
            "stringId": 1824274381064253440,
            "femaleVariant": "どんなメニューがあるの？",
            "maleVariant": "どんなメニューがあるんだ？"
          },
          {
            "stringId": 1824275188568436736,
            "femaleVariant": "どんなメニューが？"
          },
          {
            "stringId": 1824278930374144000,
            "femaleVariant": "[立ち上がる] そろそろ行かなきゃ",
            "maleVariant": "[立ち上がる] そろそろ行かないと"
          },
          {
            "stringId": 1824280338787872768,
            "femaleVariant": "ありがとう。そろそろおいとまする"
          },
          {
            "stringId": 1824330029781102596,
            "femaleVariant": "エヴリン・パーカーを探してる。ここにいる？",
            "maleVariant": "エヴリン・パーカーを探してる。ここにいるか？"
          },
          {
            "stringId": 1824480190780026884,
            "femaleVariant": "カラベラを"
          },
          {
            "stringId": 1824480190780026896,
            "femaleVariant": "話はまとまったか？"
          },
          {
            "stringId": 1824480190780026908,
            "femaleVariant": "まあね",
            "maleVariant": "まあな"
          },
          {
            "stringId": 1824480955015770112,
            "femaleVariant": "エヴリンのことを教えて",
            "maleVariant": "エヴリンのことを教えてくれ"
          },
          {
            "stringId": 1824482411244564484,
            "femaleVariant": "マテオ。エヴリンをどれくらい知ってる？"
          },
          {
            "stringId": 1824482411261341704,
            "femaleVariant": "小耳にはさむ程度には"
          },
          {
            "stringId": 1824482411278118916,
            "femaleVariant": "あのね、そんなんで引き下がるわけないでしょ",
            "maleVariant": "おいおい、そんなんで引き下がるわけないだろ"
          },
          {
            "stringId": 1824483066277408768,
            "femaleVariant": "[賄賂] これで思い出さない？",
            "maleVariant": "[賄賂] これで思い出さないか？"
          },
          {
            "stringId": 1824483066277408784,
            "femaleVariant": "エヴリンはよく来るの？",
            "maleVariant": "エヴリンはよく来るのか？"
          },
          {
            "stringId": 1824483621301268484,
            "femaleVariant": "これでどうかしら？",
            "maleVariant": "これでどうかな？"
          },
          {
            "stringId": 1824483621318045700,
            "femaleVariant": "なんだ、その金でもう1杯飲みたいって意味か？"
          },
          {
            "stringId": 1824484008502636548,
            "femaleVariant": "エヴリンはよくこの店に来るの？",
            "maleVariant": "エヴリンはよくこの店に来るのか？"
          },
          {
            "stringId": 1824484012344619008,
            "femaleVariant": "本人はなんて？"
          },
          {
            "stringId": 1824484016119492608,
            "femaleVariant": "特に何も"
          },
          {
            "stringId": 1824484019961475072,
            "femaleVariant": "ときどき店に来るよ"
          },
          {
            "stringId": 1824484023769903104,
            "femaleVariant": "ジュディに会いに？"
          },
          {
            "stringId": 1824484027477667840,
            "femaleVariant": "かもな"
          },
          {
            "stringId": 1824484031336427520,
            "femaleVariant": "そういう仲なの？",
            "maleVariant": "そういう仲なのか？"
          },
          {
            "stringId": 1824484035346182144,
            "femaleVariant": "なあ、おしゃべりは楽しいが、仕事があるんだ"
          },
          {
            "stringId": 1824484880666210304,
            "femaleVariant": "話はこれくらいで"
          },
          {
            "stringId": 1824484880666210320,
            "femaleVariant": "まだ聞きたいことが"
          },
          {
            "stringId": 1824485146064990212,
            "femaleVariant": "じゃあ、話はここまでね",
            "maleVariant": "じゃあ、話はここまでだな"
          },
          {
            "stringId": 1824485154940137472,
            "femaleVariant": "それじゃ"
          },
          {
            "stringId": 1824485481877745664,
            "femaleVariant": "まだ、他にも聞きたいことがある"
          },
          {
            "stringId": 1824485490383794176,
            "femaleVariant": "ここの常連のことを根掘り葉掘り調べたきゃ、直接聞きな。それがルールだ"
          },
          {
            "stringId": 1824485498755624960,
            "femaleVariant": "わかった、そうする"
          },
          {
            "stringId": 1824485506842243072,
            "femaleVariant": "ああ。じゃあな"
          }
        ]
      }
    }
  }
}