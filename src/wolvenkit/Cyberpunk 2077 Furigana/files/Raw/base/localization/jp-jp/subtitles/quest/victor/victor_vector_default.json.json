    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1184957046507229184,
            "femaleVariant": "ガードを下げるな、バカ"
          },
          {
            "stringId": 1184961580616962064,
            "femaleVariant": "どっちが勝ってるの？",
            "maleVariant": "どっちが勝ってる？"
          },
          {
            "stringId": 1184963401129447424,
            "femaleVariant": "ヘルナンデスだ。ったく… 昔だったら、マローンはとっくにノックアウトされてるってのに"
          },
          {
            "stringId": 1184964313793220608,
            "femaleVariant": "昔のほうがよかった？",
            "maleVariant": "昔のほうがよかったか？"
          },
          {
            "stringId": 1184964470928625664,
            "femaleVariant": "金でも賭けてるの？",
            "maleVariant": "金でも賭けてるのか？"
          },
          {
            "stringId": 1184965104805400576,
            "femaleVariant": "昔のほうがよかった？",
            "maleVariant": "昔のほうがよかったか？"
          },
          {
            "stringId": 1184967539330437120,
            "femaleVariant": "賭けてるの？",
            "maleVariant": "賭けてるのか？"
          },
          {
            "stringId": 1184970510726385664,
            "femaleVariant": "今だ、打て！"
          },
          {
            "stringId": 1184973243315441664,
            "femaleVariant": "んー…"
          },
          {
            "stringId": 1184991324473446400,
            "femaleVariant": "いや。見てるだけだ。他に趣味もないからな"
          },
          {
            "stringId": 1218604053171998720,
            "femaleVariant": "なんでだよ…"
          },
          {
            "stringId": 1218604116086558736,
            "femaleVariant": "ジャブを打て！ そっちじゃねえ、左だ！"
          },
          {
            "stringId": 1218608514988707840,
            "femaleVariant": "今じゃ地方戦レベルの選手ですら、頭蓋骨を強化してやがる… 衝撃の75%も吸収して何がボクシングだ"
          },
          {
            "stringId": 1648784590689685504,
            "femaleVariant": "あいつの姿が見えるのはどんなカラクリ？",
            "maleVariant": "あいつの姿が見えるのはどんなカラクリだ？"
          },
          {
            "stringId": 1648786331309068288,
            "femaleVariant": "ジョニーの記憶が見えるのはなぜ？",
            "maleVariant": "ジョニーの記憶が見えるのはなぜだ？"
          },
          {
            "stringId": 1648787840855855104,
            "femaleVariant": "脳を共有してるからさ"
          },
          {
            "stringId": 1651688960451727360,
            "femaleVariant": "ジョニーに殺されかけたの",
            "maleVariant": "ジョニーに殺されかけたんだ"
          },
          {
            "stringId": 1651692791445114880,
            "femaleVariant": "もう行かないと。またね",
            "maleVariant": "もう行かないと。またな"
          },
          {
            "stringId": 1651710164520718336,
            "femaleVariant": "いつでも来てくれ"
          },
          {
            "stringId": 1651760365960744960,
            "femaleVariant": "何だと！？"
          },
          {
            "stringId": 1651761245338521600,
            "femaleVariant": "あいつを抑え込む方法はない？ 殺されるのはご免よ",
            "maleVariant": "あいつを抑え込む方法はないのか？ 殺されるのはご免でね"
          },
          {
            "stringId": 1651761867320250368,
            "femaleVariant": "…体の制御を渡さない限り、大した危害は加えられないはずだ。無論、時間が経てばそれも難しくなると思うが"
          },
          {
            "stringId": 1651799863486631936,
            "femaleVariant": "そもそもその生体チップは、コンストラクトと話せるようにするのが目的で作られた代物だ。つまり、正しく機能してるってことさ"
          },
          {
            "stringId": 1720034429371994112,
            "femaleVariant": "あいつはお前さんの知覚情報や、一部の記憶にアクセスできる。お前さん側にも同じことが言えるんだ"
          },
          {
            "stringId": 1720122918079877120,
            "femaleVariant": "シルヴァーハンドとチップについて聞きたいんだけど",
            "maleVariant": "シルヴァーハンドとチップについて聞きたいんだが"
          },
          {
            "stringId": 1720123106639007744,
            "femaleVariant": "専門外だがいいだろう。わかる範囲で答えよう"
          },
          {
            "stringId": 1805307096567279620,
            "femaleVariant": "ジョニーとチップの件について話がしたい"
          },
          {
            "stringId": 1805318553593868288,
            "femaleVariant": "あいつの記憶が見えるのはなぜ？",
            "maleVariant": "あいつの記憶が見えるのはなぜだ？"
          },
          {
            "stringId": 1805325793264562176,
            "femaleVariant": "またね",
            "maleVariant": "またな"
          },
          {
            "stringId": 1805326137147158528,
            "femaleVariant": "じゃあね",
            "maleVariant": "じゃあな"
          },
          {
            "stringId": 1825872447399030784,
            "femaleVariant": "インプラントの交換を考えてる"
          },
          {
            "stringId": 1825873119913734148,
            "femaleVariant": "クロームを変えようと思ってるんだけど。何か新しいのある？",
            "maleVariant": "クロームを変えようと思ってる。何か新しいのあるか？"
          },
          {
            "stringId": 1825873119913734160,
            "femaleVariant": "見てみな"
          },
          {
            "stringId": 1873255060895592448,
            "femaleVariant": "借りを返したい"
          },
          {
            "stringId": 1873256386715086848,
            "femaleVariant": "借りてる分を返せるだけのエディーがやっと集まった"
          },
          {
            "stringId": 1873257032268804096,
            "femaleVariant": "念のために取っておけよ。お前さんのほうが必要だろ"
          },
          {
            "stringId": 1956255809570103296,
            "femaleVariant": "またな、V"
          },
          {
            "stringId": 1983660917748424704,
            "femaleVariant": "墓まで持ってくつもりはないよ、ヴィクター。ほら"
          },
          {
            "stringId": 1983661206853410816,
            "femaleVariant": "お金も受け取らずにいろいろやってくれて… ほんとありがとう"
          },
          {
            "stringId": 1983661958523019264,
            "femaleVariant": "いいんだ。友達だろ"
          },
          {
            "stringId": 1995565027464601600,
            "femaleVariant": "ジャッキーの遺体は… あんたが？"
          },
          {
            "stringId": 1995565954489671680,
            "femaleVariant": "ん… 知ってるものと思ってたが… クソッ、気を失ってる時に話してたか"
          },
          {
            "stringId": 1995566141287194624,
            "femaleVariant": "ど、どういうこと？",
            "maleVariant": "ど、どういうことだ？"
          },
          {
            "stringId": 1995566658277105664,
            "femaleVariant": "アラサカが… 連中が現れて、かっさらってった"
          },
          {
            "stringId": 1995570386073837568,
            "femaleVariant": "え！？ どこへ連れてったの？",
            "maleVariant": "え！？ どこへ連れてった？"
          },
          {
            "stringId": 1995570561899061248,
            "femaleVariant": "わからん。手を頭の上にやり、下を向かされてたからな。少しでも何か掴めないかと思ったが、やはり連中は脇が硬い"
          },
          {
            "stringId": 1995573467729649664,
            "femaleVariant": "クソコーポめ"
          },
          {
            "stringId": 1995621741467557888,
            "femaleVariant": "ジャッキーの遺体はあんたが処理を？"
          },
          {
            "stringId": 1995624622350426112,
            "femaleVariant": "そしたらどうすれば？"
          },
          {
            "stringId": 1995624718970413056,
            "femaleVariant": "手遅れになるまで悪化させなければいい。なんとかしてシルヴァーハンドを取り除く方法を見つけろ"
          },
          {
            "stringId": 1995625827608846336,
            "femaleVariant": "アラサカのおかげってわけね",
            "maleVariant": "アラサカのおかげってわけか"
          },
          {
            "stringId": 1995626137400139776,
            "femaleVariant": "賭けてもいいが、お前さんは〈Relic〉所有者のなかで一番の貧乏人だろうな"
          },
          {
            "stringId": 1995631632273924096,
            "femaleVariant": "しばらくしたら、区別がつかなくなるだろうな"
          },
          {
            "stringId": 1995633727714648064,
            "femaleVariant": "あいつがあたしを殺そうとしてきたら、どうすればいい？",
            "maleVariant": "あいつが俺を殺そうとしてきたら、どうすればいい？"
          },
          {
            "stringId": 1995634306176278528,
            "femaleVariant": "どうしてあたしにはあいつの姿が見えるの？",
            "maleVariant": "どうして俺にはあいつの姿が見えるんだ？"
          },
          {
            "stringId": 1995635268232175616,
            "femaleVariant": "もう行かないと"
          },
          {
            "stringId": 2009726921611960320,
            "femaleVariant": "仕事は順調？",
            "maleVariant": "仕事は順調か？"
          },
          {
            "stringId": 2009727931164155904,
            "femaleVariant": "繁盛してる？",
            "maleVariant": "繁盛してるか？"
          },
          {
            "stringId": 2009728380005015552,
            "femaleVariant": "それなりにな。戦争が落ち着いて、アラサカはまた傭兵を雇いだしてる… で、そいつらにはピカピカの最新インプラントが必要ってわけだ"
          },
          {
            "stringId": 2009731134136016896,
            "femaleVariant": "ミスティがいて良かったね。あんたの愚痴も聞き流してくれる",
            "maleVariant": "ミスティがいて良かったな。あんたの愚痴も聞き流してくれる"
          },
          {
            "stringId": 2009731607656161280,
            "femaleVariant": "ああ。あいつに店を貸したのは正解だったよ"
          },
          {
            "stringId": 2009731994219995136,
            "femaleVariant": "ナイトシティでの暮らしに満足してる奴なんて、あんたぐらいのもんじゃない？",
            "maleVariant": "ナイトシティでの暮らしに満足してる奴なんて、あんたぐらいのもんじゃないか？"
          },
          {
            "stringId": 2009733986011078656,
            "femaleVariant": "ある程度の年になると、幻想を捨てるようになるんだ。ずいぶん生き易くなるぞ"
          },
          {
            "stringId": 2019842418322599936,
            "femaleVariant": "どっちが勝ってるの？",
            "maleVariant": "どっちが勝ってる？"
          }
        ]
      }
    }
  }
}