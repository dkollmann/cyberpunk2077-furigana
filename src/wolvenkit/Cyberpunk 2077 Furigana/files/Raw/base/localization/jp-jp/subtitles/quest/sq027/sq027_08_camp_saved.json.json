  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今すぐキャンプへ戻れ",
              "maleVariant": "",
              "stringId": "1360357881718788112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんな――",
              "maleVariant": "",
              "stringId": "1402699551926325248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんな、ありがとう。ちょっと気持ちを落ち着けたい",
              "maleVariant": "",
              "stringId": "1804367676521074688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一緒に来て",
              "maleVariant": "",
              "stringId": "1804367999516037120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。おかげで連中は叩き潰せたよ",
              "maleVariant": "",
              "stringId": "1807122718222782464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナム、話がある",
              "maleVariant": "",
              "stringId": "1807123154111631360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちはクランの一員だ。それぞれがクランにとって何が最善かを考えなくてはならない…",
              "maleVariant": "",
              "stringId": "1807172676812607488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソウル、私に出てってほしいならそう言いなよ",
              "maleVariant": "",
              "stringId": "1807174123713269760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少なくとも、あんたの話を聞かずに済む",
              "maleVariant": "",
              "stringId": "1807179626874998784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪いが、もう少し我慢してもらおう",
              "maleVariant": "",
              "stringId": "1807185207748792320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜなら今日この日から、お前はアルデカルドスを率いるからだ。俺とともにな",
              "maleVariant": "",
              "stringId": "1807198217741602816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こういう話はきちんとしたかったが、まあいい",
              "maleVariant": "",
              "stringId": "1807198851249278976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悔しいが、認めるよ。お前は正しかった",
              "maleVariant": "",
              "stringId": "1807199135673421824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今回だけと思いたいが",
              "maleVariant": "",
              "stringId": "1807199767671148544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これは誰の決断？",
              "maleVariant": "これは誰の決断だ？",
              "stringId": "1807202533596532740"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今、何て？",
              "maleVariant": "",
              "stringId": "1807202839143190528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムはクランを救った",
              "maleVariant": "",
              "stringId": "1807203979188572160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これはあんたが決めたの？ それともクランで？",
              "maleVariant": "これはあんたが決めたのか？ それともクランで？",
              "stringId": "1807204383435591680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "水を差すな",
              "maleVariant": "",
              "stringId": "1807205022597189632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺はパナムを支持する、少なくとも今はな",
              "maleVariant": "",
              "stringId": "1807206329861083136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムは今日、クランを救った。みんなの命をね",
              "maleVariant": "パナムは今日、クランを救った。みんなの命をな",
              "stringId": "1807208056672817152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見事だった。これからはより活躍してもらう",
              "maleVariant": "",
              "stringId": "1807208889510596608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わ、わかったよ… えっと… 変えなきゃいけないことがたくさんあるね",
              "maleVariant": "",
              "stringId": "1807210412210720768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。まずは急いでキャンプの場所を移す必要がある",
              "maleVariant": "",
              "stringId": "1807211344503836672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだね。ルートを考える",
              "maleVariant": "",
              "stringId": "1807211950496878592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミリテクの奴らに見つかる前にな",
              "maleVariant": "",
              "stringId": "1807216006556618752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む] 終わってみるとさみしいもんね",
              "maleVariant": "[飲む] 終わってみるとさみしいもんだな",
              "stringId": "1807228176447115264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む] バシリスクでのことだけど…",
              "maleVariant": "[飲む] バシリスクでのことだが…",
              "stringId": "1807228630707015680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む] これからどうするつもり？",
              "maleVariant": "[飲む] これからどうするつもりだ？",
              "stringId": "1807228963684421632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は絶対に反対だからな",
              "maleVariant": "",
              "stringId": "1886628842700558336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おめでとう、パナム",
              "maleVariant": "",
              "stringId": "1886629659952304128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もうどうなってんの…",
              "maleVariant": "",
              "stringId": "1894187775896080384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "猛スピードで迫ってくる列車をギリギリで避けた気分",
              "maleVariant": "",
              "stringId": "1894189366007058432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの時――バシリスクでのこと、後悔してないよね",
              "maleVariant": "あの時――バシリスクでのこと、後悔してないよな",
              "stringId": "1894197344059805696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これからどうする？",
              "maleVariant": "",
              "stringId": "1894199170259767296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直、今は何も思いつかない。さっぱりね",
              "maleVariant": "",
              "stringId": "1894199721978515456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "蓋を開けたら、ソウルも悪い人じゃなかったってことね",
              "maleVariant": "蓋を開けたら、ソウルも悪い奴じゃなかったってことだな",
              "stringId": "1894200965774200832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "後悔してるのは、時間がなかったことだけよ",
              "maleVariant": "",
              "stringId": "1895053099902193664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "油断はしないけど",
              "maleVariant": "",
              "stringId": "1895093596544847872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真面目に聞いて。アルデカルドスのみんなが感謝してる。私たちは、いつでもあんたの味方だよ",
              "maleVariant": "",
              "stringId": "1895100129039110144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは頼もしいね",
              "maleVariant": "それは頼もしいな",
              "stringId": "1947299147600752640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰もいないね。ちょうどいい",
              "maleVariant": "",
              "stringId": "1947304251095642112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ビール？ レモネード？",
              "maleVariant": "",
              "stringId": "1947304713207279616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[寄りかかる] ビールにする",
              "maleVariant": "",
              "stringId": "1947306557878956036"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[寄りかかる] レモネードでいい",
              "maleVariant": "",
              "stringId": "1947306644902375424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ビールで",
              "maleVariant": "",
              "stringId": "1947308660215115776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レモネードでいい",
              "maleVariant": "",
              "stringId": "1947308754788282368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はいよ",
              "maleVariant": "",
              "stringId": "1947313141124296704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッピーエンドに",
              "maleVariant": "",
              "stringId": "1947313530640920576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッピーエンドって言えば…",
              "maleVariant": "",
              "stringId": "1947315458057826304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "危なかったね",
              "maleVariant": "危なかったな",
              "stringId": "1947317183779041280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vがいなかったら、きっとみんな死んでた",
              "maleVariant": "",
              "stringId": "1947331508585492480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何言ってんの",
              "maleVariant": "何言ってんだ",
              "stringId": "1947332095687389184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "終わってみるとさみしいもんね",
              "maleVariant": "終わってみるとさみしいもんだな",
              "stringId": "1947339401762304000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私も",
              "maleVariant": "",
              "stringId": "1947360021078208512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもさ… これでお別れだなんて誰も言ってない",
              "maleVariant": "",
              "stringId": "1947360586101288960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、ほら… 周りを見なよ",
              "maleVariant": "",
              "stringId": "1947362075918069760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだね",
              "maleVariant": "",
              "stringId": "1947363283357528064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その点は、埋め合わせが必要かな",
              "maleVariant": "その点は、埋め合わせが必要だな",
              "stringId": "1947363446029414400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、V… これからはさ… 毎日… 一緒にいることもできるんじゃない？",
              "maleVariant": "",
              "stringId": "1947364381258543104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このまま… キャンプに残ってよ",
              "maleVariant": "",
              "stringId": "1947365387841171456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんなで暮らしてみない？",
              "maleVariant": "",
              "stringId": "1947366248243916800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは、どういう――",
              "maleVariant": "",
              "stringId": "1947366548052766720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうしたいけど無理",
              "maleVariant": "そうしたいが無理だ",
              "stringId": "1947367413706780676"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしには合わないよ",
              "maleVariant": "俺には合わないよ",
              "stringId": "1947369350015610880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう簡単にはいかない",
              "maleVariant": "",
              "stringId": "1947370572755890176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナム、あたしもそうしたいと思ってる",
              "maleVariant": "パナム、俺もそうしたいと思ってる",
              "stringId": "1947371468826996736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こういう生き方は、あたし向けじゃない",
              "maleVariant": "こういう生き方は、俺向けじゃない",
              "stringId": "1947371532865630208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう簡単にはいかないよ",
              "maleVariant": "そう簡単にはいかないんだ",
              "stringId": "1947371624184016896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうせ… “でも”、が続くんでしょ",
              "maleVariant": "",
              "stringId": "1947374172475355136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも… 今は無理なの。先にやらなきゃいけないことがある",
              "maleVariant": "でも… 今は無理なんだ。先にやらなきゃいけないことがある",
              "stringId": "1947374404705579008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一度やって、上手くいかなかった",
              "maleVariant": "",
              "stringId": "1947376767474462720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしにこういう暮らしは無理",
              "maleVariant": "俺にこういう暮らしは無理だ",
              "stringId": "1947376841763975168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だからって、今回も同じようになるとは限らない",
              "maleVariant": "",
              "stringId": "1947377484448788480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなのわからないでしょ",
              "maleVariant": "",
              "stringId": "1947378274454335488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いえ、あたしにはわかるの",
              "maleVariant": "いや、俺にはわかるんだ",
              "stringId": "1947378582081368064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vが望むか否か。それだけでしょ？",
              "maleVariant": "",
              "stringId": "1947381196944699392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこが問題なの。あたしの望みは… 関係ない",
              "maleVariant": "そこが問題なんだ。俺の望みは… 関係ない",
              "stringId": "1947382060484784128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ごめん、V。こんなことを言うタイミングじゃなかったね",
              "maleVariant": "",
              "stringId": "1947382824871522304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私はただ… いや、今のは忘れて",
              "maleVariant": "",
              "stringId": "1947383619004264448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] 考えておくよ。約束する",
              "maleVariant": "",
              "stringId": "1947384405570482180"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] もう行かないと",
              "maleVariant": "",
              "stringId": "1947392609192898560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "考えておくよ。約束す――",
              "maleVariant": "",
              "stringId": "1947394255121674240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃあね、パナム。また――",
              "maleVariant": "それじゃあな、パナム。また――",
              "stringId": "1947394329293746176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] キャンプの移動が済んだら知らせて",
              "maleVariant": "[立ち上がる] キャンプの移動が済んだら知らせてくれ",
              "stringId": "1947396178025508864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "キャンプの移動が済んだら――",
              "maleVariant": "",
              "stringId": "1947396602539405312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも… 私は散々ヘマをした… ソウルも言ってたじゃない…",
              "maleVariant": "",
              "stringId": "1979539369352777728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "考えが変わったんだ。お前は危険も顧みずファミリーのために動いてくれた。次の日には追い出されるかもしれないのにな",
              "maleVariant": "",
              "stringId": "1979539972778905600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む]",
              "maleVariant": "",
              "stringId": "2162488503826673668"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、飲み物は？ どっちにする？",
              "maleVariant": "",
              "stringId": "2227211046882983936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バシリスクはさすがだったな。敵を木っ端みじんに… ぐあっ！",
              "maleVariant": "",
              "stringId": "2227218344938389504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、動かないで。いま傷を診てるんだから",
              "maleVariant": "",
              "stringId": "2227249210888634368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく、これで連中も懲りただろうな。何人殺したんだ？",
              "maleVariant": "",
              "stringId": "2227258059142021120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それより、失った仲間の数の方が問題だ",
              "maleVariant": "",
              "stringId": "2227277545844850688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バシリスクがもっと早く来てれば… もっと被害を抑えられたはず",
              "maleVariant": "",
              "stringId": "2227278797425164288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムはいざというときにキャンプを留守にしたんだ… きちんと説明してもらわないと",
              "maleVariant": "",
              "stringId": "2227284814288470016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムは悪くないだろ。こんなことになるなんて知る術がなかった",
              "maleVariant": "",
              "stringId": "2227286193912496128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、こいつはもう大丈夫だろう。ムーサの傷の方がひどいんだ",
              "maleVariant": "",
              "stringId": "2227296339732545536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[降りる]",
              "maleVariant": "",
              "stringId": "2412725381476151300"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}