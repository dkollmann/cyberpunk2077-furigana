            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この締め付けてくる根は何か… こんな石のようなクズからどんな枝が生えるというのか？",
                    "maleVariant": null,
                    "stringId": 1973558502138880000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人間の子よ… お前には語ることも想像することもできない… 壊れた像の山しか知らないのだから…",
                    "maleVariant": null,
                    "stringId": 1973813572860190720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "日差しが照りつけ… 朽ちた木には日陰もなく… コオロギは安らぎをもたらしてはくれず… 乾いた石からは… 水の音も聞こえてこない…",
                    "maleVariant": null,
                    "stringId": 1973814796120563712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "唯一… ゲホッ、ゲホッ！ 赤い岩の下には影がある…",
                    "maleVariant": null,
                    "stringId": 1973816109055156224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあ、この赤い岩の影に入れ！",
                    "maleVariant": null,
                    "stringId": 1973818192600834048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうすれば見せてあげよう… 朝に後ろからついてくる影でも… 夕方に迎えにくる影でもないものを！",
                    "maleVariant": null,
                    "stringId": 1973818876305940480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一掴みの塵で恐怖を見せてあげよう！",
                    "maleVariant": null,
                    "stringId": 1973819968485924864
                  }
                }
              ]
            }
          }
        }
      }
    },
