  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ずっと弾いてると指が痛くなる。痛くない弾き方ってある？",
              "maleVariant": "",
              "stringId": "1264623330770571264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこへだ！？",
              "maleVariant": "",
              "stringId": "1264632307822645248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、そんなムキになるなよ、チューマ。スティーブにはちょっと仕事を頼んでるだけだ。2時間もかからない",
              "maleVariant": "",
              "stringId": "1264658413623726080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ないな。指で鉄線を絞って音を出してんだ。痛みばっかりはどうしようもない",
              "maleVariant": "",
              "stringId": "1264669263247986688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そっか…",
              "maleVariant": "",
              "stringId": "1264669306315100160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつがどこで何するかは、俺の決めることだ。お前じゃねえ",
              "maleVariant": "",
              "stringId": "1264680617681899520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "タダ働きさせるわけじゃない",
              "maleVariant": "",
              "stringId": "1315354781185433600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー、ちょっと… その辺だよ。すぐ戻る",
              "maleVariant": "",
              "stringId": "1315402121522929664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うちの中に戻れ！ 今すぐだ！ 隙ありゃフラフラ遊びに行きやがって！",
              "maleVariant": "",
              "stringId": "1315402527212789760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また車の鍵を持ち出したな？",
              "maleVariant": "",
              "stringId": "1315403791074025472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バカ野郎！ うちの中に戻れ！",
              "maleVariant": "",
              "stringId": "1315404697815441408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "車が要るならてめえで買え！ 俺の息子には近寄るな！",
              "maleVariant": "",
              "stringId": "1315405818046930944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、お前に言ってんだよ！",
              "maleVariant": "",
              "stringId": "1315408705003151360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なにジロジロ見てる？ 文句でもあんのか？",
              "maleVariant": "",
              "stringId": "1315409662009106432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うちの中に戻れっつってんだよ！ 聞こえねえのか！？",
              "maleVariant": "",
              "stringId": "1315409694187806720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心配すんな。ずっと続けてりゃ、そのうち指の皮が分厚くなって、いくら弾いても痛まなくなる",
              "maleVariant": "",
              "stringId": "1315428532770004992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コーティング弦を買うつもりでさ",
              "maleVariant": "",
              "stringId": "1315434007024590848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クズの分際で、そいつに手をだすな",
              "maleVariant": "",
              "stringId": "1316813986744459264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何の用だ？",
              "maleVariant": "",
              "stringId": "1319638694997221376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お金を貯めてたんだけど、母さんが家のことで困ってたから渡しちゃった",
              "maleVariant": "",
              "stringId": "1319750816947200000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この野郎、人ん家の前でエラそうにしやがって！ お前みたいなクズにびびるとでも思ってんのか！？",
              "maleVariant": "",
              "stringId": "1695406297327558656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2時間だぞ。帰ってきたらとっとと俺に金を寄こせよ、スティーブ",
              "maleVariant": "",
              "stringId": "1695414508449390592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかったよ",
              "maleVariant": "",
              "stringId": "1695416261450358784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知るかよ。どうせホームレスかなんかだろ",
              "maleVariant": "",
              "stringId": "1793855822500003840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スパイかもな",
              "maleVariant": "",
              "stringId": "1793896368266346496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ捕まえてみろよ、親父。できないくせに",
              "maleVariant": "",
              "stringId": "1793969344860979200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "立ってるのもやっとなんだから",
              "maleVariant": "",
              "stringId": "1793973015531290624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "酔っ払いと一緒のゴミ捨て場じゃ、スティーブが家に居たがらないのも無理ないな",
              "maleVariant": "",
              "stringId": "1793997365059846144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ？ お前んちはマシだってのか？",
              "maleVariant": "",
              "stringId": "1794001126863994880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "息子を引き留めたきゃ、まともな家庭を築け",
              "maleVariant": "",
              "stringId": "1794132823848845312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そいつに手をだすな",
              "maleVariant": "",
              "stringId": "1794133700223500288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着け。金なら払う",
              "maleVariant": "",
              "stringId": "1794134164331626496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、いったい何してんだ？",
              "maleVariant": "",
              "stringId": "1795642290893553664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "出かけんだ",
              "maleVariant": "",
              "stringId": "1795648206086930432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やるのか？ かかって来いよ",
              "maleVariant": "",
              "stringId": "1802526930682052608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありゃ誰だ？",
              "maleVariant": "",
              "stringId": "1805179443394707456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行くよ。じゃあね",
              "maleVariant": "",
              "stringId": "1805594233750118400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うちに子供はいないが――",
              "maleVariant": "",
              "stringId": "1806632671429484544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この調子じゃ、あんたんとこからもいなくなりそうだな",
              "maleVariant": "",
              "stringId": "1806686517686132736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業が俺たちを監視してんだよ。四六時中な",
              "maleVariant": "",
              "stringId": "1806690608139165696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スティーブの奴、あいつと話すようになってから、朝から晩までギターばっかりだ",
              "maleVariant": "",
              "stringId": "1806696819567845376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちゃんとしつけた方がいいぜ",
              "maleVariant": "",
              "stringId": "1806706304919232512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "付き合ってられるかよ",
              "maleVariant": "",
              "stringId": "2006918230566461440"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}