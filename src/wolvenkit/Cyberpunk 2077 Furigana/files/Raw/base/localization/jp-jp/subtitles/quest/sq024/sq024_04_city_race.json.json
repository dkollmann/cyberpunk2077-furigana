  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、よく来てくれたね",
              "maleVariant": "",
              "stringId": "1388025120258269184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クレア？ あたしはもう着いてるよ",
              "maleVariant": "クレア？ 俺はもう着いてるぞ",
              "stringId": "1752026505280495616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "急カーブよ！",
              "maleVariant": "",
              "stringId": "1783639476469243908"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "後ろの車が突っ込もうとしてる！",
              "maleVariant": "",
              "stringId": "1783639476486021124"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "前の車！ ケツに食らいついて！",
              "maleVariant": "",
              "stringId": "1783639476502798340"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リロード中！",
              "maleVariant": "",
              "stringId": "1783639476536352772"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ガンナーを仕留めた！",
              "maleVariant": "",
              "stringId": "1783639476553129988"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その調子よ、V！",
              "maleVariant": "",
              "stringId": "1783639476586684420"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、ここよ",
              "maleVariant": "",
              "stringId": "1831217688833171456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何かアドバイスはある？",
              "maleVariant": "何かアドバイスはあるか？",
              "stringId": "1831236474047045632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく全速力で突き進んで。障害は私が取り除く",
              "maleVariant": "",
              "stringId": "1831250494430912512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最終的に勝負を決めるのは、火力よりもスピードよ",
              "maleVariant": "",
              "stringId": "1831251265830526976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "了解。今向かってるから",
              "maleVariant": "",
              "stringId": "1831257317389115392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたのよ",
              "maleVariant": "あんたのだ",
              "stringId": "1831257630854619140"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしのよ",
              "maleVariant": "俺のだ",
              "stringId": "1831257986732924928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クレアのよ",
              "maleVariant": "クレアのだ",
              "stringId": "1831269745413857280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしのよ",
              "maleVariant": "俺のだ",
              "stringId": "1831269764841873408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを受け取る]",
              "maleVariant": "",
              "stringId": "1831294346214776836"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全チェックポイントを通過しないと失格だからね",
              "maleVariant": "",
              "stringId": "1831294658371657728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、あとで。私は車にいるから",
              "maleVariant": "",
              "stringId": "1831295509463048192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スタートの時間は？",
              "maleVariant": "",
              "stringId": "1831295649234034688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1831296158741307392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まもなく第1予選開始だ！ 準備はいいか？",
              "maleVariant": "",
              "stringId": "1831397819929337856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "と言っても、今さら後には引けないぞ！",
              "maleVariant": "",
              "stringId": "1831399069043400704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これは何？",
              "maleVariant": "これは何だ？",
              "stringId": "1831501146876813312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これは何？",
              "maleVariant": "これは何だ？",
              "stringId": "1831501356994666496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ルートを示してくれる。レースの必需品よ",
              "maleVariant": "",
              "stringId": "1831501842577629184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃ、また後でね",
              "maleVariant": "",
              "stringId": "1831625776023216128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK、ビーストは準備万端よ",
              "maleVariant": "",
              "stringId": "1831626285765369856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあそれで",
              "maleVariant": "",
              "stringId": "1831626395387699200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ、ベルトを締めて神に祈れ！ レーススタートだ！",
              "maleVariant": "",
              "stringId": "1855979224088842240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "警察は何も言ってこないの？",
              "maleVariant": "警察は何も言ってこないのか？",
              "stringId": "1855991730698063876"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コーポの連中が大勢いる",
              "maleVariant": "",
              "stringId": "1855991834817466368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どう、やれそう？",
              "maleVariant": "",
              "stringId": "1855993984314380288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レース後に聞いて",
              "maleVariant": "レース後に聞いてくれ",
              "stringId": "1855996647730974720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コーポの連中が大勢いる",
              "maleVariant": "",
              "stringId": "1855998444923129856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゴールしてから聞いて。無事にたどり着ければだけど",
              "maleVariant": "ゴールしてから聞いてくれ。無事にたどり着ければだが",
              "stringId": "1855998469250093056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、弱気にならないでよ",
              "maleVariant": "",
              "stringId": "1856000556033773568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜわかるの？",
              "maleVariant": "",
              "stringId": "1856004563758354432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "元コーポだから。仕事後のガス抜きは大事よ",
              "maleVariant": "元コーポだからな。仕事後のガス抜きは大事だ",
              "stringId": "1856005372420165632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "定期的にストレスを発散しないと、過労で病院送りになる",
              "maleVariant": "",
              "stringId": "1856006353417539584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "過労はともかく、レースでも病院送りはしょっちゅうよ",
              "maleVariant": "",
              "stringId": "1856104005236822016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫、任せといて",
              "maleVariant": "大丈夫だ、任せておけ",
              "stringId": "1856109070194446336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "警察は何も言ってこないの？",
              "maleVariant": "警察は何も言ってこないのか？",
              "stringId": "1856111172211838976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中はお金を積まれると、目が悪くなるのよ",
              "maleVariant": "",
              "stringId": "1856111753911472128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "地獄の沙汰もエディー次第、か",
              "maleVariant": "",
              "stringId": "1856114270913302528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ",
              "maleVariant": "",
              "stringId": "1856115192720658432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スタート前にアドバイスはある？",
              "maleVariant": "スタート前にアドバイスはあるか？",
              "stringId": "1856116004201041924"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他の出場者を殺してもお咎めなしなの？",
              "maleVariant": "他の出場者を殺してもお咎めなしなのか？",
              "stringId": "1856116195981398016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このレースじゃ殺しも容認されてるの？",
              "maleVariant": "このレースじゃ殺しも容認されてるのか？",
              "stringId": "1856117579933945856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰が死のうと警察はお構いなし。事故として処理するだけ",
              "maleVariant": "",
              "stringId": "1856118237013610496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭を撃たれても交通事故になるわけ？",
              "maleVariant": "頭を撃たれても交通事故になるのか？",
              "stringId": "1856118834450272256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "通りによってはね",
              "maleVariant": "",
              "stringId": "1856120796294025216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アクセル全開で行くよ",
              "maleVariant": "",
              "stringId": "1856123254525267968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだ、このチップを",
              "maleVariant": "",
              "stringId": "1856123873789087744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チップに全部入ってる",
              "maleVariant": "",
              "stringId": "1856135765530120192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スリー！",
              "maleVariant": "",
              "stringId": "1856145996930748416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ツー！",
              "maleVariant": "",
              "stringId": "1856146462783705088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ワン！",
              "maleVariant": "",
              "stringId": "1856146646410334208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1856146817537937408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つかまってて！",
              "maleVariant": "つかまってろ！",
              "stringId": "1856150396453654528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい度胸ね",
              "maleVariant": "いい度胸だ",
              "stringId": "1856152170006712320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "むしろ引っぱたいてあげる！",
              "maleVariant": "むしろ引っぱたいてやるよ！",
              "stringId": "1856153183769989120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "集中を切らさないで",
              "maleVariant": "",
              "stringId": "1856155046443618304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっと？",
              "maleVariant": "やっとか！",
              "stringId": "1856156835012284416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“ありがとう”くらい言えないの？",
              "maleVariant": "",
              "stringId": "1856156944584282112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってるって！",
              "maleVariant": "",
              "stringId": "1856157495380283392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、じゃあ後で",
              "maleVariant": "",
              "stringId": "1863078313456701440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー待って、どっちの車を使うの？",
              "maleVariant": "",
              "stringId": "1863078505757151232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "1963616653590667268"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私たちの勝ちに大金を賭けておいた。絶対に勝つさ",
              "maleVariant": "",
              "stringId": "1977929348032765952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サンプソン、あれ見なよ。カモがいるよ",
              "maleVariant": "",
              "stringId": "1977931092007591936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん。ルーキーに負けるわけにはいかないな",
              "maleVariant": "",
              "stringId": "1977931360090726400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫だって。最初のコーナー辺りでクラッシュするんじゃない？",
              "maleVariant": "",
              "stringId": "1977931663640895488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ベットを締め切るぞ！ チャンスを逃すな！",
              "maleVariant": "",
              "stringId": "2008388904462454784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、どこに行く気！？",
              "maleVariant": "",
              "stringId": "2014019308506714112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V！ コースに戻って！",
              "maleVariant": "",
              "stringId": "2014019352496574464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何してるのよ！？ レースに戻って！",
              "maleVariant": "",
              "stringId": "2014019381504380928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何なのよ！？ もう私の車から出てって！",
              "maleVariant": "",
              "stringId": "2014029674594832384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "台無しよ！ 私はもう降りる！",
              "maleVariant": "",
              "stringId": "2014029715933892608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もういい！ 車から降りて！",
              "maleVariant": "",
              "stringId": "2014029741569478656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何なの！ もう降ろして！",
              "maleVariant": "",
              "stringId": "2014029766399758336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座って待つ]",
              "maleVariant": "",
              "stringId": "2054976714581295104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ピート・サンプソンの話だが、去年のレースで何したか聞いたかよ？",
              "maleVariant": "",
              "stringId": "2231449145960435712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、どう思う？",
              "maleVariant": "",
              "stringId": "2231449757473181700"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘイウッド生まれ、ヘイウッド育ち！ よその奴らなんか蹴散らしちまえ！",
              "maleVariant": "",
              "stringId": "2231449848992894980"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "雑魚どもにお前の力を見せつけてやれ！",
              "maleVariant": "",
              "stringId": "2231449943079522308"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気をつけて！ お願いだから怪我だけはしないでね！ 愛してるわよ！",
              "maleVariant": "",
              "stringId": "2231450144573886468"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バッキー！ おい、バッキー！ あんたに100エディー賭けたからね！ ぶっちぎってやんな！ お母さんも見てるよ！",
              "maleVariant": "",
              "stringId": "2231450227671437312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつホントに偉そうで嫌いなのよ… で、話って決勝レースのこと？ 優勝したんだっけ",
              "maleVariant": "",
              "stringId": "2241456326931779584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いやいや、第1予選の話さ。カブキのリトルチャイナ、ノースサイドでフィニッシュするヤツ",
              "maleVariant": "",
              "stringId": "2241456401707831296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー、でもサンプソンは確か2位通過じゃなかったっけ。それでも十分すごいけど",
              "maleVariant": "",
              "stringId": "2241456555085139968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それがだな… サンプソンはメイルストロームの連中を雇って、1位のペアを殺したんだよ。その結果、繰り上がりで1位通過になったんだ",
              "maleVariant": "",
              "stringId": "2241456655345782784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん、スーツを着た成金連中なんか眼中にない。ボコボコにしてやるよ",
              "maleVariant": "",
              "stringId": "2241865452111400960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうか、ならいい。ただ、あまり熱くなりすぎるなよ。レース中はいかに冷静でいられるかが大事なんだ",
              "maleVariant": "",
              "stringId": "2241865532927250432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は冷静だ。とにかく、早くあのクソども…",
              "maleVariant": "",
              "stringId": "2241865575591710720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのコーポの連中を歓迎してやりたいだけさ。ビスタ・デル・レイ流にな",
              "maleVariant": "",
              "stringId": "2241865677597184000"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}