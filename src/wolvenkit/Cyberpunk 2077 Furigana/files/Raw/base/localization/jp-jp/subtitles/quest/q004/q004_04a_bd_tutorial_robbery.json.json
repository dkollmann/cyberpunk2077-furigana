  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "金を出せ！ さっさとしねえとブッ殺すぞ！",
              "maleVariant": "",
              "stringId": "1177805824054931456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっ… 待ってくれ、落ち着いて…！",
              "maleVariant": "",
              "stringId": "1177810650524430336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早くしろ！ アタマ吹っ飛ばされてえのか！",
              "maleVariant": "",
              "stringId": "1177812419346313216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、わかってる",
              "maleVariant": "",
              "stringId": "1203656761373487104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいか。出し惜しみはなしだ。アドレナリンが出れば出るほど、BDの値段も上がるからな",
              "maleVariant": "",
              "stringId": "1203657093595918336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "余計なことはしなくていい。中に入って現金を奪い、外に出る。あとはスタジオのサイコ野郎どもにこのBDを売るだけだ",
              "maleVariant": "",
              "stringId": "1203772998824882176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はいよ、今日はカレー味とサーフアンドターフ味がお買い得だ。どうだい？",
              "maleVariant": "",
              "stringId": "1451721074460733440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え、賞味期限切れとかじゃないでしょうね？",
              "maleVariant": "",
              "stringId": "1466084306943864832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "記録者の感覚配列からVを切り離すよ。そうすれば、場面全体を自由に観察できるようになる",
              "maleVariant": "",
              "stringId": "1560656617783615488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "分析モード中はカメラを自由自在に動かせる。移動もズームも、すべてが思うがまま… 自分だけのサンドボックスってわけ",
              "maleVariant": "",
              "stringId": "1560656617783615512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃ、色々調べてみて",
              "maleVariant": "",
              "stringId": "1560656617783615524"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それが… 記録者の知覚が及んでいた限界ラインってところね。エディターでもそれより先に行くことはできない。わかる？",
              "maleVariant": "",
              "stringId": "1560658646098698264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、じゃあ強盗たちのシーンに戻るよ",
              "maleVariant": "",
              "stringId": "1560658646098698288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "分析モードでは再生速度を調節できる。必要なら一時停止も可能よ",
              "maleVariant": "",
              "stringId": "1560659640920498176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コンソールを使って再開できるから、やってみて",
              "maleVariant": "",
              "stringId": "1560659640937275400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すごいでしょ？ でもこれだけじゃない。早送りも早戻しも、好きなようにできるんだから",
              "maleVariant": "",
              "stringId": "1560663989021237248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さっそく、最初のとこまで早戻ししてみて",
              "maleVariant": "",
              "stringId": "1560663989021237260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うん、OK。じゃ、今度は早送りして",
              "maleVariant": "",
              "stringId": "1560664471567523840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK。ちなみにリセットして、一番最初のとこに戻ることもできるの。やってみて",
              "maleVariant": "",
              "stringId": "1560664983490715648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "簡単でしょ？ じゃあ、準備ができたらリセットして。次に進もう",
              "maleVariant": "",
              "stringId": "1560666163398762496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さ、こっからが本番。そもそもの目的はこれだからね",
              "maleVariant": "",
              "stringId": "1560671309340008448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "分析モードでは、記録者がBDに取り込んだ環境を探索したり、スキャンしたりできるの",
              "maleVariant": "",
              "stringId": "1560671309340008464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "銃にフォーカスを。最初のシーンで相棒から受け取ったヤツ。それをスキャンして",
              "maleVariant": "",
              "stringId": "1560671309356785684"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "上出来よ",
              "maleVariant": "",
              "stringId": "1560671956688887808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "続けましょ",
              "maleVariant": "",
              "stringId": "1560671956688887824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて… 分析モードでは、記録者の近くで発生したノイズや、会話を拾うことができるの",
              "maleVariant": "",
              "stringId": "1560674757896757248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BDは記録者本人が意識してなかった光景や音まで、あらゆる細部を記録してくれる",
              "maleVariant": "",
              "stringId": "1560674757896757260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エディター内で音レイヤーに切り替えると、各信号の発信元が視覚化されるはず。さっそくやってみて",
              "maleVariant": "",
              "stringId": "1560674757913534476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK。店内にいくつかの音声パターンが表示されてるでしょ。1つを選んでフォーカスしてみて",
              "maleVariant": "",
              "stringId": "1560675927671365632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ご感想は？",
              "maleVariant": "",
              "stringId": "1560678452055171072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信じられない。本当にすぐ隣で起きてるみたい",
              "maleVariant": "信じられない。本当にすぐ隣で起きてるみたいだ",
              "stringId": "1560678452071948296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これがBDレコーダーインプラントの力よ。あらゆる背景情報を記録し、エディターの機能で細部を引き立たせてやるの",
              "maleVariant": "",
              "stringId": "1560678452071948308"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのまま音の分析を続けてみて。飽きたら次に行くよ",
              "maleVariant": "",
              "stringId": "1560678452071948320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃ、男がカウンターの客を殴るまで待って",
              "maleVariant": "",
              "stringId": "1560679289640902656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スキャンは人間にも使える。そこのケガしてる子に近づいて、スキャンしてみて",
              "maleVariant": "",
              "stringId": "1560680178632024064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃ、次。強盗が痛い目に遭う場面まで移動して",
              "maleVariant": "",
              "stringId": "1560680919698432016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかる？ 彼はこの時、自分がどう撃たれたか見てない",
              "maleVariant": "",
              "stringId": "1560681443533447168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもあなたは見れちゃうの。どう？ ワクワクするでしょ",
              "maleVariant": "",
              "stringId": "1560681443550224388"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "入口の上の方で点滅してるのが見える？ 監視カメラよ",
              "maleVariant": "",
              "stringId": "1560684169394839552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "映像は店員の後ろの画面に送られてる",
              "maleVariant": "",
              "stringId": "1560684169411616776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK、これでエディターが不足データを補完して、発砲した人物を特定できる。再開して",
              "maleVariant": "",
              "stringId": "1560685578865532928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "撃ったのは彼の相棒。きっと最初からそのつもりだったのね。この手のBDは、ブラックマーケットで高く売れるから",
              "maleVariant": "",
              "stringId": "1560697181904232448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リアルな死の体験は、マニアが飛びつくのよ",
              "maleVariant": "",
              "stringId": "1560697181904232464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて… もう十分なら、いつでも終了して",
              "maleVariant": "",
              "stringId": "1560697181904232476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "煙草とブローセフのパック、あとザッパー2つ",
              "maleVariant": "",
              "stringId": "1660754145775071232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ！ 何なんだよ…",
              "maleVariant": "",
              "stringId": "1661875906944053248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何が「補充に行け。レジは俺がやる」だよ。ったく、ふざけやがって…",
              "maleVariant": "",
              "stringId": "1661884246898126848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "生データには、記録者のサイバーウェアが取得した別のレイヤーが含まれてることもある",
              "maleVariant": "",
              "stringId": "1662182858223923200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "というと？",
              "maleVariant": "",
              "stringId": "1662189239622578176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "キロシの赤外線検知ウェアを着けてるエヴリンの記録なら、熱パターンの分析ができるってこと",
              "maleVariant": "",
              "stringId": "1662189504165720064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほどね",
              "maleVariant": "なるほどな",
              "stringId": "1663182014853541888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全員伏せろ！ さっさとしやがれ！ 床にキスするんだよ！",
              "maleVariant": "",
              "stringId": "1793872359748153344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何見てんだ、クソアマ！",
              "maleVariant": "",
              "stringId": "1945715303684632576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "撃った奴を捉えてるはず、確かめてみて",
              "maleVariant": "",
              "stringId": "1968861151213776896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、ここだね",
              "maleVariant": "",
              "stringId": "1970527589012221952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "強盗の視界に画面が入るところまで戻して、スキャンしてみて",
              "maleVariant": "",
              "stringId": "1979152407093440512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、行け",
              "maleVariant": "",
              "stringId": "2231624522677809152"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}