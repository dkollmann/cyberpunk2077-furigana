          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "Vよ。ホセを始末した",
                    "maleVariant": "Vだ。ホセを始末した",
                    "stringId": 1915885788811595776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さすがだな。見事な仕事ぶりだ",
                    "maleVariant": null,
                    "stringId": 1915889054513467392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "車を向かわせよう",
                    "maleVariant": null,
                    "stringId": 1915892777780350976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカが捜査に横やりを入れた",
                    "maleVariant": null,
                    "stringId": 1915896316497858560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "続けてくれ",
                    "maleVariant": null,
                    "stringId": 1915899172953100288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "警官が殺された後、誰が奴を逃したかわかったか？",
                    "maleVariant": null,
                    "stringId": 1924546264063905792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカよ",
                    "maleVariant": "アラサカだ",
                    "stringId": 1924626100610502660
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいえ",
                    "maleVariant": "いいや",
                    "stringId": 1924626993678487552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いえ、残念だけど",
                    "maleVariant": "いや、残念だが",
                    "stringId": 1924628782817579008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう少し役に立つ奴だと思ってたんだがな",
                    "maleVariant": null,
                    "stringId": 1924689565932949504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しくじったな。カネになったかもしれないのに",
                    "maleVariant": null,
                    "stringId": 1924691102373613568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そういう仕事じゃなかったはずよ",
                    "maleVariant": "そういう仕事じゃなかったはずだ",
                    "stringId": 1924696170133032960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、確かにな。ではな、V",
                    "maleVariant": null,
                    "stringId": 1924697015453061120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカが？ まさか一介の警官の殺害に、巨大企業が関与していたとはな…",
                    "maleVariant": null,
                    "stringId": 1924700624500658176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョフリーの死はたまたまよ。ホセの件は別の理由みたい",
                    "maleVariant": "ジョフリーの死はたまたまだ。ホセの件は別の理由みたいだな",
                    "stringId": 1924701079767191552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待って。連中はミリテクの装備を大量に持ってる",
                    "maleVariant": "待て。連中はミリテクの装備を大量に持ってる",
                    "stringId": 1924720783986626560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どういうことだ？ 盗んだものか？",
                    "maleVariant": null,
                    "stringId": 1924721249873137664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いろいろ知ってるようね",
                    "maleVariant": "いろいろ知ってるようだな",
                    "stringId": 1924722779267371012
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう少し探りを入れてもいいかも",
                    "maleVariant": "もう少し探りを入れてもいいかもな",
                    "stringId": 1924722859361800192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあね。けど… 何か企んでるのは確かよ",
                    "maleVariant": "さあな。だが… 何か企んでるのは確かだ",
                    "stringId": 1924740381620408320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一体何を？",
                    "maleVariant": null,
                    "stringId": 1924740541574385664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあ、そこまでは…",
                    "maleVariant": null,
                    "stringId": 1924741306112118784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。こちらでも調べてみる。また話そう",
                    "maleVariant": null,
                    "stringId": 1924741548257677312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいから",
                    "maleVariant": "かまわない",
                    "stringId": 1925566518697234432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカはミリテクとの本格的な衝突を画策してるのかもしれない。全面戦争もあり得る",
                    "maleVariant": null,
                    "stringId": 1925567075113603072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "つまり… ホセの暗殺を口実にするってこと？ ちょっと飛躍し過ぎじゃない？",
                    "maleVariant": "つまり、ホセの暗殺を口実にするってことか？ ちょっと飛躍し過ぎだろ？",
                    "stringId": 1925568673898401792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "奴とその仲間は、アラサカの施設を襲うために雇われたの。わざわざミリテクの兵器を使ってね",
                    "maleVariant": "奴とその仲間は、アラサカの施設を襲うために雇われたんだ。わざわざミリテクの兵器を使ってな",
                    "stringId": 1925573726138896384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカの依頼で… アラサカを襲うだと…？ クソ、早くそこを離れろ",
                    "maleVariant": null,
                    "stringId": 1925581215202906112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカが戦争を望む理由は？",
                    "maleVariant": null,
                    "stringId": 1925638955937927172
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミリテクが何もしない理由は？",
                    "maleVariant": null,
                    "stringId": 1925639107503296512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でもなぜ？ 前回の戦争で十分懲りてるはずでしょ",
                    "maleVariant": "でもなぜ？ 前回の戦争で十分懲りてるはずだろ",
                    "stringId": 1925640530680975360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "戦争は格好のカモフラージュだ。世間の目をそらせたい何かがあるのかもな",
                    "maleVariant": null,
                    "stringId": 1925641378718912512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "深追いするのはやめとく… あんたはどうする？",
                    "maleVariant": "深追いするのはやめとこう… あんたはどうする？",
                    "stringId": 1925641558587445248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これはただの推測だ…",
                    "maleVariant": null,
                    "stringId": 1925643009984401408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミリテクは？ アラサカの動きに気づいてるはずでしょ",
                    "maleVariant": "ミリテクは？ アラサカの動きに気づいてるはずだろ",
                    "stringId": 1925648804868362240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やめとこう。さすがに私の専門分野外だ",
                    "maleVariant": null,
                    "stringId": 1925648949622181888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おそらくな。だがミリテクは戦争を望んではいない。連中は時間を稼ぎ、周到に動く気だろう",
                    "maleVariant": null,
                    "stringId": 1925650459890388992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一部では、高値で売れそうな情報ね",
                    "maleVariant": "一部では高値で売れそうな情報だな",
                    "stringId": 1925650703613005824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ではまたな、V",
                    "maleVariant": null,
                    "stringId": 1925733114841501696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、また",
                    "maleVariant": "ああ、また",
                    "stringId": 1957690810926731264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや… せっかく来たんだし、もう少し探りを入れてみる",
                    "maleVariant": "いや、せっかく来たんだし、もう少し探りを入れてみる",
                    "stringId": 2028574596207579136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "新しい情報は見つからないさ。そこは既に放棄されたと見ていい",
                    "maleVariant": null,
                    "stringId": 2028574884154937344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いろいろ知ってるようね。話して",
                    "maleVariant": "いろいろ知ってるようだな。話せよ",
                    "stringId": 2028575719928729600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ホセだけじゃない。同じ手口の事件をいくつか聞いてる。こいつは根が深いぞ、V",
                    "maleVariant": null,
                    "stringId": 2028579171253719040
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
