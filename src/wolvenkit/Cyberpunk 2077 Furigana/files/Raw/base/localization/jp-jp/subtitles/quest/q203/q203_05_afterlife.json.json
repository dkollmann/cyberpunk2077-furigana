    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1715685417732919296,
            "femaleVariant": "V"
          },
          {
            "stringId": 1715691388744093696,
            "femaleVariant": "ジョニーに捧げる"
          },
          {
            "stringId": 1715691590221680640,
            "femaleVariant": "ジャッキーに捧げる"
          },
          {
            "stringId": 1715691640771432448,
            "femaleVariant": "ローグに捧げる"
          },
          {
            "stringId": 1715691750578311168,
            "femaleVariant": "あたし自身に、っていうのはどう？",
            "maleVariant": "俺自身に、っていうのはどうだ？"
          },
          {
            "stringId": 1715692801872220160,
            "femaleVariant": "シルヴァーハンドをお願い。ジョニーに捧げる",
            "maleVariant": "シルヴァーハンドを頼む。ジョニーに捧げる"
          },
          {
            "stringId": 1715698509615321088,
            "femaleVariant": "V。ついにこの時が来たな。アフターライフが築く新たな伝説に立ち会えるとは… 感無量だよ"
          },
          {
            "stringId": 1715699657462108160,
            "femaleVariant": "期待は裏切らない"
          },
          {
            "stringId": 1715700198510546944,
            "femaleVariant": "アラサカから漏れた情報で裏が取れた。カジノは現在、新しいセキュリティシステムの試験を行ってる"
          },
          {
            "stringId": 1715774636534910976,
            "femaleVariant": "彼のことは噂でしか知らないけど… Vがやろうとしてることには、きっと賛成してくれる"
          },
          {
            "stringId": 1730130817202114560,
            "femaleVariant": "ボス"
          },
          {
            "stringId": 1730132208838627328,
            "femaleVariant": "準備は？ できてる？",
            "maleVariant": "準備は？ できてるか？"
          },
          {
            "stringId": 1730154586926862336,
            "femaleVariant": "何か飲む？"
          },
          {
            "stringId": 1730238423849619460,
            "femaleVariant": "[座る] 状況を教えて",
            "maleVariant": "[座る] 状況を教えてくれ"
          },
          {
            "stringId": 1795324919281217536,
            "femaleVariant": "完璧だ。いつもの席を確保してあるよ"
          },
          {
            "stringId": 1795331219159379972,
            "femaleVariant": "[寄りかかる] いつものを"
          },
          {
            "stringId": 1795331419982655488,
            "femaleVariant": "[寄りかかる] 遠慮しとく"
          },
          {
            "stringId": 1795333140100280320,
            "femaleVariant": "いつものを"
          },
          {
            "stringId": 1795333631353942016,
            "femaleVariant": "了解"
          },
          {
            "stringId": 1795334285950578688,
            "femaleVariant": "みんながざわつき始めてる。隠すのもそろそろ限界よ"
          },
          {
            "stringId": 1795336090340159488,
            "femaleVariant": "大丈夫。決行は今日だから",
            "maleVariant": "問題ない。決行は今日だ"
          },
          {
            "stringId": 1795336225228976128,
            "femaleVariant": "今日…？"
          },
          {
            "stringId": 1795337100328562688,
            "femaleVariant": "遠慮しとく",
            "maleVariant": "いや、遠慮しとく"
          },
          {
            "stringId": 1795337493972381696,
            "femaleVariant": "決行は今日だものね。例の男と話をするんでしょ？"
          },
          {
            "stringId": 1795337918150733824,
            "femaleVariant": "ええ",
            "maleVariant": "ああ"
          },
          {
            "stringId": 1795340042179833856,
            "femaleVariant": "じゃあ、私が乾杯させてもらうね。Vの健康と、作戦の成功に"
          },
          {
            "stringId": 1795342932072067072,
            "femaleVariant": "ええ",
            "maleVariant": "ああ"
          },
          {
            "stringId": 1795343125697916928,
            "femaleVariant": "頑張ってね"
          },
          {
            "stringId": 1795344828686004224,
            "femaleVariant": "ウェルズをお願い。ジャッキーに捧げる",
            "maleVariant": "ウェルズを頼む。ジャッキーに捧げる"
          },
          {
            "stringId": 1795345795271749632,
            "femaleVariant": "やっぱり、思い出す？"
          },
          {
            "stringId": 1795346162390790144,
            "femaleVariant": "ええ。ここはジャッキーの憧れだったから。アフターライフの伝説になるんだって、よく二人で話してた",
            "maleVariant": "ああ。ここはジャッキーの憧れだったからな。アフターライフの伝説になるんだって、よく二人で話してた"
          },
          {
            "stringId": 1795346876982751232,
            "femaleVariant": "彼の夢は、あなたが叶えた"
          },
          {
            "stringId": 1795346995748663296,
            "femaleVariant": "頑張ってね"
          },
          {
            "stringId": 1795348508701224960,
            "femaleVariant": "ローグに捧げる"
          },
          {
            "stringId": 1795349234231930880,
            "femaleVariant": "あなたがボックスを継いで、喜んでるはず"
          },
          {
            "stringId": 1795350527704952832,
            "femaleVariant": "どうだろう",
            "maleVariant": "どうだろうな"
          },
          {
            "stringId": 1795352046563094528,
            "femaleVariant": "初めて彼女を頼った時を覚えてる？ あなたにすごく厳しかった"
          },
          {
            "stringId": 1795352444082450432,
            "femaleVariant": "でもあの後、ローグが言ってたのよ。「あいつにはアフターライフの血が流れてる。この街を支配するか、街に殺されるか。どちらかだろうね」って"
          },
          {
            "stringId": 1795356541716131840,
            "femaleVariant": "だから手を貸したんだと思う。結局… そのとおりだった"
          },
          {
            "stringId": 1795356656354848768,
            "femaleVariant": "そうね",
            "maleVariant": "そうだな"
          },
          {
            "stringId": 1795358674402275328,
            "femaleVariant": "なら、あたしのカクテルを作って乾杯してよ。景気づけにさ",
            "maleVariant": "なら、俺のカクテルを作って乾杯してくれよ。景気づけにさ"
          },
          {
            "stringId": 1795359538059800576,
            "femaleVariant": "アフターライフのメニューになれるのは死んだ人だけ。ルールは知ってるでしょ"
          },
          {
            "stringId": 1795359669979049984,
            "femaleVariant": "あたしは1回死んでるの",
            "maleVariant": "俺は1回死んでるんだ"
          },
          {
            "stringId": 1795371768281165828,
            "femaleVariant": "任せて。必ずやり遂げる",
            "maleVariant": "任せてくれ。必ずやり遂げる"
          },
          {
            "stringId": 1795371984287821824,
            "femaleVariant": "疑ってるの？",
            "maleVariant": "疑ってるのか？"
          },
          {
            "stringId": 1795377420693008384,
            "femaleVariant": "安心して。あたし以上の適任者はこの街に存在しない",
            "maleVariant": "安心しろ。俺以上の適任者はこの街に存在しない"
          },
          {
            "stringId": 1795378485677125632,
            "femaleVariant": "あたしの腕を疑ってるの？ 手を引くとか言い出さないでよ",
            "maleVariant": "俺の腕を疑ってるのか？ 手を引くなんて言い出すなよ"
          },
          {
            "stringId": 1795378935860162560,
            "femaleVariant": "そう聞いてるが、この目で確かめないことにはな"
          },
          {
            "stringId": 1795382939944087552,
            "femaleVariant": "手を引く？ もうそんな段階は過ぎた。計画は実行するしかない"
          },
          {
            "stringId": 1795384639509622784,
            "femaleVariant": "いいか、V――こんな大仕事は、過去に例を見ない"
          },
          {
            "stringId": 1809661701225312256,
            "femaleVariant": "何か問題？",
            "maleVariant": "何か問題か？"
          },
          {
            "stringId": 1809665156140625920,
            "femaleVariant": "ううん。でもそういうことなら、特別なのを飲んでもらわないと"
          },
          {
            "stringId": 1809668463433216000,
            "femaleVariant": "ローグは大きな仕事の前に、必ずテキーラのショットを飲んだのよ。シルヴァーハンドお気に入りのね"
          },
          {
            "stringId": 1809671895162417152,
            "femaleVariant": "私がゲン担ぎ？ って聞いたら、なんて答えたと思う？"
          },
          {
            "stringId": 1809673582967123968,
            "femaleVariant": "思い出すためだってさ。ジョニーのことを考えると、本当に大事なことも思い出せるからって…"
          },
          {
            "stringId": 1873363209342808068,
            "femaleVariant": "そいつらが間違ってるだけよ",
            "maleVariant": "そいつらが間違ってるだけだ"
          },
          {
            "stringId": 1873363809044393984,
            "femaleVariant": "だからあたしを雇ったんでしょ",
            "maleVariant": "だから俺を雇ったんだろ"
          },
          {
            "stringId": 1873364021578166272,
            "femaleVariant": "だからあたしを雇ったんでしょ",
            "maleVariant": "だから俺を雇ったんだろ"
          },
          {
            "stringId": 1873364335832199168,
            "femaleVariant": "ふっ。お前を雇ったのは、わずかでも生き残る可能性があるなら、どんな手も尽くすだろうと踏んだからだ"
          },
          {
            "stringId": 1873365548472594432,
            "femaleVariant": "そいつらは、みんな間違ってる"
          },
          {
            "stringId": 1873687262833405956,
            "femaleVariant": "覚悟はできてる"
          },
          {
            "stringId": 1903882771434827776,
            "femaleVariant": "レディーファーストだ",
            "maleVariant": "まあ、座ってくれ"
          },
          {
            "stringId": 1976712403576541184,
            "femaleVariant": "戻ったのね、ウェイランド。調子はどう？",
            "maleVariant": "戻ったのか、ウェイランド。調子はどうだ？"
          },
          {
            "stringId": 1976712619331538948,
            "femaleVariant": "もう仕事に戻ったの？",
            "maleVariant": "もう仕事に戻ったのか？"
          },
          {
            "stringId": 2009531033890451456,
            "femaleVariant": "とにかくアフターライフの伝統よ。特製のカクテルを作るから、誰に捧げるかを決めて"
          },
          {
            "stringId": 2009557366116839424,
            "femaleVariant": "は？ えーと… そうなの…？ うーん… じゃあ何か作ってみる"
          },
          {
            "stringId": 2009558298376400896,
            "femaleVariant": "幸運を祈ってるわ"
          },
          {
            "stringId": 2009559665820168192,
            "femaleVariant": "ローグは大きな仕事の前に、必ずテキーラのショットを飲んだのよ。シルヴァーハンドお気に入りのね"
          },
          {
            "stringId": 2009569466465775616,
            "femaleVariant": "それと事前の情報通り、今日一時的にシステムが停止される。あとはお前の働き次第だ"
          },
          {
            "stringId": 2009601230986932224,
            "femaleVariant": "状況は？"
          },
          {
            "stringId": 2009606624375336960,
            "femaleVariant": "私の周りの人間なら、不可能と言うだろう"
          },
          {
            "stringId": 2009609542050639872,
            "femaleVariant": "レジェンドにふさわしい自信だな。全てを賭すことになってもよいと"
          },
          {
            "stringId": 2009610536905994240,
            "femaleVariant": "私は約束を守る人間だ。健闘を祈るぞ"
          },
          {
            "stringId": 2009611779560828928,
            "femaleVariant": "明日のこの時間には、カジノの顧客データは全て手に入ってる"
          },
          {
            "stringId": 2009613931255558144,
            "femaleVariant": "そっちの約束も忘れないで",
            "maleVariant": "そっちの約束も忘れるなよ"
          },
          {
            "stringId": 2009638840170049536,
            "femaleVariant": "仮にそれが、死を早めることと隣り合わせの賭けになるとしてもな"
          },
          {
            "stringId": 2009652173879242752,
            "femaleVariant": "いや… お前は失うことの苦しみも知っているようだ。最後まで戦い抜く者の気概を見てとれる"
          },
          {
            "stringId": 2009655891995521024,
            "femaleVariant": "新品同然って感じね",
            "maleVariant": "新品同然って感じだな"
          },
          {
            "stringId": 2009656169708777472,
            "femaleVariant": "すっかり元どおりね。出荷仕立てのウェイランドって感じ",
            "maleVariant": "すっかり元どおりだな。出荷仕立てのウェイランドって感じだ"
          },
          {
            "stringId": 2009659264484372480,
            "femaleVariant": "絶好調だよ"
          },
          {
            "stringId": 2009662342767964160,
            "femaleVariant": "それにしても、V… お前が今日やろうとしてんのは、誰もやったことのない大仕事だ"
          },
          {
            "stringId": 2009663025298665472,
            "femaleVariant": "気をつけるんだぞ。健闘を祈ってる"
          },
          {
            "stringId": 2009663187433680896,
            "femaleVariant": "ありがとう"
          },
          {
            "stringId": 2009664975784243200,
            "femaleVariant": "クリニックでじっとしてんのには飽きたしな。やっぱここが一番さ"
          },
          {
            "stringId": 2009668234909446144,
            "femaleVariant": "アラサカ・タワーでの武勇伝をみんなに触れ回ってるの？",
            "maleVariant": "アラサカ・タワーでの武勇伝をみんなに触れ回ってるのか？"
          },
          {
            "stringId": 2009669081538097152,
            "femaleVariant": "まあな。クレアなんてもう3回は聞いてる"
          },
          {
            "stringId": 2009670066260013056,
            "femaleVariant": "みんなにアラサカ・タワーでの武勇伝も語れるしね",
            "maleVariant": "みんなにアラサカ・タワーでの武勇伝も語れるしな"
          },
          {
            "stringId": 2009670769493798912,
            "femaleVariant": "そのとおり！"
          },
          {
            "stringId": 2050595512664190980,
            "femaleVariant": "準備は？"
          },
          {
            "stringId": 2169634013014913028,
            "femaleVariant": "[飲む]"
          },
          {
            "stringId": 2170548781791834116,
            "femaleVariant": "[立ち上がる]"
          },
          {
            "stringId": 2232849145293631488,
            "femaleVariant": "座ってくれ。話をしよう"
          },
          {
            "stringId": 2232850298341355520,
            "femaleVariant": "V、何か思うことがあるなら聞こうじゃないか"
          },
          {
            "stringId": 2232851514152644608,
            "femaleVariant": "何か気になることでも？"
          },
          {
            "stringId": 2232853090724409344,
            "femaleVariant": "V…？"
          },
          {
            "stringId": 2232863366500773888,
            "femaleVariant": "ボス？"
          },
          {
            "stringId": 2232864293676838912,
            "femaleVariant": "心に浮かぶままでいいのよ"
          },
          {
            "stringId": 2232866513503842304,
            "femaleVariant": "よう、ボス"
          },
          {
            "stringId": 2232867747602296832,
            "femaleVariant": "調子はどうだ？"
          },
          {
            "stringId": 2251555151481597952,
            "femaleVariant": "ローグ、調子はどう？ 何も問題ない？",
            "maleVariant": "ローグ、調子はどうだ？ 何も問題ないか？"
          },
          {
            "stringId": 2251556229082181632,
            "femaleVariant": "ローグ、状況は？"
          },
          {
            "stringId": 2251556931560992768,
            "femaleVariant": "依頼主が来てるようね",
            "maleVariant": "依頼主が来てるようだな"
          },
          {
            "stringId": 2251557460898934784,
            "femaleVariant": "依頼主はもう来てるようね",
            "maleVariant": "依頼主はもう来てるようだな"
          },
          {
            "stringId": 2251558769555337216,
            "femaleVariant": "アフターライフのトップは今やあんただろ。問題があればあんたの責任だ"
          },
          {
            "stringId": 2251558894595928064,
            "femaleVariant": "座りな。忙しくなる前に、あんたと話をしときたい"
          },
          {
            "stringId": 2251563033635778560,
            "femaleVariant": "「いつもの」？ そいつはダメだ、今日は“いつも”とは違うんだからね"
          },
          {
            "stringId": 2251564103267184640,
            "femaleVariant": "未知の世界に旅立つんだろ。それも危険を承知で"
          },
          {
            "stringId": 2251564761051492352,
            "femaleVariant": "もう聞き飽きたって…"
          },
          {
            "stringId": 2251564832606318592,
            "femaleVariant": "私はね、デカい仕事の前はテキーラのショットを飲むんだ。ジョニーのお気に入りのヤツをね。あんたも知ってるだろ"
          },
          {
            "stringId": 2251566981851574272,
            "femaleVariant": "ゲン担ぎってやつ？ お守りとか、勝負パンツとか…",
            "maleVariant": "ゲン担ぎってやつか？ お守りとか、勝負パンツとか…"
          },
          {
            "stringId": 2251568095959379968,
            "femaleVariant": "ゲン担ぎなんかじゃない。思い出すためさ。ジョニーを思い出せば… 自分が何のためにここにいるのかを忘れないですむ。どうしてこんなことをしてるのかをね"
          },
          {
            "stringId": 2251568890779987968,
            "femaleVariant": "ローグ…"
          },
          {
            "stringId": 2251569519623598080,
            "femaleVariant": "あんたにもアフターライフの伝統に従ってもらうよ。クレアがカクテルを作るから、誰に捧げるか決めな"
          },
          {
            "stringId": 2251572984018038784,
            "femaleVariant": "ジョニーに捧げる"
          },
          {
            "stringId": 2251572984018038808,
            "femaleVariant": "ジャッキーに捧げる"
          },
          {
            "stringId": 2251572984034816008,
            "femaleVariant": "あたし自身に、っていうのはどう？",
            "maleVariant": "俺自身に、っていうのはどうだ？"
          },
          {
            "stringId": 2251575208358113300,
            "femaleVariant": "ジョニーがいたら、あんたと一緒にやりたがっただろうね。間違いないよ"
          },
          {
            "stringId": 2251575208358113316,
            "femaleVariant": "きっとね",
            "maleVariant": "きっとな"
          },
          {
            "stringId": 2251575802640658468,
            "femaleVariant": "忘れられない？"
          },
          {
            "stringId": 2251575802640658484,
            "femaleVariant": "ここはジャッキーの憧れだったから。あんたみたいなフィクサーと… 伝説と仕事をしたがってた",
            "maleVariant": "ここはジャッキーの憧れだったからな。あんたみたいなフィクサーと… 伝説と仕事をしたがってた"
          },
          {
            "stringId": 2251575802657435652,
            "femaleVariant": "そいつの夢は、もう叶ってるさ"
          },
          {
            "stringId": 2251576361321951260,
            "femaleVariant": "そろそろ“V”をメニューに載せてもいいんじゃない？今日の記念に、あたしの名のカクテルを",
            "maleVariant": "そろそろ“V”をメニューに載せてもいいんじゃないか？ 今日の記念に、俺の名のカクテルを"
          },
          {
            "stringId": 2251576361321951276,
            "femaleVariant": "この店のカクテルに名前がつくのは死んだ奴だけだよ。知ってるだろ"
          },
          {
            "stringId": 2251576361338728460,
            "femaleVariant": "もちろん。でもローグ… あたしはすでに1回死んでるのよ",
            "maleVariant": "もちろん。だがローグ… 俺はすでに1回死んでるんだ"
          },
          {
            "stringId": 2251576888092979200,
            "femaleVariant": "[飲む]"
          },
          {
            "stringId": 2251577551396990976,
            "femaleVariant": "幸運を祈るよ。こいつを成功させられる奴がいるとしたら、あんたしかいない"
          },
          {
            "stringId": 2251578122510200832,
            "femaleVariant": "[立ち上がる] ありがとう"
          },
          {
            "stringId": 2251646316474200064,
            "femaleVariant": "世界が揺れ、今にも崩れるって時、あいつはいつも笑ってた。今もきっと笑ってる"
          },
          {
            "stringId": 2251677011447836672,
            "femaleVariant": "確かに。アフターライフの生ける伝説か――悪くはないね"
          },
          {
            "stringId": 2251677119275003904,
            "femaleVariant": "そうかい。なら、ずいぶんうぬぼれた奴として蘇ったようだね"
          },
          {
            "stringId": 2251703887524458496,
            "femaleVariant": "アフターライフにあたしたちの名前を轟かせるんだって… 「チーカ、俺たちはメジャーに行くぞ」って夢を語ってた",
