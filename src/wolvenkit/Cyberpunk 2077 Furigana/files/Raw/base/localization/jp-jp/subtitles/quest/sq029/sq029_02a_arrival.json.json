      "Properties": {
        "entries": [
          {
            "stringId": 1467767613846839296,
            "femaleVariant": "あ！"
          },
          {
            "stringId": 1467767614400487424,
            "femaleVariant": "呼んでくれてありがとう"
          },
          {
            "stringId": 1468876195438911488,
            "femaleVariant": "素敵ね、ジョス",
            "maleVariant": "素敵だな、ジョス"
          },
          {
            "stringId": 1468876896458104832,
            "femaleVariant": "素敵ね、ジョス",
            "maleVariant": "素敵だな、ジョス"
          },
          {
            "stringId": 1468877562194812928,
            "femaleVariant": "そう？ 嬉しいわ"
          },
          {
            "stringId": 1468879596985901056,
            "femaleVariant": "招待してくれてありがとう"
          },
          {
            "stringId": 1468881823473459200,
            "femaleVariant": "ランディはどう？",
            "maleVariant": "ランディはどうだ？"
          },
          {
            "stringId": 1468897740538372116,
            "femaleVariant": "リバーを手伝っただけよ",
            "maleVariant": "リバーを手伝っただけだ"
          },
          {
            "stringId": 1468898314319159296,
            "femaleVariant": "とにかく解決してよかった"
          },
          {
            "stringId": 1468899885438328832,
            "femaleVariant": "とにかく解決してよかった"
          },
          {
            "stringId": 1468899943487496192,
            "femaleVariant": "実際、あたしはリバーに手を貸しただけよ",
            "maleVariant": "実際、俺はリバーに手を貸しただけだ"
          },
          {
            "stringId": 1468901433404940288,
            "femaleVariant": "ただ… どう恩返ししたらいいのか… もしランディが…"
          },
          {
            "stringId": 1468904469795823616,
            "femaleVariant": "気にしないで、ジョス",
            "maleVariant": "気にしなくていい、ジョス"
          },
          {
            "stringId": 1468907600944422912,
            "femaleVariant": "ありがとう"
          },
          {
            "stringId": 1511127588943810560,
            "femaleVariant": "来ないの？",
            "maleVariant": "来ないのか？"
          },
          {
            "stringId": 1511319042429407236,
            "femaleVariant": "来ないの？",
            "maleVariant": "来ないのか？"
          },
          {
            "stringId": 1572068379557748736,
            "femaleVariant": "新しい曲を作ってるところだから、それに集中する"
          },
          {
            "stringId": 1725669026180337664,
            "femaleVariant": "ジャンバラヤだ。肉を混ぜるのを頼む"
          },
          {
            "stringId": 1725694887973588992,
            "femaleVariant": "バスマティか、いいね"
          },
          {
            "stringId": 1725696439882846208,
            "femaleVariant": "次はトマトっと…"
          },
          {
            "stringId": 1725697013999177728,
            "femaleVariant": "じゃあ、米を入れてくれ"
          },
          {
            "stringId": 1725697800011747332,
            "femaleVariant": "[米を入れる] 了解"
          },
          {
            "stringId": 1729849648595570688,
            "femaleVariant": "よく来てくれたわね"
          },
          {
            "stringId": 1729858342179803136,
            "femaleVariant": "こっちだ！"
          },
          {
            "stringId": 1731407383472607232,
            "femaleVariant": "ううん、ランディはいつもそうだった。でも今回のこともあって… ほら、あの子の父親も気難しい男だったから…"
          },
          {
            "stringId": 1792592307712823296,
            "femaleVariant": "こんにちは！"
          },
          {
            "stringId": 1792597148157743104,
            "femaleVariant": "こちらこそありがとう"
          },
          {
            "stringId": 1793917909808058368,
            "femaleVariant": "ジョスはもう手伝った。いいからさっさと混ぜてくれ"
          },
          {
            "stringId": 1793951003051167744,
            "femaleVariant": "それで、次はいつ行ける？"
          },
          {
            "stringId": 1793951325559590912,
            "femaleVariant": "よかった。あの子に何か持っていってあげたいの。音楽が好きだから… え？"
          },
          {
            "stringId": 1793951922560045056,
            "femaleVariant": "覚えておく。何でも言って"
          },
          {
            "stringId": 1793952342023999488,
            "femaleVariant": "本当にありがとう。すごく助かった。うん… ええ、また話しましょ"
          },
          {
            "stringId": 1805317148954824708,
            "femaleVariant": "来たか。料理を手伝ってくれ"
          },
          {
            "stringId": 1805317148954824720,
            "femaleVariant": "何を作ってるの？",
            "maleVariant": "何を作ってんだ？"
          },
          {
            "stringId": 1805317148954824744,
            "femaleVariant": "ジョスがやった方がいいんじゃない？",
            "maleVariant": "ジョスがやった方がいいんじゃないか？"
          },
          {
            "stringId": 1805324684592386052,
            "femaleVariant": "ウォード家に伝わる秘密のレシピ？"
          },
          {
            "stringId": 1805324684592386064,
            "femaleVariant": "ああ。玉ネギ、パプリカ、タイム…"
          },
          {
            "stringId": 1805543136359845892,
            "femaleVariant": "いつもと様子が違うね",
            "maleVariant": "いつもと様子が違うな"
          },
          {
            "stringId": 1805543746245201920,
            "femaleVariant": "一緒に料理するってのは、冗談かと"
          },
          {
            "stringId": 1805544474007277568,
            "femaleVariant": "今日は雰囲気がいつもと違うね",
            "maleVariant": "今日は雰囲気がいつもと違うな"
          },
          {
            "stringId": 1805544512913641472,
            "femaleVariant": "正直、一緒に料理をするなんて冗談かと"
          },
          {
            "stringId": 1805545602744815616,
            "femaleVariant": "うん… 変に普通っていうか。ぐっすり眠れたみたいな",
            "maleVariant": "ああ… 変に普通っていうか。ぐっすり眠れたみたいな"
          },
          {
            "stringId": 1805545845175586816,
            "femaleVariant": "そっちはジョスに任せて、昼間っからお酒でも…",
            "maleVariant": "そっちはジョスに任せて、昼間っから酒でも…"
          },
          {
            "stringId": 1823884956834861056,
            "femaleVariant": "始末したの？",
            "maleVariant": "始末したのか？"
          },
          {
            "stringId": 1823884987990151168,
            "femaleVariant": "いつでも命を奪える状況だったわけでしょ",
            "maleVariant": "いつでも命を奪える状況だったわけだろ"
          },
          {
            "stringId": 1823885021813018624,
            "femaleVariant": "まだこん睡状態？",
            "maleVariant": "まだこん睡状態か？"
          },
          {
            "stringId": 1823886683646595072,
            "femaleVariant": "本当にそうしたいのかわからなかった"
          },
          {
            "stringId": 1823887329804292096,
            "femaleVariant": "俺もそう思った。簡単だってな…"
          },
          {
            "stringId": 1823887919405993984,
            "femaleVariant": "ああ。生と死の間でまどろんでる。病院のベッドにいるのはただの抜け殻だ"
          },
          {
            "stringId": 1844535253764542464,
            "femaleVariant": "ピクニックなんて柄じゃない。お前は楽しめよ"
          },
          {
            "stringId": 1844539881474363392,
            "femaleVariant": "まだ入院中。ショックを受けてるけど、回復してきてる"
          },
          {
            "stringId": 1844540005474766848,
            "femaleVariant": "感謝の気持ちをちゃんと伝えてなかったわね"
          },
          {
            "stringId": 1845939007655120896,
            "femaleVariant": "無理だよ。ジョスは料理の“り”の字も知らないんだ"
          },
          {
            "stringId": 1845939459297775616,
            "femaleVariant": "でも、俺は実は大の料理好きでな。最近はあまり機会がなかった"
          },
          {
            "stringId": 1845942268642594816,
            "femaleVariant": "そうか？"
          },
          {
            "stringId": 1845944266758688768,
            "femaleVariant": "まあ、こういう俺も見てほしかったんだ"
          },
          {
            "stringId": 1846042664576880640,
            "femaleVariant": "今日がそういう日じゃないのはわかってるけど、聞かせて",
            "maleVariant": "今日がそういう日じゃないのはわかってるが、聞かせてくれ"
          },
          {
            "stringId": 1846042691554643968,
            "femaleVariant": "ランディの様子は… どう？",
            "maleVariant": "ランディの様子は… どうだ？"
          },
          {
            "stringId": 1846043889145876480,
            "femaleVariant": "昔の自分を取り戻そうとしてる"
          },
          {
            "stringId": 1846044045777965056,
            "femaleVariant": "でも肉体的に回復するのは、ずっと先のことになるだろうな"
          },
          {
            "stringId": 1846044205362843648,
            "femaleVariant": "それで… 精神的には？ ひどい経験だったでしょ…",
            "maleVariant": "それで… 精神的には？ ひどい経験だっただろ…"
          },
          {
            "stringId": 1846044378235277312,
            "femaleVariant": "実をいうと、ハリスのおかげで、良い方向に変われたかもしれない"
          },
          {
            "stringId": 1846044517838491648,
            "femaleVariant": "許せない事件だが… いいキッカケになった気がする"
          },
          {
            "stringId": 1846048007214206976,
            "femaleVariant": "ピーター・パンはどうなったの？",
            "maleVariant": "ピーター・パンはどうなった？"
          },
          {
            "stringId": 1846048161312935936,
            "femaleVariant": "始末したのかって意味か"
          },
          {
            "stringId": 1846048601681301504,
            "femaleVariant": "後で話そう"
          },
          {
            "stringId": 1846049752095002624,
            "femaleVariant": "ええ、わかった。ただ… 気になって",
            "maleVariant": "ああ、わかった。ただ… 気になってな"
          },
          {
            "stringId": 1846050296297558016,
            "femaleVariant": "わかってる"
          },
          {
            "stringId": 1846053967571849216,
            "femaleVariant": "よし、もう十分混ぜただろう"
          },
          {
            "stringId": 1846054508049862656,
            "femaleVariant": "台所にある米を頼めるか"
          },
          {
            "stringId": 1846055070556360704,
            "femaleVariant": "はいはい"
          },
          {
            "stringId": 1848698917249748992,
            "femaleVariant": "奴に会いに病院に行った"
          },
          {
            "stringId": 1848836810983215104,
            "femaleVariant": "あいつの頭に銃を押しつけた時…"
          },
          {
            "stringId": 1848838973096595456,
            "femaleVariant": "あることを思い出して…"
          },
          {
            "stringId": 1848839389590982656,
            "femaleVariant": "両親が死んだ時のことだ。この話、聞くか？"
          },
          {
            "stringId": 1848842770518773760,
            "femaleVariant": "今は… やめておいたほうがいいかも",
            "maleVariant": "今は… やめておいたほうがいいかもな"
          },
          {
            "stringId": 1848844123685130240,
            "femaleVariant": "俺の親父は農場を持っててな。でも景気が悪くなって、たたまざるを得なくなった…"
          },
          {
            "stringId": 1848844563667619840,
            "femaleVariant": "それからどうにか小さな食料品店を開いて、2階で暮らしてたんだ"
          },
          {
            "stringId": 1848848292437430272,
            "femaleVariant": "現金目的の強盗だ。レジが空だから2階にまで押し入ってきた"
          },
          {
            "stringId": 1848850226045128704,
            "femaleVariant": "親父はひざまずかされて、金の隠し場所を教えろと要求された"
          },
          {
            "stringId": 1848850575984300032,
            "femaleVariant": "あと覚えてるのは、もう何もないって連呼する親父と、ただ泣き叫ぶお袋の姿だよ…"
          },
          {
            "stringId": 1848851324013252608,
            "femaleVariant": "リバー、もう話さなくていいよ",
            "maleVariant": "リバー、もう話さなくていいんだ"
          },
          {
            "stringId": 1848851350722580480,
            "femaleVariant": "連中の一人はラリってて、そいつが最初に発砲。親父の頭が半分吹き飛んだ"
          },
          {
            "stringId": 1848851547468992512,
            "femaleVariant": "こういうのは聞き飽きてるか？"
          },
          {
            "stringId": 1848852298937274368,
            "femaleVariant": "そんな… リバー… ごめん",
            "maleVariant": "そんな… リバー… すまん"
          },
          {
            "stringId": 1848852456743768064,
            "femaleVariant": "そいつは完全に狂ってやがった。次は俺に銃を渡して、お袋の頭を狙えって言いやがったんだ"
          },
          {
            "stringId": 1848852641561579520,
            "femaleVariant": "今でも、銃身の先にあったお袋の顔が忘れられない"
          },
          {
            "stringId": 1848854783324196864,
            "femaleVariant": "それで…？"
          },
          {
            "stringId": 1848855534691815424,
            "femaleVariant": "俺は… ただ動けなかった"
          },
          {
            "stringId": 1848855891811635200,
            "femaleVariant": "結局、そいつが俺から銃を奪い取ってお袋を撃ち、連中は立ち去った。犯人はまだ捕まっていない"
          },
          {
            "stringId": 1848856729229594624,
            "femaleVariant": "だから警官になったんだ。あんな連中が自由にうろついて、誰にもとがめられずにいるのが許せなくてな"
          },
          {
            "stringId": 1848858376265666560,
            "femaleVariant": "本当につらい経験をしたのね",
            "maleVariant": "本当につらい経験をしたんだな"
          },
          {
            "stringId": 1848858380745183232,
            "femaleVariant": "ジョスも、その一部始終を目撃したの？",
            "maleVariant": "ジョスも、その一部始終を目撃したのか？"
          },
          {
            "stringId": 1848858960918089728,
            "femaleVariant": "だから… あの病院のベッドで、ハリスの額に銃身を当てた時…"
          },
          {
            "stringId": 1848859445041434624,
            "femaleVariant": "ああ。今でも俺を見ると、あの時オフクロの頭に銃を向けてた俺の姿が、ちらつくんだと思う"
          },
          {
            "stringId": 1848860489305690112,
            "femaleVariant": "子供の俺に戻ったんだ… そして同時に、両親を撃ったジャンキーにもな"
          },
          {
            "stringId": 1848863771298238464,
            "femaleVariant": "そうだったのね",
            "maleVariant": "そうだったのか"
          },
          {
            "stringId": 1848863808040341504,
            "femaleVariant": "じゃあ殺さなかったの？",
            "maleVariant": "じゃあ殺さなかったのか？"
          },
          {
            "stringId": 1848864659970592768,
            "femaleVariant": "俺だって殺しの経験はある。だが、処刑をしたことはない。一度もな"
          },
          {
            "stringId": 1848864978016276480,
            "femaleVariant": "だから急いで病院を出た。決着はついたって言い聞かせてな"
          },
          {
            "stringId": 1848867727248330752,
            "femaleVariant": "よかった。でもこれからどうするの？ 警察に戻る気はないんでしょ",
            "maleVariant": "よかった。でも、これからどうするんだ？ 警察に戻る気はないんだろ"
          },
          {
            "stringId": 1848868130287390720,
            "femaleVariant": "探偵も悪くないかな。まあ様子を見るさ"
          },
          {
            "stringId": 1872262060496261120,
            "femaleVariant": "ある晩、大きな音で目が覚めた…"
          },
          {
            "stringId": 1904172619986505728,
            "femaleVariant": "リバーは裏よ。最近あの人は、いつもあなたの話ばっかり",
            "maleVariant": "リバーは裏よ。あなたを… 待ってたわ"
          },
          {
            "stringId": 1905691732789563392,
            "femaleVariant": "私もそう言ったのよ。でもいつも同じことを言われて。型にはまらない子供のことなんて、誰も気にかけてくれないのよ"
          },
          {
            "stringId": 1905692788026118144,
            "femaleVariant": "うん、まさにそう言われた。もうその話をされるたびに耳が痛くって"
          },
          {
            "stringId": 1921535222464090116,
            "femaleVariant": "あれ、ここだと思ったのに…",
            "maleVariant": "あれ、ここだと思ったんだが…"
          },
          {
            "stringId": 1937170448015921156,
            "femaleVariant": "あの… お米は？"
          },
          {
            "stringId": 1937171366165843968,
            "femaleVariant": "調子はどう？",
            "maleVariant": "調子はどうだ？"
          },
          {
            "stringId": 1937172822092648448,
            "femaleVariant": "ジョス、お米はどこにある？"
          },
          {
            "stringId": 1937174436111159296,
            "femaleVariant": "後で電話する"
          },
          {
            "stringId": 1937204120140091392,
            "femaleVariant": "とにかく、お米は… そこよ"
          },
          {
            "stringId": 1944599319619448832,
            "femaleVariant": "[混ぜる] 代々伝わるレシピなの？",
            "maleVariant": "[混ぜる] 代々伝わるレシピなのか？"
          },
          {
            "stringId": 1968168959607242752,
            "femaleVariant": "ランディはどう？",
            "maleVariant": "ランディはどうだ？"
          },
          {
            "stringId": 1969089996066848768,
            "femaleVariant": "殺したの？",
            "maleVariant": "殺したのか？"
          },
          {
            "stringId": 1969089996083626012,
            "femaleVariant": "まだこん睡状態？"
          },
          {
            "stringId": 1969089996469501952,
            "femaleVariant": "もう話さなくていい"
          },
          {
            "stringId": 1969089996771491840,
            "femaleVariant": "本当につらい経験をしたのね",
            "maleVariant": "本当につらい経験をしたんだな"
          },
          {
            "stringId": 1969089996788269056,
            "femaleVariant": "ジョスも現場にいたの？",
            "maleVariant": "ジョスも現場にいたのか？"
          },
          {
            "stringId": 1969089996888932352,
            "femaleVariant": "そうだったのね",
            "maleVariant": "そうだったのか"
          },
          {
            "stringId": 1969089996905709572,
            "femaleVariant": "じゃあ殺さなかったのね",
            "maleVariant": "じゃあ殺さなかったんだな"
          },
          {
            "stringId": 1969106268590637056,
            "femaleVariant": "次は煮立たせて味を染み込ませる。向こうで待とう"
          },
          {
            "stringId": 1969153477763858432,
            "femaleVariant": "座れよ… ビールでも何でも好きなのを"
          },
          {
            "stringId": 1969159772726296576,
            "femaleVariant": "[座る] いい場所だね",
            "maleVariant": "[座る] いい場所だな"
          },
          {
            "stringId": 1969161237058486272,
            "femaleVariant": "いい場所だね",
            "maleVariant": "いい場所だな"
          },
          {
            "stringId": 1969188799994331136,
            "femaleVariant": "リバーおじさん、遊んでくれるんでしょ！"
          },
          {
            "stringId": 1969530769014980608,
            "femaleVariant": "ああ。悪いことなんか何も起きなかったみたいだろ"
          },
          {
            "stringId": 1970444021232238608,
            "femaleVariant": "うん、わかった",
            "maleVariant": "ああ、わかった"
          },
          {
            "stringId": 1970445471790333952,
            "femaleVariant": "ピーター・パンへの報復はどうなったの？",
            "maleVariant": "ピーター・パンへの報復はどうなった？"
          },
          {
            "stringId": 1970445528128225280,
            "femaleVariant": "ランディの様子はどう？",
            "maleVariant": "ランディの様子はどうだ？"
          },
          {
            "stringId": 1970455480255315968,
            "femaleVariant": "元気だった？",
            "maleVariant": "元気だったか？"
          },
          {
            "stringId": 1978155355433881600,
            "femaleVariant": "あー、ソイミート？ わかった",
            "maleVariant": "あー、ソイミートか？ わかった"
          },
          {
            "stringId": 1978162610673831936,
            "femaleVariant": "焦げないように混ぜ続けないと"
          },
          {
            "stringId": 1978165895501729796,
            "femaleVariant": "まだ混ぜてるか？"
          },
          {
            "stringId": 1978168286104014848,
            "femaleVariant": "俺のジャンバラヤは世界一だからな"
          },
          {
            "stringId": 1978169142832549888,
            "femaleVariant": "セロリとニンニクをみじん切りにしないと。お前は混ぜててくれ。とにかく混ぜりゃいい"
          },
          {
            "stringId": 1978170469407019008,
            "femaleVariant": "さっきから混ぜてるって"
          },
          {
            "stringId": 1978181018886885376,
            "femaleVariant": "了解、シェフ"
          },
