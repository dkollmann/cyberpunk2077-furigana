              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "止まれ！",
                    "maleVariant": null,
                    "stringId": 1923080477339914240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "フィクサーに頼まれてね。別にあんたに恨みはない",
                    "maleVariant": "フィクサーに頼まれてな。別にあんたに恨みはない",
                    "stringId": 1923082299630784516
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "フィクサーの指示よ",
                    "maleVariant": "フィクサーの指示だ",
                    "stringId": 1923083357685911556
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "GPSの信号よ。盗んだ薬に送信機がついてたの",
                    "maleVariant": "GPSの信号だ。盗んだ薬に送信機がついてたのさ",
                    "stringId": 1924328600439721984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "GPS信号で",
                    "maleVariant": null,
                    "stringId": 1925693695782653952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "止まるから、そっちも引き金に気をつけて",
                    "maleVariant": "止まるから、そっちも引き金に気をつけてくれ",
                    "stringId": 1925745996824154112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "盗んだ薬を渡せば、引き上げる",
                    "maleVariant": null,
                    "stringId": 1925760444288831488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "盗んだ！？ ハハハ！ バロペリドールは効きやしない！ あれを売って金を盗んでるのは誰だ？ ええ？",
                    "maleVariant": null,
                    "stringId": 1925852223159767040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "盗んだ！？ ハハハ！ バロペリドールは効きやしない！ あれを売って金を盗んでるのは誰だ？ ええ？",
                    "maleVariant": null,
                    "stringId": 1925852426331852800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "薬を渡せば、あたしは消える",
                    "maleVariant": "薬を渡せば、俺は消える",
                    "stringId": 1925885647769669632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしてここが？",
                    "maleVariant": null,
                    "stringId": 1925940552937082880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ぬあああッ！ 俺は、狂ってなんかない！",
                    "maleVariant": null,
                    "stringId": 1925950590997073920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サイバーサイコシスに使うバロペリドール？",
                    "maleVariant": "サイバーサイコシスに使うバロペリドールか？",
                    "stringId": 1947640913972269056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "落ち着いて…",
                    "maleVariant": "おい、落ち着けよ…",
                    "stringId": 1947696791211577344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だ、黙れ！ あっちへ行け！",
                    "maleVariant": null,
                    "stringId": 1947697116840562688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうせ効かないなら返して",
                    "maleVariant": "どうせ効かないなら返せ",
                    "stringId": 1947698691751383040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "渡さないとマックス・タックを呼ぶ",
                    "maleVariant": null,
                    "stringId": 1947699443605540864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どちらか選んで",
                    "maleVariant": "どちらか選べ",
                    "stringId": 1947700634100011008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "効かない薬なら持っててもしょうがないでしょ？ 返して",
                    "maleVariant": "効かない薬なら持っててもしょうがないだろ？ 返してくれ",
                    "stringId": 1947700927516741632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リパードクだ！ 奴らこそ本当の犯罪者だ！",
                    "maleVariant": null,
                    "stringId": 1947707187599347712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前も同じだ… リパーや政治家… ホルトのクソ野郎と違わない！",
                    "maleVariant": null,
                    "stringId": 1947710731618455552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ホルトを殺しても何も変わらない",
                    "maleVariant": null,
                    "stringId": 1947710974116335620
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたは兵士。テロリストじゃない",
                    "maleVariant": "あんたは兵士だ。テロリストじゃない",
                    "stringId": 1947738819647606784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これ以上話してもムダ",
                    "maleVariant": "これ以上話してもムダだ",
                    "stringId": 1947746420951191552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "好きにすればいい。でも薬は返して",
                    "maleVariant": "好きにすればいい。だが薬は返してくれ",
                    "stringId": 1947747827536203776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしにはどうでもいい",
                    "maleVariant": "俺にはどうでもいい",
                    "stringId": 1947748228931096580
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "市長を殺して、それで何か変わると思うなら、勝手にすればいいんじゃない？",
                    "maleVariant": "市長を殺して、それで何か変わると思うなら、勝手にすればいいんじゃないか？",
                    "stringId": 1947748228931096600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こっちは薬が必要なだけ",
                    "maleVariant": "こっちは薬が必要なだけだ",
                    "stringId": 1947748228931096620
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハハハ… そうか… わかりきったことだな…",
                    "maleVariant": null,
                    "stringId": 1947748228931096640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いよいよ正気をなくしたようね。終わりにしましょう",
                    "maleVariant": "いよいよ正気をなくしたようだな。終わりにしよう",
                    "stringId": 1947749449775550468
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前もシステムの一部… しょ、所詮はコマ… ただの奴隷だ…",
                    "maleVariant": null,
                    "stringId": 1947749449792327696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "答えるだけムダ",
                    "maleVariant": "答えるだけムダだ",
                    "stringId": 1947783465027342336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "話すだけ時間のムダよ。さあ、観念して",
                    "maleVariant": "話すだけ時間のムダだ。さあ、観念しろ",
                    "stringId": 1947783730224795648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それなら手っ取り早い",
                    "maleVariant": null,
                    "stringId": 1947784044076175360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ポリマー強化型ミリテクHKS-47。兵士だったのね",
                    "maleVariant": "ポリマー強化型ミリテクHKS-47。兵士だったのか",
                    "stringId": 1947788844926865412
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミリテク第3軽師団、第155歩兵連隊。ジャングルで7年だ… 今よりわかりやすい時代だった",
                    "maleVariant": null,
                    "stringId": 1947788844926865432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "病気になる前？",
                    "maleVariant": "病気になる前か？",
                    "stringId": 1947788844943642636
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "電池のように使い捨てやがって",
                    "maleVariant": null,
                    "stringId": 1947788844943642656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたの怒りはわかる",
                    "maleVariant": null,
                    "stringId": 1947788844993974272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "従軍先は？",
                    "maleVariant": null,
                    "stringId": 1947788844993974304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかるよ。裏切られて悔しいんでしょ",
                    "maleVariant": "わかるさ。裏切られて悔しいんだろ",
                    "stringId": 1947788845111414788
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺だけじゃない！",
                    "maleVariant": null,
                    "stringId": 1947788845128192000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかってる。でも、ホルトを殺せば、あんたと同じ立場の人を困らせることになる。みんな捕まるよ",
                    "maleVariant": "わかってる。でも、ホルトを殺せば、あんたと同じ立場の人を困らせることになる。みんな捕まるぞ",
                    "stringId": 1947788845128192020
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "従軍先は？",
                    "maleVariant": null,
                    "stringId": 1947788845161746436
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ブラジルだ。今も覚えてる… 顔についた泥… いつもヘトヘトで… ジメジメして…",
                    "maleVariant": null,
                    "stringId": 1947788845178523664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まるで、再生を止められないBDみたいなんだ…",
                    "maleVariant": null,
                    "stringId": 1947788845178523684
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "フラッシュバックね",
                    "maleVariant": "フラッシュバックか",
                    "stringId": 1947788845195300864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの頃は、ただ同じ隊の仲間がすべてで… クソッ、俺をあっさり捨てやがって…",
                    "maleVariant": null,
                    "stringId": 1947788845195300884
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もうすぐ選挙よ。今ホルトが死んだら、反サイバーサイコシス政策を打ち出した政治家が甘い汁を吸うだけ",
                    "maleVariant": "もうすぐ選挙だ。今ホルトが死んだら、反サイバーサイコシス政策を打ち出した政治家が甘い汁を吸うだけだぞ",
                    "stringId": 1947811611894190080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいや！ そんなことはない！ 大衆はわかってくれる！ システムは崩壊するんだ！",
                    "maleVariant": null,
                    "stringId": 1947827423497523200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたのような発症者はみんな狩られる",
                    "maleVariant": null,
                    "stringId": 1947829203795021824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう… 消えてくれ…",
                    "maleVariant": null,
                    "stringId": 1947859677208743936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "薬なしで帰るわけにはいかない",
                    "maleVariant": "薬なしで帰るわけにはない",
                    "stringId": 1947860220941537280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そこだ… 少し一人にしてくれ…",
                    "maleVariant": null,
                    "stringId": 1947860594855350272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だが待ってろ… 俺が奴の… 息の根を止めてやる",
                    "maleVariant": null,
                    "stringId": 1954556513713754112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "他の政治家がホルトの死を利用するだけよ",
                    "maleVariant": "他の政治家がホルトの死を利用するだけだ",
                    "stringId": 1954558463662460928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サイコと呼ばれておしまいよ",
                    "maleVariant": "サイコと呼ばれておしまいだ",
                    "stringId": 1954558734463504384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "政治家が1人死んでも何も変わらない",
                    "maleVariant": null,
                    "stringId": 1954558802679664640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ホルトを殺したら、あんたは正式にサイバーサイコと呼ばれるでしょうね",
                    "maleVariant": "ホルトを殺したら、あんたは正式にサイバーサイコと呼ばれるだろうな",
                    "stringId": 1954559218989502464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あっそ",
                    "maleVariant": "そうかよ",
                    "stringId": 1954609936916123648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "変わるさ！ 口先だけの悪魔どもは、二度と民衆を敵に回さなくなるはずだ！",
                    "maleVariant": null,
                    "stringId": 1954615156341575680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺が示してやるんだ！ これ以上俺たちを不当に扱ったら、お、俺たちを見くびったら、どうなるかをな！",
                    "maleVariant": null,
                    "stringId": 1954654076714455040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺のことはいい！ これは同じ境遇の人々のためにやるんだ！",
                    "maleVariant": null,
                    "stringId": 1954676251546718208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいえ、逆効果よ。街の人はそんな事件にどう反応する？",
                    "maleVariant": "いいや、逆効果だ。街の人はそんな事件にどう反応する？",
                    "stringId": 1954676793014587392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サイバーサイコシスを発症したテロリストと、メディアを味方につけた政治家。どっちが好かれると思う？",
                    "maleVariant": null,
                    "stringId": 1954695649212862464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "少しは違うと思うけど",
                    "maleVariant": "少しは違うと思うがな",
                    "stringId": 1954893056714313728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "黙れ！ ホルトは、研究予算を削減しやがった！ 患者のことなどお構いなしに！",
                    "maleVariant": null,
                    "stringId": 1954897356496670720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「サイバーサイコシス発症者が市長を惨殺」。そんな奴に協力する者はいない…",
                    "maleVariant": null,
                    "stringId": 1954928971734159360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "盗んだものを返すか――",
                    "maleVariant": null,
                    "stringId": 1957975186767974400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "な、なぜあんな奴らの味方を？",
                    "maleVariant": null,
                    "stringId": 1957975431597887488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マックス・タックにお迎えに来てもらうか",
                    "maleVariant": "マックス・タックに迎えに来てもらうか",
                    "stringId": 1957975704596746240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "貴様も同じ… 裏切り者の… クソったれだ！",
                    "maleVariant": null,
                    "stringId": 1957976673631965184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だからってテロ行為は…",
                    "maleVariant": null,
                    "stringId": 1957999533914165248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "昨日の夜も五月蠅かったな",
                    "maleVariant": null,
                    "stringId": 1958020391030329344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "死にかけの動物みたいな声だろ。ぐああって",
                    "maleVariant": null,
                    "stringId": 1958021105169305600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だな… かといって連中に頼めば、一生金をせびられるぞ！",
                    "maleVariant": null,
                    "stringId": 1958021136341372928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "シー、聞こえるぞ！",
                    "maleVariant": null,
                    "stringId": 1958034090231173120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "タイガークロウズの目があるから、警察に通報するわけにもいかない",
                    "maleVariant": null,
                    "stringId": 1958034876696727552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何とか追い出す方法はないのか？",
                    "maleVariant": null,
                    "stringId": 1958039733281329152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあな…",
                    "maleVariant": null,
                    "stringId": 1958039984822128640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "動くなと言っただろ！",
                    "maleVariant": null,
                    "stringId": 2004013097929965568
                  }
                }
              ]
            }
          }
        }
      }
    },
