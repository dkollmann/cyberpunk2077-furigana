          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "奴ら、もう来たの？",
                    "maleVariant": null,
                    "stringId": 1610055420243058688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何とかするから、あと1分ちょうだい！",
                    "maleVariant": null,
                    "stringId": 1610056278850641920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふう… よかった、うまくいったみたい",
                    "maleVariant": null,
                    "stringId": 1610056998173777920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さすがだね、V。恩に着るよ",
                    "maleVariant": null,
                    "stringId": 1610060807474225152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも、ほとぼりが冷めるまで、オカダがかくまってくれることになってる",
                    "maleVariant": null,
                    "stringId": 1612748491237859328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やあ、あんたがVだね",
                    "maleVariant": null,
                    "stringId": 1734578835890688000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "依頼主はどこ？",
                    "maleVariant": "依頼主はどこだ？",
                    "stringId": 1734579230172041220
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ウイルスのことを聞きたい",
                    "maleVariant": null,
                    "stringId": 1734579284496666624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "依頼主は？",
                    "maleVariant": null,
                    "stringId": 1734579393817006080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ウイルスのこと、何か知ってる？",
                    "maleVariant": "ウイルスについて、何を知ってる？",
                    "stringId": 1734579421952397312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "出発しようか",
                    "maleVariant": null,
                    "stringId": 1734580252290371584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ウイルスはどうやって外部と通信を？",
                    "maleVariant": null,
                    "stringId": 1734580252290371600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ウイルスはどうやって外部と通信を？",
                    "maleVariant": null,
                    "stringId": 1734580252323926016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ウイルスの信号を抑えればいい",
                    "maleVariant": null,
                    "stringId": 1734580898162855936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ネットワークへの信号を抑えたらどう？",
                    "maleVariant": "ネットワークへの信号を抑えたらどうだ？",
                    "stringId": 1734580999849562112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ、出発しよう",
                    "maleVariant": null,
                    "stringId": 1734581680400551936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "噂はいろいろ聞いてるよ",
                    "maleVariant": null,
                    "stringId": 1737006367093084160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私はアンブリッジ。ウイルス担当よ",
                    "maleVariant": null,
                    "stringId": 1737017640526598144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よろしくね",
                    "maleVariant": "よろしくな",
                    "stringId": 1737017780633128960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうなの？",
                    "maleVariant": "そうなのか？",
                    "stringId": 1737022107326140416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあね。かなりのやり手だって",
                    "maleVariant": null,
                    "stringId": 1737022145292980224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今は後部座席で眠ってる。こっちはいつでも行けるよ",
                    "maleVariant": null,
                    "stringId": 1737031048491749376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "移動しながらリセットする気？",
                    "maleVariant": "移動しながらリセットする気か？",
                    "stringId": 1737031451782467584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうしないとこちらの居場所がばれる可能性がある。大丈夫、大して難しい仕事じゃない",
                    "maleVariant": null,
                    "stringId": 1737031668829310976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたには無理だろうけど",
                    "maleVariant": null,
                    "stringId": 1737032643065466880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "とにかく、移動してれば相手から追跡されにくくなるの",
                    "maleVariant": null,
                    "stringId": 1737033155441643520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "高度な暗号化を施してあるから、きっと裏にいるのはミリテクだね",
                    "maleVariant": null,
                    "stringId": 1737034963287330816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "除去できる？",
                    "maleVariant": "除去できるか？",
                    "stringId": 1737035039791435776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ファームウェアは普通のヤツみたいだし、何とかね。ただし、ハードリセットは避けられない",
                    "maleVariant": null,
                    "stringId": 1737035094803927040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "つまり、ウイルスを仕込んだ奴のところにアラートが送られる",
                    "maleVariant": null,
                    "stringId": 1737035222310768640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "間違いなく、招かれざる客が現れるだろうね。それも、口より先に手が出るタイプの連中が…",
                    "maleVariant": null,
                    "stringId": 1737035331849211904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "フッ、運転はよろしく。こっちは任せて",
                    "maleVariant": null,
                    "stringId": 1737039624669024256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ソースコードにストリングが含まれてる… 簡単に言うと、インプラントを再起動したら、依頼主の座標を最寄りのルーターに送信するようになってるの",
                    "maleVariant": null,
                    "stringId": 1737040249200889856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言い換えれば、敵のランナーは三点測量で依頼主の正確な位置を割り出せるってわけ",
                    "maleVariant": null,
                    "stringId": 1737040581540761600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こっちは動き回るしかない。特定されないようにね",
                    "maleVariant": null,
                    "stringId": 1737040723945771008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "帯域幅を制限するってこと？",
                    "maleVariant": null,
                    "stringId": 1737041110056620032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうすればアクセスポイントへの接続に多少はラグを起こせる",
                    "maleVariant": null,
                    "stringId": 1737041228570873856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "へえ、なかなかやるじゃない！ けど、それでも位置がばれるのに変わりはない…",
                    "maleVariant": null,
                    "stringId": 1737041348158869504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "若干の遅れは生じる",
                    "maleVariant": null,
                    "stringId": 1737041585053159424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ないよりマシね。今すぐスクリプトを書いてみる",
                    "maleVariant": null,
                    "stringId": 1737041839915847680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クソ、見つかった！",
                    "maleVariant": null,
                    "stringId": 1737047207198904320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "依頼主の様子は？",
                    "maleVariant": null,
                    "stringId": 1737051064935174144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "心配いらないよ。これで依頼主の問題が消えてなくなったわけじゃないけどね",
                    "maleVariant": null,
                    "stringId": 1737051143972638720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あのゲートを突破したとこなんか… 惚れ惚れしたよ",
                    "maleVariant": null,
                    "stringId": 1737051671901294592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それじゃ… またよろしくね、V",
                    "maleVariant": null,
                    "stringId": 1737051745939148800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと！ 私たちを殺す気？",
                    "maleVariant": null,
                    "stringId": 1895399092352688128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大したことない",
                    "maleVariant": null,
                    "stringId": 1925862126262734852
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "依頼主の様子は？",
                    "maleVariant": null,
                    "stringId": 1925862222731726848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よくやってくれたね、V。感謝するよ",
                    "maleVariant": null,
                    "stringId": 1925864270911033344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "仕事をしただけ",
                    "maleVariant": "仕事をしただけだ",
                    "stringId": 1925864646653562880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんた、謙虚だね",
                    "maleVariant": null,
                    "stringId": 1925865128310657024
                  }
                }
              ]
            }
          }
        }
      }
    },
