          {
            "stringId": 1960480432233525252,
            "femaleVariant": "ふふふん、ふふふん、ふふふん…"
          },
          {
            "stringId": 1960480432267079684,
            "femaleVariant": "おはよう！ もうすぐコーヒーができる"
          },
          {
            "stringId": 1960480432267079696,
            "femaleVariant": "寝言言ってたよ",
            "maleVariant": "寝言言ってたぞ"
          },
          {
            "stringId": 1960480432283856904,
            "femaleVariant": "そうか？ 倉庫の中で何かを探してる夢を見たんだ"
          },
          {
            "stringId": 1960480432283856916,
            "femaleVariant": "よほど広かったのね。大声だったから",
            "maleVariant": "よほど広かったんだな。大声だった"
          },
          {
            "stringId": 1960480432283856952,
            "femaleVariant": "飲んでみろ。人生最高のコーヒーだぞ"
          },
          {
            "stringId": 1960480432820727812,
            "femaleVariant": "で、考えてたんだが…"
          },
          {
            "stringId": 1960480432820727824,
            "femaleVariant": "何？",
            "maleVariant": "なんだ？"
          },
          {
            "stringId": 1960480432820727836,
            "femaleVariant": "昨夜のことだけど… 俺たち… この先どうなる？ 長期的に見てどうなんだ？"
          },
          {
            "stringId": 1960480432854282240,
            "femaleVariant": "[キスする] あるよ",
            "maleVariant": "[キスする] ある"
          },
          {
            "stringId": 1960480432854282256,
            "femaleVariant": "[離れる] ただのセックスだったのよ",
            "maleVariant": "[離れる] ただのセックスだったんだ"
          },
          {
            "stringId": 1960480433022054404,
            "femaleVariant": "ええ、うまく行く気がする。もっと一緒にいたい",
            "maleVariant": "ああ、うまく行く気がする。もっと一緒にいたい"
          },
          {
            "stringId": 1960480433038831624,
            "femaleVariant": "試してみよう"
          },
          {
            "stringId": 1960480433038831648,
            "femaleVariant": "ああ、試そう"
          },
          {
            "stringId": 1960480433038831660,
            "femaleVariant": "でも、うまくいかなくても… 友達のままでしょ？",
            "maleVariant": "ただうまくいかなくても… 友達のままだよな？"
          },
          {
            "stringId": 1960480433038831672,
            "femaleVariant": "あまり先のことは考えるなって"
          },
          {
            "stringId": 1960480433038831684,
            "femaleVariant": "うん、そうね",
            "maleVariant": "ああ、そうだな"
          },
          {
            "stringId": 1960480433072386052,
            "femaleVariant": "セックスはよかった。でもそれだけよ",
            "maleVariant": "セックスはよかった。でもそれだけさ"
          },
          {
            "stringId": 1960480433072386064,
            "femaleVariant": "お前が好きだ、V"
          },
          {
            "stringId": 1960480433072386076,
            "femaleVariant": "あたしもリバーが好きよ。でも、だからこそなの",
            "maleVariant": "俺もリバーが好きだ。でも、だからこそなんだ"
          },
          {
            "stringId": 1960480433072386088,
            "femaleVariant": "どういうことだ？"
          },
          {
            "stringId": 1960480433072386100,
            "femaleVariant": "大げさなことにしたくない。あたしたちは楽しんだ。それでいいじゃない",
            "maleVariant": "大げさなことにしたくない。俺たちは楽しんだ。それでいいじゃないか"
          },
          {
            "stringId": 1960480433072386112,
            "femaleVariant": "ああ…"
          },
          {
            "stringId": 1960480433105940508,
            "femaleVariant": "あたしはいいから。ランディによろしくね",
            "maleVariant": "俺はいいから。ランディによろしくな"
          },
          {
            "stringId": 1960480433105940520,
            "femaleVariant": "あぁ… 悪いな、V。また近いうちに会えるか？"
          },
          {
            "stringId": 1960480433105940532,
            "femaleVariant": "もちろん"
          },
          {
            "stringId": 1960480433105940544,
            "femaleVariant": "じゃあまたな"
          },
          {
            "stringId": 1960480433223380992,
            "femaleVariant": "リバー… 先のことはまだわからない"
          },
          {
            "stringId": 1960480433223381008,
            "femaleVariant": "朝から深刻な話ね…",
            "maleVariant": "朝から深刻な話だな…"
          },
          {
            "stringId": 1960480433391153156,
            "femaleVariant": "夕べあんたは2人と寝た"
          },
          {
            "stringId": 1960480433391153168,
            "femaleVariant": "あたしの頭の中には、コンストラクトが宿ってる。アラサカの試験的な生体チップのせいでね",
            "maleVariant": "俺の頭の中には、コンストラクトが宿ってる。アラサカの試験的な生体チップのせいでな"
          },
          {
            "stringId": 1960480433407930372,
            "femaleVariant": "コンストラクト？ 他人の精神ってことか？"
          },
          {
            "stringId": 1960480433407930384,
            "femaleVariant": "そう。ジョニー・シルヴァーハンドの記憶痕跡。あのシルヴァーハンドよ。それが悪化してるの",
            "maleVariant": "そうだ。ジョニー・シルヴァーハンドの記憶痕跡。あのシルヴァーハンドさ。それが悪化してるんだ"
          },
          {
            "stringId": 1960480433407930396,
            "femaleVariant": "だが… すまん、よくわからない"
          },
          {
            "stringId": 1960480433441484804,
            "femaleVariant": "はぁ… 朝一番なのに深刻な話ね",
            "maleVariant": "はぁ… 朝一番なのに深刻な話だな"
          },
          {
            "stringId": 1960480433458262016,
            "femaleVariant": "とにかく答えてくれ"
          },
          {
            "stringId": 1960480433458262028,
            "femaleVariant": "はっきりさせたいんだ"
          },
          {
            "stringId": 1960480433475039232,
            "femaleVariant": "あたしの脳を破壊してるの",
            "maleVariant": "俺の脳を破壊してるんだ"
          },
          {
            "stringId": 1960480433491816456,
            "femaleVariant": "今はVと話してるけど、明日には…",
            "maleVariant": "今はVと話してるが、明日には…"
          },
          {
            "stringId": 1960480433676365828,
            "femaleVariant": "あいつはあたしの神経を乗っ取っちゃうの。少しずつ脳を破壊してるのよ",
            "maleVariant": "あいつは俺の神経を乗っ取っちまうんだ。少しずつ脳を破壊してるのさ"
          },
          {
            "stringId": 1960480433693143040,
            "femaleVariant": "でも元に戻せるんだろ？ 治療できれば死ななくてすむんだよな？"
          },
          {
            "stringId": 1960480433693143052,
            "femaleVariant": "実は、それがそうでもなくて。手遅れになる前に、脳からあいつを取り除く必要がある",
            "maleVariant": "実は、それがそうでもなくてな。手遅れになる前に、脳からあいつを取り除く必要がある"
          },
          {
            "stringId": 1960480433693143064,
            "femaleVariant": "そんな…"
          },
          {
            "stringId": 1960480433709920260,
            "femaleVariant": "今こうやって話をしてるあたしはVだけど、明日には自分のことしか考えられないロッカーボーイになってるかもしれない",
            "maleVariant": "今こうやって話をしてる俺はVだが、明日には自分のことしか考えられないロッカーボーイになってるかもしれない"
          },
          {
            "stringId": 1960480433709920272,
            "femaleVariant": "言っておくけど、絶対ウマが合わない男よ",
            "maleVariant": "言っておくが、絶対ウマが合わない男だぞ"
          },
          {
            "stringId": 1960480433709920284,
            "femaleVariant": "おい、マジなのかよ"
          },
          {
            "stringId": 1960480433743474688,
            "femaleVariant": "それでも付き合いたい？",
            "maleVariant": "それでも付き合いたいか？"
          },
          {
            "stringId": 1960480433743474704,
            "femaleVariant": "だから、真剣になるのはやめよう。あまり入れ込まないで",
            "maleVariant": "だから、真剣になるのはやめよう。あまり入れ込まないでくれ"
          },
          {
            "stringId": 1960480433911246852,
            "femaleVariant": "それでも付き合いたい？",
            "maleVariant": "それでも付き合いたいか？"
          },
          {
            "stringId": 1960480433911246864,
            "femaleVariant": "俺を何だと思ってる？ もちろんだ。そんなの関係ない"
          },
          {
            "stringId": 1960480433928024068,
            "femaleVariant": "だから、真剣になるのはやめよう。あまり入れ込まないで",
            "maleVariant": "だから、真剣になるのはやめよう。あまり入れ込まないでくれ"
          },
          {
            "stringId": 1960480433944801284,
            "femaleVariant": "ふざけるな。そんなの関係ないだろ"
          },
          {
            "stringId": 1960480433961578500,
            "femaleVariant": "問題は、お前がどうしたいかだ"
          },
          {
            "stringId": 1960480433961578512,
            "femaleVariant": "試してみるつもりはあるか？"
          },
          {
            "stringId": 1960480434246791168,
            "femaleVariant": "ねえ、リバー、夕べのことは… あの時は本気だった",
            "maleVariant": "なあ、リバー、夕べのことは… あの時は本気だった"
          },
          {
            "stringId": 1960480434246791180,
            "femaleVariant": "でも？"
          },
          {
            "stringId": 1960480434246791192,
            "femaleVariant": "未来のことは何にも約束できないの。ちょっと問題があって…",
            "maleVariant": "未来のことは何にも約束できない。ちょっと問題があってな…"
          },
          {
            "stringId": 1960480434246791204,
            "femaleVariant": "まだ俺に打ち明けてない秘密があるんだろ。わかるさ。警官の勘でわかる"
          },
          {
            "stringId": 1960494695970910212,
            "femaleVariant": "あーあ… 月に移住する時代に、まだ二日酔いは治せないのかよ？"
          },
          {
            "stringId": 1960494695970910224,
            "femaleVariant": "まあ… 楽しかったんだし、いいじゃない",
            "maleVariant": "まあ… 楽しかったんだし、いいだろ"
          },
          {
            "stringId": 1960494695987687428,
            "femaleVariant": "ああ。でももう行かないと。ジョスをランディの見舞いに連れてくって約束したんだ"
          },
          {
            "stringId": 1960494696004464644,
            "femaleVariant": "あと、急がなくていいぞ。疲れがとれるまでゆっくりしろ"
          },
          {
            "stringId": 1960494696004464656,
            "femaleVariant": "ランディによろしく"
          },
          {
            "stringId": 1960494696004464668,
            "femaleVariant": "ああ。またな、V"
          },
          {
            "stringId": 1960494696038019072,
            "femaleVariant": "運転できるの？",
            "maleVariant": "運転できるのか？"
          },
          {
            "stringId": 1960494696038019088,
            "femaleVariant": "ジョスに謝っておいて",
            "maleVariant": "ジョスに謝っといてくれ"
          },
          {
            "stringId": 1960494696054796292,
            "femaleVariant": "運転して大丈夫なの？",
            "maleVariant": "運転して大丈夫なのか？"
          },
          {
            "stringId": 1960494696054796304,
            "femaleVariant": "おいおい… 元NCPDだぞ。二日酔い運転のプロだ"
          },
          {
            "stringId": 1960494696054796316,
            "femaleVariant": "大丈夫さ"
          },
          {
            "stringId": 1960494696088350724,
            "femaleVariant": "ジョスに謝っておいて。昨日は食い逃げみたいになっちゃった",
            "maleVariant": "ジョスに謝っといてくれ。昨日は食い逃げみたいになったからさ"
          },
          {
            "stringId": 1960494696088350736,
            "femaleVariant": "トイレで盛大に吐いたしな？"
          },
          {
            "stringId": 1960494696088350748,
            "femaleVariant": "マジで…",
