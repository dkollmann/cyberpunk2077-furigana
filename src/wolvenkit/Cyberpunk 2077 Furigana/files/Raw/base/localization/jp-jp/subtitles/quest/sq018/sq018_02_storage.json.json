    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1699619057022443520,
            "femaleVariant": "いい写真ね",
            "maleVariant": "いい写真だな"
          },
          {
            "stringId": 1699639475498295296,
            "femaleVariant": "オフレンダの供え物は見つかったけど、もう少し見てみない？"
          },
          {
            "stringId": 1702632339975086080,
            "femaleVariant": "水槽？"
          },
          {
            "stringId": 1718781917489176576,
            "femaleVariant": "結局、マフラーを取り換えたんだ",
            "maleVariant": "結局、マフラーを取り換えたのか"
          },
          {
            "stringId": 1718782714323050496,
            "femaleVariant": "フルチューンされてる。あいつ、ほんとにバイクマニアだった…",
            "maleVariant": "フルチューンされてる。あいつ、ほんとにバイクマニアだったな…"
          },
          {
            "stringId": 1852991538098655232,
            "femaleVariant": "あら、V"
          },
          {
            "stringId": 1852992277017579524,
            "femaleVariant": "何してるの？",
            "maleVariant": "何してるんだ？"
          },
          {
            "stringId": 1852992530538090496,
            "femaleVariant": "ひとりでこんなとこにいて平気？",
            "maleVariant": "ひとりでこんなとこにいて平気か？"
          },
          {
            "stringId": 1852993264105082880,
            "femaleVariant": "ここで何を？"
          },
          {
            "stringId": 1852993476487860224,
            "femaleVariant": "ひとりなの？ ここはあんまり治安のいい場所じゃないよ、ミスティ",
            "maleVariant": "ひとりか？ ここはあんまり治安のいい場所じゃないぞ、ミスティ"
          },
          {
            "stringId": 1852995935876730880,
            "femaleVariant": "10歳の頃、よくここに座ってた。今は26歳… 何か起きるならもうとっくに起きてるでしょ"
          },
          {
            "stringId": 1852996823039135744,
            "femaleVariant": "こうやって待ってる… 彼のことをね。もう戻って来ないってわかってるのに"
          },
          {
            "stringId": 1852996931201847296,
            "femaleVariant": "バカみたいだけど… 彼のことが… 恋しくて"
          },
          {
            "stringId": 1852997822642442244,
            "femaleVariant": "ジャッキーがミスティのことを話してた"
          },
          {
            "stringId": 1852998246837571584,
            "femaleVariant": "いつ知り合ったの？",
            "maleVariant": "いつ知り合ったんだ？"
          },
          {
            "stringId": 1852998688430673920,
            "femaleVariant": "ジャッキーはよく話してたよ。ミスティがどれだけ大事な存在かってことをね",
            "maleVariant": "ジャッキーはよく話してたよ。ミスティがどれだけ大事な存在かってことをな"
          },
          {
            "stringId": 1852998900746342400,
            "femaleVariant": "いつ知り合ったの？",
            "maleVariant": "いつ知り合ったんだ？"
          },
          {
            "stringId": 1852999987859607552,
            "femaleVariant": "うそでしょ？ 少なくとも言葉は違ったと思うけど"
          },
          {
            "stringId": 1853000274917773312,
            "femaleVariant": "10年以上前。地元が同じだから、しょっちゅうお互い見かけてたんだけど、別に何かあったわけでもなかった"
          },
          {
            "stringId": 1853001989297590272,
            "femaleVariant": "そういう面もなくはなかったよ。やたらと前向きで、猪突猛進でさ…",
            "maleVariant": "そういう面もなくはなかった。やたらと前向きで、猪突猛進でさ…"
          },
          {
            "stringId": 1853002060080664580,
            "femaleVariant": "いい意味でそういう面もあった"
          },
          {
            "stringId": 1853002197435731968,
            "femaleVariant": "ミスティにとっては？"
          },
          {
            "stringId": 1853003700724617216,
            "femaleVariant": "彼は素直な人だったの。恐れることなく、精いっぱい生きようとしてた"
          },
          {
            "stringId": 1853004207111327744,
            "femaleVariant": "闇を抱えてもいたけど"
          },
          {
            "stringId": 1853004559969734656,
            "femaleVariant": "みんなそうでしょ？",
            "maleVariant": "みんなそうだろ？"
          },
          {
            "stringId": 1853004655650197504,
            "femaleVariant": "そうよ。そういう闇が、本当の自分を形作るのよね"
          },
          {
            "stringId": 1853006256448262144,
            "femaleVariant": "ママ・ウェルズが式を開くの。あいつが大事にしてたものを供えたいんだけど…",
            "maleVariant": "ママ・ウェルズが式を開くんだ。あいつが大事にしてたものを供えたくてな…"
          },
          {
            "stringId": 1853006374241095684,
            "femaleVariant": "オフレンダの供え物探しを手伝ってくれない？",
            "maleVariant": "オフレンダの供え物探しを手伝ってくれないか？"
          },
          {
            "stringId": 1853006552817782784,
            "femaleVariant": "どうして中に入らなかったの？",
            "maleVariant": "どうして中に入らなかったんだ？"
          },
          {
            "stringId": 1853006744145154052,
            "femaleVariant": "中に入らないの？",
            "maleVariant": "中に入らないのか？"
          },
          {
            "stringId": 1853007979552231424,
            "femaleVariant": "私が入っていいのかわからない"
          },
          {
            "stringId": 1853008200474611712,
            "femaleVariant": "鍵を持ってないの"
          },
          {
            "stringId": 1853029324616945664,
            "femaleVariant": "んー、開かない。何があるんだろ"
          },
          {
            "stringId": 1853029448617349120,
            "femaleVariant": "そこは彼の寝床よ。きっと… 別の鍵があるのね"
          },
          {
            "stringId": 1853029703916244992,
            "femaleVariant": "探してみましょ。どこかにしまってあるかも"
          },
          {
            "stringId": 1853030358999420928,
            "femaleVariant": "見つけた"
          },
          {
            "stringId": 1853043435463561216,
            "femaleVariant": "ああ、ジャッキー…"
          },
          {
            "stringId": 1853043801592745984,
            "femaleVariant": "ねえ、大丈夫？",
            "maleVariant": "おい、大丈夫か？"
          },
          {
            "stringId": 1853043872342265860,
            "femaleVariant": "大丈夫？",
            "maleVariant": "大丈夫か？"
          },
          {
            "stringId": 1853044462665388036,
            "femaleVariant": "これって… マンダラ…？"
          },
          {
            "stringId": 1853044783194099716,
            "femaleVariant": "ジャッキーに言ったことがあるの。ここには邪悪なエネルギーが溜まってるから、砂のマンダラとかで浄化した方がいい、って"
          },
          {
            "stringId": 1853046276617981952,
            "femaleVariant": "大丈夫どころか幸せな気分"
          },
          {
            "stringId": 1853046639123288064,
            "femaleVariant": "そしたら彼、自分なら代わりに砂の城を作るって。ウェルズ家の隠れ家だか要塞だかを作るんだって…"
          },
          {
            "stringId": 1853046850935640064,
            "femaleVariant": "裏ではミスティの言葉を信じてたってわけね",
            "maleVariant": "裏ではミスティの言葉を信じてたってわけか"
          },
          {
            "stringId": 1853047008691802112,
            "femaleVariant": "そうみたいね…"
          },
          {
            "stringId": 1853303390506635264,
            "femaleVariant": "彼のお気に入り"
          },
          {
            "stringId": 1853303505447342080,
            "femaleVariant": "開いてないね",
            "maleVariant": "開いてないな"
          },
          {
            "stringId": 1853303590642044928,
            "femaleVariant": "いざという時の“戦略物資”だって"
          },
          {
            "stringId": 1853303664461795328,
            "femaleVariant": "なにそれ？",
            "maleVariant": "なんだそれ？"
          },
          {
            "stringId": 1853303799216394240,
            "femaleVariant": "<kiroshi l=\"mex\" o=\"«Porque no puedes tocar una reserva estratégica, ¿va?»\" t=\"「戦略物資には手を付けちゃならねえ、いいな？」\" b=\"\" a=\"\"/>"
          },
          {
            "stringId": 1853306427182731264,
            "femaleVariant": "ジャッキーは体を鍛えるのが好きだったよね",
            "maleVariant": "ジャッキーは体を鍛えるのが好きだったよな"
          },
          {
            "stringId": 1853306580593594368,
            "femaleVariant": "瞑想みたいなもんだって言ってた"
          },
          {
            "stringId": 1853306716673593344,
            "femaleVariant": "バーベルは最高の相談相手なんだって。静かに悩みを聞いてくれて、下手なアドバイスは一切しない"
          },
          {
            "stringId": 1853307000292429824,
            "femaleVariant": "男の隠れ家には、やっぱ欠かせないよね",
            "maleVariant": "男の隠れ家には、やっぱ欠かせないよな"
          },
          {
            "stringId": 1853307327230038016,
            "femaleVariant": "2020年からずっと飾ってあるんじゃない？",
            "maleVariant": "2020年からずっと飾ってあるんじゃないか？"
          },
          {
            "stringId": 1853307672974905344,
            "femaleVariant": "エンジンいじりにハマってたね",
            "maleVariant": "エンジンいじりにハマってたな"
          },
          {
            "stringId": 1853307731242176512,
            "femaleVariant": "初めてバイクに乗ったのは15のとき"
          },
          {
            "stringId": 1853307881985462272,
            "femaleVariant": "今思うと、事故を起こさなかったのは奇跡よ",
            "maleVariant": "今思うと、事故を起こさなかったのは奇跡だな"
          },
          {
            "stringId": 1853309830139006976,
            "femaleVariant": "え？"
          },
          {
            "stringId": 1853309886661447680,
            "femaleVariant": "例の仕事の直前に、そうするよう言ったの",
            "maleVariant": "例の仕事の直前に、そうするよう言ったんだ"
          },
          {
            "stringId": 1854623201962545152,
            "femaleVariant": "『誰がために鐘は鳴る』アーネスト・ヘミングウェイ"
          },
          {
            "stringId": 1854623312692170752,
            "femaleVariant": "彼は何度もそれを読んでた。大きな仕事の前はいつもね"
          },
          {
            "stringId": 1854623381042548736,
            "femaleVariant": "作者はモーガン・ブラックハンドよりタフな奴なんだって"
          },
          {
            "stringId": 1854623528665272320,
            "femaleVariant": "ミスティは読んでみた？ 感想は？",
            "maleVariant": "ミスティは読んでみたか？ 感想は？"
          },
          {
            "stringId": 1854623759922417664,
            "femaleVariant": "いえ… タイミングがなくて"
          },
          {
            "stringId": 1854624844787535872,
            "femaleVariant": "“タコス”がいたのよ"
          },
          {
            "stringId": 1854624931659960320,
            "femaleVariant": "んー… タコスっていうのは…？"
          },
          {
            "stringId": 1854625261667799040,
            "femaleVariant": "ここで小さなサメを飼ってたの。その名前"
          },
          {
            "stringId": 1854626545175154688,
            "femaleVariant": "ダンテ・ゴンザレスのサイン入りボール"
          },
          {
            "stringId": 1854626658622689280,
            "femaleVariant": "ダンテもヘイウッド出身なのよ。ここじゃみんなバスケをやるけど、彼の才能はずば抜けてた"
          },
          {
            "stringId": 1854626816982831104,
            "femaleVariant": "ヴァレンティーノズを抜けたのは、彼が最初なんじゃないかな"
          },
          {
            "stringId": 1854626889443627008,
            "femaleVariant": "ジャッキーみたいに？"
          },
          {
            "stringId": 1854627103135027200,
            "femaleVariant": "そう。2人ともあそこの水が合わなかったのね"
          },
          {
            "stringId": 1854627206365237248,
            "femaleVariant": "ダンテは今や、NCブラックアウツのキャプテン"
          },
          {
            "stringId": 1854628072841334784,
            "femaleVariant": "ジャッキーの好きなテキーラね。まだ開けてない",
            "maleVariant": "ジャッキーの好きなテキーラだ。まだ開けてない"
          },
          {
            "stringId": 1854632228658401284,
            "femaleVariant": "ジャッキーの父親ね",
            "maleVariant": "ジャッキーの父親だな"
          },
          {
            "stringId": 1854636579426717696,
            "femaleVariant": "革のベルト？"
          },
          {
            "stringId": 1854637643722969088,
            "femaleVariant": "父親が戻って来た時のためよ"
          },
          {
            "stringId": 1854641754342215680,
            "femaleVariant": "会ったことは？"
          },
          {
            "stringId": 1854644902939119616,
            "femaleVariant": "ジャッキーらしい"
          },
          {
            "stringId": 1854644988603584512,
            "femaleVariant": "よりによってこれを選ぶなんてね"
          },
          {
            "stringId": 1854710069387382784,
            "femaleVariant": "思い出の品ね。彼の父親のものよ。ジャッキーとお母さんをそれで痛めつけてたの"
          },
          {
            "stringId": 1854710141328084992,
            "femaleVariant": "だけどある日、ジャッキーは反撃に打って出た。次やったら殺してやるぞって脅して、父親は病院送り。で、そのまま家を出てった"
          },
          {
            "stringId": 1854710614529462272,
            "femaleVariant": "取っておく意味がわからない"
          },
          {
            "stringId": 1854713409009668096,
            "femaleVariant": "ヴァレンティーノズの一員で、昔かたぎのワルだった"
          },
          {
            "stringId": 1854713664392450048,
            "femaleVariant": "ううん。話に聞いたことしかない。あまり父親とのいい思い出はなかったみたいね"
          },
          {
            "stringId": 1867799900019544064,
            "femaleVariant": "ええ。私の人生で最高の1年"
          },
          {
            "stringId": 1867800587348529152,
            "femaleVariant": "本当のジャッキーを知ってるのはあなただけね… みんなは彼のことを、頭が空っぽの乱暴者だと思ってるけど… 違う。彼はそんなんじゃない"
          },
          {
            "stringId": 1885228976896266240,
            "femaleVariant": "もう印刷されてない、コレクターアイテムとか言ってたけど"
          },
          {
            "stringId": 1885231847863799808,
            "femaleVariant": "でしょうね。そりゃ大事にしないと",
            "maleVariant": "だろうな。そりゃ大事にしないと"
          },
          {
            "stringId": 1886637819584892932,
            "femaleVariant": "これも… “戦略物資”？",
            "maleVariant": "これも… “戦略物資”か？"
          },
          {
            "stringId": 1886638287853768704,
            "femaleVariant": "それは幸運のお守りよ。ヘスース・デ・アバホが作った最初期のボトル"
          },
          {
            "stringId": 1886638294212333568,
            "femaleVariant": "センツォン・テキーラの創始者の…？"
          },
          {
            "stringId": 1886638300386349056,
            "femaleVariant": "世界に100本しか残ってないのよ。所有してるのはたった1%の人たち"
          },
          {
            "stringId": 1886638306744913920,
            "femaleVariant": "そんなものどうやって？"
          },
          {
            "stringId": 1886638313002815488,
            "femaleVariant": "メキシコであるおばあさんから買ったんだって。その人いわく、デ・アバホにレシピを盗まれたとか"
          },
          {
            "stringId": 1927117802346311680,
            "femaleVariant": "ミスティにとっては？"
          },
          {
            "stringId": 1927255087385604096,
            "femaleVariant": "前に彼が、こんなことを言ってた"
          },
          {
            "stringId": 1927255121829228544,
            "femaleVariant": "私が撃たれそうになったら、迷うことなく身代わりになるって"
          },
          {
            "stringId": 1927255316008726528,
            "femaleVariant": "ヘイウッド育ちらしいね",
            "maleVariant": "ヘイウッド育ちらしいな"
          },
          {
            "stringId": 1927255392630272000,
            "femaleVariant": "彼は… 夢と野心で溢れてた。代償は高くついたけど"
          },
          {
            "stringId": 1927275670177509376,
            "femaleVariant": "そこには彼の過去が詰まってる…"
          },
          {
            "stringId": 1927296362155536384,
            "femaleVariant": "まだ早すぎる？"
          },
          {
            "stringId": 1927296936741629952,
            "femaleVariant": "いえ、そうじゃないの。ママ・ウェルズと、あまりいい関係じゃなくて"
          },
          {
            "stringId": 1927296994388144128,
            "femaleVariant": "彼女はきっと、私が入るのを嫌がると思う。思い出を汚されたくないはずよ"
          },
          {
            "stringId": 1927297159526281216,
            "femaleVariant": "ジャッキーはそんなこと思わない。そっちの方が大事でしょ",
            "maleVariant": "ジャッキーはそんなこと思わない。そっちの方が大事だろ"
          },
          {
            "stringId": 1927297250643341312,
            "femaleVariant": "ほら"
          },
          {
            "stringId": 1927305746709409792,
            "femaleVariant": "まあね。でもそういう意味のことを、本気で言ってた",
            "maleVariant": "まあな。でもそういう意味のことを、本気で言ってた"
          },
          {
            "stringId": 1928727516846944256,
            "femaleVariant": "座って少し話さない？"
          },
          {
            "stringId": 1928736007493971968,
            "femaleVariant": "最近、よくここに来るの。1時間とか、2時間いることもある"
          },
          {
            "stringId": 1928738017622552576,
            "femaleVariant": "1年前までは"
          },
          {
            "stringId": 1928780645474955264,
            "femaleVariant": "選ぶのを手伝ってくれない？",
            "maleVariant": "選ぶのを手伝ってくれないか？"
          },
          {
            "stringId": 1928793042847723520,
            "femaleVariant": "ジャッキーから渡されてないの？ てっきり、そういう仲だと思ってたけど",
            "maleVariant": "ジャッキーから渡されてないのか？ てっきり、そういう仲だと思ってたけど"
          },
          {
            "stringId": 1928793270799757312,
            "femaleVariant": "断ったの。彼には自分だけの空間を持っててほしかったから"
          },
          {
            "stringId": 1937314604613824512,
            "femaleVariant": "ええ。もう… 長い話なんだけどね"
          },
          {
            "stringId": 1937317127722561536,
            "femaleVariant": "少しゆがんでる…"
          },
          {
            "stringId": 1937317133510701056,
            "femaleVariant": "綺麗な模様ね",
            "maleVariant": "綺麗な模様だ"
          },
          {
            "stringId": 1937317138443202560,
            "femaleVariant": "このマンダラ、なんか少し… ゆがんでない？",
            "maleVariant": "このマンダラ、なんか少し… ゆがんでないか？"
          },
          {
            "stringId": 1937317781463560192,
            "femaleVariant": "綺麗な模様ね",
            "maleVariant": "綺麗な模様だ"
          },
          {
            "stringId": 1937319543020908544,
            "femaleVariant": "酷い出来よ"
          },
          {
            "stringId": 1937335792912232448,
            "femaleVariant": "でもこんな美しいもの、見たことない"
          },
          {
            "stringId": 1937340415505002496,
            "femaleVariant": "オフレンダの供え物には良さそうね",
            "maleVariant": "オフレンダの供え物には良さそうだな"
          },
          {
            "stringId": 1937410900213297152,
            "femaleVariant": "ずいぶん値が張りそうなペットね",
            "maleVariant": "ずいぶん値が張りそうなペットだな"
          },
          {
            "stringId": 1937411098704539648,
            "femaleVariant": "買ったわけじゃないと思うけど"
          },
          {
            "stringId": 1942845947135303684,
            "femaleVariant": "[取る] 純粋な奴だった"
          },
          {
            "stringId": 1942850655694528512,
            "femaleVariant": "[取る] ジャッキーも喜ぶと思う"
          },
          {
            "stringId": 1942851325709426688,
            "femaleVariant": "[取る] これはいいかもね",
            "maleVariant": "[取る] これはいいかもな"
          },
          {
            "stringId": 1942855605912772608,
            "femaleVariant": "[取る] これもあったほうがいい"
          },
          {
            "stringId": 1943128215422672896,
            "femaleVariant": "サイン？"
          },
          {
            "stringId": 1943129611270266880,
            "femaleVariant": "本？"
          },
          {
            "stringId": 1943140999409606656,
            "femaleVariant": "[座る] 別におかしくなんてない"
          },
          {
            "stringId": 1943141290460749824,
            "femaleVariant": "オフレンダに持っていくものを探してる"
          },
          {
            "stringId": 1943141844008214528,
            "femaleVariant": "バカなんて言わないで、ミスティ… 全然… 普通のことよ",
            "maleVariant": "バカなんて言うな、ミスティ… 全然… 普通のことだ"
          },
