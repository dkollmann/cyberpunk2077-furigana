    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アダム・スマッシャーを見た",
                    "maleVariant": null,
                    "stringId": 1938704668044369924
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何か仕事ない？",
                    "maleVariant": "何か仕事はないか？",
                    "stringId": 1938705798711287808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの男、アダム・スマッシャーを見た",
                    "maleVariant": null,
                    "stringId": 1938706077028524032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "見たってどこで？",
                    "maleVariant": null,
                    "stringId": 1938706955114455040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この街でよ。ヨリノブが檻から出したの。ハナコが… さらわれた時に",
                    "maleVariant": "この街でだ。ヨリノブが檻から出した。ハナコが… さらわれた時にな",
                    "stringId": 1938707159695826944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "“襲撃者”は、どういうつもりだったんだろうねぇ？",
                    "maleVariant": null,
                    "stringId": 1938709664584507392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "推測だけど、ハナコを攫うつもりなんてなかったんだと思う… それが、手のつけられない状況に発展した",
                    "maleVariant": "推測だが、ハナコを攫うつもりなんてなかったんじゃないかな… それが、手のつけられない状況に発展した",
                    "stringId": 1938723066493083648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私には関係ないが、面と向かって話す機会があれば、連中に言ってやりたいことがある",
                    "maleVariant": null,
                    "stringId": 1938730604009693184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "遅かれ早かれ、連中は報いを受ける。ナイトシティは昔とは違うんだ。アラサカを含めてね",
                    "maleVariant": null,
                    "stringId": 1938731897533046784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "“謎の襲撃者”の1人も、同じようにかつての自分じゃない。連中に伝わると思うかい？",
                    "maleVariant": null,
                    "stringId": 1938735865025863680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おそらく",
                    "maleVariant": null,
                    "stringId": 1938737946642161668
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ローグも変わった",
                    "maleVariant": null,
                    "stringId": 1938740065654874112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ローグ、あんたも随分変わった",
                    "maleVariant": null,
                    "stringId": 1938741156224245760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そろそろ行くよ。やることが山積みだからね",
                    "maleVariant": null,
                    "stringId": 1938741223551213568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "当たり前じゃないかい？",
                    "maleVariant": null,
                    "stringId": 1938744376979955712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "せいぜい気をつけな",
                    "maleVariant": null,
                    "stringId": 1938747296282648576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "たぶんね。でもきっと理由があってのことよ",
                    "maleVariant": "たぶんな。でもきっと理由があってのことだ",
                    "stringId": 1938752429942972416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたはフィクサーでしょ。あたしにできる仕事はない？",
                    "maleVariant": "あんたはフィクサーだろ。俺にできる仕事はないか？",
                    "stringId": 1942841517109223424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ない",
                    "maleVariant": null,
                    "stringId": 1942841937495924736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ないって… 一つも？",
                    "maleVariant": null,
                    "stringId": 1942841977778020352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ないね",
                    "maleVariant": null,
                    "stringId": 1942842155733950464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "下手にごまかされるよりいい",
                    "maleVariant": null,
                    "stringId": 1942843957506285572
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ホントにいいのね？",
                    "maleVariant": "ホントにいいんだな？",
                    "stringId": 1942844306052947968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "率直な答えをもらえて助かる",
                    "maleVariant": null,
                    "stringId": 1942844923538382848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほかのフィクサーの仕事を受けて、ホントにいいのね？",
                    "maleVariant": "ほかのフィクサーの仕事を受けて、ホントにいいんだな？",
                    "stringId": 1942844991117008896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ご自由に。他じゃ、ちんけな仕事しか請け負えないがね",
                    "maleVariant": null,
                    "stringId": 1942876380566724608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それだけならタダでいいさ",
                    "maleVariant": null,
                    "stringId": 1942877150020182016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "常連を見てみな。みんな山ほど仕事を抱えて、互いに信頼してる",
                    "maleVariant": null,
                    "stringId": 1942877781464260608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私の身になって考えてみるんだね。あんたをクルーに入れたとわかったら、周りからはどう見えると思う？",
                    "maleVariant": null,
                    "stringId": 1942905108948656128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたはデクスの仕事を受け、これ以上ないくらい、見事に失敗した",
                    "maleVariant": null,
                    "stringId": 1942905694406385664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それは事実じゃない",
                    "maleVariant": null,
                    "stringId": 1942915239165894656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "事実かどうかは関係ない。これは実力の話じゃない、評判だよ",
                    "maleVariant": null,
                    "stringId": 1942915317549047808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたは一匹狼だ",
                    "maleVariant": null,
                    "stringId": 1942916762000236544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "面倒なことになったね。アラサカがまた“謎の襲撃者”探しを始めたってわけか",
                    "maleVariant": null,
                    "stringId": 1944522772091576320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "きっと… 最初は善意だった",
                    "maleVariant": null,
                    "stringId": 1944548118471974916
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "想定外だった… んだと思う",
                    "maleVariant": null,
                    "stringId": 1944548605950763008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "打つ手がなくなって… 出たとこ勝負をしかけたんじゃない？",
                    "maleVariant": "打つ手がなくなって… 出たとこ勝負をしかけたんじゃないか？",
                    "stringId": 1944552103161438208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヘルマンの作戦はなんとかうまくいった",
                    "maleVariant": null,
                    "stringId": 1954860296781504512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヘルマンの件… なんとか上手くいったよ",
                    "maleVariant": null,
                    "stringId": 1954861621191712768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ",
                    "maleVariant": null,
                    "stringId": 1954864297660981248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なら… 自分が得意なことをやればいい。わかったね？",
                    "maleVariant": null,
                    "stringId": 1954866600736530432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "AV一機落とすのにあそこまでやる――",
                    "maleVariant": null,
                    "stringId": 1954901063889657856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あれこそ昔ながらのやり方だよ",
                    "maleVariant": null,
                    "stringId": 1954902603400552448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "パナムがよくやってくれた",
                    "maleVariant": null,
                    "stringId": 1954906686823931908
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ今のやり方ってのは？",
                    "maleVariant": null,
                    "stringId": 1954907040202432512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ今のやり方は？",
                    "maleVariant": null,
                    "stringId": 1954907307681587200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "保守的だね。常にすべてを計算し尽くした上で行動に移す",
                    "maleVariant": null,
                    "stringId": 1954907615811936256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "昔のやり方を懐かしがってるようね",
                    "maleVariant": "昔のやり方を懐かしがってるようだな",
                    "stringId": 1954910804992925696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "昔の私に“お前はフィクサーになる”なんて言っても、到底信じないだろうね",
                    "maleVariant": null,
                    "stringId": 1954910985935200256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "個人的なことを聞いてもいい？",
                    "maleVariant": "個人的なことを聞いてもいいか？",
                    "stringId": 1969015903549726720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいけど、自己責任だよ",
                    "maleVariant": null,
                    "stringId": 1969016628677779456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたの知るジョニー・シルヴァーハンドはどんな人物だった？",
                    "maleVariant": null,
                    "stringId": 1969018020935049220
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アフターライフのボスを務めるってどんな感じ？",
                    "maleVariant": "アフターライフのボスを務めるってどんな感じだ？",
                    "stringId": 1969018532807909376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やっぱり何でもない",
                    "maleVariant": null,
                    "stringId": 1969018757387722752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ローグ… ジョニー・シルヴァーハンドを知ってるでしょ？",
                    "maleVariant": "ローグ…ジョニー・シルヴァーハンドを知ってるよな？",
                    "stringId": 1969019508067475456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "えっと… 今言ったことは忘れて",
                    "maleVariant": "えっと… 今言ったことは忘れてくれ",
                    "stringId": 1969019639533740032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "強くて尊大で、妥協を知らない男だ",
                    "maleVariant": null,
                    "stringId": 1969020061078069248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "個人的なことを聞いてもいい？",
                    "maleVariant": "個人的なことを聞いてもいいか？",
                    "stringId": 1969020275927097344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それが一番だ",
                    "maleVariant": null,
                    "stringId": 1969021270329466880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "え…？ ジョニー…？",
                    "maleVariant": null,
                    "stringId": 1969025605897625600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしてその名を…？",
                    "maleVariant": null,
                    "stringId": 1969026351124783104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "信念のためなら街を灰にしてもいい――",
                    "maleVariant": null,
                    "stringId": 1969027721739444224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そう考え、それを楽しんでた",
                    "maleVariant": null,
                    "stringId": 1969030534439706624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マッチ箱とCHOOH2の缶を手にした子供だよ",
                    "maleVariant": null,
                    "stringId": 1969031281629798400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もてたでしょうね",
                    "maleVariant": "もてただろうな",
                    "stringId": 1969035455885025280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だろうね",
                    "maleVariant": null,
                    "stringId": 1969037386875478016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "壮絶ね",
                    "maleVariant": "壮絶だな",
                    "stringId": 1969037735925456896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ともかく、死者の悪口を言うつもりはないよ",
                    "maleVariant": null,
                    "stringId": 1969039152039927808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アフターライフのボスをやるってどんな感じ？",
                    "maleVariant": "アフターライフのボスをやるってどんな感じだ？",
                    "stringId": 1969041043939467264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんだい、N54の取材か何かかい？",
                    "maleVariant": null,
                    "stringId": 1969041685936414720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "50年経った今でも、彼の噂が尽きることはない。でも、どれもいまいち要領を得ないの",
                    "maleVariant": "50年経った今でも、彼の噂が尽きることはない。だが、どれもいまいち要領を得ないんだ",
                    "stringId": 1969045455827181568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "2人の仲は知ってる。だから聞いたの",
                    "maleVariant": "2人の仲は知ってる。だから聞いたんだ",
                    "stringId": 1969046563274432512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふん… ジョニー・シルヴァーハンドね…",
                    "maleVariant": null,
                    "stringId": 1969048186168090624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "とんでもない男だった",
                    "maleVariant": null,
                    "stringId": 1969050707750100992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一番の功労者はパナムよ。彼女がいたからこそうまくいった",
                    "maleVariant": "一番の功労者はパナムだ。彼女がいたからこそうまくいった",
                    "stringId": 1969128470179815424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だからわざわざあの娘を薦めたんだ",
                    "maleVariant": null,
                    "stringId": 1969129762293882880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もういいかい？",
                    "maleVariant": null,
                    "stringId": 1969130118356738048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "冗談はおいといて、“ボス”なんて大層なもんじゃない。取り巻きだっていないんだ",
                    "maleVariant": null,
                    "stringId": 1975277588027613184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあなんて呼ばれたいの？",
                    "maleVariant": "じゃあなんて呼ばれたいんだ？",
                    "stringId": 1975279636341137408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "難しいね… すぐには思いつかない",
                    "maleVariant": null,
                    "stringId": 1975279971516358656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "少し時間をくれないかい？ よっぽど他にやることがなかったら、考えてみるよ",
                    "maleVariant": null,
                    "stringId": 1975281297671397376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その価値はあったのかねぇ？",
                    "maleVariant": null,
                    "stringId": 1975286537363726336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でないと… 浮かばれないでしょ",
                    "maleVariant": "でないと…浮かばれないだろ",
                    "stringId": 1976231240804409344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあね",
                    "maleVariant": "じゃあな",
                    "stringId": 2515728563862134784
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}