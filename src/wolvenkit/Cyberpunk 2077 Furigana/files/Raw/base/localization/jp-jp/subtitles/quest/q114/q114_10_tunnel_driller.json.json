  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミッチの言うとおりよ。こっちは三人で十分。キャロルたちを助けに行って",
                    "maleVariant": "ミッチの言うとおりだ。こっちは三人で十分。キャロルたちを助けに行くべきだ",
                    "stringId": 1792612754473635840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここまでね。これ以上は無理よ。作戦を中止して",
                    "maleVariant": "ここまでだな。これ以上は無理だ。作戦を中止しろ",
                    "stringId": 1792614196525682688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本当にいいんだな？",
                    "maleVariant": null,
                    "stringId": 1792618868628017152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今さら退くわけにはいかない",
                    "maleVariant": null,
                    "stringId": 1792627232338628608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "心配すんな",
                    "maleVariant": null,
                    "stringId": 1801341012647137280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "退く必要も、中止する必要もない。俺がミリテクを抑える",
                    "maleVariant": null,
                    "stringId": 1801344021523386368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "きばれよ、パナム",
                    "maleVariant": null,
                    "stringId": 1842869169040445440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バカじゃないの！？ 行かせないよ！",
                    "maleVariant": null,
                    "stringId": 1887950339293442048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "他に方法があんのか？",
                    "maleVariant": null,
                    "stringId": 1887951084872921088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ひとりでパンツァーなんて操縦したら、脳みそが丸焦げになる！",
                    "maleVariant": null,
                    "stringId": 1887952132173852672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ソウル！ こんなの無謀にも程がある！",
                    "maleVariant": null,
                    "stringId": 1887954557655969792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "殺されちゃうよ！",
                    "maleVariant": null,
                    "stringId": 1887955815846506496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ダメだよ、ミッチ！ 殺されちゃうよ… ダメ…",
                    "maleVariant": null,
                    "stringId": 1887963481037729792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大丈夫だ、パナム。今日は死ぬ気がしない",
                    "maleVariant": null,
                    "stringId": 1887964235626573824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミッチの言うとおり、助けなきゃ",
                    "maleVariant": "ミッチの言うとおりだ、助けないと",
                    "stringId": 1937480711788802052
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "作戦を中止すべきよ",
                    "maleVariant": "作戦を中止すべきだ",
                    "stringId": 1937481284378406912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "キャロル、今そっちに向かう。待ってろ！",
                    "maleVariant": null,
                    "stringId": 1937481753234485248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミッチ？",
                    "maleVariant": null,
                    "stringId": 1937483160641581056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、ジャックインして",
                    "maleVariant": null,
                    "stringId": 1937491747069734912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ドリルを起動して",
                    "maleVariant": null,
                    "stringId": 1938632891560341504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "イチかバチか… 行くよ",
                    "maleVariant": "イチかバチか… 行くぞ",
                    "stringId": 1938634989047885824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何してるの、V！ 急いで！",
                    "maleVariant": null,
                    "stringId": 1938639574328127488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "みんなの犠牲を無駄にはできない",
                    "maleVariant": null,
                    "stringId": 1938861075019051008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここだね",
                    "maleVariant": null,
                    "stringId": 1945864568796999684
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカ・タワーはまだ遠いの？",
                    "maleVariant": "アラサカ・タワーはまだ遠いのか？",
                    "stringId": 1945864568796999700
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう着いたも同然みたいなもんだよ",
                    "maleVariant": null,
                    "stringId": 1945864568796999716
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こんなの慣れっこだろ、ソウル",
                    "maleVariant": null,
                    "stringId": 1945897542991982592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと、どうしたの？",
                    "maleVariant": null,
                    "stringId": 1945898907415851008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "キャロルたちだ。ミリテクに囲まれ、孤立状態。このままじゃ持たない",
                    "maleVariant": null,
                    "stringId": 1945899468831830016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いま困ってんのはあいつらの方だ",
                    "maleVariant": null,
                    "stringId": 1945901969308102656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと、あれ…！ 何かあったみたいだ",
                    "maleVariant": null,
                    "stringId": 1945927838262226944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "計画通りに行く方が珍しいんだ",
                    "maleVariant": null,
                    "stringId": 1945934305979658240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "中で何があるかわからない。お前が来てくれなきゃ困る",
                    "maleVariant": null,
                    "stringId": 1945936706866376704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "心配するな。無事を確認したら追いかける",
                    "maleVariant": null,
                    "stringId": 1945938975431192576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今は時間が惜しい",
                    "maleVariant": null,
                    "stringId": 1945939588822986752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だから、俺がパンツァーで助けに行く",
                    "maleVariant": null,
                    "stringId": 1945946818964221952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ウソでしょ…",
                    "maleVariant": null,
                    "stringId": 1945947156370812928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ファミリーなんだぞ、パナム！ 見捨ててたまるか！",
                    "maleVariant": null,
                    "stringId": 1945951112153133056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ソウル、行かせる気！？ V！？ 誰か何とか言ってやってよ！",
                    "maleVariant": null,
                    "stringId": 1945953792229502976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "帰ったら一杯付き合えよな！",
                    "maleVariant": null,
                    "stringId": 1945960664579829760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ソウル… このままじゃ… みんなが…",
                    "maleVariant": null,
                    "stringId": 1945961752531955712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前らはドリルの問題を突き止めろ。動き出したら、俺が後ろを守る",
                    "maleVariant": null,
                    "stringId": 1945974160474370048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V… 行こう",
                    "maleVariant": null,
                    "stringId": 1945975000979337216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おう！ 今んとこ無事だぜ！",
                    "maleVariant": null,
                    "stringId": 1945980202704826368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お願い、無理はしないで",
                    "maleVariant": null,
                    "stringId": 1945981362094338048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大丈夫だ、俺を信じろ",
                    "maleVariant": null,
                    "stringId": 1945981656316375040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前も気をつけろよ。無事〈神輿〉にたどり着いてくれ",
                    "maleVariant": null,
                    "stringId": 1945982813289967616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃ、いっちょやるか！ 通信終了",
                    "maleVariant": null,
                    "stringId": 1945983384503840768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お願いだから… 死なないで",
                    "maleVariant": null,
                    "stringId": 1945992531458781184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ドリルがコンクリに達したとき、エンジンがオーバーヒートしたんだ",
                    "maleVariant": null,
                    "stringId": 1945997368967688192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "手動モードに切り替わった。よし",
                    "maleVariant": null,
                    "stringId": 1945999722039341056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "動いた！",
                    "maleVariant": null,
                    "stringId": 1946000217336311808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クソ！",
                    "maleVariant": null,
                    "stringId": 1946000851615739904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "パナム？ パナム、どうなってるの？",
                    "maleVariant": "パナム？ パナム、どうなってるんだ？",
                    "stringId": 1946001255913091072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "パナム！？ 返事して！",
                    "maleVariant": "パナム！？ 返事しろ！",
                    "stringId": 1946001591692292096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あとは――",
                    "maleVariant": null,
                    "stringId": 1946001949533532160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大丈夫、もっと出力を上げて",
                    "maleVariant": null,
                    "stringId": 1946006541390442496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "パナム… 死んだのかと思った",
                    "maleVariant": "パナム… 死んだのかと思ったぞ",
                    "stringId": 1946015392294629376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "平気だよ、V。何も問題ない",
                    "maleVariant": null,
                    "stringId": 1946016108111327232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アザができたけど、どうってことない",
                    "maleVariant": null,
                    "stringId": 1946017931140395008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふっ、こんな機械のモグラの中で死ぬわけにはいかないからね",
                    "maleVariant": null,
                    "stringId": 1946021619242233856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ソウル？",
                    "maleVariant": null,
                    "stringId": 1946028245789569024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "トンネルはひとまず安全だ",
                    "maleVariant": null,
                    "stringId": 1946028491408011264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミッチから連絡は？",
                    "maleVariant": null,
                    "stringId": 1946029159527084032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まだ何も…",
                    "maleVariant": null,
                    "stringId": 1946030348780040192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やっぱり…",
                    "maleVariant": null,
                    "stringId": 1946030493684854784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "通信トラブルかもしれない",
                    "maleVariant": null,
                    "stringId": 1946030893670461440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うん、かもね",
                    "maleVariant": null,
                    "stringId": 1946033564116709376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "行くぞ。仕事が残ってる",
                    "maleVariant": null,
                    "stringId": 1946033784217006080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バイパスすれば緊急停止をオーバーライドできるはず。必要なら私のパーソナルリンクを使う",
                    "maleVariant": null,
                    "stringId": 1954521720734478336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここだよ",
                    "maleVariant": null,
                    "stringId": 1995519825484206088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと… ウソでしょ！",
                    "maleVariant": "おい… ウソだろ！",
                    "stringId": 1995524882237771776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[立ち上がる] もうダメかと思った",
                    "maleVariant": "[立ち上がる] もうダメかと思ったぞ",
                    "stringId": 1998083378554273792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[降りる] アラサカ・タワーはまだ遠いの？",
                    "maleVariant": "[降りる] アラサカ・タワーはまだ遠いのか？",
                    "stringId": 2208706427836891140
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、何も言ってくれないわけ？",
                    "maleVariant": null,
                    "stringId": 2238911563337756672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "急ごう。時間が惜しい",
                    "maleVariant": null,
                    "stringId": 2238914292890136576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[ドリルを起動] イチかバチか…",
                    "maleVariant": null,
                    "stringId": 2273700868028563460
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}