    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1324354672789569536,
            "femaleVariant": "さて、テディーのとっておきを試すぞ"
          },
          {
            "stringId": 1335577276925886464,
            "femaleVariant": "全箇所異常なし、オールグリーンだ"
          },
          {
            "stringId": 1335577818242760704,
            "femaleVariant": "行き先は？"
          },
          {
            "stringId": 1335578123017666560,
            "femaleVariant": "よし、起動してみろ"
          },
          {
            "stringId": 1547329556426276864,
            "femaleVariant": "かっ飛ばせ！"
          },
          {
            "stringId": 1690846914498285568,
            "femaleVariant": "よし、問題なさそうだな"
          },
          {
            "stringId": 1690848106435596288,
            "femaleVariant": "あとはニトロのテストだな"
          },
          {
            "stringId": 1690850135572762624,
            "femaleVariant": "ええ、もちろん",
            "maleVariant": "ああ、もちろんだ"
          },
          {
            "stringId": 1690850509520130048,
            "femaleVariant": "もちろん"
          },
          {
            "stringId": 1690854755497955328,
            "femaleVariant": "あそこに開けたエリアがある。向かってくれ"
          },
          {
            "stringId": 1690858752954880000,
            "femaleVariant": "戻りましょ",
            "maleVariant": "戻るとするか"
          },
          {
            "stringId": 1690859782773952512,
            "femaleVariant": "明日までに内部を点検しとかないとな。どうも嫌な感じの音がしてた"
          },
          {
            "stringId": 1738903436180344832,
            "femaleVariant": "よし、こんなところだろう"
          },
          {
            "stringId": 1738939196547813376,
            "femaleVariant": "アフターバーナー、準備よし"
          },
          {
            "stringId": 1738939196564590596,
            "femaleVariant": "傷をつけても怒らないよね？",
            "maleVariant": "傷をつけても怒らないよな？"
          },
          {
            "stringId": 1738942517329285120,
            "femaleVariant": "おいおい…"
          },
          {
            "stringId": 1751495477150109696,
            "femaleVariant": "キャンプの東にスクラップ場がある"
          },
          {
            "stringId": 1776386503839895552,
            "femaleVariant": "よう。調子はどうだ？"
          },
          {
            "stringId": 1776514816105304064,
            "femaleVariant": "射撃練習にもってこいだろ"
          },
          {
            "stringId": 1776541682535026692,
            "femaleVariant": "左にずれてる感じがするな"
          },
          {
            "stringId": 1776543120879308800,
            "femaleVariant": "直せると思う？",
            "maleVariant": "直せると思うか？"
          },
          {
            "stringId": 1776543153611657216,
            "femaleVariant": "とりあえず撃ち続けてくれ。やりながら照準を調整してみる"
          },
          {
            "stringId": 1776554400251957248,
            "femaleVariant": "傷だ？ こいつはパンツァーだぞ。傷なんかつくわけがない"
          },
          {
            "stringId": 1776557161479720960,
            "femaleVariant": "なに？ 大丈夫なの？",
            "maleVariant": "なんだ？ 大丈夫なのか？"
          },
          {
            "stringId": 1803843100003291136,
            "femaleVariant": "マシになっただろ？"
          },
          {
            "stringId": 1803844241172746244,
            "femaleVariant": "よし、申し分ない"
          },
          {
            "stringId": 1803925089904545792,
            "femaleVariant": "そうみたいね",
            "maleVariant": "そうみたいだ"
          },
          {
            "stringId": 1803925211656802304,
            "femaleVariant": "念のためにもう一台いっとけ"
          },
          {
            "stringId": 1811326498815922176,
            "femaleVariant": "ミッチ。今回の相手は、そこらのラフェンやコンボイとはわけが違う。みんなが生還するためには、二人が協力し合ってくれないと"
          },
          {
            "stringId": 1814330408136658948,
            "femaleVariant": "作戦について心配は？"
          },
          {
            "stringId": 1815232524812341248,
            "femaleVariant": "作戦に不安は？"
          },
          {
            "stringId": 1815332981933133828,
            "femaleVariant": "じゃあ遠慮なく"
          },
          {
            "stringId": 1815347011578023936,
            "femaleVariant": "ここまで来たら、不安もクソもない"
          },
          {
            "stringId": 1815347082579202048,
            "femaleVariant": "飲まないのか？"
          },
          {
            "stringId": 1831464639870586880,
            "femaleVariant": "あいつらもあいつらなりに、互いを理解しようとはしてる"
          },
          {
            "stringId": 1834190807346499584,
            "femaleVariant": "パナムが成長すればと思ってた"
          },
          {
            "stringId": 1834190842746425348,
            "femaleVariant": "正直… あの一件の後、ソウルは退くと思ってた"
          },
          {
            "stringId": 1834200434566025216,
            "femaleVariant": "キャンプの政治をとやかく言うつもりはないが、見たところ、ソウルにはあまり選択肢がない"
          },
          {
            "stringId": 1834200497379921920,
            "femaleVariant": "パナムに引き継がせればいい"
          },
          {
            "stringId": 1834200549741613056,
            "femaleVariant": "それも手だが、ソウルもバカじゃない。パナムにその気がないことをわかってるんだよ"
          },
          {
            "stringId": 1834202663637274624,
            "femaleVariant": "成長して何になる？"
          },
          {
            "stringId": 1834202731819880448,
            "femaleVariant": "わかるでしょ。クランのリーダーを引き継ぐのよ。資質は示したはず",
            "maleVariant": "わかるだろ。クランのリーダーを引き継ぐんだ。資質は示したはず"
          },
          {
            "stringId": 1834202781111341056,
            "femaleVariant": "あいつが示したのは、クランを想う気持ちだ。上に立ちたいかどうかは別の話さ"
          },
          {
            "stringId": 1835720063184588800,
            "femaleVariant": "まあな。どうりで、誰もこいつにニトロを載せないわけだ"
          },
          {
            "stringId": 1835779613627568132,
            "femaleVariant": "[ビールを取る] いただく"
          },
          {
            "stringId": 1835780932249636864,
            "femaleVariant": "遠慮しとく"
          },
          {
            "stringId": 1835783304799637504,
            "femaleVariant": "遠慮しとく"
          },
          {
            "stringId": 1835785362474848256,
            "femaleVariant": "戦争の時は、初陣の前にストレスを抑え込むよう訓練された。どうやるか知りたいか？"
          },
          {
            "stringId": 1835786028832313344,
            "femaleVariant": "ケムカクテルだ。サプリやらブースターやらを混ぜ合わせて投与するのさ"
          },
          {
            "stringId": 1835792217729191936,
            "femaleVariant": "[en_us][pl_pl][MITCH wyrzuca / odkłada pustą butelkę.]"
          },
          {
            "stringId": 1835793337574805504,
            "femaleVariant": "さて、噂話はここまでだ"
          },
          {
            "stringId": 1835793481036779520,
            "femaleVariant": "ここで話したことは他言無用だぞ。俺はもう少しこいつの調整をする"
          },
          {
            "stringId": 1835794160480473088,
            "femaleVariant": "[en_us][pl_pl][MITCH moves to the entrance of the panzer.]"
          },
          {
            "stringId": 1835794218546417664,
            "femaleVariant": "わかった。それじゃ、頑張ってね",
            "maleVariant": "わかった。それじゃ、頑張れよ"
          },
          {
            "stringId": 1877779176789504000,
            "femaleVariant": "クランの奴らに何を期待されてるかも、あの二人は十分承知してるはずだ"
          },
          {
            "stringId": 1877781298536902656,
            "femaleVariant": "せめてパナムをそれなりの地位に就かせるとか"
          },
          {
            "stringId": 1877783072475836416,
            "femaleVariant": "だがその地位を、パナムが望んでるとは限らないだろ？"
          },
          {
            "stringId": 1877788839509618688,
            "femaleVariant": "怖気づいたのね",
            "maleVariant": "怖気づいたのか"
          },
          {
            "stringId": 1877788903481143296,
            "femaleVariant": "なるほど。クランは大事だけど、権力は求めてない。あとはそこにどう折り合いをつけるかね…",
            "maleVariant": "なるほど。クランは大事だが、権力は求めてない。あとはそこにどう折り合いをつけるかだ…"
          },
          {
            "stringId": 1877789253101547520,
            "femaleVariant": "お前何にもわかってないな"
          },
          {
            "stringId": 1877790384338890752,
            "femaleVariant": "え？",
            "maleVariant": "ん？"
          },
          {
            "stringId": 1877791400148987904,
            "femaleVariant": "パナムはお前一人のために、自分や俺たちの命を危険にさらすことを決めた。「怖気づいた」なんて、よく言えるぜ"
          },
          {
            "stringId": 1877795274679250944,
            "femaleVariant": "違う… そういうつもりで言ったわけじゃ…"
          },
          {
            "stringId": 1877798144556265472,
            "femaleVariant": "俺に聞かないでくれよ"
          },
          {
            "stringId": 1877799030040948736,
            "femaleVariant": "いいんだ、V。気持ちはわかる"
          },
          {
            "stringId": 1883419643472375808,
            "femaleVariant": "乗りな"
          },
          {
            "stringId": 1883451757798486016,
            "femaleVariant": "ええ",
            "maleVariant": "ああ"
          },
          {
            "stringId": 1883453696724561920,
            "femaleVariant": "それを何度も繰り返してると、脳が変異すんだ。神経回路が繋ぎ直され、恐怖を感じる部分がさっぱり消えちまう"
          },
          {
            "stringId": 1883457120501362692,
            "femaleVariant": "なら今も恐怖を感じないの？",
            "maleVariant": "なら今も恐怖を感じないのか？"
          },
          {
            "stringId": 1883457321089757184,
            "femaleVariant": "ひどい話ね",
            "maleVariant": "ひどい話だ"
          },
          {
            "stringId": 1883457679618863104,
            "femaleVariant": "ケムカクテルは、サイバーサイコシスを引き起こすんじゃ？"
          },
          {
            "stringId": 1883458522640412672,
            "femaleVariant": "戦時中もケムカクテルは禁止されてたでしょ",
            "maleVariant": "戦時中もケムカクテルは禁止されてただろ"
          },
          {
            "stringId": 1883465693641961472,
            "femaleVariant": "まさか"
          },
          {
            "stringId": 1883466036048162816,
            "femaleVariant": "部隊の中からサイバーサイコは出なかったの？",
            "maleVariant": "部隊の中からサイバーサイコは出なかったのか？"
          },
          {
            "stringId": 1883466713176600576,
            "femaleVariant": "ミリテクがそんなことに構うと思うか？ 発症者が出たら、台湾のクリニックに送るだけだ。毎日輸送便が出てた"
          },
          {
            "stringId": 1883469400081297408,
            "femaleVariant": "そしてその都度、新兵が補充され、そいつらもすぐクスリ漬けにされるのさ。だがまあ、金は良かった"
          },
          {
            "stringId": 1883485622726422528,
            "femaleVariant": "あんたは、今も恐怖を感じないの？",
            "maleVariant": "あんたは今も、恐怖を感じないのか？"
          },
          {
            "stringId": 1883487095279448064,
            "femaleVariant": "ならよかったんだがな"
          },
          {
            "stringId": 1883487502345039872,
            "femaleVariant": "明日のことに不安はない。だが今キャンプで起きてることは別の…"
          },
          {
            "stringId": 1883488545065791488,
            "femaleVariant": "いや、何でもない。忘れてくれ"
          },
          {
            "stringId": 1883489114635497472,
            "femaleVariant": "ひどい話ね",
            "maleVariant": "ひどい話だ"
          },
          {
            "stringId": 1883489568375304192,
            "femaleVariant": "戦争ってのはひどいもんだろ"
          },
          {
            "stringId": 1883490206312165376,
            "femaleVariant": "明日やろうとしてる戦争も？",
            "maleVariant": "明日やろうとしてる戦争もか？"
          },
          {
            "stringId": 1883490475838140416,
            "femaleVariant": "なんだよ、今更迷ってるのか？"
          },
          {
            "stringId": 1883490747763257344,
            "femaleVariant": "明日のことについて、心配する必要はない。むしろ俺が気がかりなのは…"
          },
          {
            "stringId": 1883491243295109120,
            "femaleVariant": "なによ？",
            "maleVariant": "なんだ？"
          },
          {
            "stringId": 1883491308122271744,
            "femaleVariant": "いや、気にするな"
          },
          {
            "stringId": 1883491848096968708,
            "femaleVariant": "話してよ",
            "maleVariant": "話せよ"
          },
          {
            "stringId": 1883492800136867840,
            "femaleVariant": "そう。じゃあまた明日",
            "maleVariant": "そうか。じゃあまた明日"
          },
          {
            "stringId": 1883495107624824832,
            "femaleVariant": "話してよ。あたしだってファミリーの一員でしょ？",
            "maleVariant": "話せよ。俺だってファミリーの一員だろ？"
          },
          {
            "stringId": 1883497032340598784,
            "femaleVariant": "俺はパナムとソウルの間のことが、気がかりでしょうがないんだ"
          },
          {
            "stringId": 1883498079121436672,
            "femaleVariant": "あの二人は平静を装ってるつもりのようだが、俺たちもバカじゃない"
          },
          {
            "stringId": 1883503038869139456,
            "femaleVariant": "クラン中のみんなが、ソウルは自分のことしか考えてなくてダメだとか、リーダーに相応しくない、なんて言い始めてる"
          },
          {
            "stringId": 1883512238168993796,
            "femaleVariant": "実際ソウルはどうなの？",
            "maleVariant": "実際ソウルはどうなんだ？"
          },
          {
            "stringId": 1883512343999672320,
            "femaleVariant": "あんたの意見は？"
          },
          {
            "stringId": 1883512422265384960,
            "femaleVariant": "明日の作戦に影響しないといいけど",
            "maleVariant": "明日の作戦に影響しないといいが"
          },
          {
            "stringId": 1883513670624473088,
            "femaleVariant": "実際ソウルはどうなの？",
            "maleVariant": "実際ソウルはどうなんだ？"
          },
          {
            "stringId": 1883514083612422144,
            "femaleVariant": "みんなの言う通りだが、どうすることもできん"
          },
          {
            "stringId": 1883515049141202944,
            "femaleVariant": "あんたはどうなの？",
            "maleVariant": "あんたはどうなんだ？"
          },
          {
            "stringId": 1883515261792415744,
            "femaleVariant": "なんのことだ"
          },
          {
            "stringId": 1883515408878268416,
            "femaleVariant": "あんたの意見よ。ソウルよりパナムが適任だと思ってるの？",
            "maleVariant": "あんたの意見だ。ソウルよりパナムが適任だと思ってるのか？"
          },
          {
            "stringId": 1883515833694154752,
            "femaleVariant": "明日の作戦が終わったら、自ずと答えが出るだろう"
          },
          {
            "stringId": 1883516491780452352,
            "femaleVariant": "お願いしてる側のあたしが言うセリフじゃないかもしれないけど… そんなごたついてて、明日は大丈夫なの？",
            "maleVariant": "お願いしてる側の俺が言うセリフじゃないかもしれないが… そんなごたついてて、明日は大丈夫なのか？"
          },
          {
            "stringId": 1883517014407507968,
            "femaleVariant": "いがみ合いながらも、あの二人はお前をクランに迎え入れた。だからこそ、俺たちは誰ひとり、お前を見捨てたりしない"
          },
          {
            "stringId": 1883518234429878272,
            "femaleVariant": "とはいえ――パナムはお前を助けたい一心で動いてるが、ソウルの狙いは他にある"
          },
          {
            "stringId": 1883529487126753280,
            "femaleVariant": "目当ては〈神輿〉の機材？",
            "maleVariant": "目当ては〈神輿〉の機材か？"
          },
          {
            "stringId": 1883529937142018048,
            "femaleVariant": "ああ。アラサカの最先端テックが手に入れば、南で上手くやっていける… 言いたいことはわかるか？"
          },
          {
            "stringId": 1883535775227310084,
            "femaleVariant": "ソウルはクランの信頼を取り戻せるかもしれない"
          },
          {
            "stringId": 1883536036834439168,
            "femaleVariant": "パナムはリーダーの座に就くチャンスを失う"
          },
          {
            "stringId": 1883537676220100608,
            "femaleVariant": "ソウルはクランの信頼を取り戻せるかもしれない"
          },
          {
            "stringId": 1883538239229915136,
            "femaleVariant": "ああ"
          },
          {
            "stringId": 1883538360394969088,
            "femaleVariant": "ソウルの腕は確かだ。失敗はないと思うが、無茶をしないかだけは心配だな"
          },
          {
            "stringId": 1883538572811300864,
            "femaleVariant": "パナムは下克上のチャンスを失う"
          },
          {
            "stringId": 1883542491230760960,
            "femaleVariant": "もう失ってるようなもんだろ。明日の作戦が成功すれば、その功績は指揮を執ったソウルのもんだ"
          },
          {
            "stringId": 1883543884310114304,
            "femaleVariant": "だがもし失敗したら…"
          },
          {
            "stringId": 1883544221850923008,
            "femaleVariant": "あたしを引き込んだ張本人として、パナムが責められる…",
            "maleVariant": "俺を引き込んだ張本人として、パナムが責められる…"
          },
          {
            "stringId": 1883547246279651328,
            "femaleVariant": "そう。話したくないなら、無理には聞かない",
            "maleVariant": "そうか。話したくないなら、無理には聞かない"
          },
          {
            "stringId": 1883550276278415360,
            "femaleVariant": "あとは、みんなが自分の役割を果たせばいいだけだ"
          },
          {
            "stringId": 1883550733826650112,
            "femaleVariant": "そうね",
            "maleVariant": "そうだな"
          },
          {
            "stringId": 1883551150857908224,
            "femaleVariant": "照準を調整しておくから、お前はもう休め。夜明けにまた会おう"
          },
          {
            "stringId": 1883551719941074944,
            "femaleVariant": "わかった"
          },
          {
            "stringId": 1906577523234959360,
            "femaleVariant": "噴射した途端にバラバラになる可能性はあるが"
          },
          {
            "stringId": 1913978591703068672,
            "femaleVariant": "ソウルは退くかと思ってた"
          },
          {
            "stringId": 1913978591719845888,
            "femaleVariant": "パナムが成長すればと思ってたけど",
            "maleVariant": "パナムが成長すればと思ってたが"
          },
          {
            "stringId": 1913978591719845908,
            "femaleVariant": "二人が和解しないと、この計画は上手くいかない"
          },
          {
            "stringId": 1913978592072167424,
            "femaleVariant": "パナムは怖気づいたのね",
            "maleVariant": "パナムは怖気づいたのか"
          },
          {
            "stringId": 1913978592072167440,
            "femaleVariant": "クランは大事だけど、権力は求めてないってことか…"
          },
          {
            "stringId": 1913985261669838848,
            "femaleVariant": "で？ お前はどう思ってるんだ？"
          },
          {
            "stringId": 1927237139676246016,
            "femaleVariant": "調整は済んだの？",
            "maleVariant": "調整は済んだのか？"
          },
          {
            "stringId": 1927237370765619200,
            "femaleVariant": "調整は済んだの？",
            "maleVariant": "調整は済んだのか？"
          },
          {
            "stringId": 1935931510404804608,
            "femaleVariant": "本来ならあと2日かけていじりたいところだが… そうも言ってられないだろ"
          },
          {
            "stringId": 1935932131296014336,
            "femaleVariant": "乗りな。こいつの実力を見てみよう"
          },
          {
            "stringId": 1935933721809645568,
            "femaleVariant": "血管に打ち込んだ直後は生体モニターがとち狂ったかのように荒れるが、すぐに安定する。まるで死体のようにな"
          },
          {
            "stringId": 1937231118888202240,
            "femaleVariant": "で、試し乗りするか？"
          },
          {
            "stringId": 1937231182993944576,
            "femaleVariant": "明日はお前がこいつを操縦するんだ。試し乗りするだろ？"
          },
          {
            "stringId": 1993897413214752772,
            "femaleVariant": "まだ信じられない"
          },
          {
            "stringId": 1993912112270561308,
