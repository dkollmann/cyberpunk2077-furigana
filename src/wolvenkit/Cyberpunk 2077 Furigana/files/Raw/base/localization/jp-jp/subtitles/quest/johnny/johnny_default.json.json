  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ローグはオルトのことを全部知ってたのよね。でもオルトは…？",
                    "maleVariant": "ローグはオルトのことを全部知ってたんだよな。でもオルトは…？",
                    "stringId": 1938757784240410628
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ローグが企業のために働いてると思った理由は？",
                    "maleVariant": null,
                    "stringId": 1938758348391075840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Nie przeszkadza ci, że sypiam z Kerrym?",
                    "maleVariant": "ケリーと俺が寝たのは、どう思ってるんだ？",
                    "stringId": 1938758744350150656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ローグはオルトのことを知ってたけど、オルトは…？",
                    "maleVariant": "ローグはオルトのことを知ってたが、オルトは…？",
                    "stringId": 1938949874035478528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんでローグが企業と手を組んだと思ったの？",
                    "maleVariant": "なんでローグが企業と手を組んだと思ったんだ？",
                    "stringId": 1938951191466364932
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": null,
                    "maleVariant": "その、俺がケリーと寝たのは平気なのか？",
                    "stringId": 1938952114733338628
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ローグを知ってたかって？",
                    "maleVariant": null,
                    "stringId": 1938963283560910848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": null,
                    "maleVariant": "その話をしだすと、クソほど長くなるぞ。オルトとローグは、俺の愛車遍歴のほんの一部でしかねえからな",
                    "stringId": 1938972526347309056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "かつての恋人、アナ・マリアはいかに？ 仮想空間を漂う間に、彼女の炎は消えてしまったのか？",
                    "maleVariant": null,
                    "stringId": 1938999354323652608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "昏々と眠り続けて50年、ある日目覚めると、アントニオは女になっていた！ 彼はマヌエラの不義を許せるのか？",
                    "maleVariant": null,
                    "stringId": 1938999400511328256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうぞ、続けて",
                    "maleVariant": "どうぞ、続けてくれ",
                    "stringId": 1939009082726567936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うるせえよ。もういいだろ",
                    "maleVariant": null,
                    "stringId": 1939011504282816512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": null,
                    "maleVariant": "おいおい、「ケリーと寝た」？ たった1回ファックされただけの話だろ",
                    "stringId": 1939018908286341120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": null,
                    "maleVariant": "ああ",
                    "stringId": 1939019338806480900
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": null,
                    "maleVariant": "俺ならその1回でやめとくぜ",
                    "stringId": 1939020537052024832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": null,
                    "maleVariant": "でも…？",
                    "stringId": 1939032266456715264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": null,
                    "maleVariant": "ケリーの奴は、昔あの手この手を使って俺をベッドに誘おうとしてた",
                    "stringId": 1939032715666673664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": null,
                    "maleVariant": "お前のおかげで長年の望みを叶えたわけだから、皮肉なもんだな",
                    "stringId": 1939033101106434048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": null,
                    "maleVariant": "ハッ、自意識過剰だな。自分が脇役になるとは思わないのか？",
                    "stringId": 1939034346797944832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": null,
                    "maleVariant": "脇役？ 俺には似合わない言葉だな",
                    "stringId": 1939034865583017984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "コーポどもは事実上、アフターライフをこの街から消し去った。最後の住人であるローグがどうして生きてられる？",
                    "maleVariant": null,
                    "stringId": 1949234596263251968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それだけ？",
                    "maleVariant": "それだけか？",
                    "stringId": 1949236352669995008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それとスマッシャーだ。ローグ自身が言ったんだ… あの野郎は氷山の一角だとな",
                    "maleVariant": null,
                    "stringId": 1949236459054321664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "グレイソンは、ローグのことを知っていた",
                    "maleVariant": null,
                    "stringId": 1949238282536374272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そしてローグがスマッシャーと仕事をしてたってことは… わかるだろ",
                    "maleVariant": null,
                    "stringId": 1949238733004623872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オルトとの再会は期待通りだった？",
                    "maleVariant": "オルトとの再会は期待通りだったか？",
                    "stringId": 1954991896894656512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オルトとの再会は予想と違ったんじゃない？",
                    "maleVariant": "オルトとの再会は予想と違ったんじゃないか？",
                    "stringId": 1954993114920538112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "別人になったのは知ってた… あいつは肉体を捨て、ネットに移り住んで変わっちまったんだ",
                    "maleVariant": null,
                    "stringId": 1954994167690842112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたも変わった",
                    "maleVariant": "お前も変わった",
                    "stringId": 1954995383065923588
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "話を変えましょ",
                    "maleVariant": "話を変えよう",
                    "stringId": 1954996443805405184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "別人になったのはあんたも一緒",
                    "maleVariant": "別人になったのはお前も一緒だ",
                    "stringId": 1954996717492129792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "違う話にしましょ",
                    "maleVariant": "違う話にしよう",
                    "stringId": 1954999944690290692
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なりたくてなってるわけじゃねえ。それにオルトは、AIとして常にどこかで進化し続けてる",
                    "maleVariant": null,
                    "stringId": 1955000740316205056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここでお前と縮み上がってる俺とはえらい違いだ",
                    "maleVariant": null,
                    "stringId": 1955001125017767936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今でもハグしてくれるとでも思ってたの？",
                    "maleVariant": "今でもハグしてくれるとでも思ってたのか？",
                    "stringId": 1955010377904050176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オルトが抱きついてきてくれるとでも？",
                    "maleVariant": null,
                    "stringId": 1955010735359414272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの頃みたいに… 本音で怒ったり、話し合ったりできると思ってたんだがな",
                    "maleVariant": null,
                    "stringId": 1955011547611549696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言ってくれるね",
                    "maleVariant": "言ってくれるな",
                    "stringId": 1955012281967071232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自分の過ちを正そうと？ それとも、何かを証明してみせたかったの？",
                    "maleVariant": "自分の過ちを正そうと？ それとも、何かを証明してみせたかったのか？",
                    "stringId": 1955012653095866368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうやって泣き落そうと思ってたの？",
                    "maleVariant": "そうやって泣き落そうと思ってたのか？",
                    "stringId": 1955013730864222208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オルトを説得できた。大事なのはそれだけよ",
                    "maleVariant": "オルトを説得できた。大事なのはそれだけだ",
                    "stringId": 1955014173380071424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "話を変えましょ",
                    "maleVariant": "話を変えよう",
                    "stringId": 1955015421755936768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "過ちを正すため？ それとも何かを証明したかった？",
                    "maleVariant": "過ちを正すため？ それとも何かを証明したかったのか？",
                    "stringId": 1955016063786438656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オルトを泣き落そうとでも思ってたの？",
                    "maleVariant": "オルトを、泣き落そうとでも思ってたのか？",
                    "stringId": 1955017843664506880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺のことをあそこまで意識しなくなってるとは、思わなかった",
                    "maleVariant": null,
                    "stringId": 1955019116333780992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼女を説得できた… 大事なのはそこよ",
                    "maleVariant": "彼女を説得できた… 大事なのはそこだ",
                    "stringId": 1955019964019396608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オルトの協力をとりつけた――肝心なのはそこよ",
                    "maleVariant": "オルトの協力をとりつけた――肝心なのはそこだ",
                    "stringId": 1955020364592205824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺たちを助けるためじゃない。〈神輿〉を潰したいだけだ",
                    "maleVariant": null,
                    "stringId": 1955020979980488704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいえ、きっとアラサカがソウルキラーを使ってることに責任を感じてるからよ",
                    "maleVariant": "いいや、きっとアラサカがソウルキラーを使ってることに責任を感じてるからだ",
                    "stringId": 1955021322839674880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それを思い出したのはあんたのおかげなはず",
                    "maleVariant": "それを思い出したのはお前のおかげなはず",
                    "stringId": 1955022082512015360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふん… そうかもな",
                    "maleVariant": null,
                    "stringId": 1955022735531593728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "好きに言え。それが人間ってもんだろ",
                    "maleVariant": null,
                    "stringId": 1955023588736913408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "むしろ、俺が間違ってたと証明するためだ。あいつに、俺たちにはセックスしかないなんて言っちまったからな",
                    "maleVariant": null,
                    "stringId": 1955025193863499776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何十年も前の話じゃない",
                    "maleVariant": "何十年も前の話じゃないか",
                    "stringId": 1955027270111088640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺にとっては違う。この50年間は空白みたいなもんなんだ",
                    "maleVariant": null,
                    "stringId": 1955027583274602496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もはや関係ねえ。どっちみち50年ほど遅すぎる",
                    "maleVariant": null,
                    "stringId": 1955029140804227072
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}