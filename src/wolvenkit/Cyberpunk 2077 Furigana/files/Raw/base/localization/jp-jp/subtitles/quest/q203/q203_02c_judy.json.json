    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1177499215938363392,
            "femaleVariant": "変な話だけど… やっとわかった気がするの――ここに居場所はないんだって"
          },
          {
            "stringId": 1177535336227446784,
            "femaleVariant": "んー… 金星と… 見間違えそう…"
          },
          {
            "stringId": 1316863629788311552,
            "femaleVariant": "街を見ると思うの。私は…"
          },
          {
            "stringId": 1316864134799290368,
            "femaleVariant": "特別だって？"
          },
          {
            "stringId": 1316864578808311808,
            "femaleVariant": "迷子だって？"
          },
          {
            "stringId": 1316864757485662208,
            "femaleVariant": "何？"
          },
          {
            "stringId": 1316922372894879744,
            "femaleVariant": "特別だって？"
          },
          {
            "stringId": 1316922465169567760,
            "femaleVariant": "迷子だって？"
          },
          {
            "stringId": 1316922607759126528,
            "femaleVariant": "なに？",
            "maleVariant": "なんだ？"
          },
          {
            "stringId": 1316926093628403712,
            "femaleVariant": "私はあなたと違うから…"
          },
          {
            "stringId": 1316926516179365888,
            "femaleVariant": "確かにそう感じるときもある。でも大抵は…"
          },
          {
            "stringId": 1316929054605701120,
            "femaleVariant": "孤独だって…"
          },
          {
            "stringId": 1316929398572183552,
            "femaleVariant": "あたしがいる…"
          },
          {
            "stringId": 1316930090665897984,
            "femaleVariant": "あたしがいるじゃない…",
            "maleVariant": "俺がいるじゃないか…"
          },
          {
            "stringId": 1316930592304656384,
            "femaleVariant": "ええ… もう少しの間はね…"
          },
          {
            "stringId": 1316932573458984960,
            "femaleVariant": "その気持ちはわかる"
          },
          {
            "stringId": 1316932890615476224,
            "femaleVariant": "その気持ちはわかる"
          },
          {
            "stringId": 1316933414886699008,
            "femaleVariant": "いつかは消える？"
          },
          {
            "stringId": 1316933470268289024,
            "femaleVariant": "いや。消えない"
          },
          {
            "stringId": 1316934202107228176,
            "femaleVariant": "前はよく、自分の人生を色んなステージに区切って考えてた"
          },
          {
            "stringId": 1316934779176349696,
            "femaleVariant": "最初はメガビルディングのボロ部屋、次にグループホーム暮らし、それからモックスへ…"
          },
          {
            "stringId": 1316935335408168960,
            "femaleVariant": "毎回、居場所を見つけたって思った。そして毎回、あっけなく裏切られる"
          },
          {
            "stringId": 1316937323877363712,
            "femaleVariant": "今はどういうステージなの？"
          },
          {
            "stringId": 1316937775704567808,
            "femaleVariant": "今回はそうならない"
          },
          {
            "stringId": 1316938144249671680,
            "femaleVariant": "それがこの街の暮らしよ"
          },
          {
            "stringId": 1316938831326998528,
            "femaleVariant": "じゃあ、今のステージは？"
          },
          {
            "stringId": 1316939613447254016,
            "femaleVariant": "聞いて、V。私は…"
          },
          {
            "stringId": 1316940726296768528,
            "femaleVariant": "今回はそうならない"
          },
          {
            "stringId": 1316941077762666496,
            "femaleVariant": "なるに決まってる。Vもわかってるでしょ"
          },
          {
            "stringId": 1316944783010820096,
            "femaleVariant": "まさに、ナイトシティの縮図って感じね",
            "maleVariant": "まさに、ナイトシティの縮図って感じだ"
          },
          {
            "stringId": 1316947171817943040,
            "femaleVariant": "身にしみてわかった。だから… あなたに話したいことがあるの"
          },
          {
            "stringId": 1489205975283527680,
            "femaleVariant": "大丈夫。ここ数カ月は落ち着いてる…"
          },
          {
            "stringId": 1489206299268345856,
            "femaleVariant": "疲れてるだけ"
          },
          {
            "stringId": 1489206620333928448,
            "femaleVariant": "わかってるでしょ？ 死にかけてるの"
          },
          {
            "stringId": 1489206872411598848,
            "femaleVariant": "大丈夫。何でもない"
          },
          {
            "stringId": 1489207412587622400,
            "femaleVariant": "本当に？"
          },
          {
            "stringId": 1489207761469829120,
            "femaleVariant": "でも… 最近、変よ。まだ〈神輿〉でのことを考えてるの？ それとも… 例の仕事？"
          },
          {
            "stringId": 1489210195759984640,
            "femaleVariant": "ちょっと、疲れてるだけよ…",
            "maleVariant": "ちょっと、疲れてるだけだ…"
          },
          {
            "stringId": 1489211016484614144,
            "femaleVariant": "この一週間ずっとそんな感じだけど、何か… 隠してるんじゃない？"
          },
          {
            "stringId": 1489212088112508928,
            "femaleVariant": "わかってるでしょ。死にかけてるの。この話はやめにしましょ"
          },
          {
            "stringId": 1489213846901616640,
            "femaleVariant": "〈神輿〉での出来事のせいなんでしょ？ バスルームに新しい薬があった。悪化してるの？"
          },
          {
            "stringId": 1489220121848836096,
            "femaleVariant": "心配しないで"
          },
          {
            "stringId": 1489220400300290048,
            "femaleVariant": "話題を変えましょ"
          },
          {
            "stringId": 1489231174477410304,
            "femaleVariant": "心配しないで。すぐに… 良くなるから",
            "maleVariant": "心配するな。すぐに… 良くなるから"
          },
          {
            "stringId": 1489244574389497856,
            "femaleVariant": "良くなるって…"
          },
          {
            "stringId": 1489246268418551808,
            "femaleVariant": "最近のあなたは明らかにおかしい。心ここにあらずだもの"
          },
          {
            "stringId": 1489249677448179712,
            "femaleVariant": "今はその話をしたくない"
          },
          {
            "stringId": 1489252142541303808,
            "femaleVariant": "今は？"
          },
          {
            "stringId": 1489252543047004160,
            "femaleVariant": "私たちのことが心配なのよ"
          },
          {
            "stringId": 1489257520695996416,
            "femaleVariant": "大丈夫だから",
            "maleVariant": "大丈夫だって"
          },
          {
            "stringId": 1489257644109197312,
            "femaleVariant": "あのね… 最近ずっと思ってるのよ。こ… ここから…"
          },
          {
            "stringId": 1489258022552858624,
            "femaleVariant": "なに？",
            "maleVariant": "なんだ？"
          },
          {
            "stringId": 1489258261594632192,
            "femaleVariant": "逃げ出したいって。できるだけ遠くへ。あなたが私との間に作った壁から。それにあなたは…"
          },
          {
            "stringId": 1489269950113247232,
            "femaleVariant": "外に出てるね。クリスタル・パレスが見えるかもしれない"
          },
          {
            "stringId": 1489290696499785728,
            "femaleVariant": "それをあなたと話したかったの"
          },
          {
            "stringId": 1489300981755277312,
            "femaleVariant": "ここを出たい。ナイトシティを離れて、もっと良い生き方を見つけたいの。わかってくれる？"
          },
          {
            "stringId": 1489303811836735488,
            "femaleVariant": "そうしたほうがいい"
          },
          {
            "stringId": 1489306305518235648,
            "femaleVariant": "気をつけてね、V。無事に戻って来てよ"
          },
          {
            "stringId": 1489307357097357312,
            "femaleVariant": "うん、じゃあね"
          },
          {
            "stringId": 1489309194554830848,
            "femaleVariant": "行かないで"
          },
          {
            "stringId": 1489309836551778304,
            "femaleVariant": "そうしたほうがいい。あんたのためにもね",
            "maleVariant": "そうしたほうがいい。あんたのためにもな"
          },
          {
            "stringId": 1489311881946066944,
            "femaleVariant": "ジュディは、ナイトシティを離れるべきよ"
          },
          {
            "stringId": 1489314037885431808,
            "femaleVariant": "あなたは？"
          },
          {
            "stringId": 1489314261844488192,
            "femaleVariant": "あたしはまだ、ここでやるべきことが残ってる",
            "maleVariant": "俺はまだ、ここでやるべきことが残ってる"
          },
          {
            "stringId": 1489316743664496640,
            "femaleVariant": "要は、仕事のことを言ってるんでしょ？ 例のアフターライフの件… それがそうなの？"
          },
          {
            "stringId": 1489317527395364864,
            "femaleVariant": "行かないで。あたしには、あんたが必要なの",
            "maleVariant": "行かないでくれ。俺には、あんたが必要なんだ"
          },
          {
            "stringId": 1489318391824642048,
            "femaleVariant": "V… あなたは自分の居場所を見つけたでしょ。ここはもうあなたの街よ。すっかりアフターライフのクイーン… 生ける伝説じゃない…"
          },
          {
            "stringId": 1489319827065139200,
            "femaleVariant": "あなたが何をするのかわからないけど… 手伝えることはある？"
          },
          {
            "stringId": 1489320209115901952,
            "femaleVariant": "いや。あたし一人でやらなきゃ",
            "maleVariant": "いや。俺一人でやらないと"
          },
          {
            "stringId": 1489322894191882240,
            "femaleVariant": "じゃあ引き受けるのね。その仕事"
          },
          {
            "stringId": 1702767841629556736,
            "femaleVariant": "V、大丈夫？ 具合が悪いの？"
          },
          {
            "stringId": 1702778208304877568,
            "femaleVariant": "[コーヒーを飲む]"
          },
          {
            "stringId": 1786238066200539136,
            "femaleVariant": "じゃあ、何かあったらキッチンにいるからね"
          },
          {
            "stringId": 1792187546099904512,
            "femaleVariant": "その他大勢の1人でいること、匿名でいることは、安全だと思ってた"
          },
          {
            "stringId": 1792189919052558336,
            "femaleVariant": "でもそれは幻想"
          },
          {
            "stringId": 1792441489530941440,
            "femaleVariant": "ここ数カ月は、落ち着いてる"
          },
          {
            "stringId": 1792444264297918464,
            "femaleVariant": "そんなことないって"
          },
          {
            "stringId": 1792444591923392512,
            "femaleVariant": "でも… 私や、周りの何もかもが目に入ってないじゃない。なら、例の仕事のこと？"
          },
          {
            "stringId": 1793549059775655936,
            "femaleVariant": "V… 私もバカじゃないの。あなたが壁を作って私を遠ざけてるのはわかってる"
          },
          {
            "stringId": 1793553490973945856,
            "femaleVariant": "V… 私、ダメね"
          },
          {
            "stringId": 1793556414319947776,
            "femaleVariant": "クリスタル・パレスの話じゃないことは明らかね",
            "maleVariant": "クリスタル・パレスの話じゃないことは明らかだな"
          },
          {
            "stringId": 1793639509354213376,
            "femaleVariant": "ここに居たって何もいいことはない。それにあたしはもうすぐ…",
            "maleVariant": "ここに居たって何もいいことはない。それに俺はもうすぐ…"
          },
          {
            "stringId": 1793646627322982404,
            "femaleVariant": "この街にあたしの力を示してやるの"
          },
          {
            "stringId": 1793653174564417536,
            "femaleVariant": "残り時間は少ない。今やるしかないの"
          },
          {
            "stringId": 1793656252294361088,
            "femaleVariant": "私にも本当の自分を見つけさせて。真実を。その、なんていうか… クソ、どうやって伝えればいいのかわからない…"
          },
          {
            "stringId": 1793659187166310400,
            "femaleVariant": "ありがとう、ジュディ。話してくれて… これまで付き合ってくれて"
          },
          {
            "stringId": 1793662567825666048,
            "femaleVariant": "どこにいようと、きっとあたしの名が耳に入る"
          },
          {
            "stringId": 1793672204255883264,
            "femaleVariant": "ナイトシティにあたしの爪痕を残す、最後のチャンスなのよ"
          },
          {
            "stringId": 1793672295289057280,
            "femaleVariant": "もっと違った形で一緒に過ごしたかった"
          },
          {
            "stringId": 1793672382983565312,
            "femaleVariant": "上手くいけば――どこにいようと、きっとあたしの名が耳に入る。それもうっとうしいくらいにね",
            "maleVariant": "上手くいけば――どこにいようと、きっと俺の名が耳に入る。それもうっとうしいくらいにな"
          },
          {
            "stringId": 1793683513139654656,
            "femaleVariant": "エッジを生きるVからの便りってわけね"
          },
          {
            "stringId": 1793684569131184128,
            "femaleVariant": "いいじゃない。楽しみにしてる"
          },
          {
            "stringId": 1793687614866976768,
            "femaleVariant": "ええ… そうね"
          },
          {
            "stringId": 1793687927996936192,
            "femaleVariant": "でも今やるしかないの。まだ時間が――まだやれることがあるうちに",
            "maleVariant": "だが今やるしかないんだ。まだ時間が――まだやれることがあるうちに"
          },
          {
            "stringId": 1793688605125373952,
            "femaleVariant": "やるべきことは次々に現れるでしょ。いつだって新しい試練に挑み続ける――最後の最後まで"
          },
          {
            "stringId": 1793690917126402048,
            "femaleVariant": "クイーンじゃ物足りない？ とことんやらないと気が済まないのね"
          },
          {
            "stringId": 1793691486645776384,
            "femaleVariant": "ええ、そういうこと"
          },
          {
            "stringId": 1872145575949488128,
            "femaleVariant": "[立ち上がる] シャワーを浴びてくる"
          },
          {
            "stringId": 1872145576066928640,
            "femaleVariant": "[キスする] おはよう"
          },
          {
            "stringId": 1873279731670577152,
            "femaleVariant": "おはよう"
          },
          {
            "stringId": 1873281308930207744,
            "femaleVariant": "おはよう… シャワーを浴びてくる"
          },
          {
            "stringId": 1873282441593614336,
            "femaleVariant": "おはよう"
          },
          {
            "stringId": 1873291738771746820,
            "femaleVariant": "[抱きしめる] そろそろ行かないと"
          },
          {
            "stringId": 1873291976303570944,
            "femaleVariant": "[去る] そろそろ行かないと"
          },
          {
            "stringId": 1873293309454708736,
            "femaleVariant": "そろそろ行かないと"
          },
          {
            "stringId": 1873295799294226432,
            "femaleVariant": "わかってる"
          },
          {
            "stringId": 1873296461155397632,
            "femaleVariant": "行かないと。アフターライフでみんなが待ってる"
          },
          {
            "stringId": 1873296954808201216,
            "femaleVariant": "わかった"
          },
          {
            "stringId": 1908535498032115712,
            "femaleVariant": "クソ…"
          },
          {
            "stringId": 1908538849717219328,
            "femaleVariant": "[血を洗い流す]"
          },
          {
            "stringId": 1912511109746487300,
            "femaleVariant": "[寝返りを打つ]"
          },
          {
            "stringId": 1912555781315457024,
            "femaleVariant": "シャワーを浴びてくる"
          },
          {
            "stringId": 1912558603746504708,
            "femaleVariant": "[立ち上がる] シャワーを浴びてくる"
          },
          {
            "stringId": 1912604854571593732,
            "femaleVariant": "[シャワーを浴びる]"
          },
          {
            "stringId": 1915759604586606592,
            "femaleVariant": "[寄りかかる]"
          },
          {
            "stringId": 2008145281013116928,
            "femaleVariant": "んん。私も"
          },
          {
            "stringId": 2008145639642886144,
            "femaleVariant": "私も"
          },
          {
            "stringId": 2008146930565771264,
            "femaleVariant": "どうしたの？"
          },
          {
            "stringId": 2008149180893753344,
            "femaleVariant": "何でもない…"
          },
          {
            "stringId": 2008425566350995456,
            "femaleVariant": "〈神輿〉で… 私に話してない何かがあったんじゃないの？"
          },
          {
            "stringId": 2237247569292095488,
            "femaleVariant": "自分を大事にね、V"
          },
          {
            "stringId": 2237248482257858560,
            "femaleVariant": "頑張ってね、V。私はもう少しここにいるから"
          },
          {
            "stringId": 2404283116062240772,
            "femaleVariant": "[アミュレットを取る]"
          }
        ]
      }
    }
  }
