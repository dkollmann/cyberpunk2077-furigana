  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちくしょう、まただ… なあ、手を貸してくれ",
              "maleVariant": "",
              "stringId": "1811292423955697664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[開ける]",
              "maleVariant": "",
              "stringId": "1811296530565189636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこを押せ。少し左に… ああ、そうだ",
              "maleVariant": "",
              "stringId": "1811305132646486016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっとミッチ… どういうこと！？",
              "maleVariant": "おいミッチ… どういうことだ！？",
              "stringId": "1811373862323769344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着けよ。これがスコーピオンの望みなんだ",
              "maleVariant": "",
              "stringId": "1811385601476022272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スペアタイヤみたいにトランクに詰め込まれることが？",
              "maleVariant": "スペアタイヤみたいにトランクに詰め込まれることがか？",
              "stringId": "1811385744166244352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それがスコーピオンの望みだって言うなら…",
              "maleVariant": "",
              "stringId": "1811386006964555780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソウルが怒るんじゃない？",
              "maleVariant": "ソウルが怒るんじゃないか？",
              "stringId": "1811386633811677184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "車をダメにしたら、きっとソウルが怒るでしょ。もったいないだろって",
              "maleVariant": "車をダメにしたら、きっとソウルが怒るだろ。もったいないだろって",
              "stringId": "1811386964826148864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "事故だったって伝えりゃいい",
              "maleVariant": "",
              "stringId": "1811387447355658240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、スコーピオンがそれを望んだって言うなら…",
              "maleVariant": "",
              "stringId": "1811387679434887168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、それが遺言だ。何度も聞いたよ",
              "maleVariant": "",
              "stringId": "1811388212967133184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[タンクを拾う]",
              "maleVariant": "",
              "stringId": "1811389513553694724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はい",
              "maleVariant": "ほらよ",
              "stringId": "1811398952448970752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし。それじゃあ助手席に置いてくれ",
              "maleVariant": "",
              "stringId": "1811399237124771840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[タンクを置く]",
              "maleVariant": "",
              "stringId": "1811399600636710912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "運転手は… 準備できてるみたい",
              "maleVariant": "運転手は… 準備できてるみたいだ",
              "stringId": "1811414052681568256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "急いでやるぞ。こいつの体はあと1、2時間で風船みたいに膨らんじまう",
              "maleVariant": "",
              "stringId": "1811416248164831232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やろう",
              "maleVariant": "",
              "stringId": "1811418292787367936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当にいいの？",
              "maleVariant": "本当にいいのか？",
              "stringId": "1811419470111723520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、本当にこんなことしていいの？",
              "maleVariant": "なあ、本当にこんなことしていいのか？",
              "stringId": "1811421128254312448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあな、V。俺にもわからん",
              "maleVariant": "",
              "stringId": "1811422682579488768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やろう",
              "maleVariant": "",
              "stringId": "1811422853220552704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうこなくっちゃ。仕掛けてある布きれに火をつけてくれ。俺はエンジンをかける",
              "maleVariant": "",
              "stringId": "1811423155025891328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[火をつける]",
              "maleVariant": "",
              "stringId": "1811423554004865028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "火がついた！ 急がないと爆発するよ！",
              "maleVariant": "火がついた！ 急がないと爆発するぞ！",
              "stringId": "1811423894330691584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "上手くいったな。これで約束は果たせた",
              "maleVariant": "",
              "stringId": "1811424662542635008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残りし灰よ、安らかなれ",
              "maleVariant": "",
              "stringId": "1811425287259049984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。しっかり見送れたことだし",
              "maleVariant": "ああ。しっかり見送れたことだしな",
              "stringId": "1811425434059689984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいね",
              "maleVariant": "いいな",
              "stringId": "1811425723818987520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて… ビールでもどうだ？",
              "maleVariant": "",
              "stringId": "1811722638246825984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう行かないと",
              "maleVariant": "",
              "stringId": "1811735446527168512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ごめん、そういう気分じゃない",
              "maleVariant": "すまない、そういう気分じゃない",
              "stringId": "1811735716288024576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、決まりだ",
              "maleVariant": "",
              "stringId": "1811741647050657792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… 冷えたビールは最高だ",
              "maleVariant": "",
              "stringId": "1811744593180119040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まったくね",
              "maleVariant": "まったくだ",
              "stringId": "1811747013125754880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[受け取る]",
              "maleVariant": "",
              "stringId": "1845779842239057924"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何これ？",
              "maleVariant": "何だこれ？",
              "stringId": "1845780327872352256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スコーピオンはいつも肌身離さずそれを持ってた。理由は知らん",
              "maleVariant": "",
              "stringId": "1845792706219757568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なあ… 俺はもうしばらくここに居るよ。飲み足りねえ",
              "maleVariant": "",
              "stringId": "1845792948096880640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "違う。あいつはド派手に弔って欲しがってたんだ",
              "maleVariant": "",
              "stringId": "1863237586613751808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつを運転席に座らせたら、ガソリンをセットし、それから火をつけて崖下に落とす",
              "maleVariant": "",
              "stringId": "1863238860105109504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭がおかしいんじゃないの！？",
              "maleVariant": "頭がおかしいんじゃないのか！？",
              "stringId": "1863240077191467008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミッチ。こんなのどうかしてる",
              "maleVariant": "",
              "stringId": "1863241269229441024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "文句は受け付けねえよ",
              "maleVariant": "",
              "stringId": "1863243586733395968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "タンクを取ってくれるか？",
              "maleVariant": "",
              "stringId": "1863245705880326144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃあ… ショーを始めるとするか",
              "maleVariant": "",
              "stringId": "1863249863308005376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でももしこれがあいつの最後の望みだとしたら、俺はそれを叶えてやる。たとえお前が協力してくれなくてもな",
              "maleVariant": "",
              "stringId": "1863252538233769984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ならいいさ",
              "maleVariant": "",
              "stringId": "1863258104175841280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、V！ 助かったよ。ありがとうな",
              "maleVariant": "",
              "stringId": "1863258316642504704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、ミッチ… どうしてわざわざあたしに声をかけたの？",
              "maleVariant": "なあ、ミッチ… どうしてわざわざ俺に声をかけたんだ？",
              "stringId": "1863262761832992768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてあたしに声をかけたの？",
              "maleVariant": "どうして俺に声をかけたんだ？",
              "stringId": "1863262994465869828"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“スコーピオン”の由来は？",
              "maleVariant": "",
              "stringId": "1863263606398046208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ… 彼はどうして“スコーピオン”って愛称で呼ばれてたの？",
              "maleVariant": "なあ… あいつはどうして“スコーピオン”って愛称で呼ばれてたんだ？",
              "stringId": "1863264231651332096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、それがな… 俺もまったく知らねえんだ。聞く機会がなくてよ",
              "maleVariant": "",
              "stringId": "1863264970436038656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さそり座だから、とか？",
              "maleVariant": "",
              "stringId": "1863265301551173632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へっ… デカい毒針がついてたのかもな！ ハハハ！",
              "maleVariant": "",
              "stringId": "1863266236545421312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ったく、スコーピオンの野郎… 寂しくなるぜ",
              "maleVariant": "",
              "stringId": "1863266849886883840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい奴そうだからさ",
              "maleVariant": "",
              "stringId": "1863267756393418752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真面目に答えて",
              "maleVariant": "真面目に答えろ",
              "stringId": "1863268095561617408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ… どうだろうな… でもこうして、あいつをちゃんと見送ることができたのは… お前のおかげだ",
              "maleVariant": "",
              "stringId": "1863268252848017408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "偶然のめぐり合わせさ。少しの間、俺たちの道は交わった… それだけだ",
              "maleVariant": "",
              "stringId": "1863269066895314944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、いいか… これは幸運のお守りだ。スコーピオンが持ってた。お前に預ける。貰ってやってくれ",
              "maleVariant": "",
              "stringId": "1863270157296914432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。あたしはもう行く",
              "maleVariant": "わかった。俺はもう行く",
              "stringId": "1863272334694338560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それと… ありがとうな、V。今日のことも… あのとき助けてくれたこともよ",
              "maleVariant": "",
              "stringId": "1863272509596815360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前はいい奴だ。感謝してる",
              "maleVariant": "",
              "stringId": "1863273061936320512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "遠慮しとく",
              "maleVariant": "",
              "stringId": "1873522370078363648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "遠慮しとく。今は酔いたくないの",
              "maleVariant": "遠慮しとく。今は酔いたくないんだ",
              "stringId": "1873523224223207424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "都会っ子は違うな、チューマ。まあ、弔い方は人それぞれだ",
              "maleVariant": "",
              "stringId": "1873524512897945600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コーヒーだって",
              "maleVariant": "",
              "stringId": "1873528054668906496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。そうだ… もしアシがないなら、俺の車でキャンプに戻ってもいいぞ",
              "maleVariant": "",
              "stringId": "1908015766788427776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "道中、気をつけて逝ってこいよ",
              "maleVariant": "",
              "stringId": "1934467678261235712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、何のつもり？",
              "maleVariant": "おい、何のつもりだ？",
              "stringId": "2121969965643616260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうした、V？ 容器だよ。早くしてくれ",
              "maleVariant": "",
              "stringId": "2238669820146728960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ、スコーピオンを送ってやろう",
              "maleVariant": "",
              "stringId": "2238714786642378752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、火をつけていいぞ",
              "maleVariant": "",
              "stringId": "2238716481912946688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、スコーピオンのためだ",
              "maleVariant": "",
              "stringId": "2238716544173195264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしたんだ？",
              "maleVariant": "",
              "stringId": "2238721843760185344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おいおい、まだ黙とうの時間じゃないぞ",
              "maleVariant": "",
              "stringId": "2238722204570992644"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何か考え事か？",
              "maleVariant": "",
              "stringId": "2238722248493744132"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}