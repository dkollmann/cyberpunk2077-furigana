        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんにちは、アリフ・イクバルです。WNSニュースの時間です",
              "maleVariant": "",
              "stringId": 1793557025046568960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずは国際ニュースです。アラスカが苦境に立たされています",
              "maleVariant": "",
              "stringId": 1796596829488136192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "インコーポレイテッド・プレスによると、同地の農業地帯を襲った度重なる水害により、トウモロコシ収穫量が昨年からなんと80%減少",
              "maleVariant": "",
              "stringId": 1796596985583353856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "現地自治体は、フロストコーンのハイブリッド種関連技術を提供するバイオテクニカ社に対し、8000万ブッシェルの作物提供を行う義務を負っています",
              "maleVariant": "",
              "stringId": 1796597213099180032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "難しい決断を迫られているジュノー市。契約を守ってアラスカに深刻な作物不足をもたらすか、翌年のトウモロコシ種子提供を諦めるかを選択しなければなりません",
              "maleVariant": "",
              "stringId": 1796597438970839040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラスカは自立の重要性を身をもって学ぶべきか、あるいは人道支援を受けるべきか。ぜひ投票にご参加ください",
              "maleVariant": "",
              "stringId": 1796597614913503232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "不慮の事故か、計画的テロか？ ミラノがナノ・ビーによる襲撃に遭いました",
              "maleVariant": "",
              "stringId": 1796597935022784512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "目撃者の証言によると、フェンフアン社のナノボット700万体が突如、授粉活動を停止。ロンバルディアの州都ミラノに押し寄せました。原因は未だ分かっていません",
              "maleVariant": "",
              "stringId": 1796598128296312832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナノ・ビーが建物や走行中の車両に群がり、多数の事故が発生している模様です",
              "maleVariant": "",
              "stringId": 1796598491892137984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミラノ市長ロレンツォ・リッチは、この現象の原因を究明すべく、調査委員会を立ち上げたことを発表しました",
              "maleVariant": "",
              "stringId": 1796598658875768832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分の国を持ちたいという夢が、現実になります",
              "maleVariant": "",
              "stringId": 1796598853776687104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "不動産会社のノーリミッツ・アトールは、今回新たに15の人工島を発表。販売も開始されています",
              "maleVariant": "",
              "stringId": 1796599235458351104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どの島も3平方マイル以上の広さで、マグニチュード8の地震に耐えられる設計になっています",
              "maleVariant": "",
              "stringId": 1796599498525097984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また島は公海に位置しているため、居住者はどの国の司法の影響も受けることはなく、独自に法や慣習を制定できるというわけです",
              "maleVariant": "",
              "stringId": 1796599677068230656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなただけの楽園を手に入れたいという方は、今すぐ下記アドレスへ",
              "maleVariant": "",
              "stringId": 1796599959562993664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "西海岸は今日もとても快適で過ごしやすい気候となっています。それでは、またお会いしましょう",
              "maleVariant": "",
              "stringId": 1796600163976593408
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}