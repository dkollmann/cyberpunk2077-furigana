                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[立ち上がる] オルトに関しては初耳のことばかり",
                    "maleVariant": "[立ち上がる] オルトに関しては初耳のことばかりだ",
                    "stringId": 1648982992342028288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[立ち上がる] 昔のあんたは相当なクズだったのね",
                    "maleVariant": "[立ち上がる] 昔のお前は相当なクズだったんだな",
                    "stringId": 1648991982882983936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何でオルトのこと話してくれなかったの？",
                    "maleVariant": "なぜオルトのことを話してくれなかった",
                    "stringId": 1649000478244409344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最低ね。2020年のクズ・オブ・ザ・イヤーでも狙ってたわけ？",
                    "maleVariant": "最低だな。2020年のクズ・オブ・ザ・イヤーでも狙ってたのか？",
                    "stringId": 1649001432884776960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんだ、慰めの言葉を期待してたんだがな",
