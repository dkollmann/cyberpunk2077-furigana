                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オルトのことよ",
                    "maleVariant": "オルトのことだ",
                    "stringId": 1649113850667405316
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もういい。でもオルトに関しては初耳のことばかり",
                    "maleVariant": "もういい。だがオルトに関しては初耳のことばかりだ",
                    "stringId": 1649114451358846976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
