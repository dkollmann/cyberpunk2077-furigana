        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、よく来てくれたわね",
                    "maleVariant": null,
                    "stringId": 1697943261782384640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[座る] もらうよ",
                    "maleVariant": null,
                    "stringId": 1697944056284225540
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、ちょうだい",
                    "maleVariant": "ああ、頼む",
                    "stringId": 1697945210154033152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや、今はいい",
                    "maleVariant": null,
                    "stringId": 1697945291003437056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[座る] 要らない",
                    "maleVariant": null,
                    "stringId": 1697945770798260224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "式に参加したい",
                    "maleVariant": null,
                    "stringId": 1697948702516092932
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしも式に参加したい。ちゃんと送ってあげたいの… 伝統的なやり方で",
                    "maleVariant": "俺も式に参加したい。ちゃんと送ってあげたいんだ… 伝統的なやり方で",
                    "stringId": 1697949664689430528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしも手伝うよ",
                    "maleVariant": "俺も手伝うよ",
                    "stringId": 1697949734381985792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあ、ほら、座って。ビールにする？",
                    "maleVariant": null,
                    "stringId": 1702514181381464064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[飲む]",
                    "maleVariant": null,
                    "stringId": 1702514922464649216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "手伝わせて",
                    "maleVariant": "手伝わせてくれ",
                    "stringId": 1722525784814297088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l="mex" o="Salud." t="乾杯" b="" a=""/>",
                    "maleVariant": null,
                    "stringId": 1825605937628237824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l="mex" o="No hay bronca." t="問題ないわ" b="" a=""/>",
                    "maleVariant": null,
                    "stringId": 1825607631439187968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あまり良くない",
                    "maleVariant": null,
                    "stringId": 1864865408376164356
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんとかやってる",
                    "maleVariant": null,
                    "stringId": 1864865530866618368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "正直言うとあまり良くない",
                    "maleVariant": null,
                    "stringId": 1864866024754302976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんとかやってる",
                    "maleVariant": null,
                    "stringId": 1864866333522186240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしたのよ？",
                    "maleVariant": null,
                    "stringId": 1864871197220327424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジャッキーが亡くなった、あの夜以来… なんて言うか、物事があまりうまくいってなくて",
                    "maleVariant": null,
                    "stringId": 1864871267181318144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l="mex" o="Sé cómo te sientes. Todos lo sabemos." t="気持ちはわかる。みんなそうよ" b="" a=""/>",
                    "maleVariant": null,
                    "stringId": 1864871351604269056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんとかね",
                    "maleVariant": null,
                    "stringId": 1864871887401439232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "夕飯でも食べに来ればよかったのに。ここはあなたの家なのよ",
                    "maleVariant": null,
                    "stringId": 1864871950198558720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも、こうやって来てくれたことだし。この件はこれぐれいにしときましょ",
                    "maleVariant": null,
                    "stringId": 1864872077336301568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大丈夫よ。大切なのはあなたの気持ち",
                    "maleVariant": null,
                    "stringId": 1864881922038878208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "供え物はただのシンボル。あの子のガレージで何かを探すといいわ。思い出はみんなあそこにあるから",
                    "maleVariant": null,
                    "stringId": 1864881973729480704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ガレージ？",
                    "maleVariant": null,
                    "stringId": 1864883745504153604
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ガレージを持ってたの？",
                    "maleVariant": "ガレージを持ってたのか？",
                    "stringId": 1864885510282727424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいのよ。あなたには思いやりの気持ちがある。素晴らしいことだわ",
                    "maleVariant": null,
                    "stringId": 1864886540772888576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、最初はバイク用に使ってたんだけどね。いつのまにか、そっちに住み始めたの。きっと何か見つかるわ",
                    "maleVariant": null,
                    "stringId": 1864886962216554496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "勝手に見て回っても？",
                    "maleVariant": null,
                    "stringId": 1864887180974673920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もちろんよ。あの子はもういないの。式は生き残った私たちのためにやるんだからね。ほら、鍵よ",
                    "maleVariant": null,
                    "stringId": 1864887304723419136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l="mex" o="Ahí te veo." t="じゃあね" b="" a=""/>",
                    "maleVariant": null,
                    "stringId": 1864889318509432832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あれからずいぶん経つけど、調子はどうなの？",
                    "maleVariant": null,
                    "stringId": 1896490727361007616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オフレンダの準備をしてるとこなの。まだまだやることがたくさんあるわ…",
                    "maleVariant": null,
                    "stringId": 1924407058265731072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "グラシアス。でも大丈夫よ",
                    "maleVariant": null,
                    "stringId": 1926981639971205120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "来てくれるだけで十分",
                    "maleVariant": null,
                    "stringId": 1926985406405865472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも… 棺に置くものがさ。あいつとの思い出に捧げられるようなものが… 何もない",
                    "maleVariant": null,
                    "stringId": 1927026960986206208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もちろん",
                    "maleVariant": null,
                    "stringId": 1928580049748815872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ありがとう、ママ・ウェルズ",
                    "maleVariant": null,
                    "stringId": 1928581776711544832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オフレンダって？",
                    "maleVariant": null,
                    "stringId": 1934506005374103552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オフレンダって… 何をするの？",
                    "maleVariant": "オフレンダって… 何をするんだ？",
                    "stringId": 1934506268591845376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "家族や友達が集まって、旅立った人との思い出を祝福するの。その人が生前好きだったものや、あの世で役立ちそうなものを持ち寄ってね",
                    "maleVariant": null,
                    "stringId": 1934507718629847040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "となると、ビールは欠かせないね",
                    "maleVariant": "となると、ビールは欠かせないな",
                    "stringId": 1934508314640445440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "フフフ… もちろん用意してる",
                    "maleVariant": null,
                    "stringId": 1934526562161324032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ガレージはリパブリック通りよ",
                    "maleVariant": null,
                    "stringId": 1934545599184433152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[鍵を取って立ち上がる] ありがとう",
                    "maleVariant": null,
                    "stringId": 1943264135333453824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ビリヤード台は運び出さないとダメね",
                    "maleVariant": null,
                    "stringId": 1976395134477742080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本気か？ かなりデカいぞ…",
                    "maleVariant": null,
                    "stringId": 1976445675203407872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ならどうするの？",
                    "maleVariant": null,
                    "stringId": 1976446243716149248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "壁のとこまで押してって、立て掛けるってのはどうだ？",
                    "maleVariant": null,
                    "stringId": 1976446577716965376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そう、私よ",
                    "maleVariant": null,
                    "stringId": 1983628376209567744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "モンセラート・ウェルズよ。ええ、コヨーテの",
                    "maleVariant": null,
                    "stringId": 1983631029392060420
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l="mex" o="Vamos, responde." t="もう、出てちょうだい" b="" a=""/>",
                    "maleVariant": null,
                    "stringId": 1983631138360078340
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私よ。近々ジャッキーのオフレンダを開くんだけど",
                    "maleVariant": null,
                    "stringId": 1983631263484555268
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何度も連絡したのよ。なのにいっこうに出ないし折り返しもしない…",
                    "maleVariant": null,
                    "stringId": 1988173848277590016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今度オフレンダを開くの。息子のためにね",
                    "maleVariant": null,
                    "stringId": 1998391940310036480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、もちろん来てちょうだい",
                    "maleVariant": null,
                    "stringId": 1998392346553544704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。それじゃあね",
                    "maleVariant": null,
                    "stringId": 1998392598832541696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "配達を頼みたいの",
                    "maleVariant": null,
                    "stringId": 1998395163884318720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ビールを12ケース。そうね。2つはジンジャーのやつでお願い",
                    "maleVariant": null,
                    "stringId": 1998395575093882880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そう、レモンとライムも",
                    "maleVariant": null,
                    "stringId": 1998396004741607424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "テキーラは… ええ、それも12ケース…",
                    "maleVariant": null,
                    "stringId": 1998396724970713088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふう…",
                    "maleVariant": null,
                    "stringId": 1998405380252561408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そう… 無理なのね",
                    "maleVariant": null,
                    "stringId": 1998408648806559744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった",
                    "maleVariant": null,
                    "stringId": 1998408909239283712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l="mex" o="Pfff. Bai." t="ふぅ… じゃあね" b="" a=""/>",
                    "maleVariant": null,
                    "stringId": 1998409164420739072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クソ… やっぱ重てえな",
                    "maleVariant": null,
                    "stringId": 2238924446696128512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハア… この… 動けって…",
                    "maleVariant": null,
                    "stringId": 2238983336569782272
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
