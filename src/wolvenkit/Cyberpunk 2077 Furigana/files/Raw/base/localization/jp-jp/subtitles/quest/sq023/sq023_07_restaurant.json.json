      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] バスケスは相当頭がかたいのね",
              "maleVariant": "[座る] バスケスは相当頭がかたいんだな",
              "stringId": 1380647231712051200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バスケスは相当頭がかたいのね。警官にしても",
              "maleVariant": "バスケスは相当頭がかたいんだな。警官にしても",
              "stringId": 1563588639853535232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お見事。実にいい演技だったぞ",
              "maleVariant": "",
              "stringId": 1568866410607566848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この件、あんたはどう思う？",
              "maleVariant": "この件、お前はどう思う？",
              "stringId": 1568872611047170048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつは常軌を逸した変人だ。だが企業に搾取されてることに変わりはない",
              "maleVariant": "",
              "stringId": 1568872851749888000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼がそんな状態だとわかってて、なおこだわる理由は？",
              "maleVariant": "",
              "stringId": 1773533062359912448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": 1773545705602781188
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、取引成立よ",
              "maleVariant": "よし、取引成立だ",
              "stringId": 1773546922638807040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "断る",
              "maleVariant": "",
              "stringId": 1773547592351715328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お断りよ",
              "maleVariant": "お断りだ",
              "stringId": 1773548161552322560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "賢い選択ね",
              "maleVariant": "",
              "stringId": 1773563512402972672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これがどれだけ得な話かわかってないようね…",
              "maleVariant": "",
              "stringId": 1773573910636236800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちゃんとわかってる。店に入りましょ",
              "maleVariant": "ちゃんとわかってる。店に入るぞ",
              "stringId": 1773584804267134976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何やってんだ",
              "maleVariant": "",
              "stringId": 1773586333812363264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんな別れになって残念だ",
              "maleVariant": "",
              "stringId": 1773649125412147200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うるせえ",
              "maleVariant": "",
              "stringId": 1773651305661698048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう構わないで…",
              "maleVariant": "",
              "stringId": 1773651480664838144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう一声で考えてもいい",
              "maleVariant": "",
              "stringId": 1783852380582809600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BDの制作費がどの程度かはあたしも知ってる。そんなんじゃ見合わない。全然ね",
              "maleVariant": "BDの制作費がどの程度かは俺も知ってる。そんなんじゃ見合わない。全然だ",
              "stringId": 1783853559450669060
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいわ。3倍出す… それですべて忘れて消えてちょうだい",
              "maleVariant": "",
              "stringId": 1783853559450669068
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "店には来ないで。ヨシュアには私から伝えとく",
              "maleVariant": "",
              "stringId": 1803755698729750528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちんけな傭兵にしてはやるじゃねえか",
              "maleVariant": "",
              "stringId": 1803762980695805952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "面白くなってきた",
              "maleVariant": "",
              "stringId": 1803768386751008772
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああいうコーポ女は腹が立つ",
              "maleVariant": "",
              "stringId": 1803770119652872192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうなるか、興味がわいてきた",
              "maleVariant": "",
              "stringId": 1803770253065293824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "結末は苦痛と利得だな。崇高な行いか、はたまた狂気か。だがそれらはほとんど同じ概念なのかもしれない",
              "maleVariant": "",
              "stringId": 1803771764373688320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの女、何でも金で解決できると思ってるのね…",
              "maleVariant": "あの女、何でも金で解決できると思いやがって…",
              "stringId": 1803775051802054660
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもお前は屈しなかった。ただ、ああいう女はムキになると怖いぜ。気をつけろ",
              "maleVariant": "",
              "stringId": 1803775051852386308
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨシュアには支えが必要ね",
              "maleVariant": "ヨシュアには支えが必要だ",
              "stringId": 1803777315434369024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしたちでヨシュアを支えられるかしら",
              "maleVariant": "俺たちでヨシュアを支えられるかな",
              "stringId": 1803778433719701508
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫だ。現世のイエスにだって2人の罪人が必要だろ",
              "maleVariant": "",
              "stringId": 1803778433719701516
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": 1803789808907030528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[降りる] ヨシュアは精神科に行くべき",
              "maleVariant": "[降りる] ヨシュアは精神科に行くべきだ",
              "stringId": 1805611572299063300
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直、どういう人間かはよくわからない",
              "maleVariant": "正直、どういう人間かよくわからない",
              "stringId": 1805611847495737344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どう思う？ BD界の大型新人について",
              "maleVariant": "",
              "stringId": 1805612117340479488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[降りる] 彼の信仰は本物",
              "maleVariant": "[降りる] 彼の信仰は本物だ",
              "stringId": 1805613361001947136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[降りる] 悪い人にはみえない",
              "maleVariant": "",
              "stringId": 1805613424335937536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[降りる] まだどういう人間かわからない",
              "maleVariant": "",
              "stringId": 1805614355538534400
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼の信仰心は本物だと思う",
              "maleVariant": "",
              "stringId": 1805614984365367296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭のチップが緩んでるんじゃない？ 医者に行った方がいい",
              "maleVariant": "頭のチップが緩んでるんじゃないか？ 医者に行った方がいい",
              "stringId": 1805615122525741056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その… 殺人犯みたいには見えないね",
              "maleVariant": "その… 殺人犯みたいには見えないな",
              "stringId": 1805615456191012864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私もそう思う。でも、もう手遅れよ",
              "maleVariant": "",
              "stringId": 1805619484551569408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいえ、ただの変人よ。でもその点はどうでもいい",
              "maleVariant": "",
              "stringId": 1805624876614574080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、そうかもね。でもヨシュアは完全にタガが外れちゃってる",
              "maleVariant": "",
              "stringId": 1805627137663197184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "傷つくのが怖くて、善人を演じる人もいる。もしくは宗教に縛られているだけか",
              "maleVariant": "",
              "stringId": 1805631173607055360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ教えてあげる",
              "maleVariant": "",
              "stringId": 1805634883368280064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨシュアは罪について深く考えるあまり、狂気の一歩手前まで追い詰められた哀れな男よ",
              "maleVariant": "",
              "stringId": 1805635802491281408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨシュアは、誰もが求めるのに、簡単にお目にかかれないあるモノを提供してくれる",
              "maleVariant": "",
              "stringId": 1805638538670661632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正真正銘のリアリティってやつよ。あの頭の中は完全に糞詰まりだけど、自分の言うことを信じ切ってるからね",
              "maleVariant": "",
              "stringId": 1805640006458957824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BD俳優にぴったりね",
              "maleVariant": "BD俳優にぴったりだな",
              "stringId": 1805641998921129988
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど、BD俳優にぴったりね",
              "maleVariant": "なるほど、BD俳優にぴったりだな",
              "stringId": 1805642190986698752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼はあんたが思ってる以上に冷静だと思うけど",
              "maleVariant": "奴はあんたが思ってる以上に冷静だと思うがな",
              "stringId": 1805642268480659456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは彼を過小評価してる",
              "maleVariant": "",
              "stringId": 1805642341864202240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、何をしても失敗にはならない",
              "maleVariant": "",
              "stringId": 1805685483300741120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お互い精神科医じゃないんだし、金が手に入るなら何でもいいでしょ",
              "maleVariant": "",
              "stringId": 1805694187605614592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このヨシュアって男は、うちのドル箱スターになるかもしれないのよ…",
              "maleVariant": "",
              "stringId": 1805695588301824000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それがあたしとどう関係が…？",
              "maleVariant": "それが俺とどう関係が…？",
              "stringId": 1805696411861803008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたは彼を不安にさせ、探りを入れるような質問を繰り返してる。彼が疑いを持ち始めて、私の投資を台無しにされると困るの",
              "maleVariant": "",
              "stringId": 1805696853169692672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今おとなしく立ち去ってくれるなら、彼が約束した倍の額を払うわ",
              "maleVariant": "",
              "stringId": 1805698011200249856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "賢い選択ね",
              "maleVariant": "",
              "stringId": 1805716479576399872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "店には来ないで。ヨシュアには私から伝えとく",
              "maleVariant": "",
              "stringId": 1805716631812857856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何やってんだ",
              "maleVariant": "",
              "stringId": 1805717465288171520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、俺も興味がある。想定外の終幕もありうるしな",
              "maleVariant": "",
              "stringId": 1805724991228502016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまり強盗とテロリストだ！",
              "maleVariant": "",
              "stringId": 1805727303833509888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バスケスって、根っからの警官なの？",
              "maleVariant": "バスケスは、根っからの警官なのか？",
              "stringId": 1814308228943077376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バスケスの弱みを握ってるのね",
              "maleVariant": "バスケスの弱みを握ってるんだな",
              "stringId": 1814308882616967172
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼むから、宗教の話はもうやめにしない？",
              "maleVariant": "頼むから、宗教の話はもうやめにしないか？",
              "stringId": 1814389200602554368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無駄話はもうやめにして。ヨシュアには余計なことを考えてほしくないから",
              "maleVariant": "",
              "stringId": 1814389268030185472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話の邪魔をしないで",
              "maleVariant": "話の邪魔をするな",
              "stringId": 1814459749550809092
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レイチェルはリラックスしたい時どうするの？",
              "maleVariant": "レイチェルはリラックスしたい時どうするんだ？",
              "stringId": 1814460083467739136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブレインダンスについて話して",
              "maleVariant": "ブレインダンスについて話してくれ",
              "stringId": 1814460470333562880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのブレインダンスについて、もっと詳しく教えて",
              "maleVariant": "そのブレインダンスについて、もっと詳しく教えてくれ",
              "stringId": 1814467639758053376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "タイトルは『苦難』で、ターゲットは貧困層よ。これはBD界に革命をもたらす作品なの",
              "maleVariant": "",
              "stringId": 1814469931894857728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "革命？",
              "maleVariant": "",
              "stringId": 1814471525378056192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨシュアの一時出所の期間は限られてる。一緒に撮影できるのは最後のシーンだけ",
              "maleVariant": "",
              "stringId": 1814471652348026880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも終盤までの部分は替え玉とエモバンクの素材で埋められる。それでもヨシュアとして感じられるのよ",
              "maleVariant": "",
              "stringId": 1814472912098844672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなこと可能なの？",
              "maleVariant": "そんなこと可能なのか？",
              "stringId": 1814475496075321344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのね、うちの親会社はEBMよ。技術投資は桁違い。あと数年もすれば、俳優も必要なくなるわ。もうその日が待ちきれない…",
              "maleVariant": "",
              "stringId": 1814475730419474432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最初からヨシュア抜きでも十分撮影できるんじゃないの",
              "maleVariant": "最初からヨシュア抜きでも十分撮影できるんじゃないのか",
              "stringId": 1814475975501045760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "イエス・キリストの苦しみ――それをデジタルで再現することは、まだ難しいんだ",
              "maleVariant": "",
              "stringId": 1814476427009482752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたの役割は？",
              "maleVariant": "あんたの役割は？",
              "stringId": 1814478886046031872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "宗教の話はやめない？",
              "maleVariant": "宗教の話はやめないか？",
              "stringId": 1814479100660178944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたは作品に口出しできるの？",
              "maleVariant": "あんたは作品に口出しできるのか？",
              "stringId": 1814480294375874560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼は脚本家に、キリスト教の『ルカの福音書』を使わせたの。元はヨハネを使うはずだったんだけど",
              "maleVariant": "",
              "stringId": 1814480859398955008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ… それは、大違いよね… うん…",
              "maleVariant": "ああ… それは、大違いだな… うん…",
              "stringId": 1814482992974290944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨハネの話は雑で大げさだ。ルカの方が私の心によく響く",
              "maleVariant": "",
              "stringId": 1814487459488620544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ルカのイエス様は自らの運命を受け入れていた。世界中にその覚悟を理解し――感じてもらいたい",
              "maleVariant": "",
              "stringId": 1814488924072468480
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "役そのものになろうとしているように聞こえる",
              "maleVariant": "",
              "stringId": 1814520740485554180
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたは世界に何を伝えたいの？",
              "maleVariant": "あんたは世界に何を伝えたいんだ？",
              "stringId": 1814520985164472320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まるで役そのものになろうとしているみたいね",
              "maleVariant": "まるで役そのものになろうとしてるみたいだな",
              "stringId": 1814527441590837248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "具体的に何を伝えたいわけ？",
              "maleVariant": "具体的には何を伝えたいんだ？",
              "stringId": 1814527502274027520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "イエスは慈悲深く、人々の罪のために何度でも死ぬということだ",
              "maleVariant": "",
              "stringId": 1814535801912451072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一度限りの機会だから、大切にしたいんだ",
              "maleVariant": "",
              "stringId": 1814544328211062784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨシュアと話してるの。邪魔しないで",
              "maleVariant": "ヨシュアと話してるんだ。邪魔するなよ",
              "stringId": 1814550499475087360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バッジを着けて生まれたようなものよ",
              "maleVariant": "",
              "stringId": 1814569469322223616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よほどの弱みを握ってるのね",
              "maleVariant": "よほどの弱みを握ってるんだな",
              "stringId": 1814569733194276864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スタジオの筆頭株主は企業なの。バスケスはあなたと違って、上流階級に敵を作るのを好まない",
              "maleVariant": "",
              "stringId": 1814570085666807808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨシュア、何か話題はない？",
              "maleVariant": "ヨシュア、何か話題はないか？",
              "stringId": 1814577813101502464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや… 長い間、誰とも話さずにいたからな…",
              "maleVariant": "",
              "stringId": 1814578770895986688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何でもいいの。リラックスしたい時には何するの？",
              "maleVariant": "何でもいい。リラックスしたい時には何をする？",
              "stringId": 1814579499933130752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー… そうだな… 聖書を読むか、BDや映画を見る。『バッドランズの襲撃』のリメイクの話は聞いたか？",
              "maleVariant": "",
              "stringId": 1814579768922234880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リメイクはオリジナルよりずっといい",
              "maleVariant": "",
              "stringId": 1814581589703155716
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オリジナルのほうがいい",
              "maleVariant": "",
              "stringId": 1814581867819065344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どっちも見てない",
              "maleVariant": "",
              "stringId": 1814582038258802688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どっちも見てない。BDはあまり見ないの",
              "maleVariant": "どっちも見てない。BDはあまり見なくて",
              "stringId": 1814582374792978432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オリジナルが最高よ。リメイクの10倍はいい",
              "maleVariant": "オリジナルが最高だ。リメイクの10倍はいい",
              "stringId": 1814582684701712384
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、オリジナルよりずっといい",
              "maleVariant": "ああ、オリジナルよりずっといい",
              "stringId": 1814582843145740288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着いて、レイチェル",
              "maleVariant": "落ち着けよ、レイチェル",
              "stringId": 1814589770340786176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その通りだ。君のことも知りたいから教えてくれ、休みの時やリラックスしたい時は何をする？",
              "maleVariant": "",
              "stringId": 1814591013968699392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたに関係ないでしょ",
              "maleVariant": "",
              "stringId": 1814591990251675648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真面目に聞いてるのにな",
              "maleVariant": "",
              "stringId": 1814592341902123008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "瞑想よ",
              "maleVariant": "",
              "stringId": 1814592905096486912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何ですって！？ ことの重大さをわかってるの？ 明日、彼は世紀の大傑作を生みだすのよ",
              "maleVariant": "",
              "stringId": 1815279374970675200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "集中して、彼の人生で最高のパフォーマンスをしてもらわないといけないの。二度とやり直しはできないんだから",
              "maleVariant": "",
              "stringId": 1815281519451201536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少し休ませてくれ…",
              "maleVariant": "",
              "stringId": 1815282379786838016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょうどよかった。あなたの仕事は終わりよ、もう帰って。心配しなくても金は送るわ",
              "maleVariant": "",
              "stringId": 1815282687967518720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さよなら、V",
              "maleVariant": "",
              "stringId": 1815283879284072448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スピリチュアルなタイプだとは思わなかった",
              "maleVariant": "",
              "stringId": 1815285181229588484
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい趣味ね",
              "maleVariant": "いい趣味だな",
              "stringId": 1815285661980712960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よりによってあんたが… スピリチュアルなタイプだとは思わなかった",
              "maleVariant": "",
              "stringId": 1815285752997109760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心を安らげる手段が何かしらあるのはいいことね",
              "maleVariant": "心を安らげる手段が何かしらあるのはいいことだ",
              "stringId": 1815285818025598976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから理解がない人は嫌なの",
              "maleVariant": "",
              "stringId": 1815290019560579072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真の心の安らぎは、神の恵みによってしか得られない",
              "maleVariant": "",
              "stringId": 1815290694306652160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スピリチュアルな世界は人の世界、宗教とは別のものなのに",
              "maleVariant": "",
              "stringId": 1815291794069938176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "相変わらずね",
              "maleVariant": "相変わらずだな",
              "stringId": 1815308531809148932
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨシュアには同情する",
              "maleVariant": "",
              "stringId": 1815308797979680768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの皮肉癖にはもう飽き飽きよ",
              "maleVariant": "お前の皮肉癖にはもう飽き飽きだ",
              "stringId": 1815309034018332672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼には同情する",
              "maleVariant": "奴には同情するよ",
              "stringId": 1815309108123295744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おかげでヨシュアは改心した",
              "maleVariant": "",
              "stringId": 1815310384450002944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨシュアが改心できたんだから、それでいいでしょ",
              "maleVariant": "ヨシュアが改心できたんだから、それでいいだろ",
              "stringId": 1815311045589753856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちがどう思ったって、何が変わるわけでもない。企業はあいつのような奴を利用して利益を上げ続け、大衆は空しい夢を見続けるんだ",
              "maleVariant": "",
              "stringId": 1815311191853522944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴もイカレてれば、この世界もイカレてる。お前がイカレてるのは、俺のイカレた頭がそん中に入ってるからだ",
              "maleVariant": "",
              "stringId": 1815313645135486976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それが世界を救うための第一歩だな。イカレてなきゃ話は始まらない",
              "maleVariant": "",
              "stringId": 1815316461811949568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたに聞きたいことがあってさ",
              "maleVariant": "",
              "stringId": 1831940653428793344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "乗るなよ、V",
              "maleVariant": "",
              "stringId": 1831947084202795008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大体あなたは何でまだここにいるの？ タダ飯が狙い？",
              "maleVariant": "",
              "stringId": 1831962013794328576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここにいる理由を教える",
              "maleVariant": "",
              "stringId": 1831963854355587076
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ教えてあげる――",
              "maleVariant": "じゃあ教えてやるよ――",
              "stringId": 1831964201274859520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うるさい、あたしに干渉しないで",
              "maleVariant": "うるさいぞ、俺に干渉するな",
              "stringId": 1831964272007602176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしに干渉しないで",
              "maleVariant": "俺に干渉するな",
              "stringId": 1831964358326378496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やめて！ あなたの言うことになんか興味ない！",
              "maleVariant": "",
              "stringId": 1831966377229443072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私の台詞よ！ そもそも部外者はそっちでしょ！",
              "maleVariant": "",
              "stringId": 1831967193793323008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああもう、神の恵みだ、救済だって、そんな話うんざりよ…！ 吐き気がする！",
              "maleVariant": "",
              "stringId": 1831967998713815040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BDプロデューサーの私に言わせれば、宗教なんてインチキよ",
              "maleVariant": "",
              "stringId": 1831968994743574528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だがレイチェル…",
              "maleVariant": "",
              "stringId": 1831969478195830784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく、話はここまで。食べ終わったらスタジオで撮影の準備よ",
              "maleVariant": "",
              "stringId": 1831969741799448576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それと、あなたは消えて。報酬は後で送る",
              "maleVariant": "",
              "stringId": 1831970791147200512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さよなら、V。君と会えてよかった",
              "maleVariant": "",
              "stringId": 1831971070269743104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分をイエスと同じだとでも？",
              "maleVariant": "",
              "stringId": 1846317134728683520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰もが… 彼を手本とすべきだと思う",
              "maleVariant": "",
              "stringId": 1846317976760377344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨシュア？",
              "maleVariant": "",
              "stringId": 1857721637021442048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "同情する気は起きない",
              "maleVariant": "",
              "stringId": 1857796513904222208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼に同情する気なんて一切起きない",
              "maleVariant": "奴に同情する気なんて一切起きない",
              "stringId": 1857797024669786112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、神は存在すると思う？",
              "maleVariant": "なあ、神は存在すると思うか？",
              "stringId": 1857800744681889792
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "神は存在すると思う？",
              "maleVariant": "神は存在すると思うか？",
              "stringId": 1857800808720523268
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "変な一日だった… まるで夢でも見てたみたいな感覚ね。でも… どうやら覚めたらしい",
              "maleVariant": "変な一日だった… まるで夢でも見てたみたいな感覚だ。だが… どうやら覚めたらしい",
              "stringId": 1857801543663247360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう行こう",
              "maleVariant": "",
              "stringId": 1857801615520063488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それに抗ってこそ、反逆者ジョニーでしょ？",
              "maleVariant": "おい、それに抗ってこそ反逆者ジョニーだろ？",
              "stringId": 1857802561201397760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、本物の反逆者ってのはな、ああいう奴を言うんだよ",
              "maleVariant": "",
              "stringId": 1857803411604922368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どっちなのよ。変人？ それとも救世主？",
              "maleVariant": "どっちなんだ。変人か？ それとも救世主か？",
              "stringId": 1857803618568658944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ、日常に戻りましょ",
              "maleVariant": "さて、日常に戻るか",
              "stringId": 1857809392598540288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おいおい、嘘だろ",
              "maleVariant": "",
              "stringId": 1857908716435169280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真面目に聞いてるの",
              "maleVariant": "真面目に聞いてるんだ",
              "stringId": 1857909083134779392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなこと聞いて何になる？ お前は一度死んで生き返った。コンストラクトである俺にも、“神の奇跡”が起きると思ってんのか？",
              "maleVariant": "",
              "stringId": 1857972812480274432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もういい。行きましょ",
              "maleVariant": "もういい。行こう",
              "stringId": 1857995182146347012
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分がデジタルゴーストであるという事実について、頭を巡らせたことくらいあるでしょ？",
              "maleVariant": "自分がデジタルゴーストであるという事実について、頭を巡らせたことくらいあるだろ？",
              "stringId": 1857997612258975744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コンストラクトは魂じゃない。別物よ",
              "maleVariant": "コンストラクトは魂じゃない。別物だ",
              "stringId": 1857997708979625984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一度死んで生き返ったことは否定できない",
              "maleVariant": "",
              "stringId": 1857997804458762240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは死んで生き返った。それは間違いない",
              "maleVariant": "俺は死んで生き返った。それは間違いない",
              "stringId": 1858867846150905856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、あれが奇跡だったとでも？ なら今のお前は救世主か不死鳥、いや、それともレストアされた初期モデルのアーチャーか？",
              "maleVariant": "",
              "stringId": 1858876599596244992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あれが何だったのかはわからない。あんたもそうでしょ",
              "maleVariant": "あれが何だったのかはわからない。それはお前もだろ",
              "stringId": 1858876935660658688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何がお前を生き返らせたのか教えてやるよ。アラサカのくそったれな技術だ。この世に救世主がいるとしたら、それはサブロウ・アラサカだろうな",
              "maleVariant": "",
              "stringId": 1858881851233951744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コンストラクトに命を狙われる救世主ね…",
              "maleVariant": "",
              "stringId": 1858884658196406272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこだ。だから俺とお前は違うんだ。神々の前にひれ伏すのがお前、眉間を撃って足元に崩れさせるのが俺だ",
              "maleVariant": "",
              "stringId": 1858885146212065280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはもう死んでるの？ それとも自分のコピーが残ってる限り生きてるってこと？",
              "maleVariant": "お前はもう死んでるのか？ あるいは自分のコピーが残ってる限り生きてるとも言える",
              "stringId": 1858917176366882816
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "デジタルの天国から迎えがくるってか？",
              "maleVariant": "",
              "stringId": 1858917783333003264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "デジタルの地獄からよ。でも、もし死後の世界があるなら、あんたはもうそこにいて、あたしは動くお墓と話してるってことになる",
              "maleVariant": "デジタルの地獄からだ。だが、もし死後の世界があるなら、お前はもうそこにいて、俺は動く墓と話してるってことになる",
              "stringId": 1858918150032613376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "期待に応えられなくて申し訳ないが、そんなの考えたこともねえ。俺はただ… 俺なんだ。地獄や天国の住人でもない。単なるデジタルの精神だ",
              "maleVariant": "",
              "stringId": 1858919067880546304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺はこの街でいろんな奴を見てきた。こんな野郎に出し抜かれてたまるか",
              "maleVariant": "",
              "stringId": 1877867438723272704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "逃げようなんて考えてみろ、すぐに俺が始末してやる",
              "maleVariant": "",
              "stringId": 1877867713987055616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バスケス、あなたの意見は求めてないの",
              "maleVariant": "",
              "stringId": 1877868019735040000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "偏見に囚われた人間は、大勢いる",
              "maleVariant": "",
              "stringId": 1895648604114673664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": 1966048519104311296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": 1985010349784772608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この話はもうやめにしない？",
              "maleVariant": "この話はもうやめにしないか？",
              "stringId": 2474996895498711040
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}