  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティの皆さん、こんばんは！ 今夜のゲストはマーティンさん。ナイトシティのよき市民で… うーん、マーティン、どう紹介したらいい？",
                    "maleVariant": null,
                    "stringId": 1793535807891349504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "えっと… 両腕のない男です… ご覧のとおり…",
                    "maleVariant": null,
                    "stringId": 1803809170432704512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、痛ましいよ、マーティン。それは事故で？ ノーマッドの襲撃に遭ったとか？",
                    "maleVariant": null,
                    "stringId": 1803809362380832768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いえ… そうじゃないんです。あれは56年、私がニューアメリカンオートワークス社で働いていた頃のことです",
                    "maleVariant": null,
                    "stringId": 1803809541427281920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "NAAだね。続けて",
                    "maleVariant": null,
                    "stringId": 1803809811590791168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "当時、いろいろと問題が起きて、コスト削減が決まったんです。ある日、私たちは休憩室に集められ、お前たちは効率の悪いのろまだと罵られました",
                    "maleVariant": null,
                    "stringId": 1803809953056276480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で、選択を迫ったんです。両腕をゼータテク社のサイバーウェアに変えて仕事を続けるか、退職金なしで… クビに、されるか",
                    "maleVariant": null,
                    "stringId": 1803810190219001856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それで、あなたは？",
                    "maleVariant": null,
                    "stringId": 1803810497745371136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "選択肢なんてありません！ 娘が2人いて、体の弱い妻は働けない… こうするしかなかった",
                    "maleVariant": null,
                    "stringId": 1803810632919400448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しかも腕は無償ではなく、ローンで無理やり買わされたんです。給料から天引き、20年もですよ",
                    "maleVariant": null,
                    "stringId": 1803810901740732416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最初はよかったんです。腕も動いたし… でも何年か経ったころ、NAAが倒産",
                    "maleVariant": null,
                    "stringId": 1803811164539043840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "路頭に迷いました。手当たり次第に仕事に申し込み、飛び込みで工場を訪ね、何でもやる気でしたが… どこにも… 雇ってもらえず…",
                    "maleVariant": null,
                    "stringId": 1803811412959281152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "支払いが滞るようになって… ひと月が過ぎ、ふた月と少し経った頃、ゼータテク社が家まで回収部隊をよこしたんです",
                    "maleVariant": null,
                    "stringId": 1803811743000674304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ひざまずいて、1週間待ってくれと頼みましたが… 無視されました。気絶させて、両腕をもぎ取り… 帰っていったんです。む、娘たちは… その… うう…",
                    "maleVariant": null,
                    "stringId": 1803811927550050304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、まだお辛いんですね… 涙が出るほどに",
                    "maleVariant": null,
                    "stringId": 1803812181640986624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミリテク社の新製品、ドラウプニル5-Xで乾かしてみませんか？",
                    "maleVariant": null,
                    "stringId": 1803812406975774720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "え？ ウソでしょ… そんな… まさか… こんなことが…",
                    "maleVariant": null,
                    "stringId": 1803812631941464064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "夢じゃありませんよ。このジギーQに不可能はないんです",
                    "maleVariant": null,
                    "stringId": 1803812852964507648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジーギー！ ジーギー！ ジーギー！",
                    "maleVariant": null,
                    "stringId": 1803813173107343360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "信じられない… ジギー… 何とお礼を言えば… ありがとう、本当にありがとう…",
                    "maleVariant": null,
                    "stringId": 1803813831193640960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あなたも体験談をシェアしませんか？ 悲劇の事故から感動の再会まで",
                    "maleVariant": null,
                    "stringId": 1803814178398126080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "1-900-ZIGGY-Qに今すぐお電話を！",
                    "maleVariant": null,
                    "stringId": 1803814462268620800
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}