  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしたの？",
              "maleVariant": "どうしたんだ？",
              "stringId": 1783919372995026944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジェイクの様子はどうだ？",
              "maleVariant": "",
              "stringId": 1783921470449016832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あまりよくない。出血は止められたけど、あのインプラントがないと…",
              "maleVariant": "",
              "stringId": 1783922821014904832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしたの？",
              "maleVariant": "どうしたんだ？",
              "stringId": 1783924323917914112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジェイクが撃たれて、ひどい傷でな。替えの腎臓がいる",
              "maleVariant": "",
              "stringId": 1783925023645261824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "問題は… 鎌状赤血球症ってのにかかってて、特殊な腎臓インプラントが必要なんだ",
              "maleVariant": "",
              "stringId": 1783925429268013056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "街にいけばあるはずよ",
              "maleVariant": "街にいけばあるはずだ",
              "stringId": 1783925606502522880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。ダコタが病院にいる知り合いに聞いて… どうにか用意できるとは言われてる…",
              "maleVariant": "",
              "stringId": 1783929174513049600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やったのはラフェン・シヴ？",
              "maleVariant": "やったのはラフェン・シヴか？",
              "stringId": 1783929713363673088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。今回はどうにかしのげたけどな…",
              "maleVariant": "",
              "stringId": 1783929891000836100
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういうインプラントはナイトシティの闇市場で見つかるはず",
              "maleVariant": "そういうインプラントはナイトシティの闇市場で見つかるはずだ",
              "stringId": 1783932844176728064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラフェン・シヴ―― 連中の仕業なの？",
              "maleVariant": "ラフェン・シヴ―― 連中の仕業か？",
              "stringId": 1783933141737431040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも？",
              "maleVariant": "",
              "stringId": 1783935348696309760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "任せておいて",
              "maleVariant": "任せてくれ",
              "stringId": 1783935819347550212
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪いけど、手は貸せない",
              "maleVariant": "悪いが、手は貸せない",
              "stringId": 1783936044195799040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしに任せて",
              "maleVariant": "俺に任せろ",
              "stringId": 1783936675623100420
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダコタの知り合いがリトルチャイナの病院で働いてる。インプラントを受け取るだけでいい。金は安全なルートで送るから",
              "maleVariant": "",
              "stringId": 1783936675623100428
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪いけど、今すぐには手は貸せない。ごめん",
              "maleVariant": "悪いが、今すぐには手は貸せない。すまない",
              "stringId": 1783937304500264964
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。まだ少し時間はあるから、手が空くのを待ってるぞ",
              "maleVariant": "",
              "stringId": 1783937304517042176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジェイクを助ける",
              "maleVariant": "",
              "stringId": 1795066824481464324
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも、街に行くとキャンプの人手が足りなくなるんだ。ラフェン・シヴがいつ戻ってきてもおかしくない… リスクが高すぎる",
              "maleVariant": "",
              "stringId": 1851614608562741248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "任せてくれ。方法は言えないが、大丈夫。約束する",
              "maleVariant": "",
              "stringId": 1853117736755187712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかると思うが、そういうものは、ここバッドランズじゃなかなか手に入らない",
              "maleVariant": "",
              "stringId": 1853122581880950784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "防弾チョッキのすぐ下に、弾の破片が当たったんだ",
              "maleVariant": "",
              "stringId": 1853131946134392832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは… 運が悪かったね",
              "maleVariant": "それは… 運が悪かったな",
              "stringId": 1853133030496194560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ命は救えるけど… 急がないと",
              "maleVariant": "",
              "stringId": 1853133752302358528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ごまかしても仕方ないから、はっきり言う。なあ、V… 俺たちを助けてくれないか？",
              "maleVariant": "",
              "stringId": 1853139556346679296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "助かるよ",
              "maleVariant": "",
              "stringId": 1853142751550689280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。じゃあまた後で",
              "maleVariant": "",
              "stringId": 1853144299869622272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この前の件だけど――",
              "maleVariant": "この前の件だが――",
              "stringId": 1853148528785018880
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}