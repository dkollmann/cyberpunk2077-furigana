    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1815544245858246656,
            "femaleVariant": "ハーイ、V！ 〈Us Cracks〉のブルー・ムーンだけど… ほら、バンドのさ"
          },
          {
            "stringId": 1815555852604710916,
            "femaleVariant": "喜んで力になるよ"
          },
          {
            "stringId": 1815556013766647808,
            "femaleVariant": "悪いけど、今は忙しいのよ",
            "maleVariant": "悪いが、今は忙しいんだ"
          },
          {
            "stringId": 1815556036332003328,
            "femaleVariant": "その脅迫だけど、趣味の悪い冗談か何かってことはないの？",
            "maleVariant": "その脅迫だが、趣味の悪い冗談か何かってことはないのか？"
          },
          {
            "stringId": 1815556255459221504,
            "femaleVariant": "今は都合が悪い"
          },
          {
            "stringId": 1815556453497479168,
            "femaleVariant": "イタズラじゃないの？",
            "maleVariant": "イタズラじゃないのか？"
          },
          {
            "stringId": 1815557931301773312,
            "femaleVariant": "手を貸すよ"
          },
          {
            "stringId": 1815566085548843008,
            "femaleVariant": "この街の誰もが知ってるよ。何か用？",
            "maleVariant": "この街の誰もが知ってるよ。何か用か？"
          },
          {
            "stringId": 1815566474327269376,
            "femaleVariant": "ついにバックダンサーをやってくれなんて仕事まで舞い込んできたかー… って、冗談よ。どうしたの？",
            "maleVariant": "ついにバックダンサーをやってくれなんて仕事まで舞い込んできたかー… って、冗談だよ。どうしたんだ？"
          },
          {
            "stringId": 1815567314731573248,
            "femaleVariant": "V… こないだ届いた手紙には爆弾が入ってた。その時はセキュリティが気づいてくれたけど… どれだけヤバいかわかるでしょ？"
          },
          {
            "stringId": 1815590641722368000,
            "femaleVariant": "バンドの仲間を頼ったら？",
            "maleVariant": "バンドの仲間を頼るのは？"
          },
          {
            "stringId": 1815591556785283072,
            "femaleVariant": "バンドのメンバーを頼ることはできないの？ みんなしっかりしてそうな子たちじゃない",
            "maleVariant": "バンドのメンバーを頼ることはできないのか？ みんなしっかりしてそうな子たちだが"
          },
          {
            "stringId": 1815595293155172352,
            "femaleVariant": "V… そいつはどんな見た目かもわからないのよ！ 現行犯で捕まえるしかない。それも… 人目につかないようにね"
          },
          {
            "stringId": 1815595737852059648,
            "femaleVariant": "その点、私たちは… ほら、ご存知のとおり有名人だから…"
          },
          {
            "stringId": 1815597220538523648,
            "femaleVariant": "そんな、V…"
          },
          {
            "stringId": 1815603107512623104,
            "femaleVariant": "V？ 連絡待ってたんだよ！ また脅迫文が来て――私の顔を剥いで、マスクにしてやるなんて言うのよ！ お願い、助けて！"
          },
          {
            "stringId": 1815611821380509696,
            "femaleVariant": "ああ、よかった。ケリーの言うとおり、いい人だね"
          },
          {
            "stringId": 1815612036195983360,
            "femaleVariant": "で、計画を立てたの。私がカブキ・ラウンドアバウトの近くを散歩するから、あなたは遠くから監視する"
          },
          {
            "stringId": 1815612465222950912,
            "femaleVariant": "何か怪しいことが起きたら… ほら、もしストーカーを見つけて、100パーそいつって確証が取れたら…"
          },
          {
            "stringId": 1815613667142705152,
            "femaleVariant": "やることはわかってる"
          },
          {
            "stringId": 1815614469865381888,
            "femaleVariant": "ちょ、ちょ、ちょっと待って！ 殺しちゃダメよ！ そんなことしたら〈Us Cracks〉のイメージダウンになっちゃうじゃない。ほんの少し懲らしめてやるだけでいいの"
          },
          {
            "stringId": 1816789885342883840,
            "femaleVariant": "実は… ちょっと相談があって"
          },
          {
            "stringId": 1822826494458765312,
            "femaleVariant": "ま、言われたとおりにするよ"
          },
          {
            "stringId": 1864589120446451712,
            "femaleVariant": "私のファンに… その… ちょびっと変な人がいて…"
          },
          {
            "stringId": 1864591949991038976,
            "femaleVariant": "何かあってからじゃ遅いでしょ。だからそいつの正体を突き止めて… なんとかしてほしいの"
          },
          {
            "stringId": 1864600252666138624,
            "femaleVariant": "なるほど。相手は本気みたいね",
            "maleVariant": "なるほど。相手は本気みたいだな"
          },
          {
            "stringId": 1864601869687771136,
            "femaleVariant": "そりゃ、そっちにも都合があるってことはわかるけどさ… もし気が変わったら必ず連絡してくれる？ お願い"
          },
          {
            "stringId": 1864606704378105856,
            "femaleVariant": "殺害予告の手紙を自宅に送ってくるの。何百通も。それに、尾行されたこともある"
          },
          {
            "stringId": 1864613156073295872,
            "femaleVariant": "決まりね！ それじゃあカブキに行って、ベンチで待ってて。私もすぐに行くから"
          },
          {
            "stringId": 1871916360708444160,
            "femaleVariant": "そいつについて、何かわかってることは？"
          },
          {
            "stringId": 1871916752305442816,
            "femaleVariant": "そいつについて、何かわかってることは？"
          },
          {
            "stringId": 1871917117176336384,
            "femaleVariant": "特に何も。手紙の“GC”ってサインぐらい"
          },
          {
            "stringId": 1871917451344924672,
            "femaleVariant": "それだけじゃ難しそうだけど… 一応の手掛かりね",
            "maleVariant": "それだけじゃ難しそうだが… 一応の手掛かりだな"
          },
          {
            "stringId": 1922638020127924228,
            "femaleVariant": "[座る]"
          },
          {
            "stringId": 1924004252427276288,
            "femaleVariant": "いくらで雇えるかわかんないけど… もしVが解決してくれるなら、どんな額でも払うよ。約束する"
          }
        ]
      }
    }
  }
}