    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1815330641850372096,
            "femaleVariant": "常に鬼がいると、安らぎを見いだすのは容易ではない"
          },
          {
            "stringId": 1815384163719548928,
            "femaleVariant": "そなたは薄れていく。この世界に強く根を下ろしてはいない"
          },
          {
            "stringId": 1815388204881121280,
            "femaleVariant": "わかった、やってみる"
          },
          {
            "stringId": 1815389280250335232,
            "femaleVariant": "何だか知らないけど、信用できない",
            "maleVariant": "何だか知らないが、信用できない"
          },
          {
            "stringId": 1815390271582474240,
            "femaleVariant": "何だか知らないけど、信用できない",
            "maleVariant": "何だか知らんが、信用できない"
          },
          {
            "stringId": 1815396007142199296,
            "femaleVariant": "仕方ない。気が変わったら戻ってくるんだな"
          },
          {
            "stringId": 1815396409275289600,
            "femaleVariant": "意識を解放してくれる、瞑想のブレインダンスをお渡ししよう"
          },
          {
            "stringId": 1815406796855021568,
            "femaleVariant": "いくら払うかはそなた次第だ"
          },
          {
            "stringId": 1815413424744534020,
            "femaleVariant": "タダでもらう"
          },
          {
            "stringId": 1815413810318512128,
            "femaleVariant": "[支払う] わかった"
          },
          {
            "stringId": 1815414374620172288,
            "femaleVariant": "それなら、タダで頂く",
            "maleVariant": "それなら、タダで頂くよ"
          },
          {
            "stringId": 1815414465200361472,
            "femaleVariant": "これでどう？",
            "maleVariant": "これでどうだ"
          },
          {
            "stringId": 1815416624193179648,
            "femaleVariant": "いいだろう"
          },
          {
            "stringId": 1815420380343406592,
            "femaleVariant": "瞑想術というものは、経験者の監督下で学ぶのが肝要だ"
          },
          {
            "stringId": 1815428503099641856,
            "femaleVariant": "そなたのことを待っていた。瞑想術を身に付けたいか？"
          },
          {
            "stringId": 1815429374692790272,
            "femaleVariant": "[座る] 気が変わった。やってみる"
          },
          {
            "stringId": 1815429621552746496,
            "femaleVariant": "いや、あんまり"
          },
          {
            "stringId": 1815429839673331712,
            "femaleVariant": "そうね、気が変わった",
            "maleVariant": "そうだな、気が変わった"
          },
          {
            "stringId": 1815429908745129984,
            "femaleVariant": "いや、あんまり"
          },
          {
            "stringId": 1815434311388598272,
            "femaleVariant": "あの… え？"
          },
          {
            "stringId": 1815569402773344256,
            "femaleVariant": "自身を浄化し、平静を取り戻すのだ。私が手伝ってやろう"
          },
          {
            "stringId": 1815569921189318660,
            "femaleVariant": "[座る] 続けて",
            "maleVariant": "[座る] 続けてくれ"
          },
          {
            "stringId": 1815570515639635976,
            "femaleVariant": "この街で、そなたは自然から切り離されている"
          },
          {
            "stringId": 1815570515639635984,
            "femaleVariant": "たしかに禅寺はないね",
            "maleVariant": "たしかに禅寺はないな"
          },
          {
            "stringId": 1815570515639635992,
            "femaleVariant": "静けさを取り戻し、自身との絆を回復するのだ"
          },
          {
            "stringId": 1815678494674227200,
            "femaleVariant": "どうぞ… 続けて",
            "maleVariant": "…続けてくれ"
          },
          {
            "stringId": 1815690273269714948,
            "femaleVariant": "[立つ] 姿を消す手品？",
            "maleVariant": "[立つ] 姿を消す手品か？"
          },
          {
            "stringId": 1815692633941446656,
            "femaleVariant": "なんかの… 手品？",
            "maleVariant": "なんかの… 手品か？"
          },
          {
            "stringId": 1816786727853854720,
            "femaleVariant": "瞑想は、台風の目のような静けさをもたらす"
          },
          {
            "stringId": 1816811542144847872,
            "femaleVariant": "さあ始めるぞ。導いてやろう"
          },
          {
            "stringId": 1821684636852391940,
            "femaleVariant": "まだいたの？",
            "maleVariant": "まだいたのか？"
          },
          {
            "stringId": 1821686137188487168,
            "femaleVariant": "まだいたの？",
            "maleVariant": "まだいたのか？"
          },
          {
            "stringId": 1918523800738877440,
            "femaleVariant": "座ってくれ"
          },
          {
            "stringId": 1918555450520653824,
            "femaleVariant": "何かのドッキリ？ カメラ回ってる？",
            "maleVariant": "何かのドッキリか？ カメラ回ってる？"
          },
          {
            "stringId": 1918556560299937792,
            "femaleVariant": "何かのドッキリ？ どうせカメラが回ってるんでしょ？",
            "maleVariant": "何かのドッキリか？ どうせカメラが回ってるんだろ？"
          },
          {
            "stringId": 1918559158402830336,
            "femaleVariant": "自分自身から離れていく"
          },
          {
            "stringId": 1918564683844702208,
            "femaleVariant": "普段は新しいことに前向きなのに、ここにきて怖くなったのか？"
          },
          {
            "stringId": 1918565578271641600,
            "femaleVariant": "…で、これは何？",
            "maleVariant": "…で、これは何なんだ？"
          },
          {
            "stringId": 1918568326648057856,
            "femaleVariant": "瞑想だ"
          },
          {
            "stringId": 1918568562871259136,
            "femaleVariant": "つまり、座禅みたいな？"
          },
          {
            "stringId": 1918568884826034176,
            "femaleVariant": "そのとおり"
          },
          {
            "stringId": 1918600942344491008,
            "femaleVariant": "金属、コンクリート、ネオン… 汚れた世界に囲まれ、息が詰まっているのだ"
          },
          {
            "stringId": 1918613218417987584,
            "femaleVariant": "そなたは根源。そこから不要なものを抜き取るだけでいい"
          },
          {
            "stringId": 1918621875310448640,
            "femaleVariant": "準備はいいか？"
          },
          {
            "stringId": 1918626568333971456,
            "femaleVariant": "詳しく説明して。これに何の得があるの？",
            "maleVariant": "詳しく説明してくれ。これに何の得がある？"
          },
          {
            "stringId": 1918628113129689088,
            "femaleVariant": "これに、何の得があるっていうの？",
            "maleVariant": "これに、何の得があるっていうんだ？"
          },
          {
            "stringId": 1918635637828505600,
            "femaleVariant": "自己を満たせるのだ。そうすれば、自分の声だけが聞こえるようになる"
          },
          {
            "stringId": 1918645205321142272,
            "femaleVariant": "他のすべてが沈黙と化すのだ"
          },
          {
            "stringId": 1918649382059397120,
            "femaleVariant": "具体的に言ってくれない？",
            "maleVariant": "具体的に言ってくれないか？"
          },
          {
            "stringId": 1918649606437883904,
            "femaleVariant": "自分の人生から自分自身を追い出すことはできまい"
          },
          {
            "stringId": 1918654403815690240,
            "femaleVariant": "やってみるとしましょ",
            "maleVariant": "やってみるとするか"
          },
          {
            "stringId": 1918655061583220736,
            "femaleVariant": "「渡す」？"
          },
          {
            "stringId": 1918668905370112000,
            "femaleVariant": "金儲けでやってるわけじゃないんでしょ？",
            "maleVariant": "金儲けでやってるわけじゃないんだろ？"
          }
        ]
      }
    }
  }
}