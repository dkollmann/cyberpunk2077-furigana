  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫なわけないでしょ。何もかも… 大丈夫じゃない",
              "maleVariant": "",
              "stringId": "1722965937323888640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これだけはわかる。この街が恋しくなることは、絶対にない",
              "maleVariant": "",
              "stringId": "1722977728116412416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんた、やっぱバカだよ。〈神輿〉であれだけのことがあって、それでもこの街に居続けるなんて",
              "maleVariant": "",
              "stringId": "1722978402879262720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もうこんな場所は捨てて… 新たなスタートを切ろうよ――私と一緒に",
              "maleVariant": "",
              "stringId": "1722978827275718656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[コーヒーを飲む]",
              "maleVariant": "",
              "stringId": "1723015118054871044"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしたの… 大丈夫？",
              "maleVariant": "どうしたんだ… 大丈夫か？",
              "stringId": "1794951016205643776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナム…",
              "maleVariant": "",
              "stringId": "1794951922577960960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "外に出てる",
              "maleVariant": "",
              "stringId": "1794952033055928320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティはあたしの街よ。まだここでやり残していることがある",
              "maleVariant": "ナイトシティは俺の街だ。まだここでやり残していることがある",
              "stringId": "1794961661433081860"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一緒に行っても、あたしに残された時間は少ない",
              "maleVariant": "一緒に行っても、俺に残された時間は少ない",
              "stringId": "1794962240851013632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしと一緒に残ってくれないか？",
              "maleVariant": "俺と一緒に残ってくれないか？",
              "stringId": "1794963367927607296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この話はもうしたでしょ。あと1つ、やらなきゃいけない仕事があるの",
              "maleVariant": "この話はもうしただろ。あと1つ、やらなきゃいけない仕事があるんだ",
              "stringId": "1794965985928278016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい加減、気づきなよ！ やらないといけないことなんて、もう何もない。アフターライフのみんなが既にあんたを崇拝してる。あんたはもう全てを手に入れたでしょ！",
              "maleVariant": "",
              "stringId": "1794969146319564800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "違う、すべてじゃない。パナムがいないでしょ。あたしといて。一緒に街に残って",
              "maleVariant": "違う、すべてじゃない。パナムがいないだろ。俺といろ。一緒に街に残れよ",
              "stringId": "1794972464450236416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無理よ…！ クランは私の家族、私の責任なんだ。あんたは私にそれを捨てろって言うわけ？ あんたが街を捨てればいいだけでしょ！",
              "maleVariant": "",
              "stringId": "1794973937338806272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティで、まだやり残したことがある",
              "maleVariant": "",
              "stringId": "1794978955521552388"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一緒に街を出たとしても、あたしに残された時間は限られてる",
              "maleVariant": "一緒に街を出たとしても、俺に残された時間は限られてる",
              "stringId": "1794979335928147968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティはあたしの街よ",
              "maleVariant": "ナイトシティは俺の街だ",
              "stringId": "1794980405022683136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティは癌だよ… 今の自分を鏡で見ればわかるでしょ！",
              "maleVariant": "",
              "stringId": "1794980909027028992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだやり残したことがある。ナイトシティには、新しい伝説が必要なの",
              "maleVariant": "まだやり残したことがある。ナイトシティには、新しい伝説が必要なんだ",
              "stringId": "1794986320786485248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お願いだから素直になって。あんたの状態は理解してるし、〈神輿〉でのこともわかってる、でも…",
              "maleVariant": "",
              "stringId": "1794988501556129792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このままじゃ最期、あんたを看取るのはせいぜいアフターライフの金魚の糞たちだよ…",
              "maleVariant": "",
              "stringId": "1794989126054440960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムには理解できないでしょうね",
              "maleVariant": "パナムには理解できないだろうな",
              "stringId": "1794990721366355968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "できるわけがないでしょ… あんたが何を成し遂げようと、いずれ、別の誰かに塗り替えられるだけ",
              "maleVariant": "",
              "stringId": "1794990851859542016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V… 私は明日、街を出る。そしたら、私たちは終わりなんだよ。わかってる？",
              "maleVariant": "",
              "stringId": "1794992667523411968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムを失いたくはない。でも他に道はない",
              "maleVariant": "パナムを失いたくはない。だが他に道はない",
              "stringId": "1794998937152253956"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは死ぬの。その前に、すべてに決着をつけさせて",
              "maleVariant": "俺は死ぬんだ。その前に、すべてに決着をつけさせてくれ",
              "stringId": "1795000443494592512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これでさよならね",
              "maleVariant": "これでさよならだな",
              "stringId": "1795002538297782272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ… これでさよならね",
              "maleVariant": "じゃあ… これでさよならだな",
              "stringId": "1795006834909245440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一緒に街を出たとしても、あたしには数ヶ月しか残されてない",
              "maleVariant": "一緒に街を出たとしても、俺には数ヶ月しか残されてない",
              "stringId": "1795011927717048320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だからこそ、その数カ月を、大切な人たちと過ごすべきなんだ",
              "maleVariant": "",
              "stringId": "1795017457537773568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさか… それが嫌なの？ 最期は一人でいたほうが楽――そう思ってる？",
              "maleVariant": "",
              "stringId": "1795018060578025472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなわけないでしょ",
              "maleVariant": "そんなわけないだろ",
              "stringId": "1795033148697473024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい、V？ 明日になったら、私はもうここにいない。夜明けと共にキャンプごと移動する",
              "maleVariant": "",
              "stringId": "1795034491713613824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムを失いたくはない。でも、あたしは街に残る… ここまで来て、他の生き方を選ぶことはできない",
              "maleVariant": "パナムを失いたくはない。だが、俺は街に残る… 他の生き方を選ぶことはできない",
              "stringId": "1795049110775656448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これからもスタンリーのラジオを聴き続ければ… あんたの近況が聞けるかな",
              "maleVariant": "",
              "stringId": "1795049493631725568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっとね",
              "maleVariant": "きっとな",
              "stringId": "1795053956404736000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっぱさよならは苦手だ",
              "maleVariant": "",
              "stringId": "1795054769227296768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナム、あたしは死ぬの",
              "maleVariant": "パナム、俺は死ぬんだ",
              "stringId": "1795056855675432960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあね、パナム。また会う日まで",
              "maleVariant": "じゃあな、パナム。また会う日まで",
              "stringId": "1795070467416649728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さよならは必要はない",
              "maleVariant": "",
              "stringId": "1795072256656404480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなこと言われなくたってわかってる！ これまでも――",
              "maleVariant": "",
              "stringId": "1795085855193952256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って、最後まで言わせて",
              "maleVariant": "待て、最後まで言わせろ",
              "stringId": "1795086990189391872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この最後の仕事は、ただの仕事じゃない。人生で一番でかいヤマなの。やり遂げることができれば…",
              "maleVariant": "この最後の仕事は、ただの仕事じゃない。人生で一番でかいヤマなんだ。やり遂げることができれば…",
              "stringId": "1795093889181937664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分が一番なんだって証明できる？ そんなの、あんた以外の全員がもうわかってる。あんたはただ、自分の理想に囚われてるだけだ",
              "maleVariant": "",
              "stringId": "1795095794922680320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "違う。そうじゃない。この街があたしに与え、あたしから奪ったすべてに意味があったって証明できるの",
              "maleVariant": "違う。そうじゃない。この街が俺に与え、俺から奪ったすべてに意味があったって証明できるんだ",
              "stringId": "1795096115434614784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは… わかる気がする",
              "maleVariant": "",
              "stringId": "1795097392935727104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから、あたしは残るのよ",
              "maleVariant": "だから、俺は残るんだ",
              "stringId": "1795098957042020352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソッ",
              "maleVariant": "",
              "stringId": "1795101920485568512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなに力を示したいなら、勝手にすればいい。そして気付けばいいんだ。どれだけ上を目指したって、永遠に満たされないってことに",
              "maleVariant": "",
              "stringId": "1795120936184836096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "約束する",
              "maleVariant": "",
              "stringId": "1795123087040704512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあね、パナム",
              "maleVariant": "じゃあな、パナム",
              "stringId": "1795127135500365824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。またいつか、どこかで会う日まで",
              "maleVariant": "",
              "stringId": "1795127609372831744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのときは、コーカサスの財務副大臣の話を最後まで聞くよ。トランクで運んだ奴",
              "maleVariant": "",
              "stringId": "1795129009230180352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "楽しみにしてて。でも覚悟したほうがいいよ。とにかく長いからね",
              "maleVariant": "",
              "stringId": "1795129848778846208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時間を作ってみせる",
              "maleVariant": "",
              "stringId": "1795136366374940672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "約束だよ…",
              "maleVariant": "",
              "stringId": "1795139860448276480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そろそろアフターライフに行かないと",
              "maleVariant": "",
              "stringId": "1795140381900926976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私も出るよ。じゃあね、V。頑張って",
              "maleVariant": "",
              "stringId": "1795140996081250304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ",
              "maleVariant": "ああ",
              "stringId": "1795141711864393728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「道は分かれても、いずれまた交差する」。あたしがいたクランの言葉",
              "maleVariant": "「道は分かれても、いずれまた交差する」。俺がいたクランの言葉だ",
              "stringId": "1795143495987097600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "せめてひとつだけ約束して。ここまで来たんだから、最後まで自分を貫くって",
              "maleVariant": "",
              "stringId": "1795146695083421696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたのことは… 絶対、忘れないから",
              "maleVariant": "",
              "stringId": "1795150223197728768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ノーマッドは適当だから",
              "maleVariant": "",
              "stringId": "1795153319751708672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういえば… あの話をまだ聞いてないじゃない？ コーカサスの大臣をトランクで運んだやつ",
              "maleVariant": "そういえば… あの話をまだ聞いてないよな？ コーカサスの大臣をトランクで運んだやつ",
              "stringId": "1795155139593105408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつか話すよ。どこかで、私たちの道が交差することがあれば",
              "maleVariant": "",
              "stringId": "1795155885994668032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[シャワーを浴びる] あたしを待てなかったの？",
              "maleVariant": "[シャワーを浴びる] 俺を待てなかったのか？",
              "stringId": "1859378532360908800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おはよう、V",
              "maleVariant": "",
              "stringId": "1873425199360765952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしを待てなかったの？",
              "maleVariant": "俺を待てなかったのか？",
              "stringId": "1873599116665942016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "1917125919582810112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫？",
              "maleVariant": "大丈夫か？",
              "stringId": "1917176520756785156"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ…",
              "maleVariant": "",
              "stringId": "1924234984062545920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[血を洗い流す]",
              "maleVariant": "",
              "stringId": "1924236528975704068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[寄りかかる]",
              "maleVariant": "",
              "stringId": "1924275322110177284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[抱きしめる] またいつか",
              "maleVariant": "",
              "stringId": "1925504323814256644"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[去る] アフターライフに行かないと",
              "maleVariant": "",
              "stringId": "1925505512077357056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[寝返りを打つ]",
              "maleVariant": "",
              "stringId": "1947318753156657156"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クリスタル・パレスを見ない？",
              "maleVariant": "クリスタル・パレスを見ないか？",
              "stringId": "1947354269918949376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クリスタル・パレスを探してみる？",
              "maleVariant": "クリスタル・パレスを探してみるか？",
              "stringId": "1947355240900325376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなのどうでもいい。V… 私…",
              "maleVariant": "",
              "stringId": "1947356557039366144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "起こしちゃ悪いと思って",
              "maleVariant": "",
              "stringId": "2008211146618302464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またいつか",
              "maleVariant": "",
              "stringId": "2008231834955673600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行きなよ。あんたを待ってる人がいるんだろ",
              "maleVariant": "",
              "stringId": "2237255301525073920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無事を祈ってるよ。心から",
              "maleVariant": "",
              "stringId": "2237256195633246208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[アミュレットを取る]",
              "maleVariant": "",
              "stringId": "2405696711920709632"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}