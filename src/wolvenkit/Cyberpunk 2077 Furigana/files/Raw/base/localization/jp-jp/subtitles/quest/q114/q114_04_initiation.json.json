  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、聞いてくれ。ここにいる俺たちはみんな、お前に借りがある。それもバカでかい借りがな",
              "maleVariant": "",
              "stringId": 1325791126497542144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前には感謝してる。必ず〈神輿〉までたどり着かせてやるからよ。なあ、みんな？",
              "maleVariant": "",
              "stringId": 1567935201901481984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこかに行くの？",
              "maleVariant": "どこかに行くのか？",
              "stringId": 1691141523669536768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ジャケットを受け取る] 何て言ったらいいか…",
              "maleVariant": "",
              "stringId": 1691179783473090564
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ジャケットを受け取る] 本当にありがとう",
              "maleVariant": "",
              "stringId": 1691182751228383232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ジャケットを受け取る] みんなの期待に応えてみせる",
              "maleVariant": "",
              "stringId": 1691182898633003008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何て言ったらいいか…",
              "maleVariant": "",
              "stringId": 1691184013965545472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "えっと… どうもありがとう。本当に… 何から何まで",
              "maleVariant": "",
              "stringId": 1691184280253517824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「アルデカルドスの信頼は、コブラの信頼より得られない」だっけ？",
              "maleVariant": "「アルデカルドスの信頼は、コブラの信頼より得られない」だったか？",
              "stringId": 1691184883461541888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今夜の主役はあんただよ。明日のためにも、たっぷりもてはやされときな",
              "maleVariant": "",
              "stringId": 1692113704650035200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[手を差し出す] どこかに行くの？",
              "maleVariant": "[手を差し出す] どこかに行くのか？",
              "stringId": 1696626980134080516
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よーし、みんな聞きな。今日は大いに祝いましょ。でも明日は大変な一日になる",
              "maleVariant": "",
              "stringId": 1696629637980639232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やるべきことをやったら、しっかり休め",
              "maleVariant": "",
              "stringId": 1696629637997416448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前をアルデカルドスに迎え入れる。俺たちは運命共同体だ",
              "maleVariant": "",
              "stringId": 1732786775954051072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんな集まってどうしたの？",
              "maleVariant": "みんな集まってどうしたんだ？",
              "stringId": 1775151057453502464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これは何の集まり？ 何かあったの？",
              "maleVariant": "これは何の集まりだ？ 何かあったのか？",
              "stringId": 1775151511595962368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "明日の襲撃とは関係ない",
              "maleVariant": "",
              "stringId": 1775165096594296832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ま、その通りかもね。V、あんたはバラバラだった私の人生を、さらに滅茶苦茶にしてくれた",
              "maleVariant": "",
              "stringId": 1776121914174787584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもそうやってはじめて、作り直せるものもある",
              "maleVariant": "",
              "stringId": 1776122616620044288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ファミリーへようこそ",
              "maleVariant": "",
              "stringId": 1776133912803463168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、本当に… まさかこうなるとは思わなくて。でもありがとう",
              "maleVariant": "",
              "stringId": 1776135998278520832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話したかったら、私は上にいるよ",
              "maleVariant": "",
              "stringId": 1776181635460157444
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "上がってくれ",
              "maleVariant": "",
              "stringId": 1837166754299432960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、こっちだ",
              "maleVariant": "",
              "stringId": 1837172252042121216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが、それも今日までだ",
              "maleVariant": "",
              "stringId": 1837230938257149952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんで教えてくれなかったのよ…",
              "maleVariant": "なんで教えてくれなかったんだ…",
              "stringId": 1837349649492267008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[跳び下りる] 教えてくれてもよかったのに",
              "maleVariant": "[跳び下りる] 教えてくれてもよかった",
              "stringId": 1837352180301746180
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジャケットは気に入った？ 小さくない？",
              "maleVariant": "",
              "stringId": 1837354992280588288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ぴったりよ",
              "maleVariant": "ぴったりだ",
              "stringId": 1837355876255322112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんだか懐かしい気分",
              "maleVariant": "なんだか懐かしい気分だ",
              "stringId": 1837355907897151488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ぴったりよ。ずっと前から着てたみたい",
              "maleVariant": "ぴったりだ。ずっと前から着てたみたいに",
              "stringId": 1837356054681014276
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっぱり仲間って… いいものね",
              "maleVariant": "やっぱり仲間って… いいもんだな",
              "stringId": 1837356084041142276
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何言ってんの、それじゃブチ壊しでしょ",
              "maleVariant": "",
              "stringId": 1837361497276993536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "着てたんじゃない？",
              "maleVariant": "",
              "stringId": 1837361620774080512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかるよ。初めて会った時、あんたが仲間を求めてるって気づいた。私もひとりだったから",
              "maleVariant": "",
              "stringId": 1837361744506048512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これがどれだけ大きなことかはわかってる… みんなにとっても、あたしにとっても。期待を裏切るようなことはしない",
              "maleVariant": "これがどれだけ大きなことかはわかってる… みんなにとっても、俺にとっても。期待を裏切るようなことはしない",
              "stringId": 1837364600139206656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソウル。あたしを探してたって？",
              "maleVariant": "ソウル。俺を探してたって？",
              "stringId": 1882313565410029568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "上がったらすぐにわかる",
              "maleVariant": "",
              "stringId": 1882321074388594688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは特にな！",
              "maleVariant": "",
              "stringId": 1882324394381537280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とはいえ、お前はよそ者。ナイトシティから来た傭兵――未知の世界の住人だ",
              "maleVariant": "",
              "stringId": 1882324688049926144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムはよく知ってるだろ、ハハハッ",
              "maleVariant": "",
              "stringId": 1882334028865597440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "覚悟はいいか？",
              "maleVariant": "",
              "stringId": 1882360259103289344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろん",
              "maleVariant": "もちろんだ",
              "stringId": 1882360591980032004
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当にいいの？",
              "maleVariant": "本当にいいのか？",
              "stringId": 1882360785236783104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心臓が止まるかと思った…",
              "maleVariant": "",
              "stringId": 1882361040535678976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "望むところよ",
              "maleVariant": "望むところだ",
              "stringId": 1882364114104872960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、そうかしこまるな。お前は随分前から、もう一員みたいなもんだっただろう",
              "maleVariant": "",
              "stringId": 1882367588901187584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「さておき」？",
              "maleVariant": "",
              "stringId": 1882368422527496192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムはさておき、お前は俺とミッチを助けてくれた。理由としては十分だ",
              "maleVariant": "",
              "stringId": 1882371903699152896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よく言ったもんだ",
              "maleVariant": "",
              "stringId": 1882413290087903232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんなの命がより重く感じる",
              "maleVariant": "",
              "stringId": 1882425994450169856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジャケットをもらえたのは… 光栄だし、最高の名誉だと思う。でもそれだけ重みも感じるの",
              "maleVariant": "ジャケットをもらえたのは… 光栄だし、最高の名誉だと思う。でも、それだけ重みも感じるんだ",
              "stringId": 1882427438314156032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クランの信頼を勝ち取ったときから、すでに羽織ってたんだよ",
              "maleVariant": "",
              "stringId": 1882433105892601856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう",
              "maleVariant": "",
              "stringId": 1882434479258726400
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ジャケットを受け取る] 〈神輿〉から戻ったら、一杯おごらせて",
              "maleVariant": "[ジャケットを受け取る] 〈神輿〉から戻ったら、一杯おごらせてくれ",
              "stringId": 1883390439104049152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいジャケットね",
              "maleVariant": "いいジャケットだ",
              "stringId": 1883391718366769152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈神輿〉から戻ったら打ち上げをしましょ。いい場所を知ってるの。もちろんあたしのおごりで",
              "maleVariant": "〈神輿〉から戻ったら打ち上げをしよう。いい場所を知ってるんだ。もちろん俺のおごりで",
              "stringId": 1883392951257264128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "約束だぞ！",
              "maleVariant": "",
              "stringId": 1883393513914757120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ… なんか恥ずかしいな",
              "maleVariant": "",
              "stringId": 1883395458159210496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしを仲間に迎え、信頼してくれたみんなを… 明日は生きて帰れるかわからない戦いに巻き込むのね",
              "maleVariant": "俺を仲間に迎え、信頼してくれたみんなを… 明日は生きて帰れるかわからない戦いに巻き込むわけか",
              "stringId": 1883401013716185088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さっき突然、それに気づいたの",
              "maleVariant": "さっき突然、それに気づいた",
              "stringId": 1883403160662962176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なら、仲間に迎えて正解だったね。責任を感じるのは、ファミリーの一員だからでしょ",
              "maleVariant": "",
              "stringId": 1883403868242046976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティじゃ、人混みにいても孤独を感じる。でもここにいると… 大切な何かの一部になった気がする",
              "maleVariant": "ナイトシティじゃ、人混みにいても孤独を感じる。だがここにいると… 大切な何かの一部になった気がする",
              "stringId": 1883407493613875200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここがあんたの居場所だってことよ",
              "maleVariant": "",
              "stringId": 1883408330260082688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "勘弁してよ、ソウル。心臓が止まるかと思った",
              "maleVariant": "勘弁してくれ、ソウル。心臓が止まるかと思った",
              "stringId": 1888107491861590016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも… ソウル… 本当にいいの？",
              "maleVariant": "でも… ソウル… 本当にいいのか？",
              "stringId": 1888108883598766080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "冗談に聞こえたか？",
              "maleVariant": "",
              "stringId": 1888109434830974976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしも昔クランにいたから、これがどれだけ大変な話かわかってる。アルデカルドスに加わるなんて… そんなこと…",
              "maleVariant": "俺も昔クランにいたから、これがどれだけ大変な話かわかってる。アルデカルドスに加わるなんて… そんなこと…",
              "stringId": 1888109971433451520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいから答えろ… 覚悟はいいのか？",
              "maleVariant": "",
              "stringId": 1888111599880368128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいぞ！ Vはもうよそ者なんかじゃない！",
              "maleVariant": "",
              "stringId": 1913817976938295296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、誰かハンカチ貸してやれ。今にもボロ泣きしそうだぞ！",
              "maleVariant": "",
              "stringId": 1913824100202704896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "黙っててもいいんだよ",
              "maleVariant": "",
              "stringId": 1913827634121928704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほらな？ 気に入っただろ！",
              "maleVariant": "",
              "stringId": 1913829501677727744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫？",
              "maleVariant": "",
              "stringId": 1935957184612003840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ… な、何でもない。心配しないで",
              "maleVariant": "ああ… な、何でもない。心配するな",
              "stringId": 1935957626171551744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、上がれよ",
              "maleVariant": "",
              "stringId": 1976238968963313664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、早くしな",
              "maleVariant": "",
              "stringId": 1976239042917281792
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ようこそ、V。これでお前は家族の一員だ！",
              "maleVariant": "",
              "stringId": 2005205161905614848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムはさておき、お前は俺を助けてくれた。理由としては十分だ",
              "maleVariant": "",
              "stringId": 2025525106862321664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "必ず〈神輿〉までたどり着かせてやるからよ。なあ、みんな？",
              "maleVariant": "",
              "stringId": 2025532973178810392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、V！ 何か言ってよ！",
              "maleVariant": "",
              "stringId": 2237515176171294720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、いつまでそこにいるつもり？",
              "maleVariant": "",
              "stringId": 2237516351700488192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vからも一言！",
              "maleVariant": "",
              "stringId": 2237519389332885504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハハッ、目が点になってるぞ！",
              "maleVariant": "",
              "stringId": 2237519464981352452
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうかしたの？",
              "maleVariant": "",
              "stringId": 2238701918836944896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、体調が悪いのか？",
              "maleVariant": "",
              "stringId": 2238702245841661952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アルデカルドスへようこそ！ 歓迎するよ！",
              "maleVariant": "",
              "stringId": 2238702680606437376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "我らがろくでなしのクランへようこそ！",
              "maleVariant": "",
              "stringId": 2238703352265502720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これからもよろしくな、V！",
              "maleVariant": "",
              "stringId": 2238703473648660480
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新たな仲間に乾杯しよう！",
              "maleVariant": "",
              "stringId": 2238703598320152576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "酒を持ってこい！ みんなで飲もう！",
              "maleVariant": "",
              "stringId": 2238715488735784960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "借りは必ず返す！ それがアルデカルドスだ！",
              "maleVariant": "",
              "stringId": 2238715630234824704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私から借りた金はどうなのよ？ もう1年も待ってるんだけど",
              "maleVariant": "",
              "stringId": 2238719065134919680
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}