  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もしもし、V。死後の世界がホントにあればいいのにね… なんでかわかる？ ねえ？",
                    "maleVariant": null,
                    "stringId": 1790650011999969284
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、今夜はキャンプまで帰れそうにない。砂嵐が近付いてて、ここでやり過ごすことにしたんだ",
                    "maleVariant": null,
                    "stringId": 1790650012033523716
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もしもし、V！ 調子はどう？",
                    "maleVariant": null,
                    "stringId": 1790650012083855364
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もしもし、V！ 元気にしてる？",
                    "maleVariant": null,
                    "stringId": 1790650012117409796
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もしもし、V？ パナムよ、覚えてる？ ほら、車の件の",
                    "maleVariant": null,
                    "stringId": 1790650012150964224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何なのよ！このままずっと私を避け続けるつもり？ この卑怯者！",
                    "maleVariant": null,
                    "stringId": 1790652273199587328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そしたらいつかまた会えるから。その時は、あんたを思いっきりぶん殴ってやる！",
                    "maleVariant": null,
                    "stringId": 1790824550007885824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いったい何考えてたの？ それとも何も考えてなかった？ どうなのよ！",
                    "maleVariant": null,
                    "stringId": 1790825019249840128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたのためを思って、色々やってあげたのに！ このバカ！ その恩を返すどころか、命を絶つなんて",
                    "maleVariant": null,
                    "stringId": 1790825272619356160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最低だよ。地獄に落ちて、せいぜい苦しめばいい。それでも私はあんたを許せない。さよなら",
                    "maleVariant": null,
                    "stringId": 1790825582679085056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一応伝えとこうと思って。その… ほら… 前も一度こっちの都合でドタキャンしたことがあったからさ",
                    "maleVariant": null,
                    "stringId": 1790827757408931840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リーダーがこんなに難しいとはね。見てるのとやるのとじゃ大違い",
                    "maleVariant": null,
                    "stringId": 1790828040071467008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V… あんたがいくれて本当によかった。私がどれだけ救われてるか… 言葉だけじゃ――",
                    "maleVariant": null,
                    "stringId": 1790828210897080320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "砂嵐が来るぞ！ 電気はもってあと30秒だ！",
                    "maleVariant": null,
                    "stringId": 1790828472235773952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "行かなきゃ。埋め合わせは絶対するから。約束する",
                    "maleVariant": null,
                    "stringId": 1790828782429720576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バッドランズは今日も、クーラーボックスが溶けるほどいい天気… 日陰でもクソ暑いし、砂嵐も向かって来てる… あんたもいればよかったのにね",
                    "maleVariant": null,
                    "stringId": 1790830220304240640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "冗談はさておき… こっちはうまくやってるよ。仲間は増え続ける一方で、仕事は山積み――",
                    "maleVariant": null,
                    "stringId": 1790830433173557248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "たまにはゆっくり眠りたいって思うけど… 何もかも思い通りにはならないもんね。そうでしょ、V？",
                    "maleVariant": null,
                    "stringId": 1790830719443193856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "都会に飽きたら連絡して。ながーいドライブに連れてってあげるからさ",
                    "maleVariant": null,
                    "stringId": 1790831326761635840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ… 元気でね、V",
                    "maleVariant": null,
                    "stringId": 1790831735773384704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バッドランズは今日も、クーラーボックスが溶けるほどいい天気… 日陰でもクソ暑いし、砂嵐も向かって来てる… あんたもいればよかったのに！",
                    "maleVariant": null,
                    "stringId": 1790832778309586944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "冗談はさておき… こっちはうまくやってるよ。ソウルとはそれなりに… 仲良くしてるし。仲間は増え続ける一方で、仕事は山積み。でも忙しい方がいいもんね",
                    "maleVariant": null,
                    "stringId": 1790833008107114496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたがいなかったら、私はまだナイトシティでくすぶってた…",
                    "maleVariant": null,
                    "stringId": 1790833304711516160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私のためにしてくれたこと、すべてに感謝してる。あんたのおかげで私は今ここにいるんだ",
                    "maleVariant": null,
                    "stringId": 1790833509292888064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それじゃ… また近いうちにね！ ちゃんと連絡してよ",
                    "maleVariant": null,
                    "stringId": 1790833835559407616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あのさ、仕事を紹介してもらうことってできないかな？",
                    "maleVariant": null,
                    "stringId": 1790835125240778752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "金さえ入るなら、内容は選ばない",
                    "maleVariant": null,
                    "stringId": 1790838277746774016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "仕事でも、依頼人でも、なんでもいいから… もし情報があったら連絡ちょうだい。ほんと、頼むね",
                    "maleVariant": null,
                    "stringId": 1790838577807282176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃ、そんな感じで… よろしく。またそのうちアフターライフで会えるといいね",
                    "maleVariant": null,
                    "stringId": 1790838861006688256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "借り物の人生をせいぜい謳歌してな、ジョニー。どうせ長くは続かない。この私がいる限りね",
                    "maleVariant": null,
                    "stringId": 1790839770197577728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サイコ野郎め！ どこに隠れたって絶対見つけ出してやるから。覚悟しな",
                    "maleVariant": null,
                    "stringId": 1790840190013853696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんであんたの頭からVを引き抜いてやる。方法なんかどうでもいい、やると言ったらやるんだ",
                    "maleVariant": null,
                    "stringId": 1790840468918292480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたなんてただの寄生虫よ、ジョニー。私が絶対に駆除してやる！ 聞こえた！？",
                    "maleVariant": null,
                    "stringId": 1790840687978401792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "少ない残り時間を楽しむんだね",
                    "maleVariant": null,
                    "stringId": 1790841003104849920
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}