    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1104218161305669632,
            "femaleVariant": "無駄だ、聞こえない…"
          },
          {
            "stringId": 1105015127132393472,
            "femaleVariant": "牧師の息子は誰が殺したの？ 裏BDは誰からもらった？",
            "maleVariant": "牧師の息子は誰が殺したんだ？ 裏BDは誰からもらった？"
          },
          {
            "stringId": 1105020462874284052,
            "femaleVariant": "牧師の息子が死ぬBDを回収しにきた"
          },
          {
            "stringId": 1105023953491513344,
            "femaleVariant": "牧師の息子が殺される裏BD… 心当たりがあるでしょ？ どこ？",
            "maleVariant": "牧師の息子が殺される裏BD… 心当たりがあるだろ？ どこだ？"
          },
          {
            "stringId": 1105025314123730944,
            "femaleVariant": "君が来たことは誰にも言わない。約束する！"
          },
          {
            "stringId": 1105033016661037056,
            "femaleVariant": "ど、どの牧師？"
          },
          {
            "stringId": 1105033313634537472,
            "femaleVariant": "少年ものを大量に受け取ったばかりで、誰が親かまでは… もっと情報をくれないか？"
          },
          {
            "stringId": 1105033455418789888,
            "femaleVariant": "…パパ、どれかわかった。BD_9430だ"
          },
          {
            "stringId": 1105034233445404672,
            "femaleVariant": "ブライス・ストーン、N54付きの牧師。息子はボビー・ストーン、死亡当時は12歳だった"
          },
          {
            "stringId": 1105041237647872000,
            "femaleVariant": "わからない。受け渡し場所に置いてあるんだ。俺たちは回収するだけで…"
          },
          {
            "stringId": 1229747213316890624,
            "femaleVariant": "ほら、起きなよ",
            "maleVariant": "ほら、起きろよ"
          },
          {
            "stringId": 1229753900413198336,
            "femaleVariant": "ほら、起きなよ",
            "maleVariant": "ほら、起きろよ"
          },
          {
            "stringId": 1229762424513994752,
            "femaleVariant": "パパ！ どうして… どうして殺したんだ…！"
          },
          {
            "stringId": 1229766769477394432,
            "femaleVariant": "フ、フレディ！ そんな！"
          },
          {
            "stringId": 1238864078437851136,
            "femaleVariant": "そ、それだ… そのBDだよ… 持ってけ！"
          },
          {
            "stringId": 1301017744899350528,
            "femaleVariant": "関係者のアシがつかないように、そうしてるんだ…"
          },
          {
            "stringId": 1509830974825140224,
            "femaleVariant": "そこ、カットだ"
          },
          {
            "stringId": 1509831631904804864,
            "femaleVariant": "え、ここで？ まだ――"
          },
          {
            "stringId": 1509831732853313536,
            "femaleVariant": "当然だろ？ 枕を刺してるみたいになっちまう"
          },
          {
            "stringId": 1509841697563316224,
            "femaleVariant": "じゃあ… 1フレーム動かして… 痛覚フィードバックと鉄の後味を足して…"
          },
          {
            "stringId": 1509843837815984128,
            "femaleVariant": "さすが息子、わかってるな！"
          },
          {
            "stringId": 1509844104020070400,
            "femaleVariant": "ううっ… 汗臭さは減らそう"
          },
          {
            "stringId": 1509844606782902272,
            "femaleVariant": "いや、そのままだ！ そういうのが好きな奴もいる"
          },
          {
            "stringId": 1509845150918348800,
            "femaleVariant": "そりゃいいね、パパ！"
          },
          {
            "stringId": 1538820716891574272,
            "femaleVariant": "俺らが気にしてるのは合成だけだ。誰が映像を撮ったかなんて…"
          },
          {
            "stringId": 1663260809168785408,
            "femaleVariant": "フレディ… フレデリック！ 何とか言ってくれ！"
          },
          {
            "stringId": 1663262625637961728,
            "femaleVariant": "頼む、俺を残して逝くな、一人じゃやっていけない…"
          },
          {
            "stringId": 1663272091276120068,
            "femaleVariant": "牧師の息子が殺される裏BD。どこにあるの？",
            "maleVariant": "牧師の息子が殺される裏BD。どこにある？"
          },
          {
            "stringId": 1663272166756814848,
            "femaleVariant": "牧師の息子が殺されるBDはどこ？",
            "maleVariant": "牧師の息子が殺されるBD。どこにある？"
          },
          {
            "stringId": 1663355943717801984,
            "femaleVariant": "息子を殺したな、殺しやがったな！ このクソ！ 殺した…"
          },
          {
            "stringId": 1663356477971468288,
            "femaleVariant": "どこなの！？",
            "maleVariant": "どこなんだ！？"
          },
          {
            "stringId": 1663361225873264640,
            "femaleVariant": "頼む… 消えてくれ…"
          },
          {
            "stringId": 1663374724066283520,
            "femaleVariant": "何でもするから、殺さないで！"
          },
          {
            "stringId": 1663375234798292996,
            "femaleVariant": "牧師の息子が殺される裏BD。どこにあるの？",
            "maleVariant": "牧師の息子が殺される裏BD。どこにある？"
          },
          {
            "stringId": 1663377444626075648,
            "femaleVariant": "どうして、どうしてだ… どうして！"
          },
          {
            "stringId": 1663378126670237696,
            "femaleVariant": "置いていくな、フレディ…"
          },
          {
            "stringId": 1663378191447068672,
            "femaleVariant": "かわいそうに、こんな目にあって…"
          },
          {
            "stringId": 1663385085238677504,
            "femaleVariant": "牧師の息子が殺されるBD、それを渡して",
            "maleVariant": "牧師の息子が殺されるBD、それを渡せ"
          },
          {
            "stringId": 1663385085255454720,
            "femaleVariant": "ど、どの牧師？ 教えてくれたらすぐあげるよ！ タダでいい！"
          },
          {
            "stringId": 1663385085255454732,
            "femaleVariant": "N54のストーン牧師よ",
            "maleVariant": "N54のストーン牧師だ"
          },
          {
            "stringId": 1663385085272231940,
            "femaleVariant": "僕の後ろの棚にあるBD_9430だ"
          },
          {
            "stringId": 1663385085272231952,
            "femaleVariant": "持ってけよ！ 頼むよ、頼むから殺さないで！"
          },
          {
            "stringId": 1663385085322563584,
            "femaleVariant": "撃たないで！ 僕らが撮ったわけじゃない！"
          },
          {
            "stringId": 1663385085372895232,
            "femaleVariant": "BDの素材は受け取ってるだけで、何も知らないんだ！"
          },
          {
            "stringId": 1663385085406449664,
            "femaleVariant": "受け渡し場所があるんだよ。届いたものを回収するだけ。本当だって！"
          },
          {
            "stringId": 1663422765473304576,
            "femaleVariant": "家族ぐるみでやってるのね",
            "maleVariant": "家族ぐるみでやってるのか"
          },
          {
            "stringId": 1663464030596337664,
            "femaleVariant": "ど、どうなってるんだ！？ 警備員！"
          },
          {
            "stringId": 1663465381900423168,
            "femaleVariant": "は、早まるな。俺たちはチューニングして売ってるだけだ。記録はやってない… 本当だ！"
          },
          {
            "stringId": 1663466323672666112,
            "femaleVariant": "なあ、話し合おう。望みを言ってくれ。何でもするから"
          },
          {
            "stringId": 1663491635257118720,
            "femaleVariant": "家族ぐるみでその仕事をやってるのね",
            "maleVariant": "家族ぐるみでその仕事をやってるのか"
          },
          {
            "stringId": 1663520945691004928,
            "femaleVariant": "買いたい奴がいるから売る。ただの商売だ"
          },
          {
            "stringId": 1663521236943474688,
            "femaleVariant": "何か探し物でもあるの？"
          },
          {
            "stringId": 1663529755340017668,
            "femaleVariant": "BDを渡して",
            "maleVariant": "BDを渡せ"
          },
          {
            "stringId": 1663529985976406016,
            "femaleVariant": "あるなら早くBDを渡して。今すぐよ！",
            "maleVariant": "あるなら早くBDを渡せ。今すぐだ！"
          },
          {
            "stringId": 1663530127190233088,
            "femaleVariant": "牧師の息子を殺したのは誰？",
            "maleVariant": "牧師の息子を殺したのは誰だ？"
          },
          {
            "stringId": 1663577092288516096,
            "femaleVariant": "渡される裏BDは未加工でしょ。背景に何か手掛かりはなかったの？",
            "maleVariant": "渡される裏BDは未加工だろ。背景に何か手掛かりはなかったのか？"
          },
          {
            "stringId": 1663608798374629376,
            "femaleVariant": "わかったから、落ち着いてくれ！"
          },
          {
            "stringId": 1663609085751562240,
            "femaleVariant": "持ってけよ！"
          },
          {
            "stringId": 1663610772063096832,
            "femaleVariant": "俺たちの… 後ろの棚だ"
          },
          {
            "stringId": 1671035068821639168,
            "femaleVariant": "…鏡に映ったもの、タトゥー、何でもいい"
          },
          {
            "stringId": 1671044194838614016,
            "femaleVariant": "そ、その子の肌の色や、身体的特徴とかは？ どんな死に方だった？"
          },
          {
            "stringId": 1671051628319707136,
            "femaleVariant": "BD_9430"
          },
          {
            "stringId": 1682321146501238784,
            "femaleVariant": "2バージョン出せ。汗ありと、汗なしだよ、へへ"
          },
          {
            "stringId": 1682372100231630848,
            "femaleVariant": "クソ！ 助けてくれ！"
          },
          {
            "stringId": 1682373048949641216,
            "femaleVariant": "撃つな！"
          },
          {
            "stringId": 1682507384285925376,
            "femaleVariant": "お、俺たちはチューニングして売ってるだけだ。記録はやってない… 本当だ！"
          },
          {
            "stringId": 1682508103105744896,
            "femaleVariant": "なあ、話し合おう。望みを言ってくれ。何でもするから"
          },
          {
            "stringId": 1682550204623327232,
            "femaleVariant": "俺たちがやらなくたって、他の奴らがやるだけだ"
          },
          {
            "stringId": 1682551416005431296,
            "femaleVariant": "すげえハードコアなの見せてあげようか？"
          },
          {
            "stringId": 1682551530560262144,
            "femaleVariant": "フレディ！"
          },
          {
            "stringId": 1682552070283300864,
            "femaleVariant": "俺たちはナイトシティの需要に応えてるだけだ！"
          },
          {
            "stringId": 1702833915927302144,
            "femaleVariant": "そ、そんな… 大事な息子が… 逝かないでくれ…"
          },
          {
            "stringId": 1702868416007081984,
            "femaleVariant": "もういい。自分で探す"
          },
          {
            "stringId": 1702868988831567872,
            "femaleVariant": "御託はもういい。自分で探す"
          },
          {
            "stringId": 1702872417305989120,
            "femaleVariant": "わかった…"
          },
          {
            "stringId": 1702872754477699072,
            "femaleVariant": "黙って。しゃべったら殺す",
            "maleVariant": "黙ってろ。しゃべったら殺す"
          }
        ]
      }
    }
  }
}