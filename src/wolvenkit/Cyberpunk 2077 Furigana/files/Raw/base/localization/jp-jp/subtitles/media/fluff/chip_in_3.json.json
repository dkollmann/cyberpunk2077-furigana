        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハーイ、みんな元気？ 今日の「チップイン！」のゲストは超スペシャル… NCブラックアウツのセンター、ケヴィン・キング選手！ お会い出来てとても光栄よ",
                    "maleVariant": null,
                    "stringId": 1772012556660940800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アッハッハ、呼び捨てで構わないよ",
                    "maleVariant": null,
                    "stringId": 1790997141020528640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ、ケヴィン、プレイオフ直前の気分はどう？ チームの雰囲気は？",
                    "maleVariant": null,
                    "stringId": 1790997448395902976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最高さ。今シーズンの俺たちはヤバいぜ",
                    "maleVariant": null,
                    "stringId": 1790997843683889152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "楽しみね。あなたは新しいスポンサーのバイオダイン社から、最新型インプラント、キュービット-2の提供を受けたのよね…",
                    "maleVariant": null,
                    "stringId": 1790998063498973184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "期待通りどころか、おかげでノミみたいに軽やかに飛びまわれるよ！",
                    "maleVariant": null,
                    "stringId": 1790998832046460928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "前回の試合では53得点、32アシストをたたき出した… これもインプラントのおかげさ！",
                    "maleVariant": null,
                    "stringId": 1790999287161999360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ、みんな買うべきだと？",
                    "maleVariant": null,
                    "stringId": 1790999645942763520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アッハハハ。もちろんだよ、家族みんなで同じのを使ってる。ばあちゃんもさ！",
                    "maleVariant": null,
                    "stringId": 1790999784052805632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティのみんな、ケヴィン・キング選手お墨付きのバイオダイン製“キュービット-2”は、正規販売店にて明日発売よ！ ではまた次回、明日も楽しい改造を！",
                    "maleVariant": null,
                    "stringId": 1791000442373984256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうだ。あとオールフーズに感謝だな。“良い食事で良い人生を”",
                    "maleVariant": null,
                    "stringId": 1793525088877072384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "装着中のインプラントだけど、性能は期待通り？",
                    "maleVariant": null,
                    "stringId": 1793526547806998528
                  }
                }
              ]
            }
          }
        }
      }
    },
