    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1009718730310311936,
            "femaleVariant": "こいつはキロシ光学――ここで取り扱ってる商品のなかでも最上級の一品だ"
          },
          {
            "stringId": 1009722100668456980,
            "femaleVariant": "[座る]"
          },
          {
            "stringId": 1147127188256489472,
            "femaleVariant": "新しい装備が要るの――実戦向きのヤツ。視力強化系とグリップが欲しい",
            "maleVariant": "新しい装備が要る――実戦向きのヤツ。視力強化系とグリップが欲しい"
          },
          {
            "stringId": 1147663229031034880,
            "femaleVariant": "アップグレードをしたい"
          },
          {
            "stringId": 1151561049559543808,
            "femaleVariant": "そこに座りな。リラックスしろ"
          },
          {
            "stringId": 1163169593719197696,
            "femaleVariant": "なるほどな。確かにデカい話だが、当ててやろう――まだ金は受け取ってないんだろ？"
          },
          {
            "stringId": 1163174058924904448,
            "femaleVariant": "仕事が終わったらちゃんと払う"
          },
          {
            "stringId": 1163174354791108608,
            "femaleVariant": "心配しすぎよ、ヴィク。金なら利子を付けて返すから",
            "maleVariant": "心配しすぎだ、ヴィク。金なら利子を付けて返すから"
          },
          {
            "stringId": 1174774624411668480,
            "femaleVariant": "あのデクスター・デショーンから仕事を受けたのよ？ メジャーリーグにふさわしいテックがなきゃ",
            "maleVariant": "あのデクスター・デショーンから仕事を受けたんだ――メジャーリーグにふさわしいテックがなきゃ"
          },
          {
            "stringId": 1174777272057679872,
            "femaleVariant": "ヴィク… この仕事にはあたしの人生が懸かってる。ここが分かれ道なの。ちゃんとした装備がなきゃ…",
            "maleVariant": "ヴィク… この仕事には俺の人生が懸かってる。ここが分かれ道なんだ。ちゃんとした装備がなきゃ…"
          },
          {
            "stringId": 1174777680029241344,
            "femaleVariant": "車を担保にする"
          },
          {
            "stringId": 1174814694711709696,
            "femaleVariant": "さっきも言った通り、MK1は一級品のスキャナーだ。角膜にデータが表示されるうえ、外部レンズの妨害機能まで内蔵されてる"
          },
          {
            "stringId": 1174814808226353168,
            "femaleVariant": "フム… こいつも… いけるはずだ。キロシとも互換性がある"
          },
          {
            "stringId": 1174821476213411840,
            "femaleVariant": "よし。それじゃさっそく試してみるか。繋ぐぞ"
          },
          {
            "stringId": 1174821619943821328,
            "femaleVariant": "いつでもいいよ。始めて",
            "maleVariant": "いつでもいいぞ。始めてくれ"
          },
          {
            "stringId": 1174830077053194240,
            "femaleVariant": "最高よ",
            "maleVariant": "最高だ"
          },
          {
            "stringId": 1174830429408284672,
            "femaleVariant": "これ… すごいよ、ヴィク",
            "maleVariant": "これ… すごいな、ヴィク"
          },
          {
            "stringId": 1174830470797676544,
            "femaleVariant": "まずは軽く、麻酔を打つぞ"
          },
          {
            "stringId": 1174831259645595648,
            "femaleVariant": "そりゃあよかった。次はスキャナーだ"
          },
          {
            "stringId": 1174832309697990656,
            "femaleVariant": "歯医者に来た気分ね",
            "maleVariant": "歯医者に来た気分だ"
          },
          {
            "stringId": 1174832779208380416,
            "femaleVariant": "痛かったら手をあげればいいの？ 歯医者に来たみたいな気分――いちいち聞かなくていいって…",
            "maleVariant": "痛かったら手をあげればいいのか？ 歯医者に来たみたいな気分だ――いちいち聞かなくていいって…"
          },
          {
            "stringId": 1174833521902178304,
            "femaleVariant": "そんな態度を取っていいのか？ 俺だってもう歳が歳だ。この老いぼれた生身の手が滑るかもしれん"
          },
          {
            "stringId": 1174833797635723264,
            "femaleVariant": "何か感じるか？"
          },
          {
            "stringId": 1174835892422135808,
            "femaleVariant": "武器を構えてみろ。弾数と、新しい照準が見えるはずだ"
          },
          {
            "stringId": 1174836816628633600,
            "femaleVariant": "ありがとう、ヴィク"
          },
          {
            "stringId": 1174837211765624832,
            "femaleVariant": "最高だよ、ヴィクター。なんて言ったらいいのか",
            "maleVariant": "最高だ、ヴィクター。なんて言ったらいいのか"
          },
          {
            "stringId": 1174837565764882432,
            "femaleVariant": "投薬量はしっかり守るようにな。今すぐ2回、1時間後にもう2回だ"
          },
          {
            "stringId": 1174838562415398912,
            "femaleVariant": "[立ち上がる] ありがとう、ヴィク。恩に着るよ"
          },
          {
            "stringId": 1174838871955034112,
            "femaleVariant": "これは？"
          },
          {
            "stringId": 1174839369164607488,
            "femaleVariant": "で、これは？"
          },
          {
            "stringId": 1174839691874357248,
            "femaleVariant": "ありがとう、ヴィク。恩に着るよ"
          },
          {
            "stringId": 1174839959168962560,
            "femaleVariant": "軽い興奮剤だ。神経伝達を短時間ブーストして、インプラントの副作用を抑えてくれる"
          },
          {
            "stringId": 1174840999440240640,
            "femaleVariant": "いいってことよ。応援してるぜ"
          },
          {
            "stringId": 1174905018360016896,
            "femaleVariant": "久しぶりね、ヴィッキー。まだくたばってなかったの？",
            "maleVariant": "久しぶりだな、ヴィッキー。まだくたばってなかったのか？"
          },
          {
            "stringId": 1174906652108533760,
            "femaleVariant": "相変わらず失礼な奴だな、V。それで… 今日は何の用だ？"
          },
          {
            "stringId": 1174912286149324800,
            "femaleVariant": "よし、こっからが本番だ"
          },
          {
            "stringId": 1174912586092392448,
            "femaleVariant": "そのメジャーリーグ級の腕をここに載せな"
          },
          {
            "stringId": 1176795111177637888,
            "femaleVariant": "慣れるまで少々時間がかかるかもしれんが… “一度目の正直”とは言わないだろ。徐々に思考と同期して、お前さんの意図を読み取ってくれるはずだ"
          },
          {
            "stringId": 1176820016082374656,
            "femaleVariant": "ハッ、ずいぶん思い切ったな"
          },
          {
            "stringId": 1185138270639017984,
            "femaleVariant": "ただ、メジャーに行っても、初心を忘れるなよ"
          },
          {
            "stringId": 1193787957690273812,
            "femaleVariant": "[腕を置く]"
          },
          {
            "stringId": 1193789324848828416,
            "femaleVariant": "ん… それでいい"
          },
          {
            "stringId": 1193858959724023808,
            "femaleVariant": "どうだ？ まさに魔法の目だろ"
          },
          {
            "stringId": 1193974764791336960,
            "femaleVariant": "外すぞ。いいな？"
          },
          {
            "stringId": 1193977745397977088,
            "femaleVariant": "最初は少し、違和感があるかもしれない。視界がぼやけたり、多少ちらついたり…"
          },
          {
            "stringId": 1196688739576516608,
            "femaleVariant": "マローンのガードが下がった。疲れが出てきたか、動きに精彩を欠きます。このまま一方的に押されてしまうのでしょうか！"
          },
          {
            "stringId": 1196692848501264384,
            "femaleVariant": "おおっと、パンチをかわされて？ 今のは効いたか！ コーナーに追い詰められたマローン、逃げ場を求め、苦しんでいます！ 息があがりっぱなしのマローン！ 間違いありません。今のはこのラウンド一番の強打でしょう！"
          },
          {
            "stringId": 1196694834453876736,
            "femaleVariant": "マローンはダウン寸前。絶体絶命だ！ このままではマローンは… おおーっと！ ここでラウンド終了！ ゴングに救われました！ 信じられない試合です！"
          },
          {
            "stringId": 1196695414710669312,
            "femaleVariant": "ヘルナンデスの見事な足さばき。コーナーから逃がさない！ ゼータテクのふくらはぎが光り輝いています！"
          },
          {
            "stringId": 1196698551362195456,
            "femaleVariant": "対するヘルナンデス、このチャンスを逃すまいと詰める！ ここでフックを連発！ 一気に優位に立った！"
          },
          {
            "stringId": 1214187510669447168,
            "femaleVariant": "繋いでみろ"
          },
          {
            "stringId": 1215693690470338560,
            "femaleVariant": "これだけ？",
            "maleVariant": "これだけか？"
          },
          {
            "stringId": 1215694098475454464,
            "femaleVariant": "さっき言ったウイルスは？"
          },
          {
            "stringId": 1218472226215727104,
            "femaleVariant": "好きに見てな。その間にお前さんをスキャンさせてもらう"
          },
          {
            "stringId": 1218475025678766080,
            "femaleVariant": "[繋ぐ]"
          },
          {
            "stringId": 1222464861620178944,
            "femaleVariant": "どうだ？ しっくりくるか？"
          },
          {
            "stringId": 1222787449716674560,
            "femaleVariant": "デクスター・デショーンの名前を聞いたことは？"
          },
          {
            "stringId": 1244449902516539392,
            "femaleVariant": "[銃を抜く]"
          },
          {
            "stringId": 1252936675483750400,
            "femaleVariant": "キロシなんて高級品には手が出ない"
          },
          {
            "stringId": 1252941575622004736,
            "femaleVariant": "元気にしてた？",
            "maleVariant": "元気にしてたか？"
          },
          {
            "stringId": 1265975407413596160,
            "femaleVariant": "何も"
          },
          {
            "stringId": 1344252293627924480,
            "femaleVariant": "よし。それじゃさっそく試してみるか。繋ぐぞ"
          },
          {
            "stringId": 1344252293644701704,
            "femaleVariant": "最初は少し、違和感があるかもしれない。視界がぼやけたり、多少ちらついたり…"
          },
          {
            "stringId": 1344252293644701720,
            "femaleVariant": "どうだ？ しっくりくるか？"
          },
          {
            "stringId": 1379332848001757184,
            "femaleVariant": "ん… それでいい"
          },
          {
            "stringId": 1379332848001757200,
            "femaleVariant": "まずは軽く、麻酔を打つぞ"
          },
          {
            "stringId": 1379332848001757216,
            "femaleVariant": "何か感じるか？"
          },
          {
            "stringId": 1379352138713149440,
            "femaleVariant": "恩に着るよ、ヴィク。やっぱ頼りになるね",
            "maleVariant": "恩に着るよ、ヴィク。やっぱ頼りになるな"
          },
          {
            "stringId": 1379352138729926672,
            "femaleVariant": "いいってことよ。応援してるぜ"
          },
          {
            "stringId": 1379352138729926688,
            "femaleVariant": "ただ、メジャーに行っても、初心を忘れるなよ"
          },
          {
            "stringId": 1631285549550465024,
            "femaleVariant": "この間の仕事で――クライアントの神経ソケットに繋いだんだけど… 何か混じってたみたいで",
            "maleVariant": "この間の仕事で――クライアントの神経ソケットに繋いだんだが… 何か混じってたようだ"
          },
          {
            "stringId": 1631286212502155264,
            "femaleVariant": "頭痛や吐き気はあるか？ 光に過敏になったりは？"
          },
          {
            "stringId": 1631286612957523968,
            "femaleVariant": "その全部よ",
            "maleVariant": "その全部だ"
          },
          {
            "stringId": 1631288185737637888,
            "femaleVariant": "わかった。じゃ、さっそく診てみよう。で、それ以外は大丈夫か？"
          },
          {
            "stringId": 1631294115644633088,
            "femaleVariant": "新しいフィクサーから仕事をもらった。名はデクスター・デショーン"
          },
          {
            "stringId": 1631295213931524096,
            "femaleVariant": "アフターライフの有名人か。お前さんが出世したのは結構だ…"
          },
          {
            "stringId": 1631296505978482688,
            "femaleVariant": "なによ… 何か言いたげね、ヴィク",
            "maleVariant": "なんだ… 何か言いたげだな、ヴィク"
          },
          {
            "stringId": 1631297470282526720,
            "femaleVariant": "隙を見せるな、それだけだ。デクスの噂は聞いている。見かけほどいい奴じゃない"
          },
          {
            "stringId": 1631310607782178816,
            "femaleVariant": "フム…"
          },
          {
            "stringId": 1631310853568393216,
            "femaleVariant": "今回限りだ、いいな？"
          },
          {
            "stringId": 1631311699475623936,
            "femaleVariant": "仕方ない… 車を担保にする。58クアドラ・ターボRよ",
            "maleVariant": "仕方ない… 車を担保にする。58クアドラ・ターボRだ"
          },
          {
            "stringId": 1631314189701017600,
            "femaleVariant": "フム… 58だって？ 色は黒か？"
          },
          {
            "stringId": 1631314577858686976,
            "femaleVariant": "闇夜のように、ね",
            "maleVariant": "闇夜のように、な"
          },
          {
            "stringId": 1631314805122854912,
            "femaleVariant": "いや、やめておこう。金は仕事が済んでからでいいさ"
          },
          {
            "stringId": 1631332891968335872,
            "femaleVariant": "いつも通りよ――何も感じない",
            "maleVariant": "いつも通りだ――何も感じない"
          },
          {
            "stringId": 1631333382030815232,
            "femaleVariant": "毎回同じことを聞くけど、ボケてきた？ どうせ答えはいつも一緒なのに",
            "maleVariant": "毎回同じことを聞くが、ボケてきたか？ どうせ答えはいつも一緒だ"
          },
          {
            "stringId": 1631333628236460032,
            "femaleVariant": "そうかい。医者が患者を気にして悪かったな。脳卒中や麻痺になっても同じことが言えるといいが"
          },
          {
            "stringId": 1631334617823768576,
            "femaleVariant": "口の減らない奴め"
          },
          {
            "stringId": 1631336592334319616,
            "femaleVariant": "もっと劇的に変わるヤツを見たことあるけど…",
            "maleVariant": "もっと劇的に変わるヤツを見たことあるが…"
          },
          {
            "stringId": 1631336592334319632,
            "femaleVariant": "そう急くな。しばらく様子を見ろ… 次はスキャナーだ"
          },
          {
            "stringId": 1631340820847063040,
            "femaleVariant": "さっき言った神経ウイルスは？ 確認してくれる？",
            "maleVariant": "さっき言った神経ウイルスは？ 確認してくれるか？"
          },
          {
            "stringId": 1631359882700451840,
            "femaleVariant": "とっくに治療済だ。インプラントを付けてる間に回路をフルスキャンして、駆除しておいた。完全にクリーンだ、保証する"
          },
          {
            "stringId": 1631362225118572544,
            "femaleVariant": "あらゆる監視カメラに顔がハッキリと映らなくなる。だが気をつけろ、体は普通に映っちまうからな"
          },
          {
            "stringId": 1782279395901108224,
            "femaleVariant": "久々だけど… 元気にしてた、ヴィクター？",
            "maleVariant": "久々だが… 元気にしてたか、ヴィクター？"
          },
          {
            "stringId": 1782283050683396096,
            "femaleVariant": "正直言って、大して変わり映えのない毎日さ。だがそれでいい"
          },
          {
            "stringId": 1782283673403322368,
            "femaleVariant": "俺も昔は、駆け引きと化かし合いの世界に生きてたもんだが…"
          },
          {
            "stringId": 1782286508014628864,
            "femaleVariant": "だが？"
          },
          {
            "stringId": 1782286793260855296,
            "femaleVariant": "ある日そういう生き方に別れを告げた。この街の伝説にはなれないだろうが、夜はよく眠れるようになったよ"
          },
          {
            "stringId": 1782294805472014336,
            "femaleVariant": "キロシ？ それって最高級品でしょ",
            "maleVariant": "キロシ？ それって最高級品だろ"
          },
          {
            "stringId": 1782295591518138368,
            "femaleVariant": "ああ、だからこそ役に立つはずだ"
          },
          {
            "stringId": 1782296264519380992,
            "femaleVariant": "確かに金は払うって言ったけど… いくらなんでもそれは高すぎる",
            "maleVariant": "確かに金は払うって言ったが… いくらなんでもそれは高すぎる"
          },
          {
            "stringId": 1782296587162021888,
            "femaleVariant": "金はあとでいい。今はお前さんが無事に帰ってこれるよう、最善を尽くすだけだ――そうすりゃ金も入る"
          },
          {
            "stringId": 1993925586454507520,
            "femaleVariant": "NCPDのデータと連動するスキャナーも入れといた"
          },
          {
            "stringId": 1993925981423726592,
            "femaleVariant": "ろくでなしに遭遇した時に、そいつの“ろくでなさ”がわかるってわけだ"
          },
          {
            "stringId": 2018698482083098624,
            "femaleVariant": "まだだけど、代金は払う。大事な仕事には投資しないと",
            "maleVariant": "まだだが、代金は払うよ。大事な仕事には投資しないとな"
          },
          {
            "stringId": 2018700263504670720,
            "femaleVariant": "金ならある"
          },
          {
            "stringId": 2044575130807132160,
            "femaleVariant": "[繋ぐ]"
          },
          {
            "stringId": 2105509637584019460,
            "femaleVariant": "[開ける]久しぶり、ヴィクター",
            "maleVariant": "[開ける]久しぶりだな、ヴィクター"
          }
        ]
      }
    }
  }
}