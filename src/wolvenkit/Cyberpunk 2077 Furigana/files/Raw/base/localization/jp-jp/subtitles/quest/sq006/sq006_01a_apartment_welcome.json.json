    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1238744339447402496,
            "femaleVariant": "マイク、これは私の公約の要なんだ。ナイト・コープのために方針を変えるなど――"
          },
          {
            "stringId": 1238746235826458624,
            "femaleVariant": "もう切るぞ。必ず修正してくれ。後で話し合おう"
          },
          {
            "stringId": 1277483288074252288,
            "femaleVariant": "また会えて嬉しいわ"
          },
          {
            "stringId": 1277513150881198080,
            "femaleVariant": "彼は今、討論会に備えてて――"
          },
          {
            "stringId": 1277521782775607296,
            "femaleVariant": "でももう終わる頃よ。あと来客の予定はすべて延期したから、誰にも邪魔されずに済むわ"
          },
          {
            "stringId": 1283718895691755520,
            "femaleVariant": "いや。媚を売っても、あとで痛い目を見るだけだ"
          },
          {
            "stringId": 1331697761733169152,
            "femaleVariant": "V？ 上がってきて"
          },
          {
            "stringId": 1337115269749403648,
            "femaleVariant": "どうも"
          },
          {
            "stringId": 1640233941611974656,
            "femaleVariant": "何かあったの？",
            "maleVariant": "何かあったのか？"
          },
          {
            "stringId": 1640237850065768448,
            "femaleVariant": "来て。ジェフが説明するから"
          },
          {
            "stringId": 1641291227889537072,
            "femaleVariant": "V、来てくれてよかった"
          },
          {
            "stringId": 1641291227889537088,
            "femaleVariant": "座ってくれ。君を呼んだ理由を説明しよう"
          },
          {
            "stringId": 1641297073994338304,
            "femaleVariant": "警察には連絡した？",
            "maleVariant": "警察には連絡したのか？"
          },
          {
            "stringId": 1641297073994338336,
            "femaleVariant": "どうしてあたしが？",
            "maleVariant": "なぜ俺が？"
          },
          {
            "stringId": 1641297074027892736,
            "femaleVariant": "どうしてあたしが？ 刑事に頼んだ方がいいんじゃない？",
            "maleVariant": "どうして俺が？ 刑事に頼んだ方がいいんじゃないか？"
          },
          {
            "stringId": 1641297074027892752,
            "femaleVariant": "信頼のおける知り合いに頼みたい"
          },
          {
            "stringId": 1641297074027892768,
            "femaleVariant": "君は以前、前市長の謎の死を解明してくれた"
          },
          {
            "stringId": 1641297074061447168,
            "femaleVariant": "警察には知らせたの？",
            "maleVariant": "警察には知らせたのか？"
          },
          {
            "stringId": 1641297074061447216,
            "femaleVariant": "いいえ、知らせてない。騒ぎにしたくないから"
          },
          {
            "stringId": 1641297074078224396,
            "femaleVariant": "NCPDは… 金の匂いを嗅ぎ分けるしか能のない連中だ"
          },
          {
            "stringId": 1641297074078224412,
            "femaleVariant": "それに通報したら、メディアがイナゴの大群みたいに集まってくるわ"
          },
          {
            "stringId": 1641297074078224444,
            "femaleVariant": "警察はナシね。了解",
            "maleVariant": "警察はナシだな。了解"
          },
          {
            "stringId": 1641297074111778816,
            "femaleVariant": "ん… 普通じゃないって？"
          },
          {
            "stringId": 1641297074111778832,
            "femaleVariant": "数日前の深夜、私は物音に気づいて目が覚めたんだ。それでピストルを持って寝室を出た"
          },
          {
            "stringId": 1641297074111778848,
            "femaleVariant": "すると男がいたんだ。銃を抜こうとしてるようだったから、私も引き金を引いた"
          },
          {
            "stringId": 1641297074111778864,
            "femaleVariant": "仕留めたの？",
            "maleVariant": "仕留めたのか？"
          },
          {
            "stringId": 1641297074128556044,
            "femaleVariant": "わからない。その場で気を失ってしまったんだ。翌朝、奴の痕跡はなかった"
          },
          {
            "stringId": 1641393064684687360,
            "femaleVariant": "SSIは何て言ってたの？",
            "maleVariant": "SSIは何て言ってたんだ？"
          },
          {
            "stringId": 1641393064684687376,
            "femaleVariant": "侵入者の特徴を教えて",
            "maleVariant": "侵入者の特徴を教えてくれ"
          },
          {
            "stringId": 1641393064684687392,
            "femaleVariant": "部屋からなくなった物はない？",
            "maleVariant": "部屋からなくなった物はないか？"
          },
          {
            "stringId": 1641393064718241792,
            "femaleVariant": "侵入者の見た目は？"
          },
          {
            "stringId": 1641393064718241808,
            "femaleVariant": "暗かったし、あまり覚えてないんだ。マスクか何かを着けてたと思う…"
          },
          {
            "stringId": 1641393064718241824,
            "femaleVariant": "いや、インプラントかもしれない… とにかくよく見えなくて"
          },
          {
            "stringId": 1641393064751796224,
            "femaleVariant": "警備会社の連中は何て言ってるの？",
            "maleVariant": "警備会社の連中は何て言ってるんだ？"
          },
          {
            "stringId": 1641393064751796240,
            "femaleVariant": "ここからが少し妙なんだ"
          },
          {
            "stringId": 1641393064751796272,
            "femaleVariant": "侵入された形跡も、発砲の形跡もないって"
          },
          {
            "stringId": 1641393064768573444,
            "femaleVariant": "監視カメラには何も残っておらず、外の警備員も異常はなかったと…"
          },
          {
            "stringId": 1641393064768573460,
            "femaleVariant": "ジェフが悪い夢でも見たんだろうって言うのよ"
          },
          {
            "stringId": 1641393064768573476,
            "femaleVariant": "うさんくさいね…",
            "maleVariant": "うさんくさいな…"
          },
          {
            "stringId": 1641393064768573492,
            "femaleVariant": "今日は君が来るから、妻が警備に休みを取らせたんだ"
          },
          {
            "stringId": 1641393064802127872,
            "femaleVariant": "盗まれたり… 壊されたりしたものはあるの？",
            "maleVariant": "盗まれたり… 壊されたりしたものはあるか？"
          },
          {
            "stringId": 1641393064802127888,
            "femaleVariant": "何もなくなっては…"
          },
          {
            "stringId": 1641393064802127904,
            "femaleVariant": "壊されたものもないと思う。きっとその前に、ジェフと鉢合わせしたんだわ"
          },
          {
            "stringId": 1641396362414264320,
            "femaleVariant": "エリザベスは見てないの？",
            "maleVariant": "エリザベスは見てないのか？"
          },
          {
            "stringId": 1641396784998780928,
            "femaleVariant": "私はぐっすり寝てたから。選挙のことで疲れがたまってて…"
          },
          {
            "stringId": 1641420711707938816,
            "femaleVariant": "SSIか、その裏にいる誰かが関与してるかも",
            "maleVariant": "SSIか、その裏にいる誰かが関与してるかもしれない"
          },
          {
            "stringId": 1641420711724716044,
            "femaleVariant": "それはないと思う"
          },
          {
            "stringId": 1641420711741493248,
            "femaleVariant": "そのSSIって警備会社について教えてくれる？",
            "maleVariant": "そのSSIって警備会社について教えてくれ"
          },
          {
            "stringId": 1641420711758270468,
            "femaleVariant": "警備事業を始めて15年… 街中に顧客がいる"
          },
          {
            "stringId": 1641420711758270484,
            "femaleVariant": "確か、誰かに薦められたんだ… 知り合いに"
          },
          {
            "stringId": 1641420711791824912,
            "femaleVariant": "ウェルドン・ホルト… 私と同じく市長の座を狙うライバル候補だ"
          },
          {
            "stringId": 1641420711791824960,
            "femaleVariant": "世論調査の数字も下がってるし…"
          },
          {
            "stringId": 1641432513992855552,
            "femaleVariant": "部屋の中を調べさせて",
            "maleVariant": "部屋の中を調べさせてくれ"
          },
          {
            "stringId": 1641432514009632780,
            "femaleVariant": "他に知っておくべきことは？"
          },
          {
            "stringId": 1641432514110296064,
            "femaleVariant": "部屋の中を調べさせて",
            "maleVariant": "部屋の中を調べさせてくれ"
          },
          {
            "stringId": 1641432514110296080,
            "femaleVariant": "もちろんだ。リズが案内する。私は会議があるんで失礼するが、何かわかったら連絡してくれ"
          },
          {
            "stringId": 1746210663926206464,
            "femaleVariant": "邪魔って、あたしは何しに来たの？",
            "maleVariant": "邪魔って、俺は何しに来たんだ？"
          },
          {
            "stringId": 1746210828309368832,
            "femaleVariant": "すぐにわかる"
          },
          {
            "stringId": 1751394353388064768,
            "femaleVariant": "それもそうね",
            "maleVariant": "それもそうか"
          },
          {
            "stringId": 1753339715338244096,
            "femaleVariant": "ジェファーソン"
          },
          {
            "stringId": 1775015730065559556,
            "femaleVariant": "[座る]"
          },
          {
            "stringId": 1804096183083339776,
            "femaleVariant": "もちろん、SSIには連絡した。うちの警備会社だ"
          },
          {
            "stringId": 1804163387577470976,
            "femaleVariant": "気を失ったのよね？",
            "maleVariant": "気を失ったんだよな？"
          },
          {
            "stringId": 1804164040127287296,
            "femaleVariant": "意識が戻ったのはいつ？",
            "maleVariant": "意識が戻ったのはいつだ？"
          },
          {
            "stringId": 1804172386842578944,
            "femaleVariant": "ああ。発砲してすぐに"
          },
          {
            "stringId": 1804172582683021312,
            "femaleVariant": "その後は？"
          },
          {
            "stringId": 1804172677021306880,
            "femaleVariant": "何も。翌朝ベッドで目覚めた。ひどい頭痛がしたが"
          },
          {
            "stringId": 1804173129100169216,
            "femaleVariant": "どうやってベッドに戻ったかわかる？",
            "maleVariant": "どうやってベッドに戻ったかわかるか？"
          },
          {
            "stringId": 1804173808107655168,
            "femaleVariant": "いや… まったく。エリザベスも知らないんだ"
          },
          {
            "stringId": 1804180490690891776,
            "femaleVariant": "まさかSSIが関与してるとでも？"
          },
          {
            "stringId": 1804182189853773824,
            "femaleVariant": "まだわからない"
          },
          {
            "stringId": 1804194013814853632,
            "femaleVariant": "それはないと思う。連中がよっぽどバカじゃない限りはね",
            "maleVariant": "それはないと思う。連中がよっぽどバカじゃない限りはな"
          },
          {
            "stringId": 1804197284080128000,
            "femaleVariant": "それか、SSIの裏にいる誰かね。思い当たる相手は？",
            "maleVariant": "それか、SSIの裏にいる誰かだ。思い当たる相手は？"
          },
          {
            "stringId": 1804198693819256832,
            "femaleVariant": "まだわからない。SSI以外に思い当たる人はいる？",
            "maleVariant": "まだわからない。SSI以外に思い当たる人はいるか？"
          },
          {
            "stringId": 1804212874140323840,
            "femaleVariant": "他に知っておいたほうがいいこと… 知っておくべきことはない？",
            "maleVariant": "他に知っておいたほうがいいこと… 知っておくべきことはないか？"
          },
          {
            "stringId": 1804213790729965568,
            "femaleVariant": "いや… ないな。君はどうだ？"
          },
          {
            "stringId": 1804214096058519552,
            "femaleVariant": "全部話したと思う。調べてもきっと何も出てこないわ…"
          },
          {
            "stringId": 1804214451081187328,
            "femaleVariant": "それじゃあ、あとは頼んでいいかな…？"
          },
          {
            "stringId": 1804217966780354560,
            "femaleVariant": "そう言うな。きっと何か見つけてくれる"
          },
          {
            "stringId": 1812709282360926212,
            "femaleVariant": "どうしてあたしが？ 刑事に頼んだ方がいいんじゃない？",
            "maleVariant": "どうして俺が？ 刑事に頼んだ方がいいんじゃないか？"
          },
          {
            "stringId": 1812709282360926228,
            "femaleVariant": "信頼のおける知り合いに頼みたい"
          },
          {
            "stringId": 1812709282360926244,
            "femaleVariant": "んー、気持ちはわかるけど…",
            "maleVariant": "あー、気持ちはわかるが…"
          },
          {
            "stringId": 1812709282377703428,
            "femaleVariant": "ライン市長の死は… 結局、解明されなかったが…"
          },
          {
            "stringId": 1812712386733088768,
            "femaleVariant": "今回も大いに期待しているぞ"
          },
          {
            "stringId": 1812717570406293504,
            "femaleVariant": "君の丁寧な仕事ぶりは気に入った。期待しているぞ"
          },
          {
            "stringId": 1812722187596800004,
            "femaleVariant": "ウェルドン・ホルト… 前市長も殺したしな"
          },
          {
            "stringId": 1812722192277643264,
            "femaleVariant": "調査の支持率も夫より低いの"
          },
          {
            "stringId": 1812722314415775748,
            "femaleVariant": "ライン市長の殺害に関与した連中かもしれない"
          },
          {
            "stringId": 1812722318945624064,
            "femaleVariant": "もしくはウェルドン・ホルト… 選挙の相手候補よ。支持率も夫より低いし"
          },
          {
            "stringId": 1825411027300388864,
            "femaleVariant": "何者かが家に侵入した。だが普通の泥棒じゃない"
          },
          {
            "stringId": 1825414978955513856,
            "femaleVariant": "奴の正体と目的を突き止めてくれ"
          },
          {
            "stringId": 1825490033223798784,
            "femaleVariant": "他に思い当たる人は？"
          },
          {
            "stringId": 1873600946220699648,
            "femaleVariant": "正直、思い当たる人物はいくらでもいる"
          },
          {
            "stringId": 1873601540754903040,
            "femaleVariant": "ジェフは敵が多いの。有力者もたくさんいるし、名前を挙げていったらきりがない"
          },
          {
            "stringId": 1873601893730750464,
            "femaleVariant": "なるほど"
          },
          {
            "stringId": 1873602064740913152,
            "femaleVariant": "だから君を呼んだんだ、V。何が起きてるか、調べてほしい"
          },
          {
            "stringId": 1928756579315879936,
            "femaleVariant": "どうしてそんなに小さな警備会社を雇ってるの？",
            "maleVariant": "どうしてそんなに小さな警備会社を雇ってるんだ？"
          },
          {
            "stringId": 1928785381936992256,
            "femaleVariant": "意外ね。アラサカとかミリテクみたいに、大手と契約してるのかと",
            "maleVariant": "意外だな。アラサカとかミリテクみたいに、大手と契約してるのかと"
          },
          {
            "stringId": 1928844311975301120,
            "femaleVariant": "知人に紹介されたんだ"
          },
          {
            "stringId": 1928844428224630784,
            "femaleVariant": "少なくとも、巨大企業に依存してないのはいいことよ"
          },
          {
            "stringId": 1969274872581459968,
            "femaleVariant": "どうしてあたしに連絡を？",
            "maleVariant": "どうして俺に連絡を？"
          },
          {
            "stringId": 1969284992363270144,
            "femaleVariant": "どうしてあたしに連絡を？",
            "maleVariant": "どうして俺に連絡を？"
          },
          {
            "stringId": 1969290333138325504,
            "femaleVariant": "どうしてあたしが？",
            "maleVariant": "なぜ俺が？"
          },
          {
            "stringId": 2014153079424544772,
            "femaleVariant": "[立ち上がる]"
          }
        ]
      }
    }
  }
}