        "entries": [
          {
            "stringId": 1744588215316762628,
            "femaleVariant": "クリスティーン・マルコフ？"
          },
          {
            "stringId": 1744588762522439680,
            "femaleVariant": "クリスティーン・マルコフ？ 話がしたいの",
            "maleVariant": "クリスティーン・マルコフか？ 話がしたい"
          },
          {
            "stringId": 1744665516658249728,
            "femaleVariant": "警察？"
          },
          {
            "stringId": 1744732707126628356,
            "femaleVariant": "誰と話せばいいんだっけ？"
          },
          {
            "stringId": 1744733579692855296,
            "femaleVariant": "で、誰を探してるんだっけ"
          },
          {
            "stringId": 1744734824394510336,
            "femaleVariant": "サイバーサイコの上司？"
          },
          {
            "stringId": 1744744262887133184,
            "femaleVariant": "想定の範囲内よ",
            "maleVariant": "想定の範囲内だ"
          },
          {
            "stringId": 1744745883801079812,
            "femaleVariant": "想定の範囲内ね",
            "maleVariant": "想定の範囲内だ"
          },
          {
            "stringId": 1744748578171637760,
            "femaleVariant": "次はレッド・クイーンズ・レースに行くぞ"
          },
          {
            "stringId": 1744749090447151104,
            "femaleVariant": "レッド・クイーンズ・レースについて情報屋に話を聞く"
          },
          {
            "stringId": 1811063474131423232,
            "femaleVariant": "おい、あれ何に見える？"
          },
          {
            "stringId": 1811066098423549952,
            "femaleVariant": "ポリ公の車"
          },
          {
            "stringId": 1811088891043237888,
            "femaleVariant": "クソ… ここは俺に任せろ、V"
          },
          {
            "stringId": 1811090640588099584,
            "femaleVariant": "よう、デコッパチ。そいつは新しい女か？",
            "maleVariant": "よう、デコッパチ。そいつは新しいチューマか？"
          },
          {
            "stringId": 1811095688919502852,
            "femaleVariant": "仲間なら選挙の後で釈放される"
          },
          {
            "stringId": 1811096888339451904,
            "femaleVariant": "やめときなさい",
            "maleVariant": "やめときな"
          },
          {
            "stringId": 1811097127699992576,
            "femaleVariant": "失せな",
            "maleVariant": "失せろ"
          },
          {
            "stringId": 1811098106583437312,
            "femaleVariant": "取引を断りやがって。借りは返してもらうぜ"
          },
          {
            "stringId": 1811098355590877184,
            "femaleVariant": "やれやれ、ムショ送りにした片割れの方が、お前よりはまともだったぞ。子供に手を出したところを除けばだが"
          },
          {
            "stringId": 1811099579488784384,
            "femaleVariant": "今回は見逃してやるよ、ウォード。だが、次はないからな"
          },
          {
            "stringId": 1811100419876311040,
            "femaleVariant": "失せな、子猫ども",
            "maleVariant": "失せろ、子猫どもが"
          },
          {
            "stringId": 1811135569184018432,
            "femaleVariant": "ったく… 俺に任せろって言っただろ"
          },
          {
            "stringId": 1811136143652671492,
            "femaleVariant": "クロウズと揉めてるの？",
            "maleVariant": "クロウズと揉めてるのか？"
          },
          {
            "stringId": 1811140219408863232,
            "femaleVariant": "クソ… 報告しないとだな"
          },
          {
            "stringId": 1811140806208770052,
            "femaleVariant": "どうなるかと思ったが――"
          },
          {
            "stringId": 1811140806208770060,
            "femaleVariant": "穏便に済んだな"
          },
          {
            "stringId": 1811143891975000064,
            "femaleVariant": "俺は先に後片付けをしていく"
          },
          {
            "stringId": 1811145238581465088,
            "femaleVariant": "乗ってくか？"
          },
          {
            "stringId": 1811145919484776452,
            "femaleVariant": "向こうで会いましょ",
            "maleVariant": "向こうで会おう"
          },
          {
            "stringId": 1811146365943271424,
            "femaleVariant": "そうね、一緒に行く",
            "maleVariant": "そうだな、一緒に行く"
          },
          {
            "stringId": 1811149093062955008,
            "femaleVariant": "向こうで会いましょ",
            "maleVariant": "向こうで会おう"
          },
          {
            "stringId": 1811149093079732224,
            "femaleVariant": "そうね、一緒に行く",
            "maleVariant": "そうだな、一緒に行く"
          },
          {
            "stringId": 1937321188395802624,
            "femaleVariant": "今回はやり方を変えたい。質問が先、脅しはその後だ"
          },
          {
            "stringId": 1937360861713358848,
            "femaleVariant": "刑事の仕事の9割は話をすることだ"
          },
          {
            "stringId": 1937361335132839936,
            "femaleVariant": "クリスティーン・マルコフ、42歳。ホルヴァースの雇い主らしい"
          },
          {
            "stringId": 1937374034780708864,
            "femaleVariant": "そういう意味で言えば、お前はいい刑事になれるかもな"
          },
          {
            "stringId": 1937401682189836288,
            "femaleVariant": "唯一の知り合いでもある"
          },
          {
            "stringId": 1937409892640579584,
            "femaleVariant": "ええ。こっちはウォード刑事",
            "maleVariant": "ああ。こっちはウォード刑事だ"
          },
          {
            "stringId": 1937409983187214340,
            "femaleVariant": "そうよ",
            "maleVariant": "そうだ"
          },
          {
            "stringId": 1937410604481077248,
            "femaleVariant": "違う"
          },
          {
            "stringId": 1937410700413198336,
            "femaleVariant": "ペテル・ホルヴァースのことで来た"
          },
          {
            "stringId": 1937412459923726336,
            "femaleVariant": "警察じゃない。情報を探してるだけよ",
            "maleVariant": "警察じゃない。情報を探してるだけだ"
          },
          {
            "stringId": 1937412486767271936,
            "femaleVariant": "ペテル・ホルヴァースについて調べてるの",
            "maleVariant": "ペテル・ホルヴァースについて調べてるんだ"
          },
          {
            "stringId": 1937442760112693248,
            "femaleVariant": "そうよ。でもある日から働かなくなって、演説ばかりし始めた"
          },
          {
            "stringId": 1937444417147985920,
            "femaleVariant": "詮索屋は嫌われるわよ。まあ警察じゃないならいいけど"
          },
          {
            "stringId": 1937448722634596352,
            "femaleVariant": "それでも、余計なことを言うと次の日には企業の用心棒やギャングがやってくるのよね…"
          },
          {
            "stringId": 1937449705813008384,
            "femaleVariant": "ペテル・ホルヴァースはここで働いてたんだろ？"
          },
          {
            "stringId": 1937450279627350016,
            "femaleVariant": "ここで働いてたんでしょ？",
            "maleVariant": "ここで働いてたんだろ？"
          },
          {
            "stringId": 1937453723285151744,
            "femaleVariant": "何の用？"
          },
          {
            "stringId": 1937453863374905344,
            "femaleVariant": "ペテル・ホルヴァースはここで働いてたのよね？",
            "maleVariant": "ペテル・ホルヴァースはここで働いてたんだよな？"
          },
          {
            "stringId": 1937459353165193216,
            "femaleVariant": "演説？ 何て言ってたの？",
            "maleVariant": "演説？ 何て言ってたんだ？"
          },
          {
            "stringId": 1937497985120952320,
            "femaleVariant": "ここの給料はいいの？",
            "maleVariant": "ここの給料はいいのか？"
          },
          {
            "stringId": 1937502943543255040,
            "femaleVariant": "変な言動はいつ始まったの？",
            "maleVariant": "変な言動はいつ始まったんだ？"
          },
          {
            "stringId": 1937503575272546304,
            "femaleVariant": "演説って… 具体的に何て？",
            "maleVariant": "演説って… 何を言ってたんだ？"
          },
          {
            "stringId": 1937503592888623104,
            "femaleVariant": "ここの給料はいいの？ この市場を丸ごと買えるほどのクロームを着けてたけど",
            "maleVariant": "ここの給料はいいのか？ この市場を丸ごと買えるほどのクロームを着けてたが"
          },
          {
            "stringId": 1937503611595218944,
            "femaleVariant": "変な言動はいつ始まったの？",
            "maleVariant": "変な言動はいつ始まったんだ？"
          },
          {
            "stringId": 1937515546973790208,
            "femaleVariant": "「ラインは企業の操り人形だ。でも俺は人形にしてやられた」って"
          },
          {
            "stringId": 1937539367751606272,
            "femaleVariant": "何か因縁があったの？",
            "maleVariant": "何か因縁があったのか？"
          },
          {
            "stringId": 1937540273771601920,
            "femaleVariant": "ハッ、そんなものあるわけないでしょ。どうせテレビに映ってたラインが、自分の悪口を言ってるとでも思ったんじゃない？"
          },
          {
            "stringId": 1937562256353746944,
            "femaleVariant": "大体想像はつくでしょ。ホルヴァースの頭の中じゃ、ライン市長は自分を脅かす敵だったのよ。ただそれだけ"
          },
          {
            "stringId": 1937569588886331392,
            "femaleVariant": "ペテルの“変”っていうのは普通の人の“変”とは違うのよ。そもそも普通じゃなかったんだから"
          },
          {
            "stringId": 1937575003682463744,
            "femaleVariant": "刑務所から出てきた後は、さらに悪化してた気がする。入る前も相当だったけど"
          },
          {
            "stringId": 1937583112731713536,
            "femaleVariant": "あっはっは！ 給料がいいかって？ そうね、ここにいる人たちは億万長者だらけよ"
          },
          {
            "stringId": 1937584116881645568,
            "femaleVariant": "じゃあ、あいつはどうやってインプラントを手に入れたんだ"
          },
          {
            "stringId": 1937585511974264832,
            "femaleVariant": "私だって知りたいわよ。あれだけ買えるお金があれば、こんなゴミ溜めとはおさらばできるからね"
          },
          {
            "stringId": 1937587166258094080,
            "femaleVariant": "ペテルはある日あの姿で現れたのよ。ひどく興奮してた。どこで手に入れたか聞いたら、やっと誰かに価値を見いだされたって"
          },
          {
            "stringId": 1937596270213029888,
            "femaleVariant": "それからいつもの演説を始めてね"
          },
          {
            "stringId": 1937625083303124992,
            "femaleVariant": "「やっと誰かに価値を見いだされた」… 出資者がいたのか"
          },
          {
            "stringId": 1937625122998018048,
            "femaleVariant": "ホルヴァースの動機は個人的な恨みか…"
          },
          {
            "stringId": 1937651484483870720,
            "femaleVariant": "収穫はほぼなしね",
            "maleVariant": "収穫はほぼなしだな"
          },
          {
            "stringId": 1937658941419511808,
            "femaleVariant": "収穫はほぼなしね",
            "maleVariant": "収穫はほぼなしだな"
          },
          {
            "stringId": 1937670811366940672,
            "femaleVariant": "確かにそう感じるかもしれないが――"
          },
          {
            "stringId": 1937675302728327168,
            "femaleVariant": "情報ってのは状況次第で価値が変わるもんだ"
          },
          {
            "stringId": 1937676553218121728,
            "femaleVariant": "例えば誰かが、頭が痛いと言ったとしよう"
          },
          {
            "stringId": 1937677185752719360,
            "femaleVariant": "別に大した情報じゃないだろ？ 確かに、普通ならそうだ"
          },
          {
            "stringId": 1937677724871778304,
            "femaleVariant": "でもある日、そいつが車通りの多い道にフラフラと飛び出した。さらに頭痛は慢性で、部屋のエアコンがいじられていたことが判明する"
          },
          {
            "stringId": 1937681726925660160,
            "femaleVariant": "事故か？ 偶然か？ あるいは、エアコンから毒を散布した故意の殺人か？"
          },
          {
            "stringId": 1937684583045357568,
            "femaleVariant": "そういう事件があったの？",
            "maleVariant": "そういう事件があったのか？"
          },
          {
            "stringId": 1937684827455840256,
            "femaleVariant": "いや、作り話だ。でも言いたいことはわかるだろ"
          },
          {
            "stringId": 1937690233779478528,
            "femaleVariant": "ブー。“クソ”ポリ公の車だ"
          },
          {
            "stringId": 1937690628346044416,
            "femaleVariant": "誰かがキレイにしてやるべきじゃないか？"
          },
          {
            "stringId": 1937691433937625088,
            "femaleVariant": "へへへへ。俺の小便できれいになると思うか？"
          },
          {
            "stringId": 1937691873802674176,
            "femaleVariant": "いや、でもオイラのなら話は別だ"
          },
          {
            "stringId": 1937706508517859328,
            "femaleVariant": "まあまあ、ちょっと落ち着いて",
            "maleVariant": "まあまあ、ちょっと落ち着けよ"
          },
          {
            "stringId": 1937709929996181504,
            "femaleVariant": "オイラより自分の心配をしたらどうだ？ 女の前で恥をかくのは嫌だろ？",
            "maleVariant": "オイラより自分の心配をしたらどうだ？ チューマの前で恥をかくのは嫌だろ？"
          },
          {
            "stringId": 1937712580309770240,
            "femaleVariant": "最後の警告だ。さっさと消えろ。さもないと、ライン市長殺害の共犯者として逮捕する"
          },
          {
            "stringId": 1937724615797989376,
            "femaleVariant": "その汚ねえ車を洗っとけよ"
          },
          {
            "stringId": 1937727828148314112,
            "femaleVariant": "V…"
          },
          {
            "stringId": 1937727977197101056,
            "femaleVariant": "何だと、コラ！"
          },
          {
            "stringId": 1937729107176153088,
            "femaleVariant": "失せな！",
            "maleVariant": "失せろ！"
          },
          {
            "stringId": 1937736373086412800,
            "femaleVariant": "もうすぐ選挙でしょ。警察も検挙率を上げたかったのよ",
            "maleVariant": "もうすぐ選挙だろ。警察も検挙率を上げたかったんだよ"
          },
          {
            "stringId": 1937740945515307008,
            "femaleVariant": "へへへ… ウォード、こいつは何者なんだ？"
          },
          {
            "stringId": 1937742357083152384,
            "femaleVariant": "なかなかいじめ甲斐のありそうな奴じゃねえか"
          },
          {
            "stringId": 1937743369051267072,
            "femaleVariant": "たっぷり可愛がってやるよ！"
          },
          {
            "stringId": 1937747718963945472,
            "femaleVariant": "どけ。お友達が刑務所にぶち込まれたばかりだろう。お前もそうなりたいのか？"
          },
          {
            "stringId": 1937761048277508096,
            "femaleVariant": "大丈夫よ、きっとそのうち釈放される",
            "maleVariant": "大丈夫だ、きっとそのうち釈放される"
          },
          {
            "stringId": 1937768765243219968,
            "femaleVariant": "あんたと歩いてると、いつ絡まれてもおかしくない。で？ 揉めてるのはクロウズとだけ？",
            "maleVariant": "あんたと歩いてると、いつ絡まれてもおかしくないな。で？ 揉めてるのはクロウズとだけか？"
          },
          {
            "stringId": 1937769746928459776,
            "femaleVariant": "警官なんだ、しょうがないだろ"
          },
          {
            "stringId": 1937775623618125824,
            "femaleVariant": "向こうで会おう"
          },
          {
            "stringId": 1937779026691842048,
            "femaleVariant": "お前もまだ続けるんだろ？"
          },
          {
            "stringId": 1937779318061752320,
            "femaleVariant": "当然よ",
            "maleVariant": "当然だ"
          },
          {
            "stringId": 1937787438804721664,
            "femaleVariant": "遠慮しとく"
          },
          {
            "stringId": 1937787468433285120,
            "femaleVariant": "ええ、乗ってく",
            "maleVariant": "ああ、乗ってく"
          },
          {
            "stringId": 1937789269198987264,
            "femaleVariant": "そうかい"
          },
          {
            "stringId": 1938585888324542464,
            "femaleVariant": "誰と話せばいいんだっけ？"
          },
          {
            "stringId": 1938609131311816704,
            "femaleVariant": "残りの1割は何なの？",
            "maleVariant": "残りの1割は何なんだ？"
          },
          {
            "stringId": 1938609901369253888,
            "femaleVariant": "それで？ 残りの1割は何なの？",
            "maleVariant": "それで？ 残りの1割は何なんだ？"
          },
          {
            "stringId": 1938613325330604032,
            "femaleVariant": "書類整理だ"
          },
          {
            "stringId": 1956219911244730372,
            "femaleVariant": "クリスティーン・マルコフを知ってる？",
            "maleVariant": "クリスティーン・マルコフを知ってるか？"
          },
          {
            "stringId": 1956219948456595460,
            "femaleVariant": "クリスティーン・マルコフ？"
          },
          {
            "stringId": 1956233640611008512,
            "femaleVariant": "クリスティーン・マルコフを知ってる？ ここで働いてる",
            "maleVariant": "クリスティーン・マルコフを知ってるか？ ここで働いてる"
          },
          {
            "stringId": 1956234733730516992,
            "femaleVariant": "クリスティーン・マルコフ？",
            "maleVariant": "クリスティーン・マルコフか？"
          },
          {
            "stringId": 1956235936103256064,
            "femaleVariant": "クリスティーン・マルコフを知らないか？"
          },
          {
            "stringId": 1956236547079131136,
            "femaleVariant": "さあね。何も買わないなら帰ってくれ"
          },
          {
            "stringId": 1956236835949236224,
            "femaleVariant": "クリスティーン・マルコフか？"
          },
          {
            "stringId": 1956237429023821824,
            "femaleVariant": "いいえ。彼女ならあそこよ。テックを売ってる店"
          },
          {
            "stringId": 1956238563784380416,
            "femaleVariant": "ありがとう"
          },
          {
            "stringId": 1956240430132219904,
            "femaleVariant": "見つけたぞ。行こう"
          },
          {
            "stringId": 1956348768920653824,
            "femaleVariant": "探そうにも手掛かりがない。片っ端から聞いて回るぞ"
          },
          {
            "stringId": 1957947749709668356,
            "femaleVariant": "他にも何か接点があったはずよ",
            "maleVariant": "他にも何か接点があったはずだ"
          },
          {
            "stringId": 1957947833847406592,
            "femaleVariant": "2人に他のつながりは？ よく考えて",
            "maleVariant": "2人に他のつながりは？ よく考えろ"
          },
          {
            "stringId": 1957947897919594496,
            "femaleVariant": "ホルヴァースが敵対視してた人物は他にもいたの？",
            "maleVariant": "ホルヴァースが敵対視してた人物は他にもいたのか？"
          },
          {
            "stringId": 1957947976386633728,
            "femaleVariant": "ライン以外にも、ホルヴァースが目の敵にしてた人物はいる？",
            "maleVariant": "ライン以外にも、ホルヴァースが目の敵にしてた人物はいるか？"
          },
          {
            "stringId": 1957949936804323328,
            "femaleVariant": "ホルヴァースはとにかく、楽に金を稼ぐ方法に執着してた"
          },
          {
            "stringId": 1957951571878240256,
            "femaleVariant": "ちょくちょく怪しい仕事もしてたから… そこでラインを個人的に知る人物と知り合ったとか？"
          },
          {
            "stringId": 1957953783937392640,
            "femaleVariant": "もういい、刑事さん？ 客が逃げちゃうんだけど"
          },
          {
            "stringId": 1957958927563382784,
            "femaleVariant": "大統領ね… 彼女のことも嫌いだった"
          },
          {
            "stringId": 1957962153419366400,
            "femaleVariant": "あとはアラサカのじいさん。死んだあとはその息子"
          },
          {
            "stringId": 2009798588408487936,
            "femaleVariant": "俺は左から始める。お前は右だ"
          },
          {
            "stringId": 2009807193425797120,
            "femaleVariant": "違うけど、クリスティーンなら知ってるよ。反対側の店でテックを売ってる"
          },
          {
            "stringId": 2009815491168395264,
            "femaleVariant": "さあ。知らねえな。ここは社交クラブじゃないんだ"
          },
          {
            "stringId": 2116990769994256388,
            "femaleVariant": "[降りる]"
          },
          {
            "stringId": 2120203120941555716,
            "femaleVariant": "[座って待つ]"
          }
        ]
      }
    }
  }
}