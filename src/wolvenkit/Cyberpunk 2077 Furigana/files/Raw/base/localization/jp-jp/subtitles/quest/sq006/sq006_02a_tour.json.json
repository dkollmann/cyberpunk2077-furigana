  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "来て。家を案内する",
              "maleVariant": "",
              "stringId": 1245779754901688320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも",
              "maleVariant": "",
              "stringId": 1245787038511910912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "できれば自由に見て回りたい",
              "maleVariant": "",
              "stringId": 1245787294079242240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあお願い",
              "maleVariant": "じゃあ頼む",
              "stringId": 1245789103837532160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "できれば自由に見て回りたいんだけど、いい？",
              "maleVariant": "できれば自由に見て回りたいんだが、いいか？",
              "stringId": 1245789820778303504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直言うと、時々こういうことに嫌気が差すの",
              "maleVariant": "",
              "stringId": 1245799986210918416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここがこんなに静かなのは、いつぶりかしら",
              "maleVariant": "",
              "stringId": 1245801561608278032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここが寝室よ",
              "maleVariant": "",
              "stringId": 1245813035982508032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジェフの書斎よ。指令室と倉庫とガラクタ市でもあるわ",
              "maleVariant": "",
              "stringId": 1245905794995179520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここは私がリラックスしにくる場所だったのよ。と言っても選挙前までね",
              "maleVariant": "",
              "stringId": 1245908273023537152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最近ジェフは、誰も暗殺どころか脅迫すらしてこないってことは、自分はきっと二流なんだって愚痴を言ってたの。… 冗談でね",
              "maleVariant": "",
              "stringId": 1284747087055548416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "政治に？",
              "maleVariant": "",
              "stringId": 1284823731233484800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、もちろん。念のため、後ろについていくわ",
              "maleVariant": "",
              "stringId": 1284945671294152704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジェフが不審者を見つけた場所から始めましょう",
              "maleVariant": "",
              "stringId": 1284949203485323264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "侵入前と何か変わったことは？",
              "maleVariant": "",
              "stringId": 1284970726019428352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "事件前と後で、何か変わったことは？",
              "maleVariant": "",
              "stringId": 1284978637835726848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいえ… ないと思う。何か気になってるの？",
              "maleVariant": "",
              "stringId": 1284979925638049792
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見てもいい？",
              "maleVariant": "見てもいいか？",
              "stringId": 1284990024213458944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見て回ってもいい？",
              "maleVariant": "見て回ってもいいか？",
              "stringId": 1284990776419938304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、どうぞ",
              "maleVariant": "",
              "stringId": 1285001556703965184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スタッフ代も選挙運動費も自費でまかなってるの",
              "maleVariant": "",
              "stringId": 1285077403796369408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "警備用の部屋は必要だから",
              "maleVariant": "",
              "stringId": 1285088219446767616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつもは騒がしいの？",
              "maleVariant": "いつもは騒がしいのか？",
              "stringId": 1290591644376756224
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「ペラレスに一票を」",
              "maleVariant": "",
              "stringId": 1290595394352967680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジェフは気絶したのよね",
              "maleVariant": "ジェフは気絶したんだよな",
              "stringId": 1641661081616257024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ベッドのカバーがずれてたとか、モノの配置が変わってたとか、そういうのは？",
              "maleVariant": "",
              "stringId": 1641662775863414784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ベッドまで引きずられた形跡ってこと？ 特に何もなかったわ…",
              "maleVariant": "",
              "stringId": 1641663934732832768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイト・コープやバイオテクニカ、アラサカから来る“オファー”の数々を、今のところはなんとか断れてるけど…",
              "maleVariant": "",
              "stringId": 1661710968139419648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしジェフが誰にも借りを作ることなく当選できたら――それこそが本当の勝利よ",
              "maleVariant": "",
              "stringId": 1661712690337419264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "脅迫や暗殺は、政界ではよくあるの？",
              "maleVariant": "脅迫や暗殺は、政界ではよくあるのか？",
              "stringId": 1661722937475076096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よくありすぎるくらいよ",
              "maleVariant": "",
              "stringId": 1661724355485372416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "二流で居続けたほうが、安全みたいね",
              "maleVariant": "二流で居続けたほうが、安全みたいだな",
              "stringId": 1661725958363811840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "選挙関連の物は全部ここに保管してるわ。そのほうが安上がりだから",
              "maleVariant": "",
              "stringId": 1661732654654033920
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジェフはここで侵入者を見たの",
              "maleVariant": "",
              "stringId": 1685235478926856192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "銃を撃ったのも、ここ？",
              "maleVariant": "銃を撃ったのも、ここか？",
              "stringId": 1685360719049134080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "寝室を出てすぐ。少なくとも彼の記憶では…",
              "maleVariant": "",
              "stringId": 1685362548688424960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「今のところ」ね",
              "maleVariant": "「今のところ」か",
              "stringId": 1751526507132620800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "選挙本部を兼ねてるから。大声で叫ぶ人たち、常に鳴りっぱなしの電話――今でも耳鳴りがするくらいよ…",
              "maleVariant": "",
              "stringId": 1751543334143397888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "想像つかない…",
              "maleVariant": "想像つかないな…",
              "stringId": 1751574754396082176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "選挙に勝つにはある程度の犠牲は仕方ないわ。この部屋だけじゃない。この先も色々とね",
              "maleVariant": "",
              "stringId": 1751576451445035008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "必要なら屋上に出てもいいわよ。私はここにいる",
              "maleVariant": "",
              "stringId": 1793950479366606848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": 1804262490139398144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「ペラレスに一票を」？",
              "maleVariant": "",
              "stringId": 2279328312034185220
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}