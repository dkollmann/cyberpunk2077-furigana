    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ノックする] バリー！",
              "maleVariant": "",
              "stringId": "1772111680413790212"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バリー？ バリーよね？",
              "maleVariant": "バリー？ バリーだよな？",
              "stringId": "1772112117208608768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰だ？",
              "maleVariant": "",
              "stringId": "1772112601784938496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "覚えてるよ。何の用だ？",
              "maleVariant": "",
              "stringId": "1772113695424540672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気をしっかりもって",
              "maleVariant": "気をしっかりもて",
              "stringId": "1772115389705252868"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしたちみたいな人間は死に慣れないと",
              "maleVariant": "俺たちみたいな人間は死に慣れないと",
              "stringId": "1772116722118193152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話がしたいだけ",
              "maleVariant": "話がしたいだけだ",
              "stringId": "1772117316618842112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただ話がしたいだけ。わかってる、話し相手が必要なんでしょ",
              "maleVariant": "ただ話がしたいだけだ。わかってる、話し相手が必要なんだろ",
              "stringId": "1772118135397314560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時計の針は戻せないけど、隣に座って話を聞くことはできる… 辛いのはあんた一人じゃない",
              "maleVariant": "時計の針は戻せないが、隣に座って話を聞くことはできる… 辛いのはあんた一人じゃない",
              "stringId": "1772118743839830016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "友達から様子を見るよう頼まれた",
              "maleVariant": "",
              "stringId": "1772118999457492996"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "出てきて。少しは楽しまなきゃ！",
              "maleVariant": "出てこいよ。少しは楽しまないと！",
              "stringId": "1772120695365595136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "様子を見に来ただけ。友達が、あんたが塞ぎ込んでるって。なんとなく理由はわかる",
              "maleVariant": "様子を見に来ただけだ。友達が、あんたが塞ぎ込んでるって。なんとなく理由はわかる",
              "stringId": "1772121400058998784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "入れ",
              "maleVariant": "",
              "stringId": "1772121758504218624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バリー、ナイトシティで弱みを見せたらつけこまれるよ",
              "maleVariant": "バリー、ナイトシティで弱みを見せたらつけこまれるぞ",
              "stringId": "1772122036754345984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、ちょっと待て。大した知り合いでもないのに、どうして俺の問題について話さなきゃならないんだ",
              "maleVariant": "",
              "stringId": "1772219995496017920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なかなか鋭いじゃない。あんたの友達から様子を見てほしいって頼まれたの",
              "maleVariant": "なかなか鋭いな。あんたの友達から様子を見てほしいって頼まれた",
              "stringId": "1772222129339789312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、入ってくれ。なら話をしようじゃないか",
              "maleVariant": "",
              "stringId": "1772226181909868544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ペトロワとメンデスにはもう構うなと伝えろ。特にメンデスだ",
              "maleVariant": "",
              "stringId": "1772230192000036864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしも身近な人を亡くしたの。親友だった",
              "maleVariant": "俺も身近な人を亡くしてな。親友だった",
              "stringId": "1772241123044126720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「も」？ アンドリューのことか？ わかったぞ、あいつらから聞いたんだな",
              "maleVariant": "",
              "stringId": "1772241740697333760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大親友だった… ガキの頃から知っててな。心置きなく何でも話せる唯一の相手だったんだ",
              "maleVariant": "",
              "stringId": "1772242131874902016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰にだって耐えられない限界はある",
              "maleVariant": "",
              "stringId": "1772243795369422848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "亡くなった人のことは忘れないと",
              "maleVariant": "",
              "stringId": "1772244079743234048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンドリューが亡くなった経緯は？",
              "maleVariant": "",
              "stringId": "1772244463975034880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "経緯って… 普通に老衰だよ。俺のばあちゃんといくつも変わらないんだ。不思議はない",
              "maleVariant": "",
              "stringId": "1772244910802628608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メンデスは何もわかってない。誰にでも限界はある。彼の場合は、あんたと違ってまだそこに達してないだけよ",
              "maleVariant": "メンデスは何もわかってない。誰にでも限界はある。あいつの場合は、あんたと違ってまだそこに達してないだけだ",
              "stringId": "1772245939564736512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも奴が正しい可能性もある。ひょっとしたら俺は生まれつきの弱虫なのかもしれない",
              "maleVariant": "",
              "stringId": "1772246354498842624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その友人は、あんたが悲しんでくれて、きっと喜んでる",
              "maleVariant": "その友人は、あんたが悲しんでくれて、きっと喜んでるよ",
              "stringId": "1772246468885901312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうかもな…",
              "maleVariant": "",
              "stringId": "1772246637194932224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メンデスは正しい。もっと心を強く持って、堂々としてないと",
              "maleVariant": "",
              "stringId": "1772247209633542144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ノックする] バリー、大丈夫？",
              "maleVariant": "[ノックする] バリー、大丈夫か？",
              "stringId": "1773738240396914688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、バリー！ 中にいるんでしょ？",
              "maleVariant": "なあ、バリー！ 中にいるんだろ？",
              "stringId": "1793747692638334976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "上の階のVよ。覚えてる？ 車の話をしたことがあるでしょ",
              "maleVariant": "上の階のVだ。覚えてるか？ 車の話をしたことがあるよな",
              "stringId": "1837130920566177792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すっごい目であたしを見るもんだから、バッドランズまで逃げようかと思ったくらい",
              "maleVariant": "すっごい目で俺を見るもんだから、バッドランズまで逃げようかと思ったくらいだ",
              "stringId": "1837131091962216448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、ミズタニ・シオンの話で盛り上がったじゃない。あたしは成金向けの車だって言ったけど",
              "maleVariant": "ほら、ミズタニ・シオンの話で盛り上がっただろ。俺は成金向けの車だって言ったけど",
              "stringId": "1837131550449975296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "警官を辞めたとしても、それは変わらない。気をしっかりもつのよ。いい？",
              "maleVariant": "警官を辞めたとしても、それは変わらない。気をしっかりもつんだ。いいな？",
              "stringId": "1837160190080434176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "車の趣味がいいとは言えないけど、あいつはいい奴だから、ってね",
              "maleVariant": "車の趣味がいいとは言えないが、あいつはいい奴だから、ってな",
              "stringId": "1837180973292961792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど。また論破されたくてやって来たのか？",
              "maleVariant": "",
              "stringId": "1837181622453780480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんた、人の元気づけ方をわかってるな。あの2人もいい人選するじゃないか",
              "maleVariant": "",
              "stringId": "1837181755329331200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別に解決したいことなんてない、そうは考えないのか？",
              "maleVariant": "",
              "stringId": "1837195889110401024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もっと外に出るべきよ。友達と飲みに行くとか。バリー、人生は待ってくれないのよ",
              "maleVariant": "もっと外に出るべきだ。友達と飲みに行くとかな。バリー、人生は待ってくれないぞ",
              "stringId": "1837196278341812224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "孤独感や自己嫌悪に溺れてたって、何も解決しない",
              "maleVariant": "",
              "stringId": "1837196537096814592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ… ほら、入ってくれ。何も変わりはしないがな",
              "maleVariant": "",
              "stringId": "1837197199327084544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わからないでしょ",
              "maleVariant": "わからないだろ",
              "stringId": "1837200781766017024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やるだけのことはやった",
              "maleVariant": "",
              "stringId": "1837204000844005376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしたちみたいな人間に共通する目をしてるから",
              "maleVariant": "俺たちみたいな人間に共通する目をしてるからな",
              "stringId": "1841510432347242496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「たち」？ あんた警官か？",
              "maleVariant": "",
              "stringId": "1841510576396419072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "友人を亡くすのは警官だけじゃない。あたしの仕事だって似たようなものよ",
              "maleVariant": "友人を亡くすのは警官だけじゃない。俺の仕事だって似たようなもんだ",
              "stringId": "1841510674224365568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろん慣れはする… でも身近な人が死ぬのはつらい",
              "maleVariant": "",
              "stringId": "1841510817283686400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1841511057533419520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたもか？",
              "maleVariant": "",
              "stringId": "1841511180376195072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "老衰？ ナイトシティで？ 記念碑を建ててもいいくらい珍しい話ね",
              "maleVariant": "老衰？ ナイトシティで？ 記念碑を建ててもいいくらい珍しい話だな",
              "stringId": "1841603088951787520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "祭壇に骨壺を置いとくだけで十分だ",
              "maleVariant": "",
              "stringId": "1841603265448099840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、見てきたことを全て忘れろっていうのか？ あの子供も、アンドリューも？",
              "maleVariant": "",
              "stringId": "1841696313347653632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもあんたは生きてる。過去にこだわってちゃダメよ",
              "maleVariant": "だがあんたは生きてる。過去にこだわってちゃダメだ",
              "stringId": "1841696432616882176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一人でここに座って、悲嘆に暮れてても、何も変わらないでしょ",
              "maleVariant": "一人でここに座って、悲嘆に暮れてても、何も変わらないだろ",
              "stringId": "1841696569938395136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう話は終わりだ",
              "maleVariant": "",
              "stringId": "1841696676909924352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あっそう",
              "maleVariant": "そうかい",
              "stringId": "1841696866844786688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当はいるんでしょ？",
              "maleVariant": "本当はいるんだろ？",
              "stringId": "1842890146919989248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また後で来よう…",
              "maleVariant": "また後で来るか…",
              "stringId": "1842890690468233216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へっ、ああいう話は忘れない",
              "maleVariant": "",
              "stringId": "1895218491762130944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そもそも俺の事情をどこで知った？ ストーカーか？ それとも誰かに頼まれたのか？",
              "maleVariant": "",
              "stringId": "1895290963312230400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰も俺の気持ちなんてわかりゃしないんだ！",
              "maleVariant": "",
              "stringId": "1895316828007940096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "警察時代の仲間は？",
              "maleVariant": "",
              "stringId": "1895381606264979460"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "NCPDは相変わらずの環境なのね",
              "maleVariant": "NCPDは相変わらずの環境なんだな",
              "stringId": "1895381672568537088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "NCPDの環境は、相変わらずみたいね",
              "maleVariant": "NCPDの環境は、相変わらずみたいだな",
              "stringId": "1895381798296993792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "前に一度… 目の前でメイルストロームが子供を殺したことがあった。遊びでな",
              "maleVariant": "",
              "stringId": "1895382011434745856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何もできなかった。奴には企業の後ろ盾があったんだ",
              "maleVariant": "",
              "stringId": "1895382153453879296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よくある話ね",
              "maleVariant": "よくある話だ",
              "stringId": "1895382273578745856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "署長が匿名の寄付金という名の賄賂を貰って、あとはどうなったと思う？ あの子は自殺したことになったんだ",
              "maleVariant": "",
              "stringId": "1895382362850312192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの時、俺の中で何かが死んだんだ。仕事を続ける気になれず… その数ヶ月後にアンドリューが死んで、全てが崩れ落ちたんだ",
              "maleVariant": "",
              "stringId": "1895382503074283520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほかに腹を割って話せる相手はいなかったの？ ペトロワとメンデスは？",
              "maleVariant": "ほかに腹を割って話せる相手はいなかったのか？ ペトロワとメンデスは？",
              "stringId": "1895383914172366848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ペトロワはいい子だが、この手のことは苦手だ。メンデスは話にならない",
              "maleVariant": "",
              "stringId": "1895403036356624384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴は、警官はタフであるべきだと思ってる。子供が殺されるのを見て耐えられないのは、弱虫な遺伝子のせいだとよ",
              "maleVariant": "",
              "stringId": "1895403137036697600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "友達のことは話したんでしょ",
              "maleVariant": "友達のことは話したんだろ",
              "stringId": "1895403304540422144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それだって相当迷ったんだ。だから全部は話してない。そのほうがいい",
              "maleVariant": "",
              "stringId": "1895403562372677632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンドリューはどうして亡くなったの？",
              "maleVariant": "アンドリューはどうして亡くなったんだ？",
              "stringId": "1895406270953537536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話せてよかったよ、V。俺… 少し時間をかけて考えてみる",
              "maleVariant": "",
              "stringId": "1895441300924907520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。元気でね",
              "maleVariant": "わかった。元気でな",
              "stringId": "1895441419825037312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バリー、いるの？",
              "maleVariant": "バリー、いるのか？",
              "stringId": "1905181149760307200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メンデスと話してるのかな",
              "maleVariant": "メンデスと話してるのかもな",
              "stringId": "1905184717938606080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やりきれないね",
              "maleVariant": "やりきれないな",
              "stringId": "1905551821829292032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰にだって耐えられない限界はある",
              "maleVariant": "",
              "stringId": "1967849004892667908"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰だっていつかは死ぬものよ",
              "maleVariant": "誰だっていつかは死ぬもんだ",
              "stringId": "1967849129346056192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンドリューはおじいさんみたいな存在だったの？",
              "maleVariant": "アンドリューはおじいさんみたいな存在だったのか？",
              "stringId": "1967849460209532928"
            },
            {
