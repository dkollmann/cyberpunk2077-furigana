                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][db_db][V tries to change the mode to RAPID]",
                    "maleVariant": null,
                    "stringId": 3123410688244846592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][db_db][Screen informs her, that the cooling is not working at full capacity, which makes this mode unsafe and unavailable]",
                    "maleVariant": null,
                    "stringId": 3123410688261623812
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][db_db][V engages the maintenance mode]",
                    "maleVariant": null,
                    "stringId": 3123410689603801104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][db_db][The screen begins to highlight sub-systems shutting down, and finally the whole procedure closes up as a success]",
                    "maleVariant": null,
                    "stringId": 3123410689603801112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][db_db][The bridge belowe begins moving towards the core entrance, and V can hear the sound of doors behind her opening]",
                    "maleVariant": null,
                    "stringId": 3123410689603801120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][db_db][The previous noise of the facility is stopped, like a silence before the storm]",
                    "maleVariant": null,
                    "stringId": 3123666701128699904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][db_db][Suddenly she notices ROBOT about to striker her down. HE's half broken, with only one arm left, but still dangerous.]",
                    "maleVariant": null,
                    "stringId": 3123693033220870144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT][dodge left]",
                    "maleVariant": null,
                    "stringId": 3123693928989986820
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][db_db][V fails to dodge out of the way]",
                    "maleVariant": null,
                    "stringId": 3123696668692582400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]This is just one [final] obstacle. We're [almost] here. You'll be next.",
                    "maleVariant": null,
                    "stringId": 3123702160999452672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][db_db][Whispers and other audible hallucinations are getting more intense]",
                    "maleVariant": null,
                    "stringId": 3123703077438099456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I hope this piece of iron won't get up anymore.",
                    "maleVariant": null,
                    "stringId": 3123703838637166592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I'm inside...",
                    "maleVariant": null,
                    "stringId": 3123705051579551744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][db_db][PLACEHOLDER FOR KILLING THE ROBOT]",
                    "maleVariant": null,
                    "stringId": 3123714576088961028
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][db_db][V fails to kill robot in time]",
                    "maleVariant": null,
                    "stringId": 3123715625352826888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][db_db][There is a visible feedback on the Core MAchinery]",
                    "maleVariant": null,
                    "stringId": 3124109893468819456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT][dodge right]",
                    "maleVariant": null,
                    "stringId": 3124254146602135552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT][slide tackle]",
                    "maleVariant": null,
                    "stringId": 3124254236410572800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT][coughs] Nice play, nice ... [coughs]",
                    "maleVariant": null,
                    "stringId": 3144661917453127680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Shit, oxygen is leaking from the suit. I have a few minutes to spare.",
                    "maleVariant": null,
                    "stringId": 3144669931862102016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us]Oh, shit... I don't know if you see this, Johnny. Cyberspace seen through the naked eye. ",
                    "maleVariant": null,
                    "stringId": 3144734087382437888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us]It's here, isn't it? So Mi? Militech's buried secret. ",
                    "maleVariant": null,
                    "stringId": 3144735619259699200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us]It's here, right, So Mi? Militech's buried secret. ",
                    "maleVariant": null,
                    "stringId": 3144737334847475720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us]I know you're here. Is aw you. Are you waiting for me?",
                    "maleVariant": null,
                    "stringId": 3144752005298130944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us]You're here, aren't you? So Mi? ",
                    "maleVariant": null,
                    "stringId": 3144752913029402632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Fuck me ... I have to find something to keep me warm. And fast.",
                    "maleVariant": null,
                    "stringId": 3145979817178828800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us]How did it go? Every strange technology seems like magic...? ",
                    "maleVariant": null,
                    "stringId": 3146108388698681344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us]Arasaka's Soulkiller flatlined netrunners. What would happen if Militech wanted to unleash the AIs? "Scalpel and atomic bomb..."",
                    "maleVariant": null,
                    "stringId": 3146113581179924480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us]This was your plan, wasn't it, So Mi? Control the AIs from the Blackwall? ",
                    "maleVariant": null,
                    "stringId": 3146116922547040256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us]Was this your plan, So Mi? Control the Blackwall AIs?",
                    "maleVariant": null,
                    "stringId": 3146120719163912192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us]But you must've known what would happen. How everything would go wrong. Did you have to make such a big risk?",
                    "maleVariant": null,
                    "stringId": 3146120902119452672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT][V opens up the panel beneath the terminal]",
                    "maleVariant": null,
                    "stringId": 3154670418170613760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT][The terminal is broken, from the looks of it - by the robot]",
                    "maleVariant": null,
                    "stringId": 3154683333372153856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT][When V tries to meddle with the cables the whole terminal ovverloads with sudden surge of energy and explodes in her face, damaging her suit in turn]",
                    "maleVariant": null,
                    "stringId": 3154697048276471808
                  }
                }
              ]
            }
