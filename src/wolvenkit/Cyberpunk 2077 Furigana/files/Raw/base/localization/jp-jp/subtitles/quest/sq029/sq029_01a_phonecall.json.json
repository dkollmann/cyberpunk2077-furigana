            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よう、V。元気にしてるか？",
                    "maleVariant": null,
                    "stringId": 1467566442393755648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もちろん",
                    "maleVariant": null,
                    "stringId": 1467566442427310080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "また別の機会に",
                    "maleVariant": null,
                    "stringId": 1467566442444087308
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ぜひみんなに会いたい",
                    "maleVariant": null,
                    "stringId": 1467566442460864512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、残念だな…",
                    "maleVariant": null,
                    "stringId": 1467572443536809984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よし！ ジョスが喜ぶぞ",
                    "maleVariant": null,
                    "stringId": 1467574206000128000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョスのところに食事に来ないかと思って",
                    "maleVariant": null,
                    "stringId": 1792512523980902400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "来てくれると嬉しい。どうだ？",
                    "maleVariant": null,
                    "stringId": 1792512822195916800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、調子はどうだ？",
                    "maleVariant": null,
                    "stringId": 1805450730826338308
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あのさ、夕食でも食べに来ないか？",
                    "maleVariant": null,
                    "stringId": 1805450730826338320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョスが家族を集めて、食事会をしたがってるんだ",
                    "maleVariant": null,
                    "stringId": 1805450730826338332
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もちろん、喜んで",
                    "maleVariant": null,
                    "stringId": 1805450730859892736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "また別の時に",
                    "maleVariant": null,
                    "stringId": 1805450730859892752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "断るわけないでしょ",
                    "maleVariant": "断るわけないだろ",
                    "stringId": 1805450730893447172
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よし！ じゃあジョスのところでな",
                    "maleVariant": null,
                    "stringId": 1805450730893447188
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "楽しみにしてる",
                    "maleVariant": "ああ、楽しみにしてる",
                    "stringId": 1805450730893447200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "招待はありがたいんだけど、今ちょっと忙しくて。また別の機会にね",
                    "maleVariant": "招待はありがたいんだが、今ちょっと忙しくてな。また別の機会にでも",
                    "stringId": 1805450730910224388
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "残念だな。ジョスと俺の作るシチューはうまいんだぞ。ほとんど俺がやんだが…",
                    "maleVariant": null,
                    "stringId": 1805450730910224400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうか、いいんだ。でも次は断らせないからな",
                    "maleVariant": null,
                    "stringId": 1805450730927001608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "決まりね",
                    "maleVariant": "決まりだな",
                    "stringId": 1805458216098697216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしても時間がなくて",
                    "maleVariant": null,
                    "stringId": 1805458257186099200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしもよ",
                    "maleVariant": "俺もだ",
                    "stringId": 1805466153533804548
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしも会いたかった、リバー",
                    "maleVariant": "俺も会いたかったよ、リバー",
                    "stringId": 1805467067808190464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やめてよ、リバー… 前会ってからそんなに経ってないでしょ",
                    "maleVariant": "おいおい… 前会ってからそんなに経ってないだろ",
                    "stringId": 1805467095490596864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしもよ。でも今は無理なの",
                    "maleVariant": "俺もさ。だが今は無理なんだ",
                    "stringId": 1805467674774310916
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やめてよ… まだそんなにたってないでしょ",
                    "maleVariant": "おいおい… まだそんなにたってないだろ",
                    "stringId": 1805468105864876032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしも寂しいし、リバーに会いたい。でも今はダメ。どうしようもないの",
                    "maleVariant": "俺も寂しいし、リバーに会いたい。でも今は… どうしても無理なんだ",
                    "stringId": 1805468652651122692
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ねえ、そんなに焦らなくても… また機会はあるでしょ。ね？",
                    "maleVariant": "なあ、そんなに焦らなくても… また機会はあるだろ。な？",
                    "stringId": 1805468950228602884
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "焦らなくてもまた機会はあるでしょ",
                    "maleVariant": "焦らなくてもまた機会はあるだろ",
                    "stringId": 1805469224703856640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあジョスのとこで？",
                    "maleVariant": null,
                    "stringId": 1805473551111655424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。でも近いうちに会えるだろ？",
                    "maleVariant": null,
                    "stringId": 1805473766665326592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあそうだったかもな… とにかく、来るだろ？",
                    "maleVariant": null,
                    "stringId": 1805474287983759360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、そうだな。また別の機会に",
                    "maleVariant": null,
                    "stringId": 1805474473204224000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よう、V。驚いたな。どうした？",
                    "maleVariant": null,
                    "stringId": 1805482803628838912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まだ食事会をやってないなら、行ってもいい？",
                    "maleVariant": "まだ食事会をやってないなら、行ってもいいか？",
                    "stringId": 1805486635830185984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よお、V、どうした？",
                    "maleVariant": null,
                    "stringId": 1805487568442068996
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この前、ジョスも一緒に、みんなで会いたいって言ってたでしょ？",
                    "maleVariant": "この前、ジョスも一緒に、みんなで会いたいって言ってただろ？",
                    "stringId": 1805487568442069004
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ。なんだ、時間あるのか？",
                    "maleVariant": null,
                    "stringId": 1805491788717199360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本気で言ってんだよな？",
                    "maleVariant": null,
                    "stringId": 1805492207996604416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしのことなんて忘れたかと思った",
                    "maleVariant": "俺のことをまだ覚えてたのか",
                    "stringId": 1844310344715079680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "へえ、まだあたしのこと忘れてなかったんだ？",
                    "maleVariant": "へえ、まだ俺のこと覚えてたんだな？",
                    "stringId": 1844312300602920960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、すまん、色々忙しくてさ…",
                    "maleVariant": null,
                    "stringId": 1844312700773076992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ランディはまだ入院してるから、ジョスが見舞いに行く間、子供の面倒を見ないといけなくて",
                    "maleVariant": null,
                    "stringId": 1844312989643182080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "別に説明しなくてもいいから",
                    "maleVariant": "別に説明しなくてもいいって",
                    "stringId": 1844313936666693632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だよな… で、来てくれるか？",
                    "maleVariant": null,
                    "stringId": 1844314266187993088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "またね、リバー",
                    "maleVariant": "またな、リバー",
                    "stringId": 1844318540737417216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしのこと忘れたのかと思ってた",
                    "maleVariant": "俺のことなんて忘れたかと思った",
                    "stringId": 1844363710942023680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前のことを？ まさか。ちょっと忙しかっただけだ",
                    "maleVariant": null,
                    "stringId": 1844363710942023692
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だよな… 来てくれるか？ きっと楽しい思い出になる",
                    "maleVariant": null,
                    "stringId": 1844363710942023728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしのことなんて忘れたかと思った",
                    "maleVariant": "俺の事なんて忘れたかと思った",
                    "stringId": 1844364048432500736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあね、リバー",
                    "maleVariant": "じゃあな、リバー",
                    "stringId": 1844375186876534784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もちろん。またね、リバー",
                    "maleVariant": "もちろん。またな、リバー",
                    "stringId": 1844376802874757120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ずっと… お前に会いたかった",
                    "maleVariant": null,
                    "stringId": 1844381447076581376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかったよ、リバー。じゃあね",
                    "maleVariant": "わかったよ、リバー。じゃあな",
                    "stringId": 1844389582063407104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、そうね",
                    "maleVariant": "ああ、そうだな",
                    "stringId": 1844392080543191040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よし。ならまたその時に",
                    "maleVariant": null,
                    "stringId": 1844392405215875072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なら、今日はどう？",
                    "maleVariant": "なら、今日はどうだ？",
                    "stringId": 1844397410211729408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、いいぞ。ジョスに伝える！ じゃあな！",
                    "maleVariant": null,
                    "stringId": 1844397552348303360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうよ… 今日はどう？",
                    "maleVariant": "そうさ… 今日はどうだ？",
                    "stringId": 1844403564497211392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、もちろんだ！ 来いよ！ ジョスに伝える",
                    "maleVariant": null,
                    "stringId": 1844403834962710528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "後でな！",
                    "maleVariant": null,
                    "stringId": 1844406917155393536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあまあよ",
                    "maleVariant": "まあまあだ",
                    "stringId": 1905678686658629632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もちろん。どうしたの？",
                    "maleVariant": "もちろん。どうしたんだ？",
                    "stringId": 1905679638547533824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それは美味しそう。でも、今はちょっとダメなんだ。ごめんね",
                    "maleVariant": "それはうまそうだな。でも、今はちょっとダメなんだ。すまん",
                    "stringId": 1977929760582160384
                  }
                }
              ]
            }
