        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "WNSニュース、アリフ・イクバルです。今日、ルシアス・ライン市長が死去し、ナイトシティは悲しみに包まれました",
                    "maleVariant": null,
                    "stringId": 1792253756781273088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "遺体を発見したご遺族の証言によると、就寝中に亡くなったとのことです",
                    "maleVariant": null,
                    "stringId": 1796930548648361984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "死因は公表されていませんが、冠動脈インプラントの不具合が疑われています",
                    "maleVariant": null,
                    "stringId": 1796930846880153600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ライン市長の残りの任期は、副市長であるウェルドン・ホルト氏が引き継ぐ予定となっています",
                    "maleVariant": null,
                    "stringId": 1796931181099073536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ホルト氏はまた、次期市長選に立候補すると、明かしました",
                    "maleVariant": null,
                    "stringId": 1796931393951612928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "市長であり… 良き友であったルシアス・ライン氏の早すぎる死を知り、悲しみに… 暮れています",
                    "maleVariant": null,
                    "stringId": 1796931673795575808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "涙が乾いたら、上を向かねばなりません。ルシアスが思い描いていた美しい未来を達成するために",
                    "maleVariant": null,
                    "stringId": 1796932156174090240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私は… 彼のやり残した仕事を果たすべく、全力を尽くすことをここにお約束します。どうか安らかに",
                    "maleVariant": null,
                    "stringId": 1796932325137432576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ライン市長のライバルであるジェファーソン・ペラレス元地方検事も哀悼の言葉を述べています",
                    "maleVariant": null,
                    "stringId": 1796934114561736704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ライン市長とは、政治への考えがよく一致せず、時に激しく言葉を交わしました",
                    "maleVariant": null,
                    "stringId": 1796934650342129664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ですが、白熱した議論の後でも、お互いを称え、握手は欠かしませんでした",
                    "maleVariant": null,
                    "stringId": 1796935048566128640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これで… 残念ながら最後のお別れです。本当にありがとう、この街をよくしてくれて",
                    "maleVariant": null,
                    "stringId": 1796935285040988160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "次に市長の座を継ぐのは誰でしょうか？ それはじきにわかります",
                    "maleVariant": null,
                    "stringId": 1796935592030486528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "直近の世論調査によると、現状ホルト氏とペラレス氏が互角の支持を得ています",
                    "maleVariant": null,
                    "stringId": 1796935831458136064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "では、ここで一旦CMです",
                    "maleVariant": null,
                    "stringId": 1796936027801894912
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}