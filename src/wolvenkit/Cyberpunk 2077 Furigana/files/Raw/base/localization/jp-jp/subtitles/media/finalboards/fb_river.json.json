  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自殺か… まあ、当然といえばそうなのかもしれない",
                    "maleVariant": null,
                    "stringId": 1790661351166615556
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よう、V… ようやく写真が届いたぞ",
                    "maleVariant": null,
                    "stringId": 1790661351216947204
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よう、V！ 来週ランディの誕生日にジョスが夕飯を作るんだ。あー、何日だったかな…",
                    "maleVariant": null,
                    "stringId": 1790661351267278852
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よう、V… ちょっと言いにくいんだが… 身元引受人になってくれないか？",
                    "maleVariant": null,
                    "stringId": 1790661351300833284
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もしもし、V",
                    "maleVariant": null,
                    "stringId": 1790661351317610500
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よう、V。実はな――",
                    "maleVariant": null,
                    "stringId": 1790661351351164932
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティの半分の人間が選ぶ道だ。特にいい奴はな",
                    "maleVariant": null,
                    "stringId": 1801109546592878592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺の最初の相棒、ジェイミー・シーンも… 自分の頭を撃ったんだ",
                    "maleVariant": null,
                    "stringId": 1801110212598022144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "遺書すら残さなかったが、署の上の連中が原因なのは明らかだった",
                    "maleVariant": null,
                    "stringId": 1801110478198128640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "結局、奴の死で何かが変わるわけもなく――まっとうな奴が1人減っただけだ",
                    "maleVariant": null,
                    "stringId": 1801110664475557888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "とにかく、お前が安らかに眠れてることを願ってる",
                    "maleVariant": null,
                    "stringId": 1801111231310577664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バッドランズがこんなに綺麗だとは思わなかったよ",
                    "maleVariant": null,
                    "stringId": 1801112788487880704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "落ち着いたら遊びに行こうかと思う。その… ジョスの具合が良くなったら、な",
                    "maleVariant": null,
                    "stringId": 1801112928191758336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ときどき考えるんだ、もし俺たちが…",
                    "maleVariant": null,
                    "stringId": 1801113151949488128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや、やっぱ何でもない。俺の愚痴を聞くほどお前もヒマじゃないだろう",
                    "maleVariant": null,
                    "stringId": 1801113458905432064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "頑張れよ、V。思うがままに生きるんだ",
                    "maleVariant": null,
                    "stringId": 1801113965459914752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "21日！",
                    "maleVariant": null,
                    "stringId": 1801115176758132736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうそう、21日だ。だから、もし予定が空いてて… 近くでヒマしてるようだったらだな…",
                    "maleVariant": null,
                    "stringId": 1801118678096003072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "来なよ、楽しいよ！",
                    "maleVariant": null,
                    "stringId": 1801119153360977920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うるさい、ドリアン！",
                    "maleVariant": null,
                    "stringId": 1801125979657846784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "…ほら、みんな会いたがってる",
                    "maleVariant": null,
                    "stringId": 1801126353873649664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V！ 車に乗せてくれる？",
                    "maleVariant": null,
                    "stringId": 1801126686481956864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うるさいって言っただろ",
                    "maleVariant": null,
                    "stringId": 1801126946713354240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと2人とも！ 静かにしなさい！",
                    "maleVariant": null,
                    "stringId": 1801127142000148480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "えっーと… じゃあまた連絡する。絶対来てくれよ。あと、無茶はほどほどにな",
                    "maleVariant": null,
                    "stringId": 1801127358879219712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "昨日の夜、バーで飲んでた時に、あの農場の子供たちについて話してる奴と出くわしてな",
                    "maleVariant": null,
                    "stringId": 1801129982047272960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "騙されたのは自業自得で、いい経験になっただろうってぬかしやがった",
                    "maleVariant": null,
                    "stringId": 1801130179615768576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もちろん最初に黙れと警告はしたが――",
                    "maleVariant": null,
                    "stringId": 1801130421610332160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "案の定、言うことを聞かなかった。外に出ろってなって… あとはお決まりの展開だ",
                    "maleVariant": null,
                    "stringId": 1801130633573679104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺はリトルチャイナの留置場にいる――場所はわかるよな。他に頼れる奴がいないんだ。連絡をくれ、頼んだぞ…",
                    "maleVariant": null,
                    "stringId": 1801130812855009280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "街の全ての警察署と病院、リパードクのクリニックまで… 1つ残らず探したが――",
                    "maleVariant": null,
                    "stringId": 1801131827809468416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "…誰もお前の行方を知らない。神隠しみたいだ",
                    "maleVariant": null,
                    "stringId": 1801131979207065600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "例の仕事が失敗したのなら… もう会えないのかもしれない",
                    "maleVariant": null,
                    "stringId": 1801132166843449344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だが、もし上手くいって… もし生きてるなら… 連絡してくれ、V。それぐらいしてくれてもいいだろ",
                    "maleVariant": null,
                    "stringId": 1801132386272657408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう切るよ… じゃあな",
                    "maleVariant": null,
                    "stringId": 1801132568573886464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "第5分署の連中が、違法武器店を摘発した",
                    "maleVariant": null,
                    "stringId": 1801133388929417216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ショットガンにスタンガン、マスタードガスとか、色々だ",
                    "maleVariant": null,
                    "stringId": 1801133566868570112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "上には全部処分するよう言われたんだが… 有効活用できる人間に渡したほうが、世のためになるんじゃないか… と思ってな",
                    "maleVariant": null,
                    "stringId": 1801133764302848000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前が俺とランディのためにしてくれたことは、忘れない",
                    "maleVariant": null,
                    "stringId": 1801133989369200640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こういう装備は市民を守ってくれる人間にこそ、提供すべきなんだ",
                    "maleVariant": null,
                    "stringId": 1801134725318557696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "礼はいらない。じゃあな、V",
                    "maleVariant": null,
                    "stringId": 1801135126361128960
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}