    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だが連中は知らない… “サイバー空間”と呼ばれている世界の本当の名は…",
                    "maleVariant": null,
                    "stringId": 1807074546761768960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティの住民は、みんな魂なんて持ってない",
                    "maleVariant": null,
                    "stringId": 1807076406130286596
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "奈落の底の、邪悪な門を開けたのは誰か… 知りたいか？",
                    "maleVariant": null,
                    "stringId": 1807077061666447360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サイバー空間で死人を見かけた",
                    "maleVariant": null,
                    "stringId": 1807084687397658624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "コーポの重役や指導者たちは何かと支配を強調する。しかし連中は人間ではないのだ！ それなら何なのかって？",
                    "maleVariant": null,
                    "stringId": 1811019526333177860
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "違う、そうじゃない！ アラサカ家はただの一族じゃない！ 人間ですらない！",
                    "maleVariant": null,
                    "stringId": 1811019580691357700
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "連中はバッドランズをトラックで横断するのだ！ しかし満月が昇ると、人間の仮面を脱ぎ去る！",
                    "maleVariant": null,
                    "stringId": 1811019615990620164
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本当にそれは自分だけの記憶なのか？ その目が、君だけのものではなかったとしたら…",
                    "maleVariant": null,
                    "stringId": 1811019643236818948
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "コンタクトレンズで目を誤魔化せばいいのに",
                    "maleVariant": null,
                    "stringId": 1811022716386582528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "確かに。政治家たちに会ったことがある",
                    "maleVariant": null,
                    "stringId": 1811022716386582544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "連中がどこから現れたのかを知りたいか？",
                    "maleVariant": null,
                    "stringId": 1811022716420136964
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや、サブロウは確実に死んだ",
                    "maleVariant": null,
                    "stringId": 1811022762188382208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハナコの若さは確かに怪しい",
                    "maleVariant": null,
                    "stringId": 1811022762272268300
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ただの人間が、どうやって死を恐れない忌まわしい怪物になったのか知りたいか？",
                    "maleVariant": null,
                    "stringId": 1811022762289045508
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それが本当なら、あたしはもう死んでる",
                    "maleVariant": "それが本当なら、俺はもう死んでる",
                    "stringId": 1811022804735401984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "確かに、キロシの目を通して誰かが見てる気がする",
                    "maleVariant": null,
                    "stringId": 1811022804735402000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一挙一動を監視してるのは、誰だと思う？",
                    "maleVariant": null,
                    "stringId": 1811022804768956420
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "肉体のない生命とは何か？ ネットの存在は、私たちがいる次元とは異なる世界から来た魂ではないのか？",
                    "maleVariant": null,
                    "stringId": 1811075141646589952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの冷たく青い目を見ればわかる。何世紀も前に南極の氷床で生まれた怪物だ",
                    "maleVariant": null,
                    "stringId": 1811075178103480320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "連中の体は年を取らない。しかし目は… 冷たく死んでいる！",
                    "maleVariant": null,
                    "stringId": 1811075307506147328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "機械仕掛けの目を売る者たちは、のぞき見する衝動を抑えていると本当に信じているのか？",
                    "maleVariant": null,
                    "stringId": 1811075393707483136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この街の住民は魂なんて持ち合わせてない",
                    "maleVariant": "この街の住民は魂なんて持ち合わせちゃいない",
                    "stringId": 1811296600847458304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サイバー空間で、死人を見かけたことがあるの",
                    "maleVariant": "サイバー空間で、死人を見かけたことがあるんだ",
                    "stringId": 1811296625090535424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなにわかりやすいなら、どうしてオプティクスを入れないの？ せめてコンタクトレンズとか…",
                    "maleVariant": "そんなにわかりやすいなら、どうしてオプティクスを入れないんだ？ せめてコンタクトレンズとか？",
                    "stringId": 1811296682401505280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その通りよ。過去に会った政治家には裏表があった",
                    "maleVariant": "その通りだ。過去に会った政治家には裏表があった",
                    "stringId": 1811296710302015488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サブロウは死んだ。それは間違いない。あたしが保証してあげる",
                    "maleVariant": "サブロウは死んだ。それは間違いない。俺が保証してやる",
                    "stringId": 1811297320170594304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "確かに… ハナコは80過ぎなのに、今でも数百万エディーの美貌を保ってる",
                    "maleVariant": null,
                    "stringId": 1811297340521357312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もし本当に誰かに見られてるなら、あたしは今頃、裏社会かコーポに捕まってるよ",
                    "maleVariant": "もし本当に誰かに見られてるなら、俺は今頃、裏社会かコーポに捕まってるよ",
                    "stringId": 1811297529466363904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うーん… ヴィクにキロシの目を入れてもらって以来、誰かに見られてるような気はするのよね…",
                    "maleVariant": "うーん… ヴィクにキロシの目を入れてもらって以来、誰かに見られてるような気はするんだよな…",
                    "stringId": 1811297561175302144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "真の姿を人間の皮膚という仮面で隠しているのだ！ 2つの顔を持つひきょう者、人類の敵め！",
                    "maleVariant": null,
                    "stringId": 1811450269509263360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼らの声を聞いたことがある。命ある者をあの地に導いてはならぬ。あちらの世界へ魂を持っていかれることになるぞ！",
                    "maleVariant": null,
                    "stringId": 1811450629799976960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "教えてやろう。サブロウはこの世界から消えてはいない！ 一族の墓地で休眠しているだけなのだ",
                    "maleVariant": null,
                    "stringId": 1811452412446285824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "カメラはあちこちにある。私たちの中にさえあるのだ！ 喜び、悲しみ… 人生は全て、連中にとって見せ物でしかない！",
                    "maleVariant": null,
                    "stringId": 1811453627435491328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何百万年も昔、連中は恐竜から進化して、歴史上もっとも早く知性を得た種族となった",
                    "maleVariant": null,
                    "stringId": 1811472041436684288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][db_db]DEBUB Continue normally",
                    "maleVariant": null,
                    "stringId": 1812703773632667652
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][db_db]DEBUG Jump to next",
                    "maleVariant": null,
                    "stringId": 1812703897867952128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "奴らの組織は、この邪悪な怪物の隠れみのとして存在するだけ… 不死のままでいるためだ！",
                    "maleVariant": null,
                    "stringId": 1814285474980864000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "再び戻ってくるために、使用人の血を飲み、体が回復するのを待っているのだ… 忘れるな！",
                    "maleVariant": null,
                    "stringId": 1814285597236436992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "爬虫類人は南極の雪と氷の中に隠れ、創造物の頂点たる人間を、その座から引きずり降ろそうとしているのだ！",
                    "maleVariant": null,
                    "stringId": 1814317334008078336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "連中の創造主は誰かって？ それこそが、ケンタウルス座アルファ星の科学者たちだ！",
                    "maleVariant": null,
                    "stringId": 1814317485640556544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "教えてやろう… ケンタウルス座アルファ星の科学魔術師だ！",
                    "maleVariant": null,
                    "stringId": 1814317671917985792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ケンタウルス座アルファ星の科学魔術師の助けを借りたのだ！",
                    "maleVariant": null,
                    "stringId": 1814318844645384192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もちろんケンタウルス座アルファ星の権力者たちだ！",
                    "maleVariant": null,
                    "stringId": 1814319322645045248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "連中の存在を無視すれば、向こうの勝ちになってしまう！",
                    "maleVariant": null,
                    "stringId": 1817085248800989188
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "電波のみが真実を教えてくれる！ 耳があるのなら聞き、目があるのならしかと見よ！",
                    "maleVariant": null,
                    "stringId": 1817085248800989196
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "手遅れになる前に心を開け！",
                    "maleVariant": null,
                    "stringId": 1817085248800989204
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "見覚えのある顔だ。よく来てるだろう。その目からして、真実に心を開いたというのがわかる",
                    "maleVariant": null,
                    "stringId": 1912496223087140864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "“わずか”だったら",
                    "maleVariant": null,
                    "stringId": 1912501568358490116
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "“わずか”以上に恵んであげる",
                    "maleVariant": "“わずか”以上に恵んでやる",
                    "stringId": 1912502219381579776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "仕事でもするのね",
                    "maleVariant": "仕事でもするんだな",
                    "stringId": 1912502277011316736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "“わずか”でいいんだったら恵んであげる。でも、無駄遣いしないでよ",
                    "maleVariant": "“わずか”でいいんだったら恵んでやる。だが、無駄遣いするなよ",
                    "stringId": 1912502471023042560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "応援してる。どうぞ、これを使って",
                    "maleVariant": "応援してる。ほら、これを使ってくれ",
                    "stringId": 1912502571082358784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたこそ心を開いて、仕事でも見つけるのね",
                    "maleVariant": "あんたこそ心を開いて、仕事でも見つけるんだな",
                    "stringId": 1912502624584900608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ありがとう。これで2日くらいは持つ",
                    "maleVariant": null,
                    "stringId": 1912504807065821184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "貢献してくれてありがとう。最初からわかっていたが、君は真実の信奉者だ",
                    "maleVariant": null,
                    "stringId": 1912505175476707328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それが何になる？ 働いた成果を連中に奪われるんだぞ！",
                    "maleVariant": null,
                    "stringId": 1912506782968881152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "連中の母船は我々の頭上に隠れている！ 準備しろ！ 大挙して襲ってくる日は近い！",
                    "maleVariant": null,
                    "stringId": 1912583229108559872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ギャリー様、アラサカの秘密に触れては駄目です！ 捕まりますよ！",
                    "maleVariant": null,
                    "stringId": 1912606276976812032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "すごい！ 見て！ あれ本物じゃない？ あの番組で見た！",
                    "maleVariant": null,
                    "stringId": 1912709090273042432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "早く、横に立って！ 国家転覆罪でそいつを逮捕してる写真を撮ってあげる！",
                    "maleVariant": null,
                    "stringId": 1912709394662072320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジェシカが見たら死ぬほど喜びそう！",
                    "maleVariant": null,
                    "stringId": 1912711671581941760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "スーパー感激！ もう無理！",
                    "maleVariant": null,
                    "stringId": 1912712146679144448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "パンデモニウムということを！",
                    "maleVariant": null,
                    "stringId": 1915727527469105152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "目撃者の話に耳を傾けよ！ 地獄の業火で焼かれる者を見たそうだ！",
                    "maleVariant": null,
                    "stringId": 1922857292466470912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "炎は見かけなかったけどね…",
                    "maleVariant": "炎は見かけなかったけどな…",
                    "stringId": 1922857899835244544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうやってあざけるがいいさ！ 地獄の業火に包まれた時に、最後に笑うのは誰だと思う？",
                    "maleVariant": null,
                    "stringId": 1922858209307770880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "聞いてないけど",
                    "maleVariant": "聞いてないが",
                    "stringId": 1922858752856014848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "悪魔サタンだ！",
                    "maleVariant": null,
                    "stringId": 1922870119503740928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "笑う？ それはどうかしら。サタンはユーモアのなさで有名だって聞いたことがあるけど",
                    "maleVariant": "笑う？ それはどうだろうな。サタンはユーモアのなさで有名だって聞いたことがあるぞ",
                    "stringId": 1922870179767500800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "声を聞いたの？",
                    "maleVariant": "声を聞いたって？",
                    "stringId": 1922872632546148352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その声は、何て言ってたの？",
                    "maleVariant": "その声は、何て言ってたんだ？",
                    "stringId": 1922872821893808128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "爬虫類人だ！",
                    "maleVariant": null,
                    "stringId": 1922876437467742208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それほど単純じゃないのだ！ 爬虫類人だぞ！ 神経系が人間とは全く異なる！",
                    "maleVariant": null,
                    "stringId": 1922878336464044032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "証人がここにいた！ 爬虫類人の真の顔を見たのか！？",
                    "maleVariant": null,
                    "stringId": 1922878819228434432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあ… 人間の顔をしてなかったのは確かね",
                    "maleVariant": "まあ… 人間の顔をしてなかったのは確かだ",
                    "stringId": 1922879246191804416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "南極大陸。さっき聞いた",
                    "maleVariant": null,
                    "stringId": 1922879791266775040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マイヤーズ大統領も爬虫類人なの？",
                    "maleVariant": "マイヤーズ大統領も爬虫類人なのか？",
                    "stringId": 1922893053689577472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マイヤーズ大統領も爬虫類人だと？",
                    "maleVariant": null,
                    "stringId": 1922893167673982976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あいつらは吸血鬼だ！",
                    "maleVariant": null,
                    "stringId": 1922895609832652800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカは不老不死のテックを研究してる",
                    "maleVariant": null,
                    "stringId": 1922899449432752128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "驚くようなことじゃない。アラサカは不老不死のテックを研究してるってテレビで言ってたし",
                    "maleVariant": null,
                    "stringId": 1922899567510798336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本当の正体を隠すための嘘に決まってるだろ！",
                    "maleVariant": null,
                    "stringId": 1922899832741806080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "不老不死はテクノロジーではなく、奴らの血によるものなのだ！",
                    "maleVariant": null,
                    "stringId": 1922899956607991808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "妙な説得力だな… その目で彼の死を目撃したのか？",
                    "maleVariant": null,
                    "stringId": 1922900400466018304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあ、実は…",
                    "maleVariant": null,
                    "stringId": 1922900615768031232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや… なんでもない。気にしないで",
                    "maleVariant": "いや… なんでもない。気にするな",
                    "stringId": 1922900713612754944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほら見ろ、ほら見ろ！？",
                    "maleVariant": null,
                    "stringId": 1922901036909707264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それが確たる証拠だ！ 年老いた女が子供のような顔をして街を歩いているのだ！",
                    "maleVariant": null,
                    "stringId": 1922924165090492416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うん、そうね… いったん冷静になりましょ",
                    "maleVariant": "ああ、そうだな… いったん冷静になろうか",
                    "stringId": 1922924264562606080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サブロウ・アラサカは奴らの穢れた血を体内に取り込み、闇の存在へと姿を変えた！",
                    "maleVariant": null,
                    "stringId": 1922925616789438464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "向こうのリパーが発明しそうなもんじゃない？",
                    "maleVariant": "向こうのリパーが発明しそうなもんじゃないか？",
                    "stringId": 1922956526377222144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "逆に、なぜまだしてないと思うんだ？",
                    "maleVariant": null,
                    "stringId": 1922956708661673984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "放っておけ！ 種はまかれた！ 真実は根を生やし、私たちを侵略者から解放してくれるだろう！",
                    "maleVariant": null,
                    "stringId": 1922962089702047744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人間の言葉は話さなかった。まるでこの世での姿を永遠に奪われ、苦痛とも叫びとも取れるようなわめき声だ",
                    "maleVariant": null,
                    "stringId": 1922978581084618752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その中に、ある女の声があった。氷のような声をした女王リリスだ。人間性に欠けた存在だった",
                    "maleVariant": null,
                    "stringId": 1922978693659738112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大勢の人々がストリートで命を落としている中、警察が何もしない理由は？",
                    "maleVariant": null,
                    "stringId": 1922993618872418304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こんな苦難を強いる人間がいるわけがないだろう？ 理由がない！",
                    "maleVariant": null,
                    "stringId": 1922993725827170304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その下に隠された姿とは？ 爬虫類人か？",
                    "maleVariant": null,
                    "stringId": 1923005339804614656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいや、違う！ 爬虫類人ではなく… 狼人間だ！",
                    "maleVariant": null,
                    "stringId": 1923005591211196416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "君以外も見ているとしたらどうする！",
                    "maleVariant": null,
                    "stringId": 1924174211379347456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自分はただの駒で、知らずに連中の目的を果たしているのかもしれないぞ？",
                    "maleVariant": null,
                    "stringId": 1924196058586136576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人工頭脳学は破滅をもたらす！ 最も神聖な肉体の一部を金属に、連中の金属に取り換えるなんて！",
                    "maleVariant": null,
                    "stringId": 1924209556678492160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "スイッチを連中に握られている！ 目も、息も、心でさえ、連中が自由に操れるのだ！",
                    "maleVariant": null,
                    "stringId": 1924209652811939840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だったらオプティクスを取り除いて盲目になれって？",
                    "maleVariant": null,
                    "stringId": 1924210527542427648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "仮にあんたが正しいとしても、どうしようもないでしょ？ オプティクスをやめるなんて選択肢はない",
                    "maleVariant": "仮にあんたが正しいとしても、どうしようもないだろ？ オプティクスをやめるなんて選択肢はない",
                    "stringId": 1924210705347362816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "君はもはや救いようがない… だが未来の子供たちは違う！ まだ冒とく的な手術によって穢されていないからな！",
                    "maleVariant": null,
                    "stringId": 1924255819935498240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "子供たちを救うのだ！ 女には肉体への愛を、男にはクロームへの憎悪を！",
                    "maleVariant": null,
                    "stringId": 1924255926235938816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "連中は、疲れて虐げられた大衆をおとなしくさせるために、私たちの笑いと涙をエンターテインメントとして消費しているのだ！",
                    "maleVariant": null,
                    "stringId": 1924266280143798272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "使命を果たし続けられるように、わずかでも募金をしてくれるか？",
                    "maleVariant": null,
                    "stringId": 1924326220069847040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "現金は持ってなくて…",
                    "maleVariant": null,
                    "stringId": 1945903900282494976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今は現金がないの。悪いね…",
                    "maleVariant": "今は現金がない。悪いな…",
                    "stringId": 1945904309545902080
                  }
                }
              ]
            }
          }
        }
      }
    },
