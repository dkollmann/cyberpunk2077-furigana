    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[開ける]",
                    "maleVariant": null,
                    "stringId": 1824513606298771460
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "空じゃない… クソ",
                    "maleVariant": "空じゃないか… クソ",
                    "stringId": 1826986893784260608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やあ、V。久しぶりだな",
                    "maleVariant": null,
                    "stringId": 1827029372168445952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "死んだと思ってたけど、そういうこと…",
                    "maleVariant": "死んだと思ってたが、そういうことか",
                    "stringId": 1827036092215205888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まんまと引っかかったな。俺はアバナシーにリストラされた。家も、アカウントも、インプラントも、全部奪われたんだ…",
                    "maleVariant": null,
                    "stringId": 1827037127067443200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも俺はまだ終わってない。あの女に、自分が間違っていたと証明してやるんだ",
                    "maleVariant": null,
                    "stringId": 1827037292323020800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "つまり、あたしを生贄として差し出すってわけね。そうすればアバナシーは喜んで迎え入れてくれる、そう思ってるんでしょ？",
                    "maleVariant": "つまり、俺を生贄として差し出すってわけだ。そうすれば、アバナシーは喜んで迎え入れてくれる。そう思ってるわけだな",
                    "stringId": 1827037660012486656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アバナシーはあたしになんか興味ない",
                    "maleVariant": "アバナシーは俺になんか興味ない",
                    "stringId": 1827038380912680964
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ねえ、フランク。アバナシーはあたしなんか眼中にない。下手すりゃ生きてることすら知らないかもね",
                    "maleVariant": "なあ、フランク。アバナシーは俺なんか眼中にない。下手すりゃ生きてることすら知らないんじゃないか？",
                    "stringId": 1827038599284924416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、お前はアバナシーを陥れようとした。あの女がそれを忘れたと思うか？",
                    "maleVariant": null,
                    "stringId": 1827038867972038656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "哀れな奴ね",
                    "maleVariant": "哀れな奴だ",
                    "stringId": 1827039328523395072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クビになった日に小便を漏らした、M&A担当のジョーイ・リヴァーニを覚えてる？",
                    "maleVariant": "クビになった日に小便を漏らした、M&A担当のジョーイ・リヴァーニを覚えてるか？",
                    "stringId": 1827045028112769024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いったい何の話してんだ？",
                    "maleVariant": null,
                    "stringId": 1827046239259992064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいえ、あたしじゃない。あたしは使われただけで、あれはジェンキンスの仕業だったのよ",
                    "maleVariant": "いや、俺じゃない。俺は使われただけで、あれはジェンキンスの仕業だからな",
                    "stringId": 1827047177727123456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "相変わらずムカつく奴だが、無駄口叩いてる暇はない。俺はお前を使って返り咲くんだ",
                    "maleVariant": null,
                    "stringId": 1827050036732502016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと、あんたも漏らしてるの？ 勘弁してよ",
                    "maleVariant": "おいおい、お前も漏らしてんのか？ 勘弁しろよ",
                    "stringId": 1827062584110358528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "くたばれ、V！ どいつもこいつもバカにしやがって！",
                    "maleVariant": null,
                    "stringId": 1827063037481066496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなに俺に殺されたいのか？",
                    "maleVariant": null,
                    "stringId": 1827063621915385856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やり直すしかない",
                    "maleVariant": null,
                    "stringId": 1827074289288749060
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしには関係ない",
                    "maleVariant": "俺には関係ない",
                    "stringId": 1827074554033217536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあね。はっきり言って、あたしにはどうでもいい",
                    "maleVariant": "さあな。はっきり言って、俺にはどうでもいい",
                    "stringId": 1827077277327695872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "正直に言ってくれてありがとう。自分の立場がよくわかったよ",
                    "maleVariant": null,
                    "stringId": 1827077962341425152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前の言うとおりだ。アラサカに戻るなんて、間違いなく自殺行為だな",
                    "maleVariant": null,
                    "stringId": 1827078894902976512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハッ。久々にコーポ時代が懐かしくなったか？ 権力争いってのはスリル満点だよな？",
                    "maleVariant": null,
                    "stringId": 1866521648498089984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "口出ししないで",
                    "maleVariant": "口出ししないでくれ",
                    "stringId": 1874812905695006720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "部外者のあんたは黙ってて",
                    "maleVariant": "部外者のお前は黙ってろ",
                    "stringId": 1874813692160561152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お呼びじゃないってんなら、長居する気はないさ",
                    "maleVariant": null,
                    "stringId": 1874813914509004800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "決着つけたいことがある",
                    "maleVariant": null,
                    "stringId": 1874816229446045696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしが決着をつける、それだけよ",
                    "maleVariant": "俺が決着をつける、それだけだ",
                    "stringId": 1874818569548910592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほーら、やっぱり思った通りだ。まだ抜け切れてなかったか",
                    "maleVariant": null,
                    "stringId": 1874819332576694272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "邪魔するなら消えてて。腕にオイルでも差したり、ギターのチューニングでもしてたらいいでしょ",
                    "maleVariant": "邪魔するなら消えてろ。腕にオイルでも差したり、ギターのチューニングでもしてたらいいだろ",
                    "stringId": 1874822824770981888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなところだ。さあ、早く反対を向いて、両手を頭に乗せろ",
                    "maleVariant": null,
                    "stringId": 1874834177611059200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたがここで死ぬって解決策もある",
                    "maleVariant": "お前がここで死ぬって解決策もある",
                    "stringId": 1874835579884326912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "付き合ってあげたいけど、この後やることがあって",
                    "maleVariant": "付き合ってやりたいが、この後やることがあってね",
                    "stringId": 1874836582775967744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何をするってんだ？",
                    "maleVariant": null,
                    "stringId": 1874839278388039680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "すぐにわかるよ",
                    "maleVariant": "すぐにわかるさ",
                    "stringId": 1874839595947184128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あいつがアラサカで一番情けない男だと思ってたけど… これからは違うみたい。だって、あんたがダントツでぶっちぎったからね",
                    "maleVariant": "あいつがアラサカで一番情けない男だと思ってたが… これからは違うみたいだ。だって、お前がダントツでぶっちぎったからな",
                    "stringId": 1874843852209442816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あいつ、膝がガクガク震えて歩けなかったから、警備員に引きずられてたっけ",
                    "maleVariant": null,
                    "stringId": 1874845345230671872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やってみなよ",
                    "maleVariant": "やってみろよ",
                    "stringId": 1874854347800891396
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "目を覚まして",
                    "maleVariant": "目を覚ませ",
                    "stringId": 1874854663548096512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やってみなよ、フランク。ほら、口だけじゃないんでしょ？",
                    "maleVariant": "やってみろよ、フランク。ほら、口だけじゃないんだろ？",
                    "stringId": 1874855261186084864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "目を覚ましてよ、フランク。あんたはアラサカに捨てられたんだ。今さら戻ったところで、連中が話を聞いてくれるはずがないでしょ",
                    "maleVariant": "いい加減目を覚ませよ、フランク。お前はアラサカに捨てられたんだ。今さら戻ったところで、連中が話を聞いてくれるはずがないだろ",
                    "stringId": 1874856565732069376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "出戻りは… たくさんいる",
                    "maleVariant": null,
                    "stringId": 1874857714988142592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そりゃ少しはね。でもフランク、そういう連中がどうなったか、忘れたわけじゃないでしょ？ ホントにそんな道を歩みたいわけ？",
                    "maleVariant": "そりゃ少しはな。だがフランク、そういう連中がどうなったか、忘れたわけじゃないだろ？ ホントにそんな道を歩みたいのか？",
                    "stringId": 1874858488518463488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "決まってるでしょ、フランク。もう一度やり直すの。ゲームオーバーになったら、リスタートするしかない",
                    "maleVariant": "決まってるだろ、フランク。もう一度やり直すんだ。ゲームオーバーになったら、リスタートするしかない",
                    "stringId": 1874859324527136768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やり直すって、俺はすべてを失ったんだぞ！",
                    "maleVariant": null,
                    "stringId": 1874860415348166656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "銃と弾があるんなら、あとはやる気次第で何とでもできる",
                    "maleVariant": null,
                    "stringId": 1874862006314782720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "頑張ってね、フランク",
                    "maleVariant": "頑張れよ、フランク",
                    "stringId": 1874862696210681856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それで、オフィスに戻るの？ 解雇されたあんたが銃を手に現れても、警備員に撃たれるだけよ。ついでにあたしも道連れね",
                    "maleVariant": "で？ これからオフィスに戻るのか？ 解雇されたお前が銃を手に現れても、警備員に撃たれるだけだぞ。ついでに俺も道連れにな",
                    "stringId": 1874899356508016640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "かもな。だが俺にはもう… こうするしか手がないんだ",
                    "maleVariant": null,
                    "stringId": 1874946667971923968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "決意は固いみたいだけど、こっちもあんたと一緒に死ぬ気はない",
                    "maleVariant": "決意は固いみたいだが、こっちもお前と一緒に死ぬ気はない",
                    "stringId": 1874947542517862400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺はどうすればいいんだよ？ もうどうにもならない！",
                    "maleVariant": null,
                    "stringId": 1874950099248140288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "単純な方法が最善という時もある",
                    "maleVariant": null,
                    "stringId": 1874956064387289088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "…クソッ",
                    "maleVariant": null,
                    "stringId": 1874956843756081152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "かつての仲間と思い出に浸んのもいいもんだな。ああ、昔が懐かしいぜ…",
                    "maleVariant": null,
                    "stringId": 1874958555719979008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんな気分じゃないの、ジョニー",
                    "maleVariant": "そんな気分じゃないんだ、ジョニー",
                    "stringId": 1874961933476876288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんでだよ？ 考えてもみろ。お前があいつの立場にいたかもしれないんだぞ",
                    "maleVariant": null,
                    "stringId": 1874962511955283968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃ、俺は家に帰るわ。おやすみ",
                    "maleVariant": null,
                    "stringId": 1874964066062356480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "逃げられると思うなよ！",
                    "maleVariant": null,
                    "stringId": 1934148483825934336
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}