          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時間を延長して、サントドミンゴ発電所襲撃事件についてお伝えします",
              "maleVariant": "",
              "stringId": "1744501745799192576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "発電所では正体不明の侵入者によって冷却システムが破壊され、その結果、EMPが大量に放出される事態となりました",
              "maleVariant": "",
              "stringId": "1744506598827024384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "発電所の襲撃直後、近隣住民からはインプラントに関する深刻な不具合が報告されています",
              "maleVariant": "",
              "stringId": "1744506840989360128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "強力なEMPにより、街全体が何時間も暗闇に包まれたのです",
              "maleVariant": "",
              "stringId": "1744507208863375360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "NCPDはノーマッドによる犯行の可能性が非常に高いと見て、対抗措置を検討中とのことです",
              "maleVariant": "",
              "stringId": "1744507349540331520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また続報が入り次第、ニュースをお届けします",
              "maleVariant": "",
              "stringId": "1744507582273871872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "幸いにも、周辺の集落の多くはEMPの被害を受けずに済みました。施設の損傷規模を考えれば、犠牲者の少なさは奇跡といえるでしょう",
              "maleVariant": "",
              "stringId": "1902236446178463744"
            }
          ]
        }
      }
