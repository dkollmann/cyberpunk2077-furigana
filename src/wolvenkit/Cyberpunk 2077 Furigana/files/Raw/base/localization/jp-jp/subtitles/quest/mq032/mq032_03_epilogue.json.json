  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうだった？ 弟は見つかったか？",
                    "maleVariant": null,
                    "stringId": 1772050242984755200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼は無事よ",
                    "maleVariant": "彼は無事だ",
                    "stringId": 1772050583679680516
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まだ見つかってない",
                    "maleVariant": null,
                    "stringId": 1772051511308730368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、見つけた。彼は無事よ。今は体を休めてる",
                    "maleVariant": "ああ、見つけた。彼は無事だ。今は体を休めてる",
                    "stringId": 1772052257374748672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ありがとう。その善行に天が報いてくださることだろう",
                    "maleVariant": null,
                    "stringId": 1772054096392503296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まだよ。何事も時間がかかるの",
                    "maleVariant": "まだだ。何事も時間がかかる",
                    "stringId": 1772055782653706240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その時間が問題なんだ！ 頼む！ どうか急いでくれ！",
                    "maleVariant": null,
                    "stringId": 1772056140461391872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう戻ったのか？ なぜ一人なんだ？ 弟はどうした？",
                    "maleVariant": null,
                    "stringId": 1772144772144709632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼は死んだ",
                    "maleVariant": null,
                    "stringId": 1772145239725719556
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "誤って殺してしまった",
                    "maleVariant": null,
                    "stringId": 1772145784599363584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "手に負えない状況になって… 残念だけど彼は助からなかった",
                    "maleVariant": "手に負えない状況になって… 残念だが彼は助からなかった",
                    "stringId": 1772146029999702016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼がいるところで銃撃戦になって、あたしの撃った弾が誤って当たってしまった… 本当に申し訳ない",
                    "maleVariant": "彼がいるところで銃撃戦になって、俺の撃った弾が誤って当たってしまった… 本当に申し訳ない",
                    "stringId": 1772146174568972288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "全て拙僧のせいだ。他者の因果に立ち入ったのがよくなかった。拙僧が助けを求めなければ… 弟はまだ生きてたかもしれない",
                    "maleVariant": null,
                    "stringId": 1772146884077436928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "また会ったな。もし気が変わって助けに来たんだとしたら、あいにくだがもう遅い",
                    "maleVariant": null,
                    "stringId": 1772152944746053632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "弟は顎と舌を奪われた。さらに改造を施さなければ、二度と口をきけない",
                    "maleVariant": null,
                    "stringId": 1772153718762913792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やあ、もう一度礼を言わせてくれ。君は多くの命と… 手足を救ってくれた",
                    "maleVariant": null,
                    "stringId": 1772154790894125056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "波羅提木叉の戒めは悟りへの導きであって、絶対の掟ではない。より険しい道を歩むことになったが、拙僧は決して諦めんさ",
                    "maleVariant": null,
                    "stringId": 1772155144037744640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さらばだ。正しき道を外れることのないよう、祈っている",
                    "maleVariant": null,
                    "stringId": 1772156102620758016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おお、また会ったな。もう一度礼を言わせてくれ",
                    "maleVariant": null,
                    "stringId": 1772158451095453696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "兄さん… 礼を言うことなどない。善意があろうと殺生を認められるものか。もう放っておいてくれ",
                    "maleVariant": null,
                    "stringId": 1772158790381092864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "感情に流されてはいかんが… 正直、弟が無事とわかり… 心から安心している",
                    "maleVariant": null,
                    "stringId": 1827056689081991168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいのよ。これからどうするの？ お兄さんは僧侶を続けられる？",
                    "maleVariant": "いいんだ。これからどうする？ お兄さんは僧侶を続けられそうか？",
                    "stringId": 1827078839570513920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "喜んでそうするよ",
                    "maleVariant": "喜んでそうするさ",
                    "stringId": 1827087955890257920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう… 放っておいてくれ、頼む",
                    "maleVariant": null,
                    "stringId": 1827148930920202240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何をされたの？",
                    "maleVariant": "何をされたんだ？",
                    "stringId": 1827190598595756032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "行け。拙僧らは静かに恥を耐え忍ぶのみ",
                    "maleVariant": null,
                    "stringId": 1827191262419861504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ… 拙僧が助けを求めなければ…",
                    "maleVariant": null,
                    "stringId": 1834588230267944960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんな… 今までどんな殺生もしなかった弟が…",
                    "maleVariant": null,
                    "stringId": 1834588812252790784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "コンストラクトについてどう思う？",
                    "maleVariant": null,
                    "stringId": 1915415326246027268
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "では、幸運を",
                    "maleVariant": null,
                    "stringId": 1915415782837960704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一つ教えて… インプラントを否定してるみたいだけど… コンストラクトについてはどう思う？",
                    "maleVariant": "一つ聞きたい… インプラントを否定してるみたいだが… コンストラクトについてはどう思う？",
                    "stringId": 1915415922776719360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それじゃあ、幸運を祈ってるよ",
                    "maleVariant": "それじゃあ、幸運を祈ってる",
                    "stringId": 1915415996982345728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ… 〈Relic〉のことだな？ 最近話題になっているようだが… 一概には語れない複雑な問題だ",
                    "maleVariant": null,
                    "stringId": 1915474267491516416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ある記憶痕跡がとある人物のコピーなのか、あるいはデジタル世界に存在する本人そのものなのかに依るところだ",
                    "maleVariant": null,
                    "stringId": 1915474363994062848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "参考になった",
                    "maleVariant": null,
                    "stringId": 1915474958763147268
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人のコピーはまだ人だと思う？",
                    "maleVariant": "人のコピーはまだ人だと思うか？",
                    "stringId": 1915475053722189824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "記憶痕跡がただのコピーだとしましょ。その場合、それは… 人間だと言える？",
                    "maleVariant": "記憶痕跡がただのコピーだとしよう。その場合、それは… 人間だと言えるか？",
                    "stringId": 1915475194634027008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "記憶痕跡がデジタル化された本人だとして… その行いは正しいと思う？",
                    "maleVariant": "記憶痕跡がデジタル化された本人だとして… その行いは正しいと思うか？",
                    "stringId": 1915475251911442432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そこに確かな自我があるのなら、仏教徒はそれを人間とみなすだろう",
                    "maleVariant": null,
                    "stringId": 1915509591215161344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人間の複雑な精神を完璧に複製したのなら、それはやはり人間の精神だ。だがそれに輪廻転生は可能だろうか？",
                    "maleVariant": null,
                    "stringId": 1915509720097734656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もし不可能だとしたら、そこに魂は存在していると言えるだろうか？",
                    "maleVariant": null,
                    "stringId": 1915509825878081536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "つまり… 一言でいえば「すごく複雑な話」ってことね",
                    "maleVariant": "つまり… 一言でいえば「すごく複雑な話」ってことか",
                    "stringId": 1915509975765729280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "“苦悩”こそが鍵だ。苦しみは人類の誕生から常に存在している。コンストラクトに苦悩があるのなら、それは人間と言える",
                    "maleVariant": null,
                    "stringId": 1915510066899566592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "仏教はデジタル化そのものを咎めはしない。寺院では自動販売機が教えを説いたりしている",
                    "maleVariant": null,
                    "stringId": 1915554436377985024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "デジタルへの転移に罪があるとは思わない。本人が心安らかにそれを受け入れている限りはな",
                    "maleVariant": null,
                    "stringId": 1915554556066643968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だが忘れてはならない。人は体に囚われし存在なのだ… たとえそれが、生身の肉体でなくても",
                    "maleVariant": null,
                    "stringId": 1915554685905518592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "不老不死には否定的ってこと？",
                    "maleVariant": "不老不死には否定的ってことか？",
                    "stringId": 1915555641535741952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "死がなければ、より善き生を重ねることもできず、涅槃に達することも不可能だ",
                    "maleVariant": null,
                    "stringId": 1915555733726543872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人のデジタル化に… 抵抗はない？",
                    "maleVariant": "人のデジタル化に… 抵抗はないか？",
                    "stringId": 1915557152810582016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニーはどう思う？",
                    "maleVariant": null,
                    "stringId": 1915566468695310336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうだかね。考えただけで頭痛がしてくる…",
                    "maleVariant": "どうだかな。考えただけで頭痛がしてくる…",
                    "stringId": 1915566755350822912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どう思う？ あんたはジョニー自身か… それともコピーか？",
                    "maleVariant": "どう思う？ お前はジョニー自身か… それともコピーか？",
                    "stringId": 1915568558029127680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何の違いがある？ どのみち俺は囚われてる。コードの羅列と… お前の体にな",
                    "maleVariant": null,
                    "stringId": 1915588369371086848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "シルヴァーハンドは実は死んでるのかもね",
                    "maleVariant": "シルヴァーハンドは実は死んでるのかもな",
                    "stringId": 1915588474195132416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何だ、俺は偽物だと、そう言いたいのか？",
                    "maleVariant": null,
                    "stringId": 1915588562124521472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "“本物”のシルヴァーハンドが死んでるなら、それはそいつの問題で、俺には関係ない",
                    "maleVariant": null,
                    "stringId": 1925939894228336640
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}