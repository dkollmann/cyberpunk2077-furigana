              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おい、ルシア、仕事は？ 休みでも取ったのか？",
                    "maleVariant": null,
                    "stringId": 1963295426646941696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハッ、少なくとも来週まではね！",
                    "maleVariant": null,
                    "stringId": 1979088912125861888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "停電のおかげでベルトが動かなくなって、直すのに時間がかかってるんだって",
                    "maleVariant": null,
                    "stringId": 1979097681593442304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうか… お気の毒に",
                    "maleVariant": null,
                    "stringId": 1979114021595193344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ん？ どういうこと？",
                    "maleVariant": null,
                    "stringId": 1979114635960066048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "工場の人員を減らす必要があるから、適当な理由をつけてリストラするって聞いてな",
                    "maleVariant": null,
                    "stringId": 1979114968719368192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "え… ちょっと、ウソでしょ？",
                    "maleVariant": null,
                    "stringId": 1979117100533096448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ただのうわさかもしれないが… 念のため、次の仕事を探した方がいいかもな",
                    "maleVariant": null,
                    "stringId": 1979118733442752512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最悪！ せっかくいい気分だったのに、あんたのせいで台無しよ",
                    "maleVariant": null,
                    "stringId": 1979121737772376064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どういたしまして",
                    "maleVariant": null,
                    "stringId": 1979123280085065728
                  }
                }
              ]
            }
