  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "小娘が！ 私を脅迫するなんて、百年早いんだよ！",
              "maleVariant": "ガキが！ 私を脅迫するなんて、百年早いんだよ！",
              "stringId": "1105115033406922752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その男は知らない",
              "maleVariant": "",
              "stringId": "1105115867217780756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そいつ？ 会ったこともない",
              "maleVariant": "そいつ？ 会ったこともないね",
              "stringId": "1105117448688492544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "い、言っただろう！ 私はスパイなんかじゃないって！",
              "maleVariant": "",
              "stringId": "1105120986583916544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "黙らせろ",
              "maleVariant": "",
              "stringId": "1105120986583916556"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仲間がいる",
              "maleVariant": "",
              "stringId": "1105146028659171328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。あたしひとりよ",
              "maleVariant": "ああ。俺ひとりだ",
              "stringId": "1105146774674857984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "事実です",
              "maleVariant": "",
              "stringId": "1105147704921153536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さっさと吐きな！",
              "maleVariant": "",
              "stringId": "1105151360793960448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[握手する] ええ",
              "maleVariant": "[握手する] ああ",
              "stringId": "1136864313654460416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "提案があるんだけど…",
              "maleVariant": "提案があるんだが…",
              "stringId": "1136864854736453648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、そうよ",
              "maleVariant": "ああ、そうだ",
              "stringId": "1136864941155893248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら！ さっさと答えな",
              "maleVariant": "",
              "stringId": "1136900518987251712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう一度聞く。こいつがお前に情報を売ったのか？",
              "maleVariant": "",
              "stringId": "1136900519004028928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "答えな！",
              "maleVariant": "",
              "stringId": "1137046875668561920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうなんだ！ 仲間はいるのか？",
              "maleVariant": "",
              "stringId": "1137046875668561924"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、メレディス…",
              "maleVariant": "",
              "stringId": "1145608297130311680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いけます",
              "maleVariant": "",
              "stringId": "1145609307689140224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[握手する] 提案がある",
              "maleVariant": "",
              "stringId": "1146019556153704448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボットが欲しい。情報と交換しよう",
              "maleVariant": "",
              "stringId": "1151476097103249408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを取る] わかった。それで決まりね",
              "maleVariant": "[チップを取る] わかった。それで決まりだな",
              "stringId": "1151495835917303808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど、わかった。それでいきましょ",
              "maleVariant": "なるほど… わかった。それでいこう",
              "stringId": "1151498488445485056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少しでも妙な真似をしたら、タダじゃおかないよ",
              "maleVariant": "",
              "stringId": "1151517132177096704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、やめておけ！",
              "maleVariant": "",
              "stringId": "1151537177796100096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今すぐ私を解放しないと――",
              "maleVariant": "",
              "stringId": "1175136178700750848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中へのアプローチ方法は？",
              "maleVariant": "",
              "stringId": "1175163198054797328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "次の質問だ。コンボイの情報はこのクズ、アンソニー・ギルクリストから聞いたのか？ こいつとの繋がりは？",
              "maleVariant": "",
              "stringId": "1177806367837851648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ひとりで来た",
              "maleVariant": "",
              "stringId": "1177964105545502720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何とか言ったらどうだ！",
              "maleVariant": "",
              "stringId": "1178029715163926528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前は黙ってろ！",
              "maleVariant": "",
              "stringId": "1178030944229216256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チップに細工してるんでしょ",
              "maleVariant": "チップに細工してるんだろ",
              "stringId": "1188000879964901376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミリテクじゃ、そんな見え透いた手を教わるの？ どうせウイルス入りなんでしょ",
              "maleVariant": "ミリテクじゃ、そんな見え透いた手を教わるのか？ どうせウイルス入りなんだろ",
              "stringId": "1188006148597927936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのとおりだ",
              "maleVariant": "",
              "stringId": "1188019894321991680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メレディス・スタウトだ。電話してきたのはお前か…",
              "maleVariant": "",
              "stringId": "1197934265076748288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだなのか？",
              "maleVariant": "",
              "stringId": "1244523156621430784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "質問に答えてもらうよ。ウソはすぐにわかる。ひとりで来たのか？",
              "maleVariant": "",
              "stringId": "1244524702289563648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたが追い込まれてるのはわかってる",
              "maleVariant": "",
              "stringId": "1244530099234738176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ひとりぼっちはそっちでしょ",
              "maleVariant": "ひとりぼっちはそっちだろ",
              "stringId": "1244531614167011328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "調子に乗るなよ",
              "maleVariant": "",
              "stringId": "1244535071196254208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当です",
              "maleVariant": "",
              "stringId": "1244552824795893760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そいつを解放してやれ、話を聞くとしよう",
              "maleVariant": "",
              "stringId": "1244566971461865472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボット――フラットヘッドを手に入れたい",
              "maleVariant": "",
              "stringId": "1244575184630517760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なら… 後者でいけ。ただし条件がある… 支払いにはこちらの金を使え",
              "maleVariant": "",
              "stringId": "1244592893367881728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミリテクと繋がりはない",
              "maleVariant": "",
              "stringId": "1245504548914675712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "放して！",
              "maleVariant": "放せ！",
              "stringId": "1245767233090293760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、放してよ！",
              "maleVariant": "ふざけるな、放せ！",
              "stringId": "1245767273573715968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話がしたいだけよ！",
              "maleVariant": "話がしたいだけだ！",
              "stringId": "1245767672334585856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着いて！ 話をしにきただけよ！",
              "maleVariant": "落ち着け！ 話をしにきただけだ！",
              "stringId": "1245770292465963008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつはもう死んだも同然だ！",
              "maleVariant": "",
              "stringId": "1246073684576899072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "道連れにされるぞ！",
              "maleVariant": "",
              "stringId": "1246073888403296256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シャキっとしな",
              "maleVariant": "",
              "stringId": "1248996301877538816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪いけど、その条件は呑めない。取引はなしよ",
              "maleVariant": "悪いが、その条件は呑めない。取引はなしだ",
              "stringId": "1669616323716145152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "貴様… 後悔することになるぞ",
              "maleVariant": "",
              "stringId": "1669618779179147264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その条件は呑めない",
              "maleVariant": "",
              "stringId": "1669630298767974400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ひとりじゃない。50口径のライフルを持った狙撃手が、そこら辺にいる",
              "maleVariant": "ひとりじゃない。50口径のライフルを持った狙撃手がそこら辺にいる",
              "stringId": "1717157967487000576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "嘘です",
              "maleVariant": "",
              "stringId": "1717157967503777796"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今度嘘をついたら、電撃を食らわせる。わかったか？",
              "maleVariant": "",
              "stringId": "1717157967503777816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミリテクに知り合いなんていない",
              "maleVariant": "",
              "stringId": "1717169829599469568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当です",
              "maleVariant": "",
              "stringId": "1717169829599469584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スパイ探しに協力してもいい",
              "maleVariant": "",
              "stringId": "1718792967265411072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ギルクリストなんて名前の奴は知らない。それがそこの気の毒な奴？",
              "maleVariant": "ギルクリストなんて名の奴は知らない。それがそこの気の毒な奴か？",
              "stringId": "1718793732759445504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当に知らないようです",
              "maleVariant": "",
              "stringId": "1718793732759445520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたらのコンボイはセキュリティがなってない。あたしがアドバイスしてあげようか",
              "maleVariant": "あんたらのコンボイはセキュリティがなってない。俺がアドバイスしてやろうか",
              "stringId": "1718796709809315840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "余計なお世話だよ。時間がないんだ、本題に入りな",
              "maleVariant": "",
              "stringId": "1718796709809315848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コンボイのセキュリティを改善した方がいい",
              "maleVariant": "",
              "stringId": "1718797027838222336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし。クレジットチップのウイルスは取り除いた",
              "maleVariant": "よし。クレジットチップのウイルスは除去できた",
              "stringId": "1720206704136380416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしに手を出せば、頭が粉々に吹き飛ぶことになる",
              "maleVariant": "俺に手を出せば、頭が粉々に吹き飛ぶことになるぞ",
              "stringId": "1742933181225431040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブツを取り返せなかったら、あんたはおしまいよ。助けてくれる味方もいない",
              "maleVariant": "ブツを取り返せなかったら、あんたはおしまいだ。助けてくれる味方もいない",
              "stringId": "1753441941228285952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "にしては随分と詳しいじゃないか",
              "maleVariant": "",
              "stringId": "1753478589227921408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業なんてどこも似たり寄ったり。面白みに欠けるくらいよ",
              "maleVariant": "企業なんてどこも似たり寄ったりだ。面白みに欠けるくらいな",
              "stringId": "1753512905764958208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コンボイの襲撃に関しては経験があるの。関わってそうな連中もよく知ってるし、スパイ探しに協力してもいい",
              "maleVariant": "コンボイの襲撃に関しては経験がある。関わってそうな連中もよく知ってるし、スパイ探しに協力してもいい",
              "stringId": "1753513684261335040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもあたしが今持ってる情報だけじゃ、力になれない",
              "maleVariant": "だが俺が今持ってる情報だけじゃ、力になれない",
              "stringId": "1753517710625402880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何かあった時には、あたしがハチの巣にされるのよ。正直に答えて。どんな細工がしてあるの？",
              "maleVariant": "何かあった時には、俺がハチの巣にされるんだぞ。正直に答えろ。どんな細工がしてあるんだ？",
              "stringId": "1754634957850865664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "金の流れを追跡するアルゴリズムが仕込んであるだけだ。何も心配しなくていい",
              "maleVariant": "",
              "stringId": "1754635755976585216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "支払いにはそのチップを使うこと。それ以外は考えるな",
              "maleVariant": "",
              "stringId": "1754642477466185728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "力尽くで奪うこともできるけど、向こうはボットを売る気でいる。取引してもいい",
              "maleVariant": "力尽くで奪うこともできるが、向こうはボットを売る気でいる。取引してもいい",
              "stringId": "1754643739196715008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブツがどこにあるかは把握してる。手を貸してもいいけど、こっちにも条件がある",
              "maleVariant": "ブツがどこにあるかは把握してる。手を貸してもいいが、こっちにも条件がある",
              "stringId": "1762051917446311936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん…",
              "maleVariant": "",
              "stringId": "1762060493590032384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コンボイを襲った奴らが持ってる。ボットをくれるなら、奴らの情報をやる",
              "maleVariant": "",
              "stringId": "1762062887027666944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを取る] わかった。取引成立ね",
              "maleVariant": "[チップを取る] わかった。取引成立だな",
              "stringId": "1762078548592357380"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "取引はなしよ",
              "maleVariant": "取引はなしだ",
              "stringId": "1762078923345031168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スタウト！ お前はおしまいだ！ 覚悟するんだな！",
              "maleVariant": "",
              "stringId": "1768290553269280768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手下のお前らもだ！ おい、聞いてるのか！",
              "maleVariant": "",
              "stringId": "1768291603640442880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "周囲を捜索しろ",
              "maleVariant": "",
              "stringId": "1996859587179769856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "放して",
              "maleVariant": "放せ",
              "stringId": "1996862512891359232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "放して。落ち着いて話しましょ",
              "maleVariant": "放せよ。落ち着いて話そう",
              "stringId": "1996862711986581504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "質問に答えろ！ ギルクリストを知ってるのか！？",
              "maleVariant": "",
              "stringId": "1996863229479809024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最初から普通に話せなかったわけ？",
              "maleVariant": "最初から普通に話せなかったのか？",
              "stringId": "1996869768382967808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつもこんな強引なの？",
              "maleVariant": "いつもこんな強引なのか？",
              "stringId": "1996870099917533184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "黙れ。質問をしてるのはこっちだ",
              "maleVariant": "",
              "stringId": "1996870728996024320"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}