          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "T-バグのこと、どれくらい知ってる？",
                    "maleVariant": "T-バグのこと、どれくらい知ってるんだ？",
                    "stringId": 1906683030939004932
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ねえ。バグってどんな奴なの？",
                    "maleVariant": "なあ。バグってどんな奴なんだ？",
                    "stringId": 1906683674395570176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺に惚れてるのは間違いねえ…",
                    "maleVariant": null,
                    "stringId": 1906684290371055616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こちらの番号は現在お繋ぎできません",
                    "maleVariant": null,
                    "stringId": 1958849632124293120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "メッセージをお残しください",
                    "maleVariant": null,
                    "stringId": 1958856532123471872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミスティに会った",
                    "maleVariant": null,
                    "stringId": 1958857273173102596
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたの葬式に行ってきた",
                    "maleVariant": "お前の葬式に行ってきた",
                    "stringId": 1958857798585176064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やめよう、こんなこと",
                    "maleVariant": null,
                    "stringId": 1958858045696790528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "えと… あんたのガレージで、ミスティと話をしたよ",
                    "maleVariant": "えと… お前のガレージで、ミスティと話をしたよ",
                    "stringId": 1958868465891430400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こんなこと言うのも変だけど、あんたの葬式に行ってきた…",
                    "maleVariant": "こんなこと言うのも変だけど、お前の葬式に行ってきた…",
                    "stringId": 1958868840795099140
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この話を聞いたら、きっとびっくりするよ",
                    "maleVariant": "この話を聞いたら、きっとびっくりするぞ",
                    "stringId": 1958868968520044548
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ったく、バカみたい…",
                    "maleVariant": "ったく、何やってんだ…",
                    "stringId": 1958869209726078980
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたが作った曼荼羅…",
                    "maleVariant": "お前が作った曼荼羅…",
                    "stringId": 1958889869374410752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でもミスティはすごいって",
                    "maleVariant": "でも… ミスティはすごいって",
                    "stringId": 1958898460600848384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だから… 向こうでも続けてね",
                    "maleVariant": "だから… 向こうでも続けろよ",
                    "stringId": 1958899248727347200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あれ、ひどすぎない",
                    "maleVariant": "あれ、ひどすぎないか",
                    "stringId": 1958899481578328064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ならすごいのかも",
                    "maleVariant": "ならすごいのかもな",
                    "stringId": 1958900019640422400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ママ・ウェルズがすごく…  悲しんでたよ。でもきっと大丈夫",
                    "maleVariant": "ママ・ウェルズがすごく…  悲しんでたよ。でもきっと大丈夫だ",
                    "stringId": 1958916454903205888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "賑やかだったよ。カンポ・オルタまで代理人をよこしたのには驚いた",
                    "maleVariant": null,
                    "stringId": 1958929109537804288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そいつの弔辞は棒読みだったけど、それでも敬意と、 温もりはこもってたかな…",
                    "maleVariant": "そいつの弔辞は棒読みだったが、それでも敬意と、温もりはこもってたな…",
                    "stringId": 1958930763821633536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "見せたかったよ、ジャック。きっと喜んだと思う",
                    "maleVariant": null,
                    "stringId": 1958931473430761472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "すごいことがあったよ",
                    "maleVariant": "すごいことがあったんだ",
                    "stringId": 1958936889485185024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今どこにいるの",
                    "maleVariant": "今どこにいるんだ",
                    "stringId": 1958937910563328004
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お袋さんと話をした",
                    "maleVariant": null,
                    "stringId": 1958939086310625280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この話をしたらきっと驚く",
                    "maleVariant": null,
                    "stringId": 1958939406721896448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やめよう、こんなこと",
                    "maleVariant": null,
                    "stringId": 1958940317372403712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これが最後の電話よ",
                    "maleVariant": "これが最後の電話だ",
                    "stringId": 1958940567118041088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジャッキー、これが最後の電話ね",
                    "maleVariant": "ジャッキー、これが最後の電話だ",
                    "stringId": 1958951306247557120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "…お母さんと話したよ",
                    "maleVariant": "…お袋さんと話したよ",
                    "stringId": 1958951364347056132
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジャック、あんたは一体今どこで何をしてるの…",
                    "maleVariant": "ジャック、お前は一体今どこで何をしてる…",
                    "stringId": 1958951875968258052
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こっちはもう無茶苦茶",
                    "maleVariant": "こっちはもう無茶苦茶だ",
                    "stringId": 1958957763395342336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もしあんたが… 傍にいてくれたら…",
                    "maleVariant": "もしお前が… 傍にいてくれたら…",
                    "stringId": 1958959576425189376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ねえ、ジャッキー…",
                    "maleVariant": "なあ、ジャッキー…",
                    "stringId": 1958960253301968896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ったく… 何やってるの、あたし",
                    "maleVariant": "ったく… 何やってんだ、俺は",
                    "stringId": 1958962979012366336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これが最後の電話よ",
                    "maleVariant": "これが最後の電話だ",
                    "stringId": 1958967545602789376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "デクスに狙われて、死んだと思ったら生き返って、一安心してたら… 頭の中に時限爆弾があることがわかったの",
                    "maleVariant": "デクスに狙われて、死んだと思ったら生き返って、一安心してたら… 頭の中に時限爆弾があることがわかったんだ",
                    "stringId": 1958969201748889600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "売れない映画の脚本みたいでしょ",
                    "maleVariant": "売れない映画の脚本みたいだろ",
                    "stringId": 1958970562968309760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたが今どこで何をしてるのかは知らないけど、しっかりね、チーカ",
                    "maleVariant": "お前が今どこで何をしてるのかは知らないが、しっかりな、ブロダー",
                    "stringId": 1958971257830264832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうそう、それとジョニー・シルヴァーハンドって奴に会ったよ。あんたとは馬が合わないかも",
                    "maleVariant": "そうそう、ジョニー・シルヴァーハンドって奴に会ったよ。お前とは馬が合わないかも",
                    "stringId": 1958975369389035520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "絶対毎日喧嘩になるでしょうね",
                    "maleVariant": "絶対毎日喧嘩になるだろうな",
                    "stringId": 1958976131628290048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もうあまり時間がない",
                    "maleVariant": null,
                    "stringId": 1958980924409139200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうなるかわからないけど、いよいよ決着がつく",
                    "maleVariant": null,
                    "stringId": 1958982798793265152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これは独り言だってわかってる。ただ、その… あんたはいい友達だって言いたかったの",
                    "maleVariant": "これは独り言だってわかってる。ただ、その… お前はいい友達だって言いたかったんだ",
                    "stringId": 1958984607494590464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いなくなってもそう感じる",
                    "maleVariant": null,
                    "stringId": 1958985171376820224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それじゃね、チューマ",
                    "maleVariant": "それじゃあな、チューマ",
                    "stringId": 1958986419182260224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "すごく悲しんでた。でもきっと大丈夫",
                    "maleVariant": "すごく… 悲しんでた。でもきっと大丈夫だ",
                    "stringId": 1959005206073856000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしは何にもしてあげられないけど… たまに顔を出すね",
                    "maleVariant": "俺は何にもしてあげられないが… たまに顔を出すよ",
                    "stringId": 1959005768580354048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "任しといて",
                    "maleVariant": "任しとけって",
                    "stringId": 1959087328298029056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんてな、冗談だ。気軽に飲みに誘えるタイプじゃなさそうだろう",
                    "maleVariant": null,
                    "stringId": 1959107456544821248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なるほど",
                    "maleVariant": null,
                    "stringId": 1959114288378171392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうした？",
                    "maleVariant": null,
                    "stringId": 2015561179233951744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よう、調子はどうだ？",
                    "maleVariant": null,
                    "stringId": 2015562027640987652
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミスティとはその… 真剣なの？",
                    "maleVariant": "ミスティとはその… 真剣なのか？",
                    "stringId": 2015651595207745536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お母さんは元気？",
                    "maleVariant": "お母さんは元気か？",
                    "stringId": 2015652007944036352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミスティと結構続いてるじゃない",
                    "maleVariant": "ミスティと結構続いてるじゃないか",
                    "stringId": 2015652467958521856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お母さんはどうしてる？",
                    "maleVariant": null,
                    "stringId": 2015663563352219656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう1年かな… それがどうした？",
                    "maleVariant": null,
                    "stringId": 2027289569756377088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "別に。あんたがこんなに長く、誰かと落ち着いて付き合えるなんてね",
                    "maleVariant": "別に。お前がこんなに長く、誰かと落ち着いて付き合えるなんてな",
                    "stringId": 2027289948753686528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "実際、浮いた話があるようには思えん。その気配すら感じねえ",
                    "maleVariant": null,
                    "stringId": 2028581704057155584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あのな、チーカ。俺は結構マジメなんだぜ",
                    "maleVariant": "あのな、ブロダー。俺は結構マジメなんだぜ",
                    "stringId": 2028600518010400768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ずっと、俺のことを心配してるよ",
                    "maleVariant": null,
                    "stringId": 2028635009449914368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何かあったの？",
                    "maleVariant": "何かあったのか？",
                    "stringId": 2028635584606433280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "仕事が波に乗るほど敵も増える。つまり、いつ背中を刺されてもおかしくないだろ",
                    "maleVariant": null,
                    "stringId": 2028637207684640768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "デカいヤマには危険がつきものだ。おふくろもそれをわかってるのさ",
                    "maleVariant": null,
                    "stringId": 2028637707108806656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そろそろ切るね。じゃあ、また",
                    "maleVariant": "そろそろ切るぞ。じゃあ、また",
                    "stringId": 2253377721458049024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、じゃあな",
                    "maleVariant": null,
                    "stringId": 2253378878733631488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ、またね",
                    "maleVariant": "じゃあ、またな",
                    "stringId": 2253404284203384832
                  }
                }
              ]
            }
          }
        }
