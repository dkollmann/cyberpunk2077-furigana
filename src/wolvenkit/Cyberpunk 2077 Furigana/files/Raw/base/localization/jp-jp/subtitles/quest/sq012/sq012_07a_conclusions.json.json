    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1735739653548003328,
            "femaleVariant": "まだこいつと一緒にいたのか"
          },
          {
            "stringId": 1740123369800060928,
            "femaleVariant": "黙れ、ハロルド。呼んだ理由はわかってるだろう"
          },
          {
            "stringId": 1740152551133270016,
            "femaleVariant": "隠蔽工作をした理由は聞かない。そもそもどうして関わったのかもな。俺が知りたいのは、誰に指示されたかってことだけだ"
          },
          {
            "stringId": 1740220064193957888,
            "femaleVariant": "聞いてどうする、リバー？ 手を汚していないお前には関係ないだろう"
          },
          {
            "stringId": 1740220558652067840,
            "femaleVariant": "その役目は、俺が買って出てやった"
          },
          {
            "stringId": 1740220976304082944,
            "femaleVariant": "お前は、この街の仕組みをわかってない。理想ばっか追い求めて、現実を見ることを拒否してるだけなんだよ。おめでたい奴だ"
          },
          {
            "stringId": 1740222373477076992,
            "femaleVariant": "そんな目で見るなよ、傭兵。俺が正しいのは知ってるだろ"
          },
          {
            "stringId": 1740225881123180544,
            "femaleVariant": "あたしを巻き込まないで",
            "maleVariant": "俺を巻き込むな"
          },
          {
            "stringId": 1740226496360468480,
            "femaleVariant": "ええ。生まれも育ちもナイトシティだから",
            "maleVariant": "ああ。生まれも育ちもナイトシティだからな"
          },
          {
            "stringId": 1740226781287927808,
            "femaleVariant": "この街の仕組みなら心得てる"
          },
          {
            "stringId": 1740227028701532160,
            "femaleVariant": "さあ、あたしはここの出身じゃない",
            "maleVariant": "さあ、俺はここの出身じゃない"
          },
          {
            "stringId": 1740235800383705088,
            "femaleVariant": "この街は知り尽くしてる"
          },
          {
            "stringId": 1740236080881979392,
            "femaleVariant": "ナイトシティで生きていくためのルールも、暗黙の了解もすべて把握してる"
          },
          {
            "stringId": 1740236317138735104,
            "femaleVariant": "さあ、都会のことはよく知らない",
            "maleVariant": "さあな、都会のことはよく知らない"
          },
          {
            "stringId": 1740236566750154752,
            "femaleVariant": "巻き込まないでよ。政治も警察の内輪もめも、あたしには関係ない",
            "maleVariant": "巻き込まないでくれ。政治も警察の内輪もめも、俺には関係ない"
          },
          {
            "stringId": 1740237178850103296,
            "femaleVariant": "じゃあ仕組みもわかるだろう。市庁舎もストリートも、ルールは一緒だ。ラインもそれを知っていた"
          },
          {
            "stringId": 1740238990655213568,
            "femaleVariant": "なら、時には長いものに巻かれる必要があることもわかってるだろう"
          },
          {
            "stringId": 1740240855140782080,
            "femaleVariant": "なるほど。ノーマッドか"
          },
          {
            "stringId": 1740241546278195200,
            "femaleVariant": "NCPDはいわば俺のファミリー、俺のクランだ"
          },
          {
            "stringId": 1740242428810088448,
            "femaleVariant": "じゃあ、なぜここにいる？ どうせ金のためなんだろ？ いかにも傭兵らしいな… とはいえ、別にお前を批判するつもりはない"
          },
          {
            "stringId": 1740243113203060736,
            "femaleVariant": "誰もが皆、この街で生きる道と居場所を探してる"
          },
          {
            "stringId": 1740252685795971076,
            "femaleVariant": "確かにファミリーが第一よ",
            "maleVariant": "確かにファミリーが第一だ"
          },
          {
            "stringId": 1740254689247219712,
            "femaleVariant": "ファミリーは守るためにある。身内を裏切ったりなんかしない"
          },
          {
            "stringId": 1740257095469092864,
            "femaleVariant": "ふざけないで",
            "maleVariant": "ふざけるな"
          },
          {
            "stringId": 1740261184781275136,
            "femaleVariant": "確かにファミリーが第一よ。それは間違いない",
            "maleVariant": "確かにファミリーが第一だ。それは間違いない"
          },
          {
            "stringId": 1740261683953782784,
            "femaleVariant": "この街は、はぐれ者を食い物にする"
          },
          {
            "stringId": 1740262999069413376,
            "femaleVariant": "ふざけないで。街の警官とクランじゃ、結束の意味が違う",
            "maleVariant": "ふざけるなよ。街の警官とクランじゃ、結束の意味が違う"
          },
          {
            "stringId": 1740263651451457536,
            "femaleVariant": "そうか？ 忠誠心、個人を超えた理想のために尽くすこと… 何が違う？"
          },
          {
            "stringId": 1740335408459927556,
            "femaleVariant": "風向きを読まないとね",
            "maleVariant": "風向きを読まないとな"
          },
          {
            "stringId": 1740335718452547584,
            "femaleVariant": "それでも超えてはいけない一線がある"
          },
          {
            "stringId": 1740336103959416832,
            "femaleVariant": "風向きを読まないとね。じゃないと、いつの間にか嵐に巻き込まれる",
            "maleVariant": "風向きを読まないとな。じゃないと、いつの間にか嵐に巻き込まれる"
          },
          {
            "stringId": 1740339530940665856,
            "femaleVariant": "それでも、超えてはいけない一線があるでしょ",
            "maleVariant": "それでも、超えてはいけない一線があるだろ"
          },
          {
            "stringId": 1740340225618071552,
            "femaleVariant": "俺は自分の立場というものを理解している。引退する日が来るまで、上に逆らうことはない"
          },
          {
            "stringId": 1740341604319350784,
            "femaleVariant": "お前も見習え、ウォード"
          },
          {
            "stringId": 1740341887938187268,
            "femaleVariant": "ラインはうまくやってた"
          },
          {
            "stringId": 1740342371742765056,
            "femaleVariant": "政治には興味がない"
          },
          {
            "stringId": 1740342674303078400,
            "femaleVariant": "ラインは嫌な奴だったけど、やることはやってた。あいつが当選する前はひどかったでしょ",
            "maleVariant": "ラインは嫌な奴だったが、やることはやってた。あいつが当選する前はひどかっただろ"
          },
          {
            "stringId": 1740343624950468608,
            "femaleVariant": "新合衆国がナイトシティを併合しようとした時、ミリテクのパンツァーを止めたしね",
            "maleVariant": "新合衆国がナイトシティを併合しようとした時、ミリテクのパンツァーを止めたしな"
          },
          {
            "stringId": 1740345224054034432,
            "femaleVariant": "政治には興味がない。市長なんてどいつもクズよ。違うのはスーツくらい",
            "maleVariant": "政治には興味がない。市長なんてどいつもクズだ。違うのはスーツくらいで"
          },
          {
            "stringId": 1740347465053884416,
            "femaleVariant": "ナイトシティが新合衆国に乗っ取られそうになった時、ミリテクはパンツァーを送り込んできた"
          },
          {
            "stringId": 1740348082186997760,
            "femaleVariant": "それを止めたのがラインだ。しかし人々はその偉大な功績なんかすっかり忘れ、スキャンダルにばかり食いつく"
          },
          {
            "stringId": 1740349327425523716,
            "femaleVariant": "生きる道と居場所――確かにそうね",
            "maleVariant": "生きる道と居場所――確かにそうだな"
          },
          {
            "stringId": 1740349794620657664,
            "femaleVariant": "あたしのことなんて何も知らないくせに",
            "maleVariant": "俺のことなんて何も知らないくせに"
          },
          {
            "stringId": 1740350052218032128,
            "femaleVariant": "確かに、それはあんたの言う通りよ",
            "maleVariant": "確かに、それはあんたの言う通りだ"
          },
          {
            "stringId": 1740350940202520576,
            "femaleVariant": "なら… お互いわかり合えるな"
          },
          {
            "stringId": 1740352544221810688,
            "femaleVariant": "あんたにあたしの何がわかるの？ 勝手なこと言わないで",
            "maleVariant": "あんたに俺の何がわかるんだ？ 勝手なこと言うな"
          },
          {
            "stringId": 1740353020292091904,
            "femaleVariant": "そんなつもりじゃなかった"
          },
          {
            "stringId": 1740357215334289408,
            "femaleVariant": "V、手伝ってくれて感謝するが… これは俺の問題だ。自分で片付ける"
          },
          {
            "stringId": 1740374569954955264,
            "femaleVariant": "BDの記録と、お前自らの証言もある。逮捕しようと思えば、十分できるさ"
          },
          {
            "stringId": 1740375730938302464,
            "femaleVariant": "でも意味がない。どうせお前は口を割らないだろうが、揉み消すよう命じた奴の検討はついてる"
          },
          {
            "stringId": 1740375820662853632,
            "femaleVariant": "うちの上層部が絡んでるとしたら、監査部に持ち込むしかない"
          },
          {
            "stringId": 1740377666810925056,
            "femaleVariant": "監査部だって何もしやしないさ――俺にはな。お前が目を付けられるだけだ…"
          },
          {
            "stringId": 1740378094160171008,
            "femaleVariant": "ウォード、とにかく家に帰って少し寝ろ。朝には気が楽になってるだろう"
          },
          {
            "stringId": 1740379528410812416,
            "femaleVariant": "少なくとも、俺はそうする"
          },
          {
            "stringId": 1741878788432187396,
            "femaleVariant": "どうする気？",
            "maleVariant": "どうする気だ？"
          },
          {
            "stringId": 1741880361363296256,
            "femaleVariant": "クソ…"
          },
          {
            "stringId": 1741882020814508032,
            "femaleVariant": "はあ…"
          },
          {
            "stringId": 1741883052059643904,
            "femaleVariant": "で、どうするの？",
            "maleVariant": "で、どうするんだ？"
          },
          {
            "stringId": 1741887617374998528,
            "femaleVariant": "諦めるわけにはいかない。絶対に"
          },
          {
            "stringId": 1750433448340344836,
            "femaleVariant": "そんなところでしょうね",
            "maleVariant": "そんなところだろうな"
          },
          {
            "stringId": 1750433794605305856,
            "femaleVariant": "証拠はない"
          },
          {
            "stringId": 1750434187980689408,
            "femaleVariant": "ええ。ラインの死で得したのはホルトだけだもの",
            "maleVariant": "ああ。ラインの死で得したのはホルトだけだからな"
          },
          {
            "stringId": 1750434666383003648,
            "femaleVariant": "仮にそうだとしても… 証拠は何もない"
          },
          {
            "stringId": 1750435264809521152,
            "femaleVariant": "監査部に持ち込もうと思う"
          },
          {
            "stringId": 1750435921905963012,
            "femaleVariant": "そうね",
            "maleVariant": "そうだな"
          },
          {
            "stringId": 1750436216094445568,
            "femaleVariant": "やめた方がいい"
          },
          {
            "stringId": 1750436385242337280,
            "femaleVariant": "きっと捜査も再開される"
          },
          {
            "stringId": 1750436772712140800,
            "femaleVariant": "そうね",
            "maleVariant": "そうだな"
          },
          {
            "stringId": 1750437786022432768,
            "femaleVariant": "ダメよ。クビどころか、消されるかもしれない",
            "maleVariant": "よせ。クビどころか、消されるかもしれないぞ"
          },
          {
            "stringId": 1750438728079888384,
            "femaleVariant": "それに、きっと何も変わらない"
          },
          {
            "stringId": 1750450112561795072,
            "femaleVariant": "お前は？ ペラレスたちに報告するのか？"
          },
          {
            "stringId": 1941590048749871104,
            "femaleVariant": "そりゃ、時にはモラルに反するようなこともしたが、すべては街のためにやったことだ"
          },
          {
            "stringId": 1941590738712879104,
            "femaleVariant": "彼の名を汚すわけにはいかなかった"
          },
          {
            "stringId": 1941609703191306240,
            "femaleVariant": "うかつに首をつっこんだ瞬間、頭ごと食いつくされるのがオチだ"
          },
          {
            "stringId": 1941650466574917632,
            "femaleVariant": "どうやってだ？ 俺を逮捕するつもりか？"
          },
          {
            "stringId": 1941686636960768000,
            "femaleVariant": "ハンはある意味正しかった"
          },
          {
            "stringId": 1941686832415334400,
            "femaleVariant": "撃てばよかったのに"
          },
          {
            "stringId": 1941691361626120192,
            "femaleVariant": "ハンはある意味正しかった。あいつは命令に従ってただけ。告発しても、システムは変わらない",
            "maleVariant": "ハンはある意味正しかった。あいつは命令に従ってただけだ。告発しても、システムは変わらない"
          },
          {
            "stringId": 1941691395482542080,
            "femaleVariant": "いっそ、お別れの弾を撃ち込んでやるべきだったのよ",
            "maleVariant": "いっそ、お別れの弾を撃ち込んでやるべきだったんだ"
          },
          {
            "stringId": 1941694545857830916,
            "femaleVariant": "ホルトは自らが市長になりたいがために、ラインを殺した。ありがちな動機だ。そして――"
          },
          {
            "stringId": 1941694545857830928,
            "femaleVariant": "NCPDを使って隠蔽した"
          },
          {
            "stringId": 1941696157695303680,
            "femaleVariant": "諦められないんだ、V。そのつもりもない"
          },
          {
            "stringId": 1941697452762161152,
            "femaleVariant": "そうかもな"
          },
          {
            "stringId": 1941697935727878144,
            "femaleVariant": "でも俺のやり方じゃない"
          },
          {
            "stringId": 1941702374526246912,
            "femaleVariant": "違うと思う"
          },
          {
            "stringId": 1941702961829470208,
            "femaleVariant": "いえ、違う… そうじゃない",
            "maleVariant": "いや、違う… そうじゃない"
          },
          {
            "stringId": 1941726386681765888,
            "femaleVariant": "細工されたBDがある。あとは誰の仕業なのかを突き止めるだけだ"
          },
          {
            "stringId": 1941731041352572928,
            "femaleVariant": "じゃあ何があったんだ？"
          },
          {
            "stringId": 1941731117957341184,
            "femaleVariant": "わからないけど… ハンとホルトじゃない、真の黒幕がいる気がするの",
            "maleVariant": "わからないが… ハンとホルトじゃない、真の黒幕がいる気がするんだ"
          },
          {
            "stringId": 1941731730896150528,
            "femaleVariant": "手が出せないような誰かがね",
            "maleVariant": "手が出せないような誰かがな"
          },
          {
            "stringId": 1941746597237579776,
            "femaleVariant": "お前やハンの言うとおりかもな"
          },
          {
            "stringId": 1941746972493570048,
            "femaleVariant": "だが、そんなものはクソ食らえだ"
          },
          {
            "stringId": 1941751459895533568,
            "femaleVariant": "ええ",
            "maleVariant": "ああ"
          },
          {
            "stringId": 1941751570893594624,
            "femaleVariant": "そうか。俺はもう少しここにいる。ありがとうな、V"
          },
          {
            "stringId": 2259012382972563460,
            "femaleVariant": "[降りる]"
          }
        ]
      }
    }
  }
}