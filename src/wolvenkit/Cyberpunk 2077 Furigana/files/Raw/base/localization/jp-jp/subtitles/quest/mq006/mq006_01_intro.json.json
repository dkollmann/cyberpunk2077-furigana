      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジェットコースターに何してるの？",
              "maleVariant": "ジェットコースターに何してるんだ？",
              "stringId": "1832746423922216964"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘイウッド",
              "maleVariant": "",
              "stringId": "1832746770522722304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘイウッドから",
              "maleVariant": "",
              "stringId": "1832747160693657600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[話す]",
              "maleVariant": "",
              "stringId": "1832748422138974208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつでも来てくれていいのよ",
              "maleVariant": "",
              "stringId": "1832749029860069380"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[話す]",
              "maleVariant": "",
              "stringId": "1832752007430033408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、どっか行こうよ",
              "maleVariant": "",
              "stringId": "1832752435232264192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジェットコースターをどうする気？",
              "maleVariant": "ジェットコースターをどうする気だ？",
              "stringId": "1832757185382207488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろん、乗るんだ。ジャン直したらな",
              "maleVariant": "",
              "stringId": "1832757456955002880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう3時間もやってるんだから、さすがにそろそろ終わるだろ",
              "maleVariant": "",
              "stringId": "1832758058283978752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのな、だったら自分でやったらどうなんだ！",
              "maleVariant": "",
              "stringId": "1832758725731962880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "出身地なんて関係ないでしょ？",
              "maleVariant": "出身地なんて関係ないだろ？",
              "stringId": "1832761413928505344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "出身なんて関係ある？",
              "maleVariant": "出身なんて関係あるか？",
              "stringId": "1832761572489973764"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お…！ 今度はいけたんじゃ？ ダメかな？",
              "maleVariant": "",
              "stringId": "1832766698986426368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、もう少し",
              "maleVariant": "",
              "stringId": "1832767636430147584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "直そうとしてるの？",
              "maleVariant": "直そうとしてるのか？",
              "stringId": "1832768811439247364"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こういうのには、一度も乗ったことがない",
              "maleVariant": "",
              "stringId": "1832772367470841856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一度も乗ったことがない",
              "maleVariant": "",
              "stringId": "1832773586570465284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんな調子？",
              "maleVariant": "どんな調子だ？",
              "stringId": "1832774699973627904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "修理して動かすつもり？",
              "maleVariant": "修理して動かすつもりか？",
              "stringId": "1832778624315445248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カカ・ベフ… つまり、ダメだ",
              "maleVariant": "",
              "stringId": "1832780862849044480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。もう何時間だろ？",
              "maleVariant": "",
              "stringId": "1832783626006188032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしにやらせてくれる？",
              "maleVariant": "俺にやらせてくれるか？",
              "stringId": "1832784653057019904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頑張って",
              "maleVariant": "頑張れよ",
              "stringId": "1832784653057019920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしにやらせてくれる？",
              "maleVariant": "俺にやらせてくれるか？",
              "stringId": "1832785491666161664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、助かるよ。こいつらみんな見てるだけだ",
              "maleVariant": "",
              "stringId": "1832786032479719424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ頑張って",
              "maleVariant": "じゃあ頑張れよ",
              "stringId": "1832786602619850756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう。休憩しなきゃ",
              "maleVariant": "",
              "stringId": "1832786604264017920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、ビールもある",
              "maleVariant": "",
              "stringId": "1916896516315566080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やるよ。どこから来たんだ？",
              "maleVariant": "",
              "stringId": "1916903725149736960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "集中すればなんとかなりそうなもんだけど",
              "maleVariant": "",
              "stringId": "1916934051645378560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "黙っててくれないか？",
              "maleVariant": "",
              "stringId": "1916935959969812480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "街の外から",
              "maleVariant": "",
              "stringId": "1916942908790689792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "街の外よ",
              "maleVariant": "街の外だ",
              "stringId": "1916949297806200832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ノーマッドか。ノーマッド好きだ。俺もなれるかも",
              "maleVariant": "",
              "stringId": "1916960789645512704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フフッ、タバコ一本でヘトヘトになるくせに",
              "maleVariant": "",
              "stringId": "1916965622423244800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘイウッドか！ ジェットコースターあるのかい？ やっぱりないか。残念だ",
              "maleVariant": "",
              "stringId": "1917002910322675712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー、ならお前コーポか。ん？",
              "maleVariant": "",
              "stringId": "1917005758271868928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バビロンのエージェントね！",
              "maleVariant": "",
              "stringId": "1917006179698757632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やれるって言ったのそっちだろ？",
              "maleVariant": "",
              "stringId": "1917012716689313792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だったらどうするの？",
              "maleVariant": "だったらどうするんだ？",
              "stringId": "1917026404498427904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別に。座って景色を楽しめ",
              "maleVariant": "",
              "stringId": "1917026810624495616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見えるか？ 全ていつかお前のものになるかもしれないぞ",
              "maleVariant": "",
              "stringId": "1917027771740868608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一緒に座ってけよ",
              "maleVariant": "",
              "stringId": "1917035951405420544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "上手くいってる？",
              "maleVariant": "上手くいってるか？",
              "stringId": "1917048750726729728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へっ。そりゃプレッシャーだな",
              "maleVariant": "",
              "stringId": "1917053877843939328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ諦めてないのか？",
              "maleVariant": "",
              "stringId": "1917056966311301120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別にいいじゃない。私乗りたい！",
              "maleVariant": "",
              "stringId": "1917057814617673728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私たち、ジャンを信じてる！",
              "maleVariant": "",
              "stringId": "1917059406943244288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとな",
              "maleVariant": "",
              "stringId": "1917061990902943744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ホントに？ もうどうしようもできないの？",
              "maleVariant": "",
              "stringId": "1917063983650328576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこのファミリーだ？",
              "maleVariant": "",
              "stringId": "1917089584792752128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バッカー",
              "maleVariant": "",
              "stringId": "1917089883024543744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー、バッカーか…！ 知らないや",
              "maleVariant": "",
              "stringId": "1917090058329673728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴードゥー・ボーイズはどうなったの？",
              "maleVariant": "ヴードゥー・ボーイズはどうなったんだ？",
              "stringId": "1922603558229528576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何の話かわからない",
              "maleVariant": "",
              "stringId": "1922616086296809472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫、ブリジットとプラシドの知り合いよ",
              "maleVariant": "大丈夫、ブリジットとプラシドの知り合いだ",
              "stringId": "1922616355134918656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それならもういないの知ってるだろ",
              "maleVariant": "",
              "stringId": "1922616646672601088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ネットウォッチに見つかってから誰もいなくなった",
              "maleVariant": "",
              "stringId": "1922617013892304896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え？ 一人残らず？",
              "maleVariant": "え？ 一人残らずか？",
              "stringId": "1922628904375046144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。パシフィカの誰も探し場所の見当つかない",
              "maleVariant": "",
              "stringId": "1922629314678640640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "地下に潜ったって言う奴もいるけど、消されたんだろう",
              "maleVariant": "",
              "stringId": "1922629582073909248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "関係ないだろ",
              "maleVariant": "",
              "stringId": "1922632754813227008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "プラシドとブリジットとは知り合いだったの。だから計画が成功したことも知ってる",
              "maleVariant": "プラシドとブリジットとは知り合いだった。だから計画が成功したことも知ってる",
              "stringId": "1922633200986509312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴードゥーはどうなったの？",
              "maleVariant": "ヴードゥーはどうなったんだ？",
              "stringId": "1922635738708201472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フム。うわさ話と本当の話、どっち聞きたい？",
              "maleVariant": "",
              "stringId": "1922640040017285120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴードゥー・ボーイズがどうなったか知ってる？",
              "maleVariant": "ヴードゥー・ボーイズがどうなったか知ってるか？",
              "stringId": "1922644465360326660"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほかの人たちは？",
              "maleVariant": "",
              "stringId": "1922645693049884672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うわさ話でいい",
              "maleVariant": "",
              "stringId": "1922647308611899392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真実をお願い",
              "maleVariant": "真実を頼む",
              "stringId": "1922650742337589248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "細かいことまで知らない",
              "maleVariant": "",
              "stringId": "1922651597086412800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞いたの、ブリジット向こう側のAIと取引したって話",
              "maleVariant": "",
              "stringId": "1922652072250724352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今じゃラダ・ロアと一緒で、俺たちをブラックウォールの向こう側へ連れていってくれるらしい",
              "maleVariant": "",
              "stringId": "1922652693209042944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブリジット、向こう側のAIと取引したらしい",
              "maleVariant": "",
              "stringId": "1922655547802013696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パシフィカのみんな待ってる。急に教会に通いだす者が増えた",
              "maleVariant": "",
              "stringId": "1922679819568173056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うわさ話",
              "maleVariant": "",
              "stringId": "1922687684223717428"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当の話",
              "maleVariant": "",
              "stringId": "1922688515534770176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは懐疑的なのね",
              "maleVariant": "あんたは懐疑的なんだな",
              "stringId": "1922689847192735776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "宗教に何の関係が？",
              "maleVariant": "",
              "stringId": "1922689847192735792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "疑ってるのね",
              "maleVariant": "疑ってるんだな",
              "stringId": "1922710812555497472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "宗教に何の関係が？",
              "maleVariant": "",
              "stringId": "1922711870660956160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AIはランプの精だ。何でも望みを叶えてくれる",
              "maleVariant": "",
              "stringId": "1922714492822376448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「小さくなりたい」って願ったとしたら、頭をもぎ取って叶えたと言われる",
              "maleVariant": "",
              "stringId": "1922714566105255936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴードゥー・ボーイズもそうなったと？",
              "maleVariant": "",
              "stringId": "1922714910642163712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってるの、連中姿を消したまま戻ってこないってことだけ。いとこのロヴェリーもな",
              "maleVariant": "",
              "stringId": "1922715243351134208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "子供、娘いるんだ。戻ってこれるならとっくに戻ってきてるはずだ",
              "maleVariant": "",
              "stringId": "1922715639880634368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何も。プラシド信仰心と無縁だった",
              "maleVariant": "",
              "stringId": "1922716752042283008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "黒の雄鶏を殺す、それ誰かの空腹を満たすため",
              "maleVariant": "",
              "stringId": "1922717130016182272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この辺じゃ、人気者だったのよね",
              "maleVariant": "この辺じゃ、人気者だったんだろ",
              "stringId": "1922717690761072640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんな、誰か見守っててくれると思いたいんだ",
              "maleVariant": "",
              "stringId": "1922718511351484416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴードゥー・ボーイズがいなくなって以来、昔の神戻ってきてる",
              "maleVariant": "",
              "stringId": "1922719021982830592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしが直してみようか",
              "maleVariant": "俺が直してみようか",
              "stringId": "1936995934835593220"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたたちはどういう関係？",
              "maleVariant": "あんたたちはどういう関係だ？",
              "stringId": "1936996511636279296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしが直せないかやってみようか",
              "maleVariant": "俺が直せないかやってみようか",
              "stringId": "1936997249313689600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "助かるよ",
              "maleVariant": "",
              "stringId": "1936997719008628736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これはどういう集まり？",
              "maleVariant": "これはどういう集まりだ？",
              "stringId": "1936998840599072768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジェットコースターに乗りたい？",
              "maleVariant": "ジェットコースターに乗りたいか？",
              "stringId": "1938893815518351360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほかの仲間はどこにいるの？ つるむのはやめちゃった？",
              "maleVariant": "ほかの仲間はどこだ？ つるむのはやめちまったのか？",
              "stringId": "1938900899714584576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最近この辺もいろいろ変わってな",
              "maleVariant": "",
              "stringId": "1938903054462767104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジェットコースターに乗りたい？",
              "maleVariant": "ジェットコースターに乗りたいか？",
              "stringId": "1938904834777042944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今そういう時じゃない",
              "maleVariant": "",
              "stringId": "1938907103073423360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たち幼なじみだ。近所に住んでて、洗濯も食べるのも一緒さ",
              "maleVariant": "",
              "stringId": "1938917745884942336"
