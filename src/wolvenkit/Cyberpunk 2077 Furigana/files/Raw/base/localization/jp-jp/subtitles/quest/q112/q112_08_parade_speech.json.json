    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「沈黙は決して金ならず」",
                    "maleVariant": null,
                    "stringId": 1669135755361927168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「それすなわち炎が消える音なり」",
                    "maleVariant": null,
                    "stringId": 1669136419068592128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ガダルカナル島上空で自身の戦闘機が撃ち落とされてからひと月後、1942年3月3日、捕虜収容所でサブロウ・アラサカが残した言葉です",
                    "maleVariant": null,
                    "stringId": 1669136734530584576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サブロウは当時、第3飛行中隊のパイロットで、曹長を務めていました",
                    "maleVariant": null,
                    "stringId": 1669136891548549120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クソ！",
                    "maleVariant": null,
                    "stringId": 1669217419584880640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、状況は！？",
                    "maleVariant": null,
                    "stringId": 1669218386388729856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今日はついてるね…",
                    "maleVariant": "今日はついてるな…",
                    "stringId": 1669223235054485504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "忠義の意味を履き違えてるのよ…",
                    "maleVariant": "忠義の意味を履き違えやがって…",
                    "stringId": 1669223459097427968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも今日はついてるよ",
                    "maleVariant": "だが今日はついてるな",
                    "stringId": 1669223849721348096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "OK！ 山車のシステムに入った",
                    "maleVariant": null,
                    "stringId": 1669230128560881664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ゴロウに礼を言うといい",
                    "maleVariant": null,
                    "stringId": 1698486545029283840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "遅かったわね！",
                    "maleVariant": "遅かったな！",
                    "stringId": 1713082986469015552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "な… なんとか…",
                    "maleVariant": null,
                    "stringId": 1713085187102883840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オダは… 死んだのか？",
                    "maleVariant": null,
                    "stringId": 1713085246712332288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "息はしてる…",
                    "maleVariant": null,
                    "stringId": 1713085328887136256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "頼む、V。殺さないでくれ",
                    "maleVariant": null,
                    "stringId": 1724162296454713344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やめろ！",
                    "maleVariant": null,
                    "stringId": 1724187621863591936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "殺す必要はなかっただろ！",
                    "maleVariant": null,
                    "stringId": 1724188104258883584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "逆の立場なら必ず殺されてた",
                    "maleVariant": null,
                    "stringId": 1724189454287900676
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "不安の種を残しておけない",
                    "maleVariant": null,
                    "stringId": 1724192698280054784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "逆の立場なら殺されてた",
                    "maleVariant": null,
                    "stringId": 1724194637659115520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "仕方なかった。不安の種は残しておけない",
                    "maleVariant": null,
                    "stringId": 1724194708593184768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうでしょ！",
                    "maleVariant": "そうだろ！",
                    "stringId": 1724195301483220992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "このことは忘れないからな",
                    "maleVariant": null,
                    "stringId": 1724196313199677440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "後でまた、命を狙われるかもしれないでしょ",
                    "maleVariant": "後でまた、命を狙われるかもしれないだろ",
                    "stringId": 1724197607595446272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "このことは、絶対に忘れないからな",
                    "maleVariant": null,
                    "stringId": 1724198471135531008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うう…",
                    "maleVariant": null,
                    "stringId": 1724200725154500608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、俺こそお前に礼を言う",
                    "maleVariant": null,
                    "stringId": 1724201522659463168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今の… ところはね…",
                    "maleVariant": "今の… ところはな…",
                    "stringId": 1724202997997506560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オダ！",
                    "maleVariant": null,
                    "stringId": 1725692919384248320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハナコ様が見える。お話しになられるぞ",
                    "maleVariant": null,
                    "stringId": 1740315746735321088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人類史上最大かつ最強の軍需企業の創業者にして、最高経営責任者",
                    "maleVariant": null,
                    "stringId": 1773226942591275008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "兵士、実業家、そして――",
                    "maleVariant": null,
                    "stringId": 1773227138012286976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私の父",
                    "maleVariant": null,
                    "stringId": 1773227278957678592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "先ほどの収容所での言葉は、父の友人や家族、同輩に向けられたものでは決してありません",
                    "maleVariant": null,
                    "stringId": 1773228423298027520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "敵に向けられた言葉なのです",
                    "maleVariant": null,
                    "stringId": 1773228678043275264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私がここで、父の言を引用したのは偶然ではありません",
                    "maleVariant": null,
                    "stringId": 1773228825934434304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私たちの敵が暴挙に走ったのは50年前、ここナイトシティでのことでした",
                    "maleVariant": null,
                    "stringId": 1773229002313306112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "無差別テロという卑劣極まりない行為が、勤勉なるアラサカ社従業員、無垢なるナイトシティ市民、合わせて4000人もの尊い命を無慈悲にも奪い去ったのです",
                    "maleVariant": null,
                    "stringId": 1773229207901310976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカ・タワーは崩壊しました",
                    "maleVariant": null,
                    "stringId": 1773229364399181824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ですが…",
                    "maleVariant": null,
                    "stringId": 1773229528446799872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ですが、その尖塔は今もなお摩天楼にそびえ立っています",
                    "maleVariant": null,
                    "stringId": 1773229586697293824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その姿は、いまだかつてないほどに力強く、尊いものです",
                    "maleVariant": null,
                    "stringId": 1773229822366846976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私の父は、テロと同じくらい卑劣な、許されざる暴力の犠牲者となりました",
                    "maleVariant": null,
                    "stringId": 1773229988796829696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これは私たちが価値観を分かち合う、この世界に対する冒涜にほかなりません",
                    "maleVariant": null,
                    "stringId": 1773230168111714304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティの市民が見守る中、アラサカ社は50年前と同様、これまで以上に全社が一丸となり、この試練を乗り越えることをここに誓います",
                    "maleVariant": null,
                    "stringId": 1773230362727419904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私たちは今日、この場所から強くなるのです",
                    "maleVariant": null,
                    "stringId": 1773230601903411200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私たちの声を、その耳でしかと聞き届けなさい",
                    "maleVariant": null,
                    "stringId": 1773231479687352320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "沈黙は決して金ならず",
                    "maleVariant": null,
                    "stringId": 1773231823318290432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それすなわち炎が消える音なり",
                    "maleVariant": null,
                    "stringId": 1773232269156028416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "とにかく、これで行く手を阻むものはなくなった。計画を進めるぞ！",
                    "maleVariant": null,
                    "stringId": 1773457869241335808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[接続を切る] 休憩よ",
                    "maleVariant": "[接続を切る] 休憩だ",
                    "stringId": 1794073849835307012
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "疲れたでしょ。休憩よ",
                    "maleVariant": "疲れただろ。休憩だ",
                    "stringId": 1794076688959684608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "注意を引いたぞ！ 今だ、V！",
                    "maleVariant": null,
                    "stringId": 1794077043277709312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リンクを抜け！",
                    "maleVariant": null,
                    "stringId": 1794077698058895360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "悪く思わないで",
                    "maleVariant": "悪く思うなよ",
                    "stringId": 1834462117765189632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自ら招いた結果よ…",
                    "maleVariant": "自ら招いた結果だ…",
                    "stringId": 1834463290291261440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うっ…！",
                    "maleVariant": null,
                    "stringId": 1857629018768875520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おやすみ",
                    "maleVariant": "ねんねしな",
                    "stringId": 1857629722438868992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なっ！？",
                    "maleVariant": null,
                    "stringId": 1857631771960352768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "警告したはずだ",
                    "maleVariant": null,
                    "stringId": 2004115740769206272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一瞬で終わらせてやろう、女",
                    "maleVariant": "一瞬で終わらせてやろう、クズめ",
                    "stringId": 2230175985079525376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自業自得だ！",
                    "maleVariant": null,
                    "stringId": 2230178248040751112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なぜ言われた通り、街を出なかった！",
                    "maleVariant": null,
                    "stringId": 2230182266821517312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しつこい… これで終わりにしてやろう！",
                    "maleVariant": null,
                    "stringId": 2230183836330074120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まだだ…！ ハナコ様には… 指一本… 触れさせん！",
                    "maleVariant": null,
                    "stringId": 2230199314201026568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "タケムラはどこだ？ 自らの弟子を相手に怖気づいたか？",
                    "maleVariant": null,
                    "stringId": 2230201191688937480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[接続する]",
                    "maleVariant": null,
                    "stringId": 2231360085887631364
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "気をつけろ、光学迷彩を使う気だ！ 光の反射を捉えろ！",
                    "maleVariant": null,
                    "stringId": 2252032229872345088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ダメだ！ 奴のマスクは照準を妨害する！",
                    "maleVariant": null,
                    "stringId": 2252051393278222344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もういい、V！ 殺す必要はない！ オダは自らの責務を果たそうとしただけだ！",
                    "maleVariant": null,
                    "stringId": 2252053442866814984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "不用意に近づくな！ 奴の間合いに入ったら、細切れにされるぞ！",
                    "maleVariant": null,
                    "stringId": 2252057682972614664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オダは自らに正義があると信じている。 だがそれは違う！ お前が奴の目を覚まさせてやれ！ V！",
                    "maleVariant": null,
                    "stringId": 2252059389282590728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[歌に耳を傾ける]",
                    "maleVariant": null,
                    "stringId": 2453324305031774208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[立ち上がる]",
                    "maleVariant": null,
                    "stringId": 2453407681940045824
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}