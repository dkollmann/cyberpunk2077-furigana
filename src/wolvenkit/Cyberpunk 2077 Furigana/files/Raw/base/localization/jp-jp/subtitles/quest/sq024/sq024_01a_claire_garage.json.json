  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いい車ね",
                    "maleVariant": "いい車だな",
                    "stringId": 1734409387521351680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "来たよ、クレア。すごいマシンね",
                    "maleVariant": "来たぞ、クレア。すごいマシンだな",
                    "stringId": 1734409387689123840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あら、なかなか見る目があるのね",
                    "maleVariant": null,
                    "stringId": 1734409387689123856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "冷蔵庫にビールがあるから",
                    "maleVariant": null,
                    "stringId": 1734409387705901068
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "スケジュールは？",
                    "maleVariant": null,
                    "stringId": 1734409388192440320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自分の車を使いたい",
                    "maleVariant": null,
                    "stringId": 1734409388209217544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "賞品はあるの？",
                    "maleVariant": "賞品はあるのか？",
                    "stringId": 1734409388209217560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "前は夫のディーンと乗ってたんだけど、1年ぐらい前に…",
                    "maleVariant": null,
                    "stringId": 1734409388813197312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "...",
                    "maleVariant": null,
                    "stringId": 1734409388829974528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "事故があってね",
                    "maleVariant": null,
                    "stringId": 1734409388829974540
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "苦しまずに即死したのが、不幸中の幸いよ",
                    "maleVariant": null,
                    "stringId": 1734409388829974552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それは気の毒ね、クレア…",
                    "maleVariant": "それは気の毒だな、クレア…",
                    "stringId": 1734409388829974564
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも、あんたは走り続けてる",
                    "maleVariant": null,
                    "stringId": 1734409388829974576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人も車も、立ち止まったらお終いだから",
                    "maleVariant": null,
                    "stringId": 1734409388846751752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何か賞品はあるの？",
                    "maleVariant": "何か賞品はあるのか？",
                    "stringId": 1734409389282959360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "1位には賞金が出る。その時は半々ってことでいい？",
                    "maleVariant": null,
                    "stringId": 1734409389299736584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ドライバーが要るの。レースのね",
                    "maleVariant": null,
                    "stringId": 1785290523034001408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうせなら、自分の車を使いたい",
                    "maleVariant": null,
                    "stringId": 1785292613089239040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "装備は十分なの？ これは単なるスピード勝負じゃない、命懸けのぶつかり合いなのよ",
                    "maleVariant": null,
                    "stringId": 1785292805742010368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "レースはいつなの？",
                    "maleVariant": "レースはいつなんだ？",
                    "stringId": 1785293402457251840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まずは予選を突破しないと、決勝に参加できない",
                    "maleVariant": null,
                    "stringId": 1785293533353091072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "2レース以上で3位以内に入ること",
                    "maleVariant": null,
                    "stringId": 1785293605612560384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃ、シティ・センターで会いましょ。詳しくはそこで話すから。エンジンのうなり声を辿れば、レース会場に着くはずよ",
                    "maleVariant": null,
                    "stringId": 1786416755398156288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大丈夫よ。ソープボックスに乗ってるわけじゃないから",
                    "maleVariant": "大丈夫だ。ソープボックスに乗ってるわけじゃないからな",
                    "stringId": 1827282789298348032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたはナビ役？",
                    "maleVariant": "あんたはナビ役か？",
                    "stringId": 1854851610842370052
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしがドライバーってことは、あんたはナビ役？",
                    "maleVariant": "俺がドライバーってことは、あんたはナビ役か？",
                    "stringId": 1854851691909877760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どんなレースなの？ 特殊なルールがあるとか？",
                    "maleVariant": "どんなレースなんだ？ 特殊なルールがあるとか？",
                    "stringId": 1854851728802975744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どんなレースをするの？",
                    "maleVariant": "どんなレースをするんだ？",
                    "stringId": 1854852433966141440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと違う。このレースにはドライバーとガンナーが必要なの",
                    "maleVariant": null,
                    "stringId": 1854854663205163008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "むしろルールがないのが特徴ね。ドライバーとガンナーのペアで出るの",
                    "maleVariant": null,
                    "stringId": 1854857732663939072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたの車だし、あんたが運転すればいいんじゃないの？",
                    "maleVariant": "あんたの車だし、あんたが運転すればいいんじゃないのか？",
                    "stringId": 1854859025583312896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私は整備士よ。銃もそれなりに使えるけど、ラリーレーサーにはなれない",
                    "maleVariant": null,
                    "stringId": 1854860587072360448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あなた、運転は得意なんでしょ？",
                    "maleVariant": null,
                    "stringId": 1854861507671756800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった、引き受ける",
                    "maleVariant": null,
                    "stringId": 1854862729740947460
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よし、わかった。ドライバーは任せて",
                    "maleVariant": "よし、わかった。ドライバーは任せろ",
                    "stringId": 1854862801815867392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まさか初出場ってわけじゃないよね？",
                    "maleVariant": "まさか初出場ってわけじゃないよな？",
                    "stringId": 1854862854680875008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "前にもレースに出たことはあるのよね？",
                    "maleVariant": "前にもレースに出たことはあるんだよな？",
                    "stringId": 1854863822944342016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "心配しないで。それなりに経験は積んでる",
                    "maleVariant": null,
                    "stringId": 1854864674119618560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "前のドライバーは？ やめたの？",
                    "maleVariant": "前のドライバーは？ やめたのか？",
                    "stringId": 1854867430716870656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ある意味ね。レース中の事故で死んだの",
                    "maleVariant": null,
                    "stringId": 1854867825686089728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あー… ごめん…",
                    "maleVariant": "あー… すまん…",
                    "stringId": 1854868629398622208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "...",
                    "maleVariant": null,
                    "stringId": 1854870345674264576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "レースは全部で4つ――まずはシティ・センターから",
                    "maleVariant": null,
                    "stringId": 1854871048555728896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そこまで言うなら… 運転手のあなたに任せる",
                    "maleVariant": null,
                    "stringId": 1854910158829916160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待ちきれないね",
                    "maleVariant": "待ちきれないな",
                    "stringId": 1854910866308337664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "条件は？",
                    "maleVariant": null,
                    "stringId": 1854912558944882688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バッドランズ、サントと続いて、最後はワトソンよ",
                    "maleVariant": null,
                    "stringId": 1863062461453832192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほら、頑張って…",
                    "maleVariant": null,
                    "stringId": 1935548074635116544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言われたとおり来たよ",
                    "maleVariant": "言われたとおり来たぞ",
                    "stringId": 1935549826763669504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クレア――来たよ",
                    "maleVariant": "クレア――来たぞ",
                    "stringId": 1935550558887821312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この子は“ビースト”よ。私の自慢のマシン",
                    "maleVariant": null,
                    "stringId": 1935552135526694912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うん、ちょっと待っててくれる？",
                    "maleVariant": null,
                    "stringId": 1935927010288340992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この子は“ビースト”よ",
                    "maleVariant": null,
                    "stringId": 1935927380611829760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ありがと。で、用件は？",
                    "maleVariant": "どうも。で、用件は？",
                    "stringId": 1935931751730909184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった",
                    "maleVariant": null,
                    "stringId": 1944782724081934336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった、やればいいんでしょ",
                    "maleVariant": "わかった、やればいいんだろ",
                    "stringId": 1962021937652703236
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "遠慮しとく",
                    "maleVariant": null,
                    "stringId": 1962022230448676864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "せっかくだけど、遠慮しとく",
                    "maleVariant": "せっかくだが、遠慮しとく",
                    "stringId": 1962022598406578176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本当にいいの？ あなたは適任だと思うけど",
                    "maleVariant": null,
                    "stringId": 1962022942054293504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ。あんたは楽しんで",
                    "maleVariant": "ああ。あんたは楽しんでくれ",
                    "stringId": 1962023161584164864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ。でも、あんたはどうぞ楽しんで",
                    "maleVariant": "ああ。だが、あんたはどうぞ楽しんでくれ",
                    "stringId": 1962023296909189120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まったく… しょうがないな",
                    "maleVariant": null,
                    "stringId": 1962023312948207616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "エンジンと銃声のハーモニー、あふれ出るアドレナリン… こんな楽しいことを見逃していいわけ？",
                    "maleVariant": null,
                    "stringId": 1983919625960820736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "残念ね…",
                    "maleVariant": null,
                    "stringId": 1983927096267902976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クレア？",
                    "maleVariant": null,
                    "stringId": 2003779718919307268
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あー… クレア？",
                    "maleVariant": null,
                    "stringId": 2003782011391655936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "車の下よ！",
                    "maleVariant": null,
                    "stringId": 2003787748763320320
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}