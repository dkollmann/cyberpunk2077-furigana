    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいか、ひとつアドバイスをしてやろう。誰のかわからん冷蔵庫なんか、何があったってのぞくもんじゃねえ。中から見つかんのはよくて腐った牛乳、ひどい時には――",
                    "maleVariant": null,
                    "stringId": 1777841819553275904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "誰？",
                    "maleVariant": "誰って？",
                    "stringId": 1777843908585103364
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "っておいおい、嘘だろ。こいつはレイシィ・バートモスじゃねえか！",
                    "maleVariant": null,
                    "stringId": 1777845253916499968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "このツラは間違いない。2020年には指名手配のポスターが街中にあふれてた。社会の敵、生死を問わずってな… 街にいる連中の半分は奴を追ってたんだ",
                    "maleVariant": null,
                    "stringId": 1777847462284664832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ツイてない野郎だ。こんなかで半冷凍状態になってたようだが、いつしか回路がぶっ飛んで停電し、腐っちまったようだな",
                    "maleVariant": null,
                    "stringId": 1777870255005016064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うっ… すごいニオイ…",
                    "maleVariant": "うっ… すげえニオイ…",
                    "stringId": 1892413583913177088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あのレイシィ・バートモス！？",
                    "maleVariant": null,
                    "stringId": 1892423599978237952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "DataKrashやRABIDで有名な、あのバートモス？ 初代のネットをメチャクチャにした？",
                    "maleVariant": "DataKrashやRABIDで有名な、あのバートモスか？ 初代のネットをメチャクチャにした？",
                    "stringId": 1892424009409417216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "誰それ？",
                    "maleVariant": "誰だそれ？",
                    "stringId": 1892424241941630976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前それ本気で言ってんのか？ あのバートモスだぞ！ 初代のネットをメチャクチャにしたあの野郎だよ！",
                    "maleVariant": null,
                    "stringId": 1892427240533446656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "双子の兄弟でもいない限り、ああ、 そいつだよ！",
                    "maleVariant": null,
                    "stringId": 1892430461121384448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うえ… この悪臭からすると20年くらい前ね。鼻が曲がりそう、もう行くよ",
                    "maleVariant": "うえ… この悪臭からすると20年くらい前だな。鼻が曲がりそうだ、もう行くぞ",
                    "stringId": 1892438601292369920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょ、待てよ！ 中に何かお宝が眠ってるんじゃないか？ 初代のネットのビンテージ品とか。残して行ったらもったいない",
                    "maleVariant": null,
                    "stringId": 1892440968339443712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと考えさせて",
                    "maleVariant": "ちょっと考えさせてくれ",
                    "stringId": 1892448749561892868
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あったとしても、使えないでしょ",
                    "maleVariant": "あったとしても、使えないだろ",
                    "stringId": 1892449168639971328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "考えさせて",
                    "maleVariant": "考えさせてくれ",
                    "stringId": 1892449450463645696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あったとしても、使えないでしょ",
                    "maleVariant": "あったとしても、使えないだろ",
                    "stringId": 1892449821240119296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしてそう言い切れる？ それを確かめる方法はひとつしかない",
                    "maleVariant": null,
                    "stringId": 1892451085940215808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "優柔不断は毒にしかならん。ほら、中を見ろ",
                    "maleVariant": null,
                    "stringId": 1892454134326808576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "へえ… SGIテックの“エリスラ”。最高のサイバーデッキの一つね",
                    "maleVariant": "へえ… SGIテックの“エリスラ”。最高のサイバーデッキの一つだ",
                    "stringId": 1892466087170461696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "んー… これ何だろう？ 箱… みたいだけど",
                    "maleVariant": "んー… こりゃ何だ？ 箱… みたいだけど",
                    "stringId": 1892466400518524928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前はバカか。サイバーデッキだよ",
                    "maleVariant": null,
                    "stringId": 1892468355634614272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほらな？ ゴミ漁りってのもたまには悪くないだろ",
                    "maleVariant": null,
                    "stringId": 1892470733133893632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうだ、アフターライフの常連のニックス… 彼なら何かデータを取り出せるかも",
                    "maleVariant": "そうだ、アフターライフの常連のニックス… 奴なら何かデータを取り出せるかも",
                    "stringId": 1892471503006781440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そういや、アフターライフにいるあの男… ニックスって名前だったか？ あいつ、レガシーテックに詳しそうだ",
                    "maleVariant": null,
                    "stringId": 1892471786105524224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あいつと話してみろよ… 何かデータを取り出せるかもしれないぞ",
                    "maleVariant": null,
                    "stringId": 1892472996682321920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あー、でもどうかな。前回の仕事ではもめそうになったし",
                    "maleVariant": "あー、でもどうだろな。前回の仕事ではもめそうになったし",
                    "stringId": 1892474287018004480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなの関係ねえよ。バートモスのテックだなんて聞いたらちびるぞ。絶対な",
                    "maleVariant": null,
                    "stringId": 1892475964185956352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "DataKrashやRABIDって単語に聞き覚えないか？ まったく、お前ってやつはどういう教育を受けてきたんだよ…",
                    "maleVariant": null,
                    "stringId": 1893566193516990464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "聞く価値はあるな",
                    "maleVariant": null,
                    "stringId": 1895328759075360768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[閉める] ひどい臭い",
                    "maleVariant": "[閉める] ひどい臭いだ",
                    "stringId": 2082410823603363840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[開ける]",
                    "maleVariant": null,
                    "stringId": 2288127577510813696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[開ける]",
                    "maleVariant": null,
                    "stringId": 2412819370408960000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[サイバーデッキを取る]",
                    "maleVariant": null,
                    "stringId": 2412898907129929728
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}