    },
    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1811438801777287168,
            "femaleVariant": "雅なるオアシスへようこそお戻りくださいました！"
          },
          {
            "stringId": 1811441489604730880,
            "femaleVariant": "おすすめは？"
          },
          {
            "stringId": 1811442145660985344,
            "femaleVariant": "スパンコールは勘弁して",
            "maleVariant": "スパンコールは勘弁してくれ"
          },
          {
            "stringId": 1811442913621270528,
            "femaleVariant": "客入りが悪いみたいね",
            "maleVariant": "客入りが悪いみたいだな"
          },
          {
            "stringId": 1811443426936971264,
            "femaleVariant": "何も決めてなくて… 新しいジャケットとか、ズボンで、何かいいのない？",
            "maleVariant": "何も決めてなくて… 新しいジャケットとか、ズボンで、何かいいのないか？"
          },
          {
            "stringId": 1811443957617090560,
            "femaleVariant": "あわせて新調されるのはいかがです？ もちろん、今のお召し物もお似合いですが、ファッションは変化しますから"
          },
          {
            "stringId": 1811444263667064836,
            "femaleVariant": "スパンコール？ 冗談でしょ",
            "maleVariant": "スパンコール？ 冗談だろ"
          },
          {
            "stringId": 1811444263667064844,
            "femaleVariant": "一口に雅と言っても、そのスタイルは千差万別です"
          },
          {
            "stringId": 1811444283212521476,
            "femaleVariant": "最近ちょっと寂しいんじゃない？",
            "maleVariant": "最近ちょっと寂しいんじゃないか？"
          },
          {
            "stringId": 1811444283229298688,
            "femaleVariant": "問題ありません。ジングウジは量よりも質ですから"
          },
          {
            "stringId": 1811445828914208768,
            "femaleVariant": "こちらで警報を鳴らします！"
          },
          {
            "stringId": 1811456126769389572,
            "femaleVariant": "V、伏せろ！ ありゃやべえ！"
          },
          {
            "stringId": 1811579334197731328,
            "femaleVariant": "ぐあああ！"
          },
          {
            "stringId": 1811582285410689028,
            "femaleVariant": "あいつを何とかしろ！"
          },
          {
            "stringId": 1811629837946413056,
            "femaleVariant": "マックス・タックがもうすぐ来る！ それまであいつを抑えてくれ！"
          },
          {
            "stringId": 1811630797317955584,
            "femaleVariant": "私を守ってくれぇ！"
          },
          {
            "stringId": 1811783547829837824,
            "femaleVariant": "マックス・タックだ！ 助かった！"
          },
          {
            "stringId": 1811798454252036096,
            "femaleVariant": "突入する！ 取り囲め！"
          },
          {
            "stringId": 1811799389128843268,
            "femaleVariant": "行け！ 制圧せよ！"
          },
          {
            "stringId": 1811799576916221952,
            "femaleVariant": "排除する！"
          },
          {
            "stringId": 1811802121332023300,
            "femaleVariant": "標的を無力化。証人を確保！"
          },
          {
            "stringId": 1811812972332015616,
            "femaleVariant": "そこのあなた！ 証言しなさい！"
          },
          {
            "stringId": 1812400529243594756,
            "femaleVariant": "マックス・タックへの証言を拒むと、場合によっては逮捕することもある"
          },
          {
            "stringId": 1812400529293926404,
            "femaleVariant": "私を怒らせたいのか？"
          },
          {
            "stringId": 1812403706344988672,
            "femaleVariant": "座って。両手は前に"
          },
          {
            "stringId": 1812407724270116868,
            "femaleVariant": "[座る]"
          },
          {
            "stringId": 1812415657393479684,
            "femaleVariant": "座れと言ったの"
          },
          {
            "stringId": 1812415657410256900,
            "femaleVariant": "頼むから、早く終わらせよう。清掃員を呼びたいんだ"
          },
          {
            "stringId": 1812455858874572800,
            "femaleVariant": "マックス・タックのメリッサ・ローリーよ。あなたの氏名と職業を"
          },
          {
            "stringId": 1812456786520399872,
            "femaleVariant": "ゼイン・ジャガー。雅なるオアシス、ジングウジで働いて…"
          },
          {
            "stringId": 1812457541646114816,
            "femaleVariant": "V、傭兵。ここには買い物で来た"
          },
          {
            "stringId": 1812458108565020672,
            "femaleVariant": "OKよ。何が起きたのか、最初から細かく説明して"
          },
          {
            "stringId": 1812458319538511872,
            "femaleVariant": "V様に商品をお見せしていたら… 突然、通りから悲鳴が聞こえたんです"
          },
          {
            "stringId": 1812458886474194944,
            "femaleVariant": "そしたら… 急にあの野郎が入ってきまして、うちの警備員を殺し… いや、そんな生ぬるいもんじゃない。虐殺したんです"
          },
          {
            "stringId": 1812459153265483776,
            "femaleVariant": "私は警報を鳴らし、V様は応戦してくださいました。もしいらっしゃらなかったらと思うと…"
          },
          {
            "stringId": 1812459549744652288,
            "femaleVariant": "いいのよ。あたしだって死にたくなかったし",
            "maleVariant": "いいんだ。俺だって死にたくなかったしな"
          },
          {
            "stringId": 1812460242878554116,
            "femaleVariant": "そのマンティスブレード、ヒグラシの？",
            "maleVariant": "そのマンティスブレード、ヒグラシのか？"
          },
          {
            "stringId": 1812461372706611200,
            "femaleVariant": "状況終了。警報を解除して"
          },
          {
            "stringId": 1812479466531414016,
            "femaleVariant": "本当に殺す必要あったの？",
            "maleVariant": "本当に殺す必要あったのか？"
          },
          {
            "stringId": 1812480137435504640,
            "femaleVariant": "待って… そのマンティスブレード、69年に販売終了したヒグラシ20-13じゃない？",
            "maleVariant": "おい待て… そのマンティスブレード、69年に販売終了したヒグラシ20-13じゃないか？"
          },
          {
            "stringId": 1812480592651706368,
            "femaleVariant": "目ざといね。随分前に取り付けたの。切れ味は… 抜群"
          },
          {
            "stringId": 1812480971397357568,
            "femaleVariant": "これを見ると… 思い出すの。切ったものが目に浮かぶのよ… 筋肉、腱、骨… インプラントさえもね。バターを熱いナイフで切るみたいだった"
          },
          {
            "stringId": 1812481365242503168,
            "femaleVariant": "対象は常に排除するの？ セラピーで改善する可能性だって――",
            "maleVariant": "対象は常に排除するのか？ セラピーで改善する可能性だって――"
          },
          {
            "stringId": 1812511302708060160,
            "femaleVariant": "サイバーサイコの生け捕りは難しい。野次馬に被害が出る危険性もある。滅多にしないね"
          },
          {
            "stringId": 1812514633505529856,
            "femaleVariant": "鑑識には連絡済み。もうすぐ来る"
          },
          {
            "stringId": 1812515309644111872,
            "femaleVariant": "それとあなた、店は捜査が終わるまで休業すること"
          },
          {
            "stringId": 1812515762779938816,
            "femaleVariant": "よくやってくれた。あの男の命が尽きた瞬間はたまらなかったね"
          },
          {
            "stringId": 1812522879238758400,
            "femaleVariant": "スミス！ 被害状況はまとまった？"
          },
          {
            "stringId": 1812523852535390208,
            "femaleVariant": "店内で2人が殺され、負傷者が2名。外では6人やられてます"
          },
          {
            "stringId": 1812524290605277184,
            "femaleVariant": "鑑識はまだ？"
          },
          {
            "stringId": 1812524433815592960,
            "femaleVariant": "渋滞のようで、少し遅れると"
          },
          {
            "stringId": 1813265806692933632,
            "femaleVariant": "ジングウジへようこそ"
          },
          {
            "stringId": 1813272095229698048,
            "femaleVariant": "ああ、誰かと思えば！ ようこそいらっしゃいました！"
          },
          {
            "stringId": 1813312626148204548,
            "femaleVariant": "ねえ、ゼイン。どうしたの？ 何かあった？",
            "maleVariant": "おい、ゼイン。どうした？ 何かあったのか？"
          },
          {
            "stringId": 1876499120005038080,
            "femaleVariant": "さて、今日は何をお求めですか？ 最近は、昔流行ったスパンコールがまたブームですよ"
          },
          {
            "stringId": 1876513924472094720,
            "femaleVariant": "いえいえ、お客様のようなスリムな体形でしたら、ぴったりですよ！ スターになりたかったら、まずは見た目から輝かせないと"
          },
          {
            "stringId": 1876521445077270528,
            "femaleVariant": "もし魅力をご理解頂けなかったとしたら、それはわたくしの力不足。そうですね、お客様には――"
          },
          {
            "stringId": 1876563634742345728,
            "femaleVariant": "警備員！ 扉を守るんだ！"
          },
          {
            "stringId": 1876576594135072768,
            "femaleVariant": "危うく、その端正なお顔立ちを忘れてしまうところでしたよ！"
          },
          {
            "stringId": 1876600422496190464,
            "femaleVariant": "コーポ向けのスーツ屋に冷やかしに来るから、こんな目に遭うんだぞ、V！"
          },
          {
            "stringId": 1876624384722714624,
            "femaleVariant": "聞こえました…？"
          },
          {
            "stringId": 1876640938852073472,
            "femaleVariant": "まったく！"
          },
          {
            "stringId": 1877664478715953152,
            "femaleVariant": "そんなこと言ってる場合！？",
            "maleVariant": "そんなこと言ってる場合か！？"
          },
          {
            "stringId": 1877664573758881792,
            "femaleVariant": "忙しそうだな"
          },
          {
            "stringId": 1877682696507604992,
            "femaleVariant": "よかった… 死体を片付ければ、すぐに仕事を再開できる"
          },
          {
            "stringId": 1877685692230516736,
            "femaleVariant": "座ってくれ。ほら、さっさと終わらせよう"
          },
          {
            "stringId": 1877713966738423808,
            "femaleVariant": "サイバーサイコシス訴訟法第105条に基づき、証言内容は記録ならびに保管される"
          },
          {
            "stringId": 1877751644003483648,
            "femaleVariant": "失礼、癖で"
          },
          {
            "stringId": 1877771288277925888,
            "femaleVariant": "ジョニー・シルヴァーハンド、伝説のロッカー。デジタルゴーストでもあり…"
          },
          {
            "stringId": 1877771456167526400,
            "femaleVariant": "ジョニー、黙って！",
            "maleVariant": "ジョニー、黙れ！"
          },
          {
            "stringId": 1877771572534296576,
            "femaleVariant": "たまにはお前以外の奴とも話をさせてくれよ"
          },
          {
            "stringId": 1877772509407592448,
            "femaleVariant": "ゼインと話してたら、急に…"
          },
          {
            "stringId": 1877772806548865024,
            "femaleVariant": "ゼインがあたしに派手な服を売りつけようとしてたら、いきなりあの男が現れて、警備員を殺したの",
            "maleVariant": "ゼインが俺に派手な服を売りつけようとしてたら、いきなりあの男が現れて、警備員を殺したんだ"
          },
          {
            "stringId": 1877834586952286208,
            "femaleVariant": "そしてゼインが警報を鳴らし、店内に閉じ込めた。いい判断ね",
            "maleVariant": "そしてゼインが警報を鳴らし、店内に閉じ込めた。いい判断だ"
          },
          {
            "stringId": 1877834779571503104,
            "femaleVariant": "緊急時のマニュアルに従って、正しい判断をされたようだ。おかげで被害を抑えることができた"
          },
          {
            "stringId": 1877834884529766400,
            "femaleVariant": "被害ね… おかげであんたたちが現れる前まで、命がけの鬼ごっこをする羽目になったけど",
            "maleVariant": "被害ね… おかげであんたらが現れる前まで、命がけの鬼ごっこをする羽目になったけどな"
          },
          {
            "stringId": 1877872788505346048,
            "femaleVariant": "他に、何かおかしいことは？"
          },
          {
            "stringId": 1877881403857420288,
            "femaleVariant": "何言ってんです！"
          },
          {
            "stringId": 1877908632020697088,
            "femaleVariant": "それ、悪人にしか使わないのよね？",
            "maleVariant": "それ、悪人にしか使わないんだよな？"
          },
          {
            "stringId": 1877922675322241024,
            "femaleVariant": "“滅多に”ね。あたしの耳には、そんなケースは一度もないように聞こえるけど",
            "maleVariant": "“滅多に”ね。俺の耳には、そんなケースは一度もないように聞こえるが"
          },
          {
            "stringId": 1877933966556381184,
            "femaleVariant": "うちに入らない？ 手が足りてなくて"
          },
          {
            "stringId": 1877938078836572164,
            "femaleVariant": "考えておく"
          },
          {
            "stringId": 1877938135895883776,
            "femaleVariant": "ありがとう、でもやめておく"
          },
          {
            "stringId": 1877938247313375232,
            "femaleVariant": "あたしが警官に？ そういえば… 6歳くらいまではあこがれてたっけ。ま、考えとく",
            "maleVariant": "俺が警官に？ そういえば、6歳くらいまでは憧れてたっけな… ま、考えとく"
          },
          {
            "stringId": 1877938375340310528,
            "femaleVariant": "あたしのことを買ってくれて嬉しいけど、きっとスパンコール姿の傭兵のほうが似合ってると思う",
            "maleVariant": "俺のことを買ってくれてありがたいが、きっとスパンコール姿の傭兵のほうが似合ってると思う"
          },
          {
            "stringId": 1877957115842904064,
            "femaleVariant": "いつでも歓迎よ。きっと向いてるから"
          },
          {
            "stringId": 1898174020222435328,
            "femaleVariant": "V様。店が再び営業を再開したら、またお越しください。V様の為だけに、特別なお召し物を用意しますので"
          },
          {
            "stringId": 1898174350112833536,
            "femaleVariant": "ありがと。じゃあね",
            "maleVariant": "どうも。じゃあな"
          },
          {
            "stringId": 1905457797663539200,
            "femaleVariant": "え？ それだけ？ もう終わり？",
            "maleVariant": "え？ それだけ？ もう終わりか？"
          },
          {
            "stringId": 1905464296318156800,
            "femaleVariant": "従業員を細切れにして店の評判を落とした以外に？ いえ、何も！"
          },
          {
            "stringId": 1905465107513327616,
            "femaleVariant": "協力してくれてありがとう。以上よ"
          },
          {
            "stringId": 2005472613429395456,
            "femaleVariant": "神経プロセッサーと相性が悪くて、前頭前皮質を変に刺激するっていう話を聞いた。そうじゃなかったらあたしも使ってたと思う",
            "maleVariant": "神経プロセッサーと相性が悪くて、前頭前皮質を変に刺激するっていう話を聞いた。そうじゃなかったら俺も使ってたと思う"
          },
          {
            "stringId": 2005473502420516864,
            "femaleVariant": "へえ。マックス・タックはアラサカのブレードを標準支給されるんだと思ったけど… その扱い方を見ると…"
          },
          {
            "stringId": 2114168369483497476,
            "femaleVariant": "V、傭兵よ",
            "maleVariant": "V、傭兵だ"
          },
          {
            "stringId": 2114311593137160192,
            "femaleVariant": "[立ち上がる]"
          },
          {
            "stringId": 2231422728571437060,
            "femaleVariant": "あんたと一緒にしないで",
            "maleVariant": "あんたと一緒にするな"
          },
          {
            "stringId": 2231422870909337600,
            "femaleVariant": "話を戻すけど――あんたも興奮したんでしょ？ あいつが息絶えた時… まだ血は流れてるのに、命の灯が消えた時"
          },
          {
            "stringId": 2231423120386539520,
            "femaleVariant": "確かにアドレナリンは感じたけど… 別に高揚したわけじゃない",
            "maleVariant": "確かにアドレナリンは感じたが… 別に高揚したわけじゃない"
          },
          {
            "stringId": 2231423201336606720,
            "femaleVariant": "サイコのお仲間を探してるの？ 悪いけどあたしは違う",
            "maleVariant": "サイコのお仲間を探してるのか？ 悪いが俺は違う"
          },
          {
            "stringId": 2231424297140469760,
            "femaleVariant": "まあ、少しは…",
            "maleVariant": "まあ、少しはな…"
          },
          {
            "stringId": 2231425449634545664,
            "femaleVariant": "あんたは殺しを楽しんでる。相手がサイコだろうと、普通の人間だろうと"
          },
          {
            "stringId": 2231425491946684416,
            "femaleVariant": "同じ穴のムジナでしょ？ 奴を始末した時の顔を見た。隠せないものよ"
          }
        ]
      }
    }
  }
}