  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいか、聞け。俺たちは強盗だ！ カウンターにいるおしゃべりさんよ！ そう、あんただ！ 黙って俺が見える位置に手を上げろ！",
                    "maleVariant": null,
                    "stringId": 1780995729365295104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "他の奴らはその場で指一本動かすなよ！ 少しでもおかしな真似をしたら、サイドメニューで鉛玉の盛り合わせをお届けしてやる。わかったな？",
                    "maleVariant": null,
                    "stringId": 1782067364239400960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "目立ちたがり屋がいるようね",
                    "maleVariant": "目立ちたがり屋がいるようだな",
                    "stringId": 1782192784297320448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうやら、目立ちたがり屋がいるようね",
                    "maleVariant": "どうやら、目立ちたがり屋がいるようだな",
                    "stringId": 1782196390325018624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[支払う] 金ならもってって",
                    "maleVariant": "[支払う] 金ならもってけ",
                    "stringId": 1782197147262672896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺のいったことを理解できない奴がいるみたいだな？",
                    "maleVariant": null,
                    "stringId": 1782197152480387076
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあまずはお前からだ。ほら、大人しく財布を渡せ",
                    "maleVariant": null,
                    "stringId": 1782201025215488000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "死ね、クソアマ！",
                    "maleVariant": "死ね、クソ野郎！",
                    "stringId": 1782216558551916548
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここは別のギャングのシマでしょ？",
                    "maleVariant": "ここは別のギャングのシマだろ？",
                    "stringId": 1782224722445885440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしの力量を見誤ってるみたいね",
                    "maleVariant": "俺の力量を見誤ってるみたいだな",
                    "stringId": 1782224870588702720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まずは落ち着きましょ？ エディーなら持ってって。あたし一人で、ここにいる全員の倍は持ってるから",
                    "maleVariant": "まずは落ち着けよ。エディーが欲しいなら持ってけ。俺一人で、ここにいる全員の倍は持ってる",
                    "stringId": 1782237080257978368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "星条旗の服はどうしたの？",
                    "maleVariant": "星条旗の服はどうした？",
                    "stringId": 1782237871102386180
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いったい何の話だ？",
                    "maleVariant": null,
                    "stringId": 1782237875246358528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よく今まで死なずにすんだね",
                    "maleVariant": "よく今まで死なずにすんだな",
                    "stringId": 1782239257907060740
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何を言ってやがる？",
                    "maleVariant": null,
                    "stringId": 1782239262269136896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言ってることがムチャクチャよ",
                    "maleVariant": "言ってることがムチャクチャだ",
                    "stringId": 1782245004254535680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言ってることがムチャクチャよ。手を上げた状態で、どうやって財布を取り出せって言うの？",
                    "maleVariant": "言ってることがムチャクチャだろ。手を上げた状態で、どうやって財布を取り出せって言うんだ？",
                    "stringId": 1782245457843347460
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "笑えねえな。どんな奴が真っ先に死ぬか教えてやる。お前みたいなお笑い担当だよ",
                    "maleVariant": null,
                    "stringId": 1782245462373195776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ずっと平和だったのに！ 肝心な時にシックス・ストリートは何をやってんだ？",
                    "maleVariant": null,
                    "stringId": 1782289364555816964
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "長年この店をやってるが、こんなことになるなんて、みんな死んでるじゃないか！",
                    "maleVariant": null,
                    "stringId": 1782293577079873540
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はした金のために、ここまですることはなかったろう？ で、出てってくれ！",
                    "maleVariant": null,
                    "stringId": 1782295948002484224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いらっしゃい。何を飲む？ コーヒー？ それともお酒かい？",
                    "maleVariant": null,
                    "stringId": 1786730417564311552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[寄りかかる] カラベラ・フェリスを",
                    "maleVariant": null,
                    "stringId": 1786739542960529408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうね、カラベラをもらえる？",
                    "maleVariant": "そうだな、カラベラをもらえるか？",
                    "stringId": 1790903799100891136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "エスプレッソはこの辺で一番って評判なんだ。一度は試したほうがいいよ！",
                    "maleVariant": null,
                    "stringId": 1812389505638580224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "財布はテーブルの上に出せ！ 黙って口を閉じてりゃケガせずに済む",
                    "maleVariant": null,
                    "stringId": 1812396084203077632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "上に向けて伸ばすか、財布を手に取るか。どっちかにして",
                    "maleVariant": "上に向けて伸ばすか、財布を手に取るか。どっちかにしろ",
                    "stringId": 1812401599427624960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふざけてないで、金を出せ",
                    "maleVariant": null,
                    "stringId": 1812556753393139712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "知ってるでしょ、この辺を牛耳ってるシックス・ストリートの軍用ジャケットよ。EMPグレネードを手に、悪いギャングから人々を守ってる",
                    "maleVariant": "知ってるだろ、この辺を牛耳ってるシックス・ストリートの軍用ジャケットだ。EMPグレネードを手に、悪いギャングから人々を守ってる",
                    "stringId": 1812559385050464256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼らの縄張りで強盗するくらいだから、当然仲間なのよね？ 許可なしにそんな馬鹿なことをするギャングはいないもの",
                    "maleVariant": "奴らの縄張りで強盗するくらいだから、当然仲間なんだよな？ 許可なしにそんな馬鹿なことをするギャングはいないぞ",
                    "stringId": 1812559713984561152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さっきそいつらの一人を見ましたぜ！ 銃声を聞かれたかも！",
                    "maleVariant": null,
                    "stringId": 1812559963260436480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おうおう、こんなところに聖人君子様がいらっしゃるぜ。ほかの連中はツいてたな",
                    "maleVariant": null,
                    "stringId": 1812637192107573248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたがいてくれて助かった。何でも好きなものを頼んでくれ！",
                    "maleVariant": null,
                    "stringId": 1812727409657233408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "コーヒーはやめとく。スパンキーモンキーをお願い",
                    "maleVariant": "コーヒーはやめとくよ。スパンキーモンキーを頼む",
                    "stringId": 1815479071203377152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[寄りかかる] スパンキーモンキーを1杯",
                    "maleVariant": null,
                    "stringId": 1815479609248694272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あまり時間がないから、ティアンチャをお願い",
                    "maleVariant": "あまり時間がないから、ティアンチャを頼む",
                    "stringId": 1815480061881204736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[寄りかかる] ティアンチャをお願い",
                    "maleVariant": "[寄りかかる] ティアンチャを頼む",
                    "stringId": 1815480203413798912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しけた強盗ね",
                    "maleVariant": "しけた強盗だな",
                    "stringId": 1815492401926225924
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "レストランに強盗だなんて、惨めね。今までは婆さんの財布でもスッたりしてたの？",
                    "maleVariant": "レストランに強盗だなんて、惨めだな。今までは婆さんの財布でもスッたりしてたのか？",
                    "stringId": 1815540035831447552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この人たちが武装してるように見える？ 手を出す必要はないはずよ",
                    "maleVariant": "この人たちが武装してるように見えるか？ 手を出す必要はないはずだ",
                    "stringId": 1815540238030454784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "客に手を出さないで！",
                    "maleVariant": "客に手を出すな！",
                    "stringId": 1815541281892057088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前ら、行くぞ！",
                    "maleVariant": null,
                    "stringId": 1815565365216526336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でしょうね。連中は生粋の兵士だから、どんな異変も見逃さない",
                    "maleVariant": "だろうな。連中は生粋の兵士だから、どんな異変も見逃さない",
                    "stringId": 1815590949464293376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だからコーストビューのほうがいいって！",
                    "maleVariant": null,
                    "stringId": 1815615294983094272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "黙れ！ さっさとずらかるぞ！",
                    "maleVariant": null,
                    "stringId": 1815620602354712576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "相手を正しく見極められない奴は、ストリートじゃ生きていけないよ",
                    "maleVariant": "相手を正しく見極められない奴は、ストリートじゃ生きていけない",
                    "stringId": 1815679479226023936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "前にも似たような連中に出くわしたことがあるけど、そいつらは全員死んだ。理由？ あたしの力量を計り損ねたからよ",
                    "maleVariant": "前にも似たような連中に出くわしたことがあるが、そいつらは全員死んだ。理由？ 俺の力量を計り損ねたからだ",
                    "stringId": 1815679735615438848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "フン、はったりだ。そんなのに騙されると思うのか！",
                    "maleVariant": null,
                    "stringId": 1815680053141028864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしはメイルストロームやアラサカとも渡り合った。あんたで相手が務まると思う？",
                    "maleVariant": "俺はメイルストロームやアラサカとも渡り合った。お前で相手が務まると思うか？",
                    "stringId": 1815680884636631040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その引き金を引いたらおしまいよ。約束してあげる",
                    "maleVariant": "その引き金を引いたらおしまいだ。約束してやるよ",
                    "stringId": 1815681026471215104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クソ… 行くぞ",
                    "maleVariant": null,
                    "stringId": 1815681172667875328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[離れる]",
                    "maleVariant": null,
                    "stringId": 2167863547853168644
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "飲み物がほしい",
                    "maleVariant": null,
                    "stringId": 2189463543825059844
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}