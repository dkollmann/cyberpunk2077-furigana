    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1675027099592658944,
            "femaleVariant": "リバー！？"
          },
          {
            "stringId": 1689371544446275584,
            "femaleVariant": "ここを登って、中に入る方法を探そう"
          },
          {
            "stringId": 1689392059374342144,
            "femaleVariant": "持ち上がりそうにない"
          },
          {
            "stringId": 1773826441626128384,
            "femaleVariant": "ここの予算を考えると、たいした警備は敷いてないはずだ"
          },
          {
            "stringId": 1773828616758628352,
            "femaleVariant": "自閉症児の夢を編集する部屋が匂う"
          },
          {
            "stringId": 1773841199402192896,
            "femaleVariant": "ヤウェン！"
          },
          {
            "stringId": 1773851474188701700,
            "femaleVariant": "ハリスの夢のBDはどこにある？"
          },
          {
            "stringId": 1773851480631152640,
            "femaleVariant": "銃撃で大脳皮質が損傷を受けたの。支離滅裂な断片しか得られない"
          },
          {
            "stringId": 1773854874628395012,
            "femaleVariant": "リバーのことはそこまで知らない"
          },
          {
            "stringId": 1773856102368284672,
            "femaleVariant": "まだ嫌な思いをしたことはない"
          },
          {
            "stringId": 1773857055767138304,
            "femaleVariant": "リバーとはまだ付き合いが浅くて",
            "maleVariant": "リバーとはまだ付き合いが浅くてな"
          },
          {
            "stringId": 1773857140055871488,
            "femaleVariant": "リバーはいい友達だし、特に嫌な思いもしてない",
            "maleVariant": "リバーはいい友達だし、特に嫌な思いもしてないが"
          },
          {
            "stringId": 1773858241412993024,
            "femaleVariant": "何か約束されても信じちゃダメよ！"
          },
          {
            "stringId": 1773860277261045760,
            "femaleVariant": "助言をどうも。でも今は時間がない",
            "maleVariant": "助言をどうも。だが今は時間がない"
          },
          {
            "stringId": 1773865958143823872,
            "femaleVariant": "だから協力したくないってこと？",
            "maleVariant": "だから協力したくないってことか？"
          },
          {
            "stringId": 1773870539397533696,
            "femaleVariant": "どういうことだ！？"
          },
          {
            "stringId": 1773872569105125376,
            "femaleVariant": "有効とは限らない"
          },
          {
            "stringId": 1773872570514411520,
            "femaleVariant": "好きな音楽や映画は子供に効果があるだろ。試す価値はある"
          },
          {
            "stringId": 1773872924161347604,
            "femaleVariant": "行き止まりか。残念ね",
            "maleVariant": "行き止まりか。残念だ"
          },
          {
            "stringId": 1773876053581893632,
            "femaleVariant": "残念だけど、ここまでのようね、リバー",
            "maleVariant": "残念だが、ここまでのようだな、リバー"
          },
          {
            "stringId": 1773876055108620288,
            "femaleVariant": "早合点する前に、視聴覚刺激を試したい"
          },
          {
            "stringId": 1773876059806240772,
            "femaleVariant": "それなら――"
          },
          {
            "stringId": 1773876061534294016,
            "femaleVariant": "視聴覚刺激を試すんだ"
          },
          {
            "stringId": 1776289738850799616,
            "femaleVariant": "まだ実験段階だが、成果は出はじめてる"
          },
          {
            "stringId": 1776293089244057600,
            "femaleVariant": "で、これからどこに向かうの…？",
            "maleVariant": "で、これからどこに向かうんだ…？"
          },
          {
            "stringId": 1776448006181064704,
            "femaleVariant": "空のBDケース"
          },
          {
            "stringId": 1776449705008402436,
            "femaleVariant": "ブレインダンスの編集機材――使えるか？"
          },
          {
            "stringId": 1776547502302924800,
            "femaleVariant": "V！ きっとこの部屋だ！"
          },
          {
            "stringId": 1776548097575325696,
            "femaleVariant": "調べるのを手伝ってくれ！"
          },
          {
            "stringId": 1776550220446797824,
            "femaleVariant": "今行く！"
          },
          {
            "stringId": 1782105289237073920,
            "femaleVariant": "夢を再生可能なBDとして記録するんだ"
          },
          {
            "stringId": 1782186759179866112,
            "femaleVariant": "少しなら"
          },
          {
            "stringId": 1805169597131755524,
            "femaleVariant": "ねえ、リバー！ 見つけたかも！",
            "maleVariant": "リバー！ 見つけたかもしれん！"
          },
          {
            "stringId": 1813988199772266496,
            "femaleVariant": "よう、V"
          },
          {
            "stringId": 1813988499899883524,
            "femaleVariant": "何があったの？",
            "maleVariant": "何があった？"
          },
          {
            "stringId": 1813988624302940160,
            "femaleVariant": "それで、何があったの？",
            "maleVariant": "それで、何があったんだ？"
          },
          {
            "stringId": 1813988651129708544,
            "femaleVariant": "いい車ね。新車？",
            "maleVariant": "いい車だ。新車か？"
          },
          {
            "stringId": 1813990130813050880,
            "femaleVariant": "放っておけない"
          },
          {
            "stringId": 1813990233724493824,
            "femaleVariant": "放っておけないね",
            "maleVariant": "放っておけないな"
          },
          {
            "stringId": 1813990264443576320,
            "femaleVariant": "個人的感情を理由に、捜査から外されてるとか？"
          },
          {
            "stringId": 1813990304843112448,
            "femaleVariant": "近親者を捜査から外すのは仕方ない"
          },
          {
            "stringId": 1813990337760010240,
            "femaleVariant": "ランディの失踪について、他に知ってることは？"
          },
          {
            "stringId": 1813992686150819840,
            "femaleVariant": "いい車ね",
            "maleVariant": "いい車だ"
          },
          {
            "stringId": 1813993591583612928,
            "femaleVariant": "近親者を捜査から外すのは仕方ない"
          },
          {
            "stringId": 1814003920896073728,
            "femaleVariant": "ああ。力を貸してくれると助かる"
          },
          {
            "stringId": 1814004484207878144,
            "femaleVariant": "この件には一歩たりとも近づくな、とさ"
          },
          {
            "stringId": 1814023622515040256,
            "femaleVariant": "調子はどう？",
            "maleVariant": "調子はどうだ？"
          },
          {
            "stringId": 1814023728412827648,
            "femaleVariant": "どうだかな…"
          },
          {
            "stringId": 1814040188891766784,
            "femaleVariant": "いや"
          },
          {
            "stringId": 1814041463389757440,
            "femaleVariant": "無駄話する気分じゃないか"
          },
          {
            "stringId": 1814042048075735040,
            "femaleVariant": "ああ、すまん"
          },
          {
            "stringId": 1814077877280120832,
            "femaleVariant": "犯人が捕まってよかったじゃない",
            "maleVariant": "犯人が捕まってよかったじゃないか"
          },
          {
            "stringId": 1814078485403869184,
            "femaleVariant": "おいっ子のランディが… 少し前にいなくなったんだ"
          },
          {
            "stringId": 1814079179175305216,
            "femaleVariant": "今の映像――子供がランディの靴を履いてた"
          },
          {
            "stringId": 1814079280929120256,
            "femaleVariant": "ウソでしょ…",
            "maleVariant": "ウソだろ…"
          },
          {
            "stringId": 1814142509676044288,
            "femaleVariant": "ゴナガルが指揮してるが、使えない奴でな。今のところ何も進展してない"
          },
          {
            "stringId": 1814149350468427776,
            "femaleVariant": "だから客観的に判断できるお前の手を借りたいんだ"
          },
          {
            "stringId": 1814151645440581632,
            "femaleVariant": "妹とはそんなに仲がよくなくてな… あまり知らないんだ"
          },
          {
            "stringId": 1814152076011053056,
            "femaleVariant": "俺があいつを支えてやってれば、こんなことには…"
          },
          {
            "stringId": 1814206842246340608,
            "femaleVariant": "ここで何してるの！？ 警察をクビになったんでしょ！"
          },
          {
            "stringId": 1814208460392046596,
            "femaleVariant": "あんたは？"
          },
          {
            "stringId": 1814208633415475200,
            "femaleVariant": "クビってどういうこと？",
            "maleVariant": "クビってどういうことだ？"
          },
          {
            "stringId": 1814208672623828992,
            "femaleVariant": "それで、あんたは？"
          },
          {
            "stringId": 1814210675940859904,
            "femaleVariant": "本物の警官を呼ぶわよ！"
          },
          {
            "stringId": 1814211469452845056,
            "femaleVariant": "おいおい、待て待て… 聞いてくれ"
          },
          {
            "stringId": 1814212632684339200,
            "femaleVariant": "何も聞いてないの？"
          },
          {
            "stringId": 1814213143282130944,
            "femaleVariant": "ヤウェン、違うんだ…"
          },
          {
            "stringId": 1814213515635662848,
            "femaleVariant": "ただでさえまずい立場でしょう！？"
          },
          {
            "stringId": 1814214785989681152,
            "femaleVariant": "おいがハリスにさらわれたんだ"
          },
          {
            "stringId": 1815419155016130560,
            "femaleVariant": "医学部の試験を覚えてるか？"
          },
          {
            "stringId": 1815421200192315392,
            "femaleVariant": "あん時は、りっぱな成績で合格したよな"
          },
          {
            "stringId": 1815421880692973572,
            "femaleVariant": "ピーター・パンのBDを持たずに帰れない"
          },
          {
            "stringId": 1815422651069177856,
            "femaleVariant": "NCPDでは解決できない"
          },
          {
            "stringId": 1815425328226312192,
            "femaleVariant": "ピーター・パンのBDなしには帰れない"
          },
          {
            "stringId": 1815425328326975488,
            "femaleVariant": "あんたも警察と仕事をしてるなら、どんな連中かわかってるはずよ",
            "maleVariant": "あんたも警察と仕事をしてるなら、どんな連中かわかってるはずだ"
          },
          {
            "stringId": 1815426665135546368,
            "femaleVariant": "最後の警告よ。ここから出てって！"
          },
          {
            "stringId": 1815429442536546304,
            "femaleVariant": "だからといって法律を無視する理由にはならないわ！"
          },
          {
            "stringId": 1815431440434536448,
            "femaleVariant": "そんな話を持ち出すなんて、信じられない！"
          },
          {
            "stringId": 1815451219329359872,
            "femaleVariant": "ハ！ 私も同じ風に思ってた"
          },
          {
            "stringId": 1815592442987880448,
            "femaleVariant": "試しましょう",
            "maleVariant": "試してみよう"
          },
          {
            "stringId": 1815593081579053056,
            "femaleVariant": "犯人の趣味なんて知らないでしょ！"
          },
          {
            "stringId": 1815594199881162752,
            "femaleVariant": "調べるさ。で、手伝う気はあるんだな？"
          },
          {
            "stringId": 1815594542371250176,
            "femaleVariant": "とにかく… もう行って。いま正面玄関を開ける"
          },
          {
            "stringId": 1815611377200209920,
            "femaleVariant": "確かに、視床下部の刺激の受け方と、ホルモン生成に類似性は見つかってるけど――"
          },
          {
            "stringId": 1815630564043857920,
            "femaleVariant": "わかった！"
          },
          {
            "stringId": 1834272801328074756,
            "femaleVariant": "捜査に加えてもらえないの？",
            "maleVariant": "捜査に加えてもらえないのか？"
          },
          {
            "stringId": 1834274080641126400,
            "femaleVariant": "その失踪について詳しく教えて",
            "maleVariant": "その失踪について詳しく教えてくれ"
          },
          {
            "stringId": 1834367518074081280,
            "femaleVariant": "BDの場所に心当たりは？"
          },
          {
            "stringId": 1864913049063075844,
            "femaleVariant": "[キャビネットを開ける] わざわざ侵入する必要はあったの？",
            "maleVariant": "[キャビネットを開ける] わざわざ侵入する必要はあったのか？"
          },
          {
            "stringId": 1864977188678610944,
            "femaleVariant": "この男は自分のことばっかりで、周りを不幸にするのよ！",
            "maleVariant": "どうせこの人のことをよく知らないんでしょ？ 最低な奴よ！"
          },
          {
            "stringId": 1883514965003653120,
            "femaleVariant": "まあ… 見てのとおりだ"
          },
          {
            "stringId": 1883671019888889856,
            "femaleVariant": "何をすればいいの？",
            "maleVariant": "何をすればいいんだ？"
          },
          {
            "stringId": 1883672514403291136,
            "femaleVariant": "植物状態の男から話は聞けない… だから夢を覗いてやる"
          },
          {
            "stringId": 1883680623955857408,
            "femaleVariant": "夢を覗く？"
          },
          {
            "stringId": 1883680765840773120,
            "femaleVariant": "ああ、そうだ。歩きながら説明する"
          },
          {
            "stringId": 1883753693160722432,
            "femaleVariant": "何をしてるかわかってるの？ ただでさえまずい立場でしょう？"
          },
          {
            "stringId": 1883778889603141632,
            "femaleVariant": "“法律”だ？"
          },
          {
            "stringId": 1883779644712079360,
            "femaleVariant": "クソ、ヤウェン！ いったい何様のつもりだ！？"
          },
          {
            "stringId": 1883783963972784128,
            "femaleVariant": "忘れられるわけがない。女子トイレで火事が起きて、全員が2時間避難させられたことをな…"
          },
          {
            "stringId": 1883791661476925440,
            "femaleVariant": "もうここには二度と顔を見せないで！"
          },
          {
            "stringId": 1883858199814918144,
            "femaleVariant": "試さない理由は？"
          },
          {
            "stringId": 1883858833960128516,
            "femaleVariant": "じゃあ試しましょ",
            "maleVariant": "じゃあ試そう"
          },
          {
            "stringId": 1883859388900102144,
            "femaleVariant": "試すべきじゃない理由が？"
          },
          {
            "stringId": 1883862843144327168,
            "femaleVariant": "リバーと違って、私は医大できちんとした神経科学を学んでるからよ"
          },
          {
            "stringId": 1894874415004520448,
            "femaleVariant": "研究所だ。ピーター・パンの夢のBDを入手する"
          },
          {
            "stringId": 1894877089393414144,
            "femaleVariant": "アポは？ とってるんでしょうね？",
            "maleVariant": "アポは？ とってるんだろうな？"
          },
          {
            "stringId": 1894877208176103424,
            "femaleVariant": "とってないが、まあとにかく一緒に来てくれ…"
          },
          {
            "stringId": 1894909330689495040,
            "femaleVariant": "アンソニー・ハリスのラベルがあるはず"
          },
          {
            "stringId": 1894947414986592256,
            "femaleVariant": "クビじゃない、停職だ"
          },
          {
            "stringId": 1894955960360898560,
            "femaleVariant": "それは本当に気の毒ね。でもここは証拠を保管する場所。厳重な規則があるの"
          },
          {
            "stringId": 1894957500442218500,
            "femaleVariant": "子供の命を救おうとしてるのよ",
            "maleVariant": "子供の命を救おうとしてるんだぞ"
          },
          {
            "stringId": 1894959773285224448,
            "femaleVariant": "「規則」？ おいっ子の命に関わるのよ！",
            "maleVariant": "「規則」？ おいっ子の命に関わるんだぞ！"
          },
          {
            "stringId": 1894959803601653760,
            "femaleVariant": "ランディや他の子供たちは… 今もどこかにいて、助けを待ってる"
          },
          {
            "stringId": 1894960504184639488,
            "femaleVariant": "子供の命に！"
          },
          {
            "stringId": 1894961343213211648,
            "femaleVariant": "それは警察が捜査を進めてるわ"
          },
          {
            "stringId": 1894962134812594176,
            "femaleVariant": "手段を選んでる場合じゃない"
          },
          {
            "stringId": 1894962770467753984,
            "femaleVariant": "それは警察に任せておけばいい"
          },
          {
            "stringId": 1894964067061338112,
            "femaleVariant": "もう行って"
          },
          {
            "stringId": 1894966039743172608,
            "femaleVariant": "早く行きなさい！"
          },
          {
            "stringId": 1895370679434317824,
            "femaleVariant": "正面からは行かない"
          },
          {
            "stringId": 1895372889647976448,
            "femaleVariant": "別の入口を探そう。手早くな"
          },
          {
            "stringId": 1895377178508365824,
            "femaleVariant": "つまり違法捜査ね…",
            "maleVariant": "つまり違法捜査か…"
          },
          {
            "stringId": 1895381167392133120,
            "femaleVariant": "来るのは初めてだ。手を貸してくれ"
          },
          {
            "stringId": 1895422081988792320,
            "femaleVariant": "よし… 次は？"
          },
          {
            "stringId": 1896371162655461380,
            "femaleVariant": "どうやってそのBDを探すつもり？",
            "maleVariant": "どうやってそのBDを探すつもりだ？"
          },
          {
            "stringId": 1896371162655461392,
            "femaleVariant": "自閉症児の夢を編集する部屋を探せ"
          },
          {
            "stringId": 1896371162655461408,
            "femaleVariant": "ああ、それから――ピーター・パンの本名はアンソニー・ハリスだ"
          },
          {
            "stringId": 1896372059598983168,
            "femaleVariant": "研究所は監査を受けてる最中だから無人のはずだ。好きなだけ音を立てていい"
          },
          {
            "stringId": 1896400372560269312,
            "femaleVariant": "今日は監査があって誰もいないはずだ。声は潜めなくていい"
          },
          {
            "stringId": 1896593050564530176,
            "femaleVariant": "BDの形跡も、何もないな"
          },
          {
            "stringId": 1896608396482678788,
            "femaleVariant": "何もなし、か"
          },
          {
            "stringId": 1896656211732828164,
            "femaleVariant": "鍵がかかってて入れない"
          },
          {
            "stringId": 1896670895219380228,
            "femaleVariant": "何かのチップがあるが、空だ。ほかを探そう"
          },
          {
            "stringId": 1904088086659174400,
            "femaleVariant": "何かあったか？"
          },
          {
            "stringId": 1904101395957518336,
            "femaleVariant": "まだ何も"
          },
          {
            "stringId": 1904101891858468864,
            "femaleVariant": "アンソニー・ハリス"
          },
          {
            "stringId": 1904102382759809024,
            "femaleVariant": "引き出しを見てみよう"
          },
          {
            "stringId": 1905171791985217536,
            "femaleVariant": "ハリスのラベルを探せよ"
          },
          {
            "stringId": 1905222733300469760,
            "femaleVariant": "さっき自閉症児って言った？",
            "maleVariant": "さっき自閉症児って言ったか？"
          },
          {
            "stringId": 1905223486597468160,
            "femaleVariant": "警官になりたい子がいるみたいよ",
            "maleVariant": "警官になりたい子がいるみたいだぞ"
          },
          {
            "stringId": 1905228721340518404,
            "femaleVariant": "機材はそれなりね",
            "maleVariant": "機材はそれなりだな"
          },
          {
            "stringId": 1905242175761780736,
            "femaleVariant": "ビル・グラハム？"
          },
          {
            "stringId": 1905242790327980036,
            "femaleVariant": "記録しているわけね…",
            "maleVariant": "記録しているわけか…"
          },
          {
            "stringId": 1906928719557582848,
            "femaleVariant": "俺一人じゃ抱えきれない"
          },
          {
            "stringId": 1907056638179172352,
            "femaleVariant": "これを見てくれ"
          },
          {
            "stringId": 1907068594579595264,
            "femaleVariant": "うん？"
          },
          {
            "stringId": 1907069315060359168,
            "femaleVariant": "また政治家の話？",
            "maleVariant": "また政治家の話か？"
          },
          {
            "stringId": 1907069552676069376,
            "femaleVariant": "また政治家が死んだの？",
            "maleVariant": "また政治家が死んだのか？"
          },
          {
            "stringId": 1907071111933747200,
            "femaleVariant": "とにかく見るんだ"
          },
          {
            "stringId": 1907143083640250368,
