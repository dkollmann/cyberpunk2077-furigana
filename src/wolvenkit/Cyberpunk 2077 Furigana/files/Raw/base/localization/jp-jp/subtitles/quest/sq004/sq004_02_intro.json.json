  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今回の件をどう思う？",
              "maleVariant": "",
              "stringId": 1373668961164292096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今回の件だけど、どう思う？",
              "maleVariant": "今回の件だが、どう思う？",
              "stringId": 1373669418125324288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたがクランのリーダーになったの？",
              "maleVariant": "あんたがクランのリーダーになったのか？",
              "stringId": 1373670452625240064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で… あんたがクランの新しいリーダーになったの？",
              "maleVariant": "で… あんたがクランの新しいリーダーになったのか？",
              "stringId": 1373671351632359424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、どう攻めるつもりなの？",
              "maleVariant": "で、どう攻めるつもりなんだ？",
              "stringId": 1477974923013877760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私が援護するから、あんたが潜入して。ミッチが偵察の準備をしてる",
              "maleVariant": "",
              "stringId": 1477979634525892608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レイスの連中は、セメント工場の跡地に居座ってる",
              "maleVariant": "",
              "stringId": 1479346674213941248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴らはイナゴと一緒さ。襲来しては全てを奪いつくし、また次へ向かう。でも今回は…",
              "maleVariant": "",
              "stringId": 1479347624827777024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの壁が見える？ 防衛対策をとってるみたいだ",
              "maleVariant": "",
              "stringId": 1479372057571323904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "タイヤの跡。相当重たいモノを運んでる",
              "maleVariant": "",
              "stringId": 1479376254375129088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "壊されてなきゃ、監視カメラのネットワークを利用できる。制御室さえ見つければね",
              "maleVariant": "",
              "stringId": 1479391608077905920
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このでかい建物。ソウルはきっとこの中にいる",
              "maleVariant": "",
              "stringId": 1479407034358919168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソウルはあの中に拘束されてたんだ",
              "maleVariant": "",
              "stringId": 1479513216838889472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "準備はいい？",
              "maleVariant": "",
              "stringId": 1479527876250136576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正面切って戦うのは無謀すぎる",
              "maleVariant": "",
              "stringId": 1675264653528031232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[偵察を終える] とっかかりはつかめた",
              "maleVariant": "",
              "stringId": 1695589754559422468
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "えっ、なんでパナムとVが… 一体どうなってるの？",
              "maleVariant": "",
              "stringId": 1762000635137503232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、パナム。まさかお前、本当にやる気なのか？",
              "maleVariant": "",
              "stringId": 1763494413404876800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫、ソウルを助けに行きましょ",
              "maleVariant": "大丈夫だ、ソウルを助けに行こう",
              "stringId": 1776170022476066820
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう一度確認させて",
              "maleVariant": "もう一度確認させてくれ",
              "stringId": 1776187081431855104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう一度説明してくれる？",
              "maleVariant": "もう一度説明してくれるか？",
              "stringId": 1776189133855797248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、もちろん",
              "maleVariant": "ああ、もちろんだ",
              "stringId": 1776189518020489220
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし。ミッチ、バンの準備は？",
              "maleVariant": "",
              "stringId": 1776193869778661376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ひとりで行く。向こうで会いましょ",
              "maleVariant": "ひとりで行く。向こうで会おう",
              "stringId": 1776197513957748736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃ運転は任せる",
              "maleVariant": "",
              "stringId": 1776201328408801280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。ただ、これは時間との勝負だからね",
              "maleVariant": "",
              "stringId": 1776216527777968128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連絡路だ… ガレージとメインの建物を繋いでる",
              "maleVariant": "",
              "stringId": 1782434844964433924
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの監視所はキャンプ全体に繋がってるはず",
              "maleVariant": "",
              "stringId": 1833024026031828992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うまくいく確率は？",
              "maleVariant": "",
              "stringId": 1885204108233814020
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待つのは嫌い？",
              "maleVariant": "待つのは嫌いか？",
              "stringId": 1885205295892934660
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うまくいく確率は？",
              "maleVariant": "",
              "stringId": 1885206564569231360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じっと待ってるのは、性に合わない？",
              "maleVariant": "じっと待ってるのは、性に合わないか？",
              "stringId": 1885212122491793408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまり連中は長居する気ってこと。その理由はわからないけど、もしかしたらソウルのことが関係してるのかも",
              "maleVariant": "",
              "stringId": 1886414753999167488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "中に侵入して、確かめる必要がある",
              "maleVariant": "",
              "stringId": 1886416992515989504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただ壁の一部は警備が薄いから、そこを狙う手もある…",
              "maleVariant": "",
              "stringId": 1886419436151054336
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど",
              "maleVariant": "",
              "stringId": 1886426690350817280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんなに広いの？ 砂浜でダイヤを探すようなものよ",
              "maleVariant": "こんなに広いのか？ 砂浜でダイヤを探すようなもんだ",
              "stringId": 1886431474592063488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何かしらの機材か… ソウル以外にも捕虜がいるのかも",
              "maleVariant": "",
              "stringId": 1886524568712847360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いえ、あたし1人で行く",
              "maleVariant": "いや、俺1人で行く",
              "stringId": 1886532708195966976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。当たり前でしょ",
              "maleVariant": "ああ。当たり前だろ",
              "stringId": 1886534305605033984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "運転しないの？",
              "maleVariant": "運転しないのか？",
              "stringId": 1886535741281738752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スコープの調整をしとこうと思ってさ",
              "maleVariant": "",
              "stringId": 1886535893484642304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、来てくれてありがとう",
              "maleVariant": "",
              "stringId": 1895260328319660036
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チューマなら当然でしょ",
              "maleVariant": "チューマなら当然だろ",
              "stringId": 1895260328336437248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ノーマッドは助け合うものよ",
              "maleVariant": "ノーマッドは助け合うもんだ",
              "stringId": 1895260328336437264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "タダじゃ手伝わないけどね",
              "maleVariant": "タダじゃ手伝わないがな",
              "stringId": 1895260328336437280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手を貸してあげてもいい",
              "maleVariant": "手を貸してやってもいい",
              "stringId": 1895260328369991684
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「ただし」が付くんでしょ",
              "maleVariant": "",
              "stringId": 1895260328369991696
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "報酬が要る",
              "maleVariant": "",
              "stringId": 1895260328369991708
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっぱそうきたか。ホント、傭兵ってのはガメついね",
              "maleVariant": "",
              "stringId": 1895260328369991720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それなら、西海岸一のスナイパーライフルはどう？",
              "maleVariant": "",
              "stringId": 1895260328369991732
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしも元はノーマッドよ。都会に出たって、根っこにあるものは変わらない",
              "maleVariant": "俺も元はノーマッドだ。都会に出たって、根っこにあるものは変わらない",
              "stringId": 1895260328403546116
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かにそうかもね",
              "maleVariant": "",
              "stringId": 1895260328403546136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "助けるに決まってる。チューマなんだから当然じゃない",
              "maleVariant": "助けるに決まってる。チューマなんだから当然だろ",
              "stringId": 1895260328437100548
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本気にしていいの？ 私は相当しつこいよ",
              "maleVariant": "",
              "stringId": 1895260328437100560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、耐えてみせる",
              "maleVariant": "ああ、耐えてみせる",
              "stringId": 1895260328437100572
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、一体何があったの",
              "maleVariant": "で、一体何があったんだ",
              "stringId": 1895260328571318276
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソウルがさらわれた",
              "maleVariant": "",
              "stringId": 1895260328571318288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レイスの仕業さ。あの頑固オヤジ、ヘマしやがって",
              "maleVariant": "",
              "stringId": 1895260328571318300
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜレイスだと？",
              "maleVariant": "",
              "stringId": 1895260328588095488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中のキャンプを監視したところ、複数の捕虜がいることを確認した",
              "maleVariant": "",
              "stringId": 1895260328588095500
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたなら助けてくれると思って… 呼んだんだ",
              "maleVariant": "",
              "stringId": 1895260328588095512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何があったの？",
              "maleVariant": "何があったんだ？",
              "stringId": 1895260328638427136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アルデカルドスに戻ったの？",
              "maleVariant": "アルデカルドスに戻ったのか？",
              "stringId": 1895260328638427152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつの間にクランと仲直りしたの",
              "maleVariant": "いつの間にクランと仲直りしたんだ",
              "stringId": 1895260328671981572
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中には相変わらずイラつかされてるよ",
              "maleVariant": "",
              "stringId": 1895260328671981592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも… ほら…",
              "maleVariant": "",
              "stringId": 1895260328671981604
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "家族だから？",
              "maleVariant": "家族だからか？",
              "stringId": 1895260328671981616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういうこと",
              "maleVariant": "",
              "stringId": 1895260328671981628
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、それと報酬も用意してるから。西海岸一のスナイパーライフルだよ",
              "maleVariant": "",
              "stringId": 1895260328705536004
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "感謝の気持ちと一緒に受け取って",
              "maleVariant": "",
              "stringId": 1895260328722313220
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "良さそうね",
              "maleVariant": "良さそうだな",
              "stringId": 1895260328755867652
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんな平凡な言葉じゃ、この銃のすごさは語れないよ",
              "maleVariant": "",
              "stringId": 1895260328755867664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ行こう。ミッチが待ってる",
              "maleVariant": "",
              "stringId": 1895260328772644864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミッチは？ ここで会うはずなんだけど",
              "maleVariant": "",
              "stringId": 1896767284684070912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すぐに来るよ",
              "maleVariant": "",
              "stringId": 1896785700446871552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フン、よそから手を借りるなんて聞いてない",
              "maleVariant": "",
              "stringId": 1896786388446945280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リーダーがさらわれたのに、何もしないってわけ？",
              "maleVariant": "リーダーがさらわれたのに、何もしないのか？",
              "stringId": 1897966859386167296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リーダーがさらわれたのに、何もしないつもりなの？",
              "maleVariant": "リーダーがさらわれたのに、何もしないつもりなのか？",
              "stringId": 1897967528478318592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何がクラン、何が家族よ。聞いて呆れるね",
              "maleVariant": "何がクラン、何が家族だ。聞いて呆れるな",
              "stringId": 1897968921289236480
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それぐらいにしないと…",
              "maleVariant": "",
              "stringId": 1897970372048658432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やめなって。考え方はそれぞれでしょ",
              "maleVariant": "",
              "stringId": 1897971043489619968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しょうがないでしょ！ 前回の攻撃で半分近くがやられた。物資はリノの向こうで止まってるし、装備だって… わかるでしょ",
              "maleVariant": "",
              "stringId": 1897974085534425088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レイスのキャンプにドローンを飛ばした",
              "maleVariant": "",
              "stringId": 1897977340498878464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを受け取る] もちろんよ",
              "maleVariant": "[チップを受け取る] もちろんだ",
              "stringId": 1897984564700979204
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫、行きましょ",
              "maleVariant": "大丈夫だ、行こう",
              "stringId": 1897991685622538240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やれることはやった。見てみな",
              "maleVariant": "",
              "stringId": 1897992473732259840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "調子はどう？",
              "maleVariant": "調子はどうだ？",
              "stringId": 1898034219606417408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミッチ、元気にしてた？",
              "maleVariant": "ミッチ、元気にしてたか？",
              "stringId": 1898034900409065472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、もちろんよ",
              "maleVariant": "ああ、もちろんだ",
              "stringId": 1898051771913019392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさにどん底さ。正直、この状況から抜け出せるかわからない。努力はするがな",
              "maleVariant": "",
              "stringId": 1898053626164486144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ライブ映像を用意してる。確認してくれるか？",
              "maleVariant": "",
              "stringId": 1898180665022423040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よう、V",
              "maleVariant": "",
              "stringId": 1898195964635455488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はこらえて力を溜めるしかない",
              "maleVariant": "",
              "stringId": 1898200430713577472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "二人だけで？ 他の仲間は？",
              "maleVariant": "",
              "stringId": 1898202893223010308
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね。二人が丁度いい",
              "maleVariant": "そうだな。二人が丁度いい",
              "stringId": 1898203324246466560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待ってよ。それじゃあ二人だけでやるってこと？",
              "maleVariant": "待てよ。それじゃあ二人だけでやるってことか？",
              "stringId": 1898203850413514752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "静かにやるなら、二人で十分ね",
              "maleVariant": "静かにやるなら、二人で十分だ",
              "stringId": 1898203911130259456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたたちは行かないの？",
              "maleVariant": "あんたたちは行かないのか？",
              "stringId": 1898206665345146880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソウルだってきっとわかってくれる",
              "maleVariant": "",
              "stringId": 1898212387147554816
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちだって、ラフェンのクソ野郎どもに一矢報いてやりたいさ！ だが…",
              "maleVariant": "",
              "stringId": 1898274397382057984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さすがだね",
              "maleVariant": "",
              "stringId": 1898293981761839104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソウルもその中にいる",
              "maleVariant": "",
              "stringId": 1898380863749300224
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "借りられる手は借りないと",
              "maleVariant": "",
              "stringId": 1902448430646370304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[車にもたれかかる] さて、どう攻めるつもりなの？",
              "maleVariant": "[車にもたれかかる] さて、どう攻めるつもりだ？",
              "stringId": 1902451687087218692
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[車にもたれかかる] 何か問題でも？",
              "maleVariant": "",
              "stringId": 1902452472244150276
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何か問題でも？",
              "maleVariant": "",
              "stringId": 1902452735092793344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まるで、私たちの人手が足りないって言ってるようなもんでしょ",
              "maleVariant": "",
              "stringId": 1902453756405817344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そもそも、人数をかけて正面から仕掛けるつもりはない",
              "maleVariant": "",
              "stringId": 1902454561863180288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミッチがすでに偵察の準備をしてる。潜入役はV、私は援護に専念する",
              "maleVariant": "",
              "stringId": 1902455021173022720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミッチが来たぞ",
              "maleVariant": "",
              "stringId": 1903989491691401216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だがまあ、今は俺のことよりソウルの心配が先だ",
              "maleVariant": "",
              "stringId": 1905229002574422016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "出られない穴はない。メキシコの仕事が教えてくれた",
              "maleVariant": "",
              "stringId": 1905236872699338752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何か文句でもある？",
              "maleVariant": "",
              "stringId": 1905238477976920064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だがクランの決めたことに、逆らう気はない。たとえそれが… 破滅に繋がるとしてもな",
              "maleVariant": "",
              "stringId": 1905260822460391424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたし達だけでソウルを救出できるかどうか、正直不安ね",
              "maleVariant": "俺たちだけでソウルを救出できるかどうか、正直不安だ",
              "stringId": 1905260879335153664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手遅れになる前に、なんとか助け出さないといけない",
              "maleVariant": "",
              "stringId": 1905265843730145280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたがキャンプに潜入、私は援護に回る。何より、戦闘は極力避けること",
              "maleVariant": "",
              "stringId": 1905266234622500864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人数では圧倒的に負けてるから、その分、頭を使うしかない",
              "maleVariant": "",
              "stringId": 1905267059809869824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "テクトロニカの“グラード”。使ってたの見たでしょ",
              "maleVariant": "",
              "stringId": 1912659284486594560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "テクトロニカの“グラード”。使ってたの見たでしょ。クランからあんたへの贈り物だ",
              "maleVariant": "",
              "stringId": 1912661028092321792
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "世知辛い世の中でなんとか生きてるさ",
              "maleVariant": "",
              "stringId": 1912837127959027712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レイスのキャンプにドローンを飛ばした",
              "maleVariant": "",
              "stringId": 1912855313068662784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケンカが済んだなら、映像を見てもらっていいか？",
              "maleVariant": "",
              "stringId": 1912856471854194688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、とっかかりはつかめた",
              "maleVariant": "",
              "stringId": 1912863459078119424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ったく… しょうがないな。ラフェンの連中がソウルを拉致して、アジトのどこかに拘束してる",
              "maleVariant": "",
              "stringId": 1912866846465138688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたがソウルを救出したら、3人一緒に全速力でそこを離れる。わかった？",
              "maleVariant": "",
              "stringId": 1912928886462423040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰かがやんなきゃ！",
              "maleVariant": "",
              "stringId": 1913767425693523968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今回ばかりは無茶がすぎるぞ",
              "maleVariant": "",
              "stringId": 1913768637931266048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やるしかないんだ！",
              "maleVariant": "",
              "stringId": 1913772288150151168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無視しな",
              "maleVariant": "",
              "stringId": 1913775339489533952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待て。もうひとつ渡すものがある",
              "maleVariant": "",
              "stringId": 1913821557632606208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしソウルにまだ息があったら、こいつを使うんだ",
              "maleVariant": "",
              "stringId": 1913823027685826560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[受け取る] だといいけど",
              "maleVariant": "[受け取る] だといいが",
              "stringId": 1913832125349306372
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、一緒に来てくれる？",
              "maleVariant": "",
              "stringId": 1913838345384697856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、ほとんど間に合わせだけどな",
              "maleVariant": "",
              "stringId": 1913838927117885440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それでも十分だよ",
              "maleVariant": "",
              "stringId": 1913839148359032832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だといいけど。おんぶするわけにもいかないし",
              "maleVariant": "だといいが。おんぶするわけにもいかないしな",
              "stringId": 1913841371457593344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スーパージェット――それもとびきり強力なヤツだ。きっと役に立つ",
              "maleVariant": "",
              "stringId": 1913843844234682368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ準備はいいね？",
              "maleVariant": "",
              "stringId": 1913854608177700864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドローンは光学迷彩を使ってるから見つかりはしないが、その分バッテリーを食う。手早く済ませてくれ",
              "maleVariant": "",
              "stringId": 1913902042467606528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "跡があるでしょ？",
              "maleVariant": "",
              "stringId": 1917050107637936132
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "トラックに続いてる。スキャンしてみて",
              "maleVariant": "",
              "stringId": 1917052874838388736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こうしてる間にも、ソウルは刻一刻と死に近づいてる",
              "maleVariant": "",
              "stringId": 1975154573268819968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行け、V。ソウルを助け出してくれ",
              "maleVariant": "",
              "stringId": 1975155510376996864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミッチとドローンを偵察にやったんだが… 正直、厳しいな",
              "maleVariant": "",
              "stringId": 1975161732660535296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "褒めたくはないが、連中の守りは堅そうだ",
              "maleVariant": "",
              "stringId": 1975162498926321664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別に。ソウルの代わりに、まとめ役になったのね",
              "maleVariant": "別に。ソウルの代わりに、まとめ役になったのか",
              "stringId": 1975176073807994880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私の声が大きかっただけ。賛同してくれる人も多かったし",
              "maleVariant": "",
              "stringId": 1975177298678980608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもそれとは別に、あんたとパナムがやろうとしてることには… 感謝してる",
              "maleVariant": "",
              "stringId": 1975179773234454528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その計画がどんなに無謀でもね",
              "maleVariant": "",
              "stringId": 1975181330948628480
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ずいぶんと悲観的ね",
              "maleVariant": "ずいぶんと悲観的だな",
              "stringId": 1975182793250775040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "現実的なのよ",
              "maleVariant": "",
              "stringId": 1975183097035825152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それでも幸運を祈ってる。死ぬんじゃないよ",
              "maleVariant": "",
              "stringId": 1975183981782310912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつを確認してくれ",
              "maleVariant": "",
              "stringId": 1975184967376642048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソウルの件だろ…",
              "maleVariant": "",
              "stringId": 1975190253894180864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソウルね",
              "maleVariant": "ソウルか",
              "stringId": 1975191922723856384
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は思うんだ… あいつがいないとクランは… バラバラになっちまう",
              "maleVariant": "",
              "stringId": 1975192430133977088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが這い出る時は、誰かの手があると助かるもんだ。わかるだろ",
              "maleVariant": "",
              "stringId": 1976521641679462400
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケツに根を生やす趣味がねえことは確かだな",
              "maleVariant": "",
              "stringId": 1976529065916309504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここはパナムの言うとおり、静かに忍び込み、とっとと脱出するのが一番だと思うぜ",
              "maleVariant": "",
              "stringId": 1976559210580484096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スコーピオンのことがあったってのに、よく顔を出せたもんだな？",
              "maleVariant": "",
              "stringId": 2011453307891818496
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}