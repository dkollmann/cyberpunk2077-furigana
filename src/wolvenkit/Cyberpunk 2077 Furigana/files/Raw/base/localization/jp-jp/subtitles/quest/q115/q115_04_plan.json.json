    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1669300641690820608,
            "femaleVariant": "少々不安だが、気に入った"
          },
          {
            "stringId": 1669300641690820624,
            "femaleVariant": "シンプル過ぎやしないか"
          },
          {
            "stringId": 1669300641724375040,
            "femaleVariant": "本音を言えば、カブキのドールハウスより穴だらけのプランだな"
          },
          {
            "stringId": 1669300641724375052,
            "femaleVariant": "だがまあ、何とかなるだろ！"
          },
          {
            "stringId": 1669300641774706712,
            "femaleVariant": "ローグ"
          },
          {
            "stringId": 1669300641774706724,
            "femaleVariant": "なんだい？"
          },
          {
            "stringId": 1669300641808261132,
            "femaleVariant": "ウェイランドは離陸準備を。私たちもすぐに行く"
          },
          {
            "stringId": 1669300641808261144,
            "femaleVariant": "どれぐらいかかる？ 昼寝でもしておくかな"
          },
          {
            "stringId": 1669300641858592768,
            "femaleVariant": "なんだよ、クソっ！"
          },
          {
            "stringId": 1669300641858592792,
            "femaleVariant": "ハッハッハ！ いい寝顔をしてやがる！"
          },
          {
            "stringId": 1669300641908924428,
            "femaleVariant": "何があった？ ここで何してる？"
          },
          {
            "stringId": 1669300641959256064,
            "femaleVariant": "次やったら歯をへし折るぞ"
          },
          {
            "stringId": 1669300641959256076,
            "femaleVariant": "望むところだ。でも俺がお前をボコボコにしたら、ボスに怒られちまうからな"
          },
          {
            "stringId": 1669300642110251008,
            "femaleVariant": "“私たち”？ 協力してくれるのか？"
          },
          {
            "stringId": 1669300642127028232,
            "femaleVariant": "お前は最高だぜ"
          },
          {
            "stringId": 1669300642177359872,
            "femaleVariant": "“私たち”？ 協力してくれるのか？"
          },
          {
            "stringId": 1669300642227691520,
            "femaleVariant": "お前は最高だ"
          },
          {
            "stringId": 1669384013733564416,
            "femaleVariant": "わかってるくせに、聞くんじゃないよ"
          },
          {
            "stringId": 1669384974933823488,
            "femaleVariant": "知ってるよ。だからあんたは今ここにいるんだろ"
          },
          {
            "stringId": 1670949312148672512,
            "femaleVariant": "どうやら、挨拶は済んだようだね"
          },
          {
            "stringId": 1670950900145721344,
            "femaleVariant": "ローグ、この男はなんでいるんだよ？"
          },
          {
            "stringId": 1670977539646820352,
            "femaleVariant": "私たちを運んでくれるパイロットだからさ"
          },
          {
            "stringId": 1674860816946302976,
            "femaleVariant": "ああ。これを挿して"
          },
          {
            "stringId": 1700830402770243588,
            "femaleVariant": "[チップを受け取る]"
          },
          {
            "stringId": 1700928170033549312,
            "femaleVariant": "どう思う？"
          },
          {
            "stringId": 1701018374496980992,
            "femaleVariant": "本当にいいのか。こんな計画を実行したら、もう後戻りはできない"
          },
          {
            "stringId": 1701019216377679872,
            "femaleVariant": "覚悟の上さ"
          },
          {
            "stringId": 1701020159139778560,
            "femaleVariant": "アラサカは総力を挙げてお前を追うぞ"
          },
          {
            "stringId": 1701075395942420488,
            "femaleVariant": "じゃ、遠慮なく頂く"
          },
          {
            "stringId": 1701075396143747072,
            "femaleVariant": "反重力装備だ、どんな高いとこからでも跳び下りられる"
          },
          {
            "stringId": 1701075396143747084,
            "femaleVariant": "んー… つけ方は…"
          },
          {
            "stringId": 1701075396143747096,
            "femaleVariant": "まったく、私はベビーシッターじゃないんだよ"
          },
          {
            "stringId": 1701075396143747108,
            "femaleVariant": "わかった、こうだな"
          },
          {
            "stringId": 1701075396529623040,
            "femaleVariant": "ジョニー、装備を確認しな。早く"
          },
          {
            "stringId": 1701075396579954688,
            "femaleVariant": "ジョニー、装備を。必要になる"
          },
          {
            "stringId": 1701076623581646848,
            "femaleVariant": "ふむ。こいつは何だ？"
          },
          {
            "stringId": 1701076681563705344,
            "femaleVariant": "レトロスラスター。とっておきさ"
          },
          {
            "stringId": 1701079914315124736,
            "femaleVariant": "まだオルトの件が残ってる"
          },
          {
            "stringId": 1701199205790257152,
            "femaleVariant": "ローグ…"
          },
          {
            "stringId": 1702435075012825088,
            "femaleVariant": "その通りだ。タワーに飛び降りたら、入り口を探して下方向に突き進み…"
          },
          {
            "stringId": 1702439690777268224,
            "femaleVariant": "つまりこうだな？ まずアラサカ・タワーまでこっそり飛んでって、あいさつ代わりのミサイルをお見舞いする"
          },
          {
            "stringId": 1702442052287860736,
            "femaleVariant": "〈神輿〉のアクセスポイントを目指す"
          },
          {
            "stringId": 1741487532548165636,
            "femaleVariant": "バッチリだな！"
          },
          {
            "stringId": 1741488354363314176,
            "femaleVariant": "これじゃダメだ。ズタズタにされるぞ"
          },
          {
            "stringId": 1741490194438033408,
            "femaleVariant": "確かに俺は派手な爆発が三度の飯より好きだ。気の利いた作戦だが、これじゃ死にに行くようなもんだぞ"
          },
          {
            "stringId": 1741490194438033420,
            "femaleVariant": "警備の連中が気づいた瞬間に、特殊部隊を呼ばれる。作戦はそこまでだ"
          },
          {
            "stringId": 1741490194438033432,
            "femaleVariant": "そうとは限らないよ"
          },
          {
            "stringId": 1741490544544976896,
            "femaleVariant": "アラサカに空爆を仕掛けるって… 本気か？"
          },
          {
            "stringId": 1741491514620383232,
            "femaleVariant": "ずっとこの日を待ってたんだ！"
          },
          {
            "stringId": 1741491786293841920,
            "femaleVariant": "興奮するのはまだ早いよ"
          },
          {
            "stringId": 1741493117180055552,
            "femaleVariant": "やるべきことは、他にもたくさんあるからね"
          },
          {
            "stringId": 1741519246804783108,
            "femaleVariant": "俺がオルトを説得する"
          },
          {
            "stringId": 1741522200467214336,
            "femaleVariant": "厳しいな。オルトはネットウォッチにマークされてる"
          },
          {
            "stringId": 1741522256066908160,
            "femaleVariant": "オービタル・エアーに協力者がいるとはな"
          },
          {
            "stringId": 1741523919930527744,
            "femaleVariant": "オービタル・エアーに… ちょっとした知り合いがいてね"
          },
          {
            "stringId": 1741526884665921536,
            "femaleVariant": "オルトと話す予定がある。説得は任せとけ"
          },
          {
            "stringId": 1741526965565657088,
            "femaleVariant": "オルトに頼んではみるが、期待しないでくれ"
          },
          {
            "stringId": 1741527086881705984,
            "femaleVariant": "オービタル・エアーに協力者がいるとはな。さすがフィクサーの女王だ"
          },
          {
            "stringId": 1741527573219643392,
            "femaleVariant": "そんなの知ったこっちゃない。こっちはどデカいリスクを背負ってるんだ"
          },
          {
            "stringId": 1741528323748401152,
            "femaleVariant": "あいつは、はぐれAIだ。素直に聞き入れるかどうか… しかもウォッチにマークされてる"
          },
          {
            "stringId": 1741529630508978176,
            "femaleVariant": "衛星を墜とすってこと自体、気に入らない可能性も…"
          },
          {
            "stringId": 1741530109666267136,
            "femaleVariant": "説得できないなら、私は降りる"
          },
          {
            "stringId": 1741547718042324992,
            "femaleVariant": "だから、頼んではみるって言ってるだろ"
          },
          {
            "stringId": 1741572897959186432,
            "femaleVariant": "彼を通して、寿老人へのアクセスを得る"
          },
          {
            "stringId": 1741853115936542720,
            "femaleVariant": "感謝するんだね。最高の人材を動員してるんだ"
          },
          {
            "stringId": 1742912992112939008,
            "femaleVariant": "影の協力者か？"
          },
          {
            "stringId": 1742913071049740288,
            "femaleVariant": "かなりの地位にいる人物さ"
          },
          {
            "stringId": 1742920063692705792,
            "femaleVariant": "あとは昔馴染みのネットランナーの出番さ"
          },
          {
            "stringId": 1742920826921816064,
            "femaleVariant": "頼んだよ、ジョニー。こればっかりは私にもできない"
          },
          {
            "stringId": 1742922685501804544,
            "femaleVariant": "オルトのことか"
          },
          {
            "stringId": 1742922732478009344,
            "femaleVariant": "私が知ってる限り、彼女以外にアラサカの衛星を墜とせる人物はいない"
          },
          {
            "stringId": 1743102198441447424,
            "femaleVariant": "奥に行きな。ニックスが手伝ってくれる"
          },
          {
            "stringId": 1760737551994195968,
            "femaleVariant": "次やったら歯をへし折るぞ"
          },
          {
            "stringId": 1760742711407546368,
            "femaleVariant": "行くぞ。ボスが待ってる"
          },
          {
            "stringId": 1760743244486807552,
            "femaleVariant": "ローグのことか？"
          },
          {
            "stringId": 1760743420865679360,
            "femaleVariant": "ボスは1人しかいねえ"
          },
          {
            "stringId": 1761717591187787776,
            "femaleVariant": "俺が寝てる間に計画してたのか？"
          },
          {
            "stringId": 1761717591187787788,
            "femaleVariant": "それを言うなら、あんたが死んでる間にね"
          },
          {
            "stringId": 1761717591204564992,
            "femaleVariant": "企業間の紛争に備えて、通信関係につばをつけといたのさ"
          },
          {
            "stringId": 1761717591204565004,
            "femaleVariant": "この世に、戦争ほど金になるものはないからね"
          },
          {
            "stringId": 1761719784137691136,
            "femaleVariant": "俺が寝てる間に計画を練ってたのか？"
          },
          {
            "stringId": 1763653817741107200,
            "femaleVariant": "目標はタワー内にある〈神輿〉へのアクセスポイント"
          },
          {
            "stringId": 1763656361670369280,
            "femaleVariant": "研究フロアまで行き、連中を制圧しつつ必要な作業を終わらせ、脱出する"
          },
          {
            "stringId": 1763658308381073408,
            "femaleVariant": "アラサカ・タワーはまさに要塞だ。ナイトシティいちの警備が敷かれている。正面から通してもらおうなんてのはまず論外"
          },
          {
            "stringId": 1763660551696179200,
            "femaleVariant": "だから… 自分たちで窓を作り、そこから侵入する"
          },
          {
            "stringId": 1763662185931235328,
            "femaleVariant": "具体的には、まずミリテクのストリクス戦術爆撃機で現地に向かう"
          },
          {
            "stringId": 1763667140712882176,
            "femaleVariant": "上空からの攻撃で建物の対空防衛設備を破壊"
          },
          {
            "stringId": 1763667711423438848,
            "femaleVariant": "それから76階の壁を破ってAVから降下し、反重力装備を使って着地する"
          },
          {
            "stringId": 1763671364293124096,
            "femaleVariant": "シャフトは建物の上から下まで通ってる。ここでもスラスターを使って、一気に降下する"
          },
          {
            "stringId": 1763672741115355136,
            "femaleVariant": "研究フロアを駆け抜け、アクセスポイントにたどり着いたら… 後はあんた次第だ"
          },
          {
            "stringId": 1763675177032900608,
            "femaleVariant": "拍手してもいいんだよ"
          },
          {
            "stringId": 1763678954372751360,
            "femaleVariant": "お前、爆撃機なんて持ってたのか！"
          },
          {
            "stringId": 1763679032688795648,
            "femaleVariant": "爆撃機なんて持ってたのか！"
          },
          {
            "stringId": 1763681870303223808,
            "femaleVariant": "警備は大混乱するはずだ。抵抗は最小限に抑えられる"
          },
          {
            "stringId": 1763682783520645120,
            "femaleVariant": "見ての通りだ。着地点は、熱帯雨林のど真ん中。別に褒めるわけじゃないけど、アラサカも案外、洒落た遊びをやるもんだね。そのジャングルを抜け、タワー中枢を縦に走るシャフトを目指す"
          },
          {
            "stringId": 1763686022496411648,
            "femaleVariant": "切り札は隠すもんさ"
          },
          {
            "stringId": 1763686632364990464,
            "femaleVariant": "赤いミズタニの新車を乗り回してた日が懐かしいぜ… どうやって手に入れた？"
          },
          {
            "stringId": 1763687159589003264,
            "femaleVariant": "私のツテをなめるんじゃないよ。いいから作戦に集中しな"
          },
          {
            "stringId": 1763693537481220096,
            "femaleVariant": "私たちはまず――"
          },
          {
            "stringId": 1763735193446277124,
            "femaleVariant": "衛星を墜とす？ スペースシャトルも持ってるのか？"
          },
          {
            "stringId": 1763735603363995648,
            "femaleVariant": "どうやるのかは、あえて聞かない"
          },
          {
            "stringId": 1763736274989506560,
            "femaleVariant": "まさか爆撃機だけじゃなく、武装したスペースシャトルまで持ってるってのか？"
          },
          {
            "stringId": 1763736376021901312,
            "femaleVariant": "どうやるのかは、あえて聞かない"
          },
          {
            "stringId": 1763743146115096576,
            "femaleVariant": "そうすればこの街を含むカリフォルニア全域の通信が停止して、3、4時間は稼げる"
          },
          {
            "stringId": 1763753467978473472,
            "femaleVariant": "任せろって言ってんだろ"
          },
          {
            "stringId": 1763755955334963200,
            "femaleVariant": "その間、奴らは連携どころか、援護も呼べなくなる"
          },
          {
            "stringId": 1803865174760624128,
            "femaleVariant": "装備はそこだ。好きに使いな"
          },
          {
            "stringId": 1803875512495247360,
            "femaleVariant": "よし、作戦の詳細を聞こうか"
          },
          {
            "stringId": 1803884690118938624,
            "femaleVariant": "サイバー空間の勝手はわかってるんだろ？"
          },
          {
            "stringId": 1803885158538809344,
            "femaleVariant": "まあ… Vと一緒に一応経験済みだ。何とかなるさ"
          },
          {
            "stringId": 1803904607409016832,
            "femaleVariant": "その… ありがとな"
          },
          {
            "stringId": 1803907167528292352,
            "femaleVariant": "どうかな… 単純過ぎやしないか？"
          },
          {
            "stringId": 1803908880079392768,
            "femaleVariant": "はいはい"
          },
          {
            "stringId": 1803911963849134080,
            "femaleVariant": "ついでに言うと、生きて帰れる可能性は極めて低い"
          },
          {
            "stringId": 1803912324190179328,
            "femaleVariant": "お前はそれでも――"
          },
          {
            "stringId": 1803914300663664640,
            "femaleVariant": "やるよ"
          },
          {
            "stringId": 1803914863119831040,
            "femaleVariant": "決めたんだ"
          },
          {
            "stringId": 1803915670657568768,
            "femaleVariant": "アラサカに追われる覚悟はあるのか"
          },
          {
            "stringId": 1803918567327797248,
            "femaleVariant": "過去にはもっとバカげた真似をしてきただろ"
          },
          {
            "stringId": 1803920102761828352,
            "femaleVariant": "ああ。おそらくオービタル・エアーもね"
          },
          {
            "stringId": 1803920455184027648,
            "femaleVariant": "でも、これで一矢報いることができるなら、本望よ"
          },
          {
            "stringId": 1803923618175782912,
            "femaleVariant": "で… 何か意見は？"
          },
          {
            "stringId": 1803928102339743744,
            "femaleVariant": "通信衛星“寿老人-0401”を叩き落とす"
          },
          {
            "stringId": 1804081373750628352,
            "femaleVariant": "もったいぶるなよ"
          },
          {
            "stringId": 1825490175881043972,
            "femaleVariant": "[立ち上がる]"
          },
          {
            "stringId": 1842910842724421632,
            "femaleVariant": "知ってる顔だな。クリスピン・ウェイランドだろ、その面、親父にそっくりだ"
          },
          {
            "stringId": 1842922277823188992,
            "femaleVariant": "一族の証さ"
          },
          {
            "stringId": 1842923236389425152,
            "femaleVariant": "だがどうしてお前がここに？ 何しに来た？"
          },
          {
            "stringId": 1842926529471713280,
            "femaleVariant": "ボスが説明してくれる"
          },
          {
            "stringId": 1842926771852152832,
            "femaleVariant": "ローグか？"
          },
          {
            "stringId": 1842926824230621184,
            "femaleVariant": "ボスは1人しかいねえ"
          },
          {
            "stringId": 2067896407614627840,
            "femaleVariant": "酔っ払ったわけでもないのに、よくもそんなに爆睡できんな"
          },
          {
            "stringId": 2238880589191421952,
            "femaleVariant": "ハッ、通信衛星？ こりゃ歴史に残る出来事になりそうだ"
          },
          {
            "stringId": 2238887654949494792,
            "femaleVariant": "さすが我らがボス、我らが女王だ"
          },
          {
            "stringId": 2239366991721111552,
            "femaleVariant": "話を聞いてなかったのか？ 俺はAVを準備する。その間、お前はブラックウォールの向こう側にいるAIを何とかしな。バーの方に行った、行った"
          }
        ]
      }
    }
  }
}