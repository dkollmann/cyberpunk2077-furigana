  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "タバコが吸いたくてたまらねえ。持ってるか？",
              "maleVariant": "",
              "stringId": "1531611843562201088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺はこっから出たいんだ。わかるか？",
              "maleVariant": "",
              "stringId": "1531745063764807680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "邪魔する奴は誰だろうと殺す。お前も例外じゃない",
              "maleVariant": "",
              "stringId": "1531745948058308608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうやって入ったの？",
              "maleVariant": "どうやって入ったんだ？",
              "stringId": "1531771635402067972"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "タバコは吸わない",
              "maleVariant": "",
              "stringId": "1531772520551206912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なら買ってこいよ。1本だけでいいんだ",
              "maleVariant": "",
              "stringId": "1531775927802449920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いったい、何様よ",
              "maleVariant": "いったい、何様だ",
              "stringId": "1531776637948448768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ、てんで使えねえジョイトイだな",
              "maleVariant": "",
              "stringId": "1531784168183746560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "雇い主は誰だ？ 言え！",
              "maleVariant": "",
              "stringId": "1531798393618747392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウソでしょ？",
              "maleVariant": "ウソだろ？",
              "stringId": "1531799943867060224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウソだろ？",
              "maleVariant": "",
              "stringId": "1531800673709510656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのチップか…",
              "maleVariant": "",
              "stringId": "1531800921139892224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "消えな、クソ野郎",
              "maleVariant": "",
              "stringId": "1531827162819645440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[薬を飲む]",
              "maleVariant": "",
              "stringId": "1531833199161298944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "タバコは吸わない",
              "maleVariant": "",
              "stringId": "1532744625648357376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どっから入ってきたの？",
              "maleVariant": "どっから入ってきた？",
              "stringId": "1553261098178404352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "深く考えちゃダメよ、V。忘れるの。ほら、薬を持ってきたから",
              "maleVariant": "",
              "stringId": "1640255507798315008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだ、もう1つ渡すものがあった",
              "maleVariant": "",
              "stringId": "1640255507798315016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴィクターがあなたの頭から取り出したの。お守りにって",
              "maleVariant": "",
              "stringId": "1640255507815092224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジャッキーは親友だった",
              "maleVariant": "",
              "stringId": "1653125477661462528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジャックは… 最高の友人だった。もう会えないと思うと、寂しくてたまらない",
              "maleVariant": "",
              "stringId": "1653126780949155840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼がここにいたら、「グズってねえで、やるべきことをやれ」って言うでしょうね",
              "maleVariant": "",
              "stringId": "1653127023094714368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よくあんたのことを話してた",
              "maleVariant": "",
              "stringId": "1653130307939389440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジャックは… よくあんたの話をしてた",
              "maleVariant": "",
              "stringId": "1653133702926487552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの仕事に行く前… 彼と喧嘩しちゃったの",
              "maleVariant": "",
              "stringId": "1653140217888559104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつは怒ってなかった。気にしちゃダメよ",
              "maleVariant": "あいつは怒ってなかった。気にしちゃダメだ",
              "stringId": "1653141035140304896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってる。ただ最後にもっと違う過ごし方が… できてたらって",
              "maleVariant": "",
              "stringId": "1653141880728768512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[横になる] もう疲れた",
              "maleVariant": "",
              "stringId": "1653148212466864132"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしを殺すための薬ってこと？",
              "maleVariant": "俺を殺すための薬ってことか？",
              "stringId": "1653148644480176128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "優しいのね。ありがとう、ミスティ",
              "maleVariant": "優しいんだな。ありがとう、ミスティ",
              "stringId": "1653148806229315584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミスティ、待って。ジャッキーのことだけど…",
              "maleVariant": "ミスティ、待て。ジャッキーのことだが…",
              "stringId": "1653148869361979392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おやすみ、V。いい夢を",
              "maleVariant": "",
              "stringId": "1653150122200584192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なに？",
              "maleVariant": "",
              "stringId": "1653150838956806144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それでそいつが死んで… あたしも一緒に死ぬんだと思った… 眠ったまま、その男とね",
              "maleVariant": "それでそいつが死んで… 俺も一緒に死ぬんだと思った… 眠ったまま、その男と",
              "stringId": "1654744294154096640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分が今、本当に起きてるかもわからない。既に死んでる可能性だってあるでしょ？",
              "maleVariant": "自分が今、本当に起きてるかもわからない。既に死んでる可能性だってあるだろ？",
              "stringId": "1654776913927393280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう",
              "maleVariant": "",
              "stringId": "1680714883322662912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジャッキーのことだけど…",
              "maleVariant": "ジャッキーのことだが…",
              "stringId": "1680714883322662928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何がお守りよ。あたしの命は終わりかけてるってのに",
              "maleVariant": "何がお守りだ。俺の命は終わりかけてるってのに",
              "stringId": "1680723670523760640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつを刺激して、あたしをさっさと殺させる薬ってこと？",
              "maleVariant": "あいつを刺激して、俺をさっさと殺させる薬ってことか？",
              "stringId": "1680747371126378496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これは魂の死なのよ。徐々に己が失われていくの。ある時点を超えたら、自分自身を認識できなくなる…",
              "maleVariant": "",
              "stringId": "1680749834860548096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "恐ろしくて、苦しいはずよ。でもそれに耐えなくたっていい",
              "maleVariant": "",
              "stringId": "1680750977925828608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はゆっくり休んで。少しでも寝るのよ、ね？",
              "maleVariant": "",
              "stringId": "1680754164607012864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺が知るわけねえだろ…",
              "maleVariant": "",
              "stringId": "1680853239671083008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんで会話ができて…？",
              "maleVariant": "",
              "stringId": "1680856361256669184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "感じるぞ… 互いの意識が触れ合ってる…",
              "maleVariant": "",
              "stringId": "1693928484547289100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなこと言わないで。生きてる限り希望はある。誰がなんと言おうとね",
              "maleVariant": "",
              "stringId": "1725750154300542976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本気で思ってるの？ 希望はあるって",
              "maleVariant": "本気で思ってるのか？ 希望はあると",
              "stringId": "1725751755551592448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろんよ。だってあなた、すでに一度死んで生き返ってるじゃない",
              "maleVariant": "",
              "stringId": "1725752858032787456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハロー、ナイトシティ！",
              "maleVariant": "",
              "stringId": "1735537728094892032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ、南と北カリフォルニアに挟まれた世界最高のこの場所から、今日も超元気にお届けするぜ！",
              "maleVariant": "",
              "stringId": "1735538628830064640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "毎度おなじみスタンだ！ 今日はスモッグが通常の10倍を記録したらしい。ずいぶんとゴージャスな日じゃないか！",
              "maleVariant": "",
              "stringId": "1741406203971530752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[薬を飲む]",
              "maleVariant": "",
              "stringId": "1754851376491094016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… なんなのよ",
              "maleVariant": "はあ… なんなんだ",
              "stringId": "1754851939937116160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "引っこ抜いてやるよ！",
              "maleVariant": "",
              "stringId": "1755975805704888320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダメ！ 待って！",
              "maleVariant": "やめろ！ 待て！",
              "stringId": "1755977876432773120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "グッ…",
              "maleVariant": "",
              "stringId": "1755979911341301760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ、気持ちわりい！",
              "maleVariant": "",
              "stringId": "1756064483307446272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は果物に生えたカビかよ… 少しずつ侵食してる… 止めることはできねえ",
              "maleVariant": "",
              "stringId": "1756065191691837440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞いてんのか！？",
              "maleVariant": "",
              "stringId": "1756066248757108736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "殺してやる",
              "maleVariant": "",
              "stringId": "1756069344270901252"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの頭から出てけ",
              "maleVariant": "俺の頭から出てけ",
              "stringId": "1756070011198791680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "殺して… やる",
              "maleVariant": "",
              "stringId": "1756070917755658240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何なのよ！",
              "maleVariant": "何なんだ！",
              "stringId": "1756071035716263936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やれよ。さっさと殺してくれ！",
              "maleVariant": "",
              "stringId": "1756095520016195584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "出てけ！ あたしの頭から出てけ！",
              "maleVariant": "出てけ！ 俺の頭から出てけ！",
              "stringId": "1756099862261686272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃダメだ！",
              "maleVariant": "",
              "stringId": "1756172889003417600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "口に銃を突っ込んで引き金を引け…",
              "maleVariant": "",
              "stringId": "1756173216477896704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ワトソンに入り浸ってるサイボーグ野郎に朗報があるぞ… あそこら一帯の非常線は解除された",
              "maleVariant": "",
              "stringId": "1756194282554286080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ってわけで、心の赴くままに足を運んでみてはどうかな？ 今日は新たな一章、新たな一冊、新たな人生の幕開けだ！",
              "maleVariant": "",
              "stringId": "1756197792817635328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ今朝は彼らの曲でスタートするとしよう。ザ・ルビコンズ！",
              "maleVariant": "",
              "stringId": "1756198964001529856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昨夜の狂乱はベッドの中に置き去りにして、ウェイクアップ！ 朝日を浴びながらコーヒーを淹れよう。ナイトシティは今日も快晴だ！",
              "maleVariant": "",
              "stringId": "1756204778397392896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ、クソ、クソ、クソ、クソ…！",
              "maleVariant": "",
              "stringId": "1756420060462211072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これは記憶痕跡のコピーに過ぎない… 本当の俺はきっとどこかに…",
              "maleVariant": "",
              "stringId": "1756420575841509376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭をぶち抜け。そうするしかない",
              "maleVariant": "",
              "stringId": "1756420811678834688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞いてんのか！？ 脳みそに弾をぶち込むんだよ！",
              "maleVariant": "",
              "stringId": "1756424308100980736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よく聞いて。今は大丈夫でも、この先には耐えられないほどの苦痛が待ってるかもしれない。私はあなたに選択肢を用意してあげてるの",
              "maleVariant": "",
              "stringId": "1774717953086349312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "脳を騙せばいい。普通の鎮痛剤で十分でしょ？",
              "maleVariant": "脳を騙せばいい。普通の鎮痛剤で十分だろ？",
              "stringId": "1774718837564399616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わざわざ薬なんか飲まなくても、銃をくわえて引き金を引けば済むじゃない",
              "maleVariant": "わざわざ薬なんか飲まなくても、銃をくわえて引き金を引けば済む",
              "stringId": "1774726692237500416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そしたらあなたは魂を二つ消すことになる。それでもいいの？",
              "maleVariant": "",
              "stringId": "1774726997968707584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジャッキーのことは本当に残念だった",
              "maleVariant": "",
              "stringId": "1774732426488602624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジャックのことは本当に残念だった",
              "maleVariant": "",
              "stringId": "1774759857421312000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼は特別だったわ。心が豊かで、愛情をもって人と接することができた。でも心配しないで、彼は常にそばにいるから",
              "maleVariant": "",
              "stringId": "1774768516259930112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミスティの方は大丈夫？",
              "maleVariant": "ミスティの方は大丈夫か？",
              "stringId": "1774769616828522496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。生は美しくて、力に満ち溢れてる。死よりもずっと力強いわ",
              "maleVariant": "",
              "stringId": "1774769918566752256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[起きる]",
              "maleVariant": "",
              "stringId": "1782045434573447168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オメガブロッカーよ。定期的に飲めば、急激な変化を抑えてくれる",
              "maleVariant": "",
              "stringId": "1801308235405942784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭の中のお客さんも多少おとなしくなるはずよ",
              "maleVariant": "",
              "stringId": "1801308235892482048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あとこっちは私から。疑似エンドトライジン、ブロッカーとは逆に、進行を加速させる薬",
              "maleVariant": "",
              "stringId": "1801308236899115008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "横にならせて…",
              "maleVariant": "横にならせてくれ…",
              "stringId": "1801319726121181184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、無理してでも眠るのよ",
              "maleVariant": "",
              "stringId": "1801327129218736128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの命は終わりかけてるのよ",
              "maleVariant": "俺の命は終わりかけてるんだぞ",
              "stringId": "1801330165878054912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "眠りってのは… 死に近い概念だといわれてる",
              "maleVariant": "",
              "stringId": "1802321539337052160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[眠る]",
              "maleVariant": "",
              "stringId": "1879531836931792900"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "1879681356235759620"
            },
            {
