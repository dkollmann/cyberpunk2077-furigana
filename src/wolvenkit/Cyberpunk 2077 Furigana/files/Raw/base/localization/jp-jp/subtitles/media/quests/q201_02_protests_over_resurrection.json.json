        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここからはN54ニュース、ジリアン・ジョーダンです",
                    "maleVariant": null,
                    "stringId": 1745738793486966784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この“復活劇”に対し、多くの宗教指導者、人権擁護団体、バーチュ100に名を連ねる企業の代表らが、抗議文に署名を行いました",
                    "maleVariant": null,
                    "stringId": 1745748128027959296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "技術を手にした者が思い上がり、神をも恐れぬ所業に及ぶとは。なんたる虚栄心、人類のモラルは完全に崩れ去った…",
                    "maleVariant": null,
                    "stringId": 1745748365979213824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何度でも言います。サブロウ・アラサカによる権力の絶対的な掌握は、国際法に明確に反しています。つまり国際仲裁が必要とされることは疑いようもないわけで…",
                    "maleVariant": null,
                    "stringId": 1745748582136864768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "裕福な権力者が不老不死になれる世の中なんて… そんな世界をいったい誰が望むんです？ 一方的な搾取が拡大するだけだ！ これからは資産だけでなく、体まで奪われるなんて！",
                    "maleVariant": null,
                    "stringId": 1745748747677655040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "多くのナイトシティ市民も抗議の声を上げています。アラサカ・ナイトシティ支社前の反対デモは、当初の予定を延長して2日目に突入しました",
                    "maleVariant": null,
                    "stringId": 1745748904259411968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "不老不死反対！ 身の程を知れ！ 不老不死反対！ 身の程を知れ！",
                    "maleVariant": null,
                    "stringId": 1745749083507187712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しかしサブロウ・アラサカ氏は抗議活動に対して一歩も引かない姿勢を見せており、緊張は高まるばかりです",
                    "maleVariant": null,
                    "stringId": 1745749323404599296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカ社内部の出来事に関して、外部の方々にとやかく言われる筋合いはありません",
                    "maleVariant": null,
                    "stringId": 1745749589608685568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "生を受けることに許可が必要ない以上、蘇ることも同様であると考えるべきです",
                    "maleVariant": null,
                    "stringId": 1745749809675427840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "合意の上での身体の譲与は一種の寄付に分類することができ、殺人には到底値しません",
                    "maleVariant": null,
                    "stringId": 1745751085867917312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "果たして世界は、サブロウ・アラサカ氏の主張を受け入れるのでしょうか？ CMの後、専門家にうかがいます",
                    "maleVariant": null,
                    "stringId": 1745751445453987840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私が息子を殺めたなどという主張は事実無根――法的根拠もありません",
                    "maleVariant": null,
                    "stringId": 1745754757830074368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "殺人の条件は生物学的な死です。見ての通り、この体は生きています",
                    "maleVariant": null,
                    "stringId": 1745771888055476224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "命で商売をするな！ 命は売り物じゃないぞ！ 命で商売をするな！ 命は売り物じゃないぞ！",
                    "maleVariant": null,
                    "stringId": 1751741464391782400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サブロウ・アラサカ氏が息子ヨリノブ・アラサカ氏の体を使い蘇ったというニュースに、全世界が大きな衝撃を受けています",
                    "maleVariant": null,
                    "stringId": 1751769249910951936
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}