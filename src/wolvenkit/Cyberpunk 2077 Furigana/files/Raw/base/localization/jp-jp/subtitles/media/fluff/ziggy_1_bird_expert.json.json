      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティの皆さん、こんばんは！ 今夜のスペシャルゲストは… ナイトシティ大学の生物学者、オリバー教授です！ ようこそ！",
              "maleVariant": "",
              "stringId": 1793526117690155008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも。お招きありがとう",
              "maleVariant": "",
              "stringId": 1803793378307719168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "鳥を… ナイトシティに戻そうという陳述書を出したそうですね？",
              "maleVariant": "",
              "stringId": 1803793539234775040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はい。鳥類駆除法が制定されてからもうすぐ15年ですが、この法律の成立以来、生態系は見事に破壊され、急激な昆虫の増加が…",
              "maleVariant": "",
              "stringId": 1803793715596869632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "51年の出来事を覚えてます？",
              "maleVariant": "",
              "stringId": 1803794155780685824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "えー… 51年？",
              "maleVariant": "",
              "stringId": 1803794333099081728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "鳥インフルが大流行した年ですよ。7000もの人々が命を落としましたよね",
              "maleVariant": "",
              "stringId": 1803794496056180736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、だが…",
              "maleVariant": "",
              "stringId": 1803794667703877632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さらに59年には、1万人以上が犠牲になっています。61年には1万2000人だ",
              "maleVariant": "",
              "stringId": 1803794880438976512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かに、すべて悲惨な事件です。しかし私たち人間が鳥たちの生息地を破壊してさえいなければ、これらの悲劇はすべて…",
              "maleVariant": "",
              "stringId": 1803795075088236544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "59年に2人のお子さんを失ったマーゴットさんにお越しいただいています。どうぞ、お立ちください",
              "maleVariant": "",
              "stringId": 1803795230344593408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ先生、直接おっしゃってください。お子さんが亡くなったのは、彼女のせいだって",
              "maleVariant": "",
              "stringId": 1803795608167497728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "馬鹿な、やめてくれ。私が言おうとしているのは…",
              "maleVariant": "",
              "stringId": 1803796066135162880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ皆さんは… 人間とハト、いったいどちらを選びますか？",
              "maleVariant": "",
              "stringId": 1803796310579200000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういう話じゃないんだ。鳥が絶滅してしまったら… いたッ！ おい！ やめさせてくれ！",
              "maleVariant": "",
              "stringId": 1803796556851953664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うわ！ こんなの馬鹿げて…！ やめろ！ おいっ！",
              "maleVariant": "",
              "stringId": 1803796740445028352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "生きて帰れるだけラッキーだな、このえせ学者が！ ペッ！",
              "maleVariant": "",
              "stringId": 1803799164702408704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいかみんな、覚えててくれ。この街には、ジギーQがついてるぜ",
              "maleVariant": "",
              "stringId": 1803799581012246528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それでは明日もまたジギーQの“ナイトアフターナイト”、このN54チャンネルでお会いしましょう！",
              "maleVariant": "",
              "stringId": 1803799900332998656
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}