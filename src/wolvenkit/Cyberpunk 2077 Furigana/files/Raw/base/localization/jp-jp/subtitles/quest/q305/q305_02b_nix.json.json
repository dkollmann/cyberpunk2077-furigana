            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Why are we meeting here?",
                    "maleVariant": null,
                    "stringId": 2980826641530773504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Do you have this data?",
                    "maleVariant": null,
                    "stringId": 3064897853747761156
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Are you giving history lessons on the side?",
                    "maleVariant": null,
                    "stringId": 3064898618973360128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]And? Did you manage it? Do you have this data?",
                    "maleVariant": null,
                    "stringId": 3064902004665880576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Do you give history lessons on the side?",
                    "maleVariant": null,
                    "stringId": 3064902139504365568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I haven't fucked up such a dance in a minefield for a long time.",
                    "maleVariant": null,
                    "stringId": 3064905295634239488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]It's like having to conduct an orchestra with one hand and doing fucking matrix multiplication with the other. But it worked.",
                    "maleVariant": null,
                    "stringId": 3064909474251542528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Awesome. You're the best.",
                    "maleVariant": null,
                    "stringId": 3064911359406641152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I am not asking what the data is for. But V ...",
                    "maleVariant": null,
                    "stringId": 3064916021308428288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I can handle. I will not be alone.",
                    "maleVariant": null,
                    "stringId": 3064917587193106436
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I'll try to be careful.",
                    "maleVariant": null,
                    "stringId": 3064917801018724352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I can guess what you think about all of this. And you are probably right. But in turn, I have no choice.",
                    "maleVariant": null,
                    "stringId": 3064918804396904448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You've done your job, now it's my turn.",
                    "maleVariant": null,
                    "stringId": 3064918896386379776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Well, if so ...",
                    "maleVariant": null,
                    "stringId": 3064920231215239168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Mhm. Then you have an additional gift as a reward for possible good behavior.",
                    "maleVariant": null,
                    "stringId": 3064921729068306432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]What's that?",
                    "maleVariant": null,
                    "stringId": 3064922530935345152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]A small memento of my collaboration with Arasaka. Military tech, I tweaked my way.",
                    "maleVariant": null,
                    "stringId": 3064922637973983232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Thanks, Nix.",
                    "maleVariant": null,
                    "stringId": 3064924942341378048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]S cool. You saved my ass, I may be saving your ass now.",
                    "maleVariant": null,
                    "stringId": 3064925332898189312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Good luck.",
                    "maleVariant": null,
                    "stringId": 3064927617049038848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I'm not getting into what you need this data for. For my health, they won't work for you. Anyway, I did my part.",
                    "maleVariant": null,
                    "stringId": 3064930126232686592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Not. I read a lot and listen a lot. I recommend you too.",
                    "maleVariant": null,
                    "stringId": 3069366261264871424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Screw you.",
                    "maleVariant": null,
                    "stringId": 3069368811015827456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]It's good to see you too.",
                    "maleVariant": null,
                    "stringId": 3069374180547031040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You have everything here - the route, the composition of the convoy, the type of weaponry, the decrypted data of the client they are taking to Los Padres.",
                    "maleVariant": null,
                    "stringId": 3069384542021414912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I will erase my tracks, and in the evenings I will pray that the sad gentlemen million-eurodollar armor will not knock on my door.",
                    "maleVariant": null,
                    "stringId": 3069387489744711680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You make sure that you don't have to explore Los Padres from the inside. Because that's where you don't go.",
                    "maleVariant": null,
                    "stringId": 3069389123057020928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Hi. Why are we meeting here?",
                    "maleVariant": null,
                    "stringId": 3069392119316803600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]What, are you feeling uncomfortable? ... Maybe rightly so.",
                    "maleVariant": null,
                    "stringId": 3069392119316803612
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Every now and then, there are losers who think they can jump bigger than themselves.",
                    "maleVariant": null,
                    "stringId": 3069392119333580800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]This place always reminds me of them. But you don't want to join this group, do you?",
                    "maleVariant": null,
                    "stringId": 3069392119333580812
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I'll be careful, I promise.",
                    "maleVariant": null,
                    "stringId": 3069397941530849280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I can handle it. Besides, I don't act solo.",
                    "maleVariant": null,
                    "stringId": 3069401957090390016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]But try not to go to Los Padres. Because you don't leave there anymore.",
                    "maleVariant": null,
                    "stringId": 3069403502506864640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]As soon as someone tries to fry your head it will create a dynamic ICE, one time use but unbeatable.",
                    "maleVariant": null,
                    "stringId": 3069408897623322624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Besides, you will need another warning that you will ignore.",
                    "maleVariant": null,
                    "stringId": 3069412447548010496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You better pray as well. Got it",
                    "maleVariant": null,
                    "stringId": 3072078075379511296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Hi.",
                    "maleVariant": null,
                    "stringId": 3121658390692716544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Here I am.",
                    "maleVariant": null,
                    "stringId": 3121660188203638784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Hi. Here I am. All right?",
                    "maleVariant": null,
                    "stringId": 3121661703941218304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You tell me. Every time I'm in this place, I am reminded of a certain type of people.",
                    "maleVariant": null,
                    "stringId": 3121663143040475136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT][take the splinter]",
                    "maleVariant": null,
                    "stringId": 3121842063962497028
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Thanks.",
                    "maleVariant": null,
                    "stringId": 3121844796383780864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Suckers who thought they could jump stronger than themselves and now bite the sand. You're not joining them, are you?",
                    "maleVariant": null,
                    "stringId": 3125722738610860032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I have obliterated my tracks, and in the evenings I will pray that the sad gentlemen in armor for millions of Euro-pins will not knock on my door.",
                    "maleVariant": null,
                    "stringId": 3125731565959835648
                  }
                }
              ]
            }
          }
        }
      }
