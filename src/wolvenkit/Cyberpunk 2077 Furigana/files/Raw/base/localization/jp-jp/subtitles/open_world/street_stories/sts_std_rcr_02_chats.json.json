        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "2号室の患者の様子は？ ゆうべ何か叫んでるみたいだったが",
                    "maleVariant": null,
                    "stringId": 1534550694564495360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今日はごきげんいかがかな？ 治療の効果はどうだ？ そのおかしな頭の中は少しぐらいまともになったかい？",
                    "maleVariant": null,
                    "stringId": 1534550694631604224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やめて… ほっといて… お願いだから…",
                    "maleVariant": null,
                    "stringId": 1534550694631604232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おいふざけんな、どんだけ吸えば気がすむんだ。1日1箱か？",
                    "maleVariant": null,
                    "stringId": 1534550694698713088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "別に迷惑かけちゃいないだろ？",
                    "maleVariant": null,
                    "stringId": 1534550694698713096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "副流煙って知ってるか？ 吸うなら外で吸え！",
                    "maleVariant": null,
                    "stringId": 1534550694698713104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかったよ、外で吸えばいいんだろ。そうカッカすんなって",
                    "maleVariant": null,
                    "stringId": 1534550694698713112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "…今日は気分が優れないようだ。なら薬の量を倍にしないといけないねぇ。それとも、今すぐロボトミー手術でも？",
                    "maleVariant": null,
                    "stringId": 1635959850055270400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "例の錠剤を吐き出したんで、“反対側”から入れてやったんだ",
                    "maleVariant": null,
                    "stringId": 1925778248386793472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "我々が部屋に入った時、すでに舌をだらんと出していたよ",
                    "maleVariant": null,
                    "stringId": 1925778911640473600
                  }
                }
              ]
            }
          }
        }
      }
    },
