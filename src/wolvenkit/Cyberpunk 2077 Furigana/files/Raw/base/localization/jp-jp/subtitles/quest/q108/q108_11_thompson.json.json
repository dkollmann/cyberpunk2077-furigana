    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1507038174385606656,
            "femaleVariant": "あんたの狙いは何だ？"
          },
          {
            "stringId": 1621349383045189632,
            "femaleVariant": "で… 何が狙いなんだ？"
          },
          {
            "stringId": 1621350960371929088,
            "femaleVariant": "なぜ俺を助けた？ 目的はなんだ？ スクリームシートの記事ネタでも探してんのか？"
          },
          {
            "stringId": 1621359885397856256,
            "femaleVariant": "助けに入ったところで、殺されるだけだ。あの男の動きを見たか？ 尋常じゃない速さだった"
          },
          {
            "stringId": 1621359986178592768,
            "femaleVariant": "お前とそう変わらない。彼女が狙われ、お前がそこに居合わせた"
          },
          {
            "stringId": 1621361178887655424,
            "femaleVariant": "で… お前はチンポジを直しながら、ただボケっと突っ立ってたのか"
          },
          {
            "stringId": 1621364355317960704,
            "femaleVariant": "アレはそこらで手に入るテックじゃない"
          },
          {
            "stringId": 1621366000810528772,
            "femaleVariant": "オルトは関係ない。連中の目当ては俺だ"
          },
          {
            "stringId": 1621430783228317696,
            "femaleVariant": "あんたには関係ないだろ"
          },
          {
            "stringId": 1621442129189736448,
            "femaleVariant": "待て。オルトは、あんたと会う約束をしてたのか…"
          },
          {
            "stringId": 1622651868972126208,
            "femaleVariant": "ああ。助けを求められてな"
          },
          {
            "stringId": 1622653052571160576,
            "femaleVariant": "切羽詰まってたよ。アラサカは前にも彼女をさらおうとしてる。その時は、運よく難を逃れたが"
          },
          {
            "stringId": 1622685886203629568,
            "femaleVariant": "あんたには関係ないだろ"
          },
          {
            "stringId": 1622686379369893888,
            "femaleVariant": "迷ってるのか？ 話は聞いてるぜ。色々あって険悪だったらしいな"
          },
          {
            "stringId": 1622705604398485508,
            "femaleVariant": "奴らはオルトに何をさせる気だ？"
          },
          {
            "stringId": 1622706292817989632,
            "femaleVariant": "だが正直、お前の色恋沙汰より、彼女がさらわれた理由に興味がある"
          },
          {
            "stringId": 1622719261136195584,
            "femaleVariant": "オルトはあんたと会うつもりだったのか？"
          },
          {
            "stringId": 1622722131600412672,
            "femaleVariant": "ソウルキラーというAIに聞き覚えは？"
          },
          {
            "stringId": 1622722131600412684,
            "femaleVariant": "ケッ、あるさ。くそダサい名前の都市伝説だ"
          },
          {
            "stringId": 1622722131617189896,
            "femaleVariant": "いや、ソウルキラーは実在する。アラサカの連中は、それを書いたランナーを“雇った”ってわけだ"
          },
          {
            "stringId": 1622722131617189908,
            "femaleVariant": "つまり――アラサカがソウルキラーを思いのままに使えるようになる。背筋が凍るだろ"
          },
          {
            "stringId": 1622723011431174144,
            "femaleVariant": "なら、奴らの狙いは何なんだ？"
          },
          {
            "stringId": 1622727628521017344,
            "femaleVariant": "もしそうなったら、誰もアラサカを止められない"
          },
          {
            "stringId": 1622727628521017356,
            "femaleVariant": "ソウルキラーは、そのICEに触れただけでランナーをあの世に送る"
          },
          {
            "stringId": 1622727628521017368,
            "femaleVariant": "実際に見たんだ。神経系を含め、体には一切損傷が残らない。音も臭いも全くしない"
          },
          {
            "stringId": 1622727628537794568,
            "femaleVariant": "ただ突然、脳波が停止する。まるでスイッチを切られたように…"
          },
          {
            "stringId": 1622727628537794580,
            "femaleVariant": "あるいは魂を失ったようにな"
          },
          {
            "stringId": 1622727914471886848,
            "femaleVariant": "あいつは身の危険を感じて… あんたを頼ったと？"
          },
          {
            "stringId": 1622729230644482048,
            "femaleVariant": "そうだ。他に頼れる奴がいなかったんだろ"
          },
          {
            "stringId": 1622739952912896000,
            "femaleVariant": "下らないたわ言だ"
          },
          {
            "stringId": 1622739952912896016,
            "femaleVariant": "続きは移動しながら聞かせてくれ"
          },
          {
            "stringId": 1622739952963227648,
            "femaleVariant": "続きは移動しながら聞かせてくれ"
          },
          {
            "stringId": 1622739952963227660,
            "femaleVariant": "素直に信じるとは、驚きだな"
          },
          {
            "stringId": 1622739953013559296,
            "femaleVariant": "下らないたわ言だな。行くぞ"
          },
          {
            "stringId": 1622739953013559308,
            "femaleVariant": "そうか。ならお前の考えを聞かせてみろ"
          },
          {
            "stringId": 1721538768682418176,
            "femaleVariant": "オルトは巻き添えになっただけだ。俺の弱みを握るためにな"
          },
          {
            "stringId": 1721538768682418188,
            "femaleVariant": "彼女の仕事すら知らないのか？ あの子はトップクラスのランナーなんだ"
          },
          {
            "stringId": 1721543214896316416,
            "femaleVariant": "だから何だ？ オルトとアラサカに繋がりがあるとすれば、俺様にファックされてるってことだけだ"
          },
          {
            "stringId": 1722541791839846412,
            "femaleVariant": "アラサカの狙いは彼女だ。企業間の抗争じゃ、人材の誘拐なんて珍しくもない"
          },
          {
            "stringId": 1722643074617769984,
            "femaleVariant": "彼女は最近、ITSのために高性能なブラックICEを書いた。色んな意味でヤバいプログラムだ"
          },
          {
            "stringId": 1722643966461018116,
            "femaleVariant": "[立ち上がって酒を飲む]"
          },
          {
            "stringId": 1722651583535185924,
            "femaleVariant": "[トンプソンに寄りかかる]"
          },
          {
            "stringId": 1727274195162824740,
            "femaleVariant": "いや、ネタならすでにあった。それを追ってたら、彼女が突如車でさらわれたんだ"
          },
          {
            "stringId": 1727274195179601928,
            "femaleVariant": "で、これからどうするつもりだ？ お前の女なんだろ？"
          },
          {
            "stringId": 1730019358624776192,
            "femaleVariant": "どこまで見たんだ？"
          },
          {
            "stringId": 1733148209967337472,
            "femaleVariant": "だから何だ？"
          },
          {
            "stringId": 1733152667287752704,
            "femaleVariant": "[立ち上がる]"
          },
          {
            "stringId": 1736154240920911888,
            "femaleVariant": "あんたはどうする？ 一緒に来るか？"
          },
          {
            "stringId": 1736154240920911904,
            "femaleVariant": "どこへ？"
          },
          {
            "stringId": 1736154240920911920,
            "femaleVariant": "アラサカ・タワーだ。オルトを取り返す"
          },
          {
            "stringId": 1736154240920911936,
            "femaleVariant": "2人で？ お前、気は確かか？"
          },
          {
            "stringId": 1736157622586900484,
            "femaleVariant": "[部屋を出る] 一緒に来るか？"
          },
          {
            "stringId": 1737590972660908056,
            "femaleVariant": "アトランティスに寄ってく。頼りになるチューマがいるからな"
          },
          {
            "stringId": 1737591599491252228,
            "femaleVariant": "[出る] アトランティスに知り合いがいる。会いに行こう"
          },
          {
            "stringId": 1737592021102690304,
            "femaleVariant": "[出る] ああ。まあ見てなって"
          },
          {
            "stringId": 1737593151937380352,
            "femaleVariant": "ああ。まあとにかく来い。横で見物してりゃいい"
          },
          {
            "stringId": 1737619207339704320,
            "femaleVariant": "俺にとっちゃお決まりの話さ。アラサカは俺を挑発した。そして成功した"
          },
          {
            "stringId": 1743378063537799172,
            "femaleVariant": "どこまで見てた？"
          },
          {
            "stringId": 1791137026963476480,
            "femaleVariant": "見物は得意だろ"
          },
          {
            "stringId": 1795187284186042368,
            "femaleVariant": "話せ"
          },
          {
            "stringId": 1795187591024545792,
            "femaleVariant": "話せ"
          },
          {
            "stringId": 1795187591058100224,
            "femaleVariant": "彼女が狙われ、お前がそこに居合わせたってだけだ"
          },
          {
            "stringId": 1795189403148423168,
            "femaleVariant": "ラッキーだったな"
          },
          {
            "stringId": 1795189774864420864,
            "femaleVariant": "ならあんたはラッキーだったな。オルトのいい横顔が撮れただろ"
          },
          {
            "stringId": 1795190893552406528,
            "femaleVariant": "ああ、だが話を聞く前に車でさらわれてしまった"
          },
          {
            "stringId": 1795191339138486272,
            "femaleVariant": "で、これからどうするつもりだ？ お前の女なんだろ？"
          },
          {
            "stringId": 2024203770192932868,
            "femaleVariant": "[飲む]"
          }
        ]
      }
    }
  }
}