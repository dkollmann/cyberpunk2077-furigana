  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ならどうしてトラックに乗るの？",
                    "maleVariant": "ならどうしてトラックに乗るんだ？",
                    "stringId": 1812883702697598976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "確かにノーマッドってのは獣じみたところがある",
                    "maleVariant": null,
                    "stringId": 1812883702697598992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おい、ノーマッドに喧嘩売ってんのか？",
                    "maleVariant": null,
                    "stringId": 1812892488959172608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "黙らないと今すぐ墓場まで送ってやるぞ！",
                    "maleVariant": null,
                    "stringId": 1812893101814095872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私の骨を折ることはできても、真実の追究は止められない！",
                    "maleVariant": null,
                    "stringId": 1812893463832862720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うんざり。もう黙らせてやって",
                    "maleVariant": "うんざりだ。もう黙らせてやってくれ",
                    "stringId": 1812894482629304324
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "放っておいて。危険はない",
                    "maleVariant": "放っておけ。危険はない",
                    "stringId": 1812895648729702400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "“狼人間”って誉め言葉じゃない",
                    "maleVariant": "“狼人間”って誉め言葉じゃないか",
                    "stringId": 1812896048698531840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アルデカルドスは最近どう？",
                    "maleVariant": "アルデカルドスは最近どうだ？",
                    "stringId": 1812897882297245696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もううんざりしてたの。こんなのが野放しになってると、みんなが迷惑する",
                    "maleVariant": "もううんざりしてたんだ。こんなのが野放しになってると、みんなが迷惑する",
                    "stringId": 1812902648150663168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしの先生に手を出さないで！",
                    "maleVariant": "おい、俺の先生に手を出すなよ！",
                    "stringId": 1812902675010985984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "落ち着いてよ。狼人間だと思われたくないの？ カッコいいじゃない",
                    "maleVariant": "落ち着けって。狼人間だと思われたくないのか？ カッコいいだろ",
                    "stringId": 1812902708598972416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アルデカルドスのみんなは元気にやってる？",
                    "maleVariant": "アルデカルドスのみんなは元気にやってるか？",
                    "stringId": 1812902741264211968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こいつの作り話を信じちまうバカが、いったいどれだけいると思うんだ？ “人食いノーマッド”なんて話、二度と聞きたくない",
                    "maleVariant": null,
                    "stringId": 1812904033344724992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バッカー・ファミリーは子供じみた冗談が好きでも、俺たちは違うんだ",
                    "maleVariant": null,
                    "stringId": 1812906688691478528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "誰かと思えば、Vか！ 順調だぞ。パナムもやる気になってる",
                    "maleVariant": null,
                    "stringId": 1812908178541813760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何だって？ 真面目に腹を立てたのがあほらしいな…",
                    "maleVariant": null,
                    "stringId": 1812908352437657600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "危なかった。昼間はそれほど危険な連中じゃないが、それでも一触即発だったな",
                    "maleVariant": null,
                    "stringId": 1812915043929595904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "危なかった… ありがとう。真実を隠し通すことはできない！",
                    "maleVariant": null,
                    "stringId": 1812916344650375168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ノーマッドになめた口をきくとこうなるんだ！",
                    "maleVariant": null,
                    "stringId": 1812920143012077568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうして狼人間が生まれたか知りたいか？ 何年も前、ケンタウルス座アルファ星から来た船がバッドランズに不時着したのだ",
                    "maleVariant": null,
                    "stringId": 1813937070085939200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うーん… そうやって移動できるなら、どうしてトラックやバイクに乗ってるの？",
                    "maleVariant": "うーん… そうやって移動できるなら、どうしてトラックやバイクに乗ってんだよ？",
                    "stringId": 1813937721058697216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ノーマッドのことはよく知ってるけど、確かに野性的な一面はある。でも人食いっていう話は行き過ぎよ",
                    "maleVariant": "ノーマッドのことはよく知ってるが、確かに野性的な一面はある。でも人食いっていう話は行き過ぎだって",
                    "stringId": 1813937733406728192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "日が暮れると、連中は服を脱ぎ捨て、裸で踊り、動物的な快楽行為に走る",
                    "maleVariant": null,
                    "stringId": 1813940838416424960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "暗闇の中、一瞬にして何十マイルも移動する。まるで闇夜を飛ぶ矢のように！",
                    "maleVariant": null,
                    "stringId": 1813941315694665728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そして牙の生えた獣の姿に変身すると、人間の肉体を求めて狩りを始めるのだ！",
                    "maleVariant": null,
                    "stringId": 1814329074770944000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その船には、アラサカの吸血鬼どもが求める謎の物質が積まれていた",
                    "maleVariant": null,
                    "stringId": 1814329264890355712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サブロウはそれを使って敵を狼人間に変え、服従させようとしていたのだ。しかし事故でその計画は頓挫した…",
                    "maleVariant": null,
                    "stringId": 1815414351534923776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その宇宙まで広がる大陰謀論について、確たる物証があるわけでもないんでしょ…？",
                    "maleVariant": "その宇宙まで広がる大陰謀論について、確たる物証があるわけでもないんだろ…？",
                    "stringId": 1854574298864459776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あるさ！ 今夜、連中がカブキの廃工場に集まる。何かを交換するらしい",
                    "maleVariant": null,
                    "stringId": 1854575236761165824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "調べてみろ。そしてその、ケンタウルス座アルファ星の何かを奪うんだ。私が正しかったとわかる！",
                    "maleVariant": null,
                    "stringId": 1912763937978490880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言っただろ？ 満月の夜に変身するって！ つまり、一ヶ月のうち三日だけなんだ！",
                    "maleVariant": null,
                    "stringId": 1923009676278743040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "奴らはキャンプ先で人肉を貪り食い、後には何も残らない。昼間には新しい獲物を求めて未開の地へ移動してるってわけさ…",
                    "maleVariant": null,
                    "stringId": 1923010013702111232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前が真実をどれほど否定しようと、私は構わない。だが連中が追いかけてきたらどうするつもりだ？",
                    "maleVariant": null,
                    "stringId": 1923013178824572928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "現実をわからせるために、痛い目に遭わせてやってよ",
                    "maleVariant": "現実をわからせるために、痛い目に会わせてやってくれ",
                    "stringId": 1923016151495589888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "喜んで",
                    "maleVariant": null,
                    "stringId": 1923016457478455296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ってのは冗談だけど、別に危険な男じゃない。放っといて",
                    "maleVariant": "ってのは冗談だが、別に危険な男じゃない。放っとけ",
                    "stringId": 1923016977890918400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあ… 結構面白いじゃない",
                    "maleVariant": "まあ… 結構面白いじゃないか",
                    "stringId": 1923019033150210048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "仲良く一緒の墓に入るんだな！",
                    "maleVariant": null,
                    "stringId": 1923019074069839872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この男は地獄や吸血鬼、宇宙から来た魔法使いだなんて話をしてるのよ。それがノーマッドの評判を落とすためなわけないでしょ？",
                    "maleVariant": "この男は地獄や吸血鬼、宇宙から来た魔法使いだなんて話をしてるんだ。それがノーマッドの評判を落とすためなわけないだろ？",
                    "stringId": 1923020752076652544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあ、頭のねじが外れてるのは確かだな",
                    "maleVariant": null,
                    "stringId": 1923021450394714112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、もういい。バカに割く時間なんてないからな",
                    "maleVariant": null,
                    "stringId": 1923021812782579712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあね。危険はない。政治家が正直者に思えるような話をしてるだけよ",
                    "maleVariant": "まあな。危険はない。政治家が正直者に思えるような話をしてるだけだ",
                    "stringId": 1923022525378052096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヘヘ、ソウルへの土産話ができた。いや、やっぱり仲間を食って、夜を飛ぶ矢のように姿を消すか？",
                    "maleVariant": null,
                    "stringId": 1923023737884229632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "口添えには感謝する… だが私をからかう必要はなかっただろ",
                    "maleVariant": null,
                    "stringId": 1923025965529415680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "治そうとはしないの？",
                    "maleVariant": "治そうとはしないのか？",
                    "stringId": 1923027251419136000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一種の病気なら、治療法があるはず。死体がそんなにたくさん見つかったら怪しまれるでしょ",
                    "maleVariant": "一種の病気なら、治療法があるはず。死体がそんなにたくさん見つかったら怪しまれるだろ",
                    "stringId": 1923027375939633152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "鳥は飛べることを治そうとするか？ 星は輝きを弱めようとするか？",
                    "maleVariant": null,
                    "stringId": 1923063339546296320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "狼人間は爬虫類人の掟に縛られない自由な獣だ。鎖につながれるより、人食いでも自由に生きるほうがいいだろ！",
                    "maleVariant": null,
                    "stringId": 1923063438548647936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "確かに、あんたのことは信じてない",
                    "maleVariant": null,
                    "stringId": 1924029700628004868
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたの話にも多少の真実は含まれてる",
                    "maleVariant": null,
                    "stringId": 1924029876084129792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "正直、これ以上バカげた陰謀説は聞いたことがない。ケンタウルス座アルファ星から来た吸血鬼？ 冗談でしょ？",
                    "maleVariant": "正直、これ以上バカげた陰謀説は聞いたことがない。ケンタウルス座アルファ星から来た吸血鬼？ 冗談だろ？",
                    "stringId": 1924029963208212480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたの“解釈”を真面目に受け止めるのは難しいけど、まったくのでっちあげってことでもなさそうね",
                    "maleVariant": "あんたの“解釈”を真面目に受け止めるのは難しいが、まったくのでっちあげってことでもなさそうだな",
                    "stringId": 1924030033924177920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたが歯を折られたら、火星着陸はでっち上げだって話を聞けなくなっちゃうでしょ？",
                    "maleVariant": "あんたが歯を折られたら、火星着陸はでっち上げだって話を聞けなくなっちまうだろ？",
                    "stringId": 1924062586605387776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私の言葉を信じてないんだな。君が私をからかっていることに、気づいてないと思ってるのか？",
                    "maleVariant": null,
                    "stringId": 1924063331077570560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私と同じことを耳にすれば… 世界は思ったほど単純ではないということがわかるだろう",
                    "maleVariant": null,
                    "stringId": 1924093758773379072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だが仕方がない。かのカサンドラも、誰にも信じてもらえなかった…",
                    "maleVariant": null,
                    "stringId": 1924093856215449600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "気味が悪いほど言い当ててることもある…",
                    "maleVariant": null,
                    "stringId": 1924094115775758336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "君にもわかるのか！ 闇のベールの向こうでは悪夢のような怪物がうろついているが… みんな怖くて信じないんだ",
                    "maleVariant": null,
                    "stringId": 1924094229189738496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "情報をどうも",
                    "maleVariant": null,
                    "stringId": 1924147962015629316
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。調べてみる。貴重な情報をどうも",
                    "maleVariant": null,
                    "stringId": 1924148133780766720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしてそんなこと知ってるの？",
                    "maleVariant": "どうしてそんなこと知ってるんだ？",
                    "stringId": 1924149687971725312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと待って… どうしてそんなことを知ってるの？",
                    "maleVariant": "ちょっと待て… どうしてそんなことを知ってるんだ？",
                    "stringId": 1924149737045082112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私の脳が電波を受信するからだ。あの事故以来、奴らの秘密の通信が聞こえるようになった",
                    "maleVariant": null,
                    "stringId": 1924149948857434112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「あの事故」？ それとこの件に何の関係が？",
                    "maleVariant": null,
                    "stringId": 1924150048581206016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わからないのか！ 私の頭には連中の演算装置が埋め込まれているんだ。まったくリパードクめ、いつの間に…",
                    "maleVariant": null,
                    "stringId": 1924150142902714368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だがこれのおかげで、連中の企みを聞くことができる。手伝ってくれるなら、一緒にこの星を救えるぞ！",
                    "maleVariant": null,
                    "stringId": 1924150237811425280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まずは様子を見て、ここぞという時に動くんだぞ！",
                    "maleVariant": null,
                    "stringId": 1924156577602584576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人類の運命は君の肩に懸かっている。我々人類を、私を失望させないでくれよ",
                    "maleVariant": null,
                    "stringId": 1924156673450819584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "責任重大ね",
                    "maleVariant": "責任重大だな",
                    "stringId": 1924157101974470656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なら、どこかのガキに馬鹿にされたとして、それでも同じように大騒ぎするわけ？",
                    "maleVariant": "なら、どこかのガキに馬鹿にされたとして、それでも同じように大騒ぎするのかよ？",
                    "stringId": 1924345973731737600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こいつは… 知り合いか？",
                    "maleVariant": null,
                    "stringId": 1924448446215602176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
