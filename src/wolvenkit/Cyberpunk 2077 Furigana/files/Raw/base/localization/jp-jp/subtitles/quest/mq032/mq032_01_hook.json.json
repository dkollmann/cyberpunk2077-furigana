  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この腕も… 何もかも… もはや私ではない！ 冷たく、命なき金属の塊だ！",
              "maleVariant": "",
              "stringId": "1770666438831390720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "助けてくれ！ 誰か助けてくれ！ お願いだ！",
              "maleVariant": "",
              "stringId": "1770667555170566144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしたの？",
              "maleVariant": "どうした？",
              "stringId": "1770667932674703364"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何があったの？ そのインプラント、未完成じゃ…",
              "maleVariant": "何があった？ そのインプラント、未完成じゃ…",
              "stringId": "1770668414818336768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…未完成かどうかは、問題ではないのだ！",
              "maleVariant": "",
              "stringId": "1770671627369988096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "比丘はサイバーウェアによって体を穢してはならない。波羅提木叉の誓いに反するからだ！",
              "maleVariant": "",
              "stringId": "1770672013094961152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "弟は今どこに？",
              "maleVariant": "",
              "stringId": "1770672253596352516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてそんなことに？",
              "maleVariant": "",
              "stringId": "1770672759513300992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "意味がわからない。一体どこの誰が無理やりそんなことを？",
              "maleVariant": "",
              "stringId": "1770673292475121664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メイルストロームね…",
              "maleVariant": "メイルストロームか…",
              "stringId": "1770674503437795328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの弟は、まだそいつらと？",
              "maleVariant": "",
              "stringId": "1770674733033996288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。まず拙僧を済ませた後で、弟をどう… 改造すべきか話し合っていた",
              "maleVariant": "",
              "stringId": "1770675018582212608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手を貸すよ",
              "maleVariant": "",
              "stringId": "1770675508728578048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その見返りは？",
              "maleVariant": "",
              "stringId": "1770675904150781952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見返りは？ タダで仕事はできない",
              "maleVariant": "",
              "stringId": "1770676197181636608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "拙僧に差し出せるは感謝の心のみ。どうか、今回だけは例外としてくれまいか",
              "maleVariant": "",
              "stringId": "1770677021580476416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっと天は見てくださる",
              "maleVariant": "",
              "stringId": "1770677541992939520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "関わりたくない",
              "maleVariant": "",
              "stringId": "1770678188502958080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪いけど、メイルストロームに関わるのはご免よ",
              "maleVariant": "悪いが、メイルストロームに関わるのはご免だ",
              "stringId": "1770679856326000640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、無理を言ってすまなかった。弟は人を傷つけることのない善人だ。きっと誰かが救ってくれるだろう",
              "maleVariant": "",
              "stringId": "1770680238930411520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかったよ、手を貸す",
              "maleVariant": "",
              "stringId": "1770681337334743044"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかったよ。人の心に訴えかけるのが上手ね。弟を助けてあげる",
              "maleVariant": "わかったよ。人の心に訴えかけるのが上手いな。弟を助けてやる",
              "stringId": "1770681735407747072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしが何とかする。メイルストロームは危険な連中だから、穏便に済ませられる保証はないよ",
              "maleVariant": "俺が何とかする。メイルストロームは危険な連中だから、穏便に済ませられる保証はないが",
              "stringId": "1770682195187351552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "感謝の言葉もないよ。弟が囚われている場所はここから北だ",
              "maleVariant": "",
              "stringId": "1770682883053207552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "波止場沿いの道付近を探してくれ",
              "maleVariant": "",
              "stringId": "1770683984225779712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴らは… 拙僧らが乞食をしているところに現れたんだ",
              "maleVariant": "",
              "stringId": "1825577848811536384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "拙僧らを笑い、インプラントの施しも受け取るかと聞いてきた",
              "maleVariant": "",
              "stringId": "1825583795730628608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人というより… 機械のような風貌だったよ。拙僧らも“改造”してやると言ってな… 道具のように",
              "maleVariant": "",
              "stringId": "1825586100299350016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの手の連中と渡り合える方とお見受けする。どうか、弟を救ってはくれないか！",
              "maleVariant": "",
              "stringId": "1825598467171139584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ間に合うことを祈るばかりだ。弟には… 拙僧のような目に遭ってほしくない",
              "maleVariant": "",
              "stringId": "1825598633181691904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰か！ どうか、救いの手を！",
              "maleVariant": "",
              "stringId": "1825702311242743808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ど、どうして私がこんな目に！ ヒンサの犠牲になるなんて！",
              "maleVariant": "",
              "stringId": "1834265666479964160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この、このかゆみは何だ？ なぜ感覚がある？ 私は何を感じてるんだ？ 金属に感覚などないはずだろう！",
              "maleVariant": "",
              "stringId": "1834265790379704320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お、落ち着け… 自分の魂など構うな。あいつの魂を救うんだ",
              "maleVariant": "",
              "stringId": "1834265978401964032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええと… 大丈夫？",
              "maleVariant": "ええと… 大丈夫か？",
              "stringId": "1834279627086610432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "インプラントを着けた僧侶？",
              "maleVariant": "",
              "stringId": "1834279709479518208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って… 無理やりインプラントを？",
              "maleVariant": "待て… 無理やりインプラントを？",
              "stringId": "1834309196627820544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "インプラントは… 信仰上ダメじゃなかった？",
              "maleVariant": "インプラントは… 信仰上ダメなんじゃ？",
              "stringId": "1834331093092515840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "当然だ！ 自らこんな姿になったと思うのか！？",
              "maleVariant": "",
              "stringId": "1834331203838918656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、お辞儀もできずに申し訳ない。まだ… この状況に困惑していてな",
              "maleVariant": "",
              "stringId": "1834332123364573184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "拙僧だけではない… 弟もだ。奴らは我らに薬を盛り… 拙僧の体を穢した",
              "maleVariant": "",
              "stringId": "1834332358665027584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰かー！",
              "maleVariant": "",
              "stringId": "1834400540180332544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中はあざ笑っていたよ… 同じ人間にこんな真似をして、どうして笑っていられるんだ？",
              "maleVariant": "",
              "stringId": "1837457554338988032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴ら… それをわかっていながら！ もう取り返しがつかない…",
              "maleVariant": "",
              "stringId": "1853384054875602944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クズどもを皆殺しにしてやる",
              "maleVariant": "",
              "stringId": "1853404389096275968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クローム中毒のイカれた連中め… メイルストロームが相手じゃ、穏便にはいかないよ",
              "maleVariant": "クローム中毒のイカれた連中め… メイルストロームが相手じゃ、穏便にはいかないな",
              "stringId": "1853404626862981120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "絶対許さない…",
              "maleVariant": "絶対許さねえ…",
              "stringId": "1853406581828075520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "嘆きの僧侶なんてずいぶんとドラマチックだな。お前の武勇伝に持ってこいだ",
              "maleVariant": "",
              "stringId": "1896638530641969152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "座禅でもしながら連中が死に絶えるのをじっと待ってみるか？",
              "maleVariant": "",
              "stringId": "1896639082964697088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい加減にして…",
              "maleVariant": "いい加減にしろ…",
              "stringId": "1896639323600306176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他に方法があるはずだ。助けてほしいが、頼む、殺生だけは避けてくれ",
              "maleVariant": "",
              "stringId": "1915385687633018880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "努力はする… 何にせよ弟は助け出してみせるから",
              "maleVariant": "",
              "stringId": "1915394338485682176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "力を貸してくれ… だが殺生だけは、どうか避けてほしい",
              "maleVariant": "",
              "stringId": "1915396797471899648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ジョニーに中指を立てる]",
              "maleVariant": "",
              "stringId": "2121595052244353024"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}