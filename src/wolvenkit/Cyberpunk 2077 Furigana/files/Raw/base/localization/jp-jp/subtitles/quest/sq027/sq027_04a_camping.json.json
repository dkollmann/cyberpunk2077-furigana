    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] じゃあ後は待つだけね",
              "maleVariant": "[座る] じゃあ後は待つだけだな",
              "stringId": "1757958994757701636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ後は待つだけね",
              "maleVariant": "じゃあ後は待つだけだな",
              "stringId": "1757961219349434368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何をびくびくしてんの。ソウルに盾突くのはあんたが初めてじゃないよ",
              "maleVariant": "",
              "stringId": "1785579829227966468"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "励ましてくれなくて結構だよ",
              "maleVariant": "",
              "stringId": "1785579829227966476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー、それって誰？",
              "maleVariant": "",
              "stringId": "1785579829227966484"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はあ… スコーピオンだ",
              "maleVariant": "",
              "stringId": "1785579829227966492"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつは戦争から帰ってきた時、新しいアイデアやら友達やらを山ほど抱えてた",
              "maleVariant": "",
              "stringId": "1785579829227966500"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、スネーク族に加わろうって言いだしたんだ。それが俺たちの生きる道だって",
              "maleVariant": "",
              "stringId": "1785579829244743680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] ソウルのことが心配？",
              "maleVariant": "[座る] ソウルのことが心配か？",
              "stringId": "1805482396761788416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソウルが心配？",
              "maleVariant": "ソウルが心配か？",
              "stringId": "1805483051593306112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はじめて聞く話ね。ソウルはどうしたの？",
              "maleVariant": "",
              "stringId": "1805488371883380736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クランを何だと思ってるって、怒鳴りつけてたな。1年間、どんな仕事も任せなかった",
              "maleVariant": "",
              "stringId": "1805488943650902016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい励みになったよ、ありがと",
              "maleVariant": "",
              "stringId": "1805489713255354368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "励ましはいらないんじゃなかった？",
              "maleVariant": "",
              "stringId": "1805490081951453184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む] スコーピオンに乾杯しよう",
              "maleVariant": "",
              "stringId": "1805498382646841344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スネーク族？ ならソウルは正しかった",
              "maleVariant": "",
              "stringId": "1805498795500572672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "戦争の話はあまりしないのね",
              "maleVariant": "戦争の話はあまりしないんだな",
              "stringId": "1805499234258325504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少し眠ることにする",
              "maleVariant": "",
              "stringId": "1805501050257739780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スコーピオンに乾杯しよう",
              "maleVariant": "",
              "stringId": "1805503685874487296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スネーク族の件は、ソウルが正しい",
              "maleVariant": "",
              "stringId": "1805504575603806208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "戦争の話はしないのね",
              "maleVariant": "戦争の話はしないのか",
              "stringId": "1805505087845765120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少し、眠っとこうかな",
              "maleVariant": "少し、眠っておくか",
              "stringId": "1805508909091590148"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうした方がいい。コンボイが来るまでまだあるから",
              "maleVariant": "",
              "stringId": "1805514716877230080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スコーピオンに",
              "maleVariant": "",
              "stringId": "1806703285541081088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話すことがねえからな。メキシコでパンツァーに詰め込まれて、ただ前線に送り込まれただけだ",
              "maleVariant": "",
              "stringId": "1806706739147771904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あとは何週間も独り言を言うか、車内のAIと話す毎日だ。ガイガーカウンタがピーピーうるさいから、ひたすらヨウ素剤を飲むんだよ",
              "maleVariant": "",
              "stringId": "1806707030534459392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フン。あのクソ熱い動く棺桶に座ってるのを思い出しただけで、ケツに湿疹が出そう",
              "maleVariant": "",
              "stringId": "1806719414619672576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ。それなのにバシリスクに乗るのは待ちきれないってわけ？",
              "maleVariant": "へえ。それなのにバシリスクに乗るのは待ちきれないってわけか",
              "stringId": "1806720900024045568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そいつは別だ",
              "maleVariant": "",
              "stringId": "1806721809030385664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まったく別物よ。乗ればわかる",
              "maleVariant": "",
              "stringId": "1806721954287521792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クランは独立してなきゃ。でないといずれは…",
              "maleVariant": "クランは独立してないと。じゃなきゃいずれは…",
              "stringId": "1806736160344838144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バッカーみたいになる",
              "maleVariant": "",
              "stringId": "1806737201320759296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うん",
              "maleVariant": "ああ",
              "stringId": "1806744496205271040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちだけでも十分やってける。でかい仕事をこなしゃいい",
              "maleVariant": "",
              "stringId": "1806744783162773504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのためにここに来た",
              "maleVariant": "",
              "stringId": "1806747641899716608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スコーピオンも喜ぶさ",
              "maleVariant": "",
              "stringId": "1806748277437435904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アルデカルドスの名を再び上げるためにな",
              "maleVariant": "",
              "stringId": "1806749266856972288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジャッキーにも",
              "maleVariant": "",
              "stringId": "1806982561679085568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[すり寄る] 寒くなってきた",
              "maleVariant": "",
              "stringId": "1873467562821177348"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "冷え込んできたね",
              "maleVariant": "冷え込んできたな",
              "stringId": "1873468065030361088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "緊張で眠れなさそうね",
              "maleVariant": "緊張で眠れなさそうだな",
              "stringId": "1873468440554786816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "怒ってる？",
              "maleVariant": "怒ってるのか？",
              "stringId": "1873468870303174656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何かあったら起こして",
              "maleVariant": "何かあったら起こしてくれ",
              "stringId": "1873583158342733824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "眠ろうとしても眠れなそうね",
              "maleVariant": "眠ろうとしても眠れなさそうだな",
              "stringId": "1873585510340644864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ怒ってる？",
              "maleVariant": "まだ怒ってるか？",
              "stringId": "1873585955205304320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私は少し横になる。あんたの隣で、星を眺めながらね",
              "maleVariant": "",
              "stringId": "1873586272076582912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何かあったら起こして。パナムも休むなら別だけど",
              "maleVariant": "何かあったら起こしてくれ。パナムも休むなら別だが",
              "stringId": "1873597920682201088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう？ ま、いいけど",
              "maleVariant": "",
              "stringId": "1873602703480606720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一緒に休まない？",
              "maleVariant": "一緒に休まないか？",
              "stringId": "1873609883172753408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムにそんな趣味があったなんて。ひょっとして本棚に天体図でも置いてるの？",
              "maleVariant": "パナムにそんな趣味があったなんてな。ひょっとして本棚に天体図でも置いてるのか？",
              "stringId": "1873652948373622784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろん。あれが北斗七星。その隣にある小さな星は見える？",
              "maleVariant": "",
              "stringId": "1873657120682692608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おやすみ",
              "maleVariant": "",
              "stringId": "1873657765196222464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え？ んなわけないでしょ",
              "maleVariant": "",
              "stringId": "1873662270331703296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少し眠ったら？",
              "maleVariant": "眠ったらどうだ？",
              "stringId": "1873665480870088704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うそ、死兆星が頭上に輝いたら… なんて",
              "maleVariant": "",
              "stringId": "1873668483823980544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…あれ？",
              "maleVariant": "…あれか？",
              "stringId": "1873669289415561216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[飲む]",
              "maleVariant": "",
              "stringId": "2208601324284833796"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今夜は星がよく見える。あと数時間で夜明けってとこか",
              "maleVariant": "",
              "stringId": "2228821801554231296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "日が暮れるまでまだ時間があるね",
              "maleVariant": "",
              "stringId": "2228830442961321984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだ、せっかくだし車の点検をしとこうかな",
              "maleVariant": "",
              "stringId": "2228834546517360640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？ まさか寝落ちてた？",
              "maleVariant": "",
              "stringId": "2228942036328333312"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}