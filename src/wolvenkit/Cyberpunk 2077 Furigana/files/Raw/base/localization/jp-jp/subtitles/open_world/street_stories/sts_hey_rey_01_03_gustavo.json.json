    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1293731806519615488,
            "femaleVariant": "クソ、やっぱりこうなるか"
          },
          {
            "stringId": 1689581839766220800,
            "femaleVariant": "ミエルダ！ どうやって入ってきやがった！？"
          },
          {
            "stringId": 1693901726411182084,
            "femaleVariant": "ノーラン・フレイクスの使いよ",
            "maleVariant": "ノーラン・フレイクスの使いだ"
          },
          {
            "stringId": 1693902053835329536,
            "femaleVariant": "さあ。マーサの父親の使いよ",
            "maleVariant": "さあ。マーサの父親の使いだ"
          },
          {
            "stringId": 1693920644165062656,
            "femaleVariant": "いったい何のこと？",
            "maleVariant": "いったい何のことだ？"
          },
          {
            "stringId": 1693929559980298240,
            "femaleVariant": "何があったか話して",
            "maleVariant": "何があったか話せ"
          },
          {
            "stringId": 1948751951863554048,
            "femaleVariant": "ミサだって？ パドレは本気でそんな真似を？"
          },
          {
            "stringId": 1948775816161468416,
            "femaleVariant": "マーサの父親の依頼よ",
            "maleVariant": "マーサの父親の依頼だ"
          },
          {
            "stringId": 1948776966088630272,
            "femaleVariant": "自分で蒔いた種よ",
            "maleVariant": "自分で蒔いた種だ"
          },
          {
            "stringId": 1948777232527597568,
            "femaleVariant": "そしてあんたは、彼女の保険金にたかる恋人。感動的ね",
            "maleVariant": "そしてあんたは、彼女の保険金にたかる恋人か。感動的だな"
          },
          {
            "stringId": 1948780537588817920,
            "femaleVariant": "どうせ保険金目当てでしょ",
            "maleVariant": "どうせ保険金目当てだろ"
          },
          {
            "stringId": 1948782155885518848,
            "femaleVariant": "ノーラン・フレイクスの使いよ",
            "maleVariant": "ノーラン・フレイクスの使いだ"
          },
          {
            "stringId": 1948782628818460672,
            "femaleVariant": "マーサは昏睡状態なんだ！ もう目覚めないかもしれねえ！ 治療費で破産寸前なんだよ！"
          },
          {
            "stringId": 1948789778932375552,
            "femaleVariant": "あいつに逃げられた愛しの父親が、ただ黙ってるわけないよな"
          },
          {
            "stringId": 1948790468459175936,
            "femaleVariant": "あの男は何もわかっちゃいねえ"
          },
          {
            "stringId": 1948795078737469440,
            "femaleVariant": "シックス・ストリートに襲われたんだ。待ち伏せされてな"
          },
          {
            "stringId": 1948797042594488320,
            "femaleVariant": "チェイチェックスとのパーティで、べろべろになって帰る途中、バークレー通りで襲われたんだ。とっさに銃を抜こうとしたが、一瞬早く撃たれちまった"
          },
          {
            "stringId": 1948805820752666624,
            "femaleVariant": "俺は殺されかけ、マーサは… 知っての通りだ"
          },
          {
            "stringId": 1948807065856974848,
            "femaleVariant": "あんたも俺を消しに来たなら、やれよ。ただしまっすぐ狙え。今度は誰も巻き込むな"
          },
          {
            "stringId": 1948820149384323076,
            "femaleVariant": "誰も撃つ気はない。信じるよ"
          },
          {
            "stringId": 1948826424885190656,
            "femaleVariant": "連中があんたを消そうとする理由は？"
          },
          {
            "stringId": 1948830882222383104,
            "femaleVariant": "誰も撃ったりしない。あんたを信じるよ"
          },
          {
            "stringId": 1948838743589593088,
            "femaleVariant": "シックス・ストリートがあんたを消したい理由は？"
          },
          {
            "stringId": 1948845606930554880,
            "femaleVariant": "ヴァレンティーノズが気に食わないんだろ？"
          },
          {
            "stringId": 1948846175241969664,
            "femaleVariant": "マーサが一緒だとは思わなかったのさ。シックス・ストリートの連中は俺を待ち構えてた。まるで銃殺隊のようにな"
          },
          {
            "stringId": 1948851528801263616,
            "femaleVariant": "それか、俺に恨みがある奴かもな。殺し屋を雇うくらい恨んでる奴もいる"
          },
          {
            "stringId": 1948869368149254144,
            "femaleVariant": "ノーランのこと？ そのために娘を危険にさらすとは思えない",
            "maleVariant": "ノーランのことか？ そのために娘を危険にさらすとは思えない"
          },
          {
            "stringId": 1948878310237609984,
            "femaleVariant": "知ってるのはそれだけだ"
          },
          {
            "stringId": 1948881681937932288,
            "femaleVariant": "え…？"
          },
          {
            "stringId": 1948881864759255040,
            "femaleVariant": "事情はわかった。でも、街を出てもらう",
            "maleVariant": "事情はわかった。だが、街を出てもらう"
          },
          {
            "stringId": 1948885636998832128,
            "femaleVariant": "あいつは俺の女だ"
          },
          {
            "stringId": 1948886630730113024,
            "femaleVariant": "街を…？ マーサをあのままにしろって言うのか？"
          },
          {
            "stringId": 1952105932492972032,
            "femaleVariant": "今もチューブだらけで眠ったままだろ。それでも…"
          },
          {
            "stringId": 1965060907477049344,
            "femaleVariant": "ちくしょう！"
          },
          {
            "stringId": 1965061337812639744,
            "femaleVariant": "食らえ！"
          },
          {
            "stringId": 1965063678133608448,
            "femaleVariant": "もし目覚めたとしても、あんたが死んでちゃ意味がない"
          },
          {
            "stringId": 1965064109492609024,
            "femaleVariant": "はあ、そうだな… あんたの言う通りだ"
          },
          {
            "stringId": 1965065141257838592,
            "femaleVariant": "わかったよ。俺は行く。ありがとな"
          },
          {
            "stringId": 2005253229434970112,
            "femaleVariant": "安心して帰っていいぞ。うちの人間もあんたには手出ししない"
          }
        ]
      }
    }
  }
}