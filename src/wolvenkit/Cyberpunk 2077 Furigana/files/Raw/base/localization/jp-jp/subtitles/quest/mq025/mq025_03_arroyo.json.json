    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ロストビッチだって！？ 目が見えねえのか？ テクトロニカのロゴだぞ！",
                    "maleVariant": null,
                    "stringId": 1852094905156726788
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一戦やりにきた",
                    "maleVariant": null,
                    "stringId": 1852094905207058432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一戦やりにきた。あんたがバック？",
                    "maleVariant": "一戦やりにきた。あんたがバックか？",
                    "stringId": 1852094905240612868
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、俺様がそうだ",
                    "maleVariant": null,
                    "stringId": 1852094905240612876
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "へっ。お前を見ただけで財布が濡れてきた。有り金全部賭けてやる",
                    "maleVariant": null,
                    "stringId": 1852094905240612900
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "受けて立つ",
                    "maleVariant": null,
                    "stringId": 1852094905274167296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "スナイパーライフルも入れてよ",
                    "maleVariant": "スナイパーライフルも入れてくれよ",
                    "stringId": 1852094905290944516
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "また後で",
                    "maleVariant": null,
                    "stringId": 1852094905290944532
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいライフルね",
                    "maleVariant": "いいライフルだな",
                    "stringId": 1852094905307721728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "また後で",
                    "maleVariant": null,
                    "stringId": 1852094905324498948
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何だ、怖いのか？ はっ！ そりゃ当然だ",
                    "maleVariant": null,
                    "stringId": 1852094905324498956
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やりましょ。だからその口を閉じてて",
                    "maleVariant": "やるさ。だからその口を閉じてろ",
                    "stringId": 1852094905391607812
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "提案がある。賭けを増やすってのはどう？",
                    "maleVariant": "提案がある。賭けを増やすってのはどうだ？",
                    "stringId": 1852094905425162244
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言ったろ… 持ち金を全額賭けるって",
                    "maleVariant": null,
                    "stringId": 1852094905425162252
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "準備はいいか？ 怖いなら逃げ出していいんだぜ？",
                    "maleVariant": null,
                    "stringId": 1852094905458716676
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "へえ？ ボコボコにされるのが好きらしいな",
                    "maleVariant": null,
                    "stringId": 1852094905542602756
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "このっ、クソアマ！",
                    "maleVariant": "このっ、クソ野郎！",
                    "stringId": 1852094905878147076
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほら、ライフルを渡して",
                    "maleVariant": "ほら、ライフルを渡せよ",
                    "stringId": 1852094905894924292
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "思ったとおり、てんで雑魚じゃねえか",
                    "maleVariant": null,
                    "stringId": 1852094905911701508
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "またやりたけりゃ言ってくれ。サンドバッグとして使ってやる",
                    "maleVariant": null,
                    "stringId": 1852094905928478724
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[試合を開始する] いつでも",
                    "maleVariant": null,
                    "stringId": 1852094906180136960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "相手になってあげる",
                    "maleVariant": "相手になってやるよ",
                    "stringId": 1852094906230468612
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そのライフル… 「グラッド」だっけ？ どこで手に入れたの？",
                    "maleVariant": "そのライフル… 「グラッド」だっけ？ どこで手に入れたんだ？",
                    "stringId": 1852094906297577472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これはこれは… どうやら銃に詳しいようだな",
                    "maleVariant": null,
                    "stringId": 1852103318192353280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しーっ… 負け犬は黙ってて",
                    "maleVariant": "しーっ… 負け犬は黙ってろ",
                    "stringId": 1852111028463955968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もらうべきものはもらっとく",
                    "maleVariant": null,
                    "stringId": 1853240499753545728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうかしたのか？ 哺乳瓶を武器にしたきゃ、構わんから拾えよ",
                    "maleVariant": null,
                    "stringId": 1864938740229619712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "SPT32グラッド。史上最高のスナイパーライフルだ",
                    "maleVariant": null,
                    "stringId": 1887843047505412096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "軍にいた時の記念品だ。どうして聞く？",
                    "maleVariant": null,
                    "stringId": 1887843776743882752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "調子にのるなよ？ 運がよかっただけだ…",
                    "maleVariant": null,
                    "stringId": 1887908361693839360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "確かに。ありがとね、バック",
                    "maleVariant": "確かに。ありがとな、バック",
                    "stringId": 1887936181505638400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "失せろと言っただろ！",
                    "maleVariant": null,
                    "stringId": 1887936350166990848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "興味があって。限定品だって聞いた。300丁しか製造されなかったKGBモデルでしょ",
                    "maleVariant": "興味があって。限定品だって聞いた。300丁しか製造されなかったKGBモデルだろ",
                    "stringId": 1905579341497290752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でもここは銃の話をする場所じゃねえ。骨とクロームをかち割る場所だ。で、やるのか？",
                    "maleVariant": null,
                    "stringId": 1905581420815110144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そのライフルも入れてよ。負けるのが怖いなら別だけど…",
                    "maleVariant": "そのライフルも入れてくれ。負けるのが怖いなら別だが…",
                    "stringId": 1905583150982287360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はあ？ んなわけねえだろ。いいさ。ライフルも賭けてやる",
                    "maleVariant": null,
                    "stringId": 1905583780496986112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいだろう。やるって言うよりやられることになるがな",
                    "maleVariant": null,
                    "stringId": 1905586980734160896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ついてこい",
                    "maleVariant": null,
                    "stringId": 1905588315110035456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "死んでも渡すか。やれ！",
                    "maleVariant": null,
                    "stringId": 1905603618380500992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言いたいように言えばいい。さあ、エディーを渡して",
                    "maleVariant": "言いたいように言えばいい。さあ、エディーを渡せ",
                    "stringId": 1908129431856402432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "受け取ってすぐに失せろ",
                    "maleVariant": null,
                    "stringId": 1908131149474222080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺を怒らせようとしてるなら、うまいことやってるぜ",
                    "maleVariant": null,
                    "stringId": 1908133047883321344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そのとおりよ",
                    "maleVariant": "そのとおりだ",
                    "stringId": 1908133206226685956
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "落ち着いて。ただのスポーツでしょ",
                    "maleVariant": "落ち着けよ。ただのスポーツだろ",
                    "stringId": 1908134495941611520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "落ち着いてよ、バック。ただのスポーツじゃない",
                    "maleVariant": "落ち着けって、バック。ただのスポーツだろ",
                    "stringId": 1908135318998274048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うるせえ。何がスポーツだ、ったく",
                    "maleVariant": null,
                    "stringId": 1908135630718947328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしてそこまで長いライフルが必要なのかと、不思議に思ってたの",
                    "maleVariant": "どうしてそこまで長いライフルが必要なのかと、不思議に思ってたんだ",
                    "stringId": 1908138267593986048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どういう意味だ？",
                    "maleVariant": null,
                    "stringId": 1908138900917112832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まるで何かを補おうとしてるみたい… たとえば、身体的な特徴とか",
                    "maleVariant": "まるで何かを補おうとしてるみたいでな… たとえば、身体的な特徴とか",
                    "stringId": 1908139054579634176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わからない？ あんたのイチモツはネズミ並みだって言ってるの",
                    "maleVariant": "わからないか？ お前のイチモツはネズミ並みだって言ってんだ",
                    "stringId": 1908139647536779264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何だと、てめえ！",
                    "maleVariant": null,
                    "stringId": 1908141728565874688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自信だけはあるみたいね…",
                    "maleVariant": "自信だけはあるみたいだな…",
                    "stringId": 1932922544181268480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんでか知りたいか？ この腕はミリタリーグレードだ。チタン製の骨格に、炭素繊維の外殻。何なら戦車にだって穴を開けられる",
                    "maleVariant": null,
                    "stringId": 1932922746480939008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で？ やるか？ それとも怖くてちびったか？",
                    "maleVariant": null,
                    "stringId": 1932925882864029696
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}