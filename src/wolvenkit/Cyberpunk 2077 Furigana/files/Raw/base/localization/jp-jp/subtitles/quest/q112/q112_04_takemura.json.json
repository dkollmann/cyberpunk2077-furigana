  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃ、作戦開始？",
              "maleVariant": "それじゃ、作戦開始か？",
              "stringId": "1660580220269064192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "了解。じゃあ、座って待つとしましょ",
              "maleVariant": "了解。じゃあ、座って待つとするか",
              "stringId": "1660648652486307840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんな話はもういい",
              "maleVariant": "",
              "stringId": "1660663118875684868"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ストリートのみんなは企業を憎んでる",
              "maleVariant": "",
              "stringId": "1660663443900690432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] クソ、どんどんひどくなる…",
              "maleVariant": "",
              "stringId": "1660706242226384896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、そろそろ行動を起こすか",
              "maleVariant": "",
              "stringId": "1685252933890883584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、そう焦るな。しばらくここから監視を続ける",
              "maleVariant": "",
              "stringId": "1712936061744726016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見落としがあるとまずいからな",
              "maleVariant": "",
              "stringId": "1713036092657123328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "化け猫も、いい加減飽きたようね",
              "maleVariant": "化け猫も、いい加減飽きたようだな",
              "stringId": "1766413117334360064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V",
              "maleVariant": "",
              "stringId": "1773573723300696064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なに？",
              "maleVariant": "なんだ？",
              "stringId": "1773576740515221504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "動くな。じっとしてろ",
              "maleVariant": "",
              "stringId": "1773577106929618944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見えるか？",
              "maleVariant": "",
              "stringId": "1773610744643719168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "猫だ",
              "maleVariant": "",
              "stringId": "1773611061783433216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この街じゃ珍しい",
              "maleVariant": "",
              "stringId": "1773616136740278276"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただの野良猫でしょ",
              "maleVariant": "ただの野良猫だろ",
              "stringId": "1773618395104546816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "珍しい… この街じゃあんまり見かけないんだけど",
              "maleVariant": "珍しいな… この街じゃあんまり見かけないんだが",
              "stringId": "1773620770053689344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから何？ ただの猫じゃない",
              "maleVariant": "だから何だ？ ただの猫じゃないか",
              "stringId": "1773620979282350080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダイナーで市民の声を聞いたでしょ？ みんな心の底から企業を憎んでるのよ",
              "maleVariant": "ダイナーで市民の声を聞いただろ？ みんな心の底から企業を憎んでる",
              "stringId": "1781989202595811328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう言い合うのはこの辺にしましょ",
              "maleVariant": "もう言い合うのはこの辺にしとこう",
              "stringId": "1781998416022081536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "勝手に決めつけないで",
              "maleVariant": "勝手に決めつけるな",
              "stringId": "1782006745440063488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ようやく話が通じてきた",
              "maleVariant": "",
              "stringId": "1782006745456840704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ノーマッドになりたいと思うこともある。この世界を離れ、何もかも忘れてな",
              "maleVariant": "",
              "stringId": "1782126000508637184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ようやく話が通じてきた",
              "maleVariant": "",
              "stringId": "1782227335463702528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっと我が道を行くんだろう",
              "maleVariant": "",
              "stringId": "1783526439892422656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "紺碧プラザに乗り込む時もそう言ったのか？",
              "maleVariant": "",
              "stringId": "1834479571673419776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちもな",
              "maleVariant": "",
              "stringId": "1995380886478856192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "特におかしな様子がなければ、その時はいよいよ“作戦開始”といこうじゃないか",
              "maleVariant": "",
              "stringId": "2004085483342995456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジャッキー？ どういうこと？",
              "maleVariant": "ジャッキー？ どういうことだ？",
              "stringId": "2011525265438351360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺がお前の捜索に出た時、別の班はウェルズを探しに行き、そして遺体を回収した",
              "maleVariant": "",
              "stringId": "2011526037878153216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてそんなことするのよ？ 葬儀もできない母親をあざ笑うため？",
              "maleVariant": "どうしてそんなことするんだ？ 葬儀もできない母親をあざ笑うためか？",
              "stringId": "2011528585481625600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカには… 死者から情報を引き出す技術がある",
              "maleVariant": "",
              "stringId": "2011529065544884224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いったいどういうこと？",
              "maleVariant": "いったいどういうことだ？",
              "stringId": "2011533238558597120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺も詳しい方法を知っているわけではない。わかっているのはソウルキラーを使うということぐらいだ",
              "maleVariant": "",
              "stringId": "2011533889380360192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウェルズの遺体から記憶痕跡を取り出すことも可能だったのだ。損傷し、不完全なものだったとしてもな",
              "maleVariant": "",
              "stringId": "2011535192231845888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信じられない",
              "maleVariant": "",
              "stringId": "2011536939394342912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信じられない。そんなのデタラメよ",
              "maleVariant": "信じられない。そんなのデタラメだ",
              "stringId": "2011537798622683136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "かく言うお前自身も死から蘇った。それも“デタラメ”か？",
              "maleVariant": "",
              "stringId": "2011538012012093440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこに保管されてるの？",
              "maleVariant": "どこに保管されてる？",
              "stringId": "2011541966066360320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジャッキーの精神は、どこにあるの？",
              "maleVariant": "ジャッキーの精神は、どこにあるんだ？",
              "stringId": "2011543650851168268"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは… わからない",
              "maleVariant": "",
              "stringId": "2011543650867945472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰もが企業の奴隷になりたいわけじゃない",
              "maleVariant": "",
              "stringId": "2011590749345095684"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業の奴隷として生きるのを、よしとしない人もいる",
              "maleVariant": "",
              "stringId": "2011592214935576576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜ今まで黙ってたの？",
              "maleVariant": "なぜ今まで黙ってたんだ？",
              "stringId": "2011650300626935808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前を信用できるかわからなかった。だが状況が変わった",
              "maleVariant": "",
              "stringId": "2011650478331207680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別に文句を言ってるわけじゃない",
              "maleVariant": "",
              "stringId": "2011659115879538688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今からだって変われる",
              "maleVariant": "",
              "stringId": "2012789124660797440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "思ってることを言っていいか？",
              "maleVariant": "",
              "stringId": "2012797710417862656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの日紺碧で、俺たちはそれぞれ大切な人間を失った",
              "maleVariant": "",
              "stringId": "2012798393686761472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知った風に。あんたに何がわかるの",
              "maleVariant": "知った風に。あんたに何がわかる",
              "stringId": "2012803108906541056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別に文句を言ってるわけじゃ…",
              "maleVariant": "",
              "stringId": "2012842009868181504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのことにケリがついていないから、互いに衝突し、言い争いになってるんじゃないか",
              "maleVariant": "",
              "stringId": "2012842998499188736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前に1つ、伝えるべきことがある… 一緒に居たウェルズのことだ…",
              "maleVariant": "",
              "stringId": "2012863072589127680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死者の無念を晴らすのは、生者の務めだ",
              "maleVariant": "",
              "stringId": "2012865370732175360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハナコ様はきっと俺たちに手を貸してくださる",
              "maleVariant": "",
              "stringId": "2012865911679950848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あるいは俺たちを葬り去るか",
              "maleVariant": "",
              "stringId": "2012871354477256704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "遅すぎることはない",
              "maleVariant": "",
              "stringId": "2014141091161120768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ノーマッドはそんな気楽なもんじゃない",
              "maleVariant": "",
              "stringId": "2014141091161120784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今のあたしは誰にも縛られず、自由に生きてる。あんたもそうしてみたら？",
              "maleVariant": "今の俺は誰にも縛られず、自由に生きてる。あんたもそうしてみろよ？",
              "stringId": "2014151271038918656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ノーマッドは一匹狼じゃない。大切なのはファミリーよ。血が繋がってなくてもね",
              "maleVariant": "ノーマッドは一匹狼じゃない。大切なのはファミリーだ。血が繋がってなくてもな",
              "stringId": "2014153015047299080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺の過去についてとやかく言う権利を、お前が有しているとでも思っているのか？",
              "maleVariant": "",
              "stringId": "2027094611174694912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は、自分の力を過信して楽な道を選ぼうとしたお前や、お前の友達のウェルズとは根本から違う",
              "maleVariant": "",
              "stringId": "2027271725811310592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの手は少しも汚れてないって言うの？",
              "maleVariant": "あんたの手は少しも汚れてないって言うのか？",
              "stringId": "2027328329168527360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしがアラサカ社員だったことを忘れてない？",
              "maleVariant": "俺がアラサカ社員だったことを忘れてないか？",
              "stringId": "2027344169242796032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしだってアラサカにいたから、よく知ってる。幸せな夢の世界とは程遠かった",
              "maleVariant": "俺だってアラサカにいたから、よく知ってる。幸せな夢の世界とは程遠かった",
              "stringId": "2027345135761432576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいや、お前は企業に反感を抱き、企業がもたらす秩序や世界を考えもなく忌み嫌っている。それに代わるものを何一つ提案できないくせにな",
              "maleVariant": "",
              "stringId": "2028753201010647040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前は薄汚れた通りを指さし、それがこの街の全てかのように批判するが、アラサカの下で働く何百万という人々はどうだ？ 彼らは平和で安定した暮らしを得てるではないか",
              "maleVariant": "",
              "stringId": "2028753325598253056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ千葉11区の住民は？ 彼らがスコップ缶を漁って苦しい生活をしてるのも、企業に責任があるのよ",
              "maleVariant": "じゃあ千葉11区の住民は？ 彼らがスコップ缶を漁って苦しい生活をしてるのも、企業に責任があるんだぞ",
              "stringId": "2028753422889328640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業は資本とリソースを投じてこの世界の秩序を維持してる。他の誰にそれができるというのだ？ 腐敗した政府や、フィーダーに繋がれた愚かな大衆どもか？",
              "maleVariant": "",
              "stringId": "2028753779824599040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それとも、お前のような日和見主義者か？",
              "maleVariant": "",
              "stringId": "2028756566268518400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの手は少しも汚れてないって言うの？",
              "maleVariant": "あんたの手は少しも汚れてないって言うのか？",
              "stringId": "2028829904076611644"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "汚れてない手などこの世に存在しない。問題はその汚し方だ",
              "maleVariant": "",
              "stringId": "2028829904093388804"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ？ あんたはどんな風に汚したわけ？",
              "maleVariant": "へえ？ あんたはどんな風に汚したんだ？",
              "stringId": "2028829904093388816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカ社は、それまでの俺の人生に欠けていた、生きる意味と誇りを与えてくれた。俺にとって、この事実以上に重んじるべきものはない",
              "maleVariant": "",
              "stringId": "2028829904093388832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "霊を信じるの？",
              "maleVariant": "霊を信じるのか？",
              "stringId": "2037552193874714628"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティで動物を見たのは初めてだ。もちろん、ゴキブリを除いてな",
              "maleVariant": "",
              "stringId": "2037552315492753408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "化け猫かもな",
              "maleVariant": "",
              "stringId": "2037553994506199040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただの猫よ",
              "maleVariant": "ただの猫だ",
              "stringId": "2037556175863046144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いても不思議じゃない",
              "maleVariant": "",
              "stringId": "2037556515953991680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バケネコって？",
              "maleVariant": "",
              "stringId": "2037557230009081856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バケネコ？ 何それ？",
              "maleVariant": "バケネコ？ 何だそれ？",
              "stringId": "2037560996309524480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最初に鳥がいなくなって、次は犬。猫は最後まで粘ってるみたいね",
              "maleVariant": "最初に鳥がいなくなって、次は犬。猫は最後まで粘ってるみたいだな",
              "stringId": "2037591566460674048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "霊を信じてるの？",
              "maleVariant": "霊を信じてるのか？",
              "stringId": "2037592256893444096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大げさね。ただの猫よ",
              "maleVariant": "大げさな。ただの猫だろ",
              "stringId": "2037592348111167488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "猫のおばけ？ ま、いても不思議じゃないか",
              "maleVariant": "",
              "stringId": "2037592462934433792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "猫の幽霊みたいなものだ… 不幸を呼び寄せ、死者を蘇らせる力を持つと言われている",
              "maleVariant": "",
              "stringId": "2037592901155315712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈Relic〉が死者との対話を可能にしたではないか。もはや信じる信じないの域は脱している",
              "maleVariant": "",
              "stringId": "2037593750485098496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ、それはどうだろうな",
              "maleVariant": "",
              "stringId": "2037597760625598464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昔は祖母が、狐や河童といった妖怪の話をよく聞かせてくれた… 化け猫もそうやって知ったんだ",
              "maleVariant": "",
              "stringId": "2037598986318667776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "故郷が恋しい？",
              "maleVariant": "故郷が恋しいか？",
              "stringId": "2037662730830766084"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "出身はどこなの？",
              "maleVariant": "出身はどこなんだ？",
              "stringId": "2037662935026262016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "故郷が恋しい？",
              "maleVariant": "故郷が恋しいか？",
              "stringId": "2037663511004864512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういえば出身はどこなの？",
              "maleVariant": "そういえば出身はどこなんだ？",
              "stringId": "2037663612238585856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "馬鹿げた質問だ",
              "maleVariant": "",
              "stringId": "2037664110236688384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は千葉11区のスラム街でこの世に生を受けた。とにかく、あそこを抜け出したい一心で――",
              "maleVariant": "",
              "stringId": "2037667150620549120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、やめておこう。昔のことはもう忘れた。今は何も知らなかった子供の頃だけが懐かしい",
              "maleVariant": "",
              "stringId": "2037668020301094912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしが子供の頃は…",
              "maleVariant": "俺が子供の頃は…",
              "stringId": "2037674193964261380"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "千葉11区での暮らしはどうだったの？",
              "maleVariant": "千葉11区での暮らしはどうだったんだ？",
              "stringId": "2037676335475220480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしが子供の頃は…",
              "maleVariant": "俺が子供の頃は…",
              "stringId": "2037678352398573568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしが子供の頃は…",
              "maleVariant": "俺が子供の頃は…",
              "stringId": "2037678438029484032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "千葉11区での暮らしはどうだった？",
              "maleVariant": "",
              "stringId": "2037678479905415168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "子供の頃ね… あたしは夜になるとよく星を探したのを覚えてる。街の明かりでほとんど見えなかったけどね",
              "maleVariant": "子供の頃か… 俺は夜になるとよく星を探したのを覚えてる。街の明かりでほとんど見えなかったけどな",
              "stringId": "2037678690996346880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "子供の頃か… バイクに初めて乗った時、丘を駆け回ったっけ…",
              "maleVariant": "",
              "stringId": "2037678826002604032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "子供の頃の思い出ね… お巡りから逃げるのに、毎日建物の屋根を飛び回ってた",
              "maleVariant": "子供の頃の思い出か… お巡りから逃げるのに、毎日建物の屋根を飛び回ってた",
              "stringId": "2037678927236325376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チカチカ点滅するチャーター・ヒルのネオンを見ながら眠りについてた…",
              "maleVariant": "",
              "stringId": "2037681769363824640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ガキのころシャツを洗ってた運河に、化学薬品の悪臭が漂っていたのを今でも覚えてる",
              "maleVariant": "",
              "stringId": "2037682901708464128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あ！ あとは人工トウモロコシ畑でのファーストキス",
              "maleVariant": "",
              "stringId": "2037684037609873408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつも口の中で血の味がしてさ…",
              "maleVariant": "",
              "stringId": "2037712798942257152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時々、企業の輸送車がスラム街にやってくるんだ。アラサカが子供たちを選別するのさ。ただし、比較的小ぎれいな奴だけをな",
              "maleVariant": "",
              "stringId": "2037716317610991616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして子供を？",
              "maleVariant": "",
              "stringId": "2037718816208216068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "軍に入隊させるため？",
              "maleVariant": "軍に入隊させるためか？",
              "stringId": "2037722349389242368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "軍用輸送車がスラム街に？",
              "maleVariant": "",
              "stringId": "2037724143595053056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして子供を？",
              "maleVariant": "",
              "stringId": "2037728344375058432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。その子供たちを企業兵に仕立てるのね？",
              "maleVariant": "わかった。その子供たちを企業兵に仕立てるんだな？",
              "stringId": "2037728462352441344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "輸送車って、軍用の？",
              "maleVariant": "輸送車って、軍用か？",
              "stringId": "2037728563552608256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業兵にするためさ",
              "maleVariant": "",
              "stringId": "2037729001236619264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのとおりだ",
              "maleVariant": "",
              "stringId": "2037732564968955904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それも完全武装だ。当時、千葉11区の殺人件数は日本一だったからな。ひょっとすると今でもそうかもしれない",
              "maleVariant": "",
              "stringId": "2037734752181051392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "選ばれた時は、宝くじにでも当たった気分だったよ",
              "maleVariant": "",
              "stringId": "2037738915312984064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "軍ではそれまでの生活に欠けていた全てが与えられた。一日三度の飯や、規律… 才能があると見込まれた後は、教育もな",
              "maleVariant": "",
              "stringId": "2037739664583450624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その見返りにアラサカの敵を殺してきたのね",
              "maleVariant": "その見返りにアラサカの敵を殺してきたんだな",
              "stringId": "2037752071116578820"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "食べるためなら、何でもしないと",
              "maleVariant": "",
              "stringId": "2037752510344093696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その見返りに、サブロウの敵を殺してきたんでしょ",
              "maleVariant": "その見返りに、サブロウの敵を殺してきたんだろ",
              "stringId": "2037760146359738368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "飯にありつくためなら何でもしないとね",
              "maleVariant": "飯にありつくためなら何でもしないとな",
              "stringId": "2037760284654329856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前はただ金のために手を汚すが、俺は違う。大義があるからな",
              "maleVariant": "",
              "stringId": "2038667239627448320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "周りを見てみなよ。これがあんたの崇める企業が築いた、素晴らしい世界？",
              "maleVariant": "周りを見てみろよ。これがあんたの崇める企業が築いた、素晴らしい世界か？",
              "stringId": "2038761262937563136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんなが世界を変えたがってるわけじゃないでしょ",
              "maleVariant": "みんなが世界を変えたがってるわけじゃないだろ",
              "stringId": "2038850407584260096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。残念なことにな",
              "maleVariant": "",
              "stringId": "2038850835201941504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一度に全ての問題を解決することはできない",
              "maleVariant": "",
              "stringId": "2038902374692052992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしだってそう。結果はどうあれ、アラサカ相手に強奪をやったことは、後悔してない",
              "maleVariant": "俺だってそうだ。結果はどうあれ、アラサカ相手に強奪をやったことは、後悔してない",
              "stringId": "2038909518833278976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なに？",
              "maleVariant": "なんだ？",
              "stringId": "2038955482348544000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…おぞましい連中だぜ",
              "maleVariant": "",
              "stringId": "2039085712031637504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうやってサブロウのボディガードに？",
              "maleVariant": "",
              "stringId": "2039190069821595648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで、その… 下っ端の企業兵が、どうやってサブロウ・アラサカのボディガードにまで？",
              "maleVariant": "",
              "stringId": "2039190468012040192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アカデミーを首席で卒業し、特殊部隊での任務を経たのち、サブロウ様が直々に声をかけてくださったのだ",
              "maleVariant": "",
              "stringId": "2039206057887686656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サブロウがあんたに一目ぼれしたってこと？",
              "maleVariant": "サブロウがあんたに一目ぼれしたってのか？",
              "stringId": "2039207152282263552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "限られた情報から最適な判断を下せる力、世界一の権力者に相応しい性質であるとは思わんか",
              "maleVariant": "",
              "stringId": "2039207538024013824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "100人の候補者が整列するなか… サブロウ様はそれぞれの魂を見極め、最も忠実に奉ずるであろう1人を選びだされたのだ",
              "maleVariant": "",
              "stringId": "2039213022982463488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、なるほど。コソ泥になる方がずっと立派だからな",
              "maleVariant": "",
              "stringId": "2039228135160438784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは利益だけを優先して、誰彼構わず金を巻き上げたりはしない",
              "maleVariant": "俺は利益だけを優先して、誰彼構わず金を巻き上げたりはしない",
              "stringId": "2039230187064287232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確か英語のことわざにあるだろ… “老犬に新しい芸は仕込めない”",
              "maleVariant": "",
              "stringId": "2043488478191706112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。だが今、細かい話はいいだろう",
              "maleVariant": "",
              "stringId": "2043489346311970816"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}