  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "安心してくれ、街での暮らしは今日も相変わらず素敵に輝いてるぞ。当たり前だよな！",
              "maleVariant": "",
              "stringId": 1728443389846540288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "出発しましょ",
              "maleVariant": "出発しよう",
              "stringId": 1729941983879548928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "街を見なよ。きっとアラサカの連中は、何が起きたのかわからずに混乱してるだろうね",
              "maleVariant": "",
              "stringId": 1819750276658753536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴らには絶対に見つからない",
              "maleVariant": "",
              "stringId": 1819750930768850948
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの街にいる理由はもうない",
              "maleVariant": "",
              "stringId": 1819755232631582720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あそこは自分の街だと思ってた。互いに必要として、切り離せない関係だって… だけどもう、いる理由はなくなった",
              "maleVariant": "あそこは自分の街だと思ってた。互いに必要として、切り離せない関係だってな… だがもう、いる理由はなくなった",
              "stringId": 1819757396741451776
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫よ",
              "maleVariant": "大丈夫だ",
              "stringId": 1819758924038533124
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一緒にここに来れて良かった",
              "maleVariant": "",
              "stringId": 1821102462655393792
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムがいなかったら、街を出たかわからない",
              "maleVariant": "",
              "stringId": 1821102816704983040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナム。〈神輿〉では色々あったけど… 無事にここまで… 一緒に来れて良かった",
              "maleVariant": "パナム。〈神輿〉では色々あったが… 無事にここまで… 一緒に来れて良かった",
              "stringId": 1821261074673561600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直、こうなるとは思ってなかった",
              "maleVariant": "",
              "stringId": 1821261155522965504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの求めるものは、あそこにない",
              "maleVariant": "俺の求めるものは、あそこにない",
              "stringId": 1821288328405315584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっとなんとかなる。心配しないで",
              "maleVariant": "きっとなんとかなる。心配するな",
              "stringId": 1821289883720347648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この後の計画は？ 準備はできてるの？",
              "maleVariant": "この後の計画は？ 準備はできてるのか？",
              "stringId": 1821293285938425856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大した量じゃない。大丈夫よ",
              "maleVariant": "大した量じゃない。大丈夫だ",
              "stringId": 1821315293166301184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっとなんとかなるよ",
              "maleVariant": "きっとなんとかなるさ",
              "stringId": 1821315424531902464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫。これからの計画は？",
              "maleVariant": "大丈夫だ。これからの計画は？",
              "stringId": 1821315495365308416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの求めるものは、あそこにはない",
              "maleVariant": "俺の求めるものは、あそこにはない",
              "stringId": 1821317656069402624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしあたしの気が変わっても、一緒にいてくれる？",
              "maleVariant": "もし俺の気が変わっても、一緒にいてくれるか？",
              "stringId": 1821338807072059392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このダム… 今となってはずいぶん懐かしいね",
              "maleVariant": "このダム… 今となってはずいぶん懐かしいな",
              "stringId": 1823962001582977024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてここにしたの？ ひょっとして、あたしとの思い出に浸るため？",
              "maleVariant": "どうしてここにしたんだ？ ひょっとして、俺との思い出に浸るためか？",
              "stringId": 1823962068926722048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの頃はあんたを理解してなかった",
              "maleVariant": "",
              "stringId": 1823964772424085504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はわかるの？",
              "maleVariant": "今はわかるのか？",
              "stringId": 1823970092277952516
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんな風に思ってたの？",
              "maleVariant": "どんな風に思ってたんだ？",
              "stringId": 1823970308100059136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ今は？",
              "maleVariant": "",
              "stringId": 1823971086512549888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どう思ってたの？",
              "maleVariant": "どう思ってたんだ？",
              "stringId": 1823971160667844608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "涙を流して懺悔すると思ってるなら無駄よ",
              "maleVariant": "",
              "stringId": 1823974981427130368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私と似てるのかなって思ってた。何か大切なことを証明しようとしてるのかなって",
              "maleVariant": "",
              "stringId": 1823976411517022208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、その通りよ。でも、こんな仲になるなんて、思ってもみなかったでしょ",
              "maleVariant": "ああ、その通りだ。だが、こんな仲になるなんて、思ってもみなかっただろ",
              "stringId": 1823989157486784512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "覚えてたんだ",
              "maleVariant": "",
              "stringId": 1823996367965122560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの時は、どうせ一度きりの仕事相手だと思ってた",
              "maleVariant": "",
              "stringId": 1823996722132152320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの時、ミッチとスコーピオンの手を借りて、ラフェンに仕返ししようとしてたでしょ？",
              "maleVariant": "あの時、ミッチとスコーピオンの手を借りて、ラフェンに仕返ししようとしてただろ？",
              "stringId": 1824000850132594688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もっと早くここに来て、ナイトシティや、あそこでの暮らしを見つめ直せばよかった",
              "maleVariant": "もっと早くここに来て、ナイトシティや、あそこでの暮らしを見つめ直しゃよかったな",
              "stringId": 1824025763174281216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“夢の街”だなんてさ。言い出した間抜け野郎の股間を蹴り飛ばしてやりたい",
              "maleVariant": "",
              "stringId": 1824026672482611200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー、ナイトシティね。ずっとあそこにいると思ってたの？",
              "maleVariant": "",
              "stringId": 1824051103984644096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "数ヶ月前は、街を離れるなんて死んでもありえないと思ってた。骨をうずめる覚悟だったのに",
              "maleVariant": "",
              "stringId": 1824051180622966784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたがいたから、決心できたのよ。ありがとう",
              "maleVariant": "パナムがいたから決心できたんだ。ありがとう",
              "stringId": 1824055944228237312
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティにお別れをしたら。二度と戻ることはないから",
              "maleVariant": "",
              "stringId": 1824058002188660736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしやっぱり街を離れたくないと言っても、一緒にいてくれる？",
              "maleVariant": "もしやっぱり街を離れたくないと言っても、一緒にいてくれるか？",
              "stringId": 1824061301797285888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、クランは私の家族なんだ。見捨てることなんてできない。どっちかを選べるようなものじゃないんだよ",
              "maleVariant": "",
              "stringId": 1824067975555706880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私たちと一緒に行こう。先のことはなるようになるから",
              "maleVariant": "",
              "stringId": 1824069334359207936
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“先”じゃなくて、まずは“今”を生きればいいのかもね",
              "maleVariant": "“先”じゃなくて、まずは“今”を生きればいいのかもな",
              "stringId": 1824072027454697472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私も同じ。いったい何を考えてたんだろ？",
              "maleVariant": "",
              "stringId": 1824075236718014464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしたちは、あの街で本当の自分が見つかると思い込んでたのよ",
              "maleVariant": "俺たちは、あの街で本当の自分が見つかると思い込んでたんだ",
              "stringId": 1824076721853952000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね、 ところが実際は、カジノでディーラーに乗せられた客とおんなじ。全てを賭けて、全てを失った",
              "maleVariant": "",
              "stringId": 1824077594000748544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全てじゃない",
              "maleVariant": "",
              "stringId": 1824078618467889152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、それもそうね。あのゴミ溜めから得たものもある",
              "maleVariant": "",
              "stringId": 1824078780082810880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもこれでやっとおさらばね。今度はもっと別の場所で、もっとマシな何かに賭ければいい",
              "maleVariant": "だがこれでやっとおさらばだ。今度はもっと別の場所で、もっとマシな何かに賭ければいい",
              "stringId": 1824085990242381824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつか見つかると思う？",
              "maleVariant": "",
              "stringId": 1824091433543004160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあね。見つからなくてもいいのかも",
              "maleVariant": "さあな。見つからなくてもいいのかも",
              "stringId": 1824092246650777600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、また鼻血が出てる",
              "maleVariant": "",
              "stringId": 1824093411291557888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、約束して。南に着いたら、私のツテを頼るんだ。とにかく、出来ることは何でも試さなきゃ",
              "maleVariant": "",
              "stringId": 1824118291013693440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "出発しましょ",
              "maleVariant": "出発しよう",
              "stringId": 1824128143131799552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これからの計画は？ 準備万端？",
              "maleVariant": "これからの計画は？ 準備万端か？",
              "stringId": 1824128470925045760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はヴィクの薬が効いてるし――",
              "maleVariant": "",
              "stringId": 1824138317338898432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつらなら助けてくれる。絶体絶命の仲間を何人も救ってくれたんだ",
              "maleVariant": "",
              "stringId": 1824139981739388928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着いたら、パナムのツテを頼ってみるから",
              "maleVariant": "",
              "stringId": 1824140722101153792
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、期待してる",
              "maleVariant": "ああ、期待してる",
              "stringId": 1824142063221469184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手配済みよ。仲間と合流して、装備を整えたら出発だ",
              "maleVariant": "",
              "stringId": 1824148972095795200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで、これからどうするの？",
              "maleVariant": "それで、これからどうするんだ？",
              "stringId": 1824179895323455488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの街で全てを失った。これから取り戻していかなきゃ",
              "maleVariant": "あの街で全てを失った。これから取り戻していこう",
              "stringId": 1824185522351370240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何もかも早く忘れてしまいたい",
              "maleVariant": "",
              "stringId": 1824186323832532992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの街は、手にすることのできないまやかしばかりを作り出す",
              "maleVariant": "",
              "stringId": 1824191804764450816
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。全てを失う前にそう気づければ、まだラッキーなほう",
              "maleVariant": "ああ。全てを失う前にそう気づければ、まだラッキーなほうさ",
              "stringId": 1824192209028247552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なら、こうして脱出できた私たちは… ツイてるかもね",
              "maleVariant": "",
              "stringId": 1824195502060204032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね。これから色々と取り戻していかなきゃ",
              "maleVariant": "そうだな。これから色々と取り戻していかないと",
              "stringId": 1824195869430902784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ばっちり手配済みよ。仲間と合流して、装備を整えたら出発だ",
              "maleVariant": "",
              "stringId": 1824216501933699072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいニュースは、南から砂嵐が迫ってきてるって話だ。予報では近年で最大規模のものになるらしい。こいつは見物だぞ",
              "maleVariant": "",
              "stringId": 1826862296998735872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でしょうね。わかってる",
              "maleVariant": "だろうな。わかってるさ",
              "stringId": 1844374039480102912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今どう思ってるかは関係ない",
              "maleVariant": "",
              "stringId": 1844377565849579520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "明日になれば、私たちは別の場所にいる。二人で新しいスタートを切るんだから、その時また聞いてよ",
              "maleVariant": "",
              "stringId": 1844383986741800960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "答えが楽しみ。まずは、アリゾナにたどり着かないとね",
              "maleVariant": "答えが楽しみだ。まずは、アリゾナにたどり着かないとな",
              "stringId": 1844384800906539008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもまだ生きてて――",
              "maleVariant": "",
              "stringId": 1844401336547405824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そして… 一緒に出ていける",
              "maleVariant": "",
              "stringId": 1844405794320805888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やめてよ… ほら、街にお別れを言っときな。もうこれで最後だから",
              "maleVariant": "",
              "stringId": 1844410619129360384
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティには、夢の怖さについて教わった。いつまでも付きまとって、隙あらば飼い主の手に噛みついてくるから",
              "maleVariant": "ナイトシティには、夢の怖さについて教わった。いつまでも付きまとって、隙あらば飼い主の手に噛みついてくるからな",
              "stringId": 1844415640835321856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "簡単にはいかないけど、ヘマしない限り、ほんの数時間で州境を越えられるはず",
              "maleVariant": "",
              "stringId": 1844424243201830912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…違う、今のはウソよ",
              "maleVariant": "",
              "stringId": 1845989278852182016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2人が協力してたら、それであたしは用無しだったはずよ",
              "maleVariant": "2人が協力してたら、それで俺は用無しだったはずだ",
              "stringId": 1845989844680568832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "簡単にはいかないけど、ヘマしない限り、ほんの数時間で州境を越えられるはず",
              "maleVariant": "",
              "stringId": 1845992888503758848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈神輿〉のこと？",
              "maleVariant": "",
              "stringId": 1863110459220684800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちは塵とクロームにまみれて生き、塵とクロームにまみれて死んでくんだ。じゃ、いいニュースと悪いニュースを伝えよう",
              "maleVariant": "",
              "stringId": 1863164338813661184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが砂嵐はナイトシティを直撃しない、それが悪いニュースだ。俺たちはまた生き永らえちまうな",
              "maleVariant": "",
              "stringId": 1863164766431342592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってる、パナム。約束する",
              "maleVariant": "わかってるよ、パナム。約束する",
              "stringId": 1863172809663574016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何も心配はいらない。出発しましょ",
              "maleVariant": "何も心配はいらない。出発しよう",
              "stringId": 1863173418055757824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってるって、パナム。あたしたちの時間はまだこれからよ",
              "maleVariant": "わかってるよパナム。俺たちの時間はまだこれからさ",
              "stringId": 1863173574822064128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "前にソウルが… こんなこと言ってた",
              "maleVariant": "",
              "stringId": 1863174641165135872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人が本当に自由になれるのは、失うものは何もないと、わかった時だって",
              "maleVariant": "",
              "stringId": 1863176122928852992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさに、この先数ヶ月間のあたしたちね。確かに失うものは何もない",
              "maleVariant": "まさに、この先数ヶ月間の俺たちだな。確かに失うものは何もない",
              "stringId": 1863176798044024832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心配しないで",
              "maleVariant": "心配するな",
              "stringId": 1863179402539036672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[寄りかかる] 懐かしいね",
              "maleVariant": "[寄りかかる] 懐かしいな",
              "stringId": 1873474347798560772
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK、でもその前に",
              "maleVariant": "",
              "stringId": 1885064133618561024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "せっかくだし、最後に街の姿をよく見といたら？ 嫌じゃなければ、だけど。私は車で待ってる",
              "maleVariant": "",
              "stringId": 1885064306876870656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあね、ナイトシティ。いい夢見なさいよ",
              "maleVariant": "あばよ、ナイトシティ。いい夢見るんだぞ",
              "stringId": 1892365115627864064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[離れる] じゃあね、ナイトシティ",
              "maleVariant": "[離れる] じゃあな、ナイトシティ",
              "stringId": 2005251672978878468
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしたちはこうして旅立とうとしてる…",
              "maleVariant": "俺たちはこうして旅立とうとしてる…",
              "stringId": 2005255153781436416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[出る]",
              "maleVariant": "",
              "stringId": 2038876037568724992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう関係のない話よ。アリゾナで日焼けでもしながら、全部忘れるの",
              "maleVariant": "もう関係のない話さ。アリゾナで日焼けでもしながら、全部忘れるんだ",
              "stringId": 2302277847194202112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ、確かにね… 二人してナイトシティに散々しごかれた",
              "maleVariant": "",
              "stringId": 2302280138928353280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時間と余裕――いま必要なのはそれだけ",
              "maleVariant": "時間と余裕――いま必要なのはそれだけだ",
              "stringId": 2302285482672975872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさか",
              "maleVariant": "",
              "stringId": 2302289993730813952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気づいた時には、私たちはとっくにいないけど",
              "maleVariant": "",
              "stringId": 2302295674965913600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫、奴らには絶対に見つからない",
              "maleVariant": "大丈夫だ、奴らには絶対に見つからない",
              "stringId": 2302297894893580292
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってる。だから今のうちに街をよく見とくんだ。いつかいい思い出にするために",
              "maleVariant": "",
              "stringId": 2302297895078129664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "二度と戻らないと決めた時点で、もういい思い出になってるんじゃない？",
              "maleVariant": "二度と戻らないと決めた時点で、もういい思い出になってるんじゃないか？",
              "stringId": 2302297895229124608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういうあんたは？ もう心の準備はいいの？ あのコンクリの山を見るのも最後かもしれないんだよ",
              "maleVariant": "",
              "stringId": 2302297895413673984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "約束する",
              "maleVariant": "",
              "stringId": 2302316609861251076
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう出発しましょ",
              "maleVariant": "もう出発しよう",
              "stringId": 2302317016205422592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはパナムとアリゾナに行き、落ち着いたら壊れた頭を治す方法を探すことを誓います",
              "maleVariant": "俺はパナムとアリゾナに行き、落ち着いたら壊れた頭を治す方法を探すことを誓います",
              "stringId": 2302318451009712128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、これから大移動なんだから、時間がもったいないよ",
              "maleVariant": "ほら、これから大移動なんだ。時間がもったいないぞ",
              "stringId": 2302320860050157568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どう？ これで安心した？",
              "maleVariant": "どうだ？ これで安心したか？",
              "stringId": 2302323544119504896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "合格にしとく… あのさ… 前にソウルがこんなこと言ってたんだ…",
              "maleVariant": "",
              "stringId": 2302325911451791360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…は炎上、そこらの小国の国家予算をはるかに超える大損害だ。ざまあみろ、アラサカ！ 地に膝をつく気分はどうだ？ ハハハ！",
              "maleVariant": "",
              "stringId": 2302336438315970560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[乗る]",
              "maleVariant": "",
              "stringId": 2321646029733392388
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}