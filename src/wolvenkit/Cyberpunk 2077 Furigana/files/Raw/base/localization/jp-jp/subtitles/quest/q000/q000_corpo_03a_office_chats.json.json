  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの男、東京から来た重役たちに必死に根回ししてたのよ",
                    "maleVariant": null,
                    "stringId": 1665092308867915776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "見ないで。例の彼女よ、防諜部の",
                    "maleVariant": "見ないで。例の彼よ、防諜部の",
                    "stringId": 1665092738666635264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "プラハから人員を引き上げさせる",
                    "maleVariant": null,
                    "stringId": 1665092888621391872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "3人がミリテクに襲われた",
                    "maleVariant": null,
                    "stringId": 1665093059832881152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヨーロッパの近隣施設は従業員の避難を開始している",
                    "maleVariant": null,
                    "stringId": 1699782972216987648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その3人の容体は？",
                    "maleVariant": null,
                    "stringId": 1699783634715693056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よくジェンキンスと働けるな。何をしに来たんだと思う？",
                    "maleVariant": null,
                    "stringId": 1699784691881627648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "漏洩の後始末でしょ。会社が傾くかもしれないのよ",
                    "maleVariant": null,
                    "stringId": 1699784836669001728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふん、あの程度の経歴で特殊作戦室室長になろうだなんて…",
                    "maleVariant": null,
                    "stringId": 1699785448332742656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そう、フランクフルトの件で",
                    "maleVariant": null,
                    "stringId": 1699785819344097280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "死んだよ。全員の安全が確保できたら、メディア向けに発表する",
                    "maleVariant": null,
                    "stringId": 1699855632526405632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何て言うのよ",
                    "maleVariant": null,
                    "stringId": 1699860601501577216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「セキュリティの定期メンテナンス中に悲劇的な事故が発生した」、だろうな",
                    "maleVariant": null,
                    "stringId": 1699862554386296832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "接待が功を奏す可能性はあるだろう？",
                    "maleVariant": null,
                    "stringId": 1721288780309000192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ありえないわ。それに判断はナイトシティに委ねるはずよ",
                    "maleVariant": null,
                    "stringId": 1721289034433490944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いえ、すぐに始めて。ただし目立たないようにね",
                    "maleVariant": null,
                    "stringId": 1721291367708028928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[使用する]",
                    "maleVariant": null,
                    "stringId": 1771972785564831748
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ご確認いただきたいことが。送り込んだ工作員の正体がバレたようです",
                    "maleVariant": null,
                    "stringId": 1771985058349993984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "引き上げさせて",
                    "maleVariant": "引き上げさせろ",
                    "stringId": 1771986083890876420
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "調査を続行させて",
                    "maleVariant": "調査を続行させろ",
                    "stringId": 1771986590059483136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "引き上げさせて。ただし慎重にね。向こうに悟られたくない",
                    "maleVariant": "引き上げさせろ。ただし慎重にな。向こうに悟られたくない",
                    "stringId": 1771986915738800128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "調査を続行させて。ここで引き上げたら、今までの投資が無駄になる",
                    "maleVariant": "調査を続行させろ。ここで引き上げたら、今までの投資が無駄になる",
                    "stringId": 1771987020646731776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジェンキンスさんには…",
                    "maleVariant": null,
                    "stringId": 1771989038660603904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大丈夫でしょうか…？",
                    "maleVariant": null,
                    "stringId": 1771989816334897152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "防諜部は損失を最低限に抑えようと頑張ってる",
                    "maleVariant": null,
                    "stringId": 1772059118752780288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "3人死んでるのよ。何も抑えられてない",
                    "maleVariant": null,
                    "stringId": 1772059913556611072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "4人目は出さないってことだろ",
                    "maleVariant": null,
                    "stringId": 1772059983970586624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それに、最優先すべきは採決の掌握だ",
                    "maleVariant": null,
                    "stringId": 1772060054384562176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "フランクフルトの件？",
                    "maleVariant": "フランクフルトの件か？",
                    "stringId": 1803983979962888192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "フランクフルトの件？",
                    "maleVariant": "フランクフルトの件か？",
                    "stringId": 1803983992176701444
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今は忙しい",
                    "maleVariant": null,
                    "stringId": 1803984827262627840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今は忙しいの。後で対応する",
                    "maleVariant": "今は忙しいんだ。後で対応する",
                    "stringId": 1803993871071244288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いえ、うちの管轄の別件です",
                    "maleVariant": null,
                    "stringId": 1803995741311397888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "承知しました… では後ほど、お時間のあるときに",
                    "maleVariant": null,
                    "stringId": 1803996672480440320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "話して、手短にね",
                    "maleVariant": "話せ、手短にな",
                    "stringId": 1803997775901159424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バイオテクニカに潜入させた工作員のことです",
                    "maleVariant": null,
                    "stringId": 1803997920990523392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼は最近、最高機密情報へのアクセス権限を付与されたのですが、有益な情報を掴めずにいます",
                    "maleVariant": null,
                    "stringId": 1803998169712750592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "というと？",
                    "maleVariant": null,
                    "stringId": 1803998426169274368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "“キレイ”すぎるんです。サーバーはまるで滅菌室。相手に気づかれ、虚偽のデータを掴まされている可能性があります",
                    "maleVariant": null,
                    "stringId": 1803998466837245952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう引き上げさせた方がいいでしょうか？",
                    "maleVariant": null,
                    "stringId": 1803998568121298944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "予想されるリスクは？",
                    "maleVariant": null,
                    "stringId": 1804004578676371456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "予想されるリスクは？",
                    "maleVariant": null,
                    "stringId": 1804004796058759168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "殺される可能性は低いですが、メディアへの告発、家庭崩壊をほのめかした脅迫はありえるでしょう",
                    "maleVariant": null,
                    "stringId": 1804007057661030400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼を揺さぶるには十分かと",
                    "maleVariant": null,
                    "stringId": 1804007215249420288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしから話しておく",
                    "maleVariant": "俺から話しておく",
                    "stringId": 1804008065367732224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかりました",
                    "maleVariant": null,
                    "stringId": 1804008132946358272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "同じことを二度言わせる気？",
                    "maleVariant": "同じことを二度言わせる気か？",
                    "stringId": 1804009297150930944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いえ。失礼しました",
                    "maleVariant": null,
                    "stringId": 1804009592262160384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "頼まれていたレポートです…",
                    "maleVariant": null,
                    "stringId": 1817168889814851584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "メールで送って。今忙しいの",
                    "maleVariant": "メールで送れ。今忙しいんだ",
                    "stringId": 1817169083373592580
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "遅かったじゃない",
                    "maleVariant": "遅かったじゃないか",
                    "stringId": 1817169951191867392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "メールでいい。後で見る",
                    "maleVariant": null,
                    "stringId": 1817171574991495168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "昨日までにやるんじゃなかったの？",
                    "maleVariant": "昨日までにやるんじゃなかったのか？",
                    "stringId": 1817175262522908672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "すぐに送ります… 遅れて申し訳ありません",
                    "maleVariant": null,
                    "stringId": 1817181016671789056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジェンキンスさんがお待ちです。オフィスへどうぞ",
                    "maleVariant": null,
                    "stringId": 1834140772287401988
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "すみません… 遅くなりました",
                    "maleVariant": null,
                    "stringId": 1835745587940487168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "メールで送っといて。言い訳は後で聞く",
                    "maleVariant": "メールで送っといてくれ。言い訳は後で聞く",
                    "stringId": 1835745941453205504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "死人も問題だが、ライセンスを失うわけにはいかない",
                    "maleVariant": null,
                    "stringId": 1857805434684895232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "危機管理委員会を組織し、招集をかけた",
                    "maleVariant": null,
                    "stringId": 1861902402077085696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "戦略は決まったの？",
                    "maleVariant": null,
                    "stringId": 1861903860436586496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "連中は我々を疑っているが、確信は持っていない",
                    "maleVariant": null,
                    "stringId": 1861903914123677696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "フランクフルトの次は採決…",
                    "maleVariant": null,
                    "stringId": 1861907195008929792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何も出ないわよ",
                    "maleVariant": null,
                    "stringId": 1861913346727497728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいえ、そんなことは言ってない",
                    "maleVariant": null,
                    "stringId": 1861914456053796864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そう、ノーコメントよ。何度言えばわかるの？",
                    "maleVariant": null,
                    "stringId": 1861914510411976704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "警戒するようみんなに伝えて",
                    "maleVariant": null,
                    "stringId": 1861916238851100672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺たちに探りを入れる気だな",
                    "maleVariant": null,
                    "stringId": 1861918272266457088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "月面基地を失うのは大きいもの。私たちを許すはずがない",
                    "maleVariant": null,
                    "stringId": 1861918318722568192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "採決の報告を見たか？ 酷いことになったな",
                    "maleVariant": null,
                    "stringId": 1862042477469388800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "公になったら私たちはおしまいよ？",
                    "maleVariant": null,
                    "stringId": 1862044386750124032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その心配はしなくていいだろう",
                    "maleVariant": null,
                    "stringId": 1862044428777050112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "真相を知る残りの数人も… じきに“消える”",
                    "maleVariant": null,
                    "stringId": 1862044521051738112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そこで偽の情報を流すんだ。すでに防諜部が動いている",
                    "maleVariant": null,
                    "stringId": 1878110854330642432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はあ… 長い一日になりそうです",
                    "maleVariant": null,
                    "stringId": 1878112051787657216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[座る]",
                    "maleVariant": null,
                    "stringId": 1953216036906049540
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[引き出しを開ける]",
                    "maleVariant": null,
                    "stringId": 1953224146022408192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[立ち上がる]",
                    "maleVariant": null,
                    "stringId": 1953243953304281088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[知覚ブースターを使う]",
                    "maleVariant": null,
                    "stringId": 1953265138750562304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[引き出しを閉める]",
                    "maleVariant": null,
                    "stringId": 1953267341833904128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[カードを取る]",
                    "maleVariant": null,
                    "stringId": 2031710488615821312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[使う]",
                    "maleVariant": null,
                    "stringId": 2301408378460590084
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}