  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早く入って。ドアを開けておきたくないの",
              "maleVariant": "",
              "stringId": "1790844402573406208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[データバンクを渡す] ここにある",
              "maleVariant": "",
              "stringId": "1790849247195852804"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はい。無傷だから心配しないで",
              "maleVariant": "ほら。無傷だから心配するな",
              "stringId": "1790849768749166592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "中を見たりはしてないでしょうね？",
              "maleVariant": "",
              "stringId": "1790851191658409984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさか",
              "maleVariant": "",
              "stringId": "1790894573025914884"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見た",
              "maleVariant": "",
              "stringId": "1790895377275318272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わざわざ評判を落とすような真似すると思う？",
              "maleVariant": "わざわざ評判を落とすような真似すると思うか？",
              "stringId": "1790899020917534720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だとすると不思議ね。少し前にデータバンクのGPS信号が突然オンラインになったんだけど",
              "maleVariant": "",
              "stringId": "1790899634141556736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしとは限らない",
              "maleVariant": "俺とは限らない",
              "stringId": "1790900363262586884"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。認めるよ",
              "maleVariant": "",
              "stringId": "1790904996961873920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあね。あたしが行く前に、スカベンジャーが中を見たんじゃない？",
              "maleVariant": "さあな。俺が行く前に、スカベンジャーが中を見たんじゃないか？",
              "stringId": "1790905204512813056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はいこれ。もう行って",
              "maleVariant": "",
              "stringId": "1790909271813287936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、白状する。中身を見た… すべて知ってる",
              "maleVariant": "",
              "stringId": "1790912791824199680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だと思った。で、どうするつもり？",
              "maleVariant": "",
              "stringId": "1790913172465676288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたを応援する",
              "maleVariant": "",
              "stringId": "1790913447863676932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "脅迫するに決まってる",
              "maleVariant": "",
              "stringId": "1790944644694175744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何もしない",
              "maleVariant": "",
              "stringId": "1790944867227168768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたを応援するよ。あのナイト・コープをハックするなんて、凄すぎる",
              "maleVariant": "",
              "stringId": "1790945073234604032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それは… ええと… どうも。ほら、奴らが計画を実行したら…",
              "maleVariant": "",
              "stringId": "1790948734845218816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんなデーモンを使ったの？",
              "maleVariant": "どんなデーモンを使ったんだ？",
              "stringId": "1790948969206149124"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中はどうしてあんなことができるの？",
              "maleVariant": "連中はどうしてあんなことができるんだ？",
              "stringId": "1790949928846127104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイト・コープ相手じゃ、ブラックアウトやファイアシャフトは通用しないはず",
              "maleVariant": "",
              "stringId": "1790955523108139008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おかげで助かったわ。受け取って。同じランナーのよしみで色をつけといた",
              "maleVariant": "",
              "stringId": "1790956614516371456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう。じゃあまた",
              "maleVariant": "",
              "stringId": "1790957676195377152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AIで人の意識を操る… 恐ろしい時代ね。どうしてそんなことが可能なの？",
              "maleVariant": "AIで人の意識を操る… 恐ろしい時代だ。どうしてそんなことができる？",
              "stringId": "1790960957181292544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "受け取って。約束の報酬と… ボーナスも。じゃ、これからもよろしくね",
              "maleVariant": "",
              "stringId": "1790961612046364672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう。幸運を祈ってる",
              "maleVariant": "",
              "stringId": "1790962008240320512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ネットランナーにしては、いい家に住んでるね。一体何をしたらそんな金が手に入るんだか…",
              "maleVariant": "ネットランナーにしては、いい家に住んでるな。一体何をしたらそんな金が手に入るんだか…",
              "stringId": "1790966202762424320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら… 持ってって！",
              "maleVariant": "",
              "stringId": "1790969190348386304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何もしない。暇な時に何をしようとあんたの勝手。だってここは、自由の国でしょ？",
              "maleVariant": "何もしない。暇な時に何をしようとあんたの勝手だ。だってここは、自由の国だろ？",
              "stringId": "1790972383321026560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも、無関心でいてくれるってことは、敵じゃない。これを。もう行って",
              "maleVariant": "",
              "stringId": "1790972797835702272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう。じゃあまた",
              "maleVariant": "",
              "stringId": "1790973247448313856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはプロよ。そんな真似はしない",
              "maleVariant": "俺はプロだ。そんな真似はしない",
              "stringId": "1790979428258574336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう、よかった。じゃ、これを。プロの働きに",
              "maleVariant": "",
              "stringId": "1790979845373718528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ノックする]",
              "maleVariant": "",
              "stringId": "1803778778625896452"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サンドラ？ Vよ。データバンクを持ってきた",
              "maleVariant": "サンドラ？ Vだ。データバンクを持ってきた",
              "stringId": "1803779404986478592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなた一人？ 誰かにつけられてない？",
              "maleVariant": "",
              "stringId": "1803780071243280384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見ればわかるでしょ",
              "maleVariant": "見ればわかるだろ",
              "stringId": "1831389832043806720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サンドラ！ いないの？",
              "maleVariant": "サンドラ！ いないのか？",
              "stringId": "1831396410188873728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだスカベンジャーを警戒してるの？ その… 知り合いが記憶は残らないって言ってたけど",
              "maleVariant": "まだスカベンジャーを警戒してるのか？ その… 知り合いが記憶は残らないって言ってたが",
              "stringId": "1831426402163605504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スカベンジャー？ いえ、今心配してるのは… ううん、気にしないで",
              "maleVariant": "",
              "stringId": "1831426501736382464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "データバンクは持ってきた？",
              "maleVariant": "",
              "stringId": "1831426637598277632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこらじゅうの誰もが連中の手先になり得る。あたしがつけられてないか心配してたのね",
              "maleVariant": "そこらじゅうの誰もが連中の手先になり得る。俺がつけられてないか心配してたわけか",
              "stringId": "1831558849912094720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのとおりよ",
              "maleVariant": "",
              "stringId": "1831559480349540352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう。じゃあまた",
              "maleVariant": "",
              "stringId": "1831606333157916672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "説明するのは難しい。最終的にはどんな相手も操れるようになるはずよ。私が何より恐れてるのはそれ",
              "maleVariant": "",
              "stringId": "1831608594995068928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "途方もない話だけど、あんたみたいなネットランナーが調査してるとわかって安心した",
              "maleVariant": "途方もない話だが、あんたみたいなネットランナーが調査してるとわかって安心した",
              "stringId": "1831608750452752384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、必ずやり遂げてみせる",
              "maleVariant": "",
              "stringId": "1831609122739175424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん、連中はまさにそう思わせたいのよ。まだまだわかってないようね",
              "maleVariant": "",
              "stringId": "1831646878504828928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "データベースに侵入した奴がいるってナイト・コープが知ったら、きっと大騒ぎになる…",
              "maleVariant": "",
              "stringId": "1831662215866597376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん、思ったとおりだわ。あなたもスカベンジャーと変わらないクズね",
              "maleVariant": "",
              "stringId": "1831662318408941568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんな大げさに言わないで。それで、口止め料にいくらくれる？",
              "maleVariant": "そんな大げさに言うなよ。それで、口止め料にいくらくれるんだ？",
              "stringId": "1831662502706659328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまり、誰かが起動して使った――それしか考えられない",
              "maleVariant": "",
              "stringId": "1831671809967898624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ、そう。スカベンジャーがね…",
              "maleVariant": "",
              "stringId": "1831676648919646208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待ってよ、この額じゃ約束と…",
              "maleVariant": "待てよ、この額じゃ約束と…",
              "stringId": "1831680827436285952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう？ あなたが来る前に、スカベンジャーが私の銀行口座をいじったのかもね",
              "maleVariant": "",
              "stringId": "1831680947527598080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話は終わりよ。ほら行って",
              "maleVariant": "",
              "stringId": "1831681738338451456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "相当な凶暴なデーモンを用意したんでしょ？ インプクラス？",
              "maleVariant": "相当な凶暴なデーモンを用意したんだろ？ インプクラスか？",
              "stringId": "1831728901743427584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リバイアサンよ。企業のあの手のICEを突破したいなら、ケルベロスでも不十分",
              "maleVariant": "",
              "stringId": "1831728989656039424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AGIを使って従業員の意識を操るなんて… 悪の所業もいいところね",
              "maleVariant": "AGIを使って従業員の意識を操るなんて… 悪の所業もいいところだな",
              "stringId": "1831729419689639936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "従業員どころか誰でも操れる。そこが恐ろしいのよ",
              "maleVariant": "",
              "stringId": "1831729521275682816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何を心配してるの？",
              "maleVariant": "何を心配してるんだ？",
              "stringId": "1833047359294783488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、いったい何を心配してるの？",
              "maleVariant": "じゃあ、いったい何を心配してるんだ？",
              "stringId": "1833048949607088128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたを雇ったのは、あれこれ詮索してほしいからじゃない",
              "maleVariant": "",
              "stringId": "1833077473474756608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこまで言われたら気になるでしょ。命の恩人に冷たいんじゃない？",
              "maleVariant": "そこまで言われたら気になるだろ。命の恩人に冷たいんじゃないか？",
              "stringId": "1833077618463457280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "報酬はもう支払い済みでしょ。あの一件だって、あなたには数ある仕事の一つなだけのはず",
              "maleVariant": "",
              "stringId": "1833077884868870144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで、データバンクは持ってきたの？",
              "maleVariant": "",
              "stringId": "1833078184459616256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私に心配するなって言ってくる人は、大抵何かを隠してる",
              "maleVariant": "",
              "stringId": "1833085298267635712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "データをのぞき見しなかった分、ボーナスもついてる",
              "maleVariant": "",
              "stringId": "1833125601888419840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "戦ってるのは、私独りじゃないみたいね",
              "maleVariant": "",
              "stringId": "1833130572809748480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "口止め料はいくら貰える？",
              "maleVariant": "",
              "stringId": "1915326530364096516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "冗談よ",
              "maleVariant": "冗談だ",
              "stringId": "1915326726892404736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着いて。ちょっとからかっただけよ。本気でそんなことするわけないじゃない",
              "maleVariant": "落ち着けって。ちょっとからかっただけだ。本気でそんなことするわけないだろ",
              "stringId": "1915326884380131328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "笑えるとでも思ったの？",
              "maleVariant": "",
              "stringId": "1915343327813419008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "報酬を渡すから… もう帰って",
              "maleVariant": "",
              "stringId": "1915343836750266368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいから出てって！",
              "maleVariant": "",
              "stringId": "1915344768372293632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "趣味がいいね",
              "maleVariant": "趣味がいいな",
              "stringId": "1929938222144712708"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "素敵な場所ね。趣味がいい",
              "maleVariant": "素敵な場所だな。趣味がいい",
              "stringId": "1929938914154541056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう",
              "maleVariant": "",
              "stringId": "1929939451797204992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着く場所ね",
              "maleVariant": "落ち着く場所だな",
              "stringId": "1929943421487837184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "素敵な所ね",
              "maleVariant": "素敵な所だな",
              "stringId": "1929943441368838144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ノックする] サンドラ？",
              "maleVariant": "",
              "stringId": "2088033186664976384"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}