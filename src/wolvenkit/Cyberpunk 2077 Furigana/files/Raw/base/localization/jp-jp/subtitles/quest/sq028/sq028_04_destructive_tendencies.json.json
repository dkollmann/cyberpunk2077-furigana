      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて… いっちょ自由の風を吹かせてやろうじゃねえか！",
              "maleVariant": "",
              "stringId": "1367827735816179712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] 任せて！",
              "maleVariant": "[立ち上がる] 任せろ！",
              "stringId": "1367829585420357632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "う… ああ！",
              "maleVariant": "",
              "stringId": "1367830994186407936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カオスと破壊？ 最高じゃない",
              "maleVariant": "カオスと破壊？ 最高だな",
              "stringId": "1367831724666392576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "派手にやれよ",
              "maleVariant": "",
              "stringId": "1367835283969544192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] 頭おかしいんじゃないの？",
              "maleVariant": "[立ち上がる] 頭おかしいんじゃないか？",
              "stringId": "1379149329518424064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、ケリー！ 高い船なんじゃないの？",
              "maleVariant": "おいおい、ケリー！ 高い船なんじゃないのか？",
              "stringId": "1379157566544277504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ誰のなの？",
              "maleVariant": "じゃあ誰のなんだ？",
              "stringId": "1379160931248500736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分の船をメチャクチャにして後悔しないの？",
              "maleVariant": "自分の船をメチャクチャにして後悔しないのか？",
              "stringId": "1379163397146931200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「自分の船」だ？ 俺が自分の船を“シームライ”なんて名付けると思ったか？",
              "maleVariant": "",
              "stringId": "1379163922005356544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "鉛頭のゴミクズ野郎、L.B.コヴァチェクさ",
              "maleVariant": "",
              "stringId": "1379170235724722176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コヴァチェクって、あのコヴァチェク？",
              "maleVariant": "コヴァチェクって、あのコヴァチェクか？",
              "stringId": "1379170708439560192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、そうだ。〈Us Cracks〉の件じゃ、まだしっかり“お礼”をできてなかったうえ… 他にも色々あってな",
              "maleVariant": "",
              "stringId": "1379170874248785920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、お前も何かぶっ壊せよ。全部L.B.のおごりだ",
              "maleVariant": "",
              "stringId": "1379176069649264640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だからやってんだよ",
              "maleVariant": "",
              "stringId": "1379182432005779456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺のだと思ってんのか？",
              "maleVariant": "",
              "stringId": "1379183201056583680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メチャクチャにしてあげましょ",
              "maleVariant": "メチャクチャにしてやろう",
              "stringId": "1384828260552667136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは遠慮しとく",
              "maleVariant": "俺は遠慮しとく",
              "stringId": "1384828852855500800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃ、メチャクチャにしてやりましょ！",
              "maleVariant": "よし、メチャクチャにしてやるか！",
              "stringId": "1384829201502826496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何も変わってないのね",
              "maleVariant": "何も変わってないんだな",
              "stringId": "1384829292049461248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしたってんだよ？ ほら！ 一緒にひと暴れしようぜ！",
              "maleVariant": "",
              "stringId": "1384830030012084224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは遠慮しとく",
              "maleVariant": "俺は遠慮しとく",
              "stringId": "1384830905011007488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんだよ、頭の固い奴だな…",
              "maleVariant": "",
              "stringId": "1384831594353258496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいぞ、ノッてきたな！ パーティーの始まりだ！",
              "maleVariant": "",
              "stringId": "1384833167670243328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボケてきたのか、ようやく真実に気づいたのか。どっちだろうな",
              "maleVariant": "",
              "stringId": "1701169036850786304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何いい子ぶってんだよ。ケリーにはこれが必要なんだ。そして、お前にもな",
              "maleVariant": "",
              "stringId": "1742898070961680384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。あくまでも、あんたのためだからね",
              "maleVariant": "わかった。あくまでも、お前のためだからな",
              "stringId": "1742899862046928900"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V！ ボケっと突っ立ってないで、お前も手伝えよ",
              "maleVariant": "",
              "stringId": "1837005658783346688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺にはわかる",
              "maleVariant": "",
              "stringId": "1837011808387985408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お断りよ",
              "maleVariant": "お断りだ",
              "stringId": "1955981972625059844"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "断る。生憎、あたしはまだイカレてない",
              "maleVariant": "断る。生憎、俺はまだイカレてない",
              "stringId": "1955983155150352384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[絵画を破壊する]",
              "maleVariant": "",
              "stringId": "2437221845978243076"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[コーヒーメーカーを破壊する]",
              "maleVariant": "",
              "stringId": "2437222681970139136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[レコードプレーヤーを破壊する]",
              "maleVariant": "",
              "stringId": "2437222757366947840"
            }
          ]
        }
      }
