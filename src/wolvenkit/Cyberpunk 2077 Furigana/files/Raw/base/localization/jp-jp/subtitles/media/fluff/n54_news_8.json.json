            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "N54ニュースのジリアン・ジョーダンです。予定を変更し、マイヤーズ大統領の特別メッセージを放送します",
                    "maleVariant": null,
                    "stringId": 1751875055675564032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "昨今、激しさを増すアラサカ社の攻撃的発言により、現政権に大きな不安が広がっています",
                    "maleVariant": null,
                    "stringId": 1751888072815468544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サブロウ・アラサカ氏の死にミリテク社や新合衆国が関与したという批判は、事実無根であるだけでなく、無責任と言わざるを得ません",
                    "maleVariant": null,
                    "stringId": 1751888171801042944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミリテク社と新合衆国政府はこれまで一貫してアーヴィン協定を尊重し、条項に則って適切に行動してきました",
                    "maleVariant": null,
                    "stringId": 1751888295382016000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ですがこうした挑発には、全力で応える用意があることを、明確にお伝えしておきたいと思います",
                    "maleVariant": null,
                    "stringId": 1751888401363689472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "統一戦争で、我々は一つの国家として、サブロウ・アラサカも全能ではないという真実を本人に突きつけました。必要とあれば、その御令息に喜んで同じ教訓を与えてやりましょう",
                    "maleVariant": null,
                    "stringId": 1751888503704707072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マイヤーズ大統領の毅然とした態度が、軽率なアラサカ社の発言を牽制してくれるよう祈るばかりです",
                    "maleVariant": null,
                    "stringId": 1751888621698867200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここからは通常どおりの番組をお届けします",
                    "maleVariant": null,
                    "stringId": 1751888804570521600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "以上です",
                    "maleVariant": null,
                    "stringId": 1812537319890862080
                  }
                }
              ]
            }
          }
        }
      }
    },
