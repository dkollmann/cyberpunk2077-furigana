    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティにも1ついい所があった",
              "maleVariant": "",
              "stringId": 1705580134522474496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何？",
              "maleVariant": "何だ？",
              "stringId": 1705580245285654528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "食べ物だよ。ヘイウッド7番街のタイ料理に、埠頭のピエロギ。ん～…",
              "maleVariant": "",
              "stringId": 1705580412252508160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "旅に出てしばらくしたら、きっと身に沁みてわかる",
              "maleVariant": "",
              "stringId": 1705580522579480576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "車のエアコン越しでもオゾンのにおいがする",
              "maleVariant": "",
              "stringId": 1711479453297463300
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "冗談でしょ？",
              "maleVariant": "冗談だろ？",
              "stringId": 1751619255757754368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナム！ 嵐も来てるってのに、何ちんたらしてんだ。ナイトシティから逃げ出す前にババアになっちまうぞ",
              "maleVariant": "",
              "stringId": 1772146047314714624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "余計なお世話だよ",
              "maleVariant": "",
              "stringId": 1772147966326235136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やあ、ミッチ！",
              "maleVariant": "",
              "stringId": 1772148586798014464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よう、V",
              "maleVariant": "",
              "stringId": 1772149245286965248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "タワー襲撃の直後はそんな強気じゃなかったくせに。みんな心配したんだから",
              "maleVariant": "",
              "stringId": 1772157049141878784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも、おかげでみんなが助かった",
              "maleVariant": "だが、おかげでみんなが助かった",
              "stringId": 1772157672247681024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仲間はもう向こう側に渡って、私たちを待ってる",
              "maleVariant": "",
              "stringId": 1824301261267791872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈神輿〉からのお土産は？",
              "maleVariant": "",
              "stringId": 1824321878922752000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんなテックを持ってゲートに近づいたら、問答無用で始末されるよ。アラサカは全ての検問所に人員を配備してる",
              "maleVariant": "",
              "stringId": 1824321968865406976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またアラサカ相手に盗みを働くとはね",
              "maleVariant": "またアラサカ相手に盗みを働くとはな",
              "stringId": 1824328804037423104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしとジャッキーみたいにならないといいけど",
              "maleVariant": "俺とジャッキーみたいにならないといいがな",
              "stringId": 1824329865313775616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よく考えたら、またアラサカ相手に盗みを働くってわけね",
              "maleVariant": "よく考えたら、またアラサカ相手に盗みを働くってわけか",
              "stringId": 1824331668780609536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんか、すっごいデジャヴ…",
              "maleVariant": "なんか、すっげえデジャヴ…",
              "stringId": 1824331776708440064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心配ないよ。今回は奴らの鼻先をすり抜けてやるから",
              "maleVariant": "",
              "stringId": 1824340599074582528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう願うよ。新しい門出も大変ね",
              "maleVariant": "そう願うよ。新しい門出も大変だな",
              "stringId": 1824342536641703936
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあね",
              "maleVariant": "",
              "stringId": 1824343073630056448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうこと？",
              "maleVariant": "",
              "stringId": 1824363324853841920
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジャッキーや、あの時の仕事を思い出したの。結果はパナムも知ってのとおりよ",
              "maleVariant": "ジャッキーや、あの時の仕事を思い出した。結果はパナムも知ってのとおりだ",
              "stringId": 1824363443233878016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ごめんね",
              "maleVariant": "",
              "stringId": 1824364282799321088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "違うの、別に責めてるわけじゃないよ。それに今回は、あの時と同じようにはならない。絶対にね",
              "maleVariant": "違うんだ、別に責めてるわけじゃないさ。それに今回は、あの時と同じようにはならない。絶対にな",
              "stringId": 1824364524642889728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "楽しみね",
              "maleVariant": "楽しみだ",
              "stringId": 1825387790669049856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしもこの街が恋しくなるのかな",
              "maleVariant": "俺もこの街が恋しくなるのかな",
              "stringId": 1825387884470464512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "恋しくなりそうなものはすべて、一緒に旅をすることになる",
              "maleVariant": "",
              "stringId": 1825388244341747712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ホントに？ 楽しみにしとく",
              "maleVariant": "ホントか？ 楽しみにしとくよ",
              "stringId": 1825393803522600960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "街が恋しくなると思う？",
              "maleVariant": "街が恋しくなると思うか？",
              "stringId": 1825393891233886208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしが恋しくなりそうなものは、一緒に旅することになる",
              "maleVariant": "俺が恋しくなりそうなものは、一緒に旅することになるがな",
              "stringId": 1825393992467607552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新生活はとんでもない波乱万丈になるよ",
              "maleVariant": "",
              "stringId": 1825409456715460608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それだけは間違いない",
              "maleVariant": "",
              "stringId": 1825411363160846336
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかるの。何が起きたって、きっと楽しめるはず。パナムもそう思うでしょ？",
              "maleVariant": "わかるんだ。何が起きたって、きっと楽しめるはず。パナムもそう思うだろ？",
              "stringId": 1825411486624378880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "思い出で美化された幻が恋しくなる",
              "maleVariant": "",
              "stringId": 1825413107320221696
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無限の可能性と夢… そして無限の嘘に満ちた街…",
              "maleVariant": "",
              "stringId": 1825414171264151552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何でも二人で一緒に向き合えばいいのよ。この街の思い出にもね",
              "maleVariant": "何でも二人で一緒に向き合えばいいんだ。この街の思い出にもな",
              "stringId": 1825415870393479168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、急に歯が浮くようなこと言わないで",
              "maleVariant": "",
              "stringId": 1825423199553843200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも効いたでしょ？ ほら、認めなよ",
              "maleVariant": "でも効いただろ？ ほら、認めろよ",
              "stringId": 1825424023499698176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね。かなり効いた",
              "maleVariant": "",
              "stringId": 1825427662008086528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これからは私と一緒よ。頭の中のそれのことも、二人で考えればいい",
              "maleVariant": "",
              "stringId": 1825427900278108160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだね",
              "maleVariant": "そうだな",
              "stringId": 1825429277737873408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "例の嵐が近づいてるのね",
              "maleVariant": "例の嵐が近づいてるんだな",
              "stringId": 1825438182715027456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また一緒に走れて嬉しいよ。体は？",
              "maleVariant": "",
              "stringId": 1825442253169618944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あと1ヶ月もすりゃ、ばっちり全快だ。大丈夫だって言ったろ？",
              "maleVariant": "",
              "stringId": 1825442517813424128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソウルがいないのが残念。テディーに… ボブ… スコーピオンも…",
              "maleVariant": "",
              "stringId": 1825459157825351680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、そうだな、パナム…",
              "maleVariant": "",
              "stringId": 1825464477024907264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こうして旅立てるのもミッチのおかげよ。成功させよう",
              "maleVariant": "こうして旅立てるのもミッチのおかげだ。成功させよう",
              "stringId": 1825466681316208640
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一緒に旅をするのが楽しみだ。じゃあ後でな",
              "maleVariant": "",
              "stringId": 1825467284742336512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハロー、ナイトシティ！ ヨリノブ・アラサカは、犠牲者に妹のハナコ・アラサカが含まれていたと発表した",
              "maleVariant": "",
              "stringId": 1831505171294609412
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すなわち、あの巨大企業、アラサカ社の上層部に変革が訪れようとしている！ もちろん、大量の血と涙が流される事態になるに違いない",
              "maleVariant": "",
              "stringId": 1863167211358253056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんな話もうウンザリ",
              "maleVariant": "",
              "stringId": 1863167767975948288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんながいたら、無茶やったあんたをボコボコにして、それから吐きそうになるまでハグしただろうね",
              "maleVariant": "",
              "stringId": 2005306322025312256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミッチのヤツ、何週間も寝たきりでさ。戦争でパンツァーを乗り回してシナプス鍛えてなかったら、脳みそ丸焦げになってたよ",
              "maleVariant": "",
              "stringId": 2005309974173577216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも私たちには別のルートがある",
              "maleVariant": "",
              "stringId": 2005313470813827072
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}