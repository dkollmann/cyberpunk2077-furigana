  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、俺の後ろにつけ",
              "maleVariant": "",
              "stringId": 1437364982026092544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナム… システムに異常がないか確認してくれ",
              "maleVariant": "",
              "stringId": 1440164848523558912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、ちゃんと見ろ！ 俺を殺す気か！",
              "maleVariant": "",
              "stringId": 1440298500741939200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、ゲートを突破して",
              "maleVariant": "",
              "stringId": 1441836958027366400
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボブ、出番だよ。ジャマーを忘れないで",
              "maleVariant": "",
              "stringId": 1699443458189316096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろんだ。任せろ",
              "maleVariant": "",
              "stringId": 1699444445494923264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "健闘を祈る",
              "maleVariant": "",
              "stringId": 1699446839922413568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今ので全部だな",
              "maleVariant": "",
              "stringId": 1700955106624425984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "谷の入口についたら、お前が先導しろ",
              "maleVariant": "",
              "stringId": 1796741969367322624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": 1796747936570068992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ニトロ噴射器。ランチャーも異常なし。問題ないよ",
              "maleVariant": "",
              "stringId": 1796748000591925248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあショーを始めるか",
              "maleVariant": "",
              "stringId": 1796748152274735104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ご苦労様",
              "maleVariant": "",
              "stringId": 1800812060849524736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "キャシディ、あんたなら覚えてるんじゃないか？ アルデカルドスが昔LAでやったデカいヤマ。例の地震の後の…",
              "maleVariant": "",
              "stringId": 1803872924574961664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何かひいた気がするな…",
              "maleVariant": "",
              "stringId": 1803878625087082500
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんて静かなんだろう…",
              "maleVariant": "",
              "stringId": 1803878664547094532
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おいおい、俺はそこまで年じゃない。だがその話なら知ってる",
              "maleVariant": "",
              "stringId": 1803881525112754176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっと気のせいだろ",
              "maleVariant": "",
              "stringId": 1803881913673076736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "星に飾られた暗闇と張り合えるのは、このだだっ広い砂漠だけだな",
              "maleVariant": "",
              "stringId": 1803882746275975168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "テディー。もしかしたらボブはまだ生きてるかも…",
              "maleVariant": "",
              "stringId": 1805185607592103936
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや… すまない、パナム",
              "maleVariant": "",
              "stringId": 1805187566936051712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "進もう。目標はすぐそこだ",
              "maleVariant": "",
              "stringId": 1805187655788187648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確か… あれもリニアトンネルの仕事じゃなかったか？",
              "maleVariant": "",
              "stringId": 1809603136024530944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "地上組は岩の運び出しをして、地下組は下で大陸横断線のトンネルを掘ってたって",
              "maleVariant": "",
              "stringId": 1809603304316784640
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "テディー。たとえ俺たちの過去が血で刻まれていようが、今からやろうとしてることには関係ない",
              "maleVariant": "",
              "stringId": 1809603384293773312
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここから北に、すごい景色が見える場所がある",
              "maleVariant": "",
              "stringId": 1809604237381652480
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、あの岩の柱の… なんていうんだっけ？",
              "maleVariant": "",
              "stringId": 1809604561383247872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ピナクルだ",
              "maleVariant": "",
              "stringId": 1809604668790984704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうそう。キレイよね",
              "maleVariant": "",
              "stringId": 1809604773178822656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、多分プレーリードッグだ…",
              "maleVariant": "",
              "stringId": 1809605415813304320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私たちも同じ目に遭わないといいけどね",
              "maleVariant": "",
              "stringId": 1809605497568677888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ポンコツ車に轢かれるほどマヌケじゃない",
              "maleVariant": "",
              "stringId": 1819718904238100480
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "テディー、お前って奴は… 慰められたと同時に、ムカついたよ",
              "maleVariant": "",
              "stringId": 1819718978175291392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その建設現場にラフェンの集団が襲い掛かってきて、クランの半分が殺されたって話だが、そう考えると…",
              "maleVariant": "",
              "stringId": 1887819298817830912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドローンのお出ましだ！",
              "maleVariant": "",
              "stringId": 1895209428743639044
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゴツいのも混じってやがる… 排除しろ！",
              "maleVariant": "",
              "stringId": 1895209428760416256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パンツァーの進行方向にさらに多数のドローンを確認",
              "maleVariant": "",
              "stringId": 1895291506323640320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業が警備用によく使ってるヤツだ、大したことはない",
              "maleVariant": "",
              "stringId": 1895294451480023040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ！",
              "maleVariant": "",
              "stringId": 1895300302953533440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "直せるか？ 時間がないぞ！",
              "maleVariant": "",
              "stringId": 1895302460117635072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わからない！ ちょっと待って！",
              "maleVariant": "",
              "stringId": 1895302915031846912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "駆動系がやられてる。色々積み過ぎて、耐えきれなかったんだ！",
              "maleVariant": "",
              "stringId": 1895310765812633600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし！ やった！",
              "maleVariant": "",
              "stringId": 1895313271707947008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全システム起動。いけるよ！",
              "maleVariant": "",
              "stringId": 1895313273989648384
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "グループチャンネルを開いた。みんな聞こえてるか？",
              "maleVariant": "",
              "stringId": 1896666917909581828
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バッドランズからライブでお届け、ホストはおなじみ…",
              "maleVariant": "",
              "stringId": 1896666917959913472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "テディー",
              "maleVariant": "",
              "stringId": 1896666918010245120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はいはい、わかったよ",
              "maleVariant": "",
              "stringId": 1896666918043799552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドローンは起動した？",
              "maleVariant": "",
              "stringId": 1896666918077353984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、ばっちりだ",
              "maleVariant": "",
              "stringId": 1896666918127685632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこから操作できるの？",
              "maleVariant": "",
              "stringId": 1896666918178017280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞くまでもないだろ",
              "maleVariant": "",
              "stringId": 1896666918228348928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゲートは破壊した。行くよ！",
              "maleVariant": "",
              "stringId": 1898074142653829120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダメだ！ 回路がやられてる！",
              "maleVariant": "",
              "stringId": 1912474283299958784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ、主電源もダメだ――全システムオフライン！",
              "maleVariant": "",
              "stringId": 1912481578083807232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何とかするしかないか… V、ドローンを片付けて！",
              "maleVariant": "",
              "stringId": 1996914077078409216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ、やばい！ 操縦席から火が！ 電力サージか！",
              "maleVariant": "",
              "stringId": 1998087566883835904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナム、状況は？ まだかかりそうか？",
              "maleVariant": "",
              "stringId": 1998087861240090628
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナム、火を消せる？ 無理なら早く出て！",
              "maleVariant": "パナム、火を消せるか？ 無理なら早く出ろ！",
              "stringId": 1998144506993332224
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待ってろ、ボブ。パンツァーに問題が発生した。すぐに追いつく",
              "maleVariant": "",
              "stringId": 1998144731153715200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、みんなどこにいるんだ？ 送信機に着いたぞ。何かあったのか？",
              "maleVariant": "",
              "stringId": 1998152400639905792
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "火は収まった。非常用電源に切り替える。一部システムはアナログになるけど、動けるはず",
              "maleVariant": "",
              "stringId": 1998153374808952832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ソウル、奴らに気づかれた！ ドローンがまっすぐこっちに向かってる！",
              "maleVariant": "",
              "stringId": 1998154476199628800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ動けない！ 食い止められるか？",
              "maleVariant": "",
              "stringId": 1998154939250790400
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やるしかないだろ！ みんな頑張れよ。ここは俺に任せろ！",
              "maleVariant": "",
              "stringId": 1998155242666741760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウソだろ… ボブ！？ ボブ、聞こえるか？ すぐに向か――",
              "maleVariant": "",
              "stringId": 1998155887264157696
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダメだ、ミッチ…",
              "maleVariant": "",
              "stringId": 1998155887264157708
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今は止まるわけにはいかない",
              "maleVariant": "",
              "stringId": 1998155887280934912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソッ！",
              "maleVariant": "",
              "stringId": 1998155887280934924
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっとか！ もう来ないのかと思ってたぞ",
              "maleVariant": "",
              "stringId": 1998166163157962752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいよ。早く出して！",
              "maleVariant": "いいぞ。早く出せ！",
              "stringId": 1998170017303908352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボブ、パンツァーが復活した！ すぐに行くから、踏ん張ってくれ！",
              "maleVariant": "",
              "stringId": 1998170349274681344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボブ！",
              "maleVariant": "",
              "stringId": 1998171257677041664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、乗って！",
              "maleVariant": "",
              "stringId": 1998219550171877376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[出る]ドローンは任せて！",
              "maleVariant": "[出る]ドローンは任せろ！",
              "stringId": 2014580930307182596
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあて、ひと暴れするか",
              "maleVariant": "",
              "stringId": 2024046995192803328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アルデカルドスのコンボイ――なんて壮大な眺めなんだ",
              "maleVariant": "",
              "stringId": 2024048074219450368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。そして星に飾られた暗闇と張り合えるのは、このだだっ広い砂漠だけだ",
              "maleVariant": "",
              "stringId": 2024049867921944576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちは任せて！ パナムはパンツァーを！",
              "maleVariant": "こっちは任せろ！ パナムはパンツァーを！",
              "stringId": 2024063424415789056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっとだけ時間をちょうだい！ 絶対消してやる！ パンツァーを失うわけにいかないんだ！",
              "maleVariant": "",
              "stringId": 2024068220317200384
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドローンは残り1機だ、この俺がすぐに――",
              "maleVariant": "",
              "stringId": 2024084343741870080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミリテクのドローンだ！ 来るぞ！ みんな、気をつけろ！",
              "maleVariant": "",
              "stringId": 2236658344674480128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、早く出て！ ドローンを片付けるんだよ！",
              "maleVariant": "",
              "stringId": 2236671685061005312
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここはミリテク・サイトセキュアによって保護されている区域だ！",
              "maleVariant": "",
              "stringId": 2238047385483497472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "警告はしたぞ！ 引き返さないのであれば、実力行使に移る！",
              "maleVariant": "",
              "stringId": 2238047640966942724
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただちに引き返せ！ 繰り返す、ただちに引き返せ！",
              "maleVariant": "",
              "stringId": 2238057403008176128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しっかり当ててよ！ やられるわけにはいかないんだから！",
              "maleVariant": "",
              "stringId": 2238842833106825216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何とかして、V！ ロックされたらヤバいよ！",
              "maleVariant": "",
              "stringId": 2238843450407710724
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫、あたしに任せて",
              "maleVariant": "大丈夫だ、俺に任せろ",
              "stringId": 2238844086717181952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってるって、パナム！",
              "maleVariant": "",
              "stringId": 2238845897431773184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナム、敵が来た！",
              "maleVariant": "パナム、来やがったぞ！",
              "stringId": 2238847473500221440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちに来るよ、V！",
              "maleVariant": "",
              "stringId": 2238847838035570692
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ挨拶してあげないとね",
              "maleVariant": "じゃあ挨拶してやらないとな",
              "stringId": 2238849159073558528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "橋を横断する。まずは道路にいる連中を排除しないと",
              "maleVariant": "",
              "stringId": 2238850523799416832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "失敗は許されないよ。これ以上の被害を受けるわけにはいかない。絶対に",
              "maleVariant": "",
              "stringId": 2238851998063718400
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やるしかない",
              "maleVariant": "",
              "stringId": 2238853644093157376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V… パンツァーはこれ以上持たないよ。いつシステムが落ちてもおかしくない",
              "maleVariant": "",
              "stringId": 2238856660418490368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫、きっと何とかなる",
              "maleVariant": "大丈夫だ、きっと何とかなる",
              "stringId": 2238857694113099776
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってる！ 準備して！",
              "maleVariant": "",
              "stringId": 2238859979555794944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろん、出来る限りのことはやってみるけど… 万が一のときは覚悟しといて",
              "maleVariant": "",
              "stringId": 2238860394875777024
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}