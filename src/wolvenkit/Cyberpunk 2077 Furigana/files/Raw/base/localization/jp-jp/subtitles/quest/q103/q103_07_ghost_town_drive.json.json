        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "権力を持ってる人間が嫌いなのね",
                    "maleVariant": "権力を持ってる人間が嫌いなんだな",
                    "stringId": 1665024486734950400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "“あいつら”のだ",
                    "maleVariant": null,
                    "stringId": 1665024486751727624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "鉢合わせする前に出発したがってた",
                    "maleVariant": "鉢合わせする前に出発したがってただろ",
                    "stringId": 1665024486751727636
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ソウルの方が… 私に会いたくないはずだよ",
                    "maleVariant": null,
                    "stringId": 1665024486768504832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何があったの？",
                    "maleVariant": "何があったんだ？",
                    "stringId": 1665024486768504844
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私たちは、ソウルがアルデカルドスの再生を約束したからカリフォルニアに来たんだ。一からやり直すために",
                    "maleVariant": null,
                    "stringId": 1665024486768504856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたも見たでしょ？ 結局得たのは数えるほどのテントと乾いた砂の山。とんだ約束の地だよ",
                    "maleVariant": null,
                    "stringId": 1665024486785282048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私は変革を求めてた。でもソウルは何も変えようとしない臆病者だ。あいつがいる限り、クランに未来はない",
                    "maleVariant": null,
                    "stringId": 1665024486785282060
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "権力を持ってる人間が嫌いなのね",
                    "maleVariant": "権力を持ってる人間が嫌いなんだな",
                    "stringId": 1665024486919499776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミッチとスコーピオンに好かれてるのね",
                    "maleVariant": "ミッチとスコーピオンに好かれてるんだな",
                    "stringId": 1665024486919499792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの戦闘用インプラント… スコーピオンとミッチは元軍人？",
                    "maleVariant": "あの戦闘用インプラント… スコーピオンとミッチは元軍人か？",
                    "stringId": 1665024486936276992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミッチとスコーピオンはいい奴らね",
                    "maleVariant": "ミッチとスコーピオンはいい奴らだな",
                    "stringId": 1665024486986608640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの2人は元軍人？",
                    "maleVariant": "あいつらは元軍人か？",
                    "stringId": 1665024487020163072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最高のダチだよ。もし私があいつらの立場にいたら、同じようにキャンプに残ってた。ソウルはさておき、クランが優先だからね",
                    "maleVariant": null,
                    "stringId": 1665040357226311680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたをまだ仲間の一人として扱って、気にかけてた",
                    "maleVariant": null,
                    "stringId": 1665040714664898560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ",
                    "maleVariant": null,
                    "stringId": 1665041112016482304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そういうあんたは？ 誰かいないの？",
                    "maleVariant": null,
                    "stringId": 1665041859609227264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いない",
                    "maleVariant": null,
                    "stringId": 1665046186084134916
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一応いる",
                    "maleVariant": null,
                    "stringId": 1665048480183873536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今はいないけど… もしかしたら",
                    "maleVariant": "今はいないが… もしかしたら",
                    "stringId": 1665048880236589056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "残念ながらいない",
                    "maleVariant": null,
                    "stringId": 1665049777314328576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあ… いるような、いないような",
                    "maleVariant": null,
                    "stringId": 1665049908780593152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今はいないけど、それも時間の問題的な…？",
                    "maleVariant": "今はいないが、それも時間の問題的な…？",
                    "stringId": 1665050128025251840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そう",
                    "maleVariant": null,
                    "stringId": 1665056872818405376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「いるような、いないような」ね",
                    "maleVariant": null,
                    "stringId": 1665059821078573056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はあ？ 意味不明",
                    "maleVariant": null,
                    "stringId": 1665067044190154752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "スコーピオンとミッチ？ うん、元パンツァーボーイズ。前の戦争にも参加してた",
                    "maleVariant": null,
                    "stringId": 1665067958615535616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "てことはあの巨大な輸送機を操縦してたの？",
                    "maleVariant": "てことはあの巨大な輸送機を操縦してたのか？",
                    "stringId": 1665069818135048192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "スコーピオンがね。ミッチは警護担当",
                    "maleVariant": null,
                    "stringId": 1665072036032671744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最高の戦闘用インプラントと、最悪の悪夢を持ち帰ってきた",
                    "maleVariant": null,
                    "stringId": 1665095307742539776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まず… 奴らが何人で来るかわからない。でもおそらく大人数だ",
                    "maleVariant": null,
                    "stringId": 1669106670143926272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ロッキーリッジってのはどんな場所？",
                    "maleVariant": "ロッキーリッジってのはどんな場所だ？",
                    "stringId": 1674852759585288192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ロッキーリッジってどんなとこなの？ 周りに何もない過疎地で、かつていた住民もナイトシティに移るか、とにかく逃げ出したって聞いてるけど",
                    "maleVariant": "ロッキーリッジってどんなとこなんだ？ 周りに何もない過疎地で、かつていた住民もナイトシティに移るか、とにかく逃げ出したって聞いてるが",
                    "stringId": 1674854428297523200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ならつけ足すことはほとんどない",
                    "maleVariant": null,
                    "stringId": 1674857798924103680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ホントは州をまたぐハイウェイが開通されるはずだったんだ。新しい住民が来ることを期待してね。もちろん、実現しなかったけど",
                    "maleVariant": null,
                    "stringId": 1674858227716190208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それと、私の車を奪い返すには、連中の隙を突く必要がある",
                    "maleVariant": null,
                    "stringId": 1690917133286502400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "光のショーで注意を引くってわけね",
                    "maleVariant": "光のショーで注意を引くってわけか",
                    "stringId": 1690920307217002496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こういうゴーストタウンはいくつくらいあるの？",
                    "maleVariant": "こういうゴーストタウンは、いくつくらいあるんだ？",
                    "stringId": 1692396498012729344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アメリカ全土で？ 数千だよ",
                    "maleVariant": null,
                    "stringId": 1692397343567638528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こないだI-80号線を走ってたんだけど… 日が暮れて辺りが静かになってから車を停めるとさ… 聞こえてくるんだ",
                    "maleVariant": null,
                    "stringId": 1692401451468976128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何が？",
                    "maleVariant": "何がだ？",
                    "stringId": 1692402776164397056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "板で塞がれた窓を風が吹き抜ける音… 丸まった枯草が渇いた砂地を転がる音が",
                    "maleVariant": null,
                    "stringId": 1692402879595933696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はあ？",
                    "maleVariant": null,
                    "stringId": 1762070150203858944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ローグにソウル。ソウルはあんたたちのリーダー？",
                    "maleVariant": "ローグにソウル。ソウルはあんたたちのリーダーか？",
                    "stringId": 1762074234969415680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "町はそうやって死ぬんだ。派手な音をたてることもなく、すすり泣きながら",
                    "maleVariant": null,
                    "stringId": 1762078447795462144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "古き良き陽動作戦ってわけね",
                    "maleVariant": "古き良き陽動作戦ってわけか",
                    "stringId": 1773290657992536064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本気？ 交差点の照明をつけるだけ？ それでうまくいくとは思えない",
                    "maleVariant": "本気か？ 交差点の照明をつけるだけ？ それでうまくいくとは思えない",
                    "stringId": 1773290738875494400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そういうこと",
                    "maleVariant": null,
                    "stringId": 1773296090975170560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そう。さびれたこの町を活気づけてやるんだよ",
                    "maleVariant": null,
                    "stringId": 1773299625179607040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "了解。電源のほうは任せる。あたしはアクセスポイントを探すよ",
                    "maleVariant": "了解。電源のほうは任せる。俺はアクセスポイントを探すよ",
                    "stringId": 1786411909749796864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "考えがある。奴らが交差路に来たら、少し遊んでやるつもり",
                    "maleVariant": "考えがある。奴らが交差路に来たら、少し遊んでやるつもりだ",
                    "stringId": 1786414761540972544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "任せたよ",
                    "maleVariant": null,
                    "stringId": 1786415429794263040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "了解。連中はきっと、大勢で来るはず",
                    "maleVariant": null,
                    "stringId": 1786416388528271360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うまく出し抜いて、私の車までたどり着かないと",
                    "maleVariant": null,
                    "stringId": 1786417448177233920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ラフェンが来るのは日没後だ。 それまでにサプライズを用意してやろう。電力さえ復旧させれば何とかなる",
                    "maleVariant": null,
                    "stringId": 1975084144917372928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ラフェンを迎え撃つ準備だ。時間がないから即興で工夫をしないと",
                    "maleVariant": null,
                    "stringId": 1975089265223032832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それなら大丈夫。降りてきて。奴らを驚かせてやろう！",
                    "maleVariant": null,
                    "stringId": 1975132872562860044
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さてと… 交差路への電力供給源は、あの変電所だ",
                    "maleVariant": null,
                    "stringId": 1975137553859108868
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "奴らが来たら屋根の配電盤を操作して、一斉に明かりを灯してやるの。わかった？",
                    "maleVariant": null,
                    "stringId": 1975140285760299008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "とにかく… ほら、着いたよ",
                    "maleVariant": null,
                    "stringId": 1976320502138822656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よし、着いたよ",
                    "maleVariant": null,
                    "stringId": 1976320955241095168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった",
                    "maleVariant": null,
                    "stringId": 1977636092277026816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "明かりがつけば、連中は不審に思って辺りを調べ始める… いい作戦ね",
                    "maleVariant": "明かりがつけば、連中は不審に思って辺りを調べ始める… いい作戦だ",
                    "stringId": 1977653304945668096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "陽動作戦ね！ 気に入った",
                    "maleVariant": "陽動作戦か！ 気に入った",
                    "stringId": 1977671051649871872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんな手にかかると思う？",
                    "maleVariant": "そんな手にかかると思うか？",
                    "stringId": 1977671051649871888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたは電力、あたしはラフェンに対処する",
                    "maleVariant": "あんたは電力、俺はラフェンに対処する",
                    "stringId": 1977671051666649100
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でしょ。でもまずは下の端末を起動しないと、屋根の配電盤が使えない",
                    "maleVariant": null,
                    "stringId": 1977681931942883328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "車が必要ね。バッテリーでブーストすれば起動するはず",
                    "maleVariant": null,
                    "stringId": 1977682769729941504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何をすればいい？",
                    "maleVariant": null,
                    "stringId": 1977820189238620160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいから。まずは変電所を調べてみよう",
                    "maleVariant": null,
                    "stringId": 1977821989350010880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私はもう部外者だけど",
                    "maleVariant": null,
                    "stringId": 1977834853766918144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ブレーカーがあった。でも電力が来てない",
                    "maleVariant": null,
                    "stringId": 1979011192914046976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まずは偵察をしよう。使えそうなものを探す",
                    "maleVariant": null,
                    "stringId": 1993926274672357376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "無理よ。設備はどれも動いてない",
                    "maleVariant": "無理だ。設備はどれも動いてない",
                    "stringId": 1993927791852777472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どう？ 何か見つかった？",
                    "maleVariant": null,
                    "stringId": 2005606799819386880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "全然。何か作戦はない？",
                    "maleVariant": "全然。何か作戦はないか？",
                    "stringId": 2005606931570864132
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "設備は完全に停止してる",
                    "maleVariant": null,
                    "stringId": 2005609111686197248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "全然。何かいいアイデアは？",
                    "maleVariant": null,
                    "stringId": 2005616158835339264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ロッキーリッジは完全に廃墟ね。設備はどれも動かなくなってる",
                    "maleVariant": "ロッキーリッジは完全に廃墟だな。設備はどれも動かなくなってる",
                    "stringId": 2005624070836187164
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なら動かせばいい",
                    "maleVariant": null,
                    "stringId": 2005624070852964352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "変電所を調べてみよう",
                    "maleVariant": null,
                    "stringId": 2005624070852964376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ラフェンが来るのは日が落ちてからだ。それまでにサプライズを用意しよう。まずは電力が必要だね",
                    "maleVariant": null,
                    "stringId": 2005626016624144384
                  }
                },
                {
