        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんだ？",
                    "maleVariant": null,
                    "stringId": 1344377609299402752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アーロン？ Vよ。記録を手に入れた",
                    "maleVariant": "アーロンか？ Vだ。記録を手に入れた",
                    "stringId": 1344380399920848896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それで？ 途中で問題はなかったか？",
                    "maleVariant": null,
                    "stringId": 1344390108795084800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[記録を渡す] これよ",
                    "maleVariant": "[記録を渡す] これだ",
                    "stringId": 1344390647243055124
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "捜査に使うんでしょ？",
                    "maleVariant": "捜査に使うんだろ？",
                    "stringId": 1344390895478743040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "映像を見たの",
                    "maleVariant": "映像を見た",
                    "stringId": 1344391036977782784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほら、これよ",
                    "maleVariant": "ほら、これだ",
                    "stringId": 1344393910747111424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ありがたい。お前の仕事ぶりは広めといてやる。アーロン・マッカールソンのお墨付きだ",
                    "maleVariant": null,
                    "stringId": 1344394355611770880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この記録は何かの捜査に使うんでしょ？",
                    "maleVariant": "この記録は何かの捜査に使うんだよな？",
                    "stringId": 1344394712563818496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあな。しかもかなりのヤマだ。おかげで有力な手がかりが手に入ったよ",
                    "maleVariant": null,
                    "stringId": 1344395072921640960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "手放すのが惜しいくらい。すごく興味深い内容だったから",
                    "maleVariant": "手放すのが惜しいくらいだ。すごく興味深い内容だったからな",
                    "stringId": 1344398191286001664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "中身は見るなって言ったよな？ そういう取引だろ",
                    "maleVariant": null,
                    "stringId": 1344399219444129792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "当然、カネよ",
                    "maleVariant": "当然、カネだ",
                    "stringId": 1344399883066908672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたを豚箱に入れたいの",
                    "maleVariant": "あんたを豚箱に入れたい",
                    "stringId": 1344400204182822912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まさか、『企業戦争』のチケットやフットマッサージだと思う？",
                    "maleVariant": "まさか、『企業戦争』のチケットや フットマッサージだと思うか？",
                    "stringId": 1344400878241030144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかったよ、クソッタレ。倍払えば文句ないんだろ",
                    "maleVariant": null,
                    "stringId": 1344401524516167680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "要求なんてない",
                    "maleVariant": "要求なんてないさ",
                    "stringId": 1344402605690298368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だったら何がしたいんだ？",
                    "maleVariant": null,
                    "stringId": 1344406352680165376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたをふさわしい場所に入れることよ。豚箱にね",
                    "maleVariant": "あんたをふさわしい場所に入れることさ。豚箱にな",
                    "stringId": 1344406682184687616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "残念だが、それは願い下げだ",
                    "maleVariant": null,
                    "stringId": 1344407121563197440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何もいらない",
                    "maleVariant": null,
                    "stringId": 1604210861076357120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クズ野郎め…",
                    "maleVariant": null,
                    "stringId": 1918491507518627840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おお、Vか。ハッ、また聖書協会のバカどもが押し売りに来たかと思っちまったよ。入りな",
                    "maleVariant": null,
                    "stringId": 1934265696620171264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本当にタフな奴だな。だから生き残ってこれたわけか",
                    "maleVariant": null,
                    "stringId": 1934268514722697216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あったけど、片付けた",
                    "maleVariant": null,
                    "stringId": 1934269474748551168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それはどうも。それじゃ",
                    "maleVariant": "それはどうも。じゃあな",
                    "stringId": 1934277441342459904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そういうのには律儀なの？ あの映像では、法を遵守するタイプには見えなかったけど",
                    "maleVariant": "そういうのには律儀なのか？ あの映像では、 法を遵守するタイプには見えなかったけどな",
                    "stringId": 1934279685580312576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "くだらん前置きは抜きだ。何か要求があるんだろ。さっさと言え",
                    "maleVariant": null,
                    "stringId": 1934281699399880704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたみたいなクズが慌てる姿を見たいだけ。自分は安全だと思ってた奴の焦る瞬間をね",
                    "maleVariant": "お前みたいなクズが慌てる姿を見たいだけだ。自分は安全だと思ってた奴の焦る瞬間をな",
                    "stringId": 1934285335861448704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハッ、プロの風上にも置けないな",
                    "maleVariant": null,
                    "stringId": 1934286454784315392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんただって相当なもんよ",
                    "maleVariant": "あんただって相当なもんだ",
                    "stringId": 1934286881093373952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほら、記録は好きにすればいい。報酬をさっさと出して",
                    "maleVariant": "ほら、記録は好きにすればいい。報酬をさっさと出せ",
                    "stringId": 1934288679174721536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今更驚くようなこと？",
                    "maleVariant": "今更驚くようなことか？",
                    "stringId": 1934301614290817024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "魂の牢獄にいた50年は、こういうのとは一切関わらなくて済んだんだよ",
                    "maleVariant": null,
                    "stringId": 1934301819744604160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うらやましいね",
                    "maleVariant": "うらやましいな",
                    "stringId": 1934302363477397504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なあV… みっちり懲らしめてやろうぜ。いいだろ？",
                    "maleVariant": null,
                    "stringId": 1934302533799694336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティで一番のゲス野郎でも目指してんのか？",
                    "maleVariant": null,
                    "stringId": 1934304825600954368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ひとつ言っとく。あんたはあたしの良心じゃないから",
                    "maleVariant": "ひとつ言わせてくれ。お前は俺の良心じゃないんだ",
                    "stringId": 1934306045589770240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、だな。そんなのこっちから願い下げだ",
                    "maleVariant": null,
                    "stringId": 1934306345969045504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺の願いが届いたみたいだな",
                    "maleVariant": null,
                    "stringId": 1934307315088150528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしって、期待に応える女なの",
                    "maleVariant": "俺は、期待に応える男でな",
                    "stringId": 1934308540428898304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ、さっそく記録を渡してくれ。善は急げだ",
                    "maleVariant": null,
                    "stringId": 1943157922368638976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "特に何も。気づかれなかったから",
                    "maleVariant": "特に何も。気づかれなかったからな",
                    "stringId": 1979132094567469056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "粛々と仕事を片づける。いいねえ、その姿勢",
                    "maleVariant": null,
                    "stringId": 1979133269274578944
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
