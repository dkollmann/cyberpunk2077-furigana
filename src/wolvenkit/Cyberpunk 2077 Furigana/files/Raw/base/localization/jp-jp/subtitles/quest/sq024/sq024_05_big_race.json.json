    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1388242201008414720,
            "femaleVariant": "もしもし、クレア"
          },
          {
            "stringId": 1389347495155343360,
            "femaleVariant": "そんな… ちょっと、ウソでしょ…！"
          },
          {
            "stringId": 1389360633812623360,
            "femaleVariant": "何のつもり？ 説明して！"
          },
          {
            "stringId": 1395319513533128704,
            "femaleVariant": "さあ、参加者は全員車に乗ってくれ！ トイレには行ったか？ レースを開始するぞ！"
          },
          {
            "stringId": 1399625779895652352,
            "femaleVariant": "スリー！"
          },
          {
            "stringId": 1399626150705680384,
            "femaleVariant": "ツー！"
          },
          {
            "stringId": 1399626228753289216,
            "femaleVariant": "ワン！"
          },
          {
            "stringId": 1399626321279635456,
            "femaleVariant": "..."
          },
          {
            "stringId": 1399643428201484288,
            "femaleVariant": "悪いんだけど、少し独りにさせて。お願い"
          },
          {
            "stringId": 1399675823562739712,
            "femaleVariant": "ええ",
            "maleVariant": "ああ"
          },
          {
            "stringId": 1399682503126077440,
            "femaleVariant": "優勝できたでしょ",
            "maleVariant": "優勝できただろ"
          },
          {
            "stringId": 1399682779614597120,
            "femaleVariant": "落ち着いて",
            "maleVariant": "落ち着けよ"
          },
          {
            "stringId": 1399683462447288320,
            "femaleVariant": "優勝できたでしょ",
            "maleVariant": "優勝できただろ"
          },
          {
            "stringId": 1399683525076635648,
            "femaleVariant": "落ち着いて。あんたのためでもあったのよ",
            "maleVariant": "落ち着けよ。あんたのためでもあったんだぞ"
          },
          {
            "stringId": 1399683683034124288,
            "femaleVariant": "もういい"
          },
          {
            "stringId": 1399743264733057024,
            "femaleVariant": "そんなのどうでもよかったのよ！"
          },
          {
            "stringId": 1405263834904055808,
            "femaleVariant": "準備はいいか？"
          },
          {
            "stringId": 1405322960346599424,
            "femaleVariant": "はあ… 信じられない…"
          },
          {
            "stringId": 1754678470284156928,
            "femaleVariant": "そこまでよ、サンプソン"
          },
          {
            "stringId": 1754679086729404416,
            "femaleVariant": "罪悪感は少しもないわけ？"
          },
          {
            "stringId": 1754679377562443776,
            "femaleVariant": "これっぽっちもな"
          },
          {
            "stringId": 1754682517233868800,
            "femaleVariant": "V、頼みがあるの"
          },
          {
            "stringId": 1754685178989518848,
            "femaleVariant": "追って、V！ 逃がさないで！"
          },
          {
            "stringId": 1754687656262901760,
            "femaleVariant": "チャンスは一度きりよ！"
          },
          {
            "stringId": 1754688223819341824,
            "femaleVariant": "勝つのはあたしたちよ",
            "maleVariant": "勝つのは俺たちだ"
          },
          {
            "stringId": 1754688948863508480,
            "femaleVariant": "こっちじゃない！"
          },
          {
            "stringId": 1754689783228014592,
            "femaleVariant": "クレア、あたしはゴールを目指す",
            "maleVariant": "クレア、俺はゴールを目指す"
          },
          {
            "stringId": 1754691364765835264,
            "femaleVariant": "ちょっと、何してるのよ！？"
          },
          {
            "stringId": 1754691804362448896,
            "femaleVariant": "大事なのはレースよ",
            "maleVariant": "大事なのはレースだ"
          },
          {
            "stringId": 1754692123364433920,
            "femaleVariant": "唯一のチャンスだったのに！ わかってるの！？"
          },
          {
            "stringId": 1754693115904532480,
            "femaleVariant": "あいつはまだ射程圏内よ。諦めないで…！"
          },
          {
            "stringId": 1754693692688441344,
            "femaleVariant": "いたよ！",
            "maleVariant": "いたぞ！"
          },
          {
            "stringId": 1754693850293608448,
            "femaleVariant": "距離を詰めて！"
          },
          {
            "stringId": 1766172875084013568,
            "femaleVariant": "近寄るな、このイカれ女め"
          },
          {
            "stringId": 1834161703744446464,
            "femaleVariant": "V、いよいよ決勝レースね"
          },
          {
            "stringId": 1834162228737089536,
            "femaleVariant": "あたしのを使いましょ",
            "maleVariant": "俺のを使おう"
          },
          {
            "stringId": 1834162479187369984,
            "femaleVariant": "ビーストを使う"
          },
          {
            "stringId": 1834162657747279872,
            "femaleVariant": "あたしのを使いましょ。慣れてる方がいい",
            "maleVariant": "俺の車を使おう。慣れてる方がいい"
          },
          {
            "stringId": 1834162664424611840,
            "femaleVariant": "ビーストを使わせてもらおうかな"
          },
          {
            "stringId": 1834162747673157632,
            "femaleVariant": "じゃ、スタート地点で"
          },
          {
            "stringId": 1834163220455104512,
            "femaleVariant": "了解"
          },
          {
            "stringId": 1834163456627974144,
            "femaleVariant": "賢明な判断ね"
          },
          {
            "stringId": 1861561520321859584,
            "femaleVariant": "ええ。今どこ？ あたしはもういる",
            "maleVariant": "ああ。今どこだ？ 俺はもういる"
          },
          {
            "stringId": 1861562249543553024,
            "femaleVariant": "すぐ行く。今回はハイウェイレースだから、速度重視のマシンが必要よ"
          },
          {
            "stringId": 1861564217561321472,
            "femaleVariant": "使う車はあなたに任せる"
          },
          {
            "stringId": 1861566484112871424,
            "femaleVariant": "ええ、すぐに向かう"
          },
          {
            "stringId": 1861575844423667716,
            "femaleVariant": "クレア、もう放っときましょ",
            "maleVariant": "クレア、もう放っとこう"
          },
          {
            "stringId": 1861575891416649728,
            "femaleVariant": "クレア、よして。そいつは放っとけばいい",
            "maleVariant": "クレア、よせよ。そいつは放っとけばいい"
          },
          {
            "stringId": 1861575918646071296,
            "femaleVariant": "ちょっと、なんなの？",
            "maleVariant": "おい、どうしたんだ？"
          },
          {
            "stringId": 1861576028016742400,
            "femaleVariant": "何なの？",
            "maleVariant": "何なんだ？"
          },
          {
            "stringId": 1861578017408684032,
            "femaleVariant": "あんただけは、絶対に殺してやる！"
          },
          {
            "stringId": 1861579283769724928,
            "femaleVariant": "クレア！"
          },
          {
            "stringId": 1861579852819337216,
            "femaleVariant": "サンプソンよ… こいつだけは生きて帰さない"
          },
          {
            "stringId": 1861580429720686592,
            "femaleVariant": "絶対に殺してやるから！"
          },
          {
            "stringId": 1861581142450376704,
            "femaleVariant": "新しいドライバーか？"
          },
          {
            "stringId": 1861581918983819264,
            "femaleVariant": "今のうちに逃げた方がいいぞ。クレアのパートナーは早死にするからな"
          },
          {
            "stringId": 1861586390044774400,
            "femaleVariant": "この… クソ野郎！"
          },
          {
            "stringId": 1861587199981654020,
            "femaleVariant": "クレア、やめて",
            "maleVariant": "クレア、やめろ"
          },
          {
            "stringId": 1861588294208458752,
            "femaleVariant": "クレア、よして！ まだレースがあるのよ！",
            "maleVariant": "クレア、よせ！ まだレースがあるんだぞ！"
          },
          {
            "stringId": 1861660028634796032,
            "femaleVariant": "ディーンの奴は、夫としてもドライバーとしてもクズだった"
          },
          {
            "stringId": 1861660215063220224,
            "femaleVariant": "身の程をわきまえず、調子に乗った結果があのザマだ。それが真実なんだよ"
          },
          {
            "stringId": 1861660478146744320,
            "femaleVariant": "奴は自分のエゴに殺されたんだ"
          },
          {
            "stringId": 1861660826055872512,
            "femaleVariant": "あんたがディーンを語るんじゃないよ…！"
          },
          {
            "stringId": 1861765741822480384,
            "femaleVariant": "クソ野郎！"
          },
          {
            "stringId": 1861765896021872640,
            "femaleVariant": "失せな"
          },
          {
            "stringId": 1861766097935667200,
            "femaleVariant": "クレア、その怒りはレースで活かせばいい"
          },
          {
            "stringId": 1861768726321434624,
            "femaleVariant": "コーポのゴミクズめ"
          },
          {
            "stringId": 1861769866635251712,
            "femaleVariant": "せいぜいレース中は気をつけることね"
          },
          {
            "stringId": 1861770330894372864,
            "femaleVariant": "失せな"
          },
          {
            "stringId": 1861770462830399488,
            "femaleVariant": "クレア、もういいでしょ",
            "maleVariant": "クレア、もういいだろ"
          },
          {
            "stringId": 1861773095158812672,
            "femaleVariant": "なに？",
            "maleVariant": "なんだ？"
          },
          {
            "stringId": 1861773173105758208,
            "femaleVariant": "奴のケツに… ぴったりつけて"
          },
          {
            "stringId": 1861814408164020224,
            "femaleVariant": "集中して！"
          },
          {
            "stringId": 1861816914294214656,
            "femaleVariant": "わかってる"
          },
          {
            "stringId": 1861820501262995456,
            "femaleVariant": "停めて。降りるから"
          },
          {
            "stringId": 1861823944249708548,
            "femaleVariant": "あたしには復讐なんて関係ない",
            "maleVariant": "俺には復讐なんて関係ない"
          },
          {
            "stringId": 1861824099103412224,
            "femaleVariant": "あんたを助けてあげたの",
            "maleVariant": "あんたを助けてやったんだ"
          },
          {
            "stringId": 1861824150424915968,
            "femaleVariant": "勝ちたかったの",
            "maleVariant": "勝ちたかったんだ"
          },
          {
            "stringId": 1861825885994356736,
            "femaleVariant": "あたしはドライバーとして雇われたのよ。あんたの復讐に手を貸すためじゃない",
            "maleVariant": "俺はドライバーとして雇われたんだぞ。あんたの復讐に手を貸すためじゃない"
          },
          {
            "stringId": 1861825907469193216,
            "femaleVariant": "あんたを助けたかったの",
            "maleVariant": "あんたを助けたかったんだ"
          },
          {
            "stringId": 1861825931762601984,
            "femaleVariant": "あたしはレースをしに来たの",
            "maleVariant": "俺はレースをしに来たんだ"
          },
          {
            "stringId": 1861827048001114112,
            "femaleVariant": "レースなんてどうでもいいのよ！ あのクソ野郎は私からディーンを奪ったの！"
          },
          {
            "stringId": 1861827395658584064,
            "femaleVariant": "私を愛してくれた、唯一の人を…"
          },
          {
            "stringId": 1861828206048448512,
            "femaleVariant": "私を…？ 違う！ あんたは奴を助けたのよ！ せっかくのチャンスだったのに！"
          },
          {
            "stringId": 1861828829758230528,
            "femaleVariant": "あいつが死ねば、すぐに完全武装したトラウマ・チームが駆けつける。奴らとやり合う気だったの？",
            "maleVariant": "あいつが死ねば、すぐに完全武装したトラウマ・チームが駆けつける。奴らとやり合う気だったのか？"
          },
          {
            "stringId": 1861830506120876032,
            "femaleVariant": "それで？ 見事に勝てて満足した？"
          },
          {
            "stringId": 1861832992923717632,
            "femaleVariant": "クレア…"
          },
          {
            "stringId": 1861833071273316352,
            "femaleVariant": "私は… 正義を求めてたのに"
          },
          {
            "stringId": 1861833420507844608,
            "femaleVariant": "あなたは、夫の仇を討つ唯一の機会を奪ったのよ！"
          },
          {
            "stringId": 1861834211050262528,
            "femaleVariant": "なんでそれがわからないわけ？"
          },
          {
            "stringId": 1861834881316818944,
            "femaleVariant": "あのクズは私が愛した唯一の人を殺したのよ。その意味がわかる？"
          },
          {
            "stringId": 1861835971315765248,
            "femaleVariant": "負けるのは我慢できないの。ただそれだけ。復讐なんて知らない",
            "maleVariant": "負けるのは我慢できない。ただそれだけだ。復讐なんて知らない"
          },
          {
            "stringId": 1861837409878151168,
            "femaleVariant": "あっそう… せいぜい勝利に酔いしれてれば？"
          },
          {
            "stringId": 1887814515837378560,
            "femaleVariant": "奴の車が破損してる！"
          },
          {
            "stringId": 1887851474987261952,
            "femaleVariant": "まだ間に合う！ 戻って！"
          },
          {
            "stringId": 2054867889689804800,
            "femaleVariant": "[立つ]"
          },
          {
            "stringId": 2054869672252227584,
            "femaleVariant": "[待つ]"
          },
          {
            "stringId": 2237419384292380680,
            "femaleVariant": "頑張ってね。レースが終わったら、私が入念にマッサージしてあげるから"
          },
          {
            "stringId": 2237419384376266760,
            "femaleVariant": "いいか… 人じゃなくてタイヤを狙え。パンクさせたら、俺が車ごと体当たりする"
          },
          {
            "stringId": 2237419384476930056,
            "femaleVariant": "勝つのはクレアとVに決まってる！ 他の連中に勝ち目はない！"
          },
          {
            "stringId": 2237419384577593352,
            "femaleVariant": "レースなんかどうでもいいんだよ！ 派手なクラッシュが見たいだけだ！"
          },
          {
            "stringId": 2237419384661479432,
            "femaleVariant": "神様、天使様、お願いだ… 奴らを勝たせてやってくれたまえ…"
          },
          {
            "stringId": 2237419384745365512,
            "femaleVariant": "ぶちかましてやれ、サンプソン！ お前がナンバーワンだ！"
          },
          {
            "stringId": 2237435603112300552,
            "femaleVariant": "すっごく興奮したわ！"
          },
          {
            "stringId": 2237435603212963848,
            "femaleVariant": "最悪だ！ 期待を裏切りやがって！"
          },
          {
            "stringId": 2237435603313627144,
            "femaleVariant": "フー！ しゅご… すごいレースだった！"
          },
          {
            "stringId": 2250141057290473472,
            "femaleVariant": "お友達も呼ぶといい。みんなで盛大に祝おう"
          },
          {
            "stringId": 2250142137810292736,
            "femaleVariant": "もし1位でフィニッシュしたら、夜は私がとっておきのテクでフィニッシュさせてあげる"
          },
          {
            "stringId": 2250143657423409152,
            "femaleVariant": "“もし”じゃないさ。私は必ず勝つ"
          },
          {
            "stringId": 2250156757711196160,
            "femaleVariant": "ルスラナのチームが、すでにルート上で待機してる。後は奴らに任せればいい"
          },
          {
            "stringId": 2250157929314521088,
            "femaleVariant": "トラウマが来たらどうするんだ？ ゴールドか、下手したらプラチナ契約者がいるかもしれない"
          },
          {
            "stringId": 2250159247936589824,
            "femaleVariant": "それもルスラナたちがなんとかする。とにかく、お前はできるだけ多くのタイヤを撃ち抜け。わかったな？"
          }
        ]
      }
    }
  }
}