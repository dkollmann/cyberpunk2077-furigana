  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Yes.",
                    "maleVariant": null,
                    "stringId": 3065018288488996864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Or you are too soon.",
                    "maleVariant": null,
                    "stringId": 3065021865575997444
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Great, thanks a lot.",
                    "maleVariant": null,
                    "stringId": 3065022600938151936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Then tell me what's going on.",
                    "maleVariant": null,
                    "stringId": 3068501882625200132
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Unfortunately, my case is more urgent.",
                    "maleVariant": null,
                    "stringId": 3068502112405950464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Right, I promised. So tell me what's going on.",
                    "maleVariant": null,
                    "stringId": 3068502325677920256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I remember what I promised, but it has to wait.",
                    "maleVariant": null,
                    "stringId": 3068502406879645696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]So I'm listening.",
                    "maleVariant": null,
                    "stringId": 3068502731535552516
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I do not know what you're talking about.",
                    "maleVariant": null,
                    "stringId": 3068502836628033536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Let's say you're right. But don't count on me to hear more about it.",
                    "maleVariant": null,
                    "stringId": 3068503002470813696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I have no idea what you are talking about.",
                    "maleVariant": null,
                    "stringId": 3068503090584752128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Okay, say what you want.",
                    "maleVariant": null,
                    "stringId": 3068503358651109380
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Yes, I have to pass the data on to someone.",
                    "maleVariant": null,
                    "stringId": 3068503456210620416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Bingo. This data will go to someone who needs it badly.",
                    "maleVariant": null,
                    "stringId": 3068503582677274624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I'll give you two splinters. The first has the data you requested. On the second - a virus.",
                    "maleVariant": null,
                    "stringId": 3068503935586013184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I'll do it. If there is an opportunity.",
                    "maleVariant": null,
                    "stringId": 3068504296010944516
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]What if I refuse?",
                    "maleVariant": null,
                    "stringId": 3068504396774903808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]What is this hack going to do?",
                    "maleVariant": null,
                    "stringId": 3068504432627814400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I'll do it. If I have the opportunity.",
                    "maleVariant": null,
                    "stringId": 3068504575687135232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]What if I refuse?",
                    "maleVariant": null,
                    "stringId": 3068504653432754176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Let's say I do what you ask for. What is this virus used for?",
                    "maleVariant": null,
                    "stringId": 3068504723964170240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]All right.",
                    "maleVariant": null,
                    "stringId": 3068505507560820736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]But first, let's talk about the favor you promised me.",
                    "maleVariant": null,
                    "stringId": 3068507724737024000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Let's play open cards. You plan to sabotage the Max-Tacu convoy. This is what my request is about.",
                    "maleVariant": null,
                    "stringId": 3068515647810834432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Don't make a fool of me, V. Neither of us have time for this circus.",
                    "maleVariant": null,
                    "stringId": 3068518230579351552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You bluff.",
                    "maleVariant": null,
                    "stringId": 3068521170736123904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Maybe.",
                    "maleVariant": null,
                    "stringId": 3068521512588677120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You disappointed me.",
                    "maleVariant": null,
                    "stringId": 3068521710677266432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I'm sorry, I didn't want to.",
                    "maleVariant": null,
                    "stringId": 3068521870899679232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You have. But that doesn't settle our scores.",
                    "maleVariant": null,
                    "stringId": 3068522306083885056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I don't expect details from you. I only count on cooperation.",
                    "maleVariant": null,
                    "stringId": 3068523996136738816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]If you find yourself near any Max-Tacu officer any time soon, you'll run this hack.",
                    "maleVariant": null,
                    "stringId": 3068524618084913152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]The target's status is irrelevant, the virus will find its way.",
                    "maleVariant": null,
                    "stringId": 3068525497848565760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I'll contact you when it's all over.",
                    "maleVariant": null,
                    "stringId": 3068528948804456448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Then the splinter with the virus will go back to my pocket.",
                    "maleVariant": null,
                    "stringId": 3068529695944216576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Consider if you have time to seek help from someone else.",
                    "maleVariant": null,
                    "stringId": 3068530048467079168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]In a word: I have no choice. Give me these splinters.",
                    "maleVariant": null,
                    "stringId": 3068530396728528896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]He will make a connection. Short and unstable, but enough for me to leave a few dormant surprises there.",
                    "maleVariant": null,
                    "stringId": 3068531642638143488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You are late.",
                    "maleVariant": null,
                    "stringId": 3070738860754530304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Meeting in the park - are you playing spy?",
                    "maleVariant": null,
                    "stringId": 3070739422187286528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]But I am. Do you have the data?",
                    "maleVariant": null,
                    "stringId": 3070739558049181696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Or you are too soon.",
                    "maleVariant": null,
                    "stringId": 3070740188251746304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Meeting in the park. You should also feed the pigeons. Just like in a spy movie.",
                    "maleVariant": null,
                    "stringId": 3070740260980977664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Hey, but here I am. Do you have my details?",
                    "maleVariant": null,
                    "stringId": 3070740331025854464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Did you manage it? Did you get what I asked for?",
                    "maleVariant": null,
                    "stringId": 3070741102761013248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Pigeons do not exist.",
                    "maleVariant": null,
                    "stringId": 3070742055052570624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]No, I know. OK, nevermind. Have you got something for me?",
                    "maleVariant": null,
                    "stringId": 3070742162141540352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Mhm, the tension is rising, but I don't know if I want to know the rest of this story.",
                    "maleVariant": null,
                    "stringId": 3070826044111122432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]The contents of the second splinter will become useless in a fraction of a second. I will leave it for you as a souvenir.",
                    "maleVariant": null,
                    "stringId": 3070840612388532224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Night Corp is spying on Max-Tac?",
                    "maleVariant": null,
                    "stringId": 3070843286089560064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Is it within your abilities?",
                    "maleVariant": null,
                    "stringId": 3070848345879801856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Now it all becomes clear. You would not agree to help me if you could not benefit from this help yourself.",
                    "maleVariant": null,
                    "stringId": 3070848898605182976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]More like Achaia, who will drop the horse to the Trojans. But don't worry, no one will tie this to you. Nobody will know about it.",
                    "maleVariant": null,
                    "stringId": 3070851248153620480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]So I'm supposed to be your Trojan Horse. Wonderful.",
                    "maleVariant": null,
                    "stringId": 3070852047470522368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You pay off your debt with one hand and use your other hand to climb the corporate ladder, eh?",
                    "maleVariant": null,
                    "stringId": 3070855518475186176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I have no idea what you are talking about.",
                    "maleVariant": null,
                    "stringId": 3070857232301355008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You wouldn't be asking me to do that if it weren't for Night Corp's interests.",
                    "maleVariant": null,
                    "stringId": 3070857501156241408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]With data from Max-Tac, Night Corp could shake up this city. If it isn't already shaking. What will you get for this? Promotion?",
                    "maleVariant": null,
                    "stringId": 3070858176204304384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]No comment.",
                    "maleVariant": null,
                    "stringId": 3070859634916126720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Just confirm that you complete the task.",
                    "maleVariant": null,
                    "stringId": 3070859829011738624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]My case is much more urgent.",
                    "maleVariant": null,
                    "stringId": 3070862445770895360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]The data you asked for speaks for itself.",
                    "maleVariant": null,
                    "stringId": 3070866117917601792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Well, unless you are just an intermediary? A courier in some coarser transaction that is to provide data that is more important than itself?",
                    "maleVariant": null,
                    "stringId": 3070866844421386240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Now you owe me a favor. Someday I will decide to claim it.",
                    "maleVariant": null,
                    "stringId": 3070873334452514816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I will be waiting impatiently. Thanks.",
                    "maleVariant": null,
                    "stringId": 3070873752641400832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT][take the splinters and stand up]",
                    "maleVariant": null,
                    "stringId": 3115644715712339972
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT][take the splinters]",
                    "maleVariant": null,
                    "stringId": 3115645759053848576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT][take the splinter and stand up]",
                    "maleVariant": null,
                    "stringId": 3115872673299390464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT][take the splinter]",
                    "maleVariant": null,
                    "stringId": 3115872673400053760
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}