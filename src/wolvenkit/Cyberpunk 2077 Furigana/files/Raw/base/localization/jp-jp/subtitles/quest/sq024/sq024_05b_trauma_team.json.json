    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1754877954301632512,
            "femaleVariant": "クソ！"
          },
          {
            "stringId": 1754877954301632520,
            "femaleVariant": "路肩に入った！"
          },
          {
            "stringId": 1754909511724834816,
            "femaleVariant": "乗ってく？"
          },
          {
            "stringId": 1785376738429116416,
            "femaleVariant": "ええ…",
            "maleVariant": "ああ…"
          },
          {
            "stringId": 1861890268460093440,
            "femaleVariant": "早く！ 追いかけて！"
          },
          {
            "stringId": 1861965910081933312,
            "femaleVariant": "サンプソン、調子はどう？"
          },
          {
            "stringId": 1861966229620789248,
            "femaleVariant": "クレア… た、頼む！ よせ！ 私は何も…"
          },
          {
            "stringId": 1861966703006715904,
            "femaleVariant": "命乞いしても無駄よ、クズめ"
          },
          {
            "stringId": 1861967050781626368,
            "femaleVariant": "おい！ 頼む！ こいつを止めてくれ！"
          },
          {
            "stringId": 1861967308462886912,
            "femaleVariant": "クレア、お願いだ！"
          },
          {
            "stringId": 1861981052224016384,
            "femaleVariant": "待て… 聞け！ 私はディーンを殺しちゃいない！"
          },
          {
            "stringId": 1861981642647801856,
            "femaleVariant": "うるさい、黙れ！"
          },
          {
            "stringId": 1861982250654109696,
            "femaleVariant": "あいつは自分で勝手に死んだんだよ！"
          },
          {
            "stringId": 1861982907633111040,
            "femaleVariant": "もう諦めるんだね…"
          },
          {
            "stringId": 1861983125099384836,
            "femaleVariant": "勝手に死んだって？"
          },
          {
            "stringId": 1861984251454558208,
            "femaleVariant": "勝手に死んだって？"
          },
          {
            "stringId": 1861984562671915008,
            "femaleVariant": "命が惜しくてデタラメを言ってるのよ"
          },
          {
            "stringId": 1861985086171385856,
            "femaleVariant": "頼む、こいつを止めてくれ！ 私は殺してないんだ！"
          },
          {
            "stringId": 1861985927800426496,
            "femaleVariant": "黙れって言ったのが聞こえなかった？"
          },
          {
            "stringId": 1861986641620635652,
            "femaleVariant": "そいつの話を聞きたい"
          },
          {
            "stringId": 1861986711933947904,
            "femaleVariant": "待って、クレア。そいつの話を聞きたい",
            "maleVariant": "待て、クレア。そいつの話を聞きたい"
          },
          {
            "stringId": 1861986801322954752,
            "femaleVariant": "クレア、どうなのよ？ サンプソンの言ってることは本当なの？",
            "maleVariant": "クレア、どうなんだ？ サンプソンの言ってることは本当なのか？"
          },
          {
            "stringId": 1861988143986774016,
            "femaleVariant": "…聞いてどうするのよ？"
          },
          {
            "stringId": 1861988210474881024,
            "femaleVariant": "サンプソン、話して",
            "maleVariant": "サンプソン、話せよ"
          },
          {
            "stringId": 1861988418898235392,
            "femaleVariant": "ディーンはゴール直前で私を抜こうとして、そこで制御を失ったんだ…！"
          },
          {
            "stringId": 1861988960600985600,
            "femaleVariant": "ぶつかってきたからでしょ！"
          },
          {
            "stringId": 1861989194274050048,
            "femaleVariant": "だとしてもルール違反じゃない！ そういうレースだろ！"
          },
          {
            "stringId": 1861990151028346880,
            "femaleVariant": "クレア、本当なの？",
            "maleVariant": "クレア、本当なのか？"
          },
          {
            "stringId": 1861992646639226880,
            "femaleVariant": "そんなのどうでもいい！ ディーンを殺した報いを受けさせるのよ！"
          },
          {
            "stringId": 1861994118319837184,
            "femaleVariant": "なんでだ？ お前だって… 散々殺してるだろ…"
          },
          {
            "stringId": 1861994937282859008,
            "femaleVariant": "クレア… ディーンの件は事故だったのよ",
            "maleVariant": "クレア… ディーンの件は事故だったんだ"
          },
          {
            "stringId": 1861995736297771008,
            "femaleVariant": "彼を愛してた。命よりも大切な存在だったの。なのに…"
          },
          {
            "stringId": 1861996335881916416,
            "femaleVariant": "このままこいつを見逃せって言うの？ ウソでしょ？"
          },
          {
            "stringId": 1861996679630295044,
            "femaleVariant": "そうよ。見逃すの",
            "maleVariant": "そうだ。見逃すんだよ"
          },
          {
            "stringId": 1861996760127377408,
            "femaleVariant": "好きにすればいい"
          },
          {
            "stringId": 1861998296417046528,
            "femaleVariant": "その通りよ。見逃すの",
            "maleVariant": "その通りだ。見逃してやれ"
          },
          {
            "stringId": 1861999307177201664,
            "femaleVariant": "ハッ、できるわけないでしょ…"
          },
          {
            "stringId": 1861999400206864384,
            "femaleVariant": "たとえクズだとしても、ディーンを殺したわけじゃない"
          },
          {
            "stringId": 1862000025527259136,
            "femaleVariant": "殺しても、あんたの気は晴れない"
          },
          {
            "stringId": 1862000485776625664,
            "femaleVariant": "ッ… クソッ！"
          },
          {
            "stringId": 1862000508325203968,
            "femaleVariant": "そのとおりかもね…"
          },
          {
            "stringId": 1862003529398489088,
            "femaleVariant": "クレア、頼むよ…"
          },
          {
            "stringId": 1862003745354813440,
            "femaleVariant": "Vに感謝するんだね… あんたの命の恩人よ！"
          },
          {
            "stringId": 1862004597905821696,
            "femaleVariant": "それでいいのよ",
            "maleVariant": "それでいいんだ"
          },
          {
            "stringId": 1862004771331903488,
            "femaleVariant": "行くよ… 私の気が変わらないうちに"
          },
          {
            "stringId": 1862005599572725760,
            "femaleVariant": "はあ… よかった！"
          },
          {
            "stringId": 1862005815411609600,
            "femaleVariant": "でも関係ないのよ"
          },
          {
            "stringId": 1862006070140080128,
            "femaleVariant": "..."
          },
          {
            "stringId": 1862006460713668608,
            "femaleVariant": "な…？ よせ！ ディーンはお前が思うような聖人じゃ――"
          },
          {
            "stringId": 1862006557367209984,
            "femaleVariant": "..."
          },
          {
            "stringId": 1862008554392784896,
            "femaleVariant": "死んでもらうよ。夫の仇だ"
          },
          {
            "stringId": 1883376874828079108,
            "femaleVariant": "ええ",
            "maleVariant": "ああ"
          },
          {
            "stringId": 1883377053455097856,
            "femaleVariant": "いや、いい"
          },
          {
            "stringId": 1883379346900525056,
            "femaleVariant": "いや、いい。ねえ、あんたはこれでよかったの？",
            "maleVariant": "いや、いい。なあ、あんたはこれでよかったのか？"
          },
          {
            "stringId": 1883379790859214848,
            "femaleVariant": "何が？ 意味がわからない"
          },
          {
            "stringId": 1936327436649238532,
            "femaleVariant": "ビーストはあげる"
          },
          {
            "stringId": 1936327436666015744,
            "femaleVariant": "え？ あんたの車でしょ…？",
            "maleVariant": "え？ あんたの車だろ…？"
          },
          {
            "stringId": 1936327436699570176,
            "femaleVariant": "いいえ――これは私とディーンの車よ。だけど彼はもういないから…"
          },
          {
            "stringId": 1936327436749901824,
            "femaleVariant": "じゃあね、V。あと、コーナリングの練習をしといた方がいいよ"
          },
          {
            "stringId": 1937084260788740096,
            "femaleVariant": "もう行きましょ"
          },
          {
            "stringId": 1944521393275224064,
            "femaleVariant": "わかった… 好きにして",
            "maleVariant": "わかった… 好きにしろ"
          },
          {
            "stringId": 2230469371377094656,
            "femaleVariant": "これですべて… 終わったってことでいいの？",
            "maleVariant": "これですべて… 終わったってことでいいのか？"
          },
          {
            "stringId": 2230469957120036864,
            "femaleVariant": "レースが？ それとも復讐が？"
          },
          {
            "stringId": 2230470128146976768,
            "femaleVariant": "そうね… 両方終わりよ。いつまでも過去に囚われてちゃダメだから"
          },
          {
            "stringId": 2230470376147783680,
            "femaleVariant": "そのほうがいい。でも… 本当に大丈夫なの、クレア？ 変な気は起こさないよね？",
            "maleVariant": "そのほうがいい。だが… 本当に大丈夫か、クレア？ 変な気は起こさないよな？"
          },
          {
            "stringId": 2230470618041683968,
            "femaleVariant": "私がこのままどこかの壁に車ごと突っ込むんじゃないかって？ そんな終わらせ方をするつもりはないよ"
          }
        ]
      }
    }
  }
}