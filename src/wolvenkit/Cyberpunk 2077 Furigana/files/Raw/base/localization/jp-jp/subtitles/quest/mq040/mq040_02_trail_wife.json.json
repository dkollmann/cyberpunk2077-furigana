  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「私は現場に到着した。ピンクのジャケットを着た、不貞を働く女を捜しにきたのだ」",
                    "maleVariant": null,
                    "stringId": 1805320966090403840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「女はどいつも同じだとよく言うが、この女はどこか違った…」",
                    "maleVariant": null,
                    "stringId": 1805395303032041472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「ジャケットがピンクではなく、後ろめたさがない。違う女のようだ」",
                    "maleVariant": null,
                    "stringId": 1805395340512342016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「ついに見つけた。タバコの煙に包まれ、ピンクの服を着た女。私は彼女の後をつけ…」",
                    "maleVariant": null,
                    "stringId": 1805395373211136000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "…ふざけたナレーションをいい加減やめることにした",
                    "maleVariant": null,
                    "stringId": 1805397948379574272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「私は女に近づきすぎて、怖がらせた。注意しなくては…」",
                    "maleVariant": null,
                    "stringId": 1805461926531182592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「またしても近づきすぎた。まるで磁石のような力で、女に引き寄せられているかのようだ」",
                    "maleVariant": null,
                    "stringId": 1805461952955297792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「私は女を怒らせてしまった。探偵という仕事は、存外難しいものである」",
                    "maleVariant": null,
                    "stringId": 1805461974748901376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「周囲を見回す女。何かを感じていた。私の視線かもしれない…」",
                    "maleVariant": null,
                    "stringId": 1806780176951029760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「女は距離を取って浮浪者のそばを通り過ぎた。服を汚したくはない。ただでさえ頭の中は汚れているのだから」",
                    "maleVariant": null,
                    "stringId": 1806780482648682496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "場末のバーに、浮気、そして魅力的な女… まるでノワールもんの脚本みたいだな",
                    "maleVariant": null,
                    "stringId": 1913994035820023808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「ぺぺは客の愚痴を聞くのには慣れていた。しかし今度は自分が胸の内を告白する番だった」",
                    "maleVariant": null,
                    "stringId": 1913996165989584896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「どこにでもある、ありふれた話である。何もかもが不確かな街で、不貞をはたらく妻…」",
                    "maleVariant": null,
                    "stringId": 1913997318533992448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もういい？ 集中させてよ",
                    "maleVariant": "もういいか？ 集中させてくれ",
                    "stringId": 1913998344209092608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やめてよね",
                    "maleVariant": "やめろって",
                    "stringId": 1914000258590101504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "楽しい？",
                    "maleVariant": "楽しいか？",
                    "stringId": 1914000959860953088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたの歌を聴いてるほうがまだマシ",
                    "maleVariant": "お前の歌を聴いてるほうがまだマシだ",
                    "stringId": 1914003498572500992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "吐きそうになってきた。それもナレーションしてよ",
                    "maleVariant": "吐きそうになってきた。それもナレーションしろよ",
                    "stringId": 1914008350795026432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これ、何かの罰ゲーム？",
                    "maleVariant": "これ、何かの罰ゲームか？",
                    "stringId": 1914009898442870784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかってるって",
                    "maleVariant": null,
                    "stringId": 1914010873081679872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっとやめて",
                    "maleVariant": "おい。やめろ",
                    "stringId": 1914012456196558848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "意味がわからない",
                    "maleVariant": null,
                    "stringId": 1914013555037097984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "タイミングが悪い… いないみたい",
                    "maleVariant": "タイミングが悪い… いないみたいだ",
                    "stringId": 1918333840882020368
                  }
                }
              ]
            }
          }
        }
      }
    },
