    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1153430861407727616,
            "femaleVariant": "情報は揃った？"
          },
          {
            "stringId": 1153437726644514816,
            "femaleVariant": "ええ、これで十分。助かったよ、ジュディ",
            "maleVariant": "ああ、これで十分だ。助かったよ、ジュディ"
          },
          {
            "stringId": 1187911673528070144,
            "femaleVariant": "この後はどうすればいいの？",
            "maleVariant": "この後はどうすればいいんだ？"
          },
          {
            "stringId": 1215248765245059072,
            "femaleVariant": "危険と言えば、私にはあなた達がどう見えると思う？"
          },
          {
            "stringId": 1215253551499571200,
            "femaleVariant": "この後はどうすればいいの？",
            "maleVariant": "この後はどうすればいいんだ？"
          },
          {
            "stringId": 1215277220846235648,
            "femaleVariant": "喋って歩く屍よ"
          },
          {
            "stringId": 1215278023115927552,
            "femaleVariant": "BDで貴重な情報が得られた"
          },
          {
            "stringId": 1215278850266230784,
            "femaleVariant": "落ち着いて。大丈夫だから",
            "maleVariant": "落ち着け。大丈夫だ"
          },
          {
            "stringId": 1215279355008774144,
            "femaleVariant": "BDのおかげで必要な情報が揃った。これで何も怖くない"
          },
          {
            "stringId": 1215288322699161600,
            "femaleVariant": "落ち着いて。大丈夫だから",
            "maleVariant": "落ち着け。きっと大丈夫だ"
          },
          {
            "stringId": 1215289496768737280,
            "femaleVariant": "持ってることがアラサカにバレたら、あなた達も、私も殺される"
          },
          {
            "stringId": 1215357349282836480,
            "femaleVariant": "あなた達がしくじったら、きっと私も…"
          },
          {
            "stringId": 1215358773886578688,
            "femaleVariant": "エヴリン、お願い… くれぐれも慎重にね"
          },
          {
            "stringId": 1215374043770384384,
            "femaleVariant": "ヨリノブとはどういう関係なの？",
            "maleVariant": "ヨリノブとはどういう関係なんだ？"
          },
          {
            "stringId": 1215389140211331072,
            "femaleVariant": "あんたがなぜヨリノブに詳しいのか、わかった気がする"
          },
          {
            "stringId": 1215394681994211328,
            "femaleVariant": "V…"
          },
          {
            "stringId": 1451730960049106944,
            "femaleVariant": "じゃT-バグ、また後で",
            "maleVariant": "じゃあT-バグ、また後で"
          },
          {
            "stringId": 1451731308562214912,
            "femaleVariant": "お疲れ様、じゃあまた"
          },
          {
            "stringId": 1451750420025831424,
            "femaleVariant": "仕事よ。それでしかない"
          },
          {
            "stringId": 1461723383613485056,
            "femaleVariant": "色々ありがとう"
          },
          {
            "stringId": 1461723763734867968,
            "femaleVariant": "また会えたらいいね",
            "maleVariant": "また会えたらいいな"
          },
          {
            "stringId": 1461726047147520000,
            "femaleVariant": "色々とありがとう、ジュディ"
          },
          {
            "stringId": 1461726359220514816,
            "femaleVariant": "それと心配しないで。あたしもプロだから",
            "maleVariant": "それと、心配するな。俺もプロだ"
          },
          {
            "stringId": 1461737359537729536,
            "femaleVariant": "じゃあ、もう行くから安心して。あんたに危険が及ぶことはない。その… また会えるといいね",
            "maleVariant": "じゃあ、もう行くから安心してくれ。あんたに危険が及ぶことはない。その… また会えるといいな"
          },
          {
            "stringId": 1461737956588515328,
            "femaleVariant": "あなた次第よ"
          },
          {
            "stringId": 1461738418263945216,
            "femaleVariant": "っていうと？",
            "maleVariant": "というと？"
          },
          {
            "stringId": 1461738536056778752,
            "femaleVariant": "ひとりで来るのか、またランナーとくるのか。それ次第"
          },
          {
            "stringId": 1461742173726306304,
            "femaleVariant": "必ず… ひとりで来る",
            "maleVariant": "必ず… ひとりで来るよ"
          },
          {
            "stringId": 1461742855669805056,
            "femaleVariant": "じゃあね",
            "maleVariant": "じゃあな"
          },
          {
            "stringId": 1521260301573025792,
            "femaleVariant": "持ってて。エヴリンのツケにしとくから"
          },
          {
            "stringId": 1521261569628565504,
            "femaleVariant": "BD用の携帯デバイスよ。あなたに合わせた設定をアップロードしてある"
          },
          {
            "stringId": 1521268990895845376,
            "femaleVariant": "そしてあんたは危険な橋を渡らなくて済むってわけね",
            "maleVariant": "そしてあんたは危険な橋を渡らなくて済むってわけか"
          },
          {
            "stringId": 1521282891473612800,
            "femaleVariant": "わかってるわ"
          },
          {
            "stringId": 1528438486239731716,
            "femaleVariant": "[立ち上がる]"
          },
          {
            "stringId": 1531603878123532288,
            "femaleVariant": "ここの機材には劣るけど、十分役に立つはず"
          },
          {
            "stringId": 1531611367338536960,
            "femaleVariant": "ジュディ… 大丈夫よ。そんなことにはならない"
          },
          {
            "stringId": 1531612400966037504,
            "femaleVariant": "ジュディ… 大丈夫よ。絶対バレたりしない"
          },
          {
            "stringId": 1531616201391337472,
            "femaleVariant": "男なんて、一度寝れば自分から色々喋ってくれるわ"
          },
          {
            "stringId": 1531616728195919872,
            "femaleVariant": "体だけの関係ってこと？",
            "maleVariant": "体だけの関係ってことか？"
          },
          {
            "stringId": 1540084100795854848,
            "femaleVariant": "キャッシュとあなたのデータは削除しとく。痕跡は一切残さない"
          },
          {
            "stringId": 1624111049163272192,
            "femaleVariant": "相談なんだけど、この仕事を、私から直に引き受ける気はない？ フィクサーを介さずに"
          },
          {
            "stringId": 1624112012007698432,
            "femaleVariant": "デクス抜きで"
          },
          {
            "stringId": 1624204244618440708,
            "femaleVariant": "考えておく"
          },
          {
            "stringId": 1624204831300907008,
            "femaleVariant": "デクスを裏切ることはできない"
          },
          {
            "stringId": 1624213629927288832,
            "femaleVariant": "フィクサーを裏切る気？",
            "maleVariant": "フィクサーを裏切る気か？"
          },
          {
            "stringId": 1624223442065289216,
            "femaleVariant": "もしこの話を受けたら… 必ずどこかでしわ寄せがくる"
          },
          {
            "stringId": 1624223538668498944,
            "femaleVariant": "そんなの無理。デクスは仲間よ。あたしを信用してくれてる",
            "maleVariant": "できない。デクスは仲間だ。俺を信用してくれてる"
          },
          {
            "stringId": 1624223636496445440,
            "femaleVariant": "フィクサーを裏切る気？ 何か怪しいと思ってたけど",
            "maleVariant": "フィクサーを裏切る気か？ 何か怪しいと思ってたが"
          },
          {
            "stringId": 1624237835289227264,
            "femaleVariant": "そうね。受ける受けないは別として、これはここだけの話よ"
          },
          {
            "stringId": 1624238629656850432,
            "femaleVariant": "あなたの取り分は50%。それだけあれば、何でも好きなモノを買えるわね"
          },
          {
            "stringId": 1624291085417836544,
            "femaleVariant": "その前に死ぬことになる。デクスが裏切りを許すわけがない"
          },
          {
            "stringId": 1624291795362508800,
            "femaleVariant": "フィクサーなら他にもいるわ。ただし、このオファーはこれっきりよ"
          },
          {
            "stringId": 1624296397621293056,
            "femaleVariant": "少し、考えさせて",
            "maleVariant": "少し考えさせてくれ"
          },
          {
            "stringId": 1624310363311435776,
            "femaleVariant": "だから私も誠意を示す。取り分として50%を渡すわ。それだけあれば、デクスはもう不要でしょ"
          },
          {
            "stringId": 1624312528612487172,
            "femaleVariant": "デクスを裏切ったりはしない"
          },
          {
            "stringId": 1624313197654306816,
            "femaleVariant": "考えさせて",
            "maleVariant": "考えさせてくれ"
          },
          {
            "stringId": 1624317066882969600,
            "femaleVariant": "エヴリン、これは金の問題じゃないの",
            "maleVariant": "エヴリン、これは金の問題じゃない"
          },
          {
            "stringId": 1624317609323917312,
            "femaleVariant": "あなた傭兵でしょ。金がすべてなんじゃないの"
          },
          {
            "stringId": 1624317930355945472,
            "femaleVariant": "悪いけど、信用を踏みにじるような真似はできない",
            "maleVariant": "悪いが、信用を踏みにじるような真似はできない"
          },
          {
            "stringId": 1624322597743882240,
            "femaleVariant": "そう。じゃあ忘れて。でもね、V"
          },
          {
            "stringId": 1624323098107572224,
            "femaleVariant": "なに？",
            "maleVariant": "なんだ？"
          },
          {
            "stringId": 1624323200415035392,
            "femaleVariant": "計画を実行するときは、そのウブさを捨てることね"
          },
          {
            "stringId": 1624327651813093376,
            "femaleVariant": "少し考えさせて",
            "maleVariant": "少し考えさせてくれ"
          },
          {
            "stringId": 1624344770697097216,
            "femaleVariant": "デクスはただの仲介役。それも今となっては役立たずのね"
          },
          {
            "stringId": 1624353950585712640,
            "femaleVariant": "決めるのはあなたよ"
          },
          {
            "stringId": 1624356027571499008,
            "femaleVariant": "本当にやるつもりなの？",
            "maleVariant": "本当にやるつもりなのか？"
          },
          {
            "stringId": 1624368769514729472,
            "femaleVariant": "上手く立ち回れば…"
          },
          {
            "stringId": 1624371996259905536,
            "femaleVariant": "私の番号を送るわ。心を決めたら連絡して"
          },
          {
            "stringId": 1624373632977989632,
            "femaleVariant": "わかった"
          },
          {
            "stringId": 1624373768386899968,
            "femaleVariant": "それじゃあね"
          },
          {
            "stringId": 1624374722725277696,
            "femaleVariant": "わかったわ"
          },
          {
            "stringId": 1631836209907625984,
            "femaleVariant": "エヴリン。ナイトシティに鉄則があるとすれば、それはフィクサーを裏切らないことよ",
            "maleVariant": "エヴリン。ナイトシティに鉄則があるとすれば、それはフィクサーを裏切らないことだ"
          },
          {
            "stringId": 1631838636060831744,
            "femaleVariant": "無駄よ。そんな過ちを犯したら、あんたの名は地に落ちる。この街じゃ生きていけなくなる",
            "maleVariant": "無駄だ。そんな過ちを犯したら、あんたの名は地に落ちる。この街じゃ生きていけなくなる"
          },
          {
            "stringId": 1634461398558437376,
            "femaleVariant": "本当に全て上手くいくの？",
            "maleVariant": "本当に全て上手くいくのか？"
          },
          {
            "stringId": 1634464108565360640,
            "femaleVariant": "ヨリノブとチップの情報、紺碧のBD… 風向きも良ければ計画も完璧。こんな仕事はそうそうあるもんじゃない"
          },
          {
            "stringId": 1634504061575884800,
            "femaleVariant": "それはどうも。でも続きがあるんでしょ"
          },
          {
            "stringId": 1634504834300899328,
            "femaleVariant": "話が出来過ぎてる。何か裏があるんじゃない？",
            "maleVariant": "話が出来過ぎてる。何か裏があるんじゃないか？"
          },
          {
            "stringId": 1634508587800326144,
            "femaleVariant": "あんたを信用していいのかわからない。抜け出せない泥沼に放り込まれるんじゃないかってね",
            "maleVariant": "あんたを信用していいのかわからない。抜け出せない泥沼に放り込まれるんじゃないかってな"
          },
          {
            "stringId": 1634511942186561536,
            "femaleVariant": "怖いの？ 降りたくなった？"
          },
          {
            "stringId": 1634528072590553088,
            "femaleVariant": "V。私にはあなたを陥れる理由なんてない。私たちは運命共同体よ。信じて"
          },
          {
            "stringId": 1679164748591656960,
            "femaleVariant": "ヨリノブは本当にあんたの動きに勘付いてないのね？",
            "maleVariant": "ヨリノブは本当にあんたの動きに勘付いてないんだな？"
          },
          {
            "stringId": 1679164748608434184,
            "femaleVariant": "ええ、何も気づいてない。今はネットウォッチの件で頭がいっぱいだから"
          },
          {
            "stringId": 1679164748608434196,
            "femaleVariant": "それに… もし疑われたって別に大した問題じゃないわ。彼が今から動き出したとしても間に合わないし、あなたなら何とかしてくれるでしょ？"
          },
          {
            "stringId": 1679164748692320256,
            "femaleVariant": "心配じゃないの？ アラサカはヨリノブと繋がりのある人間を片っ端から調べ上げるはずよ",
            "maleVariant": "心配じゃないのか？ アラサカはヨリノブと繋がりのある人間を片っ端から調べ上げるはずだ"
          },
          {
            "stringId": 1679164748692320268,
            "femaleVariant": "そんな人間はごまんといる。私なんか相手にならないビッグネームがズラリとね"
          },
          {
            "stringId": 1679164748692320280,
            "femaleVariant": "企業の大物や殺し屋と、ベッドの相手――アラサカがまず疑うのはどっちだと思う？"
          },
          {
            "stringId": 1679167788455424000,
            "femaleVariant": "自分の関与を疑われないか不安じゃないの？",
            "maleVariant": "自分の関与を疑われないか不安じゃないのか？"
          },
          {
            "stringId": 1679176160789524480,
            "femaleVariant": "ヨリノブには勘付かれてないのね？",
            "maleVariant": "ヨリノブには勘付かれてないんだな？"
          },
          {
            "stringId": 1733178507373780992,
            "femaleVariant": "健闘を祈るわ。私はジュディと話があるから"
          },
          {
            "stringId": 1733178555255955456,
            "femaleVariant": "じゃあね、V"
          },
          {
            "stringId": 1733178603373010944,
            "femaleVariant": "V、時間を無駄にしないで"
          },
          {
            "stringId": 1740254549625139200,
            "femaleVariant": "あとで2人で話しましょ"
          },
          {
            "stringId": 1740297433967620096,
            "femaleVariant": "V、ちょっといい？"
          },
          {
            "stringId": 1744387915588956160,
            "femaleVariant": "少し歩きましょ"
          },
          {
            "stringId": 1753402318679203840,
            "femaleVariant": "逆よ。それこそが命取りになるの"
          },
          {
            "stringId": 1753410787213860864,
            "femaleVariant": "ひとつの油断で、すべてが無に帰す。それがこの街よ"
          },
          {
            "stringId": 1753433381828845568,
            "femaleVariant": "それは自分に聞いた方がいいんじゃない？ この先ずっとギャングとケンカして暮らすつもり？ 一晩で伝説になりたくはない？"
          }
        ]
      }
    }
  }
}