        "entries": [
          {
            "stringId": 1498219624757157888,
            "femaleVariant": "会って話そう"
          },
          {
            "stringId": 1498223335105585152,
            "femaleVariant": "会ってくれない？ 直接話がしたいの",
            "maleVariant": "会ってくれないか？ 直接話がしたい"
          },
          {
            "stringId": 1498226586379051008,
            "femaleVariant": "他に頼る相手がいないの",
            "maleVariant": "他に頼る相手がいない"
          },
          {
            "stringId": 1498230005340790784,
            "femaleVariant": "ジュディ、頼れるのはあんただけなの。お願いよ",
            "maleVariant": "ジュディ、頼れるのはあんたしかいない。お願いだ"
          },
          {
            "stringId": 1498232238035918848,
            "femaleVariant": "リジーズ・バーでしょ？",
            "maleVariant": "リジーズ・バーだな？"
          },
          {
            "stringId": 1498236672203776000,
            "femaleVariant": "どうしてもエヴリンを見つけたい。力を貸して",
            "maleVariant": "どうしてもエヴリンを見つけたい。力を貸してくれ"
          },
          {
            "stringId": 1498237270328303616,
            "femaleVariant": "自分勝手な奴"
          },
          {
            "stringId": 1671985289598033920,
            "femaleVariant": "エヴリンと話がしたい"
          },
          {
            "stringId": 1671985289598033936,
            "femaleVariant": "あたしもよ",
            "maleVariant": "俺もだ"
          },
          {
            "stringId": 1671985289598033952,
            "femaleVariant": "紺碧プラザの件、聞いてるのね",
            "maleVariant": "紺碧プラザの件、聞いてるんだな"
          },
          {
            "stringId": 1671988053610815488,
            "femaleVariant": "V…？ あなたなの？"
          },
          {
            "stringId": 1671988053627592708,
            "femaleVariant": "てっきり… 死んだかと思ってた"
          },
          {
            "stringId": 1671994615700086784,
            "femaleVariant": "あたしだって不思議なくらいよ",
            "maleVariant": "俺だって不思議なくらいだ"
          },
          {
            "stringId": 1672009805841784856,
            "femaleVariant": "聞いて、エヴリンを探してるの",
            "maleVariant": "聞いてくれ、エヴリンを探してる"
          },
          {
            "stringId": 1672009805841784868,
            "femaleVariant": "彼女の話はしたくない…"
          },
          {
            "stringId": 1672009805858562048,
            "femaleVariant": "居場所を知らない？",
            "maleVariant": "居場所を知らないか？"
          },
          {
            "stringId": 1672009805858562060,
            "femaleVariant": "話聞いてる？ したくないって言ってるでしょ"
          },
          {
            "stringId": 1672021056273735680,
            "femaleVariant": "もうおしまいだと思ったもの",
            "maleVariant": "もうおしまいだと思ったよ"
          },
          {
            "stringId": 1672025339731861504,
            "femaleVariant": "紺碧の件、聞いたのね",
            "maleVariant": "紺碧の件、聞いたんだな"
          },
          {
            "stringId": 1672025339731861516,
            "femaleVariant": "話したのはエヴリン？",
            "maleVariant": "話したのはエヴリンか？"
          },
          {
            "stringId": 1672030647170588672,
            "femaleVariant": "聞くまでもない。街中の噂になってる"
          },
          {
            "stringId": 1672034065796784128,
            "femaleVariant": "要は計画通りにいかなかったのね"
          }
        ]
      }
