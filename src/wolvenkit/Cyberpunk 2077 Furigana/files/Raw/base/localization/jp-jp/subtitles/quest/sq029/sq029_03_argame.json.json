  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドリアン！ 遊ぼ！",
              "maleVariant": "",
              "stringId": "1349876033512456192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[装着する]",
              "maleVariant": "",
              "stringId": "1349879738223738880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう降参する？",
              "maleVariant": "もう降参するか？",
              "stringId": "1623958472080146432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おっそ！ ドーナツ食べすぎだぞ！",
              "maleVariant": "",
              "stringId": "1623960100980047872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やるじゃないか！",
              "maleVariant": "",
              "stringId": "1623966447297544192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "僕のほうがエイムは上だ！",
              "maleVariant": "",
              "stringId": "1623966447364653056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ終わってないぞ！",
              "maleVariant": "",
              "stringId": "1775182974196781056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだまだ！ 見ててよ！",
              "maleVariant": "",
              "stringId": "1775185292388601856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うん！ “ヘイウッドハンターズ”だ！",
              "maleVariant": "",
              "stringId": "1905474276884090880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "準備してね",
              "maleVariant": "",
              "stringId": "1905475399481167872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おおっと、すごいのを持ってるな",
              "maleVariant": "",
              "stringId": "2014421926275067904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "集中して。悪いヤツを逃がすわけにはいかないんだから！",
              "maleVariant": "",
              "stringId": "2014422772064858112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え？ ああ… 似合ってるじゃない",
              "maleVariant": "え？ ああ… 似合ってるじゃないか",
              "stringId": "2014423898420031488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、紹介しよう。ジョアン・マクレーン警部とヘンリー・キャラハン警部補だ",
              "maleVariant": "",
              "stringId": "2014424072651419648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2人はこの管区における精鋭さ。足を引っ張るなよ",
              "maleVariant": "",
              "stringId": "2014426417955221504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ始めましょ",
              "maleVariant": "じゃあ始めよう",
              "stringId": "2014427450978742272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゲームについて知っておくべきことは？",
              "maleVariant": "",
              "stringId": "2014427708878106624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一匹狼ね？ そういうタイプなら知ってる",
              "maleVariant": "一匹狼だな？ そういうタイプなら知ってる",
              "stringId": "2014427930354135040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゲームのことを、手短に教えてくれる？",
              "maleVariant": "ゲームのことを、手短に教えてくれるか？",
              "stringId": "2014433944382091264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういう警官なら聞いたことがある。一匹狼タイプね",
              "maleVariant": "そういう警官なら聞いたことがある。一匹狼タイプか",
              "stringId": "2014433982533480448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃあ、さっそく始めましょ",
              "maleVariant": "それじゃあ、さっそく始めよう",
              "stringId": "2014434519387615232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は誰とも組まないんだ。たとえ相手がお前でもそれは変わらない",
              "maleVariant": "",
              "stringId": "2015374485680697344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その通り。上司にとっては悩みの種。自分のルールにのみ従う厄介者さ",
              "maleVariant": "",
              "stringId": "2015376828618911744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいか。目標はエル・チャムコ・エンディアブラドを見つけ出すことだ。邪魔をする手下どもは排除しろ",
              "maleVariant": "",
              "stringId": "2015380893050482688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "報いを受けさせるんだ",
              "maleVariant": "",
              "stringId": "2015394490145189888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちと子供チームの2つに別れてプレイする。だが目指すゴールは同じだ",
              "maleVariant": "",
              "stringId": "2015396368706842624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スコアの高かったチームが勝利する",
              "maleVariant": "",
              "stringId": "2015396842763857920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "絶対に負けないもんね",
              "maleVariant": "",
              "stringId": "2015404544495857664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いたぞ！ 仕留めろ！",
              "maleVariant": "",
              "stringId": "2015424302519824384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すばしっこい奴だが、正義の裁きからは逃げられない",
              "maleVariant": "",
              "stringId": "2015425392099340288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "危ない！ 敵の仲間よ！",
              "maleVariant": "",
              "stringId": "2015425963598426112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちだ。きっと連中のアジトがあるはず",
              "maleVariant": "",
              "stringId": "2015427046769045504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふむ… 妙に静かね",
              "maleVariant": "",
              "stringId": "2015428202618564608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "罠だ！ 待ち伏せされてたんだ！",
              "maleVariant": "",
              "stringId": "2015428464091475968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きゃああ！ 後ろ！",
              "maleVariant": "",
              "stringId": "2015430390401085440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そろそろね",
              "maleVariant": "",
              "stringId": "2015460557546729472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心配は要らないぜ",
              "maleVariant": "",
              "stringId": "2015465646562881536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どきたまえ！ 公務執行妨害は罰則の対象だぞ！",
              "maleVariant": "",
              "stringId": "2015466841167769600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、みんな倒した",
              "maleVariant": "",
              "stringId": "2015467121666043904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、全部倒した。でも次はもっと早く動かないと",
              "maleVariant": "",
              "stringId": "2015467135859568640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "次のステージ！",
              "maleVariant": "",
              "stringId": "2015467147972718592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これが最後の警告よ！",
              "maleVariant": "",
              "stringId": "2015471807458586624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これは失礼、警部殿！ すぐに行きますよ",
              "maleVariant": "",
              "stringId": "2015472012526497792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここは俺の街だ。貴様には渡さん",
              "maleVariant": "",
              "stringId": "2015474987630211072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここまでか…",
              "maleVariant": "",
              "stringId": "2015492237108064256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ま、大人たちには勝てたし！",
              "maleVariant": "",
              "stringId": "2015492280577830912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スコアを見た？ 最悪だよ",
              "maleVariant": "",
              "stringId": "2015492305793986560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "観念したか。何か言い残したことがあれば聞いてやる",
              "maleVariant": "",
              "stringId": "2015493313483911168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、じゃあ…",
              "maleVariant": "",
              "stringId": "2015495681386622976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お昼ごはんよ！",
              "maleVariant": "",
              "stringId": "2015498188842217472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最強チームよ！",
              "maleVariant": "",
              "stringId": "2015557248283193344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もうこんなゲームやりたくない",
              "maleVariant": "",
              "stringId": "2015560018537099264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何言ってんだ！ 2人とも惜しかったじゃないか！",
              "maleVariant": "",
              "stringId": "2015560257142665216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "舞台となる巨大都市は、犯罪で溢れてるの。外は“無法地帯”と化してる",
              "maleVariant": "",
              "stringId": "2015581983301840896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "凶悪な奴なんだ！ でも俺たちなら倒せるよ！",
              "maleVariant": "",
              "stringId": "2015582528779464704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "街を支配する悪者をやっつけましょ",
              "maleVariant": "",
              "stringId": "2015584301527216128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだな… これまでに停職1回、退職2回、減俸は3回ある",
              "maleVariant": "",
              "stringId": "2015585995354943488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい加減、やめ時さ",
              "maleVariant": "",
              "stringId": "2015587080874373120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ところがいつも何かに引き戻される",
              "maleVariant": "",
              "stringId": "2015588022847942656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "街が呼んでるんだ",
              "maleVariant": "",
              "stringId": "2015588557537816576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも気を抜くな…",
              "maleVariant": "",
              "stringId": "2015707616141660160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "腐ったモラルの臭いがプンプンするぜ",
              "maleVariant": "",
              "stringId": "2015713608979992576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やってみましょ",
              "maleVariant": "まあやってみよう",
              "stringId": "2016921134153093120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうだ？ 楽しめそうか？",
              "maleVariant": "",
              "stringId": "2016923476051120128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こういうのやってみたかったの",
              "maleVariant": "こういうのやってみたかったんだ",
              "stringId": "2016924550900240388"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "たぶんね",
              "maleVariant": "たぶんな",
              "stringId": "2016925417762213888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こういうゲーム、やってみたかったの",
              "maleVariant": "こういうゲーム、やってみたかったんだ",
              "stringId": "2016932701070446592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、私だって",
              "maleVariant": "",
              "stringId": "2016960798662610944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私たちは警察なんだけど、みんなそれぞれ、暗い過去を抱えてるのよ",
              "maleVariant": "",
              "stringId": "2016980398091554816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも… この街で、エル・チャムコ・エンディアブラドに立ち向かえるのは、もう私たちしか残されてない",
              "maleVariant": "",
              "stringId": "2016981975032418304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手加減してやれよ？ あいつら、負けず嫌いだからな",
              "maleVariant": "",
              "stringId": "2017103327353655296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ",
              "maleVariant": "ああ",
              "stringId": "2017106609530753024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハハハ！",
              "maleVariant": "",
              "stringId": "2017131864424992768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無理に突っ込まないで、キャラハン。あなたを失うわけにはいかない",
              "maleVariant": "",
              "stringId": "2017149925802659840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここまでなのは貴様らさ！ ハハハ！",
              "maleVariant": "",
              "stringId": "2017157123328987136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "嘘だろ… また待ち伏せ？",
              "maleVariant": "",
              "stringId": "2017165724185546752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなあ。あと少しだったのに！",
              "maleVariant": "",
              "stringId": "2017167182880591872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またやればいいでしょ",
              "maleVariant": "",
              "stringId": "2017167447155298304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手加減してくれてありがとな。あいつらも喜んでる",
              "maleVariant": "",
              "stringId": "2017168703080910848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "勝たせてやるべきだったな。あいつらはこのゲームに入れ込んでるんだ",
              "maleVariant": "",
              "stringId": "2017172523588538368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね。つい夢中になっちゃって",
              "maleVariant": "そうだな。つい夢中になっちゃって",
              "stringId": "2017237158752702464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "たかがゲームでしょ",
              "maleVariant": "たかがゲームだろ",
              "stringId": "2017237929363787776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね、ごめん。つい夢中になっちゃって",
              "maleVariant": "そうだな、すまん。つい夢中になっちゃって",
              "stringId": "2017238361628758016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあいいさ… 一応公平な勝負はできたわけだし",
              "maleVariant": "",
              "stringId": "2017239054091571200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何よ、たかがゲームでしょ",
              "maleVariant": "おいおい、たかがゲームだろ",
              "stringId": "2017241370471452672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん… まあな",
              "maleVariant": "",
              "stringId": "2017243888849014784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "当然でしょ。いい子たちね",
              "maleVariant": "当然だろ。いい子たちだな",
              "stringId": "2017244702963421188"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真剣勝負で負けただけよ",
              "maleVariant": "真剣勝負で負けただけだ",
              "stringId": "2017245355328688128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "当然でしょ。いい子たちね",
              "maleVariant": "当然だろ。いい子たちだな",
              "stringId": "2017245841733734400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前のことを気に入ったみたいだ",
              "maleVariant": "",
              "stringId": "2017246390784905216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え？ 普通にやって負けただけよ",
              "maleVariant": "え？ 普通にやって負けただけだ",
              "stringId": "2017247189413941248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やった！ 倒した！",
              "maleVariant": "",
              "stringId": "2018570928801992712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "食らえ！",
              "maleVariant": "",
              "stringId": "2018571465320583180"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ん？ どうしたんだ、V？ 子供たちが待ってるぞ",
              "maleVariant": "",
              "stringId": "2258152387208888320"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}