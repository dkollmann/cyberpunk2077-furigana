  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなアメリカはもはや遠い過去の話さ。「むかしむかし」なんて前置きがつくぐらいにな",
                    "maleVariant": null,
                    "stringId": 1685306645375897600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だから、それが問題なんだよ。どこへ行こうと俺たちはよそ者だ。今や全ての州が事実上の国と化してる",
                    "maleVariant": null,
                    "stringId": 1685306645375897612
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "統一なんて、実体のないまやかしに過ぎない。要は心構え次第さ。今、自分がここに立ってるってことがわかりゃ十分だ",
                    "maleVariant": null,
                    "stringId": 1685306645375897624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こう考えたらどうだ。人生に期待し、幻想を抱かなけりゃ、失望することもない。それこそがきっと、本当の自由ってもんだ",
                    "maleVariant": null,
                    "stringId": 1685306645392674816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうも",
                    "maleVariant": null,
                    "stringId": 1774778002600415236
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これって…？",
                    "maleVariant": null,
                    "stringId": 1774842013752807424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうだ。アフターバーナーとRPGランチャーを上に取り付けた",
                    "maleVariant": null,
                    "stringId": 1774842317235867648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "スピードと機動性が上がったってこと？",
                    "maleVariant": "スピードと機動性が上がったってことか",
                    "stringId": 1774842457325621248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "操縦はお前に任せる。パナムはガンナーだ",
                    "maleVariant": null,
                    "stringId": 1774842638150455296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今すぐ乗ってみたい",
                    "maleVariant": null,
                    "stringId": 1774870728243204096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "決行は明日に迫ってるし、これから試乗に出るのはどう？ 感覚をつかんでおきたい",
                    "maleVariant": "決行は明日に迫ってるし、これから試乗に出るのはどうだ？ 感覚をつかんでおきたいしな",
                    "stringId": 1774888574469853188
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあ、こいつを見な",
                    "maleVariant": null,
                    "stringId": 1811031929171791872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "戦争を始めるんだってな",
                    "maleVariant": null,
                    "stringId": 1814186831775584256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アフターバーナーの性能は？",
                    "maleVariant": null,
                    "stringId": 1814267727065174016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "兵装について教えて",
                    "maleVariant": "兵装について教えてくれ",
                    "stringId": 1814267775148675072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アフターバーナーは普通のニトロ？ それとも軍用品？",
                    "maleVariant": "アフターバーナーは普通のニトロ？ それとも軍用品か？",
                    "stringId": 1814268025984831488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "兵装について詳しく教えて",
                    "maleVariant": "兵装について詳しく教えてくれ",
                    "stringId": 1814268179479580672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もちろんだ。でも日が落ちてからにしてくれ。まだ調整の甘い部分が残っててな",
                    "maleVariant": null,
                    "stringId": 1814268851809734656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "普通のじゃ、こいつは微塵も動かねえよ",
                    "maleVariant": null,
                    "stringId": 1814271259289899008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "連射式の70ミリマルチロケットランチャー。MMIも搭載済み、パーソナルリンクで同期可能だ",
                    "maleVariant": null,
                    "stringId": 1814272452485500928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どこで手に入れたの？",
                    "maleVariant": "どこで手に入れたんだ？",
                    "stringId": 1814272895420780544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "テディーがスクラップ場で見つけてきたのさ。ランチャー自体は問題ない。弾がなかったんだが――",
                    "maleVariant": null,
                    "stringId": 1814272925602992128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "幸い、ダコタがいくつか調達してくれた。ルートは謎だがな",
                    "maleVariant": null,
                    "stringId": 1814273023196057600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "てことは、亜酸化窒素噴射式の軍用品ね",
                    "maleVariant": "てことは、亜酸化窒素噴射式の軍用品か",
                    "stringId": 1835602375963627520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ軍用品ね。でも一体どうやってパンツァーに組み込んだの？",
                    "maleVariant": "じゃあ軍用品だな。だが、一体どうやってパンツァーに組み込んだんだ？",
                    "stringId": 1835603927252127748
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "明日この獣を解き放ったら、よくわかるさ",
                    "maleVariant": null,
                    "stringId": 1835605781335822336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "みんな",
                    "maleVariant": null,
                    "stringId": 1835622110902145024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なに… 今すぐ？ 了解",
                    "maleVariant": null,
                    "stringId": 1835625970433576980
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ソウルに呼ばれた",
                    "maleVariant": null,
                    "stringId": 1835625970433576992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自殺行為の間違いだろ",
                    "maleVariant": null,
                    "stringId": 1835626771092656128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいから早く片付けろ",
                    "maleVariant": null,
                    "stringId": 1835628640443617280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "“甘い”どころじゃないだろ",
                    "maleVariant": null,
                    "stringId": 1835636192405409792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ケチつけるしか能がないのか？ 必ず間に合わせるさ",
                    "maleVariant": null,
                    "stringId": 1835636620425744384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それが自由なら、なぜこんなに息の詰まるような、苦しい思いをしなきゃならない？",
                    "maleVariant": null,
                    "stringId": 1882109748257091584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だから心構え次第だって。まあ、大気汚染の可能性も否めないが",
                    "maleVariant": null,
                    "stringId": 1882121197582938112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ラフェンの装甲車も、コンボイを襲う時に同じ改造をするって聞いた。不安定らしいけど",
                    "maleVariant": "ラフェンの装甲車も、コンボイを襲う時に同じ改造をするって聞いた。不安定らしいが",
                    "stringId": 1882152793207861248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ただの装甲車とパンツァーじゃ、月とスッポンだ",
                    "maleVariant": null,
                    "stringId": 1882164037734232064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "戦争中にスコーピオンとやった改造に比べりゃ、大したことねえよ",
                    "maleVariant": null,
                    "stringId": 1882165829993885696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "準備は順調？",
                    "maleVariant": "準備は順調か？",
                    "stringId": 1882175668337897472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "準備は順調？",
                    "maleVariant": "準備は順調か？",
                    "stringId": 1882177413571014656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハッ、準備だって？ 俺に言わせりゃ、即興でなんとか間に合わせてるだけさ",
                    "maleVariant": null,
                    "stringId": 1882259641055440896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うるさいよ、ボブ",
                    "maleVariant": null,
                    "stringId": 1882260791452364800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あとで探しにきて",
                    "maleVariant": null,
                    "stringId": 1882302253808349184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった",
                    "maleVariant": null,
                    "stringId": 1882303799979798528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おまけに総質量の約60%を削ってある。なかなかのモンだろ？",
                    "maleVariant": null,
                    "stringId": 1938639738339192832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、質問はあるか？ 遠慮しなくてもいいぞ",
                    "maleVariant": null,
                    "stringId": 2236570805456359424
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
