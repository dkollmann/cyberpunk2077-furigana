  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何してる？ こっちに戻れ！",
              "maleVariant": "",
              "stringId": "1650550777023840256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずは偵察を終わらせるんだ！",
              "maleVariant": "",
              "stringId": "1650566746131677184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや… もう遅い！",
              "maleVariant": "",
              "stringId": "1650568890696089600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "来い",
              "maleVariant": "",
              "stringId": "1650570808784863232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちはツイてるな。どうやら、今日は誰も人がいないようだ",
              "maleVariant": "",
              "stringId": "1650583235081551872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[偵察を終える] 十分でしょ",
              "maleVariant": "[偵察を終える] 十分だろう",
              "stringId": "1654808777535090692"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゲートのそばにタレット",
              "maleVariant": "",
              "stringId": "1654809715515682816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこを通るのは最後の手段だな",
              "maleVariant": "",
              "stringId": "1654810085805617152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "壁のそばに変圧器",
              "maleVariant": "",
              "stringId": "1654816514851565568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "警備隊を乗せたアラサカのSUV",
              "maleVariant": "",
              "stringId": "1654816658279985152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのパイプが見える？",
              "maleVariant": "あのパイプが見えるか？",
              "stringId": "1654816681566760960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんなところね…",
              "maleVariant": "こんなところだな…",
              "stringId": "1660310067094581248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分からついて来たんだろ。ならちゃんと仕事をしろ",
              "maleVariant": "",
              "stringId": "1660312976733597696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もうすることはない",
              "maleVariant": "",
              "stringId": "1660313572861636612"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[偵察を続ける] もう少し調べてみる",
              "maleVariant": "",
              "stringId": "1660313878139858944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかったって。もう一度見てみるから",
              "maleVariant": "わかったよ。もう一度見ればいいんだろ",
              "stringId": "1660314202577661952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もうすることはないってば",
              "maleVariant": "もうすることはないって",
              "stringId": "1660314820247646208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、目的は果たせた",
              "maleVariant": "",
              "stringId": "1660315328429518848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん、美しい眺めだと言いたいところだが…",
              "maleVariant": "",
              "stringId": "1680620426843144192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし。それでは、お前は警備の弱点を探ってくれ。俺は陽動の方法を考えることにする",
              "maleVariant": "",
              "stringId": "1680621120950124544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "重装甲の扉ね",
              "maleVariant": "重装甲の扉だ",
              "stringId": "1681008278714204160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "道の脇にメンテ用トンネル",
              "maleVariant": "",
              "stringId": "1682352937914875904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なかなかやるな",
              "maleVariant": "",
              "stringId": "1683875873238827008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、偵察はこれで十分だ",
              "maleVariant": "",
              "stringId": "1683876900876861440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "監視に適した、格好の場所を見つけた",
              "maleVariant": "",
              "stringId": "1712753113502334976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "我々の計画をアラサカに悟られるわけにはいかない。ちょっとばかし騒ぎを起こして、奴らの注意をそらしてやる必要がある",
              "maleVariant": "",
              "stringId": "1712820608846282752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "通信システムを見つけた。警備が多い",
              "maleVariant": "",
              "stringId": "1712877971137642496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょうどいい、陽動に使わせてもらおう",
              "maleVariant": "",
              "stringId": "1712877971137642504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが？",
              "maleVariant": "",
              "stringId": "1720132378855084032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "情緒というものが全くない",
              "maleVariant": "",
              "stringId": "1720132478981509120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "重装備だ。近づかないのが賢明だな",
              "maleVariant": "",
              "stringId": "1720156845840928768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "倉庫まで繋がってるかもな",
              "maleVariant": "",
              "stringId": "1720160241281011712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこから上に登れそうだ",
              "maleVariant": "",
              "stringId": "1720163643733970944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう少し役に立つと思っていたんだが",
              "maleVariant": "",
              "stringId": "1720171438764068864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "入手できた情報を分析するとしよう",
              "maleVariant": "",
              "stringId": "1720183468782915584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンテナがある。セキュリティハブはあの建物の中ね",
              "maleVariant": "アンテナがある。セキュリティハブはあの建物の中か",
              "stringId": "1722935226277052416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "破壊工作にうってつけだ！",
              "maleVariant": "",
              "stringId": "1722943318414868480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前は左から監視についてくれ",
              "maleVariant": "",
              "stringId": "1740045371480829952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺はここからだ。このスコープが役に立つといいが",
              "maleVariant": "",
              "stringId": "1740045927628763136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "守衛の話が聞こえた。何かヘマをして、処分を恐れてるみたい",
              "maleVariant": "守衛の話が聞こえた。何かヘマをして、処分を恐れてるようだ",
              "stringId": "1740076845907144704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何だか、ずいぶんと焦ってる奴がいた",
              "maleVariant": "",
              "stringId": "1740078541177712640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とはいえ、どうしようもない。ここの勝手も知らないしな",
              "maleVariant": "",
              "stringId": "1740078906753249280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜこの端末から掛けてると思う？",
              "maleVariant": "",
              "stringId": "1740085235270119424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前んとこのランナー、何もかも全部消すことになってたのに――",
              "maleVariant": "",
              "stringId": "1740085664884289536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "システムログを調べられたら、俺は防諜部の奴らに殺される",
              "maleVariant": "",
              "stringId": "1740085920317403136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、聞いてるのか！？",
              "maleVariant": "",
              "stringId": "1740086140417699840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その使いと名乗って、脅すのはどう？ コーポ相手の攻め方なら熟知してる",
              "maleVariant": "その使いと名乗って、脅すのはどうだ？ コーポ相手の攻め方なら熟知してる",
              "stringId": "1740086682120450048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あそこから中に入れるかもな",
              "maleVariant": "",
              "stringId": "1744704944005320704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "守衛か？",
              "maleVariant": "",
              "stringId": "1764893628805550080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "戦場に子供を連れてきた俺の責任か…",
              "maleVariant": "",
              "stringId": "1770585507924676608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "面白い考えだ",
              "maleVariant": "",
              "stringId": "1770603989923926016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前は動くな。今そっちに行く！",
              "maleVariant": "",
              "stringId": "1771006970582405120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分のだけを消去してったんだよ！",
              "maleVariant": "",
              "stringId": "1773253519043596288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それが何の役に立つ？",
              "maleVariant": "",
              "stringId": "1773289084677918720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティで防諜を統括してる、アバナシーって女を知ってる",
              "maleVariant": "",
              "stringId": "1773289253003726848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "定期的にトラックが来てる",
              "maleVariant": "",
              "stringId": "1785237707065147392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハイジャックする手もあるな…",
              "maleVariant": "",
              "stringId": "1785238588607184896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "歩道橋は警備されてる",
              "maleVariant": "",
              "stringId": "1827042546073747456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他の経路が見つからなければ、そこを使おう",
              "maleVariant": "",
              "stringId": "1827043150523285504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[寄りかかる] 始めましょ",
              "maleVariant": "[寄りかかる] 始めよう",
              "stringId": "1906771210339893252"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "準備はいいか？",
              "maleVariant": "",
              "stringId": "1906771418025050112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪いけど、ピザを買ってきて",
              "maleVariant": "悪いが、ピザを頼めるか",
              "stringId": "1906775303242399744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ピザでも買って来てくれない？",
              "maleVariant": "ピザでも買って来てくれないか？",
              "stringId": "1906775561846407168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "始めましょ",
              "maleVariant": "始めようか",
              "stringId": "1906776545276477440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここを登るんだ",
              "maleVariant": "",
              "stringId": "1908139041174339584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちだ",
              "maleVariant": "",
              "stringId": "1908140258361360384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何をしている？",
              "maleVariant": "",
              "stringId": "1908141579063803904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、手間を取らせるな",
              "maleVariant": "",
              "stringId": "1908161235300401152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "断る。ファストフードは食べない主義だ",
              "maleVariant": "",
              "stringId": "1915814116827406336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アンテナが3つあるだろう？",
              "maleVariant": "",
              "stringId": "1994036756080508928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "破壊工作にうってつけだ。通信を断てば連中は…",
              "maleVariant": "",
              "stringId": "1994037554340446208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大混乱さ",
              "maleVariant": "",
              "stringId": "1996781339317968896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "通用口はふさがれてる",
              "maleVariant": "",
              "stringId": "2005488461689618436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゲートは守備が堅い",
              "maleVariant": "",
              "stringId": "2005494037748350980"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何かに怯えてる守衛がいた",
              "maleVariant": "",
              "stringId": "2005497001275785220"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの変圧器…",
              "maleVariant": "",
              "stringId": "2005505303497900036"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あれはメンテナンス用のトンネル？",
              "maleVariant": "あれはメンテナンス用のトンネルか？",
              "stringId": "2005510545153163264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカのSUV…",
              "maleVariant": "",
              "stringId": "2005531173109334020"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのパイプ…",
              "maleVariant": "",
              "stringId": "2005534128801808388"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "通信システム",
              "maleVariant": "",
              "stringId": "2005577997614546948"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "歩道橋…",
              "maleVariant": "",
              "stringId": "2005585227302797316"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "セキュリティハブはあの建物の中ね",
              "maleVariant": "セキュリティハブはあの建物の中だな",
              "stringId": "2005587993261735940"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "定期的にトラックが来てる",
              "maleVariant": "",
              "stringId": "2005609569046724608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "時間と材料さえあれば、おにぎりでも作って持ってきたんだがな。具は… タラコか焼き鮭か…",
              "maleVariant": "",
              "stringId": "2012720965190754304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、ここは原点に帰って梅干しでもいい。梅干しはサブロウ様の大好物だった。シンプルだがうまくて、腹持ちもいい",
              "maleVariant": "",
              "stringId": "2012729391329386496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ある時、サブロウ様が真剣に召し上がっていらっしゃったものがあってな",
              "maleVariant": "",
              "stringId": "2012743383091322880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "プロテインバーだ",
              "maleVariant": "",
              "stringId": "2012755587677102080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サブロウ・アラサカがプロテインバー？ ウソでしょ…",
              "maleVariant": "サブロウ・アラサカがプロテインバー？ ウソだろ…",
              "stringId": "2012756024304148480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真の戦士だ",
              "maleVariant": "",
              "stringId": "2012756510994407424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "食い物の話はこの辺りにしておこう。腹が減ってくる",
              "maleVariant": "",
              "stringId": "2012758565465169920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなにナイトシティが嫌い？",
              "maleVariant": "そんなにナイトシティが嫌いか？",
              "stringId": "2012761440408903680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや… 英語で言うところの“ホームシック”かもしれんな",
              "maleVariant": "",
              "stringId": "2012761996053520384"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}