                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、手間を取らせるな",
                    "maleVariant": null,
                    "stringId": 1908161235300401152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
