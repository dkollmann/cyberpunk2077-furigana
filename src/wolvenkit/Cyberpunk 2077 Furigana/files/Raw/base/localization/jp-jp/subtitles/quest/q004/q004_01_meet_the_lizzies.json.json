          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って。まずはルールの説明。無許可の録画行為は厳罰の対象よ…",
              "maleVariant": "",
              "stringId": 1509626699235123200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あら、かわいらしい子",
              "maleVariant": "どうしたの、浮かない顔して",
              "stringId": 1509962807185694736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とっておきのBDでもどう？",
              "maleVariant": "",
              "stringId": 1509965594418143232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ぜひお願い",
              "maleVariant": "ぜひ頼む",
              "stringId": 1509978411774959616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブレインダンス？ どんなのがあるの？",
              "maleVariant": "ブレインダンス？ どんなのがある？",
              "stringId": 1509978411774959632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、面白そうね",
              "maleVariant": "ああ、面白そうだな",
              "stringId": 1509980129745100800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドラッグとお触りは禁止",
              "maleVariant": "",
              "stringId": 1510019729276461056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リジーズへようこそ！",
              "maleVariant": "",
              "stringId": 1512800407319773184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "物騒なもんはしまって。ケガしたくないでしょ？",
              "maleVariant": "",
              "stringId": 1519832398829486080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、ここはそういう場所じゃないよ。武器をしまいな",
              "maleVariant": "",
              "stringId": 1519833855796477952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またおいで！",
              "maleVariant": "",
              "stringId": 1520023113446490128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウィンドウショッピングもほどほどにね",
              "maleVariant": "",
              "stringId": 1520025029857542144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なによ…",
              "maleVariant": "",
              "stringId": 1520046617571131408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、そこで止まって",
              "maleVariant": "",
              "stringId": 1520138799363686400
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダメダメ！ ちょっと待ちなさい",
              "maleVariant": "",
              "stringId": 1520140331492605952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "楽しんでらっしゃい",
              "maleVariant": "",
              "stringId": 1521465399582519296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何があるの？",
              "maleVariant": "何があるんだ？",
              "stringId": 1529834353161068544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "耐える自信ある？",
              "maleVariant": "",
              "stringId": 1531294250693160960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろん",
              "maleVariant": "",
              "stringId": 1531295010835263492
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はやめとく",
              "maleVariant": "今はやめておく",
              "stringId": 1531296846296248320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何がって？ シナプスにビンビン来る、Aランクの美男美女が目白押しさ！ しけたバーチャルポルノのブサイクどもとはわけが違う",
              "maleVariant": "",
              "stringId": 1531299878006288384
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カスタムBDか… 確かに魅力的だね",
              "maleVariant": "カスタムBDか… 確かに魅力的だ",
              "stringId": 1531301556432531456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあルールの説明ね",
              "maleVariant": "",
              "stringId": 1531302039146590208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪いけど、今はやめとく",
              "maleVariant": "悪いが、今はやめておく",
              "stringId": 1531341541688864768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう、じゃあね",
              "maleVariant": "",
              "stringId": 1531341911827804160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "口が上手いのね",
              "maleVariant": "なかなか口が上手いな",
              "stringId": 1550497689960574976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハートをがっちり掴まれ、神経もブッ飛ぶ極上の体験。ピュアなデジタルエクスタシー。一度やったらヤミツキよ",
              "maleVariant": "",
              "stringId": 1631456573151375360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんな初心者に見える？ わかってるって",
              "maleVariant": "そんな初心者に見えるか？ わかってるって",
              "stringId": 1634319266849878016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あらそう。じゃどうぞ",
              "maleVariant": "",
              "stringId": 1634319392544780288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "売り文句じゃなくて本気の警告。うちのは完全カスタムだから、シナプスごと“イッちゃう”かも",
              "maleVariant": "",
              "stringId": 1638817529158299648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっぱ、やりたくなった？",
              "maleVariant": "",
              "stringId": 1654670027243855872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無許可の録画行為は厳罰の対象よ…",
              "maleVariant": "",
              "stringId": 1715427494073069568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気になる子がいたら、直接声をかけずにカタログで探す。その子のBDを受け取ったら、ボックスに入る",
              "maleVariant": "",
              "stringId": 1751622272536829952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "前にも来たことがある",
              "maleVariant": "",
              "stringId": 1774845637731938304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "前に来たことがある",
              "maleVariant": "",
              "stringId": 1774846429314543616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ルールは簡単。気になる子がいたら、直接声をかけずにカタログで探す。その子のBDを受け取ったら、ボックスに入る",
              "maleVariant": "",
              "stringId": 1774848312053723136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正面から入ってないから",
