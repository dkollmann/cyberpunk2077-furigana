  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よう、あんた！ ブレインダンスは好きかい？ 史上最高にヤバくて強烈なヤツがあるんだ！ オプティクスの保養に最高だぜ！",
              "maleVariant": "",
              "stringId": "1811307858842497024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティの最新バーチャだ――しかも激安！",
              "maleVariant": "",
              "stringId": "1811313706272698368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今ならタイムセール！ あんただけの特別価格だぜ！",
              "maleVariant": "",
              "stringId": "1811315154817527808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "特におすすめはコイツだ。ダイブしなきゃマジで損するぜ",
              "maleVariant": "",
              "stringId": "1811315449442217984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんなBDなの？",
              "maleVariant": "どんなBDなんだ？",
              "stringId": "1811322854150385668"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんなBDなの？",
              "maleVariant": "どんなBDなんだ？",
              "stringId": "1811324676122488832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞いて驚くな、この街一番の最高傑作だ！",
              "maleVariant": "",
              "stringId": "1811325765299351552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから中身は…？",
              "maleVariant": "",
              "stringId": "1811327230252298240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今まで見た中で超クオリティ、超マニアック、超ゲキヤバなバーチャだ！ もうこいつは別次元の作品だぜ！",
              "maleVariant": "",
              "stringId": "1811327333834829824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[支払う] わかった、買うよ",
              "maleVariant": "",
              "stringId": "1811328747634044932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "興味ない",
              "maleVariant": "",
              "stringId": "1811329243350446080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこで手に入れたの？ あんたがナイトシティの販売を牛耳ってるわけ？",
              "maleVariant": "どこで手に入れた？ あんたがナイトシティの販売を牛耳ってるのか？",
              "stringId": "1811329346681319424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おいおい、んなこたどうだっていいだろう！？ こいつは違法スレスレの超手に入りにくい品なんだ。意味わかるよな？",
              "maleVariant": "",
              "stringId": "1811329435533455360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "客を乗せるのが上手いね",
              "maleVariant": "客を乗せるのが上手いな",
              "stringId": "1811330519962365952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハ！ やっぱり見たいんだろう！ バーチャはあんたのだ――もう少しエディーを積めば、俺のプレーヤーも使わせてやるぜ",
              "maleVariant": "",
              "stringId": "1811331013715832832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "興味ない",
              "maleVariant": "",
              "stringId": "1811334092100087808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はそう言ってるが、絶対気になってまた戻って来るぜ！",
              "maleVariant": "",
              "stringId": "1811335613944573952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ブレインダンスを再生]",
              "maleVariant": "",
              "stringId": "1811364514880172036"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分のがあるから大丈夫よ",
              "maleVariant": "自分のがあるから大丈夫だ",
              "stringId": "1811369600557658112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "当ててやる、そいつはたぶん、最高級で最新ファームウェアのヤツだろ？ このバーチャはもうほとんどビンテージ品なんだ――あんたので再生できるかな…",
              "maleVariant": "",
              "stringId": "1811370358048960512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「不明なファイル形式」。なんなの",
              "maleVariant": "「不明なファイル形式」。なんだよ",
              "stringId": "1811371932204806144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの機材じゃ再生できない",
              "maleVariant": "俺の機材じゃ再生できない",
              "stringId": "1811390137665204228"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの言うとおり、あたしのじゃダメみたい",
              "maleVariant": "あんたの言うとおり、俺のじゃダメみたいだ",
              "stringId": "1811390966476451840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だろ？ 互換性ってのはマジで厄介だよな",
              "maleVariant": "",
              "stringId": "1811391548645847040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃ、俺の機材で試してみないか？ 体中火が付いたみたいに興奮すること請け合いだぜ。ちょっぴり別料金がいるが",
              "maleVariant": "",
              "stringId": "1811393221283950592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、試してみる",
              "maleVariant": "",
              "stringId": "1811393622527848448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "金を返して",
              "maleVariant": "金を返せ",
              "stringId": "1811394130273513472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこまでする価値があるんでしょうね",
              "maleVariant": "そこまでする価値があるんだろうな",
              "stringId": "1811394530108125184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着け、嬢ちゃん。満足できなきゃ返金は保証する。ならいいだろ？ じゃ、こっちだ",
              "maleVariant": "落ち着け、あんちゃん。満足できなきゃ返金は保証する。ならいいだろ？ じゃ、こっちだ",
              "stringId": "1811395325532712960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もういい。エディーを返して",
              "maleVariant": "もういい。エディーを返せ",
              "stringId": "1811396983373312000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、物騒な真似はしないでくれよ。だから俺の機材で再生すれば問題ないって…",
              "maleVariant": "",
              "stringId": "1811399193838628864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこへ行くの？",
              "maleVariant": "どこへ行くんだ？",
              "stringId": "1811406029211971584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺の機材のあるとこだ、当たり前だろ？ 持ち歩いてるわけないだろよ",
              "maleVariant": "",
              "stringId": "1811406095700078592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺の機材だ。気の済むまで見てってくれ",
              "maleVariant": "",
              "stringId": "1811418181469614080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ブレインダンスを再生]",
              "maleVariant": "",
              "stringId": "1811460089713668100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "金を返して",
              "maleVariant": "金を返せ",
              "stringId": "1812826847192166400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、あんた… 大丈夫か？ あのバーチャを見始めたら、いきなりグリッチしだしてさ…",
              "maleVariant": "",
              "stringId": "1812827560945266688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "下手な芝居はやめて。わかってて見せたんでしょ。騙したのね",
              "maleVariant": "下手な芝居はやめろ。わかってて見せたんだろ。騙したんだな",
              "stringId": "1812828261360480256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい待ってくれ… とりあえず落ち着こう！ な？",
              "maleVariant": "",
              "stringId": "1812829326730473472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今回だけは見逃してやる",
              "maleVariant": "",
              "stringId": "1812836649700044804"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティを出て、二度と戻ってこないで",
              "maleVariant": "ナイトシティを出て、二度と戻ってくるな",
              "stringId": "1812837474384097280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これ以上つきあってられない",
              "maleVariant": "これ以上つきあってられるか",
              "stringId": "1812837714281508864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今回だけは見逃してやる",
              "maleVariant": "",
              "stringId": "1812838653033852928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当か！？ ああ、 助かった… ありがとう！ あんたは優しそうな奴だと思ってたよ！",
              "maleVariant": "",
              "stringId": "1812839200491188224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "優しそう？",
              "maleVariant": "",
              "stringId": "1812839534861103104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1時間やるから、ナイトシティを出て二度と戻ってこないで",
              "maleVariant": "1時間やるから、ナイトシティを出て二度と戻ってくるな",
              "stringId": "1812839969793650688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、俺の大好きな神様！ ありがとよ！ あんたならきっと――",
              "maleVariant": "",
              "stringId": "1812840515288051712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "黙って。話はまだ終わってない",
              "maleVariant": "黙れ。話はまだ終わってない",
              "stringId": "1812840787481604096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしまた見かけたら、その時は内臓を引きずり出して、一つずつ吊るしてやる。いい？",
              "maleVariant": "もしまた見かけたら、その時は内臓を引きずり出して、一つずつ吊るしてやる。いいな？",
              "stringId": "1812841079908478976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんな下らないこと、つきあってられない",
              "maleVariant": "こんな下らないこと、つきあってられるか",
              "stringId": "1812842009030701056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BDの内容は？",
              "maleVariant": "",
              "stringId": "1851624541093416960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バーチャの内容は？",
              "maleVariant": "",
              "stringId": "1851626022639030272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭クラクラ、神経ビーリビリ、超ハイパーウルトラセンセーショナルで、アドレナリン大放出なヤツよ！",
              "maleVariant": "",
              "stringId": "1851627111195136000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "嬢ちゃん、頭ん中がブッ飛ぶぜ。俺が保証するって",
              "maleVariant": "あんちゃん、頭ん中がブッ飛ぶぜ。俺が保証するって",
              "stringId": "1851627998894411776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハ！ 戻ってくると思ったぜ！ どうだ、買う気になったか？",
              "maleVariant": "",
              "stringId": "1851632325167992832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[支払う] わかった、あんたの機材を使う",
              "maleVariant": "",
              "stringId": "1851642319808548868"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずは自分ので試してみる",
              "maleVariant": "",
              "stringId": "1851642646947483648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なら、あんたのを使わせてよ",
              "maleVariant": "なら、あんたのを使わせてくれ",
              "stringId": "1851643061311164416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待ちきれないんだろ？ ん？ あんなもん体験したら、俺なら部屋中飛び回っちまうぜ！",
              "maleVariant": "",
              "stringId": "1851644888232546304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずは自分ので試してみる。ダメならまた来るよ",
              "maleVariant": "",
              "stringId": "1851646243177287680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おう、構わねえよ！ またいつでも来てくれ！",
              "maleVariant": "",
              "stringId": "1851649018380472320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "金を返して",
              "maleVariant": "金を返せ",
              "stringId": "1851661231371636736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういうことは金を払わせる前に言ってよね",
              "maleVariant": "そういうことは金を払わせる前に言ってくれよ",
              "stringId": "1851661543025201152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪い、悪い。忘れてた。じゃあ俺の機材を使ってくれ。なあ、それでいいだろ？",
              "maleVariant": "",
              "stringId": "1851662216395542528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんな機材を使うのは、あんたの歯ブラシで歯を磨くくらいお断りよ。今すぐエディーを返して",
              "maleVariant": "そんな機材を使うのは、お前の歯ブラシで歯を磨くくらいお断りだ。今すぐエディーを返せ",
              "stringId": "1851662737160327168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何だよ、こんくらいのことでキレやがって。ほらよ…",
              "maleVariant": "",
              "stringId": "1851664562252992512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また似たような真似したら、オプティクスをぶち抜いてやるからね",
              "maleVariant": "また似たような真似したら、オプティクスをぶち抜いてやるからな",
              "stringId": "1851665380964356096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "来いよ。すぐ裏に俺の機材がある",
              "maleVariant": "",
              "stringId": "1851670067444764672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "借りはきっちり返してもらうよ",
              "maleVariant": "借りはきっちり返してもらうぞ",
              "stringId": "1851684695348400128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "や、やめろ！ 頼む、許してくれ！ 命だけは！",
              "maleVariant": "",
              "stringId": "1851692184479518720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかったら、とっとと行って",
              "maleVariant": "わかったら、とっとと行け",
              "stringId": "1851753526360068096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの返り血を浴びたくないだけよ",
              "maleVariant": "お前の返り血を浴びたくないだけだ",
              "stringId": "1851755175241633792"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}