        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハーイ、“インフォフラッシュ”へようこそ！ アラサカに新たなボスが誕生しましたね。そう、ヨリノブ・アラサカ！",
                    "maleVariant": null,
                    "stringId": 1792189662581661696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "すごい偶然よね。40億人も人がいる中で、たまたま社長になったのが前の社長の息子なんて。どんな確率？",
                    "maleVariant": null,
                    "stringId": 1795434432190726144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "冗談は置いといて… ヨリノブはかなりの経歴の持ち主よ。少年時代に家出してギャングに入り、恐喝、強盗、誘拐、何でもあり",
                    "maleVariant": null,
                    "stringId": 1795434813805281280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと、アラサカの社長にはまさに適任じゃない！",
                    "maleVariant": null,
                    "stringId": 1795435214025768960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "とにかく、家出は数年で懲りて、家族に謝ったらしいけどね。サブロウは何の恨みも抱かずに許してあげたんだって。心がなきゃ、そりゃ恨みもしないよね",
                    "maleVariant": null,
                    "stringId": 1795435800959893504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカに戻ったヨリノブは、業務効率化担当副社長に任命されてる。つまり、オフィスの隅っこで反省してなさいってことね",
                    "maleVariant": null,
                    "stringId": 1795436672049725440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その彼が今や社長よ。でも去年は所得税を1エディーも納めてないとか。 まさか、アラサカの社長がそこまで低所得とは思わなかった。みなさん、ストリートで彼を見かけたらお金を恵んであげてください",
                    "maleVariant": null,
                    "stringId": 1795437269603827712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティ、今日もありがと！ ではまた！",
                    "maleVariant": null,
                    "stringId": 1795437842294095872
                  }
                }
              ]
            }
          }
        }
      }
    },
