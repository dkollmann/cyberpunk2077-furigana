    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあいくぞ！ パンツを履いてベルトを締めたら、エンジンをかけろ！",
              "maleVariant": "",
              "stringId": "1395316292542537728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スリー！",
              "maleVariant": "",
              "stringId": "1397864879895400448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ツー！",
              "maleVariant": "",
              "stringId": "1397866326594736128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ワン！",
              "maleVariant": "",
              "stringId": "1397866444605673472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1397866521596317696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クレア。次のレースだけど、準備はいい？",
              "maleVariant": "クレア。次のレースだが、準備はいいか？",
              "stringId": "1751898020796964864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4つのうちの2つ目。今回はオフロードよ",
              "maleVariant": "",
              "stringId": "1751899302358163456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "砂嵐よ… V、気をつけて",
              "maleVariant": "",
              "stringId": "1753410021805268992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "道が荒れるよ",
              "maleVariant": "",
              "stringId": "1783629146116595712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "追いつかれる！",
              "maleVariant": "",
              "stringId": "1783629176986673152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "前の奴を見失わないで！",
              "maleVariant": "",
              "stringId": "1783629208276180992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って、リロードしなきゃ…",
              "maleVariant": "",
              "stringId": "1783629234113093632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、ガンナーを1人排除！",
              "maleVariant": "",
              "stringId": "1783629270234439680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これなら勝てるよ、V。集中して",
              "maleVariant": "",
              "stringId": "1783629300366319616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "次のコースはオフロードで戦闘も激化する",
              "maleVariant": "",
              "stringId": "1832824050658844672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ビーストを使いましょ。実戦で試してみたいの",
              "maleVariant": "",
              "stringId": "1832824292452081664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいね。あたしはもう現地にいる",
              "maleVariant": "いいね。俺はもう現地にいる",
              "stringId": "1856281910533296128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK、私もすぐ行く",
              "maleVariant": "",
              "stringId": "1856282299513049088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。それじゃ",
              "maleVariant": "",
              "stringId": "1856290053053091840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "厳しいレースになるんでしょうね",
              "maleVariant": "厳しいレースになるんだろうな",
              "stringId": "1856291920441118724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "激戦になりそうね",
              "maleVariant": "激戦になりそうだな",
              "stringId": "1856292026338906112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アルデカルドスは手強そうね",
              "maleVariant": "アルデカルドスは手強そうだな",
              "stringId": "1856292045263605760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もっと観客がいるかと思ったけど",
              "maleVariant": "もっと観客がいるかと思ったが",
              "stringId": "1856292068768485376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ノーマッドが多いね",
              "maleVariant": "ノーマッドが多いな",
              "stringId": "1856292632164175872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "客入りはそこまででもないね",
              "maleVariant": "客入りはそこまででもないな",
              "stringId": "1856292676858679296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バッドランズだから。何でもありの危険なレースよ",
              "maleVariant": "",
              "stringId": "1856293799086657536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "暑すぎるからね",
              "maleVariant": "",
              "stringId": "1856294628401221632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "猛スピードでぶつかり合う命懸けのレースなんだから、熱くて当たり前でしょ",
              "maleVariant": "猛スピードでぶつかり合う命懸けのレースなんだから、熱くて当たり前だろ",
              "stringId": "1856294932001722368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中はホームレースだと思ってるのよ",
              "maleVariant": "",
              "stringId": "1856296728774447104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実際そうでしょ",
              "maleVariant": "実際そうだろ",
              "stringId": "1856297039337492480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ノーマッドに有利であることは否定しない",
              "maleVariant": "",
              "stringId": "1856297156308242432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ガンナーは大変そう",
              "maleVariant": "ガンナーは大変そうだ",
              "stringId": "1856297800670777344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドライバーもよ。ワンミスであの世行きになるからね",
              "maleVariant": "",
              "stringId": "1856298334555344896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫だって",
              "maleVariant": "",
              "stringId": "1856299040423153664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて、私は車で涼んでるから",
              "maleVariant": "",
              "stringId": "1856299347832082432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": "1856299848833306624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しっかり掴まってて",
              "maleVariant": "しっかり掴まってろ",
              "stringId": "1856320349567848448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心配しないで、これも作戦のうちよ",
              "maleVariant": "心配するな、これも作戦のうちだ",
              "stringId": "1856320780272537600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言われなくてもわかってる",
              "maleVariant": "",
              "stringId": "1856321858980417536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "上出来よ、クレア",
              "maleVariant": "上出来だ、クレア",
              "stringId": "1856323240869367808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ",
              "maleVariant": "ああ",
              "stringId": "1856324243643568128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スタートまで残り30秒！",
              "maleVariant": "",
              "stringId": "1935735591246315520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、お前！",
              "maleVariant": "",
              "stringId": "1935737086012375040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞いてんのか？",
              "maleVariant": "",
              "stringId": "1935739234720759808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい！ お前だよ！",
              "maleVariant": "",
              "stringId": "1935739433916645380"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無視して。ルイージの挑発に乗っちゃダメ",
              "maleVariant": "",
              "stringId": "1935842942427152384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なに？",
              "maleVariant": "なんだ？",
              "stringId": "1935844450229108736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうかしたの？",
              "maleVariant": "どうかしたか？",
              "stringId": "1935845019463270400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "健闘を祈ってやるよ！",
              "maleVariant": "",
              "stringId": "1935845998950694912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも",
              "maleVariant": "",
              "stringId": "1935847870281367552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今度はどんなレースなんだっけ？",
              "maleVariant": "",
              "stringId": "1938662150018043904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、また会えて嬉しいよ",
              "maleVariant": "",
              "stringId": "1938662242728939520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "2055025166241579012"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座って待つ]",
              "maleVariant": "",
              "stringId": "2056328576408567808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直、来てくれるとは思ってなかった。またボウリィと一緒に走ることになるかと",
              "maleVariant": "",
              "stringId": "2237382175312973828"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいか。お前の仕事は、アルデカルドスのクソどもに風穴を空けることだ。それ以外は考えなくていい",
              "maleVariant": "",
              "stringId": "2237382175413637120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頑張れよ、ルイージ！ お前がバッドランズ最速の男だ！",
              "maleVariant": "",
              "stringId": "2237382175480745992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんな、中途半端なレースするんじゃないよ！",
              "maleVariant": "",
              "stringId": "2237382175581409288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボウリィはいい腕をしてる",
              "maleVariant": "",
              "stringId": "2247736558789726208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあな。だがあいつはうるさすぎる… 横でぺちゃくちゃ喋られると運転に集中できない",
              "maleVariant": "",
              "stringId": "2247737851675545600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "車のトランクにガムテープがあるよ。ほら、次のレースは私が来れないかもしれないし",
              "maleVariant": "",
              "stringId": "2247739784410828800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言われなくてもわかってるって",
              "maleVariant": "",
              "stringId": "2247759225815175168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナマ言うな。今回が初めてのレースだろ。最初の区間で連中を落とさなきゃ、報酬はなしだからな",
              "maleVariant": "",
              "stringId": "2247759452357922816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はいはい… 「バッドランズは我々のものだ」、「アルデカルドスに死を」… もう聞き飽きたっつうの",
              "maleVariant": "",
              "stringId": "2247761661481062400"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}