  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なあ、あの話はマジなのか？ お前が脱走兵だって。本当のことを知っておきたい",
                    "maleVariant": null,
                    "stringId": 2167645459363680256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんてったって、俺は生粋の愛国者だからな",
                    "maleVariant": null,
                    "stringId": 2198149165436592128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "隊を抜けたのは事実だ。だがそれは愛国心ゆえの行動だ。連中は認めんだろうが",
                    "maleVariant": null,
                    "stringId": 2198154151356751872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "愛国心の意味をはき違えてないか？",
                    "maleVariant": null,
                    "stringId": 2198157997348970496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はあ… 昔、友人が反企業デモに参加した。その時、俺は命令されたんだ。そいつの眉間に鉛弾をブチ込めって…",
                    "maleVariant": null,
                    "stringId": 2198160720391790592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そして家族に訃報も伝えろと言われた。「息子さんは勇敢に戦い、英雄として死にました」ってな",
                    "maleVariant": null,
                    "stringId": 2198169935344787456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "英雄扱いされたならいいじゃないか",
                    "maleVariant": null,
                    "stringId": 2198171092100276224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺は上層部に書面で思いの丈を訴えた。その結果、降格され。インプラントも全部取られたんだ",
                    "maleVariant": null,
                    "stringId": 2198193094479536128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それで腹をくくって… 軍を抜けたんだよ",
                    "maleVariant": null,
                    "stringId": 2198195865807183872
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}