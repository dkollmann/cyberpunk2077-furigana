  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なあチューマ、聞けって！腕の代わりに… 銃を付けてやるってのはどうだ？ きっとこいつ、ションベンする度に…！",
                    "maleVariant": null,
                    "stringId": 1770836439844536320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハッ！ こいつらが集まりゃ新時代の宗派ができちまうな。“フルメタル坊主”って呼ぶのはどうだ？ ハハハハハ！",
                    "maleVariant": null,
                    "stringId": 1770836824260886528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クソ、侵入者だ！",
                    "maleVariant": null,
                    "stringId": 1770885360646111232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "助けて！ ここから出してくれ！",
                    "maleVariant": null,
                    "stringId": 1770885713202528256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やめろ！ インプラントは俺の信仰に反する！ 頼む… やめてくれ！",
                    "maleVariant": null,
                    "stringId": 1771924954711867392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "助けを呼ぶ声が聞こえた",
                    "maleVariant": null,
                    "stringId": 1771926995089768448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お兄さんに頼まれて来た",
                    "maleVariant": "兄さんに頼まれて来た",
                    "stringId": 1771927868729737216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お兄さんに感謝することね",
                    "maleVariant": "兄さんに感謝することだな",
                    "stringId": 1771928124330622976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "通りかかったら、メタル坊主がどうとかって話し声が聞こえてきてね。そんなこと考えるのは、メイルストロームくらいでしょ",
                    "maleVariant": "通りかかったら、メタル坊主がどうとかって話し声が聞こえてきてな。そんなこと考えるのは、メイルストロームくらいだろ",
                    "stringId": 1771930454702702592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "通りかかったら、メタル坊主がどうとかって話し声が聞こえてきてね。そんなこと考えるのは、メイルストロームくらいでしょ",
                    "maleVariant": "通りかかったら、メタル坊主がどうとかって話し声が聞こえてきてな。そんなこと考えるのは、メイルストロームくらいだろ",
                    "stringId": 1771930936309465088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "通りかかったら、メタル坊主がどうとかって話し声が聞こえてきてね。そんなこと考えるのは、メイルストロームくらいでしょ",
                    "maleVariant": "通りかかったら、メタル坊主がどうとかって話し声が聞こえてきてな。そんなこと考えるのは、メイルストロームくらいだろ",
                    "stringId": 1771931354666123264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "心の底から感謝する。ひとつの命も犠牲にならなくて本当によかった。今後も平和の道を歩み続けてほしい",
                    "maleVariant": null,
                    "stringId": 1771966247282425856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの化け物どもがなぜあのような過ちを犯すのか理解できんが、裁きを下すのは、俺たちではない",
                    "maleVariant": null,
                    "stringId": 1771966709595389952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "助けてくれたことには礼を言う… だが人を犠牲にして救うほどの価値は、この命にない",
                    "maleVariant": null,
                    "stringId": 1771979662948319232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お兄さんが待ってるよ",
                    "maleVariant": "兄さんが待ってるぞ",
                    "stringId": 1771981206368305156
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "命がけで助けたのよ？",
                    "maleVariant": "命がけで助けたんだぞ？",
                    "stringId": 1771981962701979648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああするしかなかった",
                    "maleVariant": null,
                    "stringId": 1771982929975590912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お兄さんが待ってるよ。近くの広場にいる",
                    "maleVariant": "兄さんが待ってるぞ。近くの広場にいる",
                    "stringId": 1771985594650476544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "早く兄に会いたいが、ひと息ついてから行くとしよう。今助けが必要なのは兄の方だ…",
                    "maleVariant": null,
                    "stringId": 1771986206834311168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしが来なかったら、今ごろ機械人間になってたのよ？",
                    "maleVariant": "俺が来なかったら、今ごろ機械人間になってたんだぞ？",
                    "stringId": 1771986447872573440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "善意は認めるが、そのために殺生を犯した君に感謝することはできない",
                    "maleVariant": null,
                    "stringId": 1771986887804731392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "仕方ないでしょ？ 説教でもしてやればよかった？ メイルストローム相手に話が通じるわけがない",
                    "maleVariant": "仕方ないだろ？ 説教でもしてやればよかったか？ メイルストローム相手に話が通じるわけがない",
                    "stringId": 1771987435798937600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "選択肢は常にある。何もしないという選択も含めてな。それこそが最も難しい道だ",
                    "maleVariant": null,
                    "stringId": 1771987759448211456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺のせいで人が死んだという思いを抱え生きるくらいなら、この体など喜んでくれてやった",
                    "maleVariant": null,
                    "stringId": 1771989500403138560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう行ってくれ。俺はひと息ついてから兄のもとへ行く。今助けが必要なのは兄の方だ",
                    "maleVariant": null,
                    "stringId": 1772028474614104064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ねえ、あんた、しっかりして！",
                    "maleVariant": "おい、あんた、しっかりしろ！",
                    "stringId": 1772205346534735872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんで俺ばっか見張りをやんなきゃなんねえんだ？ こんな野郎、何もできやしねえだろ…",
                    "maleVariant": null,
                    "stringId": 1783564401681383424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "頼む！ 俺は釈迦の教えで平穏を得た！ 穢さないでくれ…",
                    "maleVariant": null,
                    "stringId": 1783569209846939648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おい、道徳にでも目覚めたか？ 殺すことを悩んだりするなら、傭兵なんかやらずに坊さんになりゃよかったんだ",
                    "maleVariant": null,
                    "stringId": 1783618192707436552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まったく、坊さんってのがあんな恩知らずとはな。むしろ助けて悪かったって、謝ってやったらどうだ？",
                    "maleVariant": null,
                    "stringId": 1783618192791322624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "生きて返すんじゃねーぞ！",
                    "maleVariant": null,
                    "stringId": 1825807584833814528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう大丈夫よ。あたしは奴らの仲間じゃない",
                    "maleVariant": "もう大丈夫だ。俺は奴らの仲間じゃない",
                    "stringId": 1825833658254868480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ… 一体なぜここに？ どういうことだ？",
                    "maleVariant": null,
                    "stringId": 1825834159943958528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おい、パクるなよ",
                    "maleVariant": null,
                    "stringId": 1825840383620005888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "君のもたらした苦しみは、1つの善行で正当化できるものではない。君がやったのは、紛れもなく邪悪な行為だ",
                    "maleVariant": null,
                    "stringId": 1825864802371354624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヘヘ… こいつは“フルメタル坊主”になるってわけか！",
                    "maleVariant": null,
                    "stringId": 1826930857395077120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最初は断ったんだけど、あまりに必死に頼まれて… 見捨てるわけにはいかないでしょ",
                    "maleVariant": "最初は断ったんだが、あまりに必死に頼まれてな… 見捨てるわけにはいかないだろ",
                    "stringId": 1826936296954380288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "殺せば何でも解決するわけじゃない",
                    "maleVariant": null,
                    "stringId": 1826947191508025344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "世界を変えようっていうなら… ある程度の代償は仕方ないさ",
                    "maleVariant": null,
                    "stringId": 1826947519150276608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "世の中には善人と悪人っつう二種類の人間がいるなんてのは嘘っぱちだ。あるのは良い選択とクソみたいな選択さ",
                    "maleVariant": null,
                    "stringId": 1826952954683047936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その点、お前は良い選択をしたな。こんなありがたいギアを山ほど掘り当てちまったんだからよ",
                    "maleVariant": null,
                    "stringId": 1826954151502528512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今度口を開けたら、舌を引っこ抜くぞ",
                    "maleVariant": null,
                    "stringId": 1827010857871851520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "例の僧侶が囚われてるのはここね",
                    "maleVariant": "例の僧侶は囚われてるのはここか",
                    "stringId": 1834473178680508416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "他者に与えた苦しみは、いつか必ず自分に返ってくるものだ。必ずな",
                    "maleVariant": null,
                    "stringId": 1834504622068973568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう行ってくれ。俺はひと息ついてから兄のもとへ行く。今助けが必要なのは兄の方だ",
                    "maleVariant": null,
                    "stringId": 1834505760772177920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それにあのクロームバカどもがこれだけのギアを集めるのに、念仏唱えてただけだと思うか？",
                    "maleVariant": null,
                    "stringId": 1834534670247378944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうして奴らに捕まったりしたの？ 誰も近寄りすらしないのに",
                    "maleVariant": "どうして奴らに捕まったりしたんだ？ 誰も近寄りすらしないのに",
                    "stringId": 1834538763032444928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしてメイルストロームに捕まったの？",
                    "maleVariant": "どうしてメイルストロームに捕まったんだ？",
                    "stringId": 1834539226402373632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここのことをよく知らないんだ。俺たちは各地を渡り歩いていてね。ナイトシティに着いたばかりで、危険を把握できていなかった",
                    "maleVariant": null,
                    "stringId": 1834569337998655488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "僧侶は寺で暮らすんじゃ？",
                    "maleVariant": null,
                    "stringId": 1834569743453634560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それは最近の流れだ。我々兄弟が所属する宗派は、いにしえの仏教の教えに従っている",
                    "maleVariant": null,
                    "stringId": 1834570102771269632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この生き方のおかげで、人々との出会いにも恵まれてきた",
                    "maleVariant": null,
                    "stringId": 1834570241032306688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうみたいね",
                    "maleVariant": "そうみたいだな",
                    "stringId": 1834570404492722176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "世の中が危険に満ちていることは十分わかっている",
                    "maleVariant": null,
                    "stringId": 1834570529298432000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だが時に危険も冒さなければ、扉の向こうにあるものを知ることはできない。君ならこの意味がわかるだろう",
                    "maleVariant": null,
                    "stringId": 1834571138831466496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そこにあんたの声が聞こえて、かけつけたわけ",
                    "maleVariant": "そこにあんたの声が聞こえて、かけつけたんだ",
                    "stringId": 1837481552619749376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんて、本当はお兄さんに頼まれて来たのよ。弟を自分と同じ目に遭わせたくないって",
                    "maleVariant": "なんて、本当は兄さんに頼まれて来たんだ。弟を自分と同じ目に遭わせたくないとな",
                    "stringId": 1837482515615170560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんて、本当はお兄さんに頼まれたの",
                    "maleVariant": "なんて、本当は兄さんに頼まれたんだ",
                    "stringId": 1837483323102576640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "間に合ってよかった。大丈夫？ 連中に何かされた？",
                    "maleVariant": "間に合ってよかった。大丈夫か？ 連中に何かされたりは？",
                    "stringId": 1837486891045994496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まずはあんたが無事なのを確認したい",
                    "maleVariant": null,
                    "stringId": 1837487161293389824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "遊びで僧侶を改造できるほど、ありあまってるってことよね…",
                    "maleVariant": "遊びで僧侶を改造できるほど、ありあまってるってことだな…",
                    "stringId": 1841428481921761280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うまいこと言うじゃない",
                    "maleVariant": "うまいこと言うじゃないか",
                    "stringId": 1841430377713614848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どのインプラントにするか迷ってたおかげで、なんとか… 運がよかったよ",
                    "maleVariant": null,
                    "stringId": 1841445217798701056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼らの一人は、俺の顎を外したがってた。考えただけでも恐ろしい…",
                    "maleVariant": null,
                    "stringId": 1841445557486993408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "幸いにも、そうならずに済んだ。これはすべて… 君のおかげなのか？",
                    "maleVariant": null,
                    "stringId": 1841445683785875456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サイバーサイコが野放しじゃない…",
                    "maleVariant": "サイバーサイコが野放しだな…",
                    "stringId": 1841454859849285632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あー、まあ… そうね",
                    "maleVariant": "あー、まあ… そうだな",
                    "stringId": 1853419504159932416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お兄さんのほうがまともね",
                    "maleVariant": "兄さんのほうがまともだな",
                    "stringId": 1856151550321999872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう大丈夫よ",
                    "maleVariant": "もう大丈夫だ",
                    "stringId": 2025620404403453956
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お兄さんが待ってる",
                    "maleVariant": "兄さんが待ってるぞ",
                    "stringId": 2123000600827281412
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "仕方なかったのよ",
                    "maleVariant": "仕方なかったんだ",
                    "stringId": 2258954629284610052
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
