                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "テディー",
                    "maleVariant": null,
                    "stringId": 1918561518823952384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
