                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうだな",
                    "maleVariant": null,
                    "stringId": 1936068567273959424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "侵入した後は、警備や防衛設備への対処に、〈神輿〉への最短ルートの特定… ぜんぶ出たとこ勝負だけどね",
                    "maleVariant": null,
                    "stringId": 1936077536524627968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "聞かない方がいいってことか？",
                    "maleVariant": null,
                    "stringId": 1937104614367817728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。ここまで関わったら同じことだ。とことん付き合ってやるよ",
