        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "決行が明日だってことは、伝えてある",
                    "maleVariant": null,
                    "stringId": 1742988975937474560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも、みんな納得してるの？",
                    "maleVariant": "でも、みんな納得してるのか？",
                    "stringId": 1742988975937474568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカ・タワーまでトンネルを掘ろうって言うの？",
                    "maleVariant": "アラサカ・タワーまでトンネルを掘ろうって言うのか？",
                    "stringId": 1770683577578344448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "疑いたくなるのもわかるが、このチャンスを使わない手はない",
                    "maleVariant": null,
                    "stringId": 1770683860559646720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "近くでナイト・コープがリニア路線を建設してる。ミリテクが警備に当たってるけど、何とかなるでしょ",
                    "maleVariant": null,
                    "stringId": 1770684746933522432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "トンネルに入っちまえば、アラサカ・タワーの基礎部分まではほんの半マイルだ",
                    "maleVariant": null,
                    "stringId": 1770685310362767360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイト・コープのSERCを使えば、ものの数分でタワーまで掘れる",
                    "maleVariant": null,
                    "stringId": 1770695322686509056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "建設現場のミリテクにはどう対処するの？",
                    "maleVariant": "建設現場のミリテクにはどう対処するんだ？",
                    "stringId": 1770699443892178960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたはひとまずソウルに預けるから、あとで会いに来て",
                    "maleVariant": null,
                    "stringId": 1770708858326274048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "建設現場の警備に関する情報は？ ミリテク部隊の装備はどの程度なの？",
                    "maleVariant": "建設現場の警備に関する情報は？ ミリテク部隊の装備はどの程度なんだ？",
                    "stringId": 1770763903986094080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "通常レベルだよ。恐れるほどじゃないけど、素通りできるわけでもない",
                    "maleVariant": null,
                    "stringId": 1810959067551420416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "仲間と相談して、現場にドローンを飛ばしてもらおう",
                    "maleVariant": null,
                    "stringId": 1810960539987005440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "寝不足だっただけよ。もう作戦ができてるって聞いたけど？",
                    "maleVariant": "寝不足だっただけだ。もう作戦ができてるって聞いたが？",
                    "stringId": 1814046411846373380
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まだ詰めきれてない部分もあるけど、おおよその枠組みはできてる",
                    "maleVariant": null,
                    "stringId": 1814052030166913024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "地下からタワーに入るってこと？",
                    "maleVariant": "地下からタワーに入るってことか？",
                    "stringId": 1814052707547009024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "上手くいくの？",
                    "maleVariant": "上手くいくのか？",
                    "stringId": 1814052870889984000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "タワーの情報は本当に信頼できるの？",
                    "maleVariant": "タワーの情報は本当に信頼できるのか？",
                    "stringId": 1814053421887311872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ。スネーク族から仕入れたからな",
                    "maleVariant": null,
                    "stringId": 1814055328936677376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "連中はあの辺りで活動してるんだ",
                    "maleVariant": null,
                    "stringId": 1814055431764234240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リスキーな作戦ではある。だがうまくいけば〈神輿〉までたどり着けるはずだ",
                    "maleVariant": null,
                    "stringId": 1814055803010469888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "早まるな。もう少し情報が揃わない限り、ゴーサインは出せない",
                    "maleVariant": null,
                    "stringId": 1814062873751375872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どれだけ情報を集めようが、リスクが減るわけじゃないでしょ",
                    "maleVariant": null,
                    "stringId": 1814066240586305536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はあ… よし、わかった。V――お前には、命を懸けるだけの借りがある",
                    "maleVariant": null,
                    "stringId": 1814066448506343424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でだな、V… 戦争をおっぱじめるんだって？",
                    "maleVariant": null,
                    "stringId": 1834186731841966080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[テーブルに寄り掛かる] なんてことない。作戦を確認しましょ",
                    "maleVariant": "[テーブルに寄り掛かる] なんてことない。作戦を確認しよう",
                    "stringId": 1834304253991899136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "確認してみて",
                    "maleVariant": null,
                    "stringId": 1834313248609718272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いろいろ検討した結果――",
                    "maleVariant": null,
                    "stringId": 1834313354339733504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカ・タワーに侵入できそうな方法は1つしかない",
                    "maleVariant": null,
                    "stringId": 1834313504395153408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "トンネルを掘るの",
                    "maleVariant": null,
                    "stringId": 1834313765347971072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "地中掘削除去複合機。要は… 自動トンネル堀りマシーンね",
                    "maleVariant": null,
                    "stringId": 1834318768515108864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それさえ手に入れば、タワーまでトンネルを掘って〈神輿〉を目指せる",
                    "maleVariant": null,
                    "stringId": 1834320964451356672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "他に選択肢はない",
                    "maleVariant": null,
                    "stringId": 1877694120515465216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それは俺たちも同じみたいだな",
                    "maleVariant": null,
                    "stringId": 1877694522799550464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "来いよ。みんなのとこに行こう",
                    "maleVariant": null,
                    "stringId": 1877697818213871616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、周りを見ろ。戦争経験者はごくわずか、あとはケチなコンボイ襲撃か、廃品漁りしかやったことがない連中だ",
                    "maleVariant": null,
                    "stringId": 1877719413057777664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だから必要なことしか知らせてない。お前を助けるためだってことしか、な",
                    "maleVariant": null,
                    "stringId": 1877721731685806080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "計画はもう決まった。今更ひっかき回さないでくれる？",
                    "maleVariant": null,
                    "stringId": 1877810556877533184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言われなくてもわかってる！",
                    "maleVariant": null,
                    "stringId": 1877812602389262336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クランを危険にさらすんだぞ",
                    "maleVariant": null,
                    "stringId": 1877813189977698304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも、もうやるしかないんだよ。いくら説教しても何も変わらないから",
                    "maleVariant": null,
                    "stringId": 1877814474693013504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V！ 待ってたよ",
                    "maleVariant": null,
                    "stringId": 1877815825158238208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "調子はどうだ？",
                    "maleVariant": null,
                    "stringId": 1877816299953451008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やるしかないんだ。それともVをこのまま死なせるつもり？",
                    "maleVariant": null,
                    "stringId": 1877866591671676928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ドローンの巡回に加え、現場オペレーターが配備されてるってところだな",
                    "maleVariant": null,
                    "stringId": 1877927203223236608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "敵陣の真っただ中に突っ込む、無茶な作戦よ。アラサカはもちろん、他の連中も敵に回すことになる",
                    "maleVariant": "敵陣の真っただ中に突っ込む、無茶な作戦だ。アラサカはもちろん、他の連中も敵に回すことになる",
                    "stringId": 1877933781754179584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だろうな",
                    "maleVariant": null,
                    "stringId": 1877961432887848960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたたちだけじゃない。クランの皆も… 危険に晒すことになる。本当にいいの？ 降りても責めはしないから、正直に言って",
                    "maleVariant": "あんたたちだけじゃない。クランの皆も… 危険に晒すことになる。本当にいいのか？ 降りても責めはしない、正直に言ってくれ",
                    "stringId": 1877961646663135232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前を助けると決めたんだ。覚悟はできてる。これ以上の議論は必要ない",
                    "maleVariant": null,
                    "stringId": 1877962506092802048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "決まりね。じゃあ仕事にかかろう",
                    "maleVariant": null,
                    "stringId": 1877971718931869696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私はミッチと、ミリテク攻略について話してくる",
                    "maleVariant": null,
                    "stringId": 1877982651905339392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "質問はあるか？",
                    "maleVariant": null,
                    "stringId": 1880511816948903936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私抜きで勝手に話を進めないでよ",
                    "maleVariant": null,
                    "stringId": 1882019477171335168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オルトと接触する必要がある",
                    "maleVariant": null,
                    "stringId": 1882020087090245636
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "パナムとは上手くいってるの？",
                    "maleVariant": "パナムとは上手くいってるのか？",
                    "stringId": 1882020779385286656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ねえ、あんたとパナムだけど… 何かあったの？",
                    "maleVariant": "なあ、あんたとパナムだが… 何かあったのか？",
                    "stringId": 1882021135313924096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ネット回線を用意できる？ ある人の協力が必要なの",
                    "maleVariant": "ネット回線を用意できるか？ ある人の協力が必要なんだ",
                    "stringId": 1882021799305801728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オルトっていうAI",
                    "maleVariant": "オルトっていうAIだ",
                    "stringId": 1882026029647261700
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "誰のことだ？",
                    "maleVariant": null,
                    "stringId": 1882026325228253184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "話すと長くなる",
                    "maleVariant": null,
                    "stringId": 1882032020321333248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "長い話よ",
                    "maleVariant": "長い話だ",
                    "stringId": 1882032536522715140
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、はっきり言わせてもらうが――ファミリーの者を危険に晒すのは正直、気が進まない",
                    "maleVariant": null,
                    "stringId": 1882032536522715152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "止めはしないがな",
                    "maleVariant": null,
                    "stringId": 1882032536522715164
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オルトよ… 彼女は… AIなの。ブラックウォールの向こう側の存在",
                    "maleVariant": "オルト… 彼女は… AIなんだ。ブラックウォールの向こう側の存在",
                    "stringId": 1882033606707769344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何を言ってる？ AIが〈神輿〉の侵入に手を貸すって言うのか？",
                    "maleVariant": null,
                    "stringId": 1882034279339913216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ。お願いすればね",
                    "maleVariant": "ああ。お願いすればな",
                    "stringId": 1882034395975118848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうなってんだ…",
                    "maleVariant": null,
                    "stringId": 1882035906209771520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ダコタを探せ。必要な機材を用意してくれる",
                    "maleVariant": null,
                    "stringId": 1882049341018468352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ありがとう",
                    "maleVariant": null,
                    "stringId": 1882050657526607872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "礼なら、〈神輿〉から生還した後に聞く",
                    "maleVariant": null,
                    "stringId": 1882050885008879616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしのせいってこと？",
                    "maleVariant": "俺のせいってことか？",
                    "stringId": 1882078888396976132
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "パナムもそう思ってるの？",
                    "maleVariant": "パナムもそう思ってるのか？",
                    "stringId": 1882079205402472448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしのせいってこと？",
                    "maleVariant": "俺のせいってことか？",
                    "stringId": 1882079741266751488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "パナムもそう思ってるの？",
                    "maleVariant": "パナムもそう思ってるのか？",
                    "stringId": 1882079855871913984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "勘違いするな。むしろお前には感謝してるんだ",
                    "maleVariant": null,
                    "stringId": 1882080689062014976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本人に聞いてくれ",
                    "maleVariant": null,
                    "stringId": 1882082138915467264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V。お前はいがみ合ってばかりのファミリーに偶然出会った外の人間だ",
                    "maleVariant": null,
                    "stringId": 1882086466279567368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ところが、お前と関わるようになった途端、そのファミリーは手を取り合い、以前より団結が増した",
                    "maleVariant": null,
                    "stringId": 1882088392589176832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかるか？",
                    "maleVariant": null,
                    "stringId": 1882089006903717888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ… なんとなくだけど",
                    "maleVariant": "ああ… なんとなくだが",
                    "stringId": 1882089257639211008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ならいい。で、他に必要なものは？",
                    "maleVariant": null,
                    "stringId": 1882089423783981056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "悪くない計画でしょ？",
                    "maleVariant": null,
                    "stringId": 1905202056250650624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "SERCってのは？",
                    "maleVariant": null,
                    "stringId": 1905204203952402432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "可もなく不可もなくね… これ以上の案もないし、さっそく準備にかかりましょ",
                    "maleVariant": "可もなく不可もなくだな… これ以上の案もないし、さっそく準備にかかろうか",
                    "stringId": 1905211748062457856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "SERCってのは？",
                    "maleVariant": null,
