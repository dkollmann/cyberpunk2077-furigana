    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1704138168493686788,
            "femaleVariant": "お友達はどこ？",
            "maleVariant": "お友達はどこだ？"
          },
          {
            "stringId": 1704138951100481536,
            "femaleVariant": "ひどい格好ね",
            "maleVariant": "ひどい格好だな"
          },
          {
            "stringId": 1704139412574584832,
            "femaleVariant": "どうしたの？ ひどい格好じゃない",
            "maleVariant": "どうしたんだ？ ひどい格好だな"
          },
          {
            "stringId": 1704139693492289536,
            "femaleVariant": "インプラント、金、尊厳。すべてを奪われたら、こんな見た目にもなるさ"
          },
          {
            "stringId": 1704140613974245376,
            "femaleVariant": "だがいいこともある。街で人目を引かなくなったからな"
          },
          {
            "stringId": 1704140849794793472,
            "femaleVariant": "例のお友達は？"
          },
          {
            "stringId": 1704141644380520448,
            "femaleVariant": "オダなら、もうじき到着するはずだ"
          },
          {
            "stringId": 1704145084431220736,
            "femaleVariant": "その忠実なしもべがあたしたちを売る可能性だってある",
            "maleVariant": "その忠実なしもべが俺たちを売る可能性だってある"
          },
          {
            "stringId": 1704156518640795652,
            "femaleVariant": "ヨリノブが父親を絞め殺した"
          },
          {
            "stringId": 1704157033097347072,
            "femaleVariant": "身の安全を約束してくれないと、話す気にはなれない"
          },
          {
            "stringId": 1704157618940952576,
            "femaleVariant": "初めはよくある親子喧嘩にしか見えなかった"
          },
          {
            "stringId": 1704158181548113920,
            "femaleVariant": "現場で全てを目撃した"
          },
          {
            "stringId": 1704166662346915840,
            "femaleVariant": "オダのあの話… 式典の後は東京に帰るはず…"
          },
          {
            "stringId": 1709704685100744704,
            "femaleVariant": "わからないか？ 式典だ…"
          },
          {
            "stringId": 1709708126812389376,
            "femaleVariant": "ジャパンタウンの詳細な地図を入手しなければ"
          },
          {
            "stringId": 1711171976841674752,
            "femaleVariant": "まずは綿密な下準備が必要だ"
          },
          {
            "stringId": 1711308227917922308,
            "femaleVariant": "[立つ] 協力するの？ しないの？",
            "maleVariant": "[立つ] 協力するのか？ しないのか？"
          },
          {
            "stringId": 1711309147158364160,
            "femaleVariant": "[立つ] ハナコに判断させるべきよ",
            "maleVariant": "[立つ] ハナコに判断させるべきだ"
          },
          {
            "stringId": 1711310524131590144,
            "femaleVariant": "オダ！"
          },
          {
            "stringId": 1711312020206272512,
            "femaleVariant": "オダ、別に厄介なことを頼むつもりはない"
          },
          {
            "stringId": 1711385984391725056,
            "femaleVariant": "あいつはアラサカを乗っ取るために自分の父親を殺したのよ！？",
            "maleVariant": "あいつはアラサカを乗っ取るために自分の父親を殺したんだぞ！？"
          },
          {
            "stringId": 1711427821617823744,
            "femaleVariant": "次はお前のツテの番だ。頼りになるフィクサーを知らないか？"
          },
          {
            "stringId": 1712560417648021504,
            "femaleVariant": "いいお友達ね。他にあては？",
            "maleVariant": "いいお友達だな。他にあては？"
          },
          {
            "stringId": 1712560884843155456,
            "femaleVariant": "生憎、奴だけだ"
          },
          {
            "stringId": 1712582700508778496,
            "femaleVariant": "タケムラさん"
          },
          {
            "stringId": 1712582916867756032,
            "femaleVariant": "オダ"
          },
          {
            "stringId": 1712606051826536448,
            "femaleVariant": "これが例のコソ泥ですか？"
          },
          {
            "stringId": 1712606613292847104,
            "femaleVariant": "俺の証人、Vだ"
          },
          {
            "stringId": 1712612889582243840,
            "femaleVariant": "自分のことは自分で話す"
          },
          {
            "stringId": 1712613077973602304,
            "femaleVariant": "なら話してもらおう。色々知ってるそうだな"
          },
          {
            "stringId": 1712792605861744640,
            "femaleVariant": "一度会わせてくれるだけでいい"
          },
          {
            "stringId": 1712792738787627008,
            "femaleVariant": "あたしの話を信じるかどうかは、ハナコ自身が決めるべきよ",
            "maleVariant": "俺の話を信じるかどうかは、ハナコ自身が決めるべきだ"
          },
          {
            "stringId": 1712794232144400384,
            "femaleVariant": "お前も気をつけろ、わが友よ"
          },
          {
            "stringId": 1712794637112840192,
            "femaleVariant": "ここは遠い異国の地だ"
          },
          {
            "stringId": 1712797813425704964,
            "femaleVariant": "[立ち上がる] やるだけのことはやった"
          },
          {
            "stringId": 1712797996851007488,
            "femaleVariant": "[立ち上がる] 素敵なお友達ね",
            "maleVariant": "[立ち上がる] 素敵なお友達だな"
          },
          {
            "stringId": 1712799266500382720,
            "femaleVariant": "[立ち上がる] こうなると思ってた"
          },
          {
            "stringId": 1712800498703011840,
            "femaleVariant": "まあ… やるだけやったね",
            "maleVariant": "まあ… やるだけやったな"
          },
          {
            "stringId": 1712800576264081408,
            "femaleVariant": "予想通りの結果ね",
            "maleVariant": "予想通りの結果だな"
          },
          {
            "stringId": 1712806234296291328,
            "femaleVariant": "ふむ… だが、収穫が何もなかったというわけではない"
          },
          {
            "stringId": 1712806414903021568,
            "femaleVariant": "ああ、そうだな。そして有益な情報も手に入った…"
          },
          {
            "stringId": 1712806817589760000,
            "femaleVariant": "え？ それはどういうこと？",
            "maleVariant": "え？ それはどういうことだ？"
          },
          {
            "stringId": 1719646663117508608,
            "femaleVariant": "[立つ] これはアラサカの家臣であるあんたの務めよ",
            "maleVariant": "[立つ] これはアラサカの家臣であるあんたの務めだ"
          },
          {
            "stringId": 1719647711643176960,
            "femaleVariant": "オダ、あんたはアラサカの家臣でしょ？",
            "maleVariant": "オダ、あんたはアラサカの家臣だろ？"
          },
          {
            "stringId": 1719649077107232768,
            "femaleVariant": "ならば上の者に忠義を尽くすのがあんたの務めのはず。それを忘れないことね",
            "maleVariant": "ならば上の者に忠義を尽くすのがあんたの務めのはず。それを忘れないことだな"
          },
          {
            "stringId": 1719649339637108736,
            "femaleVariant": "コソ泥の説教を受けに来たつもりはない"
          },
          {
            "stringId": 1719650419804606464,
            "femaleVariant": "あたしもかつてはこんな落ちぶれてはいなかった。それはここにいるあんたの友達も同じよ",
            "maleVariant": "俺もかつてはこんな落ちぶれてはいなかった。それはここにいるあんたの友達も同じだ"
          },
          {
            "stringId": 1719650705436708864,
            "femaleVariant": "なんなら、あんたも底辺の暮らしを一度味わってみる？",
            "maleVariant": "なんなら、あんたも底辺の暮らしを一度味わってみるか？"
          },
          {
            "stringId": 1719650970667716608,
            "femaleVariant": "嫌でしょ？ なら、務めを果たしなさい。ハナコに伝えるべきことを伝えるのよ",
            "maleVariant": "嫌だろ？ なら、務めを果たすんだな。ハナコに伝えるべきことを伝えるんだ"
          },
          {
            "stringId": 1719655123230003200,
            "femaleVariant": "なるほど、奴の忠誠心に揺さぶりをかけたわけか"
          },
          {
            "stringId": 1719657319149473792,
            "femaleVariant": "時にはありのままを話さないと"
          },
          {
            "stringId": 1719657577921253376,
            "femaleVariant": "だが奴の考えは変わらないだろうな"
          },
          {
            "stringId": 1719658051726610432,
            "femaleVariant": "どうして？",
            "maleVariant": "どうしてだ？"
          },
          {
            "stringId": 1719658109691891712,
            "femaleVariant": "わかるだろう。オダは完璧主義者なんだ。間違いを犯すことを何より恐れている"
          },
          {
            "stringId": 1719661930065301504,
            "femaleVariant": "どうにかしてハナコ様の元にたどり着ければ…"
          },
          {
            "stringId": 1719668504536158208,
            "femaleVariant": "ちょっと待って",
            "maleVariant": "ちょっと待て"
          },
          {
            "stringId": 1719672295448776704,
            "femaleVariant": "ちょ、ちょっと、落ち着いてよ",
            "maleVariant": "お、おい、少し落ち着けって"
          },
          {
            "stringId": 1719678743402647552,
            "femaleVariant": "SCSMのハッキングじゃないんだから… ハナコの警備はナイトシティいち厳重だって、自分で言ってたじゃない",
            "maleVariant": "SCSMのハッキングじゃないんだぞ… ハナコの警備はナイトシティいち厳重だって、自分で言ってたじゃないか"
          },
          {
            "stringId": 1719688778845392896,
            "femaleVariant": "そうだ。だからお前に、地元のフィクサーに連絡してくれと頼んでいる"
          },
          {
            "stringId": 1719823287339323392,
            "femaleVariant": "でも、ヨリノブが掴みかかり、首に手をかけて――",
            "maleVariant": "だが、ヨリノブが掴みかかり、首に手をかけて――"
          },
          {
            "stringId": 1719824072311705600,
            "femaleVariant": "あまりいい話じゃない。まずは身の安全を約束して",
            "maleVariant": "あまりいい話じゃない。まずは身の安全を約束しろ"
          },
          {
            "stringId": 1719825867020832768,
            "femaleVariant": "ヨリノブが父親を絞め殺したのよ",
            "maleVariant": "ヨリノブが父親を絞め殺したんだ"
          },
          {
            "stringId": 1725507041219186688,
            "femaleVariant": "わかった。あたしはあの夜――紺碧プラザの例の現場にいたの",
            "maleVariant": "わかった。俺はあの夜――紺碧プラザの例の現場にいたんだ"
          },
          {
            "stringId": 1725513043737972736,
            "femaleVariant": "もし私の立場にいたら、どうされますか？"
          },
          {
            "stringId": 1725513197551489024,
            "femaleVariant": "迷わずはね、その首を即座にヨリノブに差し出すだろう"
          },
          {
            "stringId": 1725513357018927104,
            "femaleVariant": "では、命拾いをされましたね… タケムラさん"
          },
          {
            "stringId": 1725514612810321920,
            "femaleVariant": "断る"
          },
          {
            "stringId": 1725515538577100800,
            "femaleVariant": "だがこのまま見逃してはやろう。街から出ていけ"
          },
          {
            "stringId": 1725515682475282432,
            "femaleVariant": "再び相まみえた時は容赦しない。二人ともだ"
          },
          {
            "stringId": 1761669785366642688,
            "femaleVariant": "ずいぶんと… 人目につかなそうな場所を選んだのね",
            "maleVariant": "ずいぶんと… 人目につかなそうな場所を選んだな"
          },
          {
            "stringId": 1763609774580961280,
            "femaleVariant": "もういい。それ以上言うな"
          },
          {
            "stringId": 1763610088264568832,
            "femaleVariant": "既にここへ足を運んだことを後悔していますよ"
          },
          {
            "stringId": 1763610997656784896,
            "femaleVariant": "己の死を招くことになるぞ"
          },
          {
            "stringId": 1763611191416852480,
            "femaleVariant": "だが真実だ！ ハナコ様には知る権利がある！"
          },
          {
            "stringId": 1763611467100065792,
            "femaleVariant": "この街であの方をお守りする、私の関心はそれだけです"
          },
          {
            "stringId": 1763613040937144320,
            "femaleVariant": "ええ。私は誓いを立てましたので。それを果たし損ねたあなたとは違う"
          },
          {
            "stringId": 1763613335712829440,
            "femaleVariant": "そのサブロウ様殺害の目撃者が目の前にいるというのに、お前はくだらぬ式典の心配とはな… 血迷ったか、オダ！"
          },
          {
            "stringId": 1763614626988036100,
            "femaleVariant": "見て見ぬふりをする気なの？",
            "maleVariant": "見て見ぬふりをする気か？"
          },
          {
            "stringId": 1763615193135190016,
            "femaleVariant": "見て見ぬふりをする気？",
            "maleVariant": "見て見ぬふりをする気か？"
          },
          {
            "stringId": 1763615971983888384,
            "femaleVariant": "あなたは今や賞金首だ"
          },
          {
            "stringId": 1763616338951933952,
            "femaleVariant": "この場ではねてヨリノブ様に差し出さないだけ、ありがたいとお考えください"
          },
          {
            "stringId": 1763618616878436356,
            "femaleVariant": "いつまでくだらない話を続けるつもり？",
            "maleVariant": "いつまでくだらない話を続けるつもりだ？"
          },
          {
            "stringId": 1763620128186830848,
            "femaleVariant": "いつまでくだらない話を続けるつもり？",
            "maleVariant": "いつまでくだらない話を続けるつもりだ？"
          },
          {
            "stringId": 1763624629799096320,
            "femaleVariant": "秘密の会合にはうってつけの場所だ"
          },
          {
            "stringId": 1773482353273737216,
            "femaleVariant": "ああ、約束しよう"
          },
          {
            "stringId": 1773482969299554304,
            "femaleVariant": "V。オダは信用できる"
          },
          {
            "stringId": 1773498691765538816,
            "femaleVariant": "その言葉、後悔するぞ！"
          },
          {
            "stringId": 1773521029856890880,
            "femaleVariant": "何だ？"
          },
          {
            "stringId": 1773521489401614336,
            "femaleVariant": "少しは関心を持ったらどう？",
            "maleVariant": "少しは関心を持ったらどうだ？"
          },
          {
            "stringId": 1773523947750297600,
            "femaleVariant": "手を貸して",
            "maleVariant": "手を貸してくれ"
          },
          {
            "stringId": 1773525541971693568,
            "femaleVariant": "まさか、黙って見過ごすつもりか！"
          },
          {
            "stringId": 1773531768181100544,
            "femaleVariant": "会うとしたらそこしかない"
          },
          {
            "stringId": 1773536483350548480,
            "femaleVariant": "そろそろ何者なのか話してくれない？",
            "maleVariant": "そろそろ何者なのか話してくれないか？"
          },
          {
            "stringId": 1773536956065386496,
            "femaleVariant": "ハナコ様の警護官だ"
          },
          {
            "stringId": 1773543147646619648,
            "femaleVariant": "そうだ。オダの信用を得られれば、彼女に会える"
          },
          {
            "stringId": 1773547905899958272,
            "femaleVariant": "そんなことはしない。誇り高い男だ"
          },
          {
            "stringId": 1795335569242198016,
            "femaleVariant": "車で来たか。正面から堂々と会ってくれるなら期待できる"
          },
          {
            "stringId": 1834250448589950976,
            "femaleVariant": "ハナコって、サブロウ・アラサカの娘で、空母でやって来たあの？"
          },
          {
            "stringId": 1834251294195191812,
            "femaleVariant": "そいつに裏切られる可能性もある"
          },
          {
            "stringId": 1834251773033713664,
            "femaleVariant": "本当に信用できる相手なの？",
            "maleVariant": "本当に信用できる相手なのか？"
          },
          {
            "stringId": 1834257123304673280,
            "femaleVariant": "信用できるの？",
            "maleVariant": "信用できるのか？"
          },
          {
            "stringId": 1834257759144382464,
            "femaleVariant": "アラサカの掟を優先しなければいいけど",
            "maleVariant": "アラサカの掟を優先しなければいいが"
          },
          {
            "stringId": 1834261841930227712,
            "femaleVariant": "そいつがアラサカの掟を優先しないといいけど",
            "maleVariant": "そいつがアラサカの掟を優先しないといいが"
          },
          {
            "stringId": 1834310363585056768,
            "femaleVariant": "アラサカのお偉いさんだもの、そりゃ誇り高いでしょうね",
            "maleVariant": "アラサカのお偉いさんだ、そりゃ誇り高いに決まってる"
          },
          {
            "stringId": 1834310961525035008,
            "femaleVariant": "オダはお前のつまらない道徳観に収まるような男ではない"
          },
          {
            "stringId": 1834328909924929536,
            "femaleVariant": "ああ"
          },
          {
            "stringId": 1834328948109873152,
            "femaleVariant": "へえ。すごい説得力ね",
            "maleVariant": "へえ。すごい説得力だ"
          },
          {
            "stringId": 1834329218541817856,
            "femaleVariant": "わかるだろ、俺に失うものはない"
          },
          {
            "stringId": 1834329940498976768,
            "femaleVariant": "ふざけないで、こんなとこで死ぬのはごめんよ",
            "maleVariant": "ふざけるな、こんなところで死ぬのはごめんだぞ"
          },
          {
            "stringId": 1834332273404416000,
            "femaleVariant": "掟とは？"
          },
          {
            "stringId": 1834332535380643840,
            "femaleVariant": "暗黙のルールよ。日本の本社は知らないけど、ナイトシティではそういうものがあった",
            "maleVariant": "暗黙のルールだ。日本の本社は知らないが、ナイトシティではそういうものがあった"
          },
          {
            "stringId": 1834333503157571584,
            "femaleVariant": "裏切りの気配でも感じようものなら、尋問官とともに精鋭の2個分隊が航空支援つきで配備される"
          },
          {
            "stringId": 1834377669396795392,
            "femaleVariant": "お前が言いたいことは十分わかった"
          },
          {
            "stringId": 1834378923342696448,
            "femaleVariant": "だがオダの場合、精鋭部隊や航空支援は必要ない。あいつの腕なら、ひとりで十分だからな。その点は俺が保証する"
          },
          {
            "stringId": 1834379975710347264,
            "femaleVariant": "それを聞いて安心しろって言うの？",
            "maleVariant": "それを聞いて安心しろって言うのか？"
          },
          {
            "stringId": 1834380998130667520,
            "femaleVariant": "心を落ち着けたいなら、空でも見上げてはどうだ"
          },
          {
            "stringId": 1834388519943802880,
            "femaleVariant": "危険が迫ってるのか？"
          },
          {
            "stringId": 1834388735816241152,
            "femaleVariant": "いえ。ですがサブロウ様を称える式典の間は警戒が必要です"
          },
          {
            "stringId": 1904117636988420100,
            "femaleVariant": "ワカコ・オカダってフィクサーがいる"
          },
          {
            "stringId": 1904118121497640960,
            "femaleVariant": "待って、相手が誰だかわかって言ってる？",
            "maleVariant": "待て、相手が誰だかわかって言ってるのか？"
          },
          {
            "stringId": 1904119072665124864,
            "femaleVariant": "ワカコ・オカダ――ジグジグ・ストリートのパチンコ屋のオーナーよ",
            "maleVariant": "ワカコ・オカダ――ジグジグ・ストリートのパチンコ屋のオーナーだ"
          },
          {
            "stringId": 1904119559523155968,
            "femaleVariant": "では、さっそく会いに行くとしよう"
          },
          {
            "stringId": 1904119741623058432,
            "femaleVariant": "現地集合だ。俺は用事を済ませてから向かう"
          },
          {
            "stringId": 1916686033140924420,
            "femaleVariant": "V、よく来てくれた"
          },
          {
            "stringId": 1916726654421516292,
            "femaleVariant": "話せ、V"
          },
          {
            "stringId": 1916726654522179588,
            "femaleVariant": "舌でも噛んだか？"
          },
          {
            "stringId": 1916726654706728964,
            "femaleVariant": "おい、何のつもりだ？"
          },
          {
            "stringId": 1953186235718377472,
