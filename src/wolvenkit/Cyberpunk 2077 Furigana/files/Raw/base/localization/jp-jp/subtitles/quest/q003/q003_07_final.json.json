    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「どうする」って？",
                    "maleVariant": null,
                    "stringId": 1677040229716549632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミスティんとこに戻って、命のありがたみってヤツを堪能するのさ",
                    "maleVariant": null,
                    "stringId": 1677043911795367936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これからどうする？",
                    "maleVariant": null,
                    "stringId": 1698645987386155012
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これからどうするの？",
                    "maleVariant": "これからどうするんだ？",
                    "stringId": 1698646637687824384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "死ぬかもって… 本気で思ったの？",
                    "maleVariant": "死ぬかもって… 本気で思ったのか？",
                    "stringId": 1754956442981298176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この仕事に危険はつきものよ",
                    "maleVariant": "この仕事に危険はつきものだ",
                    "stringId": 1754956483867373572
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この仕事に危険はつきものよ",
                    "maleVariant": "この仕事に危険はつきものだ",
                    "stringId": 1754956488950870016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本気で死の恐怖を感じた？",
                    "maleVariant": "本気で死の恐怖を感じたか？",
                    "stringId": 1754956705410510848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さて、デクスへの報告は任せたぜ。じゃあな",
                    "maleVariant": null,
                    "stringId": 1754957630472646704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや、そんなんじゃねえけどよ… 無事でよかったってだけさ",
                    "maleVariant": null,
                    "stringId": 1754959676101816320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今回もギリギリんとこで死神のカマをかわせたんだ。祝ったっていいだろ？ まあ毎度のことだけどな…",
                    "maleVariant": null,
                    "stringId": 1754960090180284416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "下手に出ず、借りを作らず、企業とのしがらみもなしに、フラットヘッドを手に入れた",
                    "maleVariant": null,
                    "stringId": 1782359662598746112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "100点満点だったぜ、V",
                    "maleVariant": null,
                    "stringId": 1782360903089319936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その通りね",
                    "maleVariant": "その通りだな",
                    "stringId": 1782361719518982148
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ケースバイケースよ。今回は運が良かった",
                    "maleVariant": "ケースバイケースだ。今回は運が良かった",
                    "stringId": 1782361729266544640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その通りね",
                    "maleVariant": "その通りだな",
                    "stringId": 1782361732018008064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ケースバイケースよ",
                    "maleVariant": "ケースバイケースだ",
                    "stringId": 1782361955289198592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しかしよ、この街は最高だ。数えきれねえチャンスと混乱、憎悪で溢れてる",
                    "maleVariant": null,
                    "stringId": 1782362689678274560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺が言うんだから間違いねえ。お前もだいぶわかってきたな",
                    "maleVariant": null,
                    "stringId": 1782368389351534592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "素直じゃねえな。だがな、これは運じゃなく、お前の決断が生んだ結果だ。それを忘れんな",
                    "maleVariant": null,
                    "stringId": 1782369169240420352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だが根性があって、それを活かす方法を知ってりゃ、どんなことだってできちまう",
                    "maleVariant": null,
                    "stringId": 1782370505562451968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "鉛弾を食らわない限りはね",
                    "maleVariant": "鉛弾を食らわない限りはな",
                    "stringId": 1782371192404897792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そん時は、派手に散りゃあいい！ 街の伝説として語り継がれる――永遠に生きれるんだ",
                    "maleVariant": null,
                    "stringId": 1782371409602736128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ずいぶんポジティブね",
                    "maleVariant": "ずいぶんポジティブだな",
                    "stringId": 1782381308109844480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "セミナーでもやったらどう？ きっと大儲けできるんじゃない？",
                    "maleVariant": "セミナーでもやったらどうだ？ きっと大儲けできるぞ",
                    "stringId": 1782381399663112192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バカにしやがって",
                    "maleVariant": null,
                    "stringId": 1782382654196215808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "生きるか死ぬかの戦いの後は、そいつに尽きる",
                    "maleVariant": null,
                    "stringId": 1782386088727212032
                  }
                }
              ]
            }
          }
        }
      }
    },
