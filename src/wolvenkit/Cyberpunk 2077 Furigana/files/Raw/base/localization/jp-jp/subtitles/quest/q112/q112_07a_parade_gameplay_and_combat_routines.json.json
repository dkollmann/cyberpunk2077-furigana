  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "重さに耐えられると思うか？",
                    "maleVariant": null,
                    "stringId": 1742970175138807808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大丈夫だって！ パレードはもう始まるぞ",
                    "maleVariant": null,
                    "stringId": 1742970175138807816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ。しかし、こいつは凄いな…",
                    "maleVariant": null,
                    "stringId": 1742970175138807824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サブロウ・アラサカもかわいそうに。世界一の権力者が見世物にされてる",
                    "maleVariant": null,
                    "stringId": 1786663378442272768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "会社のシンボルなんだから、当然でしょ",
                    "maleVariant": null,
                    "stringId": 1786664073270673408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "死んでもなお、会社の発展に貢献するなんて",
                    "maleVariant": null,
                    "stringId": 1786664614235226112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どういうこと？ エレベーターを止めたら困るじゃない",
                    "maleVariant": null,
                    "stringId": 1786670332430532608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "警備のためだ",
                    "maleVariant": null,
                    "stringId": 1786671161526992896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "冗談だろ？ ずっと下まで階段で降りろってのか？",
                    "maleVariant": null,
                    "stringId": 1786671555959341056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一瞬で地面まで降りれる方法を教えてやろうか？ 嫌なら階段を使え",
                    "maleVariant": null,
                    "stringId": 1786672226796322816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やめとけ、時間の無駄だ",
                    "maleVariant": null,
                    "stringId": 1786672627822116864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいのが撮れるぞ！ 企業文化に喝采を送る民衆と、その恩恵をこれっぽっちも享受していない君とのコントラストを表現するんだ",
                    "maleVariant": null,
                    "stringId": 1786674071266672640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "えっと… こんな感じ？",
                    "maleVariant": null,
                    "stringId": 1786677432078581760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいねえ！ 見てるだけでギンギンになっちまう！",
                    "maleVariant": null,
                    "stringId": 1786677521819910144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんた何してんだ！",
                    "maleVariant": null,
                    "stringId": 1786691287139762176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "知らないほうがいい",
                    "maleVariant": null,
                    "stringId": 1786692372944404484
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "悪いことは言わない、知らないほうが身のためよ",
                    "maleVariant": "悪いことは言わない、知らないほうが身のためだ",
                    "stringId": 1786693039519973376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わ、わかった。俺は何も見てない！",
                    "maleVariant": null,
                    "stringId": 1786693723728396288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まったくもう、どっかよそでやってよ！",
                    "maleVariant": null,
                    "stringId": 1786695934478925824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あいつらトムを殺して、死体も返さなかったのよ！ ないと賠償金ももらえないってのに！",
                    "maleVariant": null,
                    "stringId": 1786696794982334464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "落ち着いてよ！ 誰が聞いてるかわからないでしょ！",
                    "maleVariant": null,
                    "stringId": 1786697112122048512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "予想どおりだ。どの派閥も人を送り込んでる",
                    "maleVariant": null,
                    "stringId": 1786698747195965440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だが何も起きないだろう。天下のアラサカだ。内輪揉めの流血沙汰なら、見えない所でやるだはずだからな",
                    "maleVariant": null,
                    "stringId": 1786699471434825728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うわ、すごいな！",
                    "maleVariant": null,
                    "stringId": 1786728485465636864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "録画したぞ！ こんな横暴が許されると思うな！",
                    "maleVariant": null,
                    "stringId": 1792373583164346368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカはナイトシティに見せつけようとしてるのよ",
                    "maleVariant": null,
                    "stringId": 1792390914833780736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあ見せてくれ… どちゃくそ芸術的なコントラストを…",
                    "maleVariant": null,
                    "stringId": 1792411987755274240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しーっ…！ 声が大きいって！",
                    "maleVariant": null,
                    "stringId": 1792418329559699456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "コーポが私たちみたいのを気にかけるはずないでしょ！",
                    "maleVariant": null,
                    "stringId": 1792422064788738048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この街でひとたび権力を手に入れると、人知れず死んでいくことなんてできないのね",
                    "maleVariant": null,
                    "stringId": 1793711194961219584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "このハイエナども！ 人殺し！ 泥棒！ お前ら全員地獄に落ちろ！",
                    "maleVariant": null,
                    "stringId": 1793712372319129600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V！ 隠れろ！ 丸見えだぞ！",
                    "maleVariant": null,
                    "stringId": 1796619149963665408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そこなら安全だろう",
                    "maleVariant": null,
                    "stringId": 1796632799050964992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふう、今のは危なかった",
                    "maleVariant": null,
                    "stringId": 1796842788491337728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "注意しろ",
                    "maleVariant": null,
                    "stringId": 1796850045157351424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、死にたいのか！？ 身を隠すんだ！",
                    "maleVariant": null,
                    "stringId": 1802695717945319424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、少し耳を貸せ。オダが誰かと話してる",
                    "maleVariant": null,
                    "stringId": 1876328461862191104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう一度言う。警備がなってない！ 問題外だ！",
                    "maleVariant": null,
                    "stringId": 1876329161052667904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハナコ様に何かあったらどうする！",
                    "maleVariant": null,
                    "stringId": 1876330252293128192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヨリノブ様の命に異議を唱えるのか？",
                    "maleVariant": null,
                    "stringId": 1876330591075450880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自分の仕事をしろ",
                    "maleVariant": null,
                    "stringId": 1876331655187152896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "了解した。通信終わり",
                    "maleVariant": null,
                    "stringId": 1876331896225415168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やはり予想した通り、オダも来ているようだ",
                    "maleVariant": null,
                    "stringId": 1876332344126750720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おい、こんなとこで何してる。さっさと中に戻れ",
                    "maleVariant": null,
                    "stringId": 1876489611165843456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんだよ、いいだろ。きれいだなあ！ こんな華やかなのは滅多に見れないぞ",
                    "maleVariant": null,
                    "stringId": 1876490251669618688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それにアクセスポイントはどうやったって安全だ。地雷を仕掛けてるんだぞ？ 少しでも近づいたらドカン！ 木っ端みじんさ",
                    "maleVariant": null,
                    "stringId": 1876490751429328896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こっちも忙しいんだ。行けないから自分で対処してくれ",
                    "maleVariant": null,
                    "stringId": 1876493174445195264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、それがシステム用のハッチだ。テッキーに聞けば手伝ってくれるんじゃないか",
                    "maleVariant": null,
                    "stringId": 1876493767050018816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "進展があり次第、また連絡する",
                    "maleVariant": null,
                    "stringId": 1876512102785499136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "二人で同時に聞けるように、今繋ぐ",
                    "maleVariant": null,
                    "stringId": 1883579959955234816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あのクソボーグならよく知ってる！",
                    "maleVariant": null,
                    "stringId": 1936135520497188864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アダム・クソスマッシャーだ",
                    "maleVariant": null,
                    "stringId": 1936136081275633664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたを殺した男？",
                    "maleVariant": "お前を殺した男か？",
                    "stringId": 1936136579709943812
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "落ち着いて。確かなの？",
                    "maleVariant": "落ち着け。確かなのか？",
                    "stringId": 1936136949882437632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたを殺した男ね？",
                    "maleVariant": "お前を殺した男か？",
                    "stringId": 1936137436086157312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "男？ 人間と呼べるかも怪しいぜ！",
                    "maleVariant": null,
                    "stringId": 1936137805671448576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "落ち着いて。確かなの？",
                    "maleVariant": "落ち着け。確かなのか？",
                    "stringId": 1936138262028500992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、あの声だけは忘れねえ！",
                    "maleVariant": null,
                    "stringId": 1936139180698517504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だが… 正直、いてくれてよかったよ",
                    "maleVariant": null,
                    "stringId": 1936139497620127744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こんな危なっかしいとこを行くのかよ",
                    "maleVariant": null,
                    "stringId": 1947363110788546560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "タケムラの奴、“手違い”でお前をハメようとしてるんじゃねえか？",
                    "maleVariant": null,
                    "stringId": 1947363497989914624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほらな",
                    "maleVariant": null,
                    "stringId": 1947368244633419776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "狙撃手だ！ 狙われるぞ！ 隠れろ！",
                    "maleVariant": null,
                    "stringId": 2011482945800183812
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "姿をさらすな！",
                    "maleVariant": null,
                    "stringId": 2011483441315258376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まだだ、V！",
                    "maleVariant": null,
                    "stringId": 2011486144628072456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もっと慎重にやれ",
                    "maleVariant": null,
                    "stringId": 2011487746785091588
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "状況が落ち着くまで待て。俺が合図する",
                    "maleVariant": null,
                    "stringId": 2011488839485169688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいか。まだ警戒を解くなよ",
                    "maleVariant": null,
                    "stringId": 2011490461909065736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうして？",
                    "maleVariant": null,
                    "stringId": 2012951830520995840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "復讐相手のサブロウは既にオダブツだ。代わりに叩きのめす相手がいないとな",
                    "maleVariant": null,
                    "stringId": 2012952174487478272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "叩きのめす？ 相手をするのはあたしでしょ！",
                    "maleVariant": "叩きのめす？ 相手をするのは俺だろ！",
                    "stringId": 2012952631649837056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、そうだな。悪かった。引き金はまかせるしかないもんな",
                    "maleVariant": null,
                    "stringId": 2012955981338451968
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}