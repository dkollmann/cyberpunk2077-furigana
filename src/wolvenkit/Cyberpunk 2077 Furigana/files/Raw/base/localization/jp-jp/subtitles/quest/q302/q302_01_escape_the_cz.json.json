    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]First hour in Night City and we're already stealing the car? Not bad.",
                    "maleVariant": null,
                    "stringId": 2957913068197666828
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Car? We will attract attention.",
                    "maleVariant": null,
                    "stringId": 2957913070328373260
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Quick!",
                    "maleVariant": null,
                    "stringId": 2957913070345150464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Plant your ass here!",
                    "maleVariant": null,
                    "stringId": 2957913070647140356
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Can you hear me?",
                    "maleVariant": null,
                    "stringId": 2957913070680694788
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Okay, you can go. [Do you have the coordinates.?]",
                    "maleVariant": null,
                    "stringId": 2957913070848466944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Watch out, an enemy convoy is approaching.",
                    "maleVariant": null,
                    "stringId": 2957913070882021380
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Good. Now up the stairs to the parking lot. Your ride awaits.",
                    "maleVariant": null,
                    "stringId": 2959390627427016704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Crap... Gotta open it somehow.",
                    "maleVariant": null,
                    "stringId": 3043380209427963904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]So Mi? Ideas?",
                    "maleVariant": null,
                    "stringId": 3043386107256922112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Kurt's dogs sniffed you out. You better go.",
                    "maleVariant": null,
                    "stringId": 3060851599164858368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]We have to go. Now.",
                    "maleVariant": null,
                    "stringId": 3060853438232944640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You need to restore power. There is a technical shaft and a fuse box nearby. Walk along the scaffolding.",
                    "maleVariant": null,
                    "stringId": 3060857772945465344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Wait, I have an idea. Do you see this restaurant? Find an access point and hook up. I will attract them elsewhere, clear the way for you.",
                    "maleVariant": null,
                    "stringId": 3060862004327112704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Loud and clear, So Mi.",
                    "maleVariant": null,
                    "stringId": 3060875389928235008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Here, saving your pretty presidential butt.",
                    "maleVariant": null,
                    "stringId": 3060876074639974400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Put me on the radio. I will navigate.",
                    "maleVariant": null,
                    "stringId": 3060893276000772096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I don't like these drones. Can they spot us ...?",
                    "maleVariant": null,
                    "stringId": 3062278457513238528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]So Mi confirms. Says there's a big-ass mothership we gotta watch out for.",
                    "maleVariant": null,
                    "stringId": 3062281284407025664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]They can. But these are only the combat drone's scouts. It's their mama you need worry about.",
                    "maleVariant": null,
                    "stringId": 3063480079220236288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Okay, we have two options here. First - guns blazing. Second - clean and quiet. Thoughts?",
                    "maleVariant": null,
                    "stringId": 3063719505326268416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Down the slope. Heads low. They won't notice you.",
                    "maleVariant": null,
                    "stringId": 3063726857236537344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Stop.",
                    "maleVariant": null,
                    "stringId": 3064881246265237504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]On your left.",
                    "maleVariant": null,
                    "stringId": 3064882348779986944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]So Mi ...?",
                    "maleVariant": null,
                    "stringId": 3064890762704805888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I am. I'm opening right now, get ready.",
                    "maleVariant": null,
                    "stringId": 3069322534977036300
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Someone's controlling it remotely.  I touch it, I reveal your location.",
                    "maleVariant": null,
                    "stringId": 3069888711323119616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Hey, are you coming ?!",
                    "maleVariant": null,
                    "stringId": 3079612334359470080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]They're getting fidgety. Maybe they smell blood.",
                    "maleVariant": null,
                    "stringId": 3079661674104905728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Careful. It knows where you are now.",
                    "maleVariant": null,
                    "stringId": 3080843265074290688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Son of a...!",
                    "maleVariant": null,
                    "stringId": 3080849264304742400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Nice shot!",
                    "maleVariant": null,
                    "stringId": 3080858102256918528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]It's open!",
                    "maleVariant": null,
                    "stringId": 3080860973962649600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Have you seen these before?",
                    "maleVariant": null,
                    "stringId": 3081139344953073668
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Keep moving.",
                    "maleVariant": null,
                    "stringId": 3081140440236843008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]You sound like you've seen these before.",
                    "maleVariant": null,
                    "stringId": 3081141968272121856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Okay, let's get a move on.",
                    "maleVariant": null,
                    "stringId": 3081142016607281152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Always the same shit.",
                    "maleVariant": null,
                    "stringId": 3086865478816473088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Got a bulletproof one: move your ass.",
                    "maleVariant": null,
                    "stringId": 3086881295234867200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]On a training mission in Bolivia.",
                    "maleVariant": null,
                    "stringId": 3086881844353146880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Got any tactical advice?",
                    "maleVariant": null,
                    "stringId": 3086883687917527040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]My thoughts exactly.",
                    "maleVariant": null,
                    "stringId": 3086887000394276864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Not if Myers gets behind the wheel and you hook me up to the radio. I'll call out enemy patrols.",
                    "maleVariant": null,
                    "stringId": 3086895227387355136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Songbird's idea.",
                    "maleVariant": null,
                    "stringId": 3086901806304174080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]I also have one. Let's get rid of them silently.",
                    "maleVariant": null,
                    "stringId": 3086902323730292736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Where you at? You're missing all the action!.",
                    "maleVariant": null,
                    "stringId": 3086909040622936064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]My ass has been in places much worse than this. But thanks for your concern.",
                    "maleVariant": null,
                    "stringId": 3086910553575497728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]It's not over yet. Stay focused on the road.",
                    "maleVariant": null,
                    "stringId": 3086911321485451264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Stop the car and stop the engine. I'll take them on a different route.",
                    "maleVariant": null,
                    "stringId": 3086917453893443584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Did you intercept their communication? Wow, that was fast.",
                    "maleVariant": null,
                    "stringId": 3086918744296235008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]That's what I'm here for.",
                    "maleVariant": null,
                    "stringId": 3086923473256554496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][db_db]Okay, gonna stay here.",
                    "maleVariant": null,
                    "stringId": 3091179957731549184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]We have to get out of here!",
                    "maleVariant": null,
                    "stringId": 3091231056937181184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][db_db]Songbird says we can bring the power back. Let me think.",
                    "maleVariant": null,
                    "stringId": 3093874310102999040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Not bad. Then you give me the bearings of your ripper.",
                    "maleVariant": null,
                    "stringId": 3094190334098628608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Do you think it will start? I don't want to get stuck here.",
                    "maleVariant": null,
                    "stringId": 3140465514772979712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]There's only one way to find out.",
                    "maleVariant": null,
                    "stringId": 3140472568149467136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]They slowed down. They probably want the drone to do all the work for them. Now's your chance to sneak out.",
                    "maleVariant": null,
                    "stringId": 3140477835557756928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]We gotta get around this fucking drone somehow. Try to force through the door on the right.",
                    "maleVariant": null,
                    "stringId": 3140488865167990784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Stop.",
                    "maleVariant": null,
                    "stringId": 3152058347740819516
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]On your left.",
                    "maleVariant": null,
                    "stringId": 3152058347740819528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]So Mi ...?",
                    "maleVariant": null,
                    "stringId": 3152058347757596672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Someone's controlling it remotely.  I touch it, I reveal your location.",
                    "maleVariant": null,
                    "stringId": 3152058347757596684
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Good. Now up the stairs to the parking lot. Your ride awaits.",
                    "maleVariant": null,
                    "stringId": 3152090860005715988
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Car? We will attract attention.",
                    "maleVariant": null,
                    "stringId": 3152090860005716000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Not if Myers gets behind the wheel and you hook me up to the radio. I'll call out enemy patrols.",
                    "maleVariant": null,
                    "stringId": 3152090860005716012
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][GT]Quick!",
                    "maleVariant": null,
                    "stringId": 3152090860005716024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][db_db]Songbird tells V, that the cloaking she used during the drive will deactivate, once they leave the car. it's only a matter of time till the militia will be able to track it again.",
                    "maleVariant": null,
                    "stringId": 3204347258374479872
                  }
                }
              ]
            }
          }
        }
      }
    },
