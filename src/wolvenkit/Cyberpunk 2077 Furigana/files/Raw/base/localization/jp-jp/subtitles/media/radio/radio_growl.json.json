    "ArchiveFileName": "C:\\Users\\Dan\\cyberpunk2077-furigana\\src\\wolvenkit\\Cyberpunk 2077 Furigana\\files\\Raw\\base\\localization\\jp-jp\\subtitles\\media\\radio\\radio_growl.json"
  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こないだ、どこぞのクローム野郎がカート・ハンセンについて大演説をぶってた。ドッグタウンに逃れてきた人を守ってくれてるとか言って",
              "maleVariant": "",
              "stringId": "3662535910635528192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、誰か教えてよ。私たちが“守られてる”んなら、スカベンジャーがうろついてんのはなんで？",
              "maleVariant": "",
              "stringId": "3662539113758384128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "毎日失踪者が出てるのに安心なんかできる？ 最後は切り刻まれてバケツの中だよ？",
              "maleVariant": "",
              "stringId": "3662539156288626688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何が“守ってくれる”だ",
              "maleVariant": "",
              "stringId": "3662539759899303936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私は世の中のことに精一杯目を光らせていようと思ってる。でも正直、1人でできることには限界がある",
              "maleVariant": "",
              "stringId": "3662540413925515264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だからgrowlfm.webを立ち上げたんだ",
              "maleVariant": "",
              "stringId": "3662541182540111872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろん、話のソースはちゃんと確かめること。大手のニュース局みたいなのはナシだよ",
              "maleVariant": "",
              "stringId": "3662541253608398848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここらにいる連中は、いかがわしい過去を抱えてる奴がほとんどだ",
              "maleVariant": "",
              "stringId": "3662541981789904896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもいい？ 私たちを決めるのは過去じゃない。大事なのは今、どんな自分になろうとあがいてるかだ",
              "maleVariant": "",
              "stringId": "3662549169048576000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "過去は過去。変えることのできない確定事項サ。だけど未来なら作っていける…",
              "maleVariant": "",
              "stringId": "3662549265953775616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今日はみんなに大事な質問がある。運命は… 自分で決められる？ それとも言いなりになるしかない？ 最近よく考えるんだ…",
              "maleVariant": "",
              "stringId": "3662549813981536256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "子供の頃は予め決められた道を歩むのも悪くないと思ってた。未来が定まってる安心感っていうのかな…",
              "maleVariant": "",
              "stringId": "3662550765903994880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも最近は、自分の運命を決めるのが自分じゃないことにイラついてる",
              "maleVariant": "",
              "stringId": "3662551001707765760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Growl FMは責任を負う覚悟がある。でもみんなはどう思う？ 運命って大事？ それともどうでもいいこと？",
              "maleVariant": "",
              "stringId": "3662551405753458688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんなの意見を聞かせて。うちのサイト、growlfm.webでね",
              "maleVariant": "",
              "stringId": "3662551820905668608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "73年に起きたバイオテクニカの化学物質流出事件を知ってる？",
              "maleVariant": "",
              "stringId": "3662553676482535424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知らない？ だよね。公にはなかったことになってるから。報道も一切されてない",
              "maleVariant": "",
              "stringId": "3662554368442032128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも本当にあったんだ。間違いない。だってこの目で見たから。その現場は… パシフィカだった",
              "maleVariant": "",
              "stringId": "3662554528614113280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だってそうっしょ？ 自分で道を選ぶ自由がないなら、どうして自分の行動に責任を持てんの？",
              "maleVariant": "",
              "stringId": "3662555066843979776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "怪しいものや、今すぐ世界に共有すべきことを見つけたら知らせて。私がみんなにその情報を届けるから！",
              "maleVariant": "",
              "stringId": "3662555682316148736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのせいで大勢が悲惨な目に遭ったんだ。第3度熱傷、失明、出生異常、その他もろもろ",
              "maleVariant": "",
              "stringId": "3662556825867968512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だけどメディアのクズどもは目をそむけた。片っ端から企業に買収されてね",
              "maleVariant": "",
              "stringId": "3662556884588224512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悲しいことに、ナイトシティには金のために魂を売るメディアやジャーナリストしかいない。たとえ、それが血で汚れた金でもね",
              "maleVariant": "",
              "stringId": "3662557300193419264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直言って、ヘドが出るよ",
              "maleVariant": "",
              "stringId": "3662557690431463424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今日は“変化”について。それも前向きな変化。滅多に起こりはしないけど、大抵は災難の後にやってくる",
              "maleVariant": "",
              "stringId": "3662559300708655104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少なくとも私はそう思ってる。人間ってのは保守的な生き物なんだ… 必要に迫られない限り、ちょっとでも不安になることはしたがらない",
              "maleVariant": "",
              "stringId": "3662560151749713920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それが自分のためでもね！ どん底に落ちてみないと、何かを変えようと奮い立つことができないんだ",
              "maleVariant": "",
              "stringId": "3662560822771245056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "個人レベルでもそうなんだから、社会に前向きな変化を起こしたい時はどうする？ 言葉はもちろん、行動で呼びかけたって変化は起きない",
              "maleVariant": "",
              "stringId": "3662560956250775552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もっと大きな何かが必要なんだ。ドールの1人をレイプされて殺されたリジー・ボーデンが、3人のタイガークロウズの頭に斧を突き立てた時みたいに",
              "maleVariant": "",
              "stringId": "3662561556053024768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさに強烈なメッセージ！ おかげでリジーは殺されたけど、それが決め手になった",
              "maleVariant": "",
              "stringId": "3662561839336316928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "導火線に火がつき、ついに起こった大爆発！",
              "maleVariant": "",
              "stringId": "3662562302219706368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私たちはナイトシティで暴動を起こし、モックスを結成した。そうやって象徴になったんだ。そして大勢の人の生活がマシになった",
              "maleVariant": "",
              "stringId": "3662562380401532928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それが前向きな変化だよ。小さな意思表示じゃ何も起こらない",
              "maleVariant": "",
              "stringId": "3662562630868590592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この言い回しは聞いたことあるっしょ？「ドッグタウンにいるのはクズだけ。あそこに行き着いた奴は、どんな悲惨な運命に遭おうと仕方ない」",
              "maleVariant": "",
              "stringId": "3662563863624867840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メディアが最初に言いだして、自分の考えを持たない能無しどもがそれを広めてく",
              "maleVariant": "",
              "stringId": "3662564303842238464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜかわかる？ 私たちを見下せば、残りのナイトシティが魅力的に思えるから。それでみんな満足する",
              "maleVariant": "",
              "stringId": "3662564499900784640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「暮らしは最悪だ！ でもドッグタウンの連中よりはマシか」ってね",
              "maleVariant": "",
              "stringId": "3662564699784536064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お決まりのパターンだ",
              "maleVariant": "",
              "stringId": "3662565265143160832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうやって大衆をおとなしくさせてるんだ",
              "maleVariant": "",
              "stringId": "3662566363916591104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろんドッグタウンにも欠点はある。それも山ほどね。でも、それと引き換えにささやかな自由を手に入れることができる",
              "maleVariant": "",
              "stringId": "3662566532359839744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私は断然こっちの方がいいね",
              "maleVariant": "",
              "stringId": "3662566825659129856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "寝る前にジャンクフードを食べるなって言うっしょ？ スコップチーズとかポップタードとかトーフツナとか…",
              "maleVariant": "",
              "stringId": "3662567028344676352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あとバナナバターベアも追加しとこう。昨日、寝る前に1パック食べちゃってサ",
              "maleVariant": "",
              "stringId": "3662567427222986752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おかげでものすごい夢を見たよ。リズィー・ウィズィーのやらしい夢とかじゃない。チビって冷や汗びっしょりで目覚めるようなヤツ",
              "maleVariant": "",
              "stringId": "3662567706379083776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "深い夢の中で、私は濡れた道を裸で走ってた。場所はパシフィカ。ナイトシティ市民の大群が私を追ってくる",
              "maleVariant": "",
              "stringId": "3662567893495373824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこからが妙なんだ。振り返ると、そいつらがみんな私の顔をしてる。見た目は私そのままなんだけど、どこかおかしい。目がなかったんだ",
              "maleVariant": "",
              "stringId": "3662568139784904704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで一番の極めつけは、店の窓に映った自分の姿を見たとき。そこには顔がなかった",
              "maleVariant": "",
              "stringId": "3662568380789612544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真っ赤な組織がむき出しで、まぶたのない眼球がこっちを見てた。ゾッとしたね",
              "maleVariant": "",
              "stringId": "3662569010421751808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ってわけで、夢占いの専門家がいたら、この夢の意味を教えてくれる？ マジで知りたいから、連絡待ってるよ",
              "maleVariant": "",
              "stringId": "3662569106622308352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最近は街中が移譲派と連邦派の話題で持ちきりだね",
              "maleVariant": "",
              "stringId": "3662569685218156544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自治権のある自由州か、中央政府が治める統一国家か。どっちにもメリット、デメリットがある",
              "maleVariant": "",
              "stringId": "3662570737770356736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だからみんなに聞いてみようと思ってさ。どう？ 支持してる奴はいる？",
              "maleVariant": "",
              "stringId": "3662571332925317120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スピーカーにこう叫んでるリスナーもいるだろうね。「知ったこっちゃねえ！」って",
              "maleVariant": "",
              "stringId": "3662571401074368512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかるよ。中立主義って魅力的だもん。無関心でいれば心が乱れることもない",
              "maleVariant": "",
              "stringId": "3662571781598404608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも、どうしても大昔に父親から聞いた話を思い出すんだ",
              "maleVariant": "",
              "stringId": "3662572059731091456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんなにも聞かせてあげる。昔々あるところに、自分で何も決められないロバがいました。ロバはお腹を空かせて、納屋の中でじーっとしてました",
              "maleVariant": "",
              "stringId": "3662572120011628544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "幸い、納屋の隅っこに干し草がありました。でもこれでめでたし、めでたしとはならない。なぜならロバは喉も乾いてたから",
              "maleVariant": "",
              "stringId": "3662572471511080960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "納屋の反対の隅には水おけがあった。で、こっからが問題。干し草と水は、どっちも同じぐらい離れた場所にあったんだ",
              "maleVariant": "",
              "stringId": "3662572542965243904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「どっちに先に行こう？」、ロバは悩んだ。干し草？ それとも水？ ロバは決められなかった",
              "maleVariant": "",
              "stringId": "3662573142784270336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "結局、ロバはどっちも選べずに飢えと渇きで死んでしまいましたとさ。RIP、ロバのチューマ",
              "maleVariant": "",
              "stringId": "3662573215479947264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全く同じ状況とは言えないけど、この話の教訓はただ1つ。「グズグズすんな」ってこと",
              "maleVariant": "",
              "stringId": "3662573412662566912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "判断できずに迷う時も、とにかくどっちかを選ぶんだ。手遅れになる前にね",
              "maleVariant": "",
              "stringId": "3662573659220533248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、ここで1曲いってみよう",
              "maleVariant": "",
              "stringId": "3662574217062965248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃあ、続きはこの曲の後で",
              "maleVariant": "",
              "stringId": "3662631482516234256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "次はフレッシュな音楽を聴いてみよう",
              "maleVariant": "",
              "stringId": "3662631998029750288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さーて、リクエストに応えてGrowl FMオリジナルをお届けするよ",
              "maleVariant": "",
              "stringId": "3662633962406862864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃあこのへんで音楽を。みんなにいいバイブスを届けていくよ",
              "maleVariant": "",
              "stringId": "3662658708045488144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そろそろ音楽をかけようかな。それじゃ、いってみよう",
              "maleVariant": "",
              "stringId": "3662658958613209104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新しい音楽に出会いたい？ じゃあこれならどう？",
              "maleVariant": "",
              "stringId": "3662661814296698896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チューマたち、ボリュームをマックスまで上げちゃって。音楽の時間だよ",
              "maleVariant": "",
              "stringId": "3662662210238996496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もっと曲が聴きたい？ アッシュに任せな！",
              "maleVariant": "",
              "stringId": "3662662649885941776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もっと曲が聴きたい？ アッシュに任せな！",
              "maleVariant": "",
              "stringId": "3662662939796234256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、この続きはまた後で",
              "maleVariant": "",
              "stringId": "3662683442812477456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここでみんなにビートのプレゼントだ",
              "maleVariant": "",
              "stringId": "3662684000369696784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドライブにぴったりのBGMがほしい？ 任せて、チューマ",
              "maleVariant": "",
              "stringId": "3662684287041986572"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうもカートの話は怪しいね。スカベンジャーを好きにうろつかせるだけじゃなく——いや、それだけでも最悪なんだけど",
              "maleVariant": "",
              "stringId": "3662688058224599056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "代表をもてなしてるってどういうこと？ 何をしたいのか、まるで見当がつかない",
              "maleVariant": "",
              "stringId": "3662688695473594368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつがここまで堕ちると思わなかったけど、期待しすぎたみたいだね",
              "maleVariant": "",
              "stringId": "3662688883999170560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんな警戒しな。私のカンがうずいてる。これから大変なことが起こるよ",
              "maleVariant": "",
              "stringId": "3662689915227529228"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バーゲストがそこら中を嗅ぎ回ってるんだ。何かあったのは間違いない",
              "maleVariant": "",
              "stringId": "3662689915227529244"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今は関わらないほうがよさそうだね。せめて何が起きたのかわかるまでは",
              "maleVariant": "",
              "stringId": "3662689915227529260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カートの最近の放送を聞いた？ 「コーポの居場所はない」なんてプロパガンダをブチ上げてたけど！ ハッ！",
              "maleVariant": "",
              "stringId": "3662691083072757784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんであんなに必死に宣伝してるかわかる？ あいつは企業の連中と比べられたくないんだ。似た者同士だってバレちゃうかもしれないから",
              "maleVariant": "",
              "stringId": "3662691083089534980"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どっちも利益と権力だけを追い求めて、情けの欠片もない。奴らはコインの表裏ってわけ",
              "maleVariant": "",
              "stringId": "3662691083089534996"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンセン、あんたの本性はわかってるよ",
              "maleVariant": "",
              "stringId": "3662691933476282368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バーゲストがまたやらかした。イーストサイドで今朝、一般市民を射殺したんだ。白昼堂々とね",
              "maleVariant": "",
              "stringId": "3662692217631989788"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "隠す気もまるでない。相手は連中の情報を嗅ぎ回る潜入捜査官だった、「正当な裁き」だ、なんて悪びれずに抜かしてるそうだよ",
              "maleVariant": "",
              "stringId": "3662692217648766984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まったく… 「影の戦争」は終わったんだよ、カート君。スパイがそこら中を駆け回って、破壊工作してる時代じゃないんだ",
              "maleVariant": "",
              "stringId": "3662692217648767000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴の妄想は日に日に悪化してる。このままじゃ大変なことになるよ",
              "maleVariant": "",
              "stringId": "3662692217648767016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とびきりのいいニュース！ この局の人気が爆上がり中だ。今までより… ていうか、私が思ってたよりずっと多くのリスナーが聞いてくれてる！",
              "maleVariant": "",
              "stringId": "3662694289416839196"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最近じゃ街のあちこちでGrowl FMのタグも見かけるんだ。すごいよ、チューマたち。もう最高！",
              "maleVariant": "",
              "stringId": "3662694289433616392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "てことで… この調子で突っ走るよ！",
              "maleVariant": "",
              "stringId": "3662694289433616408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よく聞かれるんだ。なんでドッグタウンなんかでやってるのって。 どっか他でやればってサ",
              "maleVariant": "",
              "stringId": "3662695223001800728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知ってる人もいると思うけど、ドッグタウンは私にとって特別な場所なんだ。どんな底に落ちた私はここで… 家族を見つけた",
              "maleVariant": "",
              "stringId": "3662695223001800744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その家族の支えで、私は立ち直ることができたんだ。どんなに絶望的な状況でも、希望はあるんだって教えてくれた",
              "maleVariant": "",
              "stringId": "3662695223001800760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "絶望といえばドッグタウンみたいなもんっしょ？",
              "maleVariant": "",
              "stringId": "3662695223001800776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから私はここにいる。ナイトシティの深き闇を照らす、小さな光になれたらと思ってね。海賊放送じゃ大した影響はないかもだけど、精一杯やれることをやってる",
              "maleVariant": "",
              "stringId": "3662696821199396864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "方法は何であれ、誰にだってできることはあるはずだからサ",
              "maleVariant": "",
              "stringId": "3662697311094104064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AVが空からドッグタウンに何か落としてるっしょ？ あれはハンセン用の投下物資なんだ",
              "maleVariant": "",
              "stringId": "3662698118396960804"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "黄金の神への贈り物。でもお宝を横取りするチャンスでもある。その度胸があるならね",
              "maleVariant": "",
              "stringId": "3662698118413737992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく頭上には注意すること。ヘビーなプレゼントが突然降ってくるかもしれないよ",
              "maleVariant": "",
              "stringId": "3662698118413738008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こないだもAVのスラスターが故障して、傭兵がペシャンコになったんだって。肉とクロームが一瞬でスープと火花になったってわけ",
              "maleVariant": "",
              "stringId": "3662698118413738024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "皮肉なことに、貨物の中身は医療品だった。シャレにもなんないね…",
              "maleVariant": "",
              "stringId": "3662698118413738040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "超ビッグニュース！ スペースフォース・ワンがドッグタウンに堕ちた！",
              "maleVariant": "",
              "stringId": "3662700775169777700"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイヤーズ大統領が乗ってたって噂だけど、真偽は定かじゃない",
              "maleVariant": "",
              "stringId": "3662700775169777716"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他に情報はないけど、バーゲストが墜落現場を封鎖してる。ってことはつまり…？",
              "maleVariant": "",
              "stringId": "3662700775186554884"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさかこんなビッグニュースを届ける日が来るなんてね。続報が入ったらすぐに伝えるよ",
              "maleVariant": "",
              "stringId": "3662700775186554900"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スペースフォース・ワンがドッグタウンで撃墜された。それだけで、とんだ大ニュースだ",
              "maleVariant": "",
              "stringId": "3662704932077363228"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも他にも何かある。この件はどうもムズムズするんだよね",
              "maleVariant": "",
              "stringId": "3662704932094140420"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "特に気になるのは… もし… 言っちゃうけど、これが計画的な攻撃だとしたら、どんな意味を持つと思う？",
              "maleVariant": "",
              "stringId": "3662704932094140436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すぐに侵略が始まる？ ナイトシティで新たな内戦が勃発する？",
              "maleVariant": "",
              "stringId": "3662704932094140452"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "色んな「もしも」が頭の中でガラガラ音を立ててる。赤ちゃんがいたらあやせそうなくらい",
              "maleVariant": "",
              "stringId": "3662705799778197504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんなの意見も聞かせて。一体何が起きたのか… そして、この先何が起きると思う？",
              "maleVariant": "",
              "stringId": "3662706045295976448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "恐ろしい時代だよね。でも大丈夫。私たちなら乗り越えられる！",
              "maleVariant": "",
              "stringId": "3662706274757959680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "例の噂は正しかったようだよ。ロザリンド・マイヤーズは確かにドッグタウンにいた。驚きだね！",
              "maleVariant": "",
              "stringId": "3662706822282403848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただし… 新合衆国大統領とのミーグリを期待してる人は残念。そのチャンスはおあずけになった",
              "maleVariant": "",
              "stringId": "3662706822282403864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうやら大統領はすでにワシントンに帰ったみたいだよ",
              "maleVariant": "",
              "stringId": "3662706822282403880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもおかしなことに… ハンセンは彼女がここにいたこと自体を否定してる",
              "maleVariant": "",
              "stringId": "3662706822282403896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "呆れちゃうよね！ 封鎖をかいくぐって逃げられたことがよっぽど悔しかったみたいだよ",
              "maleVariant": "",
              "stringId": "3662706822282403912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "かわいそうに。ま、人生上手くいかないこともあるサ",
              "maleVariant": "",
              "stringId": "3662706822299181076"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今日はブラック・サファイア周辺が騒がしかった",
              "maleVariant": "",
              "stringId": "3662709983931981832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "高級AV、リムジンのパレード。どうやら我らの王様が何かデカいことをしようとしてるみたい",
              "maleVariant": "",
              "stringId": "3662709983931981848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "君子、危うきに近寄らず。あのゴキブリキングと配下の虫ケラたちに関わるとロクなことがない",
              "maleVariant": "",
              "stringId": "3662709983931981864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハチの巣をつつくときっと痛い目を見るよ",
              "maleVariant": "",
              "stringId": "3662709983931981880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんな聞いた？ ハンセンが非常事態を宣言したってサ",
              "maleVariant": "",
              "stringId": "3662712264693833748"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドッグタウンはいつだって非常事態みたいなもんなのにね",
              "maleVariant": "",
              "stringId": "3662712264693833764"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何かあのジジイの気に食わないことが起きたらしい… でも悪いけど、それ聞いてニヤニヤしてるんだ",
              "maleVariant": "",
              "stringId": "3662712264693833780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんな気をつけな。カートの手下どもが動き出したよ。そこら中を捜索して、ドンパチやってる",
              "maleVariant": "",
              "stringId": "3662712496370409500"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "街を封鎖する徹底ぶりだ。ドッグタウンは出入り禁止だってサ",
              "maleVariant": "",
              "stringId": "3662712496387186688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カートの奴、刑務所長にでもなったつもりかっての… なに、今に始まったことじゃないって？",
              "maleVariant": "",
              "stringId": "3662712496387186704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドッグタウンの封鎖は継続中。カートはひっきりなしに放送で喚き散らしてる",
              "maleVariant": "",
              "stringId": "3662713903962054680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バーゲストの巡回はそこら中で暴れてるし、1秒おきにどっかで銃撃戦が起きてる",
              "maleVariant": "",
              "stringId": "3662713903962054696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これまでだって酷いもんだったけど、この調子じゃ銃声が生活音の一部になりそう",
              "maleVariant": "",
              "stringId": "3662713903978831880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんなも気をつけて。流れ弾を食らったら笑えないよ",
              "maleVariant": "",
              "stringId": "3662714610266071040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とびきりのニュースがあるよ！ 焦らさないで単刀直入にいこう",
              "maleVariant": "",
              "stringId": "3662715170406981632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カート・ハンセン大佐が死んだ。スタジアムでズドンだって。銃によって生き、銃によって死んだわけ",
              "maleVariant": "",
              "stringId": "3662715170406981648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "苦痛に泣き叫びながら死んだことを願うね。心の底から",
              "maleVariant": "",
              "stringId": "3662715170406981664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最高の日だよ、チューマ。喜びを噛みしめな！",
              "maleVariant": "",
              "stringId": "3662715940581859328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バーゲストのトップが変わった。これからはベネットって奴がドッグタウンの王らしい",
              "maleVariant": "",
              "stringId": "3662717014977994780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "組織の頭にどんだけのアタマがあるか見ものだけど、きっと立派な野犬並みだろうね",
              "maleVariant": "",
              "stringId": "3662717014977994796"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あ、これ褒めてないから",
              "maleVariant": "",
              "stringId": "3662717014977994812"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カート・ハンセン大佐の後釜が決まった",
              "maleVariant": "",
              "stringId": "3662717988039745560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ死んだんだから当たり前だけどサ。でもこれはさしていいニュースじゃない",
              "maleVariant": "",
              "stringId": "3662717988039745576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バーゲストはドッグタウンを覆う暗雲だ。奴らがうろついてる限り、私たちは雲が晴れるのを待たなきゃならない",
              "maleVariant": "",
              "stringId": "3662717988039745592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とはいえ、後釜はカートよりマシなはず… だよね？",
              "maleVariant": "",
              "stringId": "3662719003463966720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スカベンジャーの騒ぎがいろいろあったのを覚えてる？ 驚いたことに——",
              "maleVariant": "",
              "stringId": "3662719742416445468"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "失踪者が減ってる。それも激減！ 連中はよそへ移ったか、何かに怯えて逃げたみたいだね",
              "maleVariant": "",
              "stringId": "3662719742433222668"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あるいは誰かが正義の裁きを下したのかも。ま、正直どれでもいいけど",
              "maleVariant": "",
              "stringId": "3662719742433222684"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大事なのは辺りが少し安全になったこと。それだけで十分だよ",
              "maleVariant": "",
              "stringId": "3662719742433222700"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "重大なお知らせよ。まあ、一部の人にとってはね",
              "maleVariant": "",
              "stringId": "3662720781026779144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とあるインプラントの不具合が相次いでるって噂なんだ… ミスタースタッドの。悪い意味で、アツアツドロドロになっちゃうみたい",
              "maleVariant": "",
              "stringId": "3662720781026779160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自家製の黒焦げソーセージを作りたいわけじゃないなら、不具合が直るまでアップグレードは控えておいたほうがいいね",
              "maleVariant": "",
              "stringId": "3662720781026779176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「アッシュ、今日はちょっとおかしいぞ」って思ってるあんたは正解",
              "maleVariant": "",
              "stringId": "3662724103284645928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうにも気分が沈んじゃってサ。悪い報せを聞いたんだ",
              "maleVariant": "",
              "stringId": "3662724103284645944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私の大事な友達が… 死んじゃってね",
              "maleVariant": "",
              "stringId": "3662724103301423108"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "酷い目に遭わされたあげく、自ら命を絶った",
              "maleVariant": "",
              "stringId": "3662724103301423124"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "女優になる夢を必死で追いかけてた… あの子、本当に本気だったんだよ",
              "maleVariant": "",
              "stringId": "3662724103301423140"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも夢を噛み砕き、悪夢を吐き出すのがこの街だ。来る日も来る日もね",
              "maleVariant": "",
              "stringId": "3662724103301423156"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それが普通だって、みんな受け入れてる。でもさ、そんなのおかしいっしょ",
              "maleVariant": "",
              "stringId": "3662724103301423172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく、その友達の話がしたかったんだ。聞いてくれてありがと",
              "maleVariant": "",
              "stringId": "3662725857913004032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて、今日のニュースだよ。アドレナリンジャンキーたちにとっては朗報だね",
              "maleVariant": "",
              "stringId": "3662726198020726828"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "長いあいだ故障していたエデンビーチのコースターが、どういうわけか動きだしたんだって",
              "maleVariant": "",
              "stringId": "3662726198037504012"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "理由はさっぱりだけど、とにかく稼働してる。ガタガタうなりをあげながらね",
              "maleVariant": "",
              "stringId": "3662726198037504028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "定期点検を何度かすっ飛ばしてるかもしれないけど、死の危険があったほうがスリルも高まるってもんっしょ？",
              "maleVariant": "",
              "stringId": "3662726198037504044"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チューマたち――サブロウ・アラサカが死んだって。殺されたって噂もある",
              "maleVariant": "",
              "stringId": "3662727888610451484"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "紺碧プラザで銃撃戦があったって情報も入ってるから、あながち突飛な噂でもなさそうだね",
              "maleVariant": "",
              "stringId": "3662727888610451500"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真相に迫れないか、少し探りを入れてみようかな",
              "maleVariant": "",
              "stringId": "3662727888610451516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大物の死はそうあることじゃないからね。ましてサブロウほどの大物なら。死してなお厄介な爺さんだよ",
              "maleVariant": "",
              "stringId": "3662727888627228676"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今日は推理してみよう。犯人は誰か？ なぜサブロウ・アラサカを殺したのか？",
              "maleVariant": "",
              "stringId": "3662729063569219592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "爺さんには敵が大勢いた。容疑者リストはXLサイズのミスタースタッドより長い",
              "maleVariant": "",
              "stringId": "3662729063569219608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこで問うべきは、“クイ・ボーノ”… つまり、皇帝の死で一番得をするのは誰か？",
              "maleVariant": "",
              "stringId": "3662729063569219624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それさえわかれば犯人もきっとわかる",
              "maleVariant": "",
              "stringId": "3662729063569219640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ、誰に賭ける？ おもしろい仮説があったら聞かせて",
              "maleVariant": "",
              "stringId": "3662730192038322176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ニュースだよ、チューマたち。ジャパンタウンのドールハウス「クラウド」が当面のあいだ閉店になった",
              "maleVariant": "",
              "stringId": "3662730863814828056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜかって？ 縄張り争いで滅茶苦茶にされたんだ",
              "maleVariant": "",
              "stringId": "3662730863814828072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残念だよ。クラウドは文句なしに最高の店だったから。お客も代わりの行き場を探すのに苦労するはずサ",
              "maleVariant": "",
              "stringId": "3662730863814828088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも仕方ない。こんな街じゃなおさらね。最後には何もかもが壊される。徹底的に、心や体まで",
              "maleVariant": "",
              "stringId": "3662730863814828104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういうとこなんだ…",
              "maleVariant": "",
              "stringId": "3662730863831605260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ニュースの時間だよ。ジャパンタウンのドールハウス「クラウド」の経営者が代わった",
              "maleVariant": "",
              "stringId": "3662732409231302688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新しい経営者は元ドールらしい。最高のニュースだと思わない？",
              "maleVariant": "",
              "stringId": "3662732409248079880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新しい店がどうなったか見に行った人は、どんな感じだったか報告をよろしく",
              "maleVariant": "",
              "stringId": "3662732409248079896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すっごい興味あるからサ",
              "maleVariant": "",
              "stringId": "3662732409248079912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう聞いてるかもしれないけど、GIMことグランド・インペリアル・モールで騒ぎがあったようだね",
              "maleVariant": "",
              "stringId": "3662733553118666784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうやらアニマルズのグループが壊滅したみたい",
              "maleVariant": "",
              "stringId": "3662733553118666800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しかも驚きなのが、やったのは一匹狼だって噂だよ。たった1人のパンクにボコられたってわけ",
              "maleVariant": "",
              "stringId": "3662733553135443972"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "想像したら笑っちゃってサ！",
              "maleVariant": "",
              "stringId": "3662733553135443988"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バカデカいあいつらも、しばらくは傷をなめてるだろうね。どんだけ体を改造したって、赤っ恥をかいた心の傷は癒せないから！",
              "maleVariant": "",
              "stringId": "3662733553135444004"
            }
          ]
        }
      }
    },
