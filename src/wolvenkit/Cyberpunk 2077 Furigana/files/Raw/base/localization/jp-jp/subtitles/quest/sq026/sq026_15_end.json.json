              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マイコは正しい",
                    "maleVariant": null,
                    "stringId": 1754881289358393348
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やってくれたね",
                    "maleVariant": null,
                    "stringId": 1754881759019778048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしてあいつの言いなりになったの？",
                    "maleVariant": null,
                    "stringId": 1754888968457928704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マイコに託そうと思った",
                    "maleVariant": null,
                    "stringId": 1754889761382711296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "物事は少しずつ変えるべき",
                    "maleVariant": "物事は少しずつ変えるべきだ",
                    "stringId": 1754889816428756992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マイコは正しいよ。裏で画策してたのは事実だけど、話しても理解を得られないと思ったんでしょ",
                    "maleVariant": "マイコは正しい。裏で画策してたのは事実だが、話しても理解を得られないと思ったんだろうな",
                    "stringId": 1754890155697618944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マイコに託すことにしたの。彼女を信じて誘ったんでしょ？",
                    "maleVariant": "マイコに託すことにした。彼女を信じて誘ったんだろ？",
                    "stringId": 1754892121282691072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "革命って言えば聞こえはいいけど、これだけ大きなことは少しずつ変えていかなきゃ",
                    "maleVariant": "革命って言えば聞こえはいいが、これだけ大きなことは少しずつ変えていかないと",
                    "stringId": 1754892204229246976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もういい… ごめん。私が取り乱した。こんなことになると思わなかったから",
                    "maleVariant": null,
                    "stringId": 1754892626880872448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "気持ちはわかる",
                    "maleVariant": null,
                    "stringId": 1754894151862697988
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "エヴリンも一度に全てを手に入れようとした",
                    "maleVariant": null,
                    "stringId": 1754894895663792128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "苛立つのはわかるよ",
                    "maleVariant": "苛立つのはわかるが",
                    "stringId": 1754895331804299264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "気を悪くしないでほしいんだけど、ひとりにさせて",
                    "maleVariant": null,
                    "stringId": 1754896129057603584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "エヴリンも一度に全てを手に入れようとした、そして急ぎすぎた",
                    "maleVariant": null,
                    "stringId": 1754896322784116736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうね",
                    "maleVariant": null,
                    "stringId": 1754896322800893952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "行くね",
                    "maleVariant": null,
                    "stringId": 1754897013082669056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "また助けられた。ありがとう、V",
                    "maleVariant": null,
                    "stringId": 1754897753259884544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ごめん",
                    "maleVariant": "すまない",
                    "stringId": 1754901624552144896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしのせいじゃない",
                    "maleVariant": "俺のせいじゃない",
                    "stringId": 1754901624568922112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ごめん、ジュディ。こんなはずじゃなかった",
                    "maleVariant": "すまない、ジュディ。こんなはずじゃなかった",
                    "stringId": 1754901954341879808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ…",
                    "maleVariant": null,
                    "stringId": 1754902467758243840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "正当防衛よ。あたしのせいじゃない",
                    "maleVariant": "正当防衛だ。俺のせいじゃない",
                    "stringId": 1754902813318561792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "悪いけど… そうは思えない。方法はあったはず",
                    "maleVariant": null,
                    "stringId": 1754902813318561800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マイコを… 殺したの！？",
                    "maleVariant": null,
                    "stringId": 1775165119327617024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クソ… 早く気づくべきだった",
                    "maleVariant": null,
                    "stringId": 1775165237657321476
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クラウドは解放された。大事なのはそこよ",
                    "maleVariant": "クラウドは解放された。大事なのはそこだ",
                    "stringId": 1775167152491315200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "単にだまされるのと、人の善意を信じてだまされるのは違う",
                    "maleVariant": null,
                    "stringId": 1775167152491315216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マイコが何を企んでたかなんて、もう関係ない。クラウドはドールたちのものになった。大事なのはそこよ",
                    "maleVariant": "マイコが何を企んでたかなんて、もう関係ない。クラウドはドールたちのものになった。大事なのはそこだ",
                    "stringId": 1775167152558424068
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうね。目的は達成できた",
                    "maleVariant": null,
                    "stringId": 1775167152575201280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自分を責めないで、ジュディ。単にだまされるのと、人の善意を信じてだまされるのは違う",
                    "maleVariant": "自分を責めるなよ、ジュディ。単にだまされるのと、人の善意を信じてだまされるのは違う",
                    "stringId": 1775167152608755716
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "Vの善意は、本物だってわかってるから安心ね",
                    "maleVariant": null,
                    "stringId": 1775167152608755724
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "報酬を受け取らないだけの分別はあったようだけど",
                    "maleVariant": null,
                    "stringId": 1775174024422211584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それどころか、報酬まで受け取っちゃってさ",
                    "maleVariant": null,
                    "stringId": 1775174225866244100
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何それ。適当なこと言わないでくれる？",
                    "maleVariant": null,
                    "stringId": 1775176021649113092
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マイコからカネを受け取ったでしょ。それが唯一の真実じゃない",
                    "maleVariant": null,
                    "stringId": 1775210963204554752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ありがとう",
                    "maleVariant": null,
                    "stringId": 1775231205888737280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ありがとう",
                    "maleVariant": null,
                    "stringId": 1775231376731127812
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "惨劇を避けたかった",
                    "maleVariant": null,
                    "stringId": 1782345598342512640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もし穏便に済ませなかったら、ヒロミの他にも犠牲者が出てた",
                    "maleVariant": null,
                    "stringId": 1782345995912200196
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヒロミよりひどい支配人になってた",
                    "maleVariant": null,
                    "stringId": 1782349004838780928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしにはわかる。マイコはヒロミより傲慢な支配人になってた",
                    "maleVariant": "俺にはわかる。マイコはヒロミより傲慢な支配人になってた",
                    "stringId": 1782349433580535808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だから殺しても構わないって言うの？",
                    "maleVariant": null,
                    "stringId": 1782350665967714304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ウソでしょ… 信じられない",
                    "maleVariant": null,
                    "stringId": 1892205587805724672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうかもしれない。ごめん",
                    "maleVariant": "そうかもしれない。すまない",
                    "stringId": 1892275977655640068
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マイコもこうなることは予測できたはず",
                    "maleVariant": null,
                    "stringId": 1892277587983163392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、きっと殺さずにすむ方法はあった。こんな結果になって残念よ",
                    "maleVariant": "ああ、きっと殺さずにすむ方法はあった。こんな結果になって残念だ",
                    "stringId": 1892282898525675520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私もよ",
                    "maleVariant": null,
                    "stringId": 1892291141373222912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マイコも自分が何をしてるかわかってたはずよ。こうなることは予測できた",
                    "maleVariant": "マイコも自分が何をしてるかわかってたはずだ。こうなることは予測できた",
                    "stringId": 1892293021595820032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でもこんな風に身を滅ぼすなんて…",
                    "maleVariant": null,
                    "stringId": 1892296319258288128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうね… でも自業自得よ",
                    "maleVariant": "ああ… でも自業自得だ",
                    "stringId": 1892296690605187072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの女はきっと最初から… 私たちを裏切るつもりだったのよ",
                    "maleVariant": null,
                    "stringId": 1892332694913376256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私ってなんてバカ",
                    "maleVariant": null,
                    "stringId": 1892333729262297088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あなたのおかげよ",
                    "maleVariant": null,
                    "stringId": 1892344652135882752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふざけないで！",
                    "maleVariant": null,
                    "stringId": 1892422475668217856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この街でこれだけ大きなクラブを運営するには、強力な舵取りが必要よ",
                    "maleVariant": "この街でこれだけ大きなクラブを運営するには、強力な舵取りが必要だ",
                    "stringId": 1892422658254659584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたが信用してる相手を疑う理由がある？",
                    "maleVariant": "あんたが信用してる相手を疑う理由があるか？",
                    "stringId": 1892438285358948352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私のせいにしないでくれる？",
                    "maleVariant": null,
                    "stringId": 1892438861136224256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "してない。彼女はクラウドの過酷な現実をよくわかってるのよ… あんたよりもね",
                    "maleVariant": "してない。彼女はクラウドの過酷な現実をよくわかってるんだ… あんたよりもな",
                    "stringId": 1892439077830746112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティは駆け引きが全てよ",
                    "maleVariant": "ナイトシティは駆け引きが全てだ",
                    "stringId": 1892445515466629120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私とあなたはそこが違うのね。あなたには利益が大事だけど、私には人が大事なの",
                    "maleVariant": null,
                    "stringId": 1892445781955928064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あなたは傭兵で、あいつは卑劣な裏切り者。私はただの間抜けってわけ",
                    "maleVariant": null,
                    "stringId": 1892456948099145728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "革命の犠牲になるのはいつだってその当事者よ。クラウドを混乱させて事態が悪化すれば、結局みんなが路頭に迷うことになる",
                    "maleVariant": "革命の犠牲になるのはいつだってその当事者だ。クラウドを混乱させて事態が悪化すれば、結局みんなが路頭に迷うことになる",
                    "stringId": 1892461376864739328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "夢物語のために、どれだけの命を犠牲にするつもり？",
                    "maleVariant": "夢物語のために、どれだけの命を犠牲にするつもりだ？",
                    "stringId": 1892464001190420480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも慣れないといけないのよね… 何をやろうとしたって、物事は一筋縄じゃいかないんだから",
                    "maleVariant": null,
                    "stringId": 1892499926679248896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それとギャングの後ろ盾もね",
                    "maleVariant": "それとギャングの後ろ盾もな",
                    "stringId": 1892514551982415872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それを忘れてたわ",
                    "maleVariant": null,
                    "stringId": 1892536555586412544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わ、わかってるけど、いったん落ち着かないと",
                    "maleVariant": null,
                    "stringId": 1904483304446058496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しばらく… ひとりで考えさせて… お願い",
                    "maleVariant": null,
                    "stringId": 1910142416004907008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "またね、V",
                    "maleVariant": null,
                    "stringId": 1911209990960513024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあまたね",
                    "maleVariant": null,
                    "stringId": 1912548097131610112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[離れる] またね、ジュディ",
                    "maleVariant": "[離れる] またな、ジュディ",
                    "stringId": 1914240191504121860
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それじゃあ… またね、ジュディ",
                    "maleVariant": "それじゃあ… またな、ジュディ",
                    "stringId": 1914240515069509632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
