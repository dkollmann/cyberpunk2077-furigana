        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待って！ 落ち着いて！ 話し合いましょ！",
                    "maleVariant": null,
                    "stringId": 1235544069053497344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お願いだから、待って！ 話せばわかる！",
                    "maleVariant": null,
                    "stringId": 1235555259221688320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この状況でどんな言い訳するのか聞いてあげる。まさか、あの子が見えなかったとでも言うつもり？",
                    "maleVariant": "この状況でどんな言い訳するのか聞いてやろう。まさか、あの子が見えなかったとでも言うつもりか？",
                    "stringId": 1235559626649780224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "詳しく聞かせて",
                    "maleVariant": "詳しく聞かせてくれ",
                    "stringId": 1235574151893630976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "30秒あげる。説明して",
                    "maleVariant": "30秒やる。説明しろ",
                    "stringId": 1235574662290096128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "手を打とう",
                    "maleVariant": null,
                    "stringId": 1235579879651950592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった… ただし絶対に約束は守って。でないと必ず見つけ出す",
                    "maleVariant": "わかった… ただし絶対に約束は守れよ。でないと必ず見つけ出す",
                    "stringId": 1235581576533131264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "聞いてあげる",
                    "maleVariant": "聞いてやる",
                    "stringId": 1235586890028433408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人をひき殺しておいて保険だなんて… 無茶苦茶ね。責任を免れたつもり？ 父親が納得すると？",
                    "maleVariant": "人をひき殺しておいて保険だなんて… 無茶苦茶だな。責任を免れたつもりか？ 父親が納得すると？",
                    "stringId": 1235589303296737280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "逃がすつもりはない",
                    "maleVariant": null,
                    "stringId": 1235590759491977216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どこにも行かせない",
                    "maleVariant": null,
                    "stringId": 1235590893642596352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ん？ 何…？",
                    "maleVariant": null,
                    "stringId": 1239880860791357440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたを殺しにきた",
                    "maleVariant": null,
                    "stringId": 1239894828025004032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "話すって何を？ あたしは仕事を片付けたいだけ",
                    "maleVariant": "話すって何を？ 俺は仕事を片付けたいだけだ",
                    "stringId": 1239895392628654080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待ってよ！ お願いだから聞いて！",
                    "maleVariant": null,
                    "stringId": 1239895880224882688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そこまで言うなら",
                    "maleVariant": null,
                    "stringId": 1239900157274112000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたに逃げ場はない",
                    "maleVariant": null,
                    "stringId": 1239900157290889228
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたの行き先は死体置き場よ",
                    "maleVariant": "あんたの行き先は死体置き場だ",
                    "stringId": 1239901294568030208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "向こうが勝手に轢かれたのよ！ ね、冷静になって。お金をあげるから、見逃してよ。この街からも出てくから。それでいいでしょ？",
                    "maleVariant": null,
                    "stringId": 1239903639905386496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言われなくてもわかってる",
                    "maleVariant": null,
                    "stringId": 1239904423468482560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その子の両親か、婚約者に私を殺せって雇われたんでしょ？",
                    "maleVariant": null,
                    "stringId": 1239905349386256384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょ、ちょっと待ってよ、どういう意味？ あの子って？",
                    "maleVariant": null,
                    "stringId": 1239905680467836928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたのリムジンを血まみれにした子よ",
                    "maleVariant": "あんたのリムジンを血まみれにした子だ",
                    "stringId": 1759050123180392448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、あれね… よかった、てっきりライバル会社の刺客かと！",
                    "maleVariant": null,
                    "stringId": 1759051574644457472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうも誤解してるようね。対人賠償保険には入ってるし、保険金はとっくに支払い済み。もう解決してるの",
                    "maleVariant": null,
                    "stringId": 1903826112509956096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あら、招かれざるお客様？ どこの馬の骨か知らないけど、後悔することになるわよ",
                    "maleVariant": null,
                    "stringId": 1903829352559464448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いくらの契約か知らないけど、私の金庫にはそれよりたくさんのお金がある",
                    "maleVariant": null,
                    "stringId": 1903838571186671616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "逃がしてくれたら、金庫の中身を全部あげる。私は街のどこかに移るから、あなたのフィクサーにも、誰にもバレやしない。いい話でしょ",
                    "maleVariant": null,
                    "stringId": 1903839387213680640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあもう消えて。ただし、スピードは控えめにね",
                    "maleVariant": "じゃあもう消えろ。ただし、スピードは控えめにな",
                    "stringId": 1903844455795183616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたは罪を犯した。命で償ってもらう",
                    "maleVariant": null,
                    "stringId": 1925453871266975744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたは最低な罪を犯した。あの子が貧しいから、どうせ誰も気にしないだろうって思ったんでしょ",
                    "maleVariant": "あんたは最低な罪を犯した。あの子が貧しいから、どうせ誰も気にしないだろうって思ったんだろ",
                    "stringId": 1925454595220623360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "話はおしまいよ",
                    "maleVariant": "話は終わりだ",
                    "stringId": 1925458550969389056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "黙って、話は終わりよ。遺言なら聞いてあげるけど",
                    "maleVariant": "黙れ、話は終わりだ。遺言なら聞いてやるが",
                    "stringId": 1925459331797798912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あなたが受けた仕事についても… 同じように、解決できる方法があるはずよ",
                    "maleVariant": null,
                    "stringId": 1925463045250011136
                  }
                }
              ]
            }
          }
        }
      }
    },
