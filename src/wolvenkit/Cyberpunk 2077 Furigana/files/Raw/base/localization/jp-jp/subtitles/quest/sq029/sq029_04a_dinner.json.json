      "Properties": {
        "entries": [
          {
            "stringId": 1467850356291133440,
            "femaleVariant": "おいしそうだけど、味はどうかしら"
          },
          {
            "stringId": 1467850356307910656,
            "femaleVariant": "[座る] 環境が変わってよかったみたいね",
            "maleVariant": "[座る] 環境が変わってよかったみたいだな"
          },
          {
            "stringId": 1467850356324687872,
            "femaleVariant": "リバーは家族のよさに気づいたのよ"
          },
          {
            "stringId": 1467850356324687880,
            "femaleVariant": "家族と警察の仕事はいい組み合わせじゃない。今はこうして過去を取り戻そうとしてる"
          },
          {
            "stringId": 1492191602757197824,
            "femaleVariant": "環境が変わって、前より元気になったね",
            "maleVariant": "環境が変わって、前より元気になったな"
          },
          {
            "stringId": 1498083931053481984,
            "femaleVariant": "彼がこんなふうに悩んでない姿は、久しぶりよ"
          },
          {
            "stringId": 1725868908841242624,
            "femaleVariant": "あなたはどうなの、V？ 家族や親戚は？"
          },
          {
            "stringId": 1794038828387930112,
            "femaleVariant": "V… 私に家族を取り戻させてくれて、本当にありがとう"
          },
          {
            "stringId": 1794040322701004800,
            "femaleVariant": "座れよ"
          },
          {
            "stringId": 1805526332400963584,
            "femaleVariant": "前は、家族は荷物でしかないと思ってた"
          },
          {
            "stringId": 1805526332400963600,
            "femaleVariant": "ノーマッドとしてのファミリーはいたけど、散り散りになった"
          },
          {
            "stringId": 1805526332400963616,
            "femaleVariant": "ヘイウッドが家族よ",
            "maleVariant": "ヘイウッドが家族だ"
          },
          {
            "stringId": 1805526332434518020,
            "femaleVariant": "前は何よりもキャリアを優先してたから、家族なんて邪魔な荷物だと思ってたの",
            "maleVariant": "前は何よりもキャリアを優先してたから、家族なんて邪魔な荷物だと思ってたんだ"
          },
          {
            "stringId": 1805526332451295236,
            "femaleVariant": "前はノーマッドだったけど、ファミリーは散り散りになったの",
            "maleVariant": "前はノーマッドだったけど、ファミリーは散り散りになったんだ"
          },
          {
            "stringId": 1805526332468072452,
            "femaleVariant": "ヘイウッドで育ったの。街のみんなが家族だった",
            "maleVariant": "ヘイウッドで育ったんだ。街のみんなが家族だった"
          },
          {
            "stringId": 1846205795756281856,
            "femaleVariant": "あまり家族は好きじゃない"
          },
          {
            "stringId": 1846205836625580032,
            "femaleVariant": "いないし、欲しくもない"
          },
          {
            "stringId": 1846206095045038080,
            "femaleVariant": "いつかは持ちたいとは思ってる"
          },
          {
            "stringId": 1846206313870266372,
            "femaleVariant": "あまり好きじゃない。家族や血縁ってのは苦しみを生むから",
            "maleVariant": "あまり好きじゃない。家族や血縁ってのは苦しみを生むからな"
          },
          {
            "stringId": 1846206317611585536,
            "femaleVariant": "自分の面倒見るので精いっぱいよ",
            "maleVariant": "自分の面倒を見るので精いっぱいだ"
          },
          {
            "stringId": 1846207348856721408,
            "femaleVariant": "誰もいないし、欲しくもない"
          },
          {
            "stringId": 1846207352547708928,
            "femaleVariant": "あたしにそういう居場所はないの。それでいいのよ",
            "maleVariant": "俺にそういう居場所はないんだ。それで構わない"
          },
          {
            "stringId": 1846208050026909700,
            "femaleVariant": "そうね… この先、機会があったら持ちたいと思ってる",
            "maleVariant": "そうだな… この先、機会があったら持ちたいと思ってる"
          },
          {
            "stringId": 1846208053600456704,
            "femaleVariant": "こうやってみんなで集まって喋ったりするのは楽しいから",
            "maleVariant": "こうやってみんなで集まって喋ったりするのは楽しいからな"
          },
          {
            "stringId": 1846213434909265920,
            "femaleVariant": "家族がいなくてもうまくはいかなかったけど",
            "maleVariant": "家族がいなくてもうまくはいかなかったがな"
          },
          {
            "stringId": 1846213626169528320,
            "femaleVariant": "大昔のことみたいね",
            "maleVariant": "大昔のことみたいだな"
          },
          {
            "stringId": 1846214057947959296,
            "femaleVariant": "近所で助け合うのが当たり前の世界よ",
            "maleVariant": "近所で助け合うのが当たり前の世界さ"
          },
          {
            "stringId": 1846265908621873152,
            "femaleVariant": "私には子供がすべてよ"
          },
          {
            "stringId": 1846267123325865984,
            "femaleVariant": "あたしはいい親になれそう？",
            "maleVariant": "俺はいい親になれそうかな？"
          },
          {
            "stringId": 1846267527841320960,
            "femaleVariant": "ここにランディもいたら最高だったね",
            "maleVariant": "ここにランディもいたら最高だったな"
          },
          {
            "stringId": 1846275782953013248,
            "femaleVariant": "あたしが母親になるのを想像できる？",
            "maleVariant": "俺が父親になるのを想像できるか？"
          },
          {
            "stringId": 1846275810836746240,
            "femaleVariant": "家族と言えば、ランディがいないのが残念ね",
            "maleVariant": "家族と言えば、ランディがいないのが残念だな"
          },
          {
            "stringId": 1846277477552492544,
            "femaleVariant": "次は一緒だ"
          },
          {
            "stringId": 1846277638731206656,
            "femaleVariant": "ジャンバラヤが大好きなのよ"
          },
          {
            "stringId": 1846279320244457472,
            "femaleVariant": "私だって、別に特別いいお母さんってわけでもないし…"
          },
          {
            "stringId": 1846279464226525184,
            "femaleVariant": "Vがママになると思う人！",
            "maleVariant": "Vがパパになると思う人！"
          },
          {
            "stringId": 1846282931523424256,
            "femaleVariant": "ふう、危ないところだった"
          },
          {
            "stringId": 1846284047258619904,
            "femaleVariant": "相棒に感謝だろ"
          },
          {
            "stringId": 1846286076664221696,
            "femaleVariant": "ありがとう、ジョス！"
          },
          {
            "stringId": 1846288481392922624,
            "femaleVariant": "そんな面と向かって…"
          },
          {
            "stringId": 1846288790899003392,
            "femaleVariant": "兄はあなたのことが好きよ"
          },
          {
            "stringId": 1846289068746477568,
            "femaleVariant": "ジョス…"
          },
          {
            "stringId": 1846289389543624704,
            "femaleVariant": "隠してどうするのよ？"
          },
          {
            "stringId": 1846295256301178880,
            "femaleVariant": "リバーとくっつける気？",
            "maleVariant": "リバーとくっつける気か？"
          },
          {
            "stringId": 1846295256301178896,
            "femaleVariant": "リバーに頼まれて言ってるんでしょ",
            "maleVariant": "リバーに頼まれて言ってるんだろ"
          },
          {
            "stringId": 1846295259975389184,
            "femaleVariant": "やめてよ。あたしが早々に結婚するタイプに見える？",
            "maleVariant": "勘弁してくれ、俺が早々に結婚するタイプに見えるか？"
          },
          {
            "stringId": 1846295263616045056,
            "femaleVariant": "しないの？"
          },
          {
            "stringId": 1846295267239923712,
            "femaleVariant": "Vとリバーおじさんはお似合いだと思う人？"
          },
          {
            "stringId": 1846295281483780100,
            "femaleVariant": "そう言えって彼に頼まれたんでしょう？",
            "maleVariant": "そう言えってリバーに頼まれたんだろ？"
          },
          {
            "stringId": 1846295285023772672,
            "femaleVariant": "あら、よくわかったね"
          },
          {
            "stringId": 1883388680721108996,
            "femaleVariant": "ねえ！ そろそろご飯よ！"
          },
          {
            "stringId": 1914119652338327552,
            "femaleVariant": "過ちを犯して、人生が狂った時もあったけど、子供がいたから… 今でも何とかやれてる"
          },
          {
            "stringId": 1914126121062391812,
            "femaleVariant": "[手を挙げる] はーい！"
          },
          {
            "stringId": 1914126741534171136,
            "femaleVariant": "まあ、いいじゃないか"
          },
          {
            "stringId": 1914127155948183552,
            "femaleVariant": "リバー警部、至急応援を乞う"
          },
          {
            "stringId": 1914134651202985984,
            "femaleVariant": "V巡査、すぐに助けに向かう！"
          },
          {
            "stringId": 1914206182641786880,
            "femaleVariant": "ありがとね！",
            "maleVariant": "ありがとな！"
          },
          {
            "stringId": 1924415122102685696,
            "femaleVariant": "はーい！"
          },
          {
            "stringId": 1960615631227334660,
            "femaleVariant": "着いた"
          },
          {
            "stringId": 1960615631244111880,
            "femaleVariant": "ここは？"
          },
          {
            "stringId": 1960615631244111896,
            "femaleVariant": "給水塔。いい場所だろ"
          },
          {
            "stringId": 1960615631277666308,
            "femaleVariant": "俺が支えるから、乗り越えて、反対側から開けてくれないか？"
          },
          {
            "stringId": 1960615631277666324,
            "femaleVariant": "わかった。じゃあ、いいよ",
            "maleVariant": "わかった。じゃあ、いいぞ"
          },
          {
            "stringId": 1960615631311220752,
            "femaleVariant": "[乗り越える] わかった"
          },
          {
            "stringId": 1960615631428661268,
            "femaleVariant": "大西洋と太平洋の間で一番ひどい街らしいぞ"
          },
          {
            "stringId": 1960615631428661284,
            "femaleVariant": "でもこの景色は大好きだ。見てると落ち着く"
          },
          {
            "stringId": 1960615631579656192,
            "femaleVariant": "なかなかがんばってるじゃない",
            "maleVariant": "なかなかがんばってるじゃないか"
          },
          {
            "stringId": 1960615631579656208,
            "femaleVariant": "うまくやれてるか？ チャンスはあるかな？"
          },
          {
            "stringId": 1960615631613210640,
            "femaleVariant": "ダメよ、リバー。できない",
            "maleVariant": "ダメだ、リバー。できない"
          },
          {
            "stringId": 1960615631663542276,
            "femaleVariant": "リバー、ごめん。できない",
            "maleVariant": "リバー、すまん。できない"
          },
          {
            "stringId": 1960615631663542292,
            "femaleVariant": "このトレーラーパークで最高の色男を振るってのか"
          },
          {
            "stringId": 1960615631663542308,
            "femaleVariant": "リバー"
          },
          {
            "stringId": 1960615631680319488,
            "femaleVariant": "いいんだ、気にするな"
          },
          {
            "stringId": 1960615631965532160,
            "femaleVariant": "なかなかがんばってるじゃない",
            "maleVariant": "なかなかがんばってるじゃないか"
          },
          {
            "stringId": 1960615631982309380,
            "femaleVariant": "リバーのことは好きだけど、そういうのじゃない",
            "maleVariant": "リバーのことは好きだが、そういうのじゃないな"
          },
          {
            "stringId": 1960615631999086596,
            "femaleVariant": "リバー… もしこの関係がどこかに向かうと思ってるのなら…"
          },
          {
            "stringId": 1960615631999086612,
            "femaleVariant": "チャンスはない、か？"
          },
          {
            "stringId": 1960615632015863820,
            "femaleVariant": "友達のままでいない？",
            "maleVariant": "友達のままでいよう"
          },
          {
            "stringId": 1960615632015863836,
            "femaleVariant": "ああ、そうだな"
          },
          {
            "stringId": 1960615632049418240,
            "femaleVariant": "[飲む] 友情に"
          },
          {
            "stringId": 1960615632066195468,
            "femaleVariant": "[飲む] ジョスと子供たちに"
          },
          {
            "stringId": 1960615632066195484,
            "femaleVariant": "[飲む] ナイトシティに"
          },
          {
            "stringId": 1960615632099749892,
            "femaleVariant": "ジョスと子供たちに乾杯"
          },
          {
            "stringId": 1960615632099749908,
            "femaleVariant": "ああ、家族に"
          },
          {
            "stringId": 1960615632133304324,
            "femaleVariant": "友情に"
          },
          {
            "stringId": 1960615632133304340,
            "femaleVariant": "友情に"
          },
          {
            "stringId": 1960615632166858752,
            "femaleVariant": "あたしを好きになったら苦労するよ",
            "maleVariant": "俺を好きになったら苦労するぞ"
          },
          {
            "stringId": 1960615632183635972,
            "femaleVariant": "友達でしょ。そのままでいよう",
            "maleVariant": "友達だろ。そのままでいよう"
          },
          {
            "stringId": 1960615632217190400,
            "femaleVariant": "あたしを好きにならないで…",
            "maleVariant": "俺を好きにならないでくれ…"
          },
          {
            "stringId": 1960615632217190416,
            "femaleVariant": "もう遅いよ"
          },
          {
            "stringId": 1960615632250744836,
            "femaleVariant": "飲むだろ？"
          },
          {
            "stringId": 1960615632301076484,
            "femaleVariant": "もっともってない？",
            "maleVariant": "もっと持ってないか？"
          },
          {
            "stringId": 1960615632301076500,
            "femaleVariant": "ウイスキー2本とビール1ケースあれば十分だろ？"
          },
          {
            "stringId": 1960615632301076516,
            "femaleVariant": "それはどうかな"
          },
          {
            "stringId": 1960615632317853696,
            "femaleVariant": "覚悟しろよ。長い夜、そしてもっと長い朝になるからな"
          },
          {
            "stringId": 1960615632401739780,
            "femaleVariant": "まったく。まさかサツなんかとヤることになるなんて"
          },
          {
            "stringId": 1960615632401739796,
            "femaleVariant": "耳栓して、目玉をくり抜いておかねえと"
          },
          {
            "stringId": 1960615632435294212,
            "femaleVariant": "そうね。あたしは今、外を見渡せるまさにこの場所が好きかな",
            "maleVariant": "そうだな。俺は今、外を見渡せるまさにこの場所が好きだ"
          },
          {
            "stringId": 1960615632452071432,
            "femaleVariant": "街の喧騒から離れて、ちょっとだけ故郷を思い出す",
            "maleVariant": "街の喧騒から離れて、ちょっとだけ故郷を思い出すよ"
          },
          {
            "stringId": 1960615632485625860,
            "femaleVariant": "んー、そうかな？",
            "maleVariant": "んー、言うほどか？"
          },
          {
            "stringId": 1960615632485625876,
            "femaleVariant": "高層ビルが… 台無しにしてない？",
            "maleVariant": "高層ビルが… 台無しにしてるだろ"
          },
          {
            "stringId": 1960615632535957508,
            "femaleVariant": "いい眺めね。お勧めの場所なだけある",
            "maleVariant": "いい眺めだ。お勧めの場所なだけある"
          },
          {
            "stringId": 1960615632535957524,
            "femaleVariant": "言ったろ…"
          },
          {
            "stringId": 1960615632569511936,
            "femaleVariant": "[座る] あたしもこの眺めが好き",
            "maleVariant": "[座る] 俺もこの眺めが好きだ"
          },
          {
            "stringId": 1960615632569511952,
            "femaleVariant": "[座る] 高層ビルが邪魔だね",
            "maleVariant": "[座る] 高層ビルが邪魔だな"
          },
          {
            "stringId": 1960615632586289156,
            "femaleVariant": "[座る] 今いる場所が好き",
            "maleVariant": "[座る] 今いる場所が好きだ"
          },
          {
            "stringId": 1960615632586289172,
            "femaleVariant": "[座る] 企業はストリートを無茶苦茶にして高みの見物ね",
            "maleVariant": "[座る] 企業はストリートを無茶苦茶にして高みの見物だ"
          },
          {
            "stringId": 1960615632586289188,
            "femaleVariant": "[座る] ここからだとビルの中で何が起きてるかを見ずにすむ"
          },
          {
            "stringId": 1960615632720506884,
            "femaleVariant": "からかってるわけじゃないんだよな？"
          },
          {
            "stringId": 1960615632770838528,
            "femaleVariant": "それが口説き文句？",
            "maleVariant": "それが口説き文句か？"
          },
          {
            "stringId": 1960615632770838544,
            "femaleVariant": "寂しいと感じることはある？",
            "maleVariant": "寂しいと感じることはあるか？"
          },
          {
            "stringId": 1960615632787615748,
            "femaleVariant": "それがあんたの口説き文句？",
            "maleVariant": "それがあんたの口説き文句か？"
          },
          {
            "stringId": 1960615632787615764,
            "femaleVariant": "それ以上ひどいのは、3回しか聞いたことないよ。いや、大差ないかも",
            "maleVariant": "それ以上ひどいのは、3回しか聞いたことないな。いや、大差ないかも"
          },
          {
            "stringId": 1960615632787615780,
            "femaleVariant": "聞かせてくれ"
          },
          {
            "stringId": 1960615632804392972,
            "femaleVariant": "そう？ えーとね…",
            "maleVariant": "そうか？ えーとだな…"
          },
          {
            "stringId": 1960615632837947396,
            "femaleVariant": "寂しいことはある？",
            "maleVariant": "寂しいことはあるか？"
          },
          {
            "stringId": 1960615632837947412,
            "femaleVariant": "もちろん。でも俺は人付き合いに向いてないみたいでな"
          },
          {
            "stringId": 1960615632837947428,
            "femaleVariant": "そういう話だったらあたしも負けてない。過去にひどい別れを経験してるから。たぶん引くレベルだと思うよ",
            "maleVariant": "そういう話だったら俺も負けてない。過去にひどい別れを経験してるからな。たぶん引くレベルだと思うぞ"
          },
          {
            "stringId": 1960615632854724608,
            "femaleVariant": "なら聞かせてくれよ"
          },
          {
            "stringId": 1960615633039273988,
            "femaleVariant": "ここだと企業が街に垂れ流してるゴミの匂いがしない"
          },
          {
            "stringId": 1960615633039274004,
            "femaleVariant": "クソをして流しての繰り返しで、まるで便所扱いだからさ",
            "maleVariant": "クソをしては流しての繰り返しで、まるで便所扱いだからな"
          },
          {
            "stringId": 1960615633039274020,
            "femaleVariant": "ほう、そいつはなかなかうまい例えをするじゃないか"
          },
          {
            "stringId": 1960615633072828420,
            "femaleVariant": "これだけ遠いと、高層ビルも悪くないって思えてくる。中で何が起きてるか見ずにすむし",
            "maleVariant": "これだけ遠いと、高層ビルも悪くないって思えてくる。中で何が起きてるか見ずにすむしな"
          },
          {
            "stringId": 1960615633072828436,
            "femaleVariant": "というと？"
          },
          {
            "stringId": 1960615633072828452,
            "femaleVariant": "胃潰瘍になるストレスやプレッシャーに、裏切りは日常茶飯事"
          },
          {
            "stringId": 1960615633089605632,
            "femaleVariant": "少なくとも、懐かしいとは思わないね",
            "maleVariant": "少なくとも、懐かしいとは思わないな"
          },
          {
            "stringId": 1960615633123160064,
            "femaleVariant": "今夜のメインイベントといこうぜ"
          },
          {
            "stringId": 1960615633408372740,
            "femaleVariant": "あの腐った街に乾杯"
          },
          {
            "stringId": 1960615633408372756,
            "femaleVariant": "ナイトシティに"
          },
          {
            "stringId": 1960615633475481604,
            "femaleVariant": "ただ、食器洗いをしたくなかっただけでしょ",
            "maleVariant": "ただ、食器洗いをしたくなかっただけだろ"
          },
          {
            "stringId": 1960615633475481620,
            "femaleVariant": "それもある。理由が2つあったわけだ"
          },
          {
            "stringId": 1960615633509036036,
            "femaleVariant": "これって、ロマンチックな散歩？",
            "maleVariant": "これって、ロマンチックな散歩か？"
          },
          {
            "stringId": 1960615633509036052,
            "femaleVariant": "花を摘んでやるつもりはないがな"
          },
          {
            "stringId": 1960615633676808196,
            "femaleVariant": "こっちに――クソ、また門が開かない。いつか直さないと"
          },
          {
            "stringId": 1960615633945243648,
            "femaleVariant": "「その飲み込み方… 美しい」"
          },
          {
            "stringId": 1960615633945243664,
            "femaleVariant": "「あと2つ願いをかなえてやる」"
          },
          {
            "stringId": 1960615633945243680,
            "femaleVariant": "「タダでヤってやる」"
          },
          {
            "stringId": 1960615633978798084,
            "femaleVariant": "「その飲み込み方… 美しい」"
          },
          {
            "stringId": 1960615633978798100,
            "femaleVariant": "マジかよ？"
          },
          {
            "stringId": 1960615633978798116,
            "femaleVariant": "そいつとダイナーに座ってて――"
          },
          {
            "stringId": 1960615633995575300,
            "femaleVariant": "5分間、無言で口を開けっぱなしであたしを見てから、そう言われたの",
            "maleVariant": "5分間、無言で口を開けっぱなしで俺を見てから、そう言われたんだ"
          },
          {
            "stringId": 1960615633995575316,
            "femaleVariant": "うわ…"
          },
          {
            "stringId": 1960615634012352516,
            "femaleVariant": "バーで急に肩を叩かれて「君は僕に会えた。あと2つ願いをかなえてやる」って言われたの",
            "maleVariant": "バーで急に肩を叩かれて「君は僕に会えた。あと2つ願いをかなえてやる」って言われた"
          },
          {
            "stringId": 1960615634029129728,
            "femaleVariant": "なんて返したんだ？"
          },
          {
            "stringId": 1960615634029129744,
            "femaleVariant": "2つ目の願い事を言ってやった。失せろってね",
            "maleVariant": "2つ目の願い事を言ってやった。失せろってな"
          },
          {
            "stringId": 1960615634029129760,
            "femaleVariant": "3つ目は？"
          },
          {
            "stringId": 1960615634029129776,
            "femaleVariant": "腕を骨折したから、早くよくなってって。あたしって優しいでしょ",
            "maleVariant": "腕を骨折したから、早くよくなれよって。俺って優しいだろ"
          },
          {
            "stringId": 1960615634096238596,
            "femaleVariant": "よし、俺のも聞いてくれ"
          },
          {
            "stringId": 1960615634113015820,
            "femaleVariant": "一度、ふくらはぎがセクシーって言われたことがある"
          },
          {
            "stringId": 1960615634113015836,
            "femaleVariant": "それだけ？",
            "maleVariant": "それだけか？"
          },
          {
            "stringId": 1960615634113015852,
            "femaleVariant": "戦争で下半身をなくす前の、彼氏の脚にそっくりなんだとさ"
          },
          {
            "stringId": 1960615634113015868,
            "femaleVariant": "あー…"
          },
          {
            "stringId": 1960615634113015884,
            "femaleVariant": "でも確かに、俺のふくらはぎは悪かない"
          },
          {
            "stringId": 1960615634146570240,
            "femaleVariant": "恋に落ちたのは一度だけ",
            "maleVariant": "恋に落ちたのは一度だけだ"
          },
          {
            "stringId": 1960615634146570256,
            "femaleVariant": "彼氏の顎を骨折させたの",
            "maleVariant": "彼氏の顎を骨折させた"
          },
          {
            "stringId": 1960615634146570272,
            "femaleVariant": "彼氏をスカイダイビングに行かせたことがある"
          },
          {
            "stringId": 1960615634180124676,
            "femaleVariant": "本気で恋に落ちたのは一度だけ。あたしはバカで… 彼は… とてもいい人だった",
            "maleVariant": "本気で恋に落ちたのは一度だけ。俺はバカで… 相手は… とてもいい人だった"
          },
          {
            "stringId": 1960615634180124692,
            "femaleVariant": "でも…？"
          },
          {
            "stringId": 1960615634180124708,
            "femaleVariant": "まあ… 変な癖があったの、ベッドの上でね。付き合った後になってわかったことだけど",
            "maleVariant": "まあ… 変な癖があったのさ、ベッドの上でな。付き合った後になってわかったことだが"
          },
          {
            "stringId": 1960615634180124724,
            "femaleVariant": "マスクを付けないと“元気”にならなかったの。それもいわゆる仮面舞踏会のじゃなくて、歴代アメリカ大統領のヤツ",
            "maleVariant": "マスクを付けないと“元気”にならなかったんだ。それもいわゆる仮面舞踏会のヤツじゃなくて、歴代アメリカ大統領のヤツ"
          },
          {
            "stringId": 1960615634196901888,
            "femaleVariant": "何だそれ…？"
          },
          {
            "stringId": 1960615634196901904,
            "femaleVariant": "毎日リンカーンと寝るのが辛くて、こっちから振っちゃった",
            "maleVariant": "毎日リンカーンと寝るのが辛くて、こっちから振っちまった"
          },
          {
            "stringId": 1960615634196901920,
            "femaleVariant": "ハハハハ、さすがに作り話だろ"
          },
          {
            "stringId": 1960615634213679108,
            "femaleVariant": "男の顎を潰して、破局になったことがある",
            "maleVariant": "相手の顎を潰して、破局になったことがある"
          },
          {
            "stringId": 1960615634230456324,
            "femaleVariant": "何かされたのか？"
          },
          {
            "stringId": 1960615634230456340,
            "femaleVariant": "“お前は重い”って言われてさ"
          },
          {
            "stringId": 1960615634230456356,
            "femaleVariant": "ってのはウソで、単にそいつが別の女と寝てる現場を見ちゃったからよ",
            "maleVariant": "ってのはウソで、単にそいつが別の奴と寝てる現場を見ちゃったんだよ"
          },
          {
            "stringId": 1960615634230456372,
            "femaleVariant": "よくある話だな"
          },
          {
            "stringId": 1960615634264010756,
            "femaleVariant": "付き合ってた男にスカイダイビングを予約したら――"
          },
          {
            "stringId": 1960615634280787976,
            "femaleVariant": "戻ってこなかった"
          },
          {
            "stringId": 1960615634280787992,
            "femaleVariant": "V、辛かっただろう。親しい人が亡くなるつらさはよくわかる"
          },
          {
            "stringId": 1960615634280788008,
            "femaleVariant": "違う違う。飛行機から落ちるついでに、インストラクターとも恋に落ちたのよ",
            "maleVariant": "違う違う。飛行機から落ちるついでに、インストラクターとも恋に落ちたんだよ"
          },
          {
            "stringId": 1960615634364674052,
            "femaleVariant": "面白い話だが、俺の元カノとのエピソードにゃかなわないな"
          },
          {
            "stringId": 1960615634364674068,
            "femaleVariant": "何があったの？",
            "maleVariant": "何があったんだ？"
          },
          {
            "stringId": 1960615634381451264,
            "femaleVariant": "撃ったんだ"
          },
          {
            "stringId": 1960615634381451280,
            "femaleVariant": "ウソでしょ…",
            "maleVariant": "ウソだろ…"
          },
          {
            "stringId": 1960615634381451296,
            "femaleVariant": "仕方がなかった。法律を守る側と破る側になっちまったのさ"
          },
          {
            "stringId": 1960615634381451312,
            "femaleVariant": "だが心配するな。デート相手を撃つのは、その1回だけにしておきたい"
          },
          {
            "stringId": 1960615636277276676,
            "femaleVariant": "この辺は悪い場所じゃないんだ。別にまぶしいほどの魅力があるってわけでもないけどな"
          },
          {
            "stringId": 1960615636277276692,
            "femaleVariant": "コーポ・プラザとは大違いね",
            "maleVariant": "コーポ・プラザとは大違いだが"
          },
          {
            "stringId": 1960615636277276708,
            "femaleVariant": "ああ、だが子育てするには悪かない。ほかに酷い場所はいくらでもある"
          },
          {
            "stringId": 1960615636294053888,
            "femaleVariant": "ランディがああいう風に育ったのには、理由があるはずだけど…",
            "maleVariant": "ランディがああいう風に育ったのには、理由があるはずだけどな…"
          },
          {
            "stringId": 1960615636294053900,
            "femaleVariant": "確かに。でもランディは親父の血を引いてるからな。どこに住もうが問題は起こすさ"
          },
          {
            "stringId": 1960615637149691908,
            "femaleVariant": "いいものがあるんだ"
          },
          {
            "stringId": 1960615637183246336,
            "femaleVariant": "[受け取る] あんたの拳銃？",
            "maleVariant": "[受け取る] リバーの拳銃？"
          },
          {
            "stringId": 1960615637183246352,
            "femaleVariant": "[受け取る] 急にどうしたの？",
            "maleVariant": "[受け取る] 急にどうした？"
          },
          {
            "stringId": 1960615637233577988,
            "femaleVariant": "あんたの拳銃？"
          },
          {
            "stringId": 1960615637233578000,
            "femaleVariant": "ずっと持ってたヤツなんだが、新しい持ち主を見つけていい頃なんじゃないかと思ってな"
          },
          {
            "stringId": 1960615637250355200,
            "femaleVariant": "すごく嬉しい",
            "maleVariant": "ありがとう"
          },
          {
            "stringId": 1960615637283909636,
            "femaleVariant": "ちょっと、急にどうしたの？",
            "maleVariant": "おい、急にどうしたんだ？"
          },
          {
            "stringId": 1960615637283909648,
            "femaleVariant": "俺の人生… ここらで一旦幕を閉じて、次のステージに上がりたいんだが、こいつがあると色々思い出しちまってな。お前に持っててほしいんだ"
          },
          {
            "stringId": 1960615637300686852,
            "femaleVariant": "ありがとう"
          },
          {
            "stringId": 1960615637334241284,
            "femaleVariant": "自分の足は絶対に撃つなよ"
          },
          {
            "stringId": 1960615637334241296,
            "femaleVariant": "やけに具体的ね",
            "maleVariant": "やけに具体的だな"
          },
          {
            "stringId": 1960615637351018496,
            "femaleVariant": "ジョスの旦那が酔ってそうしたんだ。まさにこの場所でな"
          },
          {
            "stringId": 1960615637351018508,
            "femaleVariant": "つまり、ここは家族にとって思い出の特別な場所ってわけ？",
            "maleVariant": "つまり、ここは家族にとって思い出の特別な場所ってわけか？"
          },
          {
            "stringId": 1960615637351018520,
            "femaleVariant": "そんなところだ"
          },
          {
            "stringId": 1976297740003901440,
            "femaleVariant": "間一髪だな。アルバムを見せられるところだった"
          },
          {
            "stringId": 1976304465301929984,
            "femaleVariant": "大丈夫",
            "maleVariant": "大丈夫だ"
          },
          {
            "stringId": 1976318053085282304,
            "femaleVariant": "押すだけでいい。内側からのほうが開きやすいんだ"
          },
          {
            "stringId": 1976319710355456000,
            "femaleVariant": "[開ける] わかった"
          },
          {
            "stringId": 1976324687903784960,
            "femaleVariant": "どうぞ",
            "maleVariant": "ほらよ"
          },
          {
            "stringId": 1976339659975229440,
            "femaleVariant": "なあ、V。ここに来た理由はわかるだろ？ わかると言ってくれ"
          },
          {
            "stringId": 1976350496295157760,
            "femaleVariant": "さてと。お話の時間はこれくらいで終わりにしよう"
          },
          {
            "stringId": 1976658197332033536,
            "femaleVariant": "何か強いのを貰える？",
            "maleVariant": "何か強いのを貰えるか？"
          },
          {
            "stringId": 1976658291888422912,
            "femaleVariant": "レモネードか何か持ってない？",
            "maleVariant": "レモネードか何か持ってないか？"
          },
          {
            "stringId": 1976658417532993536,
            "femaleVariant": "レモネードか何かある？",
            "maleVariant": "レモネードか何かないか？"
          },
          {
            "stringId": 1976658935042998272,
            "femaleVariant": "何か強いのをもらえると嬉しい"
          },
          {
            "stringId": 1976660893027991552,
            "femaleVariant": "俺だけ飲んでもいいのか？ きっと何本も開けちまうぜ？"
          },
          {
            "stringId": 1978237707875012608,
            "femaleVariant": "[手を挙げる] はーい！"
          },
          {
            "stringId": 1978239002371444736,
            "femaleVariant": "はーい！"
          },
          {
            "stringId": 1978240299132801024,
            "femaleVariant": "あはは、自分で手をあげてるー！"
          },
          {
            "stringId": 1978240799932698624,
            "femaleVariant": "V、付き合ってる人はいるの？ ほら、白状しなさいよ！"
          },
          {
            "stringId": 1978242553621532672,
            "femaleVariant": "あはは、Vが手を挙げた！"
          },
          {
            "stringId": 1978251060190138368,
            "femaleVariant": "飲んだり、だべったりするのさ"
          },
          {
            "stringId": 1978252021289734144,
            "femaleVariant": "何をするの？",
            "maleVariant": "何をするんだ？"
          },
          {
            "stringId": 1978253564609056768,
            "femaleVariant": "まあ、ほら、眺めがいい"
          },
          {
            "stringId": 1978254380115972096,
            "femaleVariant": "眺めね。なるほど"
          },
          {
            "stringId": 1978269728835915776,
            "femaleVariant": "違う、本当だって！",
            "maleVariant": "いや、本当なんだって！"
          },
          {
            "stringId": 1978288540725563392,
            "femaleVariant": "「タダでヤってやる」"
          },
          {
            "stringId": 1978289000337395712,
            "femaleVariant": "ハハッ！ ウソだろ！？"
          },
          {
            "stringId": 1978289339958579200,
            "femaleVariant": "中折れ帽姿のジゴロから、情報を引き出す仕事を受けてね",
            "maleVariant": "中折れ帽姿のジゴロから、情報を引き出す仕事を受けてな"
          },
          {
            "stringId": 1978290941897494528,
            "femaleVariant": "普通に話をしてたと思ったら、急にそんなことを言い出すもんだから、笑いそうになっちゃった",
            "maleVariant": "普通に話をしてたと思ったら、急にそんなことを言い出すもんだから、笑いそうになったよ"
          },
          {
            "stringId": 1978291476335710208,
            "femaleVariant": "寛大な申し出だぞ、V。聞いてもよかったんじゃないか"
          },
          {
            "stringId": 1978313508343533568,
            "femaleVariant": "よし。でもまだ酒は飲める"
          },
          {
            "stringId": 1978316606776561664,
            "femaleVariant": "そうこなくっちゃな"
          },
          {
            "stringId": 1978317241055989760,
            "femaleVariant": "レモ…？ まあ、あるけど、でも本気か？"
          },
          {
            "stringId": 1978317628106362880,
            "femaleVariant": "これは食事会のはずでしょ？ 強いのを飲むつもりはないの",
            "maleVariant": "これは食事会のはずだろ？ 強いのを飲むつもりはないさ"
          },
          {
            "stringId": 1978320355158937600,
            "femaleVariant": "何言ってるのよ。自分のお気に入りの場所なんだから好きにして",
            "maleVariant": "何言ってんだ。自分のお気に入りの場所なんだから好きにしろって"
          },
          {
            "stringId": 1978320405004046336,
            "femaleVariant": "最後まで付き合えよ？ 俺は酔うとけっこうチャーミングになるんだぜ"
          },
          {
            "stringId": 1978321463159836672,
            "femaleVariant": "はっ。どうかな"
          },
          {
            "stringId": 1978324522820718592,
            "femaleVariant": "もう遅い"
          },
          {
            "stringId": 1978343710301900800,
            "femaleVariant": "うーん…"
          },
          {
            "stringId": 1979239884910637056,
            "femaleVariant": "うお！ すげえな。さすがだよ"
          },
          {
            "stringId": 1979244526461661184,
            "femaleVariant": "さあ行こう…"
          },
          {
            "stringId": 2010064559694172160,
            "femaleVariant": "[キスする] あたしたちに",
            "maleVariant": "[キスする] 俺たちに"
          },
          {
            "stringId": 2010065029154230272,
            "femaleVariant": "あたしたちに",
            "maleVariant": "俺たちに"
          },
          {
            "stringId": 2010066574872694784,
            "femaleVariant": "おいおい… Ｖ、急に何してるんだよ？"
          },
          {
            "stringId": 2010067624254001152,
            "femaleVariant": "ごめん… あたし、てっきり…",
            "maleVariant": "すまん… 俺は、てっきり…"
          },
          {
            "stringId": 2010072045033639936,
            "femaleVariant": "あー… 俺はだな、お前を…"
          },
          {
            "stringId": 2010073147212845056,
            "femaleVariant": "いいの。説明しなくてもわかる。大丈夫だから",
            "maleVariant": "いいんだ。説明しなくてもわかる。大丈夫だって"
          },
          {
            "stringId": 2018520169670529024,
            "femaleVariant": "ゲームは楽しかった？"
          },
          {
            "stringId": 2018543018342895616,
            "femaleVariant": "うん！ 2人で大人に勝ったんだ！"
          },
          {
            "stringId": 2018545188106686464,
            "femaleVariant": "別にいいの。Vが強すぎるだけ"
          },
          {
            "stringId": 2018558115723366400,
            "femaleVariant": "え？ ああ、悪者もやっつけたよ！"
          },
          {
            "stringId": 2018558297722605568,
            "femaleVariant": "悪者のこと？"
          },
          {
            "stringId": 2018562840002842624,
            "femaleVariant": "ならもっと一緒に遊んでもらったら？"
          },
          {
            "stringId": 2018564675849703424,
            "femaleVariant": "うん！ でも次は私と組んでもらう！"
          },
          {
            "stringId": 2059396238429061120,
            "femaleVariant": "[キスする]"
          },
          {
            "stringId": 2060428751989571588,
            "femaleVariant": "[食べる]"
          },
          {
            "stringId": 2191088344450232324,
            "femaleVariant": "[立ち上がる]"
          }
        ]
      }
    }
