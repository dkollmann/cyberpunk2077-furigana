    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1764913447628161024,
            "femaleVariant": "やってみる"
          },
          {
            "stringId": 1764916827582873600,
            "femaleVariant": "まだ街を出たいの？",
            "maleVariant": "まだ街を出たいのか？"
          },
          {
            "stringId": 1764918152194408448,
            "femaleVariant": "そうね。楽しみにしておく",
            "maleVariant": "そうだな。楽しみにしておく"
          },
          {
            "stringId": 1764918455073488896,
            "femaleVariant": "今でもナイトシティを出たいの？",
            "maleVariant": "今でもナイトシティを出たいのか？"
          },
          {
            "stringId": 1764948946422054916,
            "femaleVariant": "ナイトシティの外の暮らしはどう？",
            "maleVariant": "ナイトシティの外の暮らしはどうだ？"
          },
          {
            "stringId": 1764949371909029888,
            "femaleVariant": "最近は何してるの？",
            "maleVariant": "最近は何してるんだ？"
          },
          {
            "stringId": 1764949870863433728,
            "femaleVariant": "で、文明から離れた感想は？"
          },
          {
            "stringId": 1764950158609465344,
            "femaleVariant": "最近は何してるの？",
            "maleVariant": "最近は何してるんだ？"
          },
          {
            "stringId": 1918257954544402436,
            "femaleVariant": "いいBD作品は作れてる？",
            "maleVariant": "いいBD作品は作れてるか？"
          },
          {
            "stringId": 1918258839928422404,
            "femaleVariant": "ジュディに会いたい"
          },
          {
            "stringId": 1918260361521250304,
            "femaleVariant": "ねえ。ダムでの話はまだ生きてる？",
            "maleVariant": "なあ。ダムでの話はまだ生きてるか？"
          },
          {
            "stringId": 1918261154949349376,
            "femaleVariant": "もちろん。今夜、湖のほとりのキャビンに来れる？"
          },
          {
            "stringId": 1918261439893585920,
            "femaleVariant": "全力を尽くす。じゃあね",
            "maleVariant": "全力を尽くす。じゃあな"
          },
          {
            "stringId": 1918262350141440000,
            "femaleVariant": "やっぱりダイビングに行きたい"
          },
          {
            "stringId": 1918264497843191808,
            "femaleVariant": "気が変わって、やっぱダイビングしたくなったの",
            "maleVariant": "気が変わって、やっぱダイビングしたくなったんだ"
          },
          {
            "stringId": 1918265709644726272,
            "femaleVariant": "最高。今夜、湖畔のキャビンに来て。待たせないでね"
          },
          {
            "stringId": 1918271157844295680,
            "femaleVariant": "モックスの方は最近どう？",
            "maleVariant": "モックスの方は最近どうだ？"
          },
          {
            "stringId": 1918271446865395712,
            "femaleVariant": "チューニングの方はどう？ いいBDは撮れた？",
            "maleVariant": "チューニングの方はどうだ？ いいBDは撮れたか？"
          },
          {
            "stringId": 1918271768602066948,
            "femaleVariant": "モックスの方は最近どう？",
            "maleVariant": "モックスの方は最近どうだ？"
          },
          {
            "stringId": 1944402023397965828,
            "femaleVariant": "気が変わった。クラウドの件、協力する"
          },
          {
            "stringId": 1944402759666089984,
            "femaleVariant": "ヒロミの件に協力する"
          },
          {
            "stringId": 1944405656554422272,
            "femaleVariant": "あのさ… 考えたんだけど――やっぱりクラウドの件に協力するよ",
            "maleVariant": "ジュディ… 考えたんだが――やっぱりクラウドの件に協力するよ"
          },
          {
            "stringId": 1944406059627036676,
            "femaleVariant": "前言撤回。ヒロミの件、やっぱり手を貸すよ",
            "maleVariant": "前言撤回だ。ヒロミの件、やっぱり手を貸すよ"
          },
          {
            "stringId": 1944414908434071556,
            "femaleVariant": "[切る] またね、ジュディ",
            "maleVariant": "[切る] またな、ジュディ"
          },
          {
            "stringId": 1944416917170143236,
            "femaleVariant": "情報が得られるなら何でも"
          },
          {
            "stringId": 1944417205822144512,
            "femaleVariant": "どうしてポルノの仕事を？"
          },
          {
            "stringId": 1944417354669604864,
            "femaleVariant": "話題を変えましょ",
            "maleVariant": "話題を変えよう"
          },
          {
            "stringId": 1944417761852637184,
            "femaleVariant": "他にも話したいことがあるの",
            "maleVariant": "他にも話したいことがあるんだ"
          },
          {
            "stringId": 1944418147795714048,
            "femaleVariant": "仕事柄、広く浅くがモットーなの。情報が収集できるなら、バーチャの中身は問わない",
            "maleVariant": "仕事柄、広く浅くがモットーでな。情報が収集できるなら、バーチャの中身は問わない"
          },
          {
            "stringId": 1944418241714569220,
            "femaleVariant": "そういえば、どうしてポルノの業界に？"
          },
          {
            "stringId": 1944418642287378432,
            "femaleVariant": "あんたがリーダーになればいい"
          },
          {
            "stringId": 1944418642287378448,
            "femaleVariant": "そもそもどうして仲間になったの？",
            "maleVariant": "そもそもどうして仲間になったんだ？"
          },
          {
            "stringId": 1944418642287378464,
            "femaleVariant": "他にも聞きたいことがある"
          },
          {
            "stringId": 1944418642404818948,
            "femaleVariant": "他にも聞きたいことがある"
          },
          {
            "stringId": 1944418642455150596,
            "femaleVariant": "代わりにモックスのリーダーになるってのは？"
          },
          {
            "stringId": 1944418642505482244,
            "femaleVariant": "そもそもなぜモックスと関わるように？"
          },
          {
            "stringId": 1944427770133069824,
            "femaleVariant": "[寄りかかる] もっと二人だけで一緒にいたい"
          },
          {
            "stringId": 1944429089845657604,
            "femaleVariant": "あたしが飛び回ってばかりじゃなければ、もっと一緒にいれるんだけどね…",
            "maleVariant": "俺が飛び回ってばかりじゃなければ、もっと一緒にいれるんだけどな…"
          },
          {
            "stringId": 1944429611818401792,
            "femaleVariant": "[離れる] もう少し話しましょ",
            "maleVariant": "[離れる] もう少し話そう"
          },
          {
            "stringId": 1944429847706058756,
            "femaleVariant": "もう少し話しましょ。いつ急な仕事が入るかわからないし",
            "maleVariant": "もう少し話さないか。いつ急な仕事が入るかわからないしな"
          },
          {
            "stringId": 1944430879202852864,
            "femaleVariant": "ナイトシティが恋しくならない？",
            "maleVariant": "ナイトシティが恋しくならないか？"
          },
          {
            "stringId": 1944431942744129536,
            "femaleVariant": "話題を変えましょ",
            "maleVariant": "話題を変えよう"
          },
          {
            "stringId": 1944432265369993220,
            "femaleVariant": "ナイトシティが恋しくならない？",
            "maleVariant": "ナイトシティが恋しくならないか？"
          },
          {
            "stringId": 1944432265437102080,
            "femaleVariant": "話題を変えましょ",
            "maleVariant": "話題を変えよう"
          },
          {
            "stringId": 1944438953103835136,
            "femaleVariant": "たった今、Vのことを考えてた。もちろん、服を着てない裸の姿をね"
          },
          {
            "stringId": 1944439096968462336,
            "femaleVariant": "もしもし"
          },
          {
            "stringId": 1944439140689887232,
            "femaleVariant": "どうしたの？"
          },
          {
            "stringId": 1944449961339895808,
            "femaleVariant": "あ、そうそう。エリザベスなんとかって人から電話があったの。バーチャ関連の仕事を頼みたいんだって"
          },
          {
            "stringId": 1944450315137826820,
            "femaleVariant": "それなら今まさに進行中"
          },
          {
            "stringId": 1944450638116012032,
            "femaleVariant": "厄介な仕事だった"
          },
          {
            "stringId": 1944450705946296320,
            "femaleVariant": "その仕事なら、いまやってるとこ",
            "maleVariant": "その仕事なら、いまやってるとこだ"
          },
          {
            "stringId": 1944451124403617796,
            "femaleVariant": "はあ… あれはひどい仕事だった"
          },
          {
            "stringId": 1944452106843832324,
            "femaleVariant": "ジャパンタウンのパレードでひと騒ぎあったらしいね"
          },
          {
            "stringId": 1944452106877386752,
            "femaleVariant": "あれはあたし",
            "maleVariant": "あれは俺だ"
          },
          {
            "stringId": 1944452106894163980,
            "femaleVariant": "何のことだかさっぱり"
          },
          {
            "stringId": 1944452106944495620,
            "femaleVariant": "あれは… 実はあたしのせいなの",
            "maleVariant": "あれは… 実は俺のせいなんだ"
          },
          {
            "stringId": 1944452107028381700,
            "femaleVariant": "何のことだかさっぱり",
            "maleVariant": "何のことだかさっぱりだ"
          },
          {
            "stringId": 1944453102236057600,
            "femaleVariant": "こっちは特に変わりないんだけど",
            "maleVariant": "こっちは特に変わりないんだが"
          },
          {
            "stringId": 1944453174713630724,
            "femaleVariant": "他にはなんかあった？"
          },
          {
            "stringId": 1944464546964946944,
            "femaleVariant": "そもそもなんでブレインダンスの道に？"
          },
          {
            "stringId": 1944465548547964932,
            "femaleVariant": "バーチャの道に進んだきっかけは？"
          },
          {
            "stringId": 1944491996000272384,
            "femaleVariant": "よかった。もう誰を頼っていいのか、わからなくなってたのよ"
          },
          {
            "stringId": 1944511969443692544,
            "femaleVariant": "面白い案件だった"
          },
          {
            "stringId": 1944512049722671104,
            "femaleVariant": "仕事を回してくれて助かる",
            "maleVariant": "仕事を回してくれて助かるよ"
          },
          {
            "stringId": 1944512179259555844,
            "femaleVariant": "仕事を回してくれてありがとう",
            "maleVariant": "仕事を回してくれてありがとな"
          },
          {
            "stringId": 1944512792785567748,
            "femaleVariant": "なかなか面白い仕事だった",
            "maleVariant": "なかなか… 面白い仕事だった"
          },
          {
            "stringId": 1944550020639342592,
            "femaleVariant": "どういう風に聞いてる？"
          },
          {
            "stringId": 1944550532327653376,
            "femaleVariant": "なんて聞いてる？"
          },
          {
            "stringId": 1944552284204548096,
            "femaleVariant": "コーポが発砲して、死者が出たって"
          },
          {
            "stringId": 1944636420013408256,
            "femaleVariant": "わかった、マイコに電話して約束を取り付ける"
          },
          {
            "stringId": 1946138052819378176,
            "femaleVariant": "あなたを紹介しといた"
          },
          {
            "stringId": 1946141668443643904,
            "femaleVariant": "いいの、水臭い。儲かる仕事だといいわね"
          },
          {
            "stringId": 1947506736188055552,
            "femaleVariant": "さすが、V。うまくいってる？"
          },
          {
            "stringId": 1947507403954806784,
            "femaleVariant": "今のところはなんとも"
          },
          {
            "stringId": 1947507635480387584,
            "femaleVariant": "なるほど、いわゆるクライアントの“守秘義務”ってヤツ？"
          },
          {
            "stringId": 1947511693989601280,
            "femaleVariant": "解決できたの？"
          },
          {
            "stringId": 1947511903436365824,
            "femaleVariant": "一応、ね",
            "maleVariant": "一応、な"
          },
          {
            "stringId": 1947513089602314240,
            "femaleVariant": "たっぷり儲けたみたいね"
          },
          {
            "stringId": 1947520639852830720,
            "femaleVariant": "やっぱり！ 聞いた瞬間から、あなたが関わってると思ったの！"
          },
          {
            "stringId": 1947523040739549184,
            "femaleVariant": "いつも通りよ。あたしのことより、そっちはどうなの？",
            "maleVariant": "いつも通りさ。俺のことより、そっちはどうなんだ？"
          },
          {
            "stringId": 1947528116820467712,
            "femaleVariant": "そっちは何かあった？",
            "maleVariant": "そっちは何かあったか？"
          },
          {
            "stringId": 1947575929235267584,
            "femaleVariant": "相変わらず、スージーにイライラさせられてる"
          },
          {
            "stringId": 1947682064638369792,
            "femaleVariant": "「いい」の定義による。ポルノは含まれる？"
          },
          {
            "stringId": 1947685685027364864,
            "femaleVariant": "時間ならたっぷりあるよ"
          },
          {
            "stringId": 1947698200461402112,
            "femaleVariant": "リアルな殺人モノばっか見てるのね"
          },
          {
            "stringId": 1947701449755156480,
            "femaleVariant": "仕事柄ね。でもジュディにとってはアートなんでしょ",
            "maleVariant": "仕事柄、な。でもジュディにとってはアートなんだろ"
          },
          {
            "stringId": 1947714112258269184,
            "femaleVariant": "わからない？ たった2分の実体験が1週間の稼ぎを生む"
          },
          {
            "stringId": 1947714488621555712,
            "femaleVariant": "儲かるのはわかるけど、“性の商品化”みたいなのは好みじゃないのかなって",
            "maleVariant": "儲かるのはわかるが、“性の商品化”みたいなのは好みじゃないのかなってな"
          },
          {
            "stringId": 1947716692090773504,
            "femaleVariant": "私のバーチャは性を商品化してるわけじゃない。私のアクターはただ自分の好きなことをしてる。みんなそれを感じたいだけ"
          },
          {
            "stringId": 1947721441586405376,
            "femaleVariant": "小さい頃からテッキーオタクだったのよ。初めてバーチャを体験したとき思ったの。「私ならもっとよくできる」って"
          },
          {
            "stringId": 1947722477915037696,
            "femaleVariant": "やってみたら案の定上手くいった。そこからはとんとん拍子"
          },
          {
            "stringId": 1947734151837474816,
            "femaleVariant": "どうぞ"
          },
          {
            "stringId": 1947738791727554560,
            "femaleVariant": "スージーたちなら街を変えられるんじゃないかと思ったの。そう信じてたのよ"
          },
          {
            "stringId": 1947739250064318464,
            "femaleVariant": "結局は金のためだった？",
            "maleVariant": "結局は金のためだったか？"
          },
          {
            "stringId": 1947739437700702208,
            "femaleVariant": "んー、そこまでは言わないかな。モックスのおかげで救われたジョイトイも少なからずいるし"
          },
          {
            "stringId": 1947751691309506560,
            "femaleVariant": "量より質よ、V"
          },
          {
            "stringId": 1947754763888844800,
            "femaleVariant": "今はやり残したことを片付けて。そしたら一緒にナイトシティを出ればいいじゃない"
          },
          {
            "stringId": 1947760252521504768,
            "femaleVariant": "うん。私も言われるもの――毎日楽しそうねって"
          },
          {
            "stringId": 1947762648693825536,
            "femaleVariant": "うん… いつかね"
          },
          {
            "stringId": 1947763052487860224,
            "femaleVariant": "でも今はまだ… この街に残る理由があるから"
          },
          {
            "stringId": 1947766499517882368,
            "femaleVariant": "何言ってんの。あの街の騒音、におい。何もかもまだ覚えてるわよ"
          },
          {
            "stringId": 1947771059447750656,
            "femaleVariant": "いいよ。運転中だから時間はたっぷりある"
          },
          {
            "stringId": 1947772631254786048,
            "femaleVariant": "一日中だって話せる"
          },
          {
            "stringId": 1947783607026933760,
            "femaleVariant": "ナイトシティのこと？ オレゴンはいいよ。人生ってこういうふうに生きるべきね"
          },
          {
            "stringId": 1947785877336580096,
            "femaleVariant": "今は休んでるけど、もうすぐ仕事に復帰する。新しいバーチャサイクルのアイデアがあって"
          },
          {
            "stringId": 1948736435471998976,
            "femaleVariant": "早朝、クラウドの上にあるバルコニーで会いましょう。今回はちゃんと来てね"
          },
          {
            "stringId": 1948742177809719296,
            "femaleVariant": "V、今度は… がっかりさせないでね"
          },
          {
            "stringId": 1948755024610549760,
            "femaleVariant": "ハハハ。面白い提案だけど、お断りよ、バーチャの調整だけで十分"
          },
          {
            "stringId": 1948755514203267072,
            "femaleVariant": "人付き合いは苦手なの。気づいてなかった？"
          },
          {
            "stringId": 1948786706654912512,
            "femaleVariant": "ハナコと話したかっただけなのに、思わぬ事態になっちゃって",
            "maleVariant": "ハナコと話したかっただけなのに、思わぬ事態になっちまってな"
          },
          {
            "stringId": 1948787529879347200,
            "femaleVariant": "企業が相手なら、そりゃああなるって"
          },
          {
            "stringId": 1948788320102998016,
            "femaleVariant": "あーら、不思議… 私には真逆のことを言ってるように聞こえるけど"
          },
          {
            "stringId": 1948810409237241856,
            "femaleVariant": "話せてよかった"
          },
          {
            "stringId": 1948811144599396352,
            "femaleVariant": "私もよ。いつでも電話してね、V"
          },
          {
            "stringId": 1948827181017423876,
            "femaleVariant": "違法ブレインダンスのこと、何か知ってる？",
            "maleVariant": "違法ブレインダンスのこと、何か知ってるか？"
          },
          {
            "stringId": 1948829125463203840,
            "femaleVariant": "この間、裏BDに接する機会があったんだけど、そっち方面は詳しい？",
            "maleVariant": "この間、裏BDに接する機会があったんだが、そっち方面は詳しいか？"
          },
          {
            "stringId": 1948830856737677312,
            "femaleVariant": "裏BDについては、知らなければ知らないほうがいい"
          },
          {
            "stringId": 1948834178274123776,
            "femaleVariant": "制作者についてはなおのこと"
          },
          {
            "stringId": 2314253418925121536,
            "femaleVariant": "もう行かないと"
          }
        ]
      }
    }
  }
}