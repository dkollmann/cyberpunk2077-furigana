    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1788805295008079872,
            "femaleVariant": "うっ…"
          },
          {
            "stringId": 1788810308879081472,
            "femaleVariant": "おいおい"
          },
          {
            "stringId": 1788815630494556164,
            "femaleVariant": "で、少しはマシになったか？"
          },
          {
            "stringId": 1788815743120007172,
            "femaleVariant": "ここはいったいどこ？",
            "maleVariant": "ここはいったいどこだ？"
          },
          {
            "stringId": 1788825514556481536,
            "femaleVariant": "全然"
          },
          {
            "stringId": 1788826667067334656,
            "femaleVariant": "カーペットにしこたま吐いてもおとがめなしの場所だ"
          },
          {
            "stringId": 1788826963285860352,
            "femaleVariant": "俺たちゃ贅沢言えねえだろ"
          },
          {
            "stringId": 1795455182183927808,
            "femaleVariant": "いいだろう。あんた、最初に会った時から、どうも懐かしい感じがしたんだ"
          },
          {
            "stringId": 1795455763229249536,
            "femaleVariant": "ジョニーに聞いたのね",
            "maleVariant": "ジョニーに聞いたんだな"
          },
          {
            "stringId": 1795457417378861056,
            "femaleVariant": "あたしたち… まさか？",
            "maleVariant": "俺たち… まさか…"
          },
          {
            "stringId": 1795458088803045376,
            "femaleVariant": "驚いてなさそうね",
            "maleVariant": "驚いてなさそうだな"
          },
          {
            "stringId": 1795458484879560704,
            "femaleVariant": "最悪の気分",
            "maleVariant": "最悪の気分だ"
          },
          {
            "stringId": 1795459506528129024,
            "femaleVariant": "驚いてなさそうね",
            "maleVariant": "驚いてなさそうだな"
          },
          {
            "stringId": 1795461387472146432,
            "femaleVariant": "最悪の気分",
            "maleVariant": "最悪の気分だ"
          },
          {
            "stringId": 1795464400458813440,
            "femaleVariant": "最初は驚いたが、もう慣れた。あんた覚えてないようだね"
          },
          {
            "stringId": 1795465457977069568,
            "femaleVariant": "だろうね。あんたは何も覚えてないんだろ？"
          },
          {
            "stringId": 1795466334385278976,
            "femaleVariant": "記憶にあるのは巨大なブラックホール"
          },
          {
            "stringId": 1795467281576562688,
            "femaleVariant": "断片的にだけね… 全部覚えてなくてよかった",
            "maleVariant": "断片的にだけな… 全部覚えてなくてよかった"
          },
          {
            "stringId": 1795467840459182080,
            "femaleVariant": "あたしたち… その…",
            "maleVariant": "俺たち… その…"
          },
          {
            "stringId": 1795470306458275840,
            "femaleVariant": "いや。あの野郎ベロベロでね"
          },
          {
            "stringId": 1795486087946506244,
            "femaleVariant": "スマッシャーの話は聞いた？",
            "maleVariant": "スマッシャーの話は聞いたか？"
          },
          {
            "stringId": 1795493846855258112,
            "femaleVariant": "変な感じね",
            "maleVariant": "変な感じだな"
          },
          {
            "stringId": 1795495847857033216,
            "femaleVariant": "ジョニーから… スマッシャーのことは聞いた？",
            "maleVariant": "ジョニーから… スマッシャーのことは聞いたか？"
          },
          {
            "stringId": 1795496077067358208,
            "femaleVariant": "なんか… 変な感じね。ジョニーの存在を認識してる人がいるなんて",
            "maleVariant": "なんか… 変な感じだよ。ジョニーの存在を認識してる人がいるってのは"
          },
          {
            "stringId": 1795505963092217856,
            "femaleVariant": "ああ。ナイトシティに戻ってたとはね"
          },
          {
            "stringId": 1795506592841797632,
            "femaleVariant": "ジョニーが糸口を見つけた"
          },
          {
            "stringId": 1795507013916364800,
            "femaleVariant": "たしか… グレイソン？"
          },
          {
            "stringId": 1795507311040860160,
            "femaleVariant": "ジェレミア・グレイソン、スマッシャーの手下だ。なんでも、ストリッパーから聞いたんだとさ"
          },
          {
            "stringId": 1795509543467552772,
            "femaleVariant": "手を貸そうか？"
          },
          {
            "stringId": 1795510389609664512,
            "femaleVariant": "スマッシャーが今までどうしてたのか知ってる？",
            "maleVariant": "スマッシャーが今までどうしてたのか知ってるか？"
          },
          {
            "stringId": 1795514649160257536,
            "femaleVariant": "スマッシャーが今までどうしてたのか知ってる？",
            "maleVariant": "スマッシャーが今までどうしてたのか知ってるか？"
          },
          {
            "stringId": 1795514958917996544,
            "femaleVariant": "あたしに手伝える？",
            "maleVariant": "俺にも手伝えるか？"
          },
          {
            "stringId": 1795515424385077248,
            "femaleVariant": "その状態で？ 何の役にも立たないよ"
          },
          {
            "stringId": 1795515622322671616,
            "femaleVariant": "何かわかったら連絡する"
          },
          {
            "stringId": 1795515949411274752,
            "femaleVariant": "わかった"
          },
          {
            "stringId": 1795519915259924480,
            "femaleVariant": "あいつがこの頭のどこかにいるなんてね"
          },
          {
            "stringId": 1795521939246485504,
            "femaleVariant": "あんた、顔が変わってきてる"
          },
          {
            "stringId": 1795522367937908736,
            "femaleVariant": "まだVのままだけど、あの意地悪い笑い方、私にはわかる"
          },
          {
            "stringId": 1795523748484681728,
            "femaleVariant": "話し方、動き方… タバコの吸い方も"
          },
          {
            "stringId": 1795524227390312448,
            "femaleVariant": "あたしは吸わない",
            "maleVariant": "俺は吸わない"
          },
          {
            "stringId": 1795528585943257088,
            "femaleVariant": "知ってる。2人を見間違うことはないよ"
          },
          {
            "stringId": 1795529035035774976,
            "femaleVariant": "だから安心しな"
          },
          {
            "stringId": 1795530615701180416,
            "femaleVariant": "手掛かりは“エブニケ”って単語だけ。とにかく、調べてみるとしよう"
          },
          {
            "stringId": 1795531101602910208,
            "femaleVariant": "あの野郎だけは逃がさない"
          },
          {
            "stringId": 1795536996260974592,
            "femaleVariant": "2023年のアラサカ襲撃後に姿を消し、死んだと思われてた"
          },
          {
            "stringId": 1795537538785808384,
            "femaleVariant": "そしたら突然また現れ、いくつかアラサカの仕事をしたかと思うと、再び行方不明"
          },
          {
            "stringId": 1795537945012539392,
            "femaleVariant": "その後見た者はいない"
          },
          {
            "stringId": 1795539829597212672,
            "femaleVariant": "少なくとも、グレイソン以外はね"
          },
          {
            "stringId": 1796459783775391748,
            "femaleVariant": "もう二度とやらない"
          },
          {
            "stringId": 1796460117222559744,
            "femaleVariant": "…もう二度とやらない"
          },
          {
            "stringId": 1796461589658144768,
            "femaleVariant": "だまされた。ローグと話すだけって言ったのに",
            "maleVariant": "だましたな。ローグと話すだけって言っただろ"
          },
          {
            "stringId": 1796462272591499264,
            "femaleVariant": "吐くのは馬鹿みたいに飲んだそっちのはずなのに"
          },
          {
            "stringId": 1796467281647439872,
            "femaleVariant": "なんであたしが吐かなきゃいけないのよ",
            "maleVariant": "なんで俺が吐かなきゃならないんだ"
          },
          {
            "stringId": 1796467601823830016,
            "femaleVariant": "だまされた。ローグと話すだけのはずだったでしょ",
            "maleVariant": "だましたな。ローグと話すだけのはずだったろ"
          },
          {
            "stringId": 1796484651803807744,
            "femaleVariant": "薬の副作用がここまでとは思わなかった"
          },
          {
            "stringId": 1796487731849007104,
            "femaleVariant": "まあ、人生は不公平だ"
          },
          {
            "stringId": 1796488273383985152,
            "femaleVariant": "ほんと、いいタイミングで体から出たよね",
            "maleVariant": "ほんと、いいタイミングで体から出たよな"
          },
          {
            "stringId": 1796501798068228096,
            "femaleVariant": "ついでに他のものもいろいろ飲んでなきゃ、ここまでひどくなかったでしょうね",
            "maleVariant": "ついでに他のものもいろいろ飲んでなきゃ、ここまでひどくなかっただろうな"
          },
          {
            "stringId": 1796515255073181696,
            "femaleVariant": "話はした"
          },
          {
            "stringId": 1796534891764322304,
            "femaleVariant": "ストリッパーと、エンパシーでの喧嘩と、タトゥーは？"
          },
          {
            "stringId": 1796536229881516032,
            "femaleVariant": "仕方ないだろ。ローグのとこに手ぶらじゃ行けなかったんだ"
          },
          {
            "stringId": 1796551220676333568,
            "femaleVariant": "出てやっただけでも感謝しろ"
          },
          {
            "stringId": 1796557204740182020,
            "femaleVariant": "実はちょっと、心配したのよね…",
            "maleVariant": "実は少し、心配したんだ…"
          },
          {
            "stringId": 1796558047979835392,
            "femaleVariant": "もちろん。感謝感激よ",
            "maleVariant": "もちろん。感謝感激さ"
          },
          {
            "stringId": 1796564645989126148,
            "femaleVariant": "“エブニケ”のことだけど…",
            "maleVariant": "“エブニケ”のことだが…"
          },
          {
            "stringId": 1796565479061786624,
            "femaleVariant": "あたしが直接ローグと話せたのに",
            "maleVariant": "俺が直接ローグと話せたのに"
          },
          {
            "stringId": 1796581167855898624,
            "femaleVariant": "実は考えてた。そのままあたしの体を乗っ取る気なんじゃ、って",
            "maleVariant": "実は考えてた。そのまま俺の体を乗っ取る気なんじゃ、って"
          },
          {
            "stringId": 1796581885618753536,
            "femaleVariant": "もちろん、すっごく感謝してる。見てこの笑顔",
            "maleVariant": "もちろん感謝してるって。ほら、見ろよ、この笑顔"
          },
          {
            "stringId": 1796582412658216960,
            "femaleVariant": "ああ、まぶしいくらいだ"
          },
          {
            "stringId": 1796582808432742400,
            "femaleVariant": "じゃ、行くか"
          },
          {
            "stringId": 1796594351409008640,
            "femaleVariant": "随分と… 楽しんでたみたいだし",
            "maleVariant": "随分と… 楽しんでたみたいだしな"
          },
          {
            "stringId": 1796597968039907328,
            "femaleVariant": "結局信用してないんだな"
          },
          {
            "stringId": 1796599575867625472,
            "femaleVariant": "例の“エブニケ”… グレイソンへの手掛かりが、スマッシャーにもつながるって話――"
          },
          {
            "stringId": 1796600713882963968,
            "femaleVariant": "ほんとにそんなので辿りつけるの？",
            "maleVariant": "ほんとにそんなので辿りつけるのか？"
          },
          {
            "stringId": 1796602896296775680,
            "femaleVariant": "ローグに任しときゃいい"
          },
          {
            "stringId": 1796603428637839360,
            "femaleVariant": "ウダウダ言ってねえで、もう行こうぜ"
          },
          {
            "stringId": 1796605238245134336,
            "femaleVariant": "あたしが話せばよかった",
            "maleVariant": "俺が話してもよかったのに"
          },
          {
            "stringId": 1796606738161799168,
            "femaleVariant": "実際そう思ってたでしょ。見たのよ",
            "maleVariant": "実際そう思ってただろ。見たんだ"
          },
          {
            "stringId": 1796607241344061440,
            "femaleVariant": "でも約束どおり戻ったろ。もう行くぞ"
          },
          {
            "stringId": 1796608567029338112,
            "femaleVariant": "へえ、お前がローグを説得できたか？"
          },
          {
            "stringId": 1796609163341926400,
            "femaleVariant": "結局俺の助けが必要になったに決まってる"
          },
          {
            "stringId": 1796609553999400960,
            "femaleVariant": "ああ… また吐きそう",
            "maleVariant": "ああ… また吐きそうだ"
          },
          {
            "stringId": 1796611110606278656,
            "femaleVariant": "ここを出よう。外の空気でも吸え"
          },
          {
            "stringId": 1892529470903701504,
            "femaleVariant": "今朝早く連絡がきた。もちろんあんただと思って出たんだ――"
          },
          {
            "stringId": 1892529471558012928,
            "femaleVariant": "話の内容を聞いて、「ついにイカれちまったか」って思ったよ"
          },
          {
            "stringId": 1892529472178769920,
            "femaleVariant": "それで、その“イカれた奴”がどんな顔してるか見に来たってわけ？",
            "maleVariant": "それで、その“イカれた奴”がどんな顔してるか見に来たってわけか？"
          },
          {
            "stringId": 1892529472833081344,
            "femaleVariant": "ああ。そしたらジョニー・シルヴァーハンドがいた"
          },
          {
            "stringId": 1914074064732573696,
            "femaleVariant": "V？"
          },
          {
            "stringId": 1914074513304997888,
            "femaleVariant": "ん… うう"
          },
          {
            "stringId": 1914075541932888064,
            "femaleVariant": "毎朝そうやって血を吐くのかい？"
          },
          {
            "stringId": 1914075834242322432,
            "femaleVariant": "お願い、他の話をしてくれる？",
            "maleVariant": "頼む、他の話をしてくれるか？"
          },
          {
            "stringId": 1963314385100992516,
            "femaleVariant": "[立ち上がる]"
          },
          {
            "stringId": 2128480590018375684,
            "femaleVariant": "[立ち上がる]"
          }
        ]
      }
    }
  }
}