      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "モックスはクラウドの乗っ取りに協力するって？",
              "maleVariant": "",
              "stringId": "1734259821088063488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リジーズ・バーの制圧だって一筋縄じゃいかなかった",
              "maleVariant": "",
              "stringId": "1734259821104840712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その話なら知ってるけど、そう簡単にいったわけじゃないでしょ",
              "maleVariant": "その話なら知ってるが、そう簡単にいったわけじゃないだろ",
              "stringId": "1734259821138395136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろん、簡単にいくなんて思ってない",
              "maleVariant": "",
              "stringId": "1734259821138395144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもエヴリンは、安全だと思ってクラウドを頼ったのよ。それがあんな結末を生んだ",
              "maleVariant": "",
              "stringId": "1734259821155172352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "モックスはその件に協力してくれるの？",
              "maleVariant": "モックスはその件に協力してくれるのか？",
              "stringId": "1734259821188726784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいえ。あなたとやるの。クラウド内部にも協力者がいる",
              "maleVariant": "",
              "stringId": "1734259821188726792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジュディ。気分はどう？",
              "maleVariant": "ジュディ。気分はどうだ？",
              "stringId": "1734260098314780672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何を思いついたの？",
              "maleVariant": "何を思いついたんだ？",
              "stringId": "1734260434412748804"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悲しみが癒えるには時間がかかる",
              "maleVariant": "",
              "stringId": "1734262998793437184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "傷が癒えるには時間がかかる",
              "maleVariant": "",
              "stringId": "1734263369888677888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何を思いついたの？",
              "maleVariant": "何を思いついたんだ？",
              "stringId": "1734263600843833344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ今は“怒り”の段階ね。しばらくそこにいよう。怒りなら向き合える",
              "maleVariant": "",
              "stringId": "1734264170665197568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "モックスの店、リジーズ・バーも以前は売春宿だった。タイガークロウズが女の子を殺したせいで、みんなが立ち上がり、今は私たちのものになってる",
              "maleVariant": "",
              "stringId": "1734264802344157184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クラウドにも同じ変化が必要なのよ",
              "maleVariant": "",
              "stringId": "1734265363240042496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手を貸す？",
              "maleVariant": "",
              "stringId": "1734267195010039808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつ決行？",
              "maleVariant": "いつ決行だ？",
              "stringId": "1734268812602097664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早朝。クラウドは閉まってる。その時間なら誰もいない",
              "maleVariant": "",
              "stringId": "1734389407448809472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無理よ、ジュディ。無茶が過ぎる",
              "maleVariant": "無理だ、ジュディ。無茶が過ぎる",
              "stringId": "1770643260435316736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他にいい考えは？",
              "maleVariant": "",
              "stringId": "1770643804704980992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最悪。でも思いついた",
              "maleVariant": "",
              "stringId": "1846096795288690688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気持ちの段階をひとつずつ、乗り越えて行くの。ショック、怒り、絶望…",
              "maleVariant": "気持ちの段階をひとつずつ、乗り越えて行くんだ。ショック、怒り、絶望…",
              "stringId": "1846105026140528640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "計画はそれだけ？",
              "maleVariant": "計画はそれだけか？",
              "stringId": "1846119952141737984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クラウドを乗っ取る、っていうの？ そんなことをするには、色々と計画不足に思えるけど",
              "maleVariant": "クラウドを乗っ取る、っていうのか？ そんなことをするには、色々と計画不足に思えるがな",
              "stringId": "1846120473158180864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "たとえば？",
              "maleVariant": "",
              "stringId": "1846126585752281088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "始まりと真ん中がない。ゴールしかないじゃない",
              "maleVariant": "始まりと真ん中がない。ゴールしかないじゃないか",
              "stringId": "1846126811691048960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真ん中は大丈夫。内部に協力者がいるのよ。始まりは、あなたの役目",
              "maleVariant": "",
              "stringId": "1846135388791070720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手を貸す？",
              "maleVariant": "",
              "stringId": "1846154026969423872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "協力する",
              "maleVariant": "",
              "stringId": "1847329468871041028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無理よ。やめた方がいい",
              "maleVariant": "無理だ。やめた方がいい",
              "stringId": "1847330226060353536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ない",
              "maleVariant": "",
              "stringId": "1847461638436511744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあこのまま何もしないの？ クラウドで人が死んでるのに",
              "maleVariant": "",
              "stringId": "1847461716735778816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この計画でだって、人は死ぬでしょ",
              "maleVariant": "この計画でだって、人は死ぬだろ",
              "stringId": "1847523507507519488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしても無理ってわけね",
              "maleVariant": "",
              "stringId": "1847532527458934784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早朝って？",
              "maleVariant": "",
              "stringId": "1847564012052242432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8時前、入口の… 待って、やっぱりクラウドの上のバルコニーで待ってる",
              "maleVariant": "",
              "stringId": "1847564124476366848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V… 私はこんなこと二度と起こさせない",
              "maleVariant": "",
              "stringId": "1847577675668611072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よく考えた上でのことなの？",
              "maleVariant": "よく考えた上でのことなのか？",
              "stringId": "1847579887811649536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジュディ、よく考えた上でのことなの？",
              "maleVariant": "ジュディ、よく考えた上でのことなのか？",
              "stringId": "1847580248303689728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいえ、まったく",
              "maleVariant": "",
              "stringId": "1847584745251889152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私がわかってるのは、当時、モックスは指をくわえてないで、行動を起こしたってこと",
              "maleVariant": "",
              "stringId": "1847586144790470656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで… 計画は？",
              "maleVariant": "",
              "stringId": "1906895370260037636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずはマイコ・マエダと会って話す。彼女がこちら側についてくれなかったら、もうお手上げよ",
              "maleVariant": "",
              "stringId": "1906895370293592064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "協力してくれそうなの？",
              "maleVariant": "協力してくれそうなのか？",
              "stringId": "1906895370360700944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイコって何者？",
              "maleVariant": "マイコって何者だ？",
              "stringId": "1906895370360700960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クラウドにずいぶんツテがあるのね",
              "maleVariant": "クラウドにずいぶんツテがあるんだな",
              "stringId": "1906895370377478156"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイコは協力してくれそうなの？",
              "maleVariant": "マイコは協力してくれそうなのか？",
              "stringId": "1906895370411032576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その自信があれば、とっくに本人に連絡してるし、今こんな話もしてない",
              "maleVariant": "",
              "stringId": "1906895370411032592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "てことは、袋叩きにされて、メガタワーの窓から放り出される可能性もあるってこと？",
              "maleVariant": "てことは、袋叩きにされて、メガタワーの窓から放り出される可能性もあるってことか？",
              "stringId": "1906895370411032608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、そうね",
              "maleVariant": "",
              "stringId": "1906895370427809792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ま、それでもいいけど",
              "maleVariant": "ま、それでもいいがな",
              "stringId": "1906895370427809804"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイコをよく知ってるの？",
              "maleVariant": "マイコをよく知ってるのか？",
              "stringId": "1906895370478141444"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "かなりね",
              "maleVariant": "",
              "stringId": "1906895370478141460"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エヴリンにマイコ… クラウドにずいぶんツテがあるのね",
              "maleVariant": "エヴリンにマイコ… クラウドにずいぶんツテがあるんだな",
              "stringId": "1906895370528473092"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "前に働いてたからね。行動チップの仕上げを手伝った",
              "maleVariant": "",
              "stringId": "1906895370545250304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてやめたの？",
              "maleVariant": "どうしてやめたんだ？",
              "stringId": "1906895370562027520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも合わなくて。搾取の香りがプンプンしてた",
              "maleVariant": "",
              "stringId": "1906895370562027532"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイコ・マエダ… それって、何者？",
              "maleVariant": "マイコ・マエダ… そいつは何者だ？",
              "stringId": "1906895371048566788"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クラウドの裏の支配人。結構なやり手よ",
              "maleVariant": "",
              "stringId": "1906895371065344000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、表向きは？",
              "maleVariant": "",
              "stringId": "1906895371065344016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただのドールとして働いてる",
              "maleVariant": "",
              "stringId": "1906895371082121216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。協力する",
              "maleVariant": "",
              "stringId": "1906953285226446848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わあ、やった！",
              "maleVariant": "",
              "stringId": "1912528292027260928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつマイコと会う？",
              "maleVariant": "",
              "stringId": "1915317265128710148"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイコをよく知ってるの？",
              "maleVariant": "マイコをよく知ってるのか？",
              "stringId": "1915319250242461696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はできない",
              "maleVariant": "",
              "stringId": "1944822751565955072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はできない。ただでさえいろいろ忙しくて",
              "maleVariant": "今はできない。ただでさえいろいろ忙しいんだ",
              "stringId": "1944832319931006976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。正直、この作戦はかなり時間がかかると思うから…",
              "maleVariant": "",
              "stringId": "1944832766926372864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも、手が空いたら声をかけて",
              "maleVariant": "",
              "stringId": "1988091944792449024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "忍び込むのよ。誰にも気づかれないように",
              "maleVariant": "",
              "stringId": "1993774778962108416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "バルコニーね… 了解",
              "maleVariant": "バルコニーだな… 了解",
              "stringId": "1993777352268275712"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
