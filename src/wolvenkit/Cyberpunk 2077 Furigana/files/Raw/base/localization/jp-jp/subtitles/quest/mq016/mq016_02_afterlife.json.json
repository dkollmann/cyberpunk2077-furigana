              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バートモスのデッキを見つけた",
                    "maleVariant": null,
                    "stringId": 1776488793974415364
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ニックス、いいものを見つけたの… バートモスのデッキよ",
                    "maleVariant": "ニックス、いいものを見つけた… バートモスのデッキだ",
                    "stringId": 1776489533782532096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "へえ、他には？ アラン・チューリングのテープか？",
                    "maleVariant": null,
                    "stringId": 1776490191046746112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここから何か取り出せる？",
                    "maleVariant": "ここから何か取り出せるか？",
                    "stringId": 1776493559123189760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "んー… ぼろぼろだし、湿気による損傷もある。メムキューブの暗号はおそらく神レベルだ",
                    "maleVariant": null,
                    "stringId": 1776494604511518720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。始めて",
                    "maleVariant": "わかった。始めてくれ",
                    "stringId": 1776498314507624448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "触らないでおこう",
                    "maleVariant": null,
                    "stringId": 1776498617168601088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やっぱり… 触らないでおこう",
                    "maleVariant": null,
                    "stringId": 1776499958943227904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やってみるしかない。始めて",
                    "maleVariant": "やってみるしかない。始めてくれ",
                    "stringId": 1776501448793563136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。でも気が変わったら… また来てくれ",
                    "maleVariant": null,
                    "stringId": 1776552096943742976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「ほら」？ ずいぶん軽々しいな。 Vならたとえ聖杯を見つけても、酒のつまみを入れる器にでもしちまいそうだぜ、まったく…",
                    "maleVariant": null,
                    "stringId": 1776555433277693952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さて、始めよう。なるほど… データはほとんど破損してるが… 全てじゃない。パスワードは… 辞書攻撃なら…？ ダメか。ならコマンドを一発挟んだら…？",
                    "maleVariant": null,
                    "stringId": 1776556759466287104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほら",
                    "maleVariant": null,
                    "stringId": 1776567416941092864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ディスクは感染してると思う？",
                    "maleVariant": "ディスクは感染してると思うか？",
                    "stringId": 1776580228409106432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ディスクに、大昔のヴェクターが仕込まれてる可能性は？ DataKrashやRABIDみたいな。だとしたら…",
                    "maleVariant": null,
                    "stringId": 1776580996939816960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よし、やった！",
                    "maleVariant": null,
                    "stringId": 1776581987298877444
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "できるの？ できないの？",
                    "maleVariant": "できるのか？ どうなんだ？",
                    "stringId": 1776620870795436032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "試してはみる。が、失敗する可能性も高い。そしたらデータは完全に消え、取り戻せないぞ",
                    "maleVariant": null,
                    "stringId": 1776621669273477120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "気が変わった。やってみて",
                    "maleVariant": "気が変わった。やってみてくれ",
                    "stringId": 1777680136599937024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "気が変わった… やってみましょ",
                    "maleVariant": "気が変わった… やってみよう",
                    "stringId": 1777681532816629764
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本当だって。ほら",
                    "maleVariant": null,
                    "stringId": 1892492791733604352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "忙しいんだ、さっさと消えろ",
                    "maleVariant": null,
                    "stringId": 1892494082706821120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うおお、マジかよ…！ すげえな、V！",
                    "maleVariant": null,
                    "stringId": 1892497134499188736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どこで見つけたんだ？",
                    "maleVariant": null,
                    "stringId": 1892497466285412352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "冷蔵庫の中で",
                    "maleVariant": null,
                    "stringId": 1892498251190685700
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "関係ないでしょ",
                    "maleVariant": "関係ないだろ",
                    "stringId": 1892498680670638080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "郊外で見つけたの。冷蔵庫の中… バートモス本人の死体のそばでね",
                    "maleVariant": "郊外で見つけた。冷蔵庫の中… バートモス本人の死体のそばでな",
                    "stringId": 1892498840909828096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ははは！ バートモスらしいな。ネットウォッチが指名手配して探し回ったんだぞ。上海、ラホール、サンティアゴ・デ・チレ…",
                    "maleVariant": null,
                    "stringId": 1892499490808844288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その間もネットを騒がせ続けてた。延命装置がダウンしなかったら、被害はさらに広がってたはずだ",
                    "maleVariant": null,
                    "stringId": 1892500695278735360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クリスマスツリーの下にあったの",
                    "maleVariant": "クリスマスツリーの下にあった",
                    "stringId": 1892503669677805568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや。マジな話、さ？",
                    "maleVariant": null,
                    "stringId": 1892505635699085312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マジな話、関係ないでしょ",
                    "maleVariant": "マジな話、関係ないだろ",
                    "stringId": 1892505900376444928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よし。カウンターの上に置いてくれ… 落とすなよ！",
                    "maleVariant": null,
                    "stringId": 1892528112135360512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その時は、やけ酒するしかないだろうな",
                    "maleVariant": null,
                    "stringId": 1892529284325888000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リスクは百も承知だ。でも正直… ものすごく興味がある。あんたもそうだろ？",
                    "maleVariant": null,
                    "stringId": 1892529732529213440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お！ いいぞ。ハードルはあとひとつ、こいつは簡単に抜けれるはずだ。へえ、あのバートモスほどの奴が、この程度とは",
                    "maleVariant": null,
                    "stringId": 1893598395856375808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああーっ！ ああああああっ！",
                    "maleVariant": null,
                    "stringId": 1893601473586319360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クソッ、まずいことになった",
                    "maleVariant": null,
                    "stringId": 1893601910079148032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、早くどうにかしろ。あと数分でこいつの脳みそはこんがり焼き上がっちまうぞ",
                    "maleVariant": null,
                    "stringId": 1893603023129989120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リセットしかない",
                    "maleVariant": null,
                    "stringId": 1893605573015162880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だと思った！ クソッ！",
                    "maleVariant": null,
                    "stringId": 1893609298077208576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クソッ！",
                    "maleVariant": null,
                    "stringId": 1893613233542320128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よしと… あたしにだってできるはず",
                    "maleVariant": "よしと… 俺にだってできるはずだ",
                    "stringId": 1893616828044070912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よし、やってみるか…",
                    "maleVariant": null,
                    "stringId": 1893617235428429824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バカ野郎、甘く見るな。21世紀最強のハッカーが使ってたサイバーデッキなんだぞ？",
                    "maleVariant": null,
                    "stringId": 1893619011766505472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ、あんたがやる？ でしょ？ いいから黙ってて",
                    "maleVariant": "じゃあ、お前がやるか？ だよな？ いいから黙ってろ",
                    "stringId": 1893619490806353920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここにあるのは、脳みそを焼いちまう機械だぞ。本気でジャックインするのか？",
                    "maleVariant": null,
                    "stringId": 1893623322252726272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しーっ",
                    "maleVariant": null,
                    "stringId": 1893624004632432640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ… うう… クソ…",
                    "maleVariant": null,
                    "stringId": 1893627263019438080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "息してる？",
                    "maleVariant": "生きてるな？",
                    "stringId": 1893635180724539392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんとかな… 頭の中はまだくすぶってる気がするが",
                    "maleVariant": null,
                    "stringId": 1893635492109668352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一体… 何が起きたんだ？",
                    "maleVariant": null,
                    "stringId": 1893636410729353216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だから、あたしが代わりにやってあげたの",
                    "maleVariant": "だから、俺が代わりにやってやったんだ",
                    "stringId": 1893655377707585536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だから電源を切ったの。正解ね",
                    "maleVariant": "だから電源を切った。正解だな",
                    "stringId": 1893655684227321856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "けいれんを起こして、電気が消えたと思ったら、火花が散り始めた…",
                    "maleVariant": null,
                    "stringId": 1893655992290562048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ま、待てよ… 素人のくせに、バートモスのデッキに侵入したってのか？",
                    "maleVariant": null,
                    "stringId": 1893667058777116672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おい待てよ、デッキは… あーあ、全部消えてる",
                    "maleVariant": null,
                    "stringId": 1893670150063050752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "死んだほうがよかった？",
                    "maleVariant": "死んだほうがよかったか？",
                    "stringId": 1893671476587188228
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これでよかったのよ",
                    "maleVariant": "これでよかった",
                    "stringId": 1893672192336777216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ。ネットには詳しいから",
                    "maleVariant": "ああ。ネットには詳しくてな",
                    "stringId": 1893674124736516100
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "運がよかった",
                    "maleVariant": null,
                    "stringId": 1893674463065853952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "運がよかったのよ",
                    "maleVariant": "運がよかったんだ",
                    "stringId": 1893678344659324928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうやら街一番のネットランナーはあたしみたいね",
                    "maleVariant": "どうやら街一番のネットランナーは俺みたいだな",
                    "stringId": 1893678550096334848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "死んだほうがマシだった？",
                    "maleVariant": "死んだほうがマシだったか？",
                    "stringId": 1893678795781885952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "中身がわからず終いで、きっとよかったのよ。最悪の場合、またネットを破壊してた",
                    "maleVariant": "中身がわからず終いで、きっとよかったんだ。最悪の場合、またネットを破壊してた",
                    "stringId": 1893679085323079680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よし… 骨とう品から何が出てきたか見てみよう…",
                    "maleVariant": null,
                    "stringId": 1893679331075739648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "普段なら代金を請求するところだが、今回は… 俺があんたに借りを作っちまったみたいだな",
                    "maleVariant": null,
                    "stringId": 1893679647477256192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それは疑問だが… 素質はあるかもな",
                    "maleVariant": null,
                    "stringId": 1893682957772967936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だとしたら運がよすぎるぞ",
                    "maleVariant": null,
                    "stringId": 1893684551205834752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ログ… メール… チャット履歴… どれも興味深いが、特別なものはないな",
                    "maleVariant": null,
                    "stringId": 1893685786529026048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そう。まあ、後で自分でも調べてみる。まだ何かあるかもしれない",
                    "maleVariant": "そうか。まあ、後で自分でも調べてみるよ。まだ何かあるかもしれない",
                    "stringId": 1893686443021488128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなことはないけど… この先一生、中に何が入ってたか気になるじゃないか",
                    "maleVariant": null,
                    "stringId": 1893691498600648704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも俺、思うんだ… 突然、電話や電報の時代に戻ったら、世界はどうなるんだろうって… もしかしたらその方がいい世の中になるんじゃないか、ってさ",
                    "maleVariant": null,
                    "stringId": 1893694949657202688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これをやるよ。デーモンがいくつか入ってる。バートモスのほどじゃないが… それでも役に立つはずだ",
                    "maleVariant": null,
                    "stringId": 1893701343353556992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ありがとう、ニックス… 一人で大丈夫？ 助けが必要なら言ってよ",
                    "maleVariant": "ありがとう、ニックス… 一人で大丈夫か？ 助けが必要なら言ってくれ",
                    "stringId": 1893703672165355520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "心配しなくて結構。鎮痛剤をウイスキーで体の中に流し込めば、すぐに元通りになるさ",
                    "maleVariant": null,
                    "stringId": 1893707259150913536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[デッキを置く]",
                    "maleVariant": null,
                    "stringId": 1963163708217937920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[デッキを見せる] 本当だって",
                    "maleVariant": null,
                    "stringId": 2414185841483603972
                  }
                }
              ]
            }
          }
        }
      }
