  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺にも親父を殴り倒せるだけの強さがあればな",
              "maleVariant": "",
              "stringId": "1264751944270893056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこ行くの？",
              "maleVariant": "",
              "stringId": "1264763932464140304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "好きに決まってる",
              "maleVariant": "",
              "stringId": "1264764054937817088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "趣味じゃない",
              "maleVariant": "",
              "stringId": "1264769726207250432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他のバンドの方がいい",
              "maleVariant": "",
              "stringId": "1264769905991897088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケリーは悪くない",
              "maleVariant": "",
              "stringId": "1264771357556625408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実のところ、ケリーは好きじゃない",
              "maleVariant": "",
              "stringId": "1264771369854324736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は他のバンドの方がいい",
              "maleVariant": "",
              "stringId": "1264771386631540736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、着いたよ！",
              "maleVariant": "",
              "stringId": "1264782239309254656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのうち体格でも勝るようになる。でもいいか。あんな奴を殴るのは拳の無駄遣いだ",
              "maleVariant": "",
              "stringId": "1315488378827530240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして？",
              "maleVariant": "",
              "stringId": "1315496923211980800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "父親の脆さに気づくってのは、案外辛いもんだぜ",
              "maleVariant": "",
              "stringId": "1315497671576477696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これからもこんな調子なら… 逃げ出そうと思ってる",
              "maleVariant": "",
              "stringId": "1315503943201808384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう何年かしたら家を出られるだろ。そしたら親父のことなんかアッと言う間に忘れられる",
              "maleVariant": "",
              "stringId": "1315506636985163776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "かもね。でもそのためには… 強くならないと",
              "maleVariant": "",
              "stringId": "1315508535914356736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈タイムマシン〉だ",
              "maleVariant": "",
              "stringId": "1315533967002918912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいね！ 前にケリー・ユーロダインを見かけたよ",
              "maleVariant": "",
              "stringId": "1315534509208985600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ファンなのか？",
              "maleVariant": "",
              "stringId": "1315539833961799680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "当たり前だろ！ レジェンドだぜ。好きじゃないの？",
              "maleVariant": "",
              "stringId": "1315540018544730112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラジオで言ってたよ。ケリーはシルヴァーハンドの死を利用して売名行為に走ったって",
              "maleVariant": "",
              "stringId": "1315555681736810496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メディアはろくでもない連中ばかりだ。音楽を扱う奴らは特に脳みそが腐ってやがる",
              "maleVariant": "",
              "stringId": "1315567742810284032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケリーはいいミュージシャンだ。それだけ知ってりゃいい",
              "maleVariant": "",
              "stringId": "1315568632053063680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴は本物さ。あらゆる面でずっとそうだった",
              "maleVariant": "",
              "stringId": "1315569161105793024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして？",
              "maleVariant": "",
              "stringId": "1315573971334946816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "金や名声… そういう俗っぽいもんばっかに気を取られてた。音楽への愛が足りてなかったんだよ",
              "maleVariant": "",
              "stringId": "1315574092533555200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それさえあれば、もっと魂のこもった音楽で、伝説になれたはずだ",
              "maleVariant": "",
              "stringId": "1315574845713113088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとな。ここで待ってるか？",
              "maleVariant": "",
              "stringId": "1315583147985559552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケリーを知ってるみたいな言い方だね",
              "maleVariant": "",
              "stringId": "1315598153275891712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知ってるんだよ",
              "maleVariant": "",
              "stringId": "1315600409392013312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "当ててみる",
              "maleVariant": "",
              "stringId": "1315613234063695872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだな… 〈Ten Inch Monkeys〉、〈Rubicones〉、あとは… 〈Velvet Dusk〉？",
              "maleVariant": "",
              "stringId": "1315613388950953984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい線いってる。どれも悪くない",
              "maleVariant": "",
              "stringId": "1315614015865184256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし！",
              "maleVariant": "",
              "stringId": "1315621537258889216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一緒に行くよ。新譜をチェックしたいしさ",
              "maleVariant": "",
              "stringId": "1486752971351613440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうやって金を稼いでる？",
              "maleVariant": "",
              "stringId": "1677952401177296896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺もミュージシャンになってみせるよ",
              "maleVariant": "",
              "stringId": "1677969988279844864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "忘れるな、楽な道じゃないぞ",
              "maleVariant": "",
              "stringId": "1677971878031900672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "保険を用意しといたほうがいい",
              "maleVariant": "",
              "stringId": "1677972056256266240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰にも言わない？",
              "maleVariant": "",
              "stringId": "1695451709560283136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誓おう",
              "maleVariant": "",
              "stringId": "1695454524609355776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フアレスさん家を掃除してお金をもらってる。つっても大して汚れてないけどね",
              "maleVariant": "",
              "stringId": "1695454684261343232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "寂しくて、話し相手が欲しいだけなのかも",
              "maleVariant": "",
              "stringId": "1695460326892068864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "懐かしいのばっか挙げたな。〈Ten Inch Monkeys〉は思い出補正込みだが…",
              "maleVariant": "",
              "stringId": "1695483204337360896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈Rubicones〉と〈Velvet Dusk〉は、ガチでよかった",
              "maleVariant": "",
              "stringId": "1695488776201789440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってるよ。とにかく練習を頑張るつもりさ…",
              "maleVariant": "",
              "stringId": "1695503610767171584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "始めはそれでいいが、後に待つのは苦労ばかりだ",
              "maleVariant": "",
              "stringId": "1695514870342705152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "好き勝手なことを言う批評家にアンチ、絶えないプレッシャーに、滅茶苦茶なライフスタイル…",
              "maleVariant": "",
              "stringId": "1695515375353683968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボロボロになっちまう",
              "maleVariant": "",
              "stringId": "1695523428786462720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "保険？",
              "maleVariant": "",
              "stringId": "1695523986343682048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何でもいい。収入を確保するんだよ",
              "maleVariant": "",
              "stringId": "1695524102408462336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで最悪の事態は避けられる。やけくそになったら終いだからな",
              "maleVariant": "",
              "stringId": "1695524777422970880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "音楽ってのは体の内に宿るもんだ。お前の血や肉と一緒さ",
              "maleVariant": "",
              "stringId": "1709891868884164608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そっか！",
              "maleVariant": "",
              "stringId": "1709892226238865408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるべく多くの人に向けて書けばいい",
              "maleVariant": "",
              "stringId": "1730062023349985280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分の知ってることについて書くんだ",
              "maleVariant": "",
              "stringId": "1730062023349985296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真実を書け",
              "maleVariant": "",
              "stringId": "1730062542235721728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずは曲作りからだな。経験を積めば積むほど、いいものが書けるようになる",
              "maleVariant": "",
              "stringId": "1730069569691410432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "曲作りを始めるといい",
              "maleVariant": "",
              "stringId": "1730076055461126148"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "楽な道じゃないぞ",
              "maleVariant": "",
              "stringId": "1730084607697313792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "保険も用意しとけよ",
              "maleVariant": "",
              "stringId": "1730084914435153920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "始めるなら早い方がいい",
              "maleVariant": "",
              "stringId": "1730093877427920896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実は数曲考えてる",
              "maleVariant": "",
              "stringId": "1730094533148631040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほう",
              "maleVariant": "",
              "stringId": "1730094999857225728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも歌詞で苦戦してて。とりあえず書いてみても、どっかで聞いたようなのしかできない",
              "maleVariant": "",
              "stringId": "1730095161472147456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何を書くべきかわからないんだ",
              "maleVariant": "",
              "stringId": "1730095714868617216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "多くの人に向けて歌詞を書くのが大事だ",
              "maleVariant": "",
              "stringId": "1730105577690587136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直に、真実だけを書け。自分の気持ちに対しては特にな",
              "maleVariant": "",
              "stringId": "1730105743533367296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分の知ってることを書け。曲作りだけじゃなく、ものを書く時の鉄則だ",
              "maleVariant": "",
              "stringId": "1730105760562241536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一般ウケを狙えってこと？",
              "maleVariant": "",
              "stringId": "1730120786387886080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういう意味じゃない。誰もに好かれる歌詞じゃなく、誰もが共感する歌詞を書くんだ",
              "maleVariant": "",
              "stringId": "1730120999475306496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それと、なるべくシンプルにな。自分のためだけに書くのは絶対にご法度だ",
              "maleVariant": "",
              "stringId": "1730124115977396224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんで？",
              "maleVariant": "",
              "stringId": "1730136685350182912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "音楽ってのは自分のためにあるものじゃない。誰かに贈るものだ",
              "maleVariant": "",
              "stringId": "1730136891995152384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知ってることなんて何もないよ。自分のうちのことぐらいだ",
              "maleVariant": "",
              "stringId": "1730138223854444544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まず第一に、そんなわけねえ。次に、タワー暮らしってのはいい題材だ",
              "maleVariant": "",
              "stringId": "1730138536766300160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3つ目は？ 普通3つでセットだろ？",
              "maleVariant": "",
              "stringId": "1730139329808523264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前はまだ若い。書きたいことはそのうちいくらでも出てくる",
              "maleVariant": "",
              "stringId": "1730139754825736192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人生にもみくちゃにされれば、おのずと音楽の方からお前を見つけてくれるさ",
              "maleVariant": "",
              "stringId": "1730141083900010496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そりゃあ、自分の気持ちに嘘はつけないよ",
              "maleVariant": "",
              "stringId": "1730148122160226304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本気で惚れてないならラブソングなんか書くな",
              "maleVariant": "",
              "stringId": "1730150401697341440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お涙ちょうだいが売れるからって理由で、泣かせる曲を書くんじゃない",
              "maleVariant": "",
              "stringId": "1730151101307248640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー… うん",
              "maleVariant": "",
              "stringId": "1730153626899013632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "胸をえぐられるような何かを感じたら、それが曲を書くべき時だ",
              "maleVariant": "",
              "stringId": "1730155207363092480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "真実が指をすり抜けても、音楽ならそれを掴めることがある",
              "maleVariant": "",
              "stringId": "1730156659766681600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "親父に何て？",
              "maleVariant": "",
              "stringId": "1796475072117014532"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しっかりしろって言ってやったんだ",
              "maleVariant": "",
              "stringId": "1796475072217677828"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "惨めな野郎だって、言ってやった",
              "maleVariant": "",
              "stringId": "1796475072301563908"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大したことじゃない",
              "maleVariant": "",
              "stringId": "1796475072368672772"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何の解決にもならないからだ",
              "maleVariant": "",
              "stringId": "1802571092307542016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言わない",
              "maleVariant": "",
              "stringId": "1802633730580348932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさかクスリの密売か？",
              "maleVariant": "",
              "stringId": "1802634686411898880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クスリか？",
              "maleVariant": "",
              "stringId": "1802634849788428288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なわけないだろ！",
              "maleVariant": "",
              "stringId": "1802638102672506880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありえる話だな",
              "maleVariant": "",
              "stringId": "1802639411916111872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "父親の弱さに気づいちまうからだ",
              "maleVariant": "",
              "stringId": "1803831805030236164"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何の解決にもならないからだ",
              "maleVariant": "",
              "stringId": "1803832010987339776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ",
              "maleVariant": "",
              "stringId": "1806952472899182592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、親父は？",
              "maleVariant": "",
              "stringId": "1806954147584106496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何も。少しは反省してるといいんだがな",
              "maleVariant": "",
              "stringId": "1806956975786516480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、親父は？",
              "maleVariant": "",
              "stringId": "1806972831933358080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別に何も",
              "maleVariant": "",
              "stringId": "1806990298491609088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話したくないの？",
              "maleVariant": "",
              "stringId": "1806992668088819712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "意味がないからな",
              "maleVariant": "",
              "stringId": "1806992960834461696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ぶちのめせば清々するだろう。でもやがてそれが、お前を内側から蝕んでいく",
              "maleVariant": "",
