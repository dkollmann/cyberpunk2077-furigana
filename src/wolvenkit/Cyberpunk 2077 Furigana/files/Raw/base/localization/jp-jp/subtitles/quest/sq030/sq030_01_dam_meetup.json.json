            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "フンフンフン…",
                    "maleVariant": null,
                    "stringId": 1392412829519167504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "せっかくだけど遠慮しとく。ダイビングは好きじゃないし",
                    "maleVariant": "せっかくだが遠慮しとく。ダイビングは好きじゃないんだ",
                    "stringId": 1392413464419352576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[立ち上がる] もちろんやる！",
                    "maleVariant": null,
                    "stringId": 1392428470733094912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[立ち上がる] ダイビングは好きじゃない",
                    "maleVariant": null,
                    "stringId": 1392493040248012800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "断る理由がない",
                    "maleVariant": null,
                    "stringId": 1392493694324555776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうこなくっちゃ！ それじゃ早速はじめよっか。機材はそこね",
                    "maleVariant": null,
                    "stringId": 1393665946961297408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "誰かやりたい人がいるか、ツテを当たってみようか",
                    "maleVariant": null,
                    "stringId": 1404001566564487168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ううん、大丈夫。気にしないで",
                    "maleVariant": null,
                    "stringId": 1404002064260599808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自分で何とかするからさ",
                    "maleVariant": null,
                    "stringId": 1404003691197566976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待った？",
                    "maleVariant": "待ったか？",
                    "stringId": 1740289813051850752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V！ 元気そうね",
                    "maleVariant": null,
                    "stringId": 1740290255299264512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここで何をするの？",
                    "maleVariant": "ここで何をするんだ？",
                    "stringId": 1740290382604779524
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クラウドの様子は？",
                    "maleVariant": null,
                    "stringId": 1740291030641524736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "モックスは最近どうしてる？",
                    "maleVariant": null,
                    "stringId": 1740292017057939456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で… 何をするのか教えてくれる？",
                    "maleVariant": "で… 何をするのか教えてくれるか？",
                    "stringId": 1740292865515307008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最近クラウドはどう？ 何か聞いてる？",
                    "maleVariant": "最近クラウドはどうだ？ 何か知ってるか？",
                    "stringId": 1740293426276974592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "モックスはどうしてる？ なにか事件はあった？",
                    "maleVariant": "モックスはどうしてる？ なにか事件はあったか？",
                    "stringId": 1740294280455372800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあね。あんまり顔出してないから",
                    "maleVariant": null,
                    "stringId": 1740319804892364800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バーチャの撮り方について試行錯誤してたんだけど、2人のアクターの体験を同時に記録する方法を編み出したの",
                    "maleVariant": null,
                    "stringId": 1740324377572917248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あなたがよければ、それを試せないかと思って",
                    "maleVariant": null,
                    "stringId": 1740325008765337600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうやって別々の神経トラックをミックスするの？",
                    "maleVariant": "どうやって別々の神経トラックをミックスするんだ？",
                    "stringId": 1740326339903209472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいえ、撮るのは水の中よ",
                    "maleVariant": null,
                    "stringId": 1740327230723047424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最初は、2つの別々のデータストリームから1つの感覚プロフィールを作ろうとしたの。でも上手くいかなくて",
                    "maleVariant": null,
                    "stringId": 1740329393289412608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "相互干渉で、ホワイトノイズだらけになりそうだもんね…",
                    "maleVariant": "相互干渉で、ホワイトノイズだらけになりそうだもんな…",
                    "stringId": 1740330120732721152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうすればいいの？",
                    "maleVariant": "どうすればいいんだ？",
                    "stringId": 1754747665059323904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どう？ やってみない？",
                    "maleVariant": null,
                    "stringId": 1763356428702040064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あれだけのことがあったにしては、でしょ？",
                    "maleVariant": "あれだけのことがあったにしては、だろ？",
                    "stringId": 1773381435330879488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今日は辛気臭い話はなし！ いい？",
                    "maleVariant": null,
                    "stringId": 1773389418534232064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうして？",
                    "maleVariant": "なんでだよ？",
                    "stringId": 1773420775385817088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "物事の進め方に納得いかないことが多くてさ",
                    "maleVariant": null,
                    "stringId": 1773420867962494976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どこでもできそうな実験だけど、どうしてわざわざこの湖に？ そもそもここは何なの？",
                    "maleVariant": "どこでもできそうな実験だが、どうしてわざわざこの湖に？ そもそもここは何なんだ？",
                    "stringId": 1773513361828044800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんでこんな場所で？",
                    "maleVariant": null,
                    "stringId": 1773531654328639488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "別々の体験トラックをどうミックスするの？",
                    "maleVariant": "別々の体験トラックをどうミックスするんだ？",
                    "stringId": 1773552673546264576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何をすればいい？",
                    "maleVariant": "何をすればいいんだ？",
                    "stringId": 1773553907560833024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あなたは単にバーチャを記録するだけでいい",
                    "maleVariant": null,
                    "stringId": 1773661006965989376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それだけ…？",
                    "maleVariant": null,
                    "stringId": 1773674296249114624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まぁまぁ、そう慌てないで。これは私の最高傑作になるかも",
                    "maleVariant": null,
                    "stringId": 1773674432446554112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そう、なんか前衛アートみたいな仕上がりになっちゃって。でも何度かトライするうちに… 特定の感覚を、個別に隔離する方法がわかったの",
                    "maleVariant": null,
                    "stringId": 1773704441651486720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "変わったことはないかな",
                    "maleVariant": null,
                    "stringId": 1834514647880523776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "浮かない顔してる",
                    "maleVariant": "浮かない顔してるな",
                    "stringId": 1834535362759450624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "落ち込むようなことでもあったの？",
                    "maleVariant": "落ち込むようなことでもあったのか？",
                    "stringId": 1834535708453986304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ん？ ううん…",
                    "maleVariant": null,
                    "stringId": 1834539052589342720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "新しい状況をみんなで受け入れようとしてる。私も含めてね",
                    "maleVariant": null,
                    "stringId": 1834540353159127040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大変そうだね…",
                    "maleVariant": "大変そうだな…",
                    "stringId": 1834546095714951168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジュディ",
                    "maleVariant": null,
                    "stringId": 1834549333314985984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クラウドやモックス… アラサカにミリテク。せっかく二人になったんだし、そういう話はやめにしない？",
                    "maleVariant": null,
                    "stringId": 1834549417469501440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなこともないって",
                    "maleVariant": null,
                    "stringId": 1834568849528938496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今日は… そういう暗くなるようなことは… 考えたくないんだ",
                    "maleVariant": null,
                    "stringId": 1835782486994149376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "元は知り合いのコテージだったんだけど、使われてないから、たまに勝手にお邪魔させてもらってるの",
                    "maleVariant": null,
                    "stringId": 1902378277690630144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "つまり、ここで撮るってわけ？",
                    "maleVariant": "つまり、ここで撮るってわけか？",
                    "stringId": 1902379965059129344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ダメだ、絶対にお断りだ。他のお人よしに当たるよう言ってやれ",
                    "maleVariant": null,
                    "stringId": 1902518254768447488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしてよ？",
                    "maleVariant": "どうしてだよ？",
                    "stringId": 1902533985203609604
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしてよ？",
                    "maleVariant": "どうしてだよ？",
                    "stringId": 1902536073950224384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあな。嫌な予感がするだけさ",
                    "maleVariant": null,
                    "stringId": 1902541150886780928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それを別の神経トラックに置いとけば、干渉を回避できる… なるほどね",
                    "maleVariant": "それを別の神経トラックに置いとけば、干渉を回避できる… なるほどな",
                    "stringId": 1918565809914601472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ただ… 深い暗闇のことを考えると… 手が冷や汗でびっしょりになるんだ。手なんてないし、汗もかかないのによ",
                    "maleVariant": null,
                    "stringId": 1928785933200220160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "嫌だったら岸で待ってていいのよ",
                    "maleVariant": "嫌だったら岸で待ってればいいだろ",
                    "stringId": 1928788332174336000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[待つ]",
                    "maleVariant": null,
                    "stringId": 2189588777486508032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そこまで考えこまなくても",
                    "maleVariant": null,
                    "stringId": 2253559970273124352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうしたの、V？",
                    "maleVariant": null,
                    "stringId": 2253561366691143688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほら、腹をくくりなよ",
                    "maleVariant": null,
                    "stringId": 2253561427156230152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[座る] もうあたしとは会いたくないのかと思ってた",
                    "maleVariant": "[座る] もう俺とは会いたくないのかと思ってた",
                    "stringId": 2255002567583006724
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[座る] あれだけのことがあったにしては？",
                    "maleVariant": null,
                    "stringId": 2255042866204180480
                  }
                }
              ]
            }
          }
        }
      }
