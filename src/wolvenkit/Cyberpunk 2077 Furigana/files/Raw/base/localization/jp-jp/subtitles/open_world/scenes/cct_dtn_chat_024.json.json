          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あのさ、考えてみたんだ。みんな幾何学模様を着てるでしょ",
                    "maleVariant": null,
                    "stringId": 2172219423136133120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アクセサリーは控え目で、ジャラジャラつけたりしない。たいていモノクロか、せいぜい2～3色",
                    "maleVariant": null,
                    "stringId": 2178118965853405184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「少なさは豊かさ」ってやつ？ それがネオミリタリズムよ",
                    "maleVariant": null,
                    "stringId": 2178119142483935232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうそう。ジングウジとか。シンプルなシルエット、ペンシルスカート、アシンメトリーのジャケット…",
                    "maleVariant": null,
                    "stringId": 2178119469689978880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "マーケットで結構いいコピー品を売ってるのを見かけたけど",
                    "maleVariant": null,
                    "stringId": 2178119730609242112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私も。でも質がね！ 本物とは全然違う。本物は光ってるっていうか…",
                    "maleVariant": null,
                    "stringId": 2178119931398963200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうね。でも、欲しいモノに手が届かないなら、持ってるモノでなんとかしないと",
                    "maleVariant": null,
                    "stringId": 2178120083887079424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ホントそれ。それに、いい女ってのはお金をかけずにオシャレできるものよ",
                    "maleVariant": null,
                    "stringId": 2178120308835991552
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
