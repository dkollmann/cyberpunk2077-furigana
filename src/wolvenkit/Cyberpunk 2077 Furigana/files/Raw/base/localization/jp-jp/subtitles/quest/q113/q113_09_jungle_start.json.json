    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1730332124934664192,
            "femaleVariant": "島1つに過ぎない"
          },
          {
            "stringId": 1730334429100732416,
            "femaleVariant": "それと、指示するまで一言も喋らないように"
          },
          {
            "stringId": 1730334717266194432,
            "femaleVariant": "たとえどんなことが起きても… 私を信じて"
          },
          {
            "stringId": 1730335383405555712,
            "femaleVariant": "1万2千人が住んでる島よ"
          },
          {
            "stringId": 1730335640147292160,
            "femaleVariant": "周りに他の島が腐るほどあるだろ。そこに移住させればいい"
          },
          {
            "stringId": 1730336438491115520,
            "femaleVariant": "コストがかさむだろう"
          },
          {
            "stringId": 1730342228258025472,
            "femaleVariant": "遅くなりました。申し訳ありません"
          },
          {
            "stringId": 1730343109951057920,
            "femaleVariant": "武器の用意はしておいて"
          },
          {
            "stringId": 1730343430211334144,
            "femaleVariant": "我が兄は、どこにいるのです？"
          },
          {
            "stringId": 1730343735070126080,
            "femaleVariant": "なら… 始めましょう… 彼抜きで"
          },
          {
            "stringId": 1730345230624714752,
            "femaleVariant": "きちんと説明してください"
          },
          {
            "stringId": 1730347484425580544,
            "femaleVariant": "ヨリノブは父を殺し、その死を口実に世界中で戦争を始めようとしている"
          },
          {
            "stringId": 1730347754924634112,
            "femaleVariant": "ここに… 目撃者がいます"
          },
          {
            "stringId": 1730348236564951040,
            "femaleVariant": "本当よ、この目で見た",
            "maleVariant": "本当だ、この目で見た"
          },
          {
            "stringId": 1730348664350404608,
            "femaleVariant": "全員ヨリノブに騙されてるのよ",
            "maleVariant": "全員ヨリノブに騙されてるんだ"
          },
          {
            "stringId": 1730348783435083776,
            "femaleVariant": "あんたたちは真実を調べようとすらしてない"
          },
          {
            "stringId": 1730349503261536256,
            "femaleVariant": "すべて本当よ。一部始終をこの目で見た",
            "maleVariant": "すべて本当だ。一部始終をこの目で見た"
          },
          {
            "stringId": 1730349944670089216,
            "femaleVariant": "紛れもない事実だ"
          },
          {
            "stringId": 1730350205086035968,
            "femaleVariant": "裏切り者は黙っていろ！"
          },
          {
            "stringId": 1730350797221097472,
            "femaleVariant": "今の証言で不十分だというなら、父の言葉を聞いてもらうしかないようですね"
          },
          {
            "stringId": 1730352110474457088,
            "femaleVariant": "なに？"
          },
          {
            "stringId": 1730354735387340800,
            "femaleVariant": "見下げ果てた奴らめ"
          },
          {
            "stringId": 1761818864418570240,
            "femaleVariant": "あともう少しよ、V"
          },
          {
            "stringId": 1761819092118945796,
            "femaleVariant": "ええ、でも急がないと",
            "maleVariant": "ああ、だが急がないと"
          },
          {
            "stringId": 1761819306749870080,
            "femaleVariant": "そうするしかないでしょ？",
            "maleVariant": "そうするしかないだろ？"
          },
          {
            "stringId": 1761819467307827200,
            "femaleVariant": "耐えるしかない"
          },
          {
            "stringId": 1761821759696289792,
            "femaleVariant": "体がどんどん弱ってく。急がないとまずい"
          },
          {
            "stringId": 1761822181324505088,
            "femaleVariant": "無理って言ったら、何とかしてくれるの？",
            "maleVariant": "無理って言ったら、何とかしてくれるのか？"
          },
          {
            "stringId": 1761822710830219264,
            "femaleVariant": "気分は最悪だけど… 耐えるしかない",
            "maleVariant": "気分は最悪だが… 耐えるしかない"
          },
          {
            "stringId": 1761823166230970368,
            "femaleVariant": "ここが… 正念場よ"
          },
          {
            "stringId": 1762185025043841024,
            "femaleVariant": "ハナコ様"
          },
          {
            "stringId": 1762186170172719104,
            "femaleVariant": "久しぶりね、オダ",
            "maleVariant": "久しぶりだな… オダ"
          },
          {
            "stringId": 1763182300645834752,
            "femaleVariant": "正気とは思えない"
          },
          {
            "stringId": 1763195064835653632,
            "femaleVariant": "なかなか面白い話ね… だけど、どうせ証拠はないんでしょ"
          },
          {
            "stringId": 1763400557260718080,
            "femaleVariant": "どうなってる！？"
          },
          {
            "stringId": 1763607659929419776,
            "femaleVariant": "一人も逃がすな"
          },
          {
            "stringId": 1811268650187898880,
            "femaleVariant": "な… 何をしてるんです？ その席は…"
          },
          {
            "stringId": 1811269326745911296,
            "femaleVariant": "V、あなたはここよ"
          },
          {
            "stringId": 1811423298689449988,
            "femaleVariant": "さっさとどいて",
            "maleVariant": "さっさとどけ"
          },
          {
            "stringId": 1811425393811406848,
            "femaleVariant": "悪いけど、どいてもらえる？",
            "maleVariant": "悪いが、どいてもらえるか？"
          },
          {
            "stringId": 1811426227622264832,
            "femaleVariant": "お父上が見たら何と言うか…"
          },
          {
            "stringId": 1811427691048484864,
            "femaleVariant": "聞こえなかったの！？"
          },
          {
            "stringId": 1811428200488648704,
            "femaleVariant": "ごちゃごちゃ言ってないで、さっさとどいて",
            "maleVariant": "ごちゃごちゃ言ってないで、さっさとどけ"
          },
          {
            "stringId": 1811428300732514308,
            "femaleVariant": "悪いんだけど、どいてもらえる？",
            "maleVariant": "悪いが、どいてもらえるか？"
          },
          {
            "stringId": 1811430041838436352,
            "femaleVariant": "一体何なんだ…"
          },
          {
            "stringId": 1811434432335200256,
            "femaleVariant": "[座る] ありがとう",
            "maleVariant": "[座る] どうも"
          },
          {
            "stringId": 1812536846438121472,
            "femaleVariant": "私を一体何だと思って…"
          },
          {
            "stringId": 1815589611446599680,
            "femaleVariant": "主を守れ！"
          },
          {
            "stringId": 1816945237225504768,
            "femaleVariant": "先に始めていろとのことだけど…"
          },
          {
            "stringId": 1816945798020726784,
            "femaleVariant": "ハナコ！ どうしてここに？"
          },
          {
            "stringId": 1816947693426704384,
            "femaleVariant": "何を… です？"
          },
          {
            "stringId": 1816959946733518848,
            "femaleVariant": "ハナコ様。ご説明ください"
          },
          {
            "stringId": 1816975677135126528,
            "femaleVariant": "席を空けなさい、早く"
          },
          {
            "stringId": 1816977436226224128,
            "femaleVariant": "それに、何なのですか、この… 連中は"
          },
          {
            "stringId": 1825551335476510720,
            "femaleVariant": "失礼"
          },
          {
            "stringId": 1837123952670806016,
            "femaleVariant": "知ってたの？",
            "maleVariant": "知ってたのか？"
          },
          {
            "stringId": 1837124158661464064,
            "femaleVariant": "何のことだ？"
          },
          {
            "stringId": 1837124208305246208,
            "femaleVariant": "サブロウよ",
            "maleVariant": "サブロウだよ"
          },
          {
            "stringId": 1837127671642722304,
            "femaleVariant": "彼のコンストラクトと話した",
            "maleVariant": "奴のコンストラクトと話した"
          },
          {
            "stringId": 1837130183661719552,
            "femaleVariant": "まさか、サブロウ様はそんなこと一度も…"
          },
          {
            "stringId": 1837130817957924864,
            "femaleVariant": "気分はどう、タケムラ？",
            "maleVariant": "気分はどうだ、タケムラ？"
          },
          {
            "stringId": 1837131082903719936,
            "femaleVariant": "ナイトシティはいつだってあたしたちを驚かせてくれる。言葉も出ないほどにね",
            "maleVariant": "ナイトシティはいつだって俺たちを驚かせてくれる。言葉も出ないほどにな"
          },
          {
            "stringId": 1837131762313859072,
            "femaleVariant": "なんだと…！？"
          },
          {
            "stringId": 1837132297792262144,
            "femaleVariant": "死んだ時に備えて、記憶痕跡のデジタルコピーを作ってたのよ",
            "maleVariant": "死んだ時に備えて、記憶痕跡のデジタルコピーを作ってたんだ"
          },
          {
            "stringId": 1837136196263612416,
            "femaleVariant": "ボディガードに教えたら、仕事が雑になるとでも思ったんじゃない？",
            "maleVariant": "ボディガードに教えたら、仕事が雑になるとでも思ったんじゃないか？"
          },
          {
            "stringId": 1837140337132179456,
            "femaleVariant": "ありえん…"
          },
          {
            "stringId": 1837156410191646720,
            "femaleVariant": "そうなったら、あたしが連中に“対処”しろってこと？",
            "maleVariant": "そうなったら、俺が連中に“対処”しろってことか？"
          },
          {
            "stringId": 1837177179026100224,
            "femaleVariant": "面白い"
          },
          {
            "stringId": 1837179497805123584,
            "femaleVariant": "デタラメにも程が…"
          },
          {
            "stringId": 1837192778481537024,
            "femaleVariant": "まったく… 証拠もなしに、どうやって信じろと言うんですか？"
          },
          {
            "stringId": 1837194905530535936,
            "femaleVariant": "ヨリノブは、飼い犬たちをよく躾けてるようね",
            "maleVariant": "ヨリノブは、飼い犬たちをよく躾けてるようだな"
          },
          {
            "stringId": 1837195050166915072,
            "femaleVariant": "サブロウ・アラサカが殺されたっていうのに、あんたたちは死因の調査もしなかった",
            "maleVariant": "サブロウ・アラサカが殺されたっていうのに、あんたらは死因の調査すらしなかった"
          },
          {
            "stringId": 1837301592165847040,
            "femaleVariant": "持ちこたえられる？"
          },
          {
            "stringId": 1837304529940254720,
            "femaleVariant": "少し落ち着いて…"
          },
          {
            "stringId": 1837304977908699136,
            "femaleVariant": "説教なら全て終わってからにして！",
            "maleVariant": "説教なら全て終わってからにしろ！"
          },
          {
            "stringId": 1837306949600677888,
            "femaleVariant": "ええ、そうね"
          },
          {
            "stringId": 1841477113854431232,
            "femaleVariant": "はい、ハナコ様"
          },
          {
            "stringId": 1841525616517451776,
            "femaleVariant": "移住はなしだ。1万2千人ぐらい、誰も気づきやしない"
          },
          {
            "stringId": 1843127674853003264,
            "femaleVariant": "あ… アラサカ様？"
          },
          {
            "stringId": 1843132983399026688,
            "femaleVariant": "いずれにせよ、仮にも役員である者たちが、なんと情けない"
          },
          {
            "stringId": 1843133783890333696,
            "femaleVariant": "こんなものに我々が騙されるとでも？"
          },
          {
            "stringId": 1843134920429277184,
            "femaleVariant": "バヘット。お前がミリテクとの全面戦争を憂慮していた時、私はこう言ったはずだ"
          },
          {
            "stringId": 1843136731294863360,
            "femaleVariant": "トリックだろう！"
          },
          {
            "stringId": 1843144370431070208,
            "femaleVariant": "お前には3つのものが欠けている。自信、知性、そして根性だ"
          },
          {
            "stringId": 1843151640200089600,
            "femaleVariant": "見たところ、あれから何も変わっていないようだな"
          },
          {
            "stringId": 1843153757384085504,
            "femaleVariant": "ほ… 本物だ…"
          },
          {
            "stringId": 1843154157889785856,
            "femaleVariant": "アラサカ… 様…"
          },
          {
            "stringId": 1843157165960728576,
            "femaleVariant": "赤子のようにまんまと騙されたか、日和見主義に走ったか…"
          },
          {
            "stringId": 1843188214396751872,
            "femaleVariant": "お爺様… 如何いたしましょう？"
          },
          {
            "stringId": 1843190186021621760,
            "femaleVariant": "ハナコの指示に従え"
          },
          {
            "stringId": 1843212599610445824,
            "femaleVariant": "異議のある人は？"
          },
          {
            "stringId": 1845868980276695040,
            "femaleVariant": "貴様…"
          },
          {
            "stringId": 1845869530049286144,
            "femaleVariant": "Vは味方よ"
          },
          {
            "stringId": 1845871355410386944,
            "femaleVariant": "私に？ それとも金に？"
          },
          {
            "stringId": 1845880640576032768,
            "femaleVariant": "これ以上茶番につきあってられるか。警備を呼べ"
          },
          {
            "stringId": 1845882236408041472,
            "femaleVariant": "いい考えね。ヨリノブのとこに送って、紺碧で何があったのか問いただしてもらいましょ",
            "maleVariant": "いい考えだ。ヨリノブのとこに送って、紺碧で何があったのか問いただしてもらおう"
          },
          {
            "stringId": 1845885426243342336,
            "femaleVariant": "でも武器は持たせるのよ。あいつは怒ると人を絞め殺すから",
            "maleVariant": "だが武器は持たせろよ。あいつは怒ると人を絞め殺すからな"
          },
          {
            "stringId": 1845889650041688064,
            "femaleVariant": "何を見たんだね"
          },
          {
            "stringId": 1845889901028839424,
            "femaleVariant": "ヨリノブがサブロウを絞め殺す瞬間を"
          },
          {
            "stringId": 1845890535610257408,
            "femaleVariant": "ますます面白くなってきた"
          },
          {
            "stringId": 1845893248402198528,
            "femaleVariant": "あんたたちは騙されてるのよ",
            "maleVariant": "あんたらは騙されてるんだよ"
          },
          {
            "stringId": 1845894798096863232,
            "femaleVariant": "だから、お前は何者なんだ？"
          },
          {
            "stringId": 1845998954392457216,
            "femaleVariant": "なかなか面白い話ね… だけど、どうせ証拠はないんでしょ"
          },
          {
            "stringId": 1863401380156088320,
            "femaleVariant": "周囲で怪しい動きがあれば直ちに報告しろ"
          },
          {
            "stringId": 1863407712498163712,
            "femaleVariant": "たった… これだけ？"
          },
          {
            "stringId": 1863407943470096384,
            "femaleVariant": "いえ、残りはすでに配置につかせています"
          },
          {
            "stringId": 1863409061923201024,
            "femaleVariant": "ここにいるのは、特に忠実な者たちです"
          },
          {
            "stringId": 1863410370260836352,
            "femaleVariant": "そうですね――比率は2対8といったところでしょうか"
          },
          {
            "stringId": 1863411138456002560,
            "femaleVariant": "いいわ。それなら問題ない"
          },
          {
            "stringId": 1863412863774564352,
            "femaleVariant": "行きましょう"
          },
          {
            "stringId": 1865012892738068480,
            "femaleVariant": "ハナコの発する言葉は、すべて私の言葉同然だ"
          },
          {
            "stringId": 1865019346614296576,
            "femaleVariant": "クソ！ ハナコ、これはお前の仕業だな？"
          },
          {
            "stringId": 1865022177148739584,
            "femaleVariant": "檻の中のネズミのようにここに閉じ込めて、共食いでもさせようというのか！？"
          },
          {
            "stringId": 1865022845267812352,
            "femaleVariant": "こいつの他にも、ゴロツキどもを連れてきたんだな！"
          },
          {
            "stringId": 1865023541572608000,
            "femaleVariant": "今のは？"
          },
          {
            "stringId": 1865026292784373760,
            "femaleVariant": "ハナコ、いったいどうなってる！？"
          },
          {
            "stringId": 1865029326943887360,
            "femaleVariant": "何が起きてるの！？"
          },
          {
            "stringId": 1865029933725458432,
            "femaleVariant": "どうやら納得していない人がいるようね"
          },
          {
            "stringId": 1865030265780117504,
            "femaleVariant": "ロックダウンされてる！"
          },
          {
            "stringId": 1865038608200441856,
            "femaleVariant": "ぐはあっ！"
          },
          {
            "stringId": 1866466116045185024,
            "femaleVariant": "ヨリノブをアラサカ社CEOの座から引きずり下ろすのです"
          },
          {
            "stringId": 1866473600847781888,
            "femaleVariant": "は？ 今、何と？"
          },
          {
            "stringId": 1872075979106361344,
            "femaleVariant": "ここの元社員よ。アバナシーの部署にいた",
            "maleVariant": "ここの元社員だ。アバナシーの部署にいた"
          },
          {
            "stringId": 1872076665529376768,
            "femaleVariant": "CEOが死んだにもかかわらず、あんたたちがやってるのは責任の放棄と転嫁――いつからそんな会社になったの？",
            "maleVariant": "CEOが死んだにもかかわらず、あんたらがやってるのは責任の放棄と転嫁――いつからそんな会社になったんだ？"
          },
          {
            "stringId": 1872078813432455168,
            "femaleVariant": "事件の目撃者がここにいるのよ？ 証言を聞いて、ヨリノブの逮捕に乗り出すのが筋でしょ",
            "maleVariant": "事件の目撃者がここにいるんだ。証言を聞いて、ヨリノブの逮捕に乗り出すのが筋だろ"
          },
          {
            "stringId": 1905582051571023872,
            "femaleVariant": "役員はあんた側？",
            "maleVariant": "役員はあんた側か？"
          },
          {
            "stringId": 1905582686051778560,
            "femaleVariant": "ある意味ではそうね"
          },
          {
            "stringId": 1914104974943535108,
            "femaleVariant": "V？ 席について"
          },
          {
            "stringId": 1914104975060975620,
            "femaleVariant": "座ってちょうだい"
          },
          {
            "stringId": 1914104975178416128,
            "femaleVariant": "座ろうが立っていようが、とんだ茶番だな"
          },
          {
            "stringId": 2000974106692755456,
            "femaleVariant": "馬鹿げてる"
          },
          {
            "stringId": 2007155863423098880,
            "femaleVariant": "あなたたちは皆、そのことを知っていた"
          },
          {
            "stringId": 2025682974039416832,
            "femaleVariant": "どういうこと？",
            "maleVariant": "どういうことだ？"
          },
          {
            "stringId": 2025683231888449536,
            "femaleVariant": "私と私の金、どちらに忠誠を尽くすつもりでいるのかはわからない"
          },
          {
            "stringId": 2025683680225992704,
            "femaleVariant": "どっちもそう変わらないんじゃない？",
            "maleVariant": "どっちもそう変わらないだろ？"
          },
          {
            "stringId": 2025720647395385344,
            "femaleVariant": "動機はどうあれ、重要なのはあたしたちと一緒に戦ってくれるかどうかね",
            "maleVariant": "動機はどうあれ、重要なのは俺たちと一緒に戦ってくれるかどうかだな"
          },
          {
            "stringId": 2025739414993182720,
            "femaleVariant": "はっきり言ってくれるのね"
          },
          {
            "stringId": 2025740170622214144,
            "femaleVariant": "だからあたしがいるんでしょ",
            "maleVariant": "だから俺がいるんだろ"
          },
          {
            "stringId": 2025748002562187264,
            "femaleVariant": "それはあくまで最後の手段よ"
          },
          {
            "stringId": 2025753643615854592,
            "femaleVariant": "役員会が敵に回るならそうするしかない"
          },
          {
            "stringId": 2025770605951655936,
            "femaleVariant": "“もし”そうなったらね"
          },
          {
            "stringId": 2025797265853849600,
            "femaleVariant": "もし彼らの説得に失敗したら、我々は窮地に立たされる"
          },
          {
            "stringId": 2085340488626421764,
            "femaleVariant": "久しぶりね",
            "maleVariant": "久しぶりだな"
          },
          {
            "stringId": 2257441266453553152,
            "femaleVariant": "何者なの？ それに… どういうことよ… 武器を持ってるじゃない"
          },
