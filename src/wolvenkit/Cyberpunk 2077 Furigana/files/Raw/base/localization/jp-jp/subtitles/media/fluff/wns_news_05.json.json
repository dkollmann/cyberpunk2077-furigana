  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こんにちは、アリフ・イクバルです。WNSニュースの時間です",
                    "maleVariant": null,
                    "stringId": 1793558880455663616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まずは“あの街”の今に迫ります。2020年代初頭のミリテク・アラサカ紛争にて、生物兵器による未曾有かつ甚大な被害を受けた結果、アジアの大都市・釜山からはあらゆる生命が消し去られました",
                    "maleVariant": null,
                    "stringId": 1796608806356762624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人々に加え、動植物までもが完全に死滅した釜山ですが、現在においてもなお、朝鮮半島南端とその周辺海域に近づくのは、自殺行為に等しいとされています",
                    "maleVariant": null,
                    "stringId": 1796608975085223936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しかしそれは生物に限った話。潮力発電を動力とする自動システムは、野放しで今も稼働を続けています",
                    "maleVariant": null,
                    "stringId": 1796609214512873472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "釜山に存在するAIについて、ネットウォッチは「放置を続けると脅威になりかねない」と警告しています",
                    "maleVariant": null,
                    "stringId": 1796609541752471552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカ社はこの環境危機について、二次被害を含めた全責任は爆弾を投下したミリテク社にあると結論づけています。なお新合衆国はこの件についてのコメントを控えています",
                    "maleVariant": null,
                    "stringId": 1796609825253867520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヨーロッパでは、ベネチアにおける不法採掘および略奪の撲滅対策に、パダニア政府が再び乗り出しました",
                    "maleVariant": null,
                    "stringId": 1796610379673747456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最近では略奪犯4名と国家憲兵隊7名が亡くなっています",
                    "maleVariant": null,
                    "stringId": 1796610586369048576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アドリア海に沈むこの街からは、何世紀も前の貴重な芸術品が数多く見つかっていますが、収集家が略奪行為に拍車をかけています",
                    "maleVariant": null,
                    "stringId": 1796610828497829888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "公式発表によると、ブラックマーケットで売買されたベネチアの芸術作品類の総額は、パダニアのGDPの3分の1に相当すると見られています",
                    "maleVariant": null,
                    "stringId": 1796611064335155200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "盗品の多くは軌道ステーションに住む個人収集家のもとに流れているため、取り戻すのが困難というわけです",
                    "maleVariant": null,
                    "stringId": 1796611265628192768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "続きまして、世界初の軍隊の完全自動化を2080年までに実現すると、スイスが発表しました",
                    "maleVariant": null,
                    "stringId": 1796611610718748672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "スイス軍は既に9割がロボトマイズ兵士、ドローン、自動化車両で構成されており、残りを人間の士官が占めている形です",
                    "maleVariant": null,
                    "stringId": 1796611801442140160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "スイス政府はこの割合を10割に引き上げるため、CVS-パーケム自動司令システムの導入を開始する予定です",
                    "maleVariant": null,
                    "stringId": 1796612023857692672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "以下、政府の公式声明です。「スイスはついに、真の中立性を獲得することになります。CVS-パーケムは我が国を攻撃者の立場にすることなく、守ってくれるでしょう」",
                    "maleVariant": null,
                    "stringId": 1796612235653267456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "世界の最新ニュースをお届けしました。それではまた",
                    "maleVariant": null,
                    "stringId": 1796612502192898048
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}