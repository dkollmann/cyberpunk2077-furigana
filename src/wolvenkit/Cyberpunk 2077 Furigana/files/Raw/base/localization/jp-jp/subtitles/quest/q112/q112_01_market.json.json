  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、来たか",
              "maleVariant": "",
              "stringId": "1643116584492875776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何故この場所に？",
              "maleVariant": "",
              "stringId": "1643117143157391364"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サブロウ様をパレードで弔いたいと提案したのは、誰だと思う？",
              "maleVariant": "",
              "stringId": "1643118453508292608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それがどうしたの？",
              "maleVariant": "それがどうしたんだ？",
              "stringId": "1643121473541390336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "伝統を重んじるタイプには思えないけど",
              "maleVariant": "伝統を重んじるタイプには思えないが",
              "stringId": "1643122058495803392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブの狙いは何？",
              "maleVariant": "ヨリノブの狙いは何だ？",
              "stringId": "1643122915090120704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "式典がヨリノブの正統性を確かなものにする",
              "maleVariant": "",
              "stringId": "1643123905851834368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど、ヨリノブは賢い。パレードを通して、アラサカや、その価値観との結びつきをアピールするのね",
              "maleVariant": "なるほど、ヨリノブは賢い。パレードを通して、アラサカや、その価値観との結びつきをアピールするわけか",
              "stringId": "1643125303024828416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、どこまでも抜け目のない男だ",
              "maleVariant": "",
              "stringId": "1643127550299357184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "日本の伝統を利用することで、ヨリノブは自身の権力の強大さを示そうとしている",
              "maleVariant": "",
              "stringId": "1643127703458562048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "山車はきっと壮観だろう。しかし見る奴が見れば、そこに敵対者への声明を読み取ることができるはずだ",
              "maleVariant": "",
              "stringId": "1643128638721245184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも、それがどうしたっていうの？",
              "maleVariant": "だが、それがどうしたっていうんだ？",
              "stringId": "1644748867821912064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いくつかこちらで新たな情報をつかんだ",
              "maleVariant": "",
              "stringId": "1698600601962692608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺にいい考えがある",
              "maleVariant": "",
              "stringId": "1711266717562322944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "空を見るんだ",
              "maleVariant": "",
              "stringId": "1711563643113455616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前と別れた後、こちらは式典で使われる山車について調べてみた",
              "maleVariant": "",
              "stringId": "1711566497521876992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウイルス入りのチップがある",
              "maleVariant": "",
              "stringId": "1711566497555431424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを受け取る]",
              "maleVariant": "",
              "stringId": "1711566497605763072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、方法は？",
              "maleVariant": "",
              "stringId": "1711566501028315136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッキングは得意じゃない",
              "maleVariant": "",
              "stringId": "1711566501028315152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、方法は？",
              "maleVariant": "",
              "stringId": "1711566504215986176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正直なところ、ハッキングは得意じゃないんだけど",
              "maleVariant": "正直なところ、ハッキングは得意じゃないんだが",
              "stringId": "1711566504249540608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺もサッパリだが――",
              "maleVariant": "",
              "stringId": "1711566504249540616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "山車は全て一ヶ所に保管されている。アラサカ工業団地だ。準備もそこで進められる",
              "maleVariant": "",
              "stringId": "1711569540120330240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "歩きながら話そう",
              "maleVariant": "",
              "stringId": "1711571853195100160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "問題は、山車の乗っ取りだけじゃない。当然ながら警備も障害となる… 特に狙撃手だな。何も手を打たないと、俺は蜂の巣にされてしまう",
              "maleVariant": "",
              "stringId": "1711572544248627200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが、オカダさんが監視カメラの話をしてただろう",
              "maleVariant": "",
              "stringId": "1711573935180496896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "伝統を重んじるタイプには思えないけど",
              "maleVariant": "伝統を重んじるタイプには思えないが",
              "stringId": "1714341936410529792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのとおりだ",
              "maleVariant": "",
              "stringId": "1714343242885894144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「これが私の力だ、愚民ども。背中に気をつけろ」",
              "maleVariant": "",
              "stringId": "1714347411420090368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "声明というと？",
              "maleVariant": "",
              "stringId": "1714349377978241024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そして、自らの正統性を",
              "maleVariant": "",
              "stringId": "1714354294189068288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "会えて嬉しいよ、ゴロウ",
              "maleVariant": "会えて嬉しいぞ、ゴロウ",
              "stringId": "1714358578536386560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゴロウ。また会えて嬉しいよ",
              "maleVariant": "ゴロウ。また会えて嬉しいぞ",
              "stringId": "1714359452428013568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、何故この場所に？",
              "maleVariant": "",
              "stringId": "1714359614143598592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、俺もだ、V",
              "maleVariant": "",
              "stringId": "1714360581282992128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それなら可能かもしれない",
              "maleVariant": "",
              "stringId": "1719999005323038720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それだったら色々とやり方はある。必要なのは――",
              "maleVariant": "",
              "stringId": "1719999561370308608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "必要なものはわかってる。手配済みだ",
              "maleVariant": "",
              "stringId": "1720009300359647232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "高くはついたが、チップを買った仲介人によると性能は確かとのことだ",
              "maleVariant": "",
              "stringId": "1720013574405754880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなことができるの？",
              "maleVariant": "そんなことができるのか？",
              "stringId": "1724371748889956352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "群衆の前よ？ どうかしてる",
              "maleVariant": "群衆の前だぞ？ どうかしてる",
              "stringId": "1724371748889956368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "不可能よ。やりようがない",
              "maleVariant": "不可能だ。やりようがない",
              "stringId": "1724371748889956384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなことができるの？",
              "maleVariant": "そんなことができるのか？",
              "stringId": "1724373624230383616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうかしてる。群衆の前よ？ 警備の目もあるのに…",
              "maleVariant": "どうかしてる。群衆の前だぞ？ 警備の目もあるってのに…",
              "stringId": "1724373701741121536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "机上の空論ね。仮にあんたが最高のインプラントで装備を固めてたとしても無理よ",
              "maleVariant": "机上の空論だな。仮にあんたが最高のインプラントで装備を固めてたとしても無理だ",
              "stringId": "1724373802756739072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "施設に侵入してハナコ様がお乗りになる山車を見つけ出し、そのシステムにウイルスを仕掛ければいい",
              "maleVariant": "",
              "stringId": "1724380572011073536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうすれば、俺が乗り込む直前にお前がセキュリティを切ることができるだろう",
              "maleVariant": "",
              "stringId": "1724386493898559488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、俺の話を聞け",
              "maleVariant": "",
              "stringId": "1724388168130498560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心配無用だ",
              "maleVariant": "",
              "stringId": "1724388318437576704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話は読めた。汚れ仕事はあたしの役目ってわけね",
              "maleVariant": "話は読めた。汚れ仕事は俺の役目ってわけか",
              "stringId": "1724405553470246912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なら配置を交代して、お前がハナコ様の山車に飛び込み、「初めまして、Vです」とでも挨拶するか？",
              "maleVariant": "",
              "stringId": "1724410364940914688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうはいくまい。素性が知れている俺が行くしかないのだ",
              "maleVariant": "",
              "stringId": "1724418373981179904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "単刀直入ね",
              "maleVariant": "単刀直入だな",
              "stringId": "1725524809985429504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これは一体どういうことだ。アラサカには十分な人員がいないのか？",
              "maleVariant": "",
              "stringId": "1731660491478888448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかりませんか、街はいま一触即発の状況です",
              "maleVariant": "",
              "stringId": "1731661366947573760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "邪魔して悪いんだけど…",
              "maleVariant": "邪魔して悪いんだが…",
              "stringId": "1732780378098319360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少しお友達を借りさせて",
              "maleVariant": "少しお友達を借りたいんだが",
              "stringId": "1732781618672779280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "邪魔して悪いんだけど…",
              "maleVariant": "邪魔して悪いんだが…",
              "stringId": "1732782147775840256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほんとお喋りが達者な奴でしょ、でもそいつに用事があって",
              "maleVariant": "ほんとお喋りが達者な奴だろ、だがそいつに用事があるんだ",
              "stringId": "1732782508787974144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待ってたぞ",
              "maleVariant": "",
              "stringId": "1732783915305877504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい…",
              "maleVariant": "",
              "stringId": "1732786405564825600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいか、山車は確実にこの経路を通る",
              "maleVariant": "",
              "stringId": "1732801942743236632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんとかしてハナコ様の山車に乗ることができれば、直接話ができるかもしれない",
              "maleVariant": "",
              "stringId": "1732801942743236644"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもどうやって？",
              "maleVariant": "だがどうやって？",
              "stringId": "1763553732555837440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこで手に入れたの？",
              "maleVariant": "どこで手に入れたんだ？",
              "stringId": "1763555229637152768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこで手に入れたの？",
              "maleVariant": "どこで手に入れたんだ？",
              "stringId": "1763556256486658048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "運河の向こうだ",
              "maleVariant": "",
              "stringId": "1763556749871026176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カブキ地区？ ならそれなりにまともかもね",
              "maleVariant": "カブキ地区？ ならそれなりにまともかもな",
              "stringId": "1763560242719625216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "使い物にならなかったとしても、あたしが何とかできる",
              "maleVariant": "使い物にならなかったとしても、俺が何とかできる",
              "stringId": "1763563642941214720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "暴動を鎮圧するなら、名前を出さずにやるのが一番ですから",
              "maleVariant": "",
              "stringId": "1770473777101934592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカも下手に刺激して、ジャパンタウンを炎に包みたくないんですよ",
              "maleVariant": "",
              "stringId": "1770492434590863360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "当然、跳び乗るしかなかろう",
              "maleVariant": "",
              "stringId": "1770501671874334720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フッ、すぐにわかる",
              "maleVariant": "",
              "stringId": "1772240340670877696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "決まりね",
              "maleVariant": "決まりだな",
              "stringId": "1780946454967095296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜあたしが汚れ仕事を？",
              "maleVariant": "なぜ俺が汚れ仕事を？",
              "stringId": "1780946454967095316"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "決まりね",
              "maleVariant": "決まりだな",
              "stringId": "1780950257556656128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "構わないのか？ お前に危険な役目を押し付けることになるが",
              "maleVariant": "",
              "stringId": "1783722654030499840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別に。今に始まったことじゃないから",
              "maleVariant": "別に。今に始まったことじゃないからな",
              "stringId": "1834426652978712576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手をかけた張本人さ。とんだ皮肉だな",
              "maleVariant": "",
              "stringId": "1871963463881662464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうしてくれると助かる",
              "maleVariant": "",
              "stringId": "1996584541047562240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし… 早速下準備といこう。チップの動作確認も兼ねてな",
              "maleVariant": "",
              "stringId": "1996671649260257280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "目の前に例の監視カメラ制御室がある",
              "maleVariant": "",
              "stringId": "1996728020454359040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ネットワークに侵入し、チップのウイルスを仕掛けるんだ",
              "maleVariant": "",
              "stringId": "1996728467550388224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいけど… その間あんたはどうするの？ ビールでも飲んで一休み？",
              "maleVariant": "で… その間あんたはどうするんだ？ ビールでも飲んで一休みか？",
              "stringId": "2005630399353266176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕事中は酒を一滴も飲まん主義でね。俺は入口を見張ってる。さあ、かかれ",
              "maleVariant": "",
              "stringId": "2005631121343979520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それにアクセスすれば、狙撃手の位置を手に取るように特定できる。お前が排除に当たり、俺が位置を変えるという作戦だ",
              "maleVariant": "",
              "stringId": "2005638444749758464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[待つ]",
              "maleVariant": "",
              "stringId": "2014108158846320640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "2069428144805330948"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}