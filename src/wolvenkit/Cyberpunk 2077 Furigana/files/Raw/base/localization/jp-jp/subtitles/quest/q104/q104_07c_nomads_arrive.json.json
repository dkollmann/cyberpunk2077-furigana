    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1530133159572402176,
            "femaleVariant": "ミッチがアルデカルドスを何人か集めてくれたんだ"
          },
          {
            "stringId": 1530151565738635264,
            "femaleVariant": "誰にも捕捉されることなく、ここまで来れた。だが、長居はしたくないな"
          },
          {
            "stringId": 1560376755230875652,
            "femaleVariant": "[ヘルマンをバイクにもたせかける]"
          },
          {
            "stringId": 1641948515690893316,
            "femaleVariant": "いくつか質問をする"
          },
          {
            "stringId": 1641953883494043648,
            "femaleVariant": "まずは、いくつか質問に答えてもらう"
          },
          {
            "stringId": 1641954543341948928,
            "femaleVariant": "知らない方がいい"
          },
          {
            "stringId": 1641954791141429248,
            "femaleVariant": "ソウル"
          },
          {
            "stringId": 1641956783150616576,
            "femaleVariant": "説明すると長くなる"
          },
          {
            "stringId": 1641957218620035072,
            "femaleVariant": "悪いことは言わない。知らない方がいい"
          },
          {
            "stringId": 1641957680463237120,
            "femaleVariant": "複雑な話なの。説明すると長くなる",
            "maleVariant": "複雑な話だ。説明すると長くなる"
          },
          {
            "stringId": 1641960289068666880,
            "femaleVariant": "よほど重要な情報を握ってるんだろうね"
          },
          {
            "stringId": 1641961300181143552,
            "femaleVariant": "なら今度聞かせて"
          },
          {
            "stringId": 1641968681384431616,
            "femaleVariant": "片付いた。制圧完了だ"
          },
          {
            "stringId": 1641969249276416000,
            "femaleVariant": "ミッチを助けてくれたと聞いた"
          },
          {
            "stringId": 1641969529204264960,
            "femaleVariant": "例の“重要”人物だな"
          },
          {
            "stringId": 1641970360548872196,
            "femaleVariant": "協力してくれてありがとう"
          },
          {
            "stringId": 1641970686832168960,
            "femaleVariant": "ソウル… 私はスコーピオンを…"
          },
          {
            "stringId": 1641971489638731776,
            "femaleVariant": "あれはパナムのせいじゃない"
          },
          {
            "stringId": 1641971930929844224,
            "femaleVariant": "ソウル、パナムは何も悪くないの",
            "maleVariant": "ソウル、パナムは何も悪くないんだ"
          },
          {
            "stringId": 1641972056507305984,
            "femaleVariant": "協力してくれてありがとう"
          },
          {
            "stringId": 1641973953171574784,
            "femaleVariant": "互いに恩を返しただけだ"
          },
          {
            "stringId": 1641974972337115136,
            "femaleVariant": "よそ者が口を挟むな。第一、責めるつもりも、咎めるつもりもない"
          },
          {
            "stringId": 1672053616975564800,
            "femaleVariant": "こいつがヘルマン？"
          },
          {
            "stringId": 1672065560642080768,
            "femaleVariant": "こいつはどうするの？"
          },
          {
            "stringId": 1699343313190645760,
            "femaleVariant": "ええ",
            "maleVariant": "そうだ"
          },
          {
            "stringId": 1699343616086503424,
            "femaleVariant": "外に運んでバイクに乗せよう"
          },
          {
            "stringId": 1699441584290525184,
            "femaleVariant": "パナムと一緒にな。2人のおかげで死なずに済んだ"
          },
          {
            "stringId": 1699455307650560000,
            "femaleVariant": "お前はもう何もするな"
          },
          {
            "stringId": 1699456676704940032,
            "femaleVariant": "…何か、できることはない？"
          },
          {
            "stringId": 1699463914748243968,
            "femaleVariant": "全部私が悪いの！？"
          },
          {
            "stringId": 1699469142461640704,
            "femaleVariant": "ハッ、何も変わってないのね… そうやって背を向けることしかできないのかよ！"
          },
          {
            "stringId": 1699476316986404864,
            "femaleVariant": "気をつけて帰りな"
          },
          {
            "stringId": 1699480277248241664,
            "femaleVariant": "よく言うよ！"
          },
          {
            "stringId": 1699480576134344704,
            "femaleVariant": "ソウル、聞いて。パナムはあたしに手を貸してくれただけ。あたしがいなければ、あのAVが墜落することも、スコーピオンたちが追うこともなかった",
            "maleVariant": "ソウル、聞いてくれ。パナムは俺に手を貸してくれただけだ。俺がいなければ、あのAVが墜落することも、スコーピオンたちが追うこともなかった"
          },
          {
            "stringId": 1699482743649988608,
            "femaleVariant": "アルデカルドスは、自分のケツは自分で拭き、仲間のケツは全力で守る。それが揺らぐことはない"
          },
          {
            "stringId": 1699485095127805952,
            "femaleVariant": "だが、クランの一員としての自覚がないなら関係のないことだ"
          },
          {
            "stringId": 1699501212428791812,
            "femaleVariant": "じゃあね、パナム",
            "maleVariant": "じゃあな、パナム"
          },
          {
            "stringId": 1699501915629023232,
            "femaleVariant": "これからどうするの？",
            "maleVariant": "これからどうするんだ？"
          },
          {
            "stringId": 1699503221299081216,
            "femaleVariant": "じゃあ行くね。またいつか",
            "maleVariant": "じゃあ行くよ。またいつか"
          },
          {
            "stringId": 1699503332481691648,
            "femaleVariant": "この後は… ナイトシティに戻るつもり？",
            "maleVariant": "この後は… ナイトシティに戻るつもりか？"
          },
          {
            "stringId": 1699504372115435520,
            "femaleVariant": "うん… まあ、たぶんね"
          },
          {
            "stringId": 1699507374683451392,
            "femaleVariant": "ソウルと話すべき",
            "maleVariant": "ソウルと話すべきだ"
          },
          {
            "stringId": 1699509146877554688,
            "femaleVariant": "ソウルもミッチも、あんたを家族だと思ってる"
          },
          {
            "stringId": 1699514487048630272,
            "femaleVariant": "やり直せないの？",
            "maleVariant": "やり直せないのか？"
          },
          {
            "stringId": 1699515196674535424,
            "femaleVariant": "いろいろ複雑なの"
          },
          {
            "stringId": 1699578414096838656,
            "femaleVariant": "ひとまず落ち着いて… 色々考えないと"
          },
          {
            "stringId": 1699580646691303424,
            "femaleVariant": "それが家族よ",
            "maleVariant": "それが家族だ"
          },
          {
            "stringId": 1699719625675763712,
            "femaleVariant": "私も行かなきゃ。スコーピオンのバイクを頼んだよ。あんたのもんだ"
          },
          {
            "stringId": 1730146699619987456,
            "femaleVariant": "奴らだ！ 増援が来るぞ！"
          },
          {
            "stringId": 1730148615594831872,
            "femaleVariant": "奴らだ！ 増援が来るぞ！"
          },
          {
            "stringId": 1730149146610495488,
            "femaleVariant": "またね。スコーピオンのバイクを頼んだよ。そいつはあんたのモンだ"
          },
          {
            "stringId": 1730150361935245312,
            "femaleVariant": "ずらかろう！"
          },
          {
            "stringId": 1730151544426983424,
            "femaleVariant": "ずらかろう！"
          },
          {
            "stringId": 1761829411262951428,
            "femaleVariant": "どうしてここに？"
          },
          {
            "stringId": 1761830911229947908,
            "femaleVariant": "ソウルもいつかわかってくれる"
          },
          {
            "stringId": 1761831662178136076,
            "femaleVariant": "ミッチ、会えて嬉しいけど… どうしてここに？",
            "maleVariant": "ミッチ、会えて嬉しいが… なぜここに？"
          },
          {
            "stringId": 1761831662178136088,
            "femaleVariant": "ソウルだよ。仲間の遺体を集めたあと、経緯を伝えたら、すぐに動ける奴を招集してな"
          },
          {
            "stringId": 1761831662178136100,
            "femaleVariant": "そしてここに向かったってわけだ。心配してたのさ――お前と、パナムのことを"
          },
          {
            "stringId": 1761831662178136112,
            "femaleVariant": "ソウルが？"
          },
          {
            "stringId": 1761831662194913280,
            "femaleVariant": "たとえ関係がギクシャクしてても、家族には違いねえ。ソウルは家族のためなら何でもする"
          },
          {
            "stringId": 1761831662194913292,
            "femaleVariant": "でも、今の私は家族かどうかも怪しいのに…"
          },
          {
            "stringId": 1761831662194913304,
            "femaleVariant": "わかってんだろ"
          },
          {
            "stringId": 1761831662194913316,
            "femaleVariant": "戻って来いよ"
          },
          {
            "stringId": 1761831662211690512,
            "femaleVariant": "そうか"
          },
          {
            "stringId": 1761834655837450240,
            "femaleVariant": "ソウルなら心配ない。きっとわかってくれる"
          },
          {
            "stringId": 1761834655837450252,
            "femaleVariant": "どうでもいいよ"
          },
          {
            "stringId": 1761834655854227476,
            "femaleVariant": "そう言ってやるなよ、パナム"
          },
          {
            "stringId": 1761834655854227488,
            "femaleVariant": "今回のことをソウルに伝えたら、すぐにお前を追うよう指示したんだぜ。心配してたんだ"
          },
          {
            "stringId": 1761834655854227500,
            "femaleVariant": "ソウルが？ ホントに…？"
          },
          {
            "stringId": 1761834655854227512,
            "femaleVariant": "たとえ関係がギクシャクしてても、家族には違いねえ。ソウルは家族のためなら何でもする"
          },
          {
            "stringId": 1761834655871004676,
            "femaleVariant": "でも、今の私は家族かどうかも怪しいのに…"
          },
          {
            "stringId": 1761834655871004688,
            "femaleVariant": "わかってんだろ"
          },
          {
            "stringId": 1761834655871004700,
            "femaleVariant": "戻って来いよ"
          },
          {
            "stringId": 1761834655871004712,
            "femaleVariant": "..."
          },
          {
            "stringId": 1761834655871004724,
            "femaleVariant": "そうか"
          },
          {
            "stringId": 1770694735939743744,
            "femaleVariant": "あんたはここ",
            "maleVariant": "お前はここだ"
          },
          {
            "stringId": 1915491918382260224,
            "femaleVariant": "なに驚いてんの？ 暑さで頭をやられちゃった？ あんたと違って私は逃げたりしない… 約束だからね"
          },
          {
            "stringId": 1915494027446083584,
            "femaleVariant": "墜落現場でミッチを助けることはできたけど、スコーピオンは… ダメだった"
          },
          {
            "stringId": 1955868560841400320,
            "femaleVariant": "協力してくれてありがとう"
          },
          {
            "stringId": 1955873013095096320,
            "femaleVariant": "会えて嬉しいけど… どうしてここに？",
            "maleVariant": "会えて嬉しいが… なぜここに？"
          },
          {
            "stringId": 1985053416122609664,
            "femaleVariant": "よし、残党はいないか？"
          },
          {
            "stringId": 1985054071943983104,
            "femaleVariant": "確認済みだ"
          },
          {
            "stringId": 1985057030824128512,
            "femaleVariant": "気をつけて帰れよ"
          },
          {
            "stringId": 1985066796355022848,
            "femaleVariant": "パナムがあんたへの加勢を決めたんだ。ただ2人だけじゃ、カン・タオに返り討ちにあうだろうとソウルが心配してな"
          },
          {
            "stringId": 1985069677154004992,
            "femaleVariant": "ソウルが？"
          },
          {
            "stringId": 1985069973087318016,
            "femaleVariant": "たとえ関係がギクシャクしてても、家族には違いねえ。ソウルは家族のためなら何でもする"
          },
          {
            "stringId": 1985070380471676928,
            "femaleVariant": "でも、今の私は家族かどうかも怪しいのに…"
          },
          {
            "stringId": 1985070703466639360,
            "femaleVariant": "わかってんだろ"
          },
          {
            "stringId": 1985071023324262400,
            "femaleVariant": "戻って来いよ"
          },
          {
            "stringId": 1985071582760529920,
            "femaleVariant": "そうか"
          },
          {
            "stringId": 1985076939205505024,
            "femaleVariant": "お前がVの後を追ったって聞いて、すぐに追いかけるよう指示したんだぜ。何かあったらと心配してたんだ"
          },
          {
            "stringId": 1985085991872491520,
            "femaleVariant": "パナム？"
          },
          {
            "stringId": 1985088609671835648,
            "femaleVariant": "残念ね",
            "maleVariant": "残念だ"
          },
          {
            "stringId": 1985088882653917184,
            "femaleVariant": "ああ、そうだね"
          },
          {
            "stringId": 1985088970717523968,
            "femaleVariant": "そいつが例の奴？ ほら、バイクに乗せなよ"
          },
          {
            "stringId": 1985110388159148060,
            "femaleVariant": "じゃあ行くね。またいつか",
            "maleVariant": "じゃあ行くよ。またいつか"
          },
          {
            "stringId": 1985110390524735488,
            "femaleVariant": "奴らだ！ 増援が来るぞ！"
          },
          {
            "stringId": 1985110392756105216,
            "femaleVariant": "ずらかろう！"
          },
          {
            "stringId": 1985110395004252160,
            "femaleVariant": "じゃあ、またね"
          },
          {
            "stringId": 1985111307282149428,
            "femaleVariant": "ソウルもミッチも、あんたを家族だと思ってる"
          },
          {
            "stringId": 1985111309463187456,
            "femaleVariant": "やり直せないの？",
            "maleVariant": "やり直せないのか？"
          },
          {
            "stringId": 1985111311778443264,
            "femaleVariant": "いろいろ複雑なの"
          },
          {
            "stringId": 1985111314227916800,
            "femaleVariant": "それが家族よ",
            "maleVariant": "それが家族だ"
          },
          {
            "stringId": 1985111316442509312,
            "femaleVariant": "奴らだ！ 増援が来るぞ！"
          },
          {
            "stringId": 1985111318774542336,
            "femaleVariant": "ずらかろう！"
          },
          {
            "stringId": 1985111321257570304,
            "femaleVariant": "じゃ、私も行くよ"
          },
          {
            "stringId": 2150701112294309888,
            "femaleVariant": "何なのよ…！"
          },
          {
            "stringId": 2228731043811356672,
            "femaleVariant": "偵察態勢は整ってる"
          },
          {
            "stringId": 2228732904555606016,
            "femaleVariant": "今のところ問題なしだ"
          },
          {
            "stringId": 2228735767486853120,
            "femaleVariant": "よし。連中が来たら知らせろ"
          },
          {
            "stringId": 2230116999491678208,
            "femaleVariant": "なんか… 恥ずかしいところを見せちゃったね。なんというか、ややこしいことになっててさ"
          },
          {
            "stringId": 2230121768163667968,
            "femaleVariant": "でも心配しないで。私は大丈夫だから… Vも気をつけてね"
          },
          {
            "stringId": 2230286950475915264,
            "femaleVariant": "仲間の死体は現場から回収した。キャンプに持ち帰ってやらないと"
          },
          {
            "stringId": 2230288155818221568,
            "femaleVariant": "例の男… ミッチとパナムはあいつを助けにきたの？"
          },
          {
            "stringId": 2231361892591529984,
            "femaleVariant": "急いだほうがいい。これ以上ここに長居したら、俺たちも危ないぞ"
          },
          {
            "stringId": 2231363429032194048,
            "femaleVariant": "ふん… 喧嘩を売られたら、買うまでだ"
          },
          {
            "stringId": 2231364126259736576,
            "femaleVariant": "血の気の多い奴だな…"
          },
          {
            "stringId": 2231370699656851456,
            "femaleVariant": "そうらしい。AVの墜落とスコーピオンのことも… 無関係ってわけじゃなさそうだ"
          },
          {
            "stringId": 2231372757365616640,
            "femaleVariant": "仕事を受けたなんて聞いてなかったが… ソウルは企業とやりあう気なのか？"
          },
          {
            "stringId": 2231373791009894400,
            "femaleVariant": "正直、何が何だかわからない"
          },
          {
            "stringId": 2231374719544913920,
            "femaleVariant": "ミッチだ。あいつに話を聞こう"
          }
        ]
      }
    }
  }
}