    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[写真を見せる] この男はあんた？",
              "maleVariant": "[写真を見せる] この男はあんたか？",
              "stringId": 1851853768162398212
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この写真の男って、あんた？",
              "maleVariant": "この写真の男、あんたか？",
              "stringId": 1853304404003377152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え… これをどこで手に入れたんだ！？",
              "maleVariant": "",
              "stringId": 1853304721545744384
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何か当てはあるの？",
              "maleVariant": "何か当てはあるか？",
              "stringId": 1853306301623947264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョーシーのボロボロのバイクと一緒に見つけたの。彼女に何があったのか調べてる",
              "maleVariant": "ジョーシーのボロボロのバイクと一緒に見つけたんだ。彼女に何があったのか調べてる",
              "stringId": 1853306569690304512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや。連中に追われて、地下鉄で南に逃げたってことしか… ジャパンタウン方面からのレッドラインだ。だがどこで降りたかはわからない",
              "maleVariant": "",
              "stringId": 1853310760605306880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ… 手掛かりがないよりマシね",
              "maleVariant": "まあ… 手掛かりがないよりマシだな",
              "stringId": 1853311442330701824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョーシーの遺体を見つけた。あんたのせいよ",
              "maleVariant": "ジョーシーの遺体を見つけた。お前のせいだ",
              "stringId": 1853311655988547584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョーシーは死んだ。あんたのせいよ",
              "maleVariant": "ジョーシーは死んだ。お前のせいでな",
              "stringId": 1853313414156898312
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何か新しい情報はある？",
              "maleVariant": "何か新しい情報はあるか？",
              "stringId": 1889614415180451840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョーシーの新しい情報は？",
              "maleVariant": "",
              "stringId": 1889614583841804288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョーシーのバイクを見つけた",
              "maleVariant": "",
              "stringId": 1889615162068553728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女が盗んだバイクを見つけた",
              "maleVariant": "",
              "stringId": 1889615449311268864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ならあんた警官か？ さっさとバッジを見せろよ",
              "maleVariant": "",
              "stringId": 1922827693297291264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは一般市民よ。ただ事実を明らかにしようとしてるだけ",
              "maleVariant": "俺は一般市民だ。ただ事実を明らかにしようとしてるだけさ",
              "stringId": 1922834420323373056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしあたしが警官だったら、あんたは今頃地面で手錠をかけられてるでしょ",
              "maleVariant": "もし俺が警官だったら、あんたは今頃地面で手錠をかけられてるだろ",
              "stringId": 1922835043630501888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いったい何言ってやがる！？",
              "maleVariant": "",
              "stringId": 1922848143029096448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってるくせに。罠にかけたのね。仲間が彼女を始末するのを知ってて",
              "maleVariant": "わかってるくせに。罠にかけたんだろ。仲間が彼女を始末するのを知っててな",
              "stringId": 1922848374957330432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕方なかったんだ。あの女はボスを裏切ったんだぞ。他に選択肢はなかった。でなきゃ俺が始末されてたんだ！",
              "maleVariant": "",
              "stringId": 1922849486414336000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまり選択肢はあったのね",
              "maleVariant": "つまり選択肢はあったんだな",
              "stringId": 1922852086815707140
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "情けないクズね",
              "maleVariant": "情けないクズだな",
              "stringId": 1922852240495005696
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしあんたが単に彼女をだましてたのなら… 利用するために好きなふりしてただけなら、この街にあふれるクズの一人で済んでた",
              "maleVariant": "もしお前が単に彼女をだましてたのなら… 利用するために好きなふりしてただけなら、この街にあふれるクズの一人ですんでた",
              "stringId": 1922852780587143168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ自分の命を選んだだけで、選択肢はあったわけだ",
              "maleVariant": "",
              "stringId": 1922854307078918144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前ならどうしたんだ？ 自分が死ぬのを選んだのか？",
              "maleVariant": "",
              "stringId": 1922855367113756672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてここに毎晩戻ってきてると思う？ 景色を楽しむためじゃない。自分のしたことはわかってるし、責任を感じてる",
              "maleVariant": "",
              "stringId": 1922856970042527744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "くだらない感傷ね",
              "maleVariant": "くだらない感傷だ",
              "stringId": 1922859631848509440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまり、あんたはクズ以下の存在ってことよ、ジェイミー",
              "maleVariant": "つまり、お前はクズ以下の存在ってことだよ、ジェイミー",
              "stringId": 1922863355501268992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "失血死の辛さはわかる？ ジェイミー。彼女はゆっくり、苦しみながら死んでいった。爪がはがれるほどアスファルトを引っかいてたのよ。その情けない顔を鏡で見るたびにそれを想像することね",
              "maleVariant": "失血死の辛さはわかるか、ジェイミー。彼女はゆっくり、苦しみながら死んでいった。爪がはがれるほどアスファルトを引っかいてたんだ。その情けない顔を鏡で見るたびにそれを想像するんだな",
              "stringId": 1922865316086730752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うるせえ、黙れ！ てめえなんか殺してやる！",
              "maleVariant": "",
              "stringId": 1922873611950280704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも、あんたは彼女を本気で好きだったんでしょ。なのに彼女を差し出した",
              "maleVariant": "だが、お前は彼女を本気で好きだったんだろ。なのに彼女を差し出した",
              "stringId": 1922876932865970176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やれるもんなら、やってみるといい",
              "maleVariant": "やれるもんなら、やってみろよ",
              "stringId": 1922881996632412160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女に何か、ひどいことが起きたのね？",
              "maleVariant": "彼女に何か、ひどいことがあったんだな？",
              "stringId": 1922885206583595008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ… クロウズの奴らはあいつを気に入らなかったんだ。街を出ろって言ったのに、ジョーシーは… それを無視して",
              "maleVariant": "",
              "stringId": 1922887311000129536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつは自分のバイクを壊された時、仕返しするって言ったから止めたんだ… でもボスのバイクを盗んで、追いかけられて、銃撃戦になった――その後はわからない",
              "maleVariant": "",
              "stringId": 1922889518965641216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから確かに、何かひどいことが起きたんだろう。何かわかったら知らせてくれ。何でもいい。いいだろ？",
              "maleVariant": "",
              "stringId": 1922893744156495872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また後で",
              "maleVariant": "",
              "stringId": 1922896534895382532
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこを探せばいいかわかる？",
              "maleVariant": "どこを探せばいいかわかるか？",
              "stringId": 1922896718018695168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ後で",
              "maleVariant": "",
              "stringId": 1922898398139437056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何も。そっちもダメそうか？",
              "maleVariant": "",
              "stringId": 1922907825995304960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで… ジョーシーは？",
              "maleVariant": "",
              "stringId": 1922909599665803264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだわからない。探してるとこ",
              "maleVariant": "まだわからない。探してるとこだ",
              "stringId": 1922910099693948928
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}