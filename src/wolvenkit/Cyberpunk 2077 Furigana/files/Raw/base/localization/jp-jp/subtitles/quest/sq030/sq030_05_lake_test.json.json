    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1395348872451047424,
            "femaleVariant": "フンフンフン…"
          },
          {
            "stringId": 1395421141030387712,
            "femaleVariant": "よし"
          },
          {
            "stringId": 1395429570155364352,
            "femaleVariant": "うん、シグナルは良好。あとは基本的なチェックをいくつか手伝って"
          },
          {
            "stringId": 1395439771843543040,
            "femaleVariant": "次は… 音声のチェック"
          },
          {
            "stringId": 1395441654498836480,
            "femaleVariant": "その曲なら知ってる"
          },
          {
            "stringId": 1395444193243938816,
            "femaleVariant": "曲名はわからないけど、こんな感じだった"
          },
          {
            "stringId": 1395448308678246400,
            "femaleVariant": "それ知ってる！ エッタ・ソレンティーノの「Only You」"
          },
          {
            "stringId": 1395448447006392320,
            "femaleVariant": "曲名はわからないんだけど、ちゃんと聞こえたよ。こんな感じ…",
            "maleVariant": "曲名はわからないが、ちゃんと聞こえたぞ。こんな感じ…"
          },
          {
            "stringId": 1395452313131270144,
            "femaleVariant": "まずはモーションセンサーと記録機能"
          },
          {
            "stringId": 1395476029554802688,
            "femaleVariant": "モーションの調整は完了っと。私の方に来て",
            "maleVariant": "モーションの調整は完了っと。戻ってきて、V"
          },
          {
            "stringId": 1395489315063054336,
            "femaleVariant": "感傷的な曲は趣味じゃないんだけど… なんだか近頃、頭を離れなくて"
          },
          {
            "stringId": 1395490370769371136,
            "femaleVariant": "うん。設定は問題なさそうね"
          },
          {
            "stringId": 1396911122395676672,
            "femaleVariant": "信じられない"
          },
          {
            "stringId": 1396911768805031936,
            "femaleVariant": "湖の底に街があるなんて"
          },
          {
            "stringId": 1396913168863707136,
            "femaleVariant": "あそこを探索する気？",
            "maleVariant": "あそこを探索する気か？"
          },
          {
            "stringId": 1396915296667680784,
            "femaleVariant": "信じられない… 一体どういうこと？",
            "maleVariant": "信じられない… 一体どういうことだ？"
          },
          {
            "stringId": 1396915609092997120,
            "femaleVariant": "ウソでしょ… こんなところに街が沈んでるなんて…",
            "maleVariant": "ウソだろ… こんなところに街が沈んでるなんて…"
          },
          {
            "stringId": 1396917884184150016,
            "femaleVariant": "すごいでしょ？"
          },
          {
            "stringId": 1396918688366444544,
            "femaleVariant": "気に入った？"
          },
          {
            "stringId": 1397900115623002112,
            "femaleVariant": "眺めはどう？",
            "maleVariant": "ちょっと、どこ見てんの"
          },
          {
            "stringId": 1404236571471450112,
            "femaleVariant": "こっちまで泳いで、私を真っすぐ見て"
          },
          {
            "stringId": 1404259944045940736,
            "femaleVariant": "フンフンフン…"
          },
          {
            "stringId": 1405226566315347968,
            "femaleVariant": "私たちだけのアトランティス"
          },
          {
            "stringId": 1405230930891313152,
            "femaleVariant": "なんて言ったらいいか… 今まで見たこともない光景なのは間違いない"
          },
          {
            "stringId": 1741558539680202752,
            "femaleVariant": "よし、こんなとこね"
          },
          {
            "stringId": 1764996410860617728,
            "femaleVariant": "設定は大丈夫よ",
            "maleVariant": "設定は大丈夫だ"
          },
          {
            "stringId": 1782099299114115072,
            "femaleVariant": "キャリブレーションをするから"
          },
          {
            "stringId": 1782103737459499008,
            "femaleVariant": "私の周りを泳いでみて"
          },
          {
            "stringId": 1782108887678935040,
            "femaleVariant": "ちゃんと… 聞こえてるけど",
            "maleVariant": "ちゃんと… 聞こえてるぞ"
          },
          {
            "stringId": 1782109106067955712,
            "femaleVariant": "範囲を広げるの。レベルは完璧に取らないと"
          },
          {
            "stringId": 1782299025361035268,
            "femaleVariant": "ええ、いい眺め",
            "maleVariant": "いい眺めだ"
          },
          {
            "stringId": 1782300205017423872,
            "femaleVariant": "あ、ごめん",
            "maleVariant": "ああ、すまん"
          },
          {
            "stringId": 1782300311837958144,
            "femaleVariant": "待って、どこを見てるかわかるの？",
            "maleVariant": "待て、どこを見てるかわかるのか？"
          },
          {
            "stringId": 1782302290811248640,
            "femaleVariant": "視覚センサーが刺激を受けてるのは、否定できないね",
            "maleVariant": "視覚センサーが刺激を受けてるのは、否定できないな"
          },
          {
            "stringId": 1782308983997685760,
            "femaleVariant": "ああ… ごめん",
            "maleVariant": "ああ… すまん"
          },
          {
            "stringId": 1782309491240034304,
            "femaleVariant": "待って。同じものが見えるの？ リアルタイムで？",
            "maleVariant": "待て。同じものが見えるのか？ リアルタイムで？"
          },
          {
            "stringId": 1782318042503143424,
            "femaleVariant": "あんまり興奮しないで。酸素の量は限られてるよ",
            "maleVariant": "今のは聞かなかったことにしとく"
          },
          {
            "stringId": 1782334331686707200,
            "femaleVariant": "謝る必要はないよ",
            "maleVariant": "一回だけなら許してあげる"
          },
          {
            "stringId": 1782338317668016128,
            "femaleVariant": "ええ。全部お見通しよ",
            "maleVariant": "ええ。正直言って… ちょっと変な感じね"
          },
          {
            "stringId": 1795274115472121856,
            "femaleVariant": "そんなにいいお尻してたんだ",
            "maleVariant": "そんなにいいケツしてたとは… 気づかなかったよ"
          },
          {
            "stringId": 1835925363543990272,
            "femaleVariant": "えっと、周りを観察するんだったよね",
            "maleVariant": "えっと、周りを観察するんだったよな"
          },
          {
            "stringId": 1835926310668165120,
            "femaleVariant": "集中できるといいんだけど",
            "maleVariant": "そう、景色をね"
          },
          {
            "stringId": 1855014318310645760,
            "femaleVariant": "んー… どこがダメなんだろ。単純に設定の問題？"
          },
          {
            "stringId": 1857415626962448384,
            "femaleVariant": "わかった。ジュディがそうしたいなら、付き合うよ"
          },
          {
            "stringId": 1857833897181274112,
            "femaleVariant": "大きすぎるよ！",
            "maleVariant": "大きすぎるって！"
          },
          {
            "stringId": 1857834701095133184,
            "femaleVariant": "待って。レベルが大きすぎるか。これで… いいはず"
          },
          {
            "stringId": 1862100099638452224,
            "femaleVariant": "これで良くなった？"
          },
          {
            "stringId": 1862101175477428224,
            "femaleVariant": "ええ、ばっちり",
            "maleVariant": "ああ、ばっちりだ"
          },
          {
            "stringId": 1862129844048584704,
            "femaleVariant": "いい？"
          },
          {
            "stringId": 1863285596914147328,
            "femaleVariant": "設定は大丈夫よ",
            "maleVariant": "設定は大丈夫だ"
          },
          {
            "stringId": 1863285603759251456,
            "femaleVariant": "え？ ああ… 思った以上の感度ね。少し下げないと"
          },
          {
            "stringId": 1866398991413620736,
            "femaleVariant": "うわ、びっくりした！"
          },
          {
            "stringId": 1867766968559034368,
            "femaleVariant": "ハミングするから、タイトルを答えて"
          },
          {
            "stringId": 1900935126233104388,
            "femaleVariant": "何かゆかりのある場所なの？",
            "maleVariant": "何かゆかりのある場所なのか？"
          },
          {
            "stringId": 1900939029653733376,
            "femaleVariant": "あそこを探索する気？",
            "maleVariant": "あそこを探索する気か？"
          },
          {
            "stringId": 1900939610464174080,
            "femaleVariant": "ラグーナ・ベンドと呼ばれてた場所よ。ほんの15年前までは人が住んでた"
          },
          {
            "stringId": 1900940395621105664,
            "femaleVariant": "ここと何かゆかりがある、ってこと？",
            "maleVariant": "ここと何かゆかりがある、ってことか？"
          },
          {
            "stringId": 1900940529218076672,
            "femaleVariant": "何があったの？ なぜ水の底に？",
            "maleVariant": "何があったんだ？ なぜ水の底に？"
          },
          {
            "stringId": 1900940777118220288,
            "femaleVariant": "始めましょ",
            "maleVariant": "始めよう"
          },
          {
            "stringId": 1900943117103644672,
            "femaleVariant": "最高じゃない。やりましょ",
            "maleVariant": "面白そうだ。始めよう"
          },
          {
            "stringId": 1900943312558211072,
            "femaleVariant": "そうか… 思い出した"
          },
          {
            "stringId": 1900986615257350144,
            "femaleVariant": "この街にいったい何が？"
          },
          {
            "stringId": 1900990045862477824,
            "femaleVariant": "どうしてあたしにここを？",
            "maleVariant": "どうして俺にここを？"
          },
          {
            "stringId": 1900999202363654144,
            "femaleVariant": "そうか… 思い出した"
          },
          {
            "stringId": 1901002420267237376,
            "femaleVariant": "どうしてあたしにここを？",
            "maleVariant": "どうして俺にここを？"
          },
          {
            "stringId": 1901002630888407040,
            "femaleVariant": "うってつけ？ いったいどいういう意味？",
            "maleVariant": "うってつけ？ いったいどいういう意味だ？"
          },
          {
            "stringId": 1901002699289116672,
            "femaleVariant": "うってつけって、どういう意味？",
            "maleVariant": "うってつけって、どういう意味だ？"
          },
          {
            "stringId": 1901008045550321664,
            "femaleVariant": "金儲けの引き替えよ"
          },
          {
            "stringId": 1901008846863712256,
            "femaleVariant": "NCダム社がダムの建造計画を立てて、住民が反対した。そこから先は有名な話ね",
            "maleVariant": "NCダム社がダムの建造計画を立てて、住民が反対した。そこから先は有名な話だ"
          },
          {
            "stringId": 1901009424335486976,
            "femaleVariant": "正解。私はここで育ったの"
          },
          {
            "stringId": 1901010057054633984,
            "femaleVariant": "ラグーナ・ベンドが地図から消えて以来、初めて来た"
          },
          {
            "stringId": 1901014844953313280,
            "femaleVariant": "私の知られざる過去と向き合う覚悟はいい？"
          },
          {
            "stringId": 1901028030385471488,
            "femaleVariant": "大切な場所だから"
          },
          {
            "stringId": 1901028314742505472,
            "femaleVariant": "ここなら私の実験にうってつけなの"
          },
          {
            "stringId": 1901029829892882432,
            "femaleVariant": "これから私を包み込む感情の渦を、ありのままの形で捉えてほしいの"
          },
          {
            "stringId": 1901034579589840900,
            "femaleVariant": "[手すりを握る] 潜ろう！"
          },
          {
            "stringId": 1901035122768986112,
            "femaleVariant": "出発！",
            "maleVariant": "出発だ！"
          },
          {
            "stringId": 1902600120401879040,
            "femaleVariant": "そう。あなたがだけどね"
          },
          {
            "stringId": 1902746591470247936,
            "femaleVariant": "家も公園も、何もかもが水の底に沈んだ"
          },
          {
            "stringId": 1902819286023102464,
            "femaleVariant": "これを掴んで離さないで"
          },
          {
            "stringId": 1902831822143447040,
            "femaleVariant": "あなたはその感情に嫌でも反応することになる… それを記録したいのよ"
          },
          {
            "stringId": 1966402443716288516,
            "femaleVariant": "よし、こんなとこね。これで同期…"
          },
          {
            "stringId": 1966402443733065748,
            "femaleVariant": "完了"
          },
          {
            "stringId": 1966404144221347872,
            "femaleVariant": "次は？"
          },
          {
            "stringId": 1966404144221347888,
            "femaleVariant": "じゃあいよいよサプライズの時間ね。行くよ…"
          },
          {
            "stringId": 1966409164769681408,
            "femaleVariant": "音楽の才能もありそうじゃない？"
          },
          {
            "stringId": 2010042641268039680,
            "femaleVariant": "一人で行き過ぎないで。スーツが水圧に耐えられない"
          },
          {
            "stringId": 2010042680577056768,
            "femaleVariant": "V、本気よ！ 深く潜りすぎたら、命の保証はできない！"
          },
          {
            "stringId": 2066599565160296448,
            "femaleVariant": "ちゃんと聞こえてる"
          }
        ]
      }
    }
  }
}