    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "運転手が要る",
              "maleVariant": "",
              "stringId": "1264586597559455744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのアザ、どうした？",
              "maleVariant": "",
              "stringId": "1264590844644577280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "練習してるか？",
              "maleVariant": "",
              "stringId": "1264590964316459008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのアザ、どうした？ 転んだわけでもないだろ？",
              "maleVariant": "",
              "stringId": "1264591528634896384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちゃんとコードの練習してんのか？",
              "maleVariant": "",
              "stringId": "1264591651947433984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "車と運転手を探してるんだ。今日は街でいろいろと野暮用があってな",
              "maleVariant": "",
              "stringId": "1264592423145721856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やあ！",
              "maleVariant": "",
              "stringId": "1264594654750330880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "えーっと…",
              "maleVariant": "",
              "stringId": "1264598509919686656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいよ、任せといて",
              "maleVariant": "",
              "stringId": "1264604419173928960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "出かける時は声をかけろって言うんだけど",
              "maleVariant": "",
              "stringId": "1264605653725368320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行こう",
              "maleVariant": "",
              "stringId": "1264606516644696064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うん。でも指が追い付かなくて",
              "maleVariant": "",
              "stringId": "1314083384664481792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全然上達しないんだ",
              "maleVariant": "",
              "stringId": "1314085190429794304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのうち上手くなる",
              "maleVariant": "",
              "stringId": "1314087623327440896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "技術より心だ",
              "maleVariant": "",
              "stringId": "1314091938142183424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく練習しろ。そのうち上手くなる",
              "maleVariant": "",
              "stringId": "1314092658169962496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なにもリンダ・オズを目指せって言ってるわけじゃない",
              "maleVariant": "",
              "stringId": "1314092770057216000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "親の隙を見て練習してるけど… あんまりチャンスがなくてさ",
              "maleVariant": "",
              "stringId": "1314094322385903616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分でも下手だってわかってる",
              "maleVariant": "",
              "stringId": "1314094893851435008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頑張ってるけど…",
              "maleVariant": "",
              "stringId": "1314101950667251712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "親父か？",
              "maleVariant": "",
              "stringId": "1314134841359032320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いずれ何も考えずに弾けるようになるさ。コツさえ掴めばな",
              "maleVariant": "",
              "stringId": "1314139910510960640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。ある程度は譜面通りに弾かなきゃならんが、それより心を込めろ",
              "maleVariant": "",
              "stringId": "1314143242331840512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よう、元気か",
              "maleVariant": "",
              "stringId": "1314147183417319424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今日はどうしたの？",
              "maleVariant": "",
              "stringId": "1314151985257865216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈Curse These Metal Hands〉の新盤を買ったんだけどさ、最初の4曲がもう最高だよ",
              "maleVariant": "",
              "stringId": "1314152753201373184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "初めて聞いた時は鳥肌立った。でも… 後半はイマイチなんだよね",
              "maleVariant": "",
              "stringId": "1314152753218150400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で？ 何か用があるんだろ？",
              "maleVariant": "",
              "stringId": "1314152753251704832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その… 起こすの忘れて、仕事に遅れさせちゃってさ。俺が悪いんだ…",
              "maleVariant": "",
              "stringId": "1314214705906802688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっと酔いが残ってて、虫の居所が悪かったんだ",
              "maleVariant": "",
              "stringId": "1316758917898526768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前のせいじゃない",
              "maleVariant": "",
              "stringId": "1316761269829328896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "たかが朝寝坊をしたくらいで、手を出すとはな",
              "maleVariant": "",
              "stringId": "1316766731048017920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うん…",
              "maleVariant": "",
              "stringId": "1316768043865169920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "後から謝ってくれたしね",
              "maleVariant": "",
              "stringId": "1316772680987009024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、何しに来たの？",
              "maleVariant": "",
              "stringId": "1316791474572713984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう一曲練習してたんだ… 聞く？",
              "maleVariant": "",
              "stringId": "1719850304828825600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今は急いでる。また今度な",
              "maleVariant": "",
              "stringId": "1719932576101785600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいか、観客が求めてるのは魂のこもった音楽だ。技術なんておざなりでいい",
              "maleVariant": "",
              "stringId": "1760583057622654976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうなの？",
              "maleVariant": "",
              "stringId": "1760583256030011392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうせ母さんは何も聞いちゃいないから",
              "maleVariant": "",
              "stringId": "1760615184481345536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰？ 何か用？",
              "maleVariant": "",
              "stringId": "1802751272594505728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺だ",
              "maleVariant": "",
              "stringId": "1802752227788521476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リチャード・ナイトだ",
              "maleVariant": "",
              "stringId": "1802752509628973056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着け。俺だ、スティーブ",
              "maleVariant": "",
              "stringId": "1802753005127270400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リチャード・ナイトだ。この街を作った奴だぞ、さっさと開けろ",
              "maleVariant": "",
              "stringId": "1802753087906054144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "目覚まし時計をやるよ",
              "maleVariant": "",
              "stringId": "1805495973840642048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うちの目覚まし時計を持ってけ。鍵は開いてる",
              "maleVariant": "",
              "stringId": "1805498175430811648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "使わないの？",
              "maleVariant": "",
              "stringId": "1805548132779122688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラームは好きじゃないんだよ",
              "maleVariant": "",
              "stringId": "1805556128481501184"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}