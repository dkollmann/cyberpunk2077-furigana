  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここは立入禁止だ。誰も通すなよ",
                    "maleVariant": null,
                    "stringId": 1711179099325886464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "了解です、ボス。お任せを",
                    "maleVariant": null,
                    "stringId": 1711184213306535936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "てめえ… いったい誰だ？ どうやって入った？",
                    "maleVariant": null,
                    "stringId": 1711293200534368256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "説明するから落ち着いて",
                    "maleVariant": "説明するから落ち着け",
                    "stringId": 1711293941231677444
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいえ。ナイトシティから姿を消して。あんたは眉間を撃ち抜かれたと言っておけば、みんな鵜呑みにする。新しい人生をやり直せばいい",
                    "maleVariant": "いいや。ナイトシティから姿を消すんだ。あんたは眉間を撃ち抜かれたと言っておけば、みんな鵜呑みにする。新しい人生をやり直せばいい",
                    "stringId": 1711294126905126912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティから消えて",
                    "maleVariant": "ナイトシティから消えろ",
                    "stringId": 1711307551731380228
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やり直せだと？ 俺はここでいろんなもんを手にしてきた。クラブに、手下に、女たち…",
                    "maleVariant": null,
                    "stringId": 1711312302720184320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今のはイエス、それともノー？",
                    "maleVariant": "今のはイエスか、それともノーか？",
                    "stringId": 1711312500406120448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「仕事は片付いた」って報告しに行け",
                    "maleVariant": null,
                    "stringId": 1711320821016064000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "名案ね。手間が省ける",
                    "maleVariant": "名案だ。手間が省ける",
                    "stringId": 1712622413690466304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大勢の敵もね",
                    "maleVariant": "大勢の敵もな",
                    "stringId": 1712629786169602048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "選べる立場じゃないの。いつ死んでもおかしくないのよ",
                    "maleVariant": "選べる立場じゃない。いつ死んでもおかしくないんだ",
                    "stringId": 1712630146309320704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クソったれ！ どうしてこんなわけわかんねえことになっちまんだ！",
                    "maleVariant": null,
                    "stringId": 1712638082200924160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "静かにして。でなきゃ黙らせるだけよ",
                    "maleVariant": "静かにしろ。でなきゃ黙らせるだけだ",
                    "stringId": 1924295667317264384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なるほどな。俺も賞金首になったってわけか。誰の差し金だ？ ミリテクか？ アスカガか？ それともディノの野郎か？",
                    "maleVariant": null,
                    "stringId": 1924297598760701952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあ、どっちでもいい。てめえの頭からメモリーチップを引っこ抜けばわかることだ！",
                    "maleVariant": null,
                    "stringId": 1924303495297814528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やってごらん",
                    "maleVariant": "やってみろよ",
                    "stringId": 1924307833382555648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうぞ、やってみなよ",
                    "maleVariant": "やれよ、できるもんならな",
                    "stringId": 1924309216328470528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "座って、ドージー。こっちに話し合う気がなきゃ、あんたはとっくに死んでてもおかしくないのよ",
                    "maleVariant": "座れよ、ドージー。こっちに話し合う気がなきゃ、あんたはとっくに死んでてもおかしくないんだ",
                    "stringId": 1924311313379807232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふん。穏便にやりたきゃ失せやがれ。おしゃべりは嫌いだ。さっさとケリをつけようぜ。俺とお前でな、かかって来いよ！",
                    "maleVariant": null,
                    "stringId": 1924315765214072832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたが勝って、あたしを叩きのめしたとする。それで、抱えてる問題は全部解決するの？",
                    "maleVariant": "あんたが勝って、俺を叩きのめしたとしよう。それで抱えてる問題は全部解決するのか？",
                    "stringId": 1924317758129229824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "相当いろんな連中の怒りを買ってるでしょ。自分でも言ってたけど、ミリテク、アスカガ、ディノ… あんたが死ぬまで連中はあきらめない",
                    "maleVariant": "相当いろんな連中の怒りを買ってるだろ。自分でも言ってたが、ミリテク、アスカガ、ディノ… あんたが死ぬまで連中はあきらめない",
                    "stringId": 1924318550248706048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だから何だ？ 自分で脳天撃ち抜けってか？",
                    "maleVariant": null,
                    "stringId": 1924321128336379904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうしてくれると手間が省ける",
                    "maleVariant": "そうしてくれると弾を節約できる",
                    "stringId": 1924322847984242688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "面白えじゃねえか",
                    "maleVariant": null,
                    "stringId": 1924323344505950208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "頼んだぞ。奥さんの調子は？ インプラントの手術後、どうだ？",
                    "maleVariant": null,
                    "stringId": 1925697050385895424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だいぶ回復してます。いつもすんません、ボス",
                    "maleVariant": null,
                    "stringId": 1925717032553467904
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
