    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティの皆さん、こんにちは！ やってきました！ 本日のゲストはクロームの体と黄金の声を持つあの伝説のシンガー。お迎えしましょう。リズィー・ウィズィー！！",
              "maleVariant": "",
              "stringId": 1792221869786439680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハーイ、ジギー。元気にしてたかしら？",
              "maleVariant": "",
              "stringId": 1795458996903854080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪くはないよ。だって、今からこの星で一番ホットなスターと話ができるんだから",
              "maleVariant": "",
              "stringId": 1795459261463773184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "褒め過ぎよ…",
              "maleVariant": "",
              "stringId": 1795459507216433152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "間違っちゃないだろ。さて… リズィー、みんなが知りたいのはやっぱり、ニューアルバムのFuck All Wallsだ。そこで質問なんだけど、どこの壁を、どうして… その… アレしてほしいのかな…",
              "maleVariant": "",
              "stringId": 1795459681162608640
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね、“壁”は色んな解釈ができるって、言えばいいかしら",
              "maleVariant": "",
              "stringId": 1795459959530176512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもジャケットにはカリフォルニアを南北で分ける実際の壁が描かれてるよね。待って、画像を用意するとしよう",
              "maleVariant": "",
              "stringId": 1795460145388175360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう。私にとって… 壁ってのは、社会にできた亀裂だと思ってる。思いやりの喪失ね…",
              "maleVariant": "",
              "stringId": 1795460530492391424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど。壁の向こう側にあるものは見えないし、見えないならそれを気にする必要もないって、ことだね",
              "maleVariant": "",
              "stringId": 1795460751548989440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのとおり。昔カリフォルニアは一つの州だった。アメリカが一つの国だったようにね。考え方は人それぞれ違ったかもしれないけど、みんな何かで… 繋がってたと思うの",
              "maleVariant": "",
              "stringId": 1795460938900160512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも今は？ みんな自分のことばっかりで、誰もが周りから孤立してる。他人との関係を、優劣でしか見られなくなってるの",
              "maleVariant": "",
              "stringId": 1795461140780400640
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの壁の周りでは、毎日多くの人が死んでる。自分や家族のために、より良い暮らしを求めただけなのに",
              "maleVariant": "",
              "stringId": 1795461332980187136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも、国境を守る権利はどの国にもあるとは思わないかい？",
              "maleVariant": "",
              "stringId": 1795461795343482880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それには大好きな引用で答えさせてもらうわ。「以下の事実を自明のことと信じる。すなわちこの世のすべての人間は、生まれながらにして平等であり、その創造主によって…」",
              "maleVariant": "",
              "stringId": 1795462003380961280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「…生命、自由、幸福の追求を含む不可侵の権利を与えられている」",
              "maleVariant": "",
              "stringId": 1795463664862216192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから歌詞にした。「すべての壁を破れ」って",
              "maleVariant": "",
              "stringId": 1795464004969938944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "皆さんも信じることのために立ち上がり、世の中を変えたくなってきたんじゃ？",
              "maleVariant": "",
              "stringId": 1795464329256747008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今日リズィー・ウィズィーのニューアルバムを購入すると、ボーナストラック『Born Equal』が付いてきます！",
              "maleVariant": "",
              "stringId": 1795464534995746816
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ファンのみんな、本当にいいアルバムなのよ！ みんなで一緒に声を上げましょ！",
              "maleVariant": "",
              "stringId": 1795465068376997888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それではまた次回！ いくぞ、Fuck All Walls！",
              "maleVariant": "",
              "stringId": 1795465342046945280
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
