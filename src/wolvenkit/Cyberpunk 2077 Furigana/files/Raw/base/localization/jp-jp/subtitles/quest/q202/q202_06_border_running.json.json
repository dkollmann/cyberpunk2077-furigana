  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドローンが来るぞ！",
              "maleVariant": "",
              "stringId": 1561979228634337280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっば、ふと思い出しちゃった",
              "maleVariant": "",
              "stringId": 1660199261822672896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いよいよだね",
              "maleVariant": "",
              "stringId": 1660199465447743488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの警官は誰？",
              "maleVariant": "",
              "stringId": 1660199567755206656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "準備は？",
              "maleVariant": "",
              "stringId": 1701068833867747328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいよ",
              "maleVariant": "いいぞ",
              "stringId": 1701068833884524556
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ならジャックインして。出発しよう",
              "maleVariant": "",
              "stringId": 1701068833884524568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[接続する] わかった",
              "maleVariant": "",
              "stringId": 1701069528763256836
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうなればよかったのか、正直、もうわからない。でも…",
              "maleVariant": "",
              "stringId": 1701083350303559680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "街を離れるのが？",
              "maleVariant": "",
              "stringId": 1701086232948035584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。今日、モックスの子たちにお別れを言ってきたの。みんな、私が音を上げるって思ってるみたい",
              "maleVariant": "",
              "stringId": 1701086398203613184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも北には違う世界がある、って信じ込んでたんだ。向こうには明るい未来が待ってるんだって",
              "maleVariant": "",
              "stringId": 1701092295613140992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの新しい門出だからね。ここは真のノーマッドらしく、舵を取ってもらわないと",
              "maleVariant": "",
              "stringId": 1708227779594342412
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私はニトロ担当。ミッチが直結方式に改造したんだ",
              "maleVariant": "",
              "stringId": 1708227779594342424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手動でタイミングを合わせないと、こっちが木っ端みじんになる",
              "maleVariant": "",
              "stringId": 1708227779594342436
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リバー？ 別れを言いに来たの",
              "maleVariant": "リバーか？ 別れを言いに来たんだ",
              "stringId": 1708241803820191744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう… でも、こうなるはずじゃなかったんでしょ？",
              "maleVariant": "",
              "stringId": 1708241921529139200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "尻尾をまいてナイトシティに逃げ帰って来るか、みんなで賭けまで始めたのよ。もう何人がそっちに賭けたかわからないくらい",
              "maleVariant": "",
              "stringId": 1708269202205102080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エヴリンともナイトシティを出る計画を立ててたって話、したっけ？",
              "maleVariant": "",
              "stringId": 1708269279464181760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "続きはこうでしょ？ 反対側に行ってみても、何も変わらなかった。ナイトシティが答えだと、理想の世界が待ってると思ってたのに",
              "maleVariant": "続きはこうだろ？ 反対側に行ってみても、何も変わらなかった。ナイトシティが答えだと、理想の世界が待ってると思ってたのに",
              "stringId": 1708295148001873920
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう… そして、結末はこう…",
              "maleVariant": "",
              "stringId": 1708295370954297344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "違う。確かに帰るのは前と同じ場所かもしれないけど、パナム自身は変わった",
              "maleVariant": "違う。確かに帰るのは前と同じ場所かもしれないが、パナム自身は変わっただろ",
              "stringId": 1708295505440460800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かにそうかもね。あんたと、ふざけた仲間たちも一緒だし",
              "maleVariant": "",
              "stringId": 1708295620414722048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新たなスタートだ",
              "maleVariant": "",
              "stringId": 1709537784683614208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "荒野の真ん中で途方に暮れるケリー・ユーロダイン… まさかあんなものが見れるとはね",
              "stringId": 1731568847077298176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ケリーは別れを言いに来たんだ。まだナイトシティでやることがあるってさ",
              "stringId": 1731569834953330688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "そっか、ごめん",
              "stringId": 1731570327012298752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "今思えば、こうなるしかなかったのかもな。でも…",
              "stringId": 1732989726361440256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何かわからなくても、楽しめばいいよ。私もいま同じ気分",
              "maleVariant": "",
              "stringId": 1732989726495657984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これがお望みだったんでしょ？",
              "maleVariant": "",
              "stringId": 1764934064863498240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "彼とはうまくいかなかったの？",
              "stringId": 1825808890670477312
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしたの？",
              "maleVariant": "どうした？",
              "stringId": 1825828280602324992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "州境の壁――子供の頃はあんなものができるなんて、思ってもみなかった",
              "maleVariant": "",
              "stringId": 1825828541555142656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナム、このままどこまでもブッ飛ばして",
              "maleVariant": "パナム、このままどこまでもブッ飛ばせ",
              "stringId": 1825842521103265792
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "キャロル、キャシディ！ 移動開始！",
              "maleVariant": "",
              "stringId": 1826958727940009984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私はジャミングをかけながら西に向かう",
              "maleVariant": "",
              "stringId": 1826959182669672448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "食い付いた。こっちは東に行って敵を引きつけとくぞ。健闘を祈る！",
              "maleVariant": "",
              "stringId": 1826959902093471744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ああ…",
              "stringId": 1845931801603338240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあみんな、損することになるね",
              "maleVariant": "じゃあみんな、損することになるな",
              "stringId": 1845935198150717440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっば、ふと思い出しちゃった",
              "maleVariant": "",
              "stringId": 1863282797719449604
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしたの？",
              "maleVariant": "どうした？",
              "stringId": 1863282797719449616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "州境の壁――当たり前だけど、子供の頃はあんなものなかった",
              "maleVariant": "",
              "stringId": 1863282797719449628
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも北には違う世界がある、って信じ込んでたんだ。向こうには明るい未来が待ってるんだって",
              "maleVariant": "",
              "stringId": 1863282797736226824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "続きはこうでしょ？ 実際行ってみても、何も変わらなかった。ナイトシティが答えだと、理想の世界が待ってると思ってたのに",
              "maleVariant": "続きはこうだろ？ 実際行ってみても、何も変わらなかった。ナイトシティが答えだと、理想の世界が待ってると思ってたのに",
              "stringId": 1863282797736226836
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう… そして、結末はこう――尻尾を巻いてすごすごと家に帰りましたとさ",
              "maleVariant": "",
              "stringId": 1863282797753004032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "違う。確かに帰るのは前と同じ場所かもしれないけど、パナム自身は変わったでしょ",
              "maleVariant": "違う。確かに帰るのは前と同じ場所かもしれないが、パナム自身は変わっただろ",
              "stringId": 1863282797753004044
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かにそうかもね",
              "maleVariant": "",
              "stringId": 1863282797753004056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "尻尾を巻いてすごすごと家に帰りましたとさ… クソッ",
              "maleVariant": "",
              "stringId": 1863294233992622080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "二人でよく夢について話してた",
              "maleVariant": "",
              "stringId": 1863296098008428544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残念だったね…",
              "maleVariant": "残念だったな…",
              "stringId": 1863296554097045504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたが気にすることない。いつまでもその夢に囚われてちゃダメだって思って、話しただけだから",
              "maleVariant": "",
              "stringId": 1863298483627880448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふーん。しばしの？ それとも、これっきり？",
              "maleVariant": "",
              "stringId": 1863302290713735168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これっきりだろうね。ナイトシティを離れられない、離れたくないんだって",
              "maleVariant": "これっきりだろうな。ナイトシティを離れられない、離れたくないんだとさ",
              "stringId": 1863302835771928576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃ、向こう側でな！",
              "maleVariant": "",
              "stringId": 1905250490999500800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ行くよ！",
              "maleVariant": "",
              "stringId": 1905254988635566080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "準備はいい？",
              "maleVariant": "",
              "stringId": 1905271102144901120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつでもどうぞ",
              "maleVariant": "",
              "stringId": 1905271549324816384
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ行くよ。フルスピードで突っ切って！",
              "maleVariant": "",
              "stringId": 1905271714714611712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": 1905280459150585856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大きな穴を避けて、真っすぐ進んで。みんなと離れすぎないでね",
              "maleVariant": "",
              "stringId": 1905283704065486848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなのは絶対ご免ね",
              "maleVariant": "そんなのは絶対ご免だな",
              "stringId": 1905294837107257344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ニトロ、いけるよ",
              "maleVariant": "",
              "stringId": 1905299256074956800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言い残したことはない",
              "maleVariant": "",
              "stringId": 1905299980095713280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティともお別れだよ。挨拶しといたら？",
              "maleVariant": "",
              "stringId": 1905301292795424768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どう？",
              "maleVariant": "",
              "stringId": 1905304388409880576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最高よ、パナム。ここから全てが始まるのね",
              "maleVariant": "最高だ、パナム。ここから全てが始まるんだな",
              "stringId": 1905304718837149696
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちはいいぞ。パナム、先に行け",
              "maleVariant": "",
              "stringId": 1905475713984229376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それに向こうに着けば、それが何なのかはきっとわかる。きっとね",
              "maleVariant": "",
              "stringId": 1905482267953328128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ニトロを点火して。任せたよ",
              "maleVariant": "ニトロを点火しろ。任せたぞ",
              "stringId": 1905485024265367552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも？",
              "maleVariant": "",
              "stringId": 2006628425265704960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "閉じこもってた部屋からやっと出られた気分なの。何て言うか、希望… いや達成感かな？",
              "maleVariant": "閉じこもってた部屋からやっと出られた気分なんだ。何て言うか、希望… いや達成感かな？",
              "stringId": 2006629654716870656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "でも？",
              "stringId": 2006632393597382656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "閉じこもってた部屋からやっと出られた気分なんだ。何て言うか、希望… いや達成感かな？",
              "stringId": 2006633232709840896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、あまり脇にそれるとやられるよ！",
              "maleVariant": "",
              "stringId": 2018512956644286464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんなから離れないで！ こんなところで死にたくないでしょ？",
              "maleVariant": "",
              "stringId": 2018513770708361220
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[息をつく]",
              "maleVariant": "",
              "stringId": 2374935935424815104
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}