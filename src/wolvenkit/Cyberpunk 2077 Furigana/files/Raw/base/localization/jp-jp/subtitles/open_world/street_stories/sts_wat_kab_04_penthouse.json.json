  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Скоро вернусь. Покурю пойду.\" t=\"一服してくる。すぐ戻るよ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1501079970921865216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Не выходил бы ты на балкон. Вдруг кто смотрит?\" t=\"バルコニーに出ないほうがいい。誰が見てるかわからないでしょ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1506454803943809024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Надеюсь, ты смотришь.\" t=\"お前が見てるだろ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1506454803943809040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Миш, хватит. Это вопрос государственной безопасности.\" t=\"ふざけないで、ミーシャ。国家の安全に関する問題よ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1506454803943809056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Может, бросим всё и просто останемся здесь? Мне нравится этот город.\" t=\"何もかも捨てて、ここに残れないかな？ 気に入ったんだ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1506454803943809068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Миш, что ты несёшь? Мы ещё даже не закончили с переговорами.\" t=\"ミーシャ、何を言ってるの？ 交渉はまだ終わってないのよ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1506454803943809080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Миша! Прячься!\" t=\"ミーシャ！ 隠れて！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1506583389107412992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Что... Опять?!\" t=\"何…？ また！？\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1506592638671806464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Какого...\" t=\"何…？\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1506592638705360896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Не приближайся ко мне! Не подходи!\" t=\"さ、下がれ！ これ以上近づくな！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1506669720114323456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Надя!\" t=\"ナディア！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1506960871048372224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Пиздец тебе!\" t=\"死んじまえ！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1507053292436348928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Мне приказали закрыть контракт с «Арасакой» к концу недели.\" t=\"今週末までにアラサカと契約を締結しろってさ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1667996294734778368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Но как? Времени совсем мало.\" t=\"どうやって？ 時間がない…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1667996294734778380"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Мы предложим больше. Нельзя позволить китайцам нас обойти.\" t=\"オファーの額を上げよう。中国に計画を台無しにされるわけにはいかない\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1667996294751555592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Может, заодно и нефть им всю за так отдадим?\" t=\"どうせなら、タダで石油でもつけてやれば！？\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1667996294751555604"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Товарищ Надежда, вы ставите под сомнение авторитет генерального секретаря?\" t=\"“同志ナデジダ、書記長閣下の権威に逆らうつもりかね？”\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1667996294751555616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Как скажешь, милая. Я просто пошутил.\" t=\"わかってる。ただの冗談だ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1668058291866275840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Пошутил — и будет.\" t=\"ならやめて\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1668058429036793856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Да?.. Это защищённая линия... Да... Мы делаем всё, что в наших силах.\" t=\"もしもし… セキュアな回線です… ええ… 最善を尽くしています…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1675376660839763968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Не смешно. И ты это знаешь.\" t=\"笑えない。わかってるくせに\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1695229997932187648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Конечно. Мы купим эту технологию, не сомневайтесь. До свидания.\" t=\"ええ、技術は必ず買い取ります。お任せください\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1695231591096619008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Это займёт некоторое время... Если можно... Да, конечно, товарищ генерал...\" t=\"まだ時間がかかりそうです… お言葉ですが… もちろんです、将軍…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1699899273859403776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Ну... за народные массы!\" t=\"無産階級に万歳三唱！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1699903816844283904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Пф-ф...\" t=\"もう…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1702628443769114624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Что? Я просто пью.\" t=\"俺？ 俺は飲んでるだけだよ…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1702644821452492800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Ты что-то сделал?\" t=\"何かしたの？\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1702645038616776704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Э-э, Надюш, милая, это ты унесла кейс?\" t=\"なあ、ナディア、ブリーフケースをどこかに移したか？\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1740607815033393152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Очень смешно.\" t=\"ふざけないでよ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1740609012859506688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Я не шучу. Ты его видела?\" t=\"ふざけるもんか。最後に見たのは？\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1740610447126925312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1740610747204210688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Он лежал на столе. Я думала, ты его забрал.\" t=\"テーブルの上よ――あなたが持って行ったのかと\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1740611162993954816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Надя!\" t=\"ナディア！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1740612144377204736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Я не брал...\" t=\"持ってないぞ…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1740612426754527232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Ну тогда прекрати дрожать, давай искать.\" t=\"震えてる場合じゃない、探すわよ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1740612559646855168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Ну, нашёл?\" t=\"あった？\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1740612986224349184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Должно быть, упал куда-то...\" t=\"どこかに落っことしたんじゃないか…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1740613479055069184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Нужно позвонить.\" t=\"報告しなきゃ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1740613539436269568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Может, за диваном?\" t=\"ソファの後ろはどうだ…？\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1740613647632535552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Миша, нужно позвонить. Его явно кто-то забрал.\" t=\"ミーシャ、きっと誰かに取られたのよ。報告しないと\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1740614698003697664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Нет, нет, нет! Пять минут, дай мне ещё пять минут!\" t=\"ま、待ってくれ！ あと5分、あと5分だけくれ！\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1740615594863333376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Гм.\" t=\"ええ\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1740617061863100416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Красный.\" t=\"レッド\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1740617245808496640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Подтверждаю. Мы потеряли 7K-T.\" t=\"ご報告します。7K-Tを紛失しました\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1747582416854552576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"Он принимает всех потерянных, отвергнутых, отчаявшихся, заманивает, втирается в доверие — и обирает их до нитки.\" t=\"この街は迷える者、見捨てられた者、絶望した者の前に夢をチラつかせて、気を取られてる隙に、すべてを奪い去ろうとする\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1747604396618125312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"rus\" o=\"У-у-ух...\" t=\"はあ…\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1934391222845411328"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}