    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1350205065034645504,
            "femaleVariant": "V、起きろ！"
          },
          {
            "stringId": 1350205757027696640,
            "femaleVariant": "ヤウェンから連絡だ！ 野郎、あのアニメで夢を見たらしい！"
          },
          {
            "stringId": 1360103195343503360,
            "femaleVariant": "[BDを見る]"
          },
          {
            "stringId": 1563326653576429568,
            "femaleVariant": "いや、署に連絡して片方に人を送る"
          },
          {
            "stringId": 1567513111711367168,
            "femaleVariant": "頼むぞ…"
          },
          {
            "stringId": 1679407983095681024,
            "femaleVariant": "つまり、2つに絞り込める"
          },
          {
            "stringId": 1679408655878819840,
            "femaleVariant": "問題はあたしたちがどっちに行くかね。エッジウッドか、ポピーか…",
            "maleVariant": "問題は俺たちがどっちに行くかだな。エッジウッドか、ポピーか…"
          },
          {
            "stringId": 1679408903057543168,
            "femaleVariant": "二つだとしても、こっちには時間がない。手分けして行く？",
            "maleVariant": "二つだとしても、こっちには時間がない。手分けして行くか？"
          },
          {
            "stringId": 1679421789167390720,
            "femaleVariant": "よし、状況を整理しましょ",
            "maleVariant": "よし、一旦状況を整理するぞ"
          },
          {
            "stringId": 1679444398311444480,
            "femaleVariant": "エッジウッドにしましょ。直感を信じる",
            "maleVariant": "エッジウッドにしよう。直感を信じる"
          },
          {
            "stringId": 1679445182965059584,
            "femaleVariant": "ポピーだと思う。あたしの勘よ",
            "maleVariant": "ポピーだと思う。俺の勘だ"
          },
          {
            "stringId": 1679446478283575296,
            "femaleVariant": "よし。じゃあ行くぞ"
          },
          {
            "stringId": 1679447094577827840,
            "femaleVariant": "よし。じゃあ行こう"
          },
          {
            "stringId": 1679454115893055488,
            "femaleVariant": "つまり… エッジウッド農場しか考えられない"
          },
          {
            "stringId": 1679458085164257280,
            "femaleVariant": "じゃあ、決まりね？",
            "maleVariant": "じゃあ、決まりだな？"
          },
          {
            "stringId": 1760648744553910272,
            "femaleVariant": "別の農場にも部隊を回すよう手配する"
          },
          {
            "stringId": 1775196635816660992,
            "femaleVariant": "まだ眠いんだけど…",
            "maleVariant": "まだ眠いんだよ…"
          },
          {
            "stringId": 1775200262950096896,
            "femaleVariant": "BDは？"
          },
          {
            "stringId": 1775200351953227776,
            "femaleVariant": "ここにある。挿して、コンピューターと接続しろ。モニターで見たい"
          },
          {
            "stringId": 1785277670864588800,
            "femaleVariant": "間違ってないことを祈るよ… よし、行こう"
          },
          {
            "stringId": 1785303164515389440,
            "femaleVariant": "10-33、シティ東の農場への応援を要請"
          },
          {
            "stringId": 1785304687215513600,
            "femaleVariant": "違う。そうだ… いや、そうじゃない。ああクソ…"
          },
          {
            "stringId": 1785305203953766400,
            "femaleVariant": "「付近に対応可能な部隊なし」だとよ。チクショウ… こっちで合ってるといいが"
          },
          {
            "stringId": 1841391996025049088,
            "femaleVariant": "きっと間に合う。警察が手がかりすらつかめない中、2日でここまで来たのよ",
            "maleVariant": "きっと間に合う。警察が手がかりすらつかめない中、2日でここまで来たんだ"
          },
          {
            "stringId": 1841392024026222592,
            "femaleVariant": "今考えても仕方ない。運転に集中して",
            "maleVariant": "今考えても仕方ない。運転に集中しろ"
          },
          {
            "stringId": 1841393104193720320,
            "femaleVariant": "もちろん、俺も信じてるさ… ありがとう、V… 感謝してる"
          },
          {
            "stringId": 1841404259582742528,
            "femaleVariant": "そう言うのは簡単だが、俺のおいっ子なんだぞ…"
          },
          {
            "stringId": 1841412577575440384,
            "femaleVariant": "クソ！ 時間をムダにした！"
          },
          {
            "stringId": 1841412984087384064,
            "femaleVariant": "ピーター・パンの野郎！ 全部済んだら殺してやる！"
          },
          {
            "stringId": 1841413569008242688,
            "femaleVariant": "よりによって農場を間違うなんて…"
          },
          {
            "stringId": 1841414027764436992,
            "femaleVariant": "悔やんでも仕方ない。とにかく急ぎましょ",
            "maleVariant": "悔やんでも仕方ない。とにかく急ごう"
          },
          {
            "stringId": 1909813919306715136,
            "femaleVariant": "農場はどこだ？ 場所は覚えてるよな？"
          },
          {
            "stringId": 1909814879953326080,
            "femaleVariant": "街の南東、そう遠くない"
          },
          {
            "stringId": 1914236583446286336,
            "femaleVariant": "ハリスは、水没した学校の近くに住み続けていた"
          },
          {
            "stringId": 1914244010837250048,
            "femaleVariant": "農場からそう遠くない位置に、シティのビル群と〈Us Cracks〉のタワーが見えた…"
          },
          {
            "stringId": 1914245645491736576,
            "femaleVariant": "農場がゴミ埋立地の近くだってこともわかってる"
          },
          {
            "stringId": 1914249298998956032,
            "femaleVariant": "〈Us Cracks〉のPRタワーは1時間ごとに色が変わる作りだった。確か時間は…"
          },
          {
            "stringId": 1914253216260788224,
            "femaleVariant": "てことは農場は街の東側、正確には南東にあるはずだ"
          },
          {
            "stringId": 1914257185666207744,
            "femaleVariant": "ソーラーパネルがないところは除いていい"
          },
          {
            "stringId": 1914260304919760896,
            "femaleVariant": "消防署の証明書には南部を示すスタンプがあった"
          },
          {
            "stringId": 1914261593661607936,
            "femaleVariant": "農場はペトロケムの設計だったから、それ以外は消せる"
          },
          {
            "stringId": 1914264407955705856,
            "femaleVariant": "ハリスが送ったゲームの配達コードはバッドランズのものだ"
          },
          {
            "stringId": 1914266149061627904,
            "femaleVariant": "ハリスのIPアドレスはナイトシティの外、10マイルちょっとの圏内に絞れる"
          },
          {
            "stringId": 1914341891966636032,
            "femaleVariant": "夜の11時"
          },
          {
            "stringId": 1914368158677995520,
            "femaleVariant": "行くぞ！"
          },
          {
            "stringId": 1918128413384609792,
            "femaleVariant": "ヤウェンも、なかなか仕事が速いね",
            "maleVariant": "ヤウェンも、なかなか仕事が速いな"
          },
          {
            "stringId": 1927385320477659140,
            "femaleVariant": "クソ、この農場であることを祈るぞ"
          },
          {
            "stringId": 1996674663725740032,
            "femaleVariant": "[立ち上がる] エッジウッド農場にしましょ",
            "maleVariant": "[立ち上がる] エッジウッド農場にしよう"
          },
          {
            "stringId": 1996674663725740048,
            "femaleVariant": "[立ち上がる] ポピー農場にしましょ",
            "maleVariant": "[立ち上がる] ポピー農場にしよう"
          },
          {
            "stringId": 1996674666762416128,
            "femaleVariant": "きっと生きてる"
          },
          {
            "stringId": 1996674666829525004,
            "femaleVariant": "運転に集中して",
            "maleVariant": "運転に集中しろ"
          },
          {
            "stringId": 2005497436815101952,
            "femaleVariant": "必ず見つかるって"
          },
          {
            "stringId": 2005499972104097792,
            "femaleVariant": "クソ！ 俺がもっと早く、あいつの悩みに気づいてやれてれば…"
          },
          {
            "stringId": 2005500861028110336,
            "femaleVariant": "あんたのせいじゃない"
          },
          {
            "stringId": 2005501169745661952,
            "femaleVariant": "俺は伯父なんだぞ？ なのに力になれなかった…"
          },
          {
            "stringId": 2005502319421165568,
            "femaleVariant": "クソ！"
          },
          {
            "stringId": 2005512702622044160,
            "femaleVariant": "うう！"
          },
          {
            "stringId": 2005514122175844352,
            "femaleVariant": "頼むぞ、ランディ… 無事でいてくれ"
          },
          {
            "stringId": 2005514165343621120,
            "femaleVariant": "クソ… ちくしょう…！"
          },
          {
            "stringId": 2005518458482200576,
            "femaleVariant": "ちくしょう…"
          },
          {
            "stringId": 2329166852071841792,
            "femaleVariant": "[乗る]"
          }
        ]
      }
    }
  }
}