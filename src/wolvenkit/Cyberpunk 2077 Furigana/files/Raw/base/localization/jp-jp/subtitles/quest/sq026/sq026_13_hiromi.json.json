    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここまで来るのに随分手間取ったじゃない",
              "maleVariant": "",
              "stringId": 1754534318445674496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっと来たわね",
              "maleVariant": "",
              "stringId": 1754534318479228928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私にそんな権限はない。今はまだね",
              "maleVariant": "",
              "stringId": 1774853448918876160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "警報を鳴らされたくなかったから、来るのに時間がかかったの",
              "maleVariant": "警報を鳴らされたくなかったから、来るのに時間がかかった",
              "stringId": 1774857136232185856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待ちくたびれたわ…",
              "maleVariant": "",
              "stringId": 1774857329740595200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。やってみる",
              "maleVariant": "",
              "stringId": 1774859047995949056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、一体どういうつもり！？",
              "maleVariant": "",
              "stringId": 1775009433977339904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうなるの？",
              "maleVariant": "どうなるんだ？",
              "stringId": 1775010442254467076
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ。あんたはクラウドを去るか、あたしに殺される",
              "maleVariant": "ああ。あんたはクラウドを去るか、俺に殺される",
              "stringId": 1775010793418375168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、もちろんわかってる。あんたはクラウドを去って、二度と戻ってこない",
              "maleVariant": "ああ、もちろんわかってる。あんたはクラウドを去って、二度と戻ってこない",
              "stringId": 1775011148273270784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうなるって言うの？",
              "maleVariant": "どうなるって言うんだ？",
              "stringId": 1775011341697794052
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中は必ず報復に出る。クラウドはきっと破滅するわよ",
              "maleVariant": "",
              "stringId": 1775017857632944128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも",
              "maleVariant": "",
              "stringId": 1775148358217744388
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの金は欲しくない",
              "maleVariant": "",
              "stringId": 1775148860124938240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも",
              "maleVariant": "",
              "stringId": 1775149296315777024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの金はいらない",
              "maleVariant": "",
              "stringId": 1775149438653677572
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "明日の昼、お前の言う条件で契約の提示をさせてもらう",
              "maleVariant": "",
              "stringId": 1775149881706397696
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "覚えとくよ",
              "maleVariant": "",
              "stringId": 1775150509224607744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいのよ。素晴らしい働きには対価を払うべきだから",
              "maleVariant": "",
              "stringId": 1775153377222574080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "報酬も受け取って、ジュディとも仲良くすればいいじゃない",
              "maleVariant": "",
              "stringId": 1775154374862946304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前の仕事もこれで終わりだろ。安定した職が必要な時は、タイガークロウズがいつでも相談に乗ってやる",
              "maleVariant": "",
              "stringId": 1775155470348042240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クーデターに協力してやったんだから、それに見合った金額が必要よ",
              "maleVariant": "クーデターに協力してやったんだから、それに見合った金額が必要だ",
              "stringId": 1782335288239964160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "就任初日から節約に熱心ね",
              "maleVariant": "就任初日から節約に熱心だな",
              "stringId": 1782335860275920896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのタイガークロウズの幹部を相手に、脅迫したのよ？",
              "maleVariant": "あのタイガークロウズの幹部を相手に、脅迫したんだぞ？",
              "stringId": 1782336800152338432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしあの場所にあたしがいなかったら、生きて帰れたと思う？",
              "maleVariant": "もしあの場所に俺がいなかったら、生きて帰れたと思うか？",
              "stringId": 1782337505399390208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "初日から節約を欠かさないなんて… あんたはきっといいボスになれるよ",
              "maleVariant": "",
              "stringId": 1782337691811037188
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。ボーナスを付けるから、就任を祝ってちょうだい",
              "maleVariant": "",
              "stringId": 1782337691811037196
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつらがバーチャにジャックインしてなかったら、今頃ゲームオーバーよ",
              "maleVariant": "",
              "stringId": 1888201944785969152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "警備を引き上げてくれれば、ここまで苦労しないで済んだのに",
              "maleVariant": "",
              "stringId": 1888226855478845440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あとの2人は？",
              "maleVariant": "",
              "stringId": 1888228752545767424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あとの2人は何者？",
              "maleVariant": "あとの2人は何者だ？",
              "stringId": 1888230042462019588
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“今はまだ”？ ずいぶん自信があるのね",
              "maleVariant": "“今はまだ”？ ずいぶん自信があるんだな",
              "stringId": 1888233058216927232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“今はまだ”？ まるでもうすぐあんたがトップに立つみたいね",
              "maleVariant": "“今はまだ”？ まるでもうすぐあんたがトップに立つみたいだな",
              "stringId": 1888235031351746560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "失敗のしようがない。計画は完璧に進んでるわ",
              "maleVariant": "",
              "stringId": 1888237581186588672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本日の大事なお客様。この2人の目の前で、ヒロミには自分の小便にまみれて死んでもらうの",
              "maleVariant": "",
              "stringId": 1888246874606624768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ショーの後に別のショーが始まるってわけ。きっと脳裏に焼き付くわ",
              "maleVariant": "",
              "stringId": 1888248863881125888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これから2人を起こす。気分の悪い目覚めになるでしょうね",
              "maleVariant": "",
              "stringId": 1888249500593250304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やってみる。2人を起こして",
              "maleVariant": "やってみる。2人を起こしてくれ",
              "stringId": 1888273520130744324
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "計画と違う",
              "maleVariant": "",
              "stringId": 1888275691639365632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "殺しはなしだって言ったでしょ",
              "maleVariant": "殺しはなしだって言っただろ",
              "stringId": 1888276748083879936
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "殺しはなしだって初めに言ったはずよ",
              "maleVariant": "殺しはなしだって初めに言ったはずだ",
              "stringId": 1888283314367455232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "始めよう",
              "maleVariant": "",
              "stringId": 1888296548084441088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "警備を引き上げさせてくれたらよかったのに",
              "maleVariant": "",
              "stringId": 1888298215689379840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さっさと終わりにしない？",
              "maleVariant": "さっさと終わりにしないか？",
              "stringId": 1888298928268075008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつがターゲットのヒロミ・サトウ",
              "maleVariant": "",
              "stringId": 1888300350439120896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつら、何を見てるの？",
              "maleVariant": "こいつら、何を見てるんだ？",
              "stringId": 1888304114491416576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何を見てるの？",
              "maleVariant": "何を見てるんだ？",
              "stringId": 1888309522039791616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クラウドが誇る次期スタードールのサンプル映像よ。私が編集したの",
              "maleVariant": "",
              "stringId": 1888309964354314240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またまた。かわいいジュディの前だからカッコつけてたんじゃないの？",
              "maleVariant": "",
              "stringId": 1888331190367903744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "殺しはなし？ 結構よ。奴をこのピカピカの床に跪かせ、命乞いさせれば十分だから",
              "maleVariant": "",
              "stringId": 1888331808339877888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった？",
              "maleVariant": "",
              "stringId": 1888333034251051008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイコ、目撃者はなしにする計画でしょ",
              "maleVariant": "マイコ、目撃者はなしにする計画だろ",
              "stringId": 1888335291038261252
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "力を示すんでしょ？ 目撃者なしにどうやって示すのよ",
              "maleVariant": "",
              "stringId": 1888335291055038464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、エヴリンとは違うが… 待て。どうなってる！？ こいつは誰だ？",
              "maleVariant": "",
              "stringId": 1889759654446850048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "次世代のエヴリンよ。もっと賢い子だといいけど",
              "maleVariant": "",
              "stringId": 1889770928299794432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジュディに手を貸したおかげで、ここまで来れた。だけど彼女は、私やあなたとは違う種類の人間なのよ",
              "maleVariant": "",
              "stringId": 1889868760507703296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "システムには逆らえない。お互いわかってるはずよ。それが無視できない現実",
              "maleVariant": "",
              "stringId": 1889869065869811712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この舞台でのあなたの役目は、合図を受けたら頷き、然るべき時にその力を振るうことだったはず",
              "maleVariant": "",
              "stringId": 1889869412050886656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヒロミ・サトウの時代はもう終わったの",
              "maleVariant": "",
              "stringId": 1889883904025518080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クラウドは独立を宣言する",
              "maleVariant": "",
              "stringId": 1889899282374025220
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヒロミには退いてもらう",
              "maleVariant": "",
              "stringId": 1889900131871580160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クラウドのドールたちは独立を宣言し、今後クラブはドール自身の手で運営していく",
              "maleVariant": "",
              "stringId": 1889902515343548416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヒロミには退いてもらう",
              "maleVariant": "",
              "stringId": 1889902615117651968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クラブの運営には口を出さなければ、収益の一部を渡してもいい",
              "maleVariant": "",
              "stringId": 1889909568183164928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかくすぐにやめさせろ！",
              "maleVariant": "",
              "stringId": 1889919154701164544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そいつは今聞いた。んでそれからどうする？",
              "maleVariant": "",
              "stringId": 1889923013863497728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V。話は私がする",
              "maleVariant": "",
              "stringId": 1889923473559216128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これからはマイコが正式な支配人になる",
              "maleVariant": "",
              "stringId": 1889924273647869956
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クラウドはドールたちのものになる",
              "maleVariant": "",
              "stringId": 1889932702940618752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自治権さえ渡せば、あんたたちにはマイコが毎月アガリを渡す。運営もこれまでどおり、彼女が続けていく…",
              "maleVariant": "自治権さえ渡せば、あんたらにはマイコが毎月アガリを渡す。運営もこれまでどおり、彼女が続けていく…",
              "stringId": 1889935979430240256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これからはドールが自分たちで店を運営するの。あんたたちは必要ない",
              "maleVariant": "これからはドールが自分たちで店を運営する。あんたらは必要ない",
              "stringId": 1889936004596064256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイコ？",
              "maleVariant": "",
              "stringId": 1889940829572390912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "異論はないわ",
              "maleVariant": "",
              "stringId": 1889941091028525056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、一つ忘れてるわよ。私がサトウの座に就く",
              "maleVariant": "",
              "stringId": 1889953199514411008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言ってることがちぐはぐじゃねえか",
              "maleVariant": "",
              "stringId": 1889953996734160896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヒロミには落とし前をつけてもらう。後でな",
              "maleVariant": "",
              "stringId": 1889954533621850112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "客を皆殺しにして、代わりにドールにゃお互いをファックさせる気か？",
              "maleVariant": "",
              "stringId": 1889964201761005568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2人を起こして",
              "maleVariant": "2人を起こせ",
              "stringId": 1889979850038140928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫、私が命令して…",
              "maleVariant": "",
              "stringId": 1890827040826548224
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうなるかわかってるでしょうね？",
              "maleVariant": "",
              "stringId": 1890836762568908800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それか、ここであたしに殺される",
              "maleVariant": "それか、ここで俺に殺される",
              "stringId": 1890844652004401152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最初から計画済みってわけ… でも忘れないで、私にもドールチップが入ってるのよ",
              "maleVariant": "",
              "stringId": 1890844988924452864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうとは限らない",
              "maleVariant": "",
              "stringId": 1890857663691710464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もうあんたには関係ないことよ",
              "maleVariant": "もうあんたには関係ないことだ",
              "stringId": 1890858144308617216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もうあんたには関係ないことよ。諦めて、ここから出てって",
              "maleVariant": "もうあんたには関係ないことだ。諦めて、ここから出ていけ",
              "stringId": 1890858764545515520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうとは限らないでしょ。今回の犠牲に懲りてクロウズは手を引くかも",
              "maleVariant": "そうとは限らないだろ。今回の犠牲に懲りてクロウズは手を引くかも",
              "stringId": 1890858793939197952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リジーズ・バーだって手放したんだから、次はクラウドが解放される番なんじゃない？ もう諦めることね、マイコ",
              "maleVariant": "リジーズ・バーだって手放したんだから、次はクラウドが解放される番じゃないか？ もう諦めろよ、マイコ",
              "stringId": 1890861784276623360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね、でもその前に… この壁をあんたの血で染めてやる！",
              "maleVariant": "",
              "stringId": 1890862516082008064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはあたしたちを裏切ろうとした",
              "maleVariant": "あんたは俺たちを裏切ろうとした",
              "stringId": 1890863091288858624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんなを騙してたんでしょ？ 協力する振りをして、クラウドの解放計画に乗った風に見せかけて…",
              "maleVariant": "みんなを騙してたんだろ？ 協力する振りをして、クラウドの解放計画に乗った風に見せかけて…",
              "stringId": 1890863657318572032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だけど本当の狙いは、ヒロミの地位を奪うこと",
              "maleVariant": "でも本当の狙いは、ヒロミの地位を奪うこと",
              "stringId": 1890870288446087168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お見事ね。でもひとつ見逃してる。この話はまだ終わってないの。死体に私は止められない。あんたの死体にもね！",
              "maleVariant": "",
              "stringId": 1890870827548368896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中はミリテクのCEO以上に損得勘定がしっかりしてるから",
              "maleVariant": "連中はミリテクのCEO以上に損得勘定がしっかりしてるからな",
              "stringId": 1890885344957259776
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "出てって… 今すぐに！",
              "maleVariant": "",
              "stringId": 1890887139632832512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このナイトシティで、血を見ずに正義や平和が為せると思ってるなら、そんなのは夢に過ぎない",
              "maleVariant": "",
              "stringId": 1890897471411544064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "覚悟を決めて飛び込んで、内側から掌握するしかないの",
              "maleVariant": "",
              "stringId": 1890899726906908672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "任せる",
              "maleVariant": "",
              "stringId": 1890915842412732416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヒロミに取って代わる気なのね",
              "maleVariant": "ヒロミに取って代わる気なんだな",
              "stringId": 1890929018583867392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイコがクラウドを引き継ぐ",
              "maleVariant": "",
              "stringId": 1890930120578523136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クラウドの日々の運営はマイコがやる。これ以上の適任はいないはずよ",
              "maleVariant": "クラウドの日々の運営はマイコがやる。これ以上の適任はいないはずだ",
              "stringId": 1890933191262035968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見てのとおり、ドールは彼女を信頼して、何でもやるからね",
              "maleVariant": "見てのとおり、ドールは彼女を信頼して、何でもやるからな",
              "stringId": 1890934876248170496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヒロミの何が気に入らねえかわかるか？ こいつにはビジョンってもんがねえ",
              "maleVariant": "",
              "stringId": 1890938360137736192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どっちの手を使うか指示しねえと、自分の鼻くそすらほじれねえようなおこちゃまだ",
              "maleVariant": "",
              "stringId": 1890939495988813824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この惨劇を止めさせろ",
              "maleVariant": "",
              "stringId": 1890939792760987648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだやるべきことが一つ残ってたな",
              "maleVariant": "",
              "stringId": 1890940260224557056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そして、今がまさにその時よ",
              "maleVariant": "",
              "stringId": 1890948070790356992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうよ",
              "maleVariant": "",
              "stringId": 1890949657378119680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで、タイガークロウズに加わることになっても？",
              "maleVariant": "それで、タイガークロウズに加わることになってもか？",
              "stringId": 1890950336603709440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうは言っても、他のギャングからクラウドを守るには、こうするしかないでしょ？",
              "maleVariant": "",
              "stringId": 1890950806667747328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "納得いかないね",
              "maleVariant": "納得いかないな",
              "stringId": 1890954251449622528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今さら後には引けないわ。トムとロキシーは行動を開始してる。革命はもう始まってるの",
              "maleVariant": "",
              "stringId": 1890959575229358080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "足並みをそろえて全てを勝ち取るか、全てを無駄にするかよ",
              "maleVariant": "",
              "stringId": 1890969784416952320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話を合わせるから、2人を起こして",
              "maleVariant": "話を合わせるから、2人を起こせ",
              "stringId": 1890974435749093376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヒロミに取って代わる気なのね",
              "maleVariant": "ヒロミに取って代わる気なんだな",
              "stringId": 1890974634005454848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全然納得いかない",
              "maleVariant": "",
              "stringId": 1890975023522078720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう。新支配人として、最初の送金をさせてもらうわ",
              "maleVariant": "",
              "stringId": 1890992842334756864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "断る。自分にご褒美でも買うことね",
              "maleVariant": "断る。自分にご褒美でも買うんだな",
              "stringId": 1891001203595894784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかったわよ。2倍にする。でもそこまでよ",
              "maleVariant": "",
              "stringId": 1891007796689461248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんなのは聞いてない",
              "maleVariant": "",
              "stringId": 1901434026983972864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私はクロウズをよく知ってる。力と冷酷さが何より物を言うの。これが最善の方法よ",
              "maleVariant": "",
              "stringId": 1901434298657431552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイコ…",
              "maleVariant": "",
              "stringId": 1901449208233746432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だがてめえには、今ここで、落とし前をつけてもらう",
              "maleVariant": "",
              "stringId": 1901453784823390208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今どうすべきかは、私が誰よりも理解してる。私を信じて",
              "maleVariant": "",
              "stringId": 1906940935453868032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジュディ、ちょっと黙ってて",
              "maleVariant": "ジュディ、ちょっと黙っててくれ",
              "stringId": 1907161376076398596
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "勝手に話を進めないで",
              "maleVariant": "勝手に話を進めるな",
              "stringId": 1907165594757693444
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って、マイコ。話が飛躍しすぎよ",
              "maleVariant": "待て、マイコ。話が飛躍しすぎだろ",
              "stringId": 1907167328448086016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジュディ、集中できないでしょ",
              "maleVariant": "ジュディ、集中できないだろ",
              "stringId": 1907167407384887296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どううまくやったって、こいつは何かしらケチをつけてくるぜ",
              "maleVariant": "",
              "stringId": 1911250153602351104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラ探しに必死で、ウンザリね",
              "maleVariant": "アラ探しに必死で、ウンザリだ",
              "stringId": 1911259618686861312
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "可愛げなんてなくてもいいでしょ。毎日ギャングを相手にしてたらこうなるのよ",
              "maleVariant": "可愛げなんてなくてもいいだろ。毎日ギャングを相手にしてたらこうなるもんだ",
              "stringId": 1911259672558501888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、俺を見てみろ",
              "maleVariant": "",
              "stringId": 1911332612478140416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私は長い間、ヒロミのうんざりするような無能ぶりを見てきたわ",
              "maleVariant": "",
              "stringId": 1912666910237982720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V！ マイコは何をしてるの？",
              "maleVariant": "",
              "stringId": 1912671340865847296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その熱弁っぷり… つまりマイコ、お前がクラウドの次期支配人の座につきたいということか？",
              "maleVariant": "",
              "stringId": 1912671496289976320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、是非とも",
              "maleVariant": "",
              "stringId": 1912672574779752448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもここまで来ると… もはや彼が役立たずなのは明らかでしょ",
              "maleVariant": "",
              "stringId": 1912672708443832320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クラウドには、リストラが必要よ",
              "maleVariant": "",
              "stringId": 1912672960437616640
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やめさせろ",
              "maleVariant": "",
              "stringId": 1912807129176145920
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "意思の統一が図れてないようだな…",
              "maleVariant": "",
              "stringId": 1912832473694273536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう",
              "maleVariant": "",
              "stringId": 1912919214384021504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人はちっとも驚いてない時、こういう顔になる",
              "maleVariant": "",
              "stringId": 1914057579070971904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "可愛げのない女だ",
              "maleVariant": "",
              "stringId": 1914113390996578304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしもウンザリしてる",
              "maleVariant": "俺もウンザリしてる",
              "stringId": 1914135560946671620
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "可愛げなんか要らない",
              "maleVariant": "",
              "stringId": 1914140217697853440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "考え抜いたうえで、決めたことなの",
              "maleVariant": "",
              "stringId": 1914181307431870464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして3人いるの？ マイコはどういうつもり？",
              "maleVariant": "",
              "stringId": 1914213899891859456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そして今度は、ヒロミの自宅にも侵入してみせた",
              "maleVariant": "",
              "stringId": 1914223528654888960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それだけじゃない。ヒロミが無能なおかげで、今、店はこんな事態になってるの…",
              "maleVariant": "",
              "stringId": 1914223530064175104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "な… どういうことだ！？",
              "maleVariant": "",
              "stringId": 1914223531876114432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オズワルド・フォレストを殺し――",
              "maleVariant": "",
              "stringId": 1914224642964021248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここにいるのは、前にもクラウドの客を痛めつけた傭兵",
              "maleVariant": "",
              "stringId": 1914240899364904960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何言ってるの！ クロウズを追い出すはずじゃない！",
              "maleVariant": "",
              "stringId": 1914273595810078720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それには今の話を飲めというんだな",
              "maleVariant": "",
              "stringId": 1914281148023529472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "集中もクソも、計画は終わりよ！？",
              "maleVariant": "",
              "stringId": 1914285307229593600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、私と来て",
              "maleVariant": "",
              "stringId": 1914310226025865216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正式な支配人としてね",
              "maleVariant": "正式な支配人としてな",
              "stringId": 1914314955959263232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこの傭兵すら言うこと聞いてねえじゃねえか",
              "maleVariant": "",
              "stringId": 1914319830763921408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "警戒されたくなかった",
              "maleVariant": "",
              "stringId": 1915481891614556160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "たどり着けないんじゃないかと思った",
              "maleVariant": "",
              "stringId": 1915754456614502400
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[武器を抜く] さっさと始めよう",
              "maleVariant": "",
              "stringId": 1938861676714156032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[武器を抜く] さっさと始めよう",
              "maleVariant": "",
              "stringId": 1938863658355023872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おしゃべりはもういい。さっさと始めよう",
              "maleVariant": "",
              "stringId": 1938867103455666176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早く始末して！ そのために来たんでしょ！",
              "maleVariant": "",
              "stringId": 2257573582918606848
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}