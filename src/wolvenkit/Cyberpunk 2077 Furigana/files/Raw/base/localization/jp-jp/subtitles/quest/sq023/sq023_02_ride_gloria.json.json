    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1373339725379457040,
            "femaleVariant": "自分の存在する意義や、人生そのものを変えるような経験をしたことはあるか？"
          },
          {
            "stringId": 1373340545835651072,
            "femaleVariant": "ある"
          },
          {
            "stringId": 1373340960971083776,
            "femaleVariant": "ない"
          },
          {
            "stringId": 1374792601532420096,
            "femaleVariant": "話したくない"
          },
          {
            "stringId": 1374792717915967488,
            "femaleVariant": "殺人を正すことはできない"
          },
          {
            "stringId": 1374792717932744716,
            "femaleVariant": "どうしてあたしにそんな話を？",
            "maleVariant": "どうして俺にそんな話を？"
          },
          {
            "stringId": 1374792717932744732,
            "femaleVariant": "悔い改めて生まれ変わった話？ 勘弁してよ…",
            "maleVariant": "悔い改めて生まれ変わった話？ 勘弁してくれ…"
          },
          {
            "stringId": 1374793352900038656,
            "femaleVariant": "いいえ、ない",
            "maleVariant": "いや、ないな"
          },
          {
            "stringId": 1374793352916815872,
            "femaleVariant": "まあ… あるかもね",
            "maleVariant": "まあ… あるかもな"
          },
          {
            "stringId": 1374793442372931584,
            "femaleVariant": "初対面でする話じゃない"
          },
          {
            "stringId": 1374793687890710528,
            "femaleVariant": "12年前、私は実刑判決を受けた。強盗殺人と――"
          },
          {
            "stringId": 1374794501266919424,
            "femaleVariant": "ならば、私がやろうとしてることがわかるはずだ"
          },
          {
            "stringId": 1374794635232989184,
            "femaleVariant": "残念だな。誰もがそういう経験をすべきだ。私はした"
          },
          {
            "stringId": 1374794856390250496,
            "femaleVariant": "…じゃあ、聞くだけでいい"
          },
          {
            "stringId": 1374795439700496384,
            "femaleVariant": "その他の余罪でだ。当時の私は冷酷だった。人を撃ち殺しても、気にかけることすらなかった"
          },
          {
            "stringId": 1374795530834333696,
            "femaleVariant": "だが檻の中にいると、考える時間はいくらでもある。それで自分の過ちに気づき、正したいと思ったんだ"
          },
          {
            "stringId": 1374796745018232832,
            "femaleVariant": "こんな話をあたしにする理由がわからない",
            "maleVariant": "こんな話を俺にする理由がわからない"
          },
          {
            "stringId": 1374796745035010048,
            "femaleVariant": "ちょっとしたミスなら正せても、殺人は殺人でしょ",
            "maleVariant": "ちょっとしたミスなら正せても、殺人は殺人だろ"
          },
          {
            "stringId": 1374796745051787264,
            "femaleVariant": "懺悔と罪滅ぼしと更生の話？ テレビのドキュメンタリーじゃ人気のネタよね。いつも途中で寝ちゃうけど",
            "maleVariant": "懺悔と罪滅ぼしと更生の話か？ テレビのドキュメンタリーじゃ人気のネタだな。いつも途中で寝ちゃうが"
          },
          {
            "stringId": 1374797275983564800,
            "femaleVariant": "どうして人を撃ったの？",
            "maleVariant": "どうして人を撃ったんだ？"
          },
          {
            "stringId": 1374797446591074304,
            "femaleVariant": "どうして人を撃ったの？",
            "maleVariant": "どうして人を撃ったんだ？"
          },
          {
            "stringId": 1374798802961879040,
            "femaleVariant": "相手の目つきが気に入らなかった。私の手には銃があり、それを止めさせる力があった"
          },
          {
            "stringId": 1374799260996653056,
            "femaleVariant": "今でも撃たれた人たちの表情が… 脳裏に焼き付いている。神よ、忘れさせたもうな"
          },
          {
            "stringId": 1374800620806787072,
            "femaleVariant": "家族に償うことはできる…"
          },
          {
            "stringId": 1374800743548899328,
            "femaleVariant": "やろうとしていることを理解してほしいんだ"
          },
          {
            "stringId": 1374800956334329856,
            "femaleVariant": "これはテレビじゃない。まったく違うものになる"
          },
          {
            "stringId": 1374801127227052032,
            "femaleVariant": "それで、あたしにどうしてほしいの？",
            "maleVariant": "それで、俺にどうしてほしいんだ？"
          },
          {
            "stringId": 1374801457134227456,
            "femaleVariant": "1日一緒にいてくれたら、君が元々約束されていた報酬と同じ額を払う"
          },
          {
            "stringId": 1374801588600492032,
            "femaleVariant": "何それ―― そんな金ないでしょ？"
          },
          {
            "stringId": 1374801803298525184,
            "femaleVariant": "支払うのは私じゃない、スタジオだ"
          },
          {
            "stringId": 1374802162096066560,
            "femaleVariant": "どうもとらえどころがない話ね。帰るから降ろして",
            "maleVariant": "どうもとらえどころがない話だ。帰るから降ろせ"
          },
          {
            "stringId": 1374802162112843776,
            "femaleVariant": "引き受ける"
          },
          {
            "stringId": 1374802162112843792,
            "femaleVariant": "次の角で降ろしてくれる？",
            "maleVariant": "次の角で降ろしてくれるか？"
          },
          {
            "stringId": 1374802162129620992,
            "femaleVariant": "夜までだけでいいのね…？",
            "maleVariant": "夜までだけでいいんだな…？"
          },
          {
            "stringId": 1374802162146398220,
            "femaleVariant": "結局あたしに何をしてほしいの？",
            "maleVariant": "結局俺に何をしてほしいんだ？"
          },
          {
            "stringId": 1374802678230339584,
            "femaleVariant": "何をさせたいのか、まだよくわからない"
          },
          {
            "stringId": 1374803224110616576,
            "femaleVariant": "グロリア・エルアフマルの家につくわ"
          },
          {
            "stringId": 1374805886336028672,
            "femaleVariant": "ああ、そうだ"
          },
          {
            "stringId": 1374806005471039488,
            "femaleVariant": "誰も強要はしない"
          },
          {
            "stringId": 1374806142490562560,
            "femaleVariant": "本当にいいのか？ 考えてみろ、楽に稼げるぞ"
          },
          {
            "stringId": 1374806377354809344,
            "femaleVariant": "そばにいて、話をしてくれ。それだけだ"
          },
          {
            "stringId": 1386324526545817600,
            "femaleVariant": "おかしな真似をしたらすぐに降りるから",
            "maleVariant": "おかしな真似をしたらすぐに降りるぞ"
          },
          {
            "stringId": 1754758574545801216,
            "femaleVariant": "きちんと話せば、グロリアもわかってくれると思ったのに…"
          },
          {
            "stringId": 1754759797185417216,
            "femaleVariant": "そうすれば、きっと許してくれるんじゃないかって…"
          },
          {
            "stringId": 1772143330161823744,
            "femaleVariant": "少し時間をくれ…"
          },
          {
            "stringId": 1805121511547756544,
            "femaleVariant": "ああ、そっち系の話…",
            "maleVariant": "ああ、そっち系の話かよ…"
          },
          {
            "stringId": 1805121740103770112,
            "femaleVariant": "やっぱついてくるんじゃなかった…",
            "maleVariant": "やっぱついてくるんじゃなかったな…"
          },
          {
            "stringId": 1805236924935856128,
            "femaleVariant": "他の人々を闇の道から遠ざけることも"
          },
          {
            "stringId": 1805313140338483200,
            "femaleVariant": "あなたを殺す契約はまだ有効なのよ。実行しない理由がある？",
            "maleVariant": "あんたを殺す契約はまだ有効だ。実行しない理由があるか？"
          },
          {
            "stringId": 1805314352962101248,
            "femaleVariant": "俺だ。変な真似をしたらお前を殺す"
          },
          {
            "stringId": 1805314734945755136,
            "femaleVariant": "君もバスケスも、落ち着け。フィクサーの望みは満たされる。必ずな"
          },
          {
            "stringId": 1805317958083829760,
            "femaleVariant": "どうやって？",
            "maleVariant": "どうやってだ？"
          },
          {
            "stringId": 1805318077822820352,
            "femaleVariant": "信用しろ。必ず全員が満足する結果になる"
          },
          {
            "stringId": 1805410341890842624,
            "femaleVariant": "レイチェルと話せば？"
          },
          {
            "stringId": 1805410686511636480,
            "femaleVariant": "バスケスと話せば？"
          },
          {
            "stringId": 1805411707656888320,
            "femaleVariant": "一人が寂しいの？",
            "maleVariant": "一人が寂しいのか？"
          },
          {
            "stringId": 1805411928864481280,
            "femaleVariant": "レイチェルじゃ嫌なの？ おしゃべりは得意そうだけど",
            "maleVariant": "レイチェルじゃ嫌なのか？ おしゃべりは得意そうだが"
          },
          {
            "stringId": 1805412002835226624,
            "femaleVariant": "バスケスと話せばいい。犬のおやつでもやれば、話し相手くらいにはなってくれる"
          },
          {
            "stringId": 1805412100596064256,
            "femaleVariant": "一人が寂しいの？",
            "maleVariant": "一人が寂しいのか？"
          },
          {
            "stringId": 1805415443187916800,
            "femaleVariant": "彼女にとって私は、監督として売れるためのネタでしかない"
          },
          {
            "stringId": 1805416469970313216,
            "femaleVariant": "私は君と話がしたいんだ"
          },
          {
            "stringId": 1805417130321534976,
            "femaleVariant": "黙ってろ"
          },
          {
            "stringId": 1805417863167107072,
            "femaleVariant": "バスケスは私がウソつきで、電気椅子で焼かれるべきクズだとしか思ってない"
          },
          {
            "stringId": 1805419384927707136,
            "femaleVariant": "私は君と話がしたいんだ"
          },
          {
            "stringId": 1805419849052610560,
            "femaleVariant": "そのとおりだ"
          },
          {
            "stringId": 1805419905960927232,
            "femaleVariant": "私のそばにいてくれ"
          },
          {
            "stringId": 1811090004730470400,
            "femaleVariant": "あなたを殺す契約はまだ生きてる",
            "maleVariant": "あんたを殺す契約はまだ生きてる"
          },
          {
            "stringId": 1812492135626727428,
            "femaleVariant": "当然でしょ？",
            "maleVariant": "当然だろ？"
          },
          {
            "stringId": 1812496194135941120,
            "femaleVariant": "そろそろ食事にしましょ"
          },
          {
            "stringId": 1812508839962501120,
            "femaleVariant": "当然でしょ？ 息子を殺した奴を、簡単に許せるわけない",
            "maleVariant": "当然だろ？ 息子を殺した奴を、簡単に許せるわけない"
          },
          {
            "stringId": 1812508903984357376,
            "femaleVariant": "あたしだったらあなたを許してたかもしれない",
            "maleVariant": "俺だったらあんたを許してたかもしれない"
          },
          {
            "stringId": 1812508984900870144,
            "femaleVariant": "グロリアは許すべきだった"
          },
          {
            "stringId": 1812526066673672192,
            "femaleVariant": "どこで食べるの？",
            "maleVariant": "どこで食べるんだ？"
          },
          {
            "stringId": 1812529919594881024,
            "femaleVariant": "どこで食べるの？",
            "maleVariant": "どこで食べるんだ？"
          },
          {
            "stringId": 1812530472840355840,
            "femaleVariant": "パイZ"
          },
          {
            "stringId": 1812531461219704832,
            "femaleVariant": "あそこのコーヒーはうまいんだ"
          },
          {
            "stringId": 1812532582743040000,
            "femaleVariant": "ヨシュア、こうなるのはわかってたでしょ"
          },
          {
            "stringId": 1812551452816072704,
            "femaleVariant": "〈たが罪も、汝が許さば許され、汝が許さずば許されん。これこの世の理なり〉"
          },
          {
            "stringId": 1812609010662662144,
            "femaleVariant": "何言ってんだ？ 自分の息子を殺されたことがあんのか？"
          },
          {
            "stringId": 1812609436887834624,
            "femaleVariant": "黙って！"
          },
          {
            "stringId": 1812610089655754756,
            "femaleVariant": "本当に十字架に張り付けられてもいいの？",
            "maleVariant": "本当に十字架に張り付けられてもいいのか？"
          },
          {
            "stringId": 1812610929422524416,
            "femaleVariant": "パイZならおいしいチーズケーキがある"
          },
          {
            "stringId": 1812611067364794368,
            "femaleVariant": "十字架刑のブレインダンスなんて本気？",
            "maleVariant": "十字架刑のブレインダンスなんて本気か？"
          },
          {
            "stringId": 1812611555397230592,
            "femaleVariant": "本当に十字架に張り付けられてもいいの？",
            "maleVariant": "本当に十字架に張り付けられてもいいのか？"
          },
          {
            "stringId": 1812611622774530048,
            "femaleVariant": "本気で十字架刑のBDを作るつもり？",
            "maleVariant": "本気で十字架刑のBDを作るつもりか？"
          },
          {
            "stringId": 1812611690168606720,
            "femaleVariant": "パイZのチーズケーキは結構いける"
          },
          {
            "stringId": 1812612215916224512,
            "femaleVariant": "これは… もはや義務だ"
          },
          {
            "stringId": 1812619046004744192,
            "femaleVariant": "ああ"
          },
          {
            "stringId": 1812625832103403520,
            "femaleVariant": "ええ、本気よ"
          },
          {
            "stringId": 1812625944309424128,
            "femaleVariant": "絶対に炎上するでしょ…",
            "maleVariant": "絶対に炎上するだろ…"
          },
          {
            "stringId": 1812626139747213312,
            "femaleVariant": "覚悟の上よ"
          },
          {
            "stringId": 1812628354406834176,
            "femaleVariant": "私はこれで全世界を震撼させ、人々を集団的悪夢から覚ましたいと考えている"
          },
          {
            "stringId": 1812636581299249152,
            "femaleVariant": "乳糖不耐症でね"
          },
          {
            "stringId": 1812636780478357504,
            "femaleVariant": "なら、ヴィーガン向けのアボカドアイスは？"
          },
          {
            "stringId": 1833431425571381252,
            "femaleVariant": "ナイトシティを知り尽くすのは不可能",
            "maleVariant": "ナイトシティを知り尽くすのは不可能だ"
          },
          {
            "stringId": 1833432267233976320,
            "femaleVariant": "この街に詳しいのね？"
          },
          {
            "stringId": 1833433583926665216,
            "femaleVariant": "関係ないでしょ？",
            "maleVariant": "関係ないだろ？"
          },
          {
            "stringId": 1833434232483504128,
            "femaleVariant": "関係ないでしょ？",
            "maleVariant": "関係ないだろ？"
          },
          {
            "stringId": 1833434516186226688,
            "femaleVariant": "まあそうだけど、ここ結構気に入ってて"
          },
          {
            "stringId": 1833434680921710592,
            "femaleVariant": "ナイトシティを知り尽くすのは不可能よ。いつだって驚かされる。だから魅力的なの",
            "maleVariant": "ナイトシティを知り尽くすのは不可能だ。いつだって驚かされる。だから魅力的なんだ"
          },
          {
            "stringId": 1833436625266827264,
            "femaleVariant": "ふうん、確かにね"
          },
          {
            "stringId": 1857678486558760960,
            "femaleVariant": "ヨシュアと入って、テーブルを取っといて"
          },
          {
            "stringId": 1857680234023247872,
            "femaleVariant": "飛び入りゲストに話があるの"
          }
        ]
      }
    }
  }
}