  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ギャリー様を返して！",
                    "maleVariant": null,
                    "stringId": 1814352345054748672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ケンタウルス座アルファ星の使いが預言者様を拉致した！",
                    "maleVariant": null,
                    "stringId": 1814353839535595520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "預言者ギャリー様、お戻りください！",
                    "maleVariant": null,
                    "stringId": 1814354107736170496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ギャリーに何かあったの？",
                    "maleVariant": "ギャリーに何かあったのか？",
                    "stringId": 1814356304913932288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それで、彼をAVに放り込んで、母船まで飛んで行っちゃったの！",
                    "maleVariant": null,
                    "stringId": 1814357585837596672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "周りは見てただけ？",
                    "maleVariant": "周りは見てただけか？",
                    "stringId": 1814358508852912128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうよ！ 警察は彼が病気だって。知りすぎてて、危険な存在だと思われてたの！",
                    "maleVariant": null,
                    "stringId": 1814359203832307712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼の記念碑を作るために募金してるの。彼の勇気を… そしてケンタウルス座アルファ星の罪を忘れないようにね！",
                    "maleVariant": null,
                    "stringId": 1912517988035239936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "少しなら出せる",
                    "maleVariant": null,
                    "stringId": 1912520111208701956
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼のためならたっぷり出すよ",
                    "maleVariant": "あいつのためならたっぷり出す",
                    "stringId": 1912536983182417920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。ギャリーのためなら少し寄付する",
                    "maleVariant": null,
                    "stringId": 1912538115980042240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあ、そうね。ギャリーは献身的に活動してたし。気が変わる前にこれを受け取って",
                    "maleVariant": "まあ、そうだな。ギャリーは献身的に活動してたし。気が変わる前にこれを受け取ってくれ",
                    "stringId": 1912538142655815680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この変わったチップに見覚えは？",
                    "maleVariant": null,
                    "stringId": 1913897331563077632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "また今度",
                    "maleVariant": null,
                    "stringId": 1913902399037284352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "このチップなんだけど、見てもらえない？",
                    "maleVariant": "このチップなんだが、見てもらえないか？",
                    "stringId": 1913902625898799104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ありがとう。これで鼻の分にはなる",
                    "maleVariant": null,
                    "stringId": 1913908260375797760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何て言ったらいいのか… ギャリー様も喜ばれるわ！",
                    "maleVariant": null,
                    "stringId": 1913909000720785408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この謎のチップについて何か知ってる？",
                    "maleVariant": "この謎のチップについて何か知ってるか？",
                    "stringId": 1913909637164474372
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ギャリー様をしのんでぜひ寄付を！",
                    "maleVariant": null,
                    "stringId": 1913911958476857344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "警告したのに！ 青い目をした、黒いスーツの連中がやって来たの。言ってたとおりよ！",
                    "maleVariant": null,
                    "stringId": 1913913786069327872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあ… ギャリー様はあなたが熱心な信者だとおっしゃってたし…",
                    "maleVariant": null,
                    "stringId": 1913918723251675136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あなたも連中の仲間でしょ？",
                    "maleVariant": null,
                    "stringId": 1913920196442234880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう行かなきゃ。頑張って",
                    "maleVariant": "もう行かないと。頑張ってな",
                    "stringId": 1913921493505581056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと！ 早く隠して！ それは絶対に知られちゃダメ！",
                    "maleVariant": null,
                    "stringId": 1913934475228782592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "え、どうして？ これは何なの？",
                    "maleVariant": "え、どうして？ これは何なんだ？",
                    "stringId": 1913934764602204160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ギャリー様のおっしゃった通り、ついに連中が来た！ 私たちを人形みたいに操る計画が始まったのよ！",
                    "maleVariant": null,
                    "stringId": 1913935088821903360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと、落ち着いてよ！ なんたら魔術師のこと？",
                    "maleVariant": "おいおい、落ち着けって！ なんたら魔術師のことか？",
                    "stringId": 1913935548131745792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ギャリー様が言ってたの。「そして空は闇に包まれ、連中は誰にも気づかれずに地上に降り立つであろう」ってね！",
                    "maleVariant": null,
                    "stringId": 1913935988365893632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "知ってることを教えたら最後、顔を切り取って私のふりをするのよ！",
                    "maleVariant": null,
                    "stringId": 1925572571009105920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "見せて。わかる範囲で答えてみる",
                    "maleVariant": null,
                    "stringId": 1925585731359326208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "夜が来る… 終わりのない夜が！",
                    "maleVariant": null,
                    "stringId": 1925623277812432896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "止めようと思わなかったの？",
                    "maleVariant": "止めようと思わなかったのか？",
                    "stringId": 1925626219328159748
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "母船？",
                    "maleVariant": null,
                    "stringId": 1925626323481116672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたは、その母船とやらを見たの？",
                    "maleVariant": "あんたは、その母船とやらを見たのか？",
                    "stringId": 1925626431610273792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいえ、黒い霧を吹きつけられて、何も見えなかったから！ かわいそうに… みんなのために犠牲になったのよ！",
                    "maleVariant": null,
                    "stringId": 1925629712210313216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "追悼として、少し寄付してくれない？",
                    "maleVariant": null,
                    "stringId": 1925630543689138176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "えーと、まあまた別の機会にでも…",
                    "maleVariant": null,
                    "stringId": 1925648379144814592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ、頑張ってね",
                    "maleVariant": "じゃあ、頑張ってな",
                    "stringId": 1925648702475321344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お願い。ギャリー様は、相応しい功績を残された",
                    "maleVariant": null,
                    "stringId": 1925652220842065920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほんとにありがとう！ みんなに、世界に真実を広めてね！",
                    "maleVariant": null,
                    "stringId": 1925661735251591168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やっと終わったか。超自然的な存在の秘密を探るべく、随分長々と話をしていたが、収穫はあったか？",
                    "maleVariant": null,
                    "stringId": 1925810091273732096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "宇宙人が世界を支配してる",
                    "maleVariant": null,
                    "stringId": 1925820734773116932
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オチのない話はつまらない",
                    "maleVariant": null,
                    "stringId": 1925820816947920896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この世は宇宙の果てから来た宇宙人に支配されてて、あたしたちにはどうしようもないって事実がわかった",
                    "maleVariant": "この世は宇宙の果てから来た宇宙人に支配されてて、俺たちにはどうしようもないって事実がわかった",
                    "stringId": 1925820936854683648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オチのない話はつまらない",
                    "maleVariant": null,
                    "stringId": 1925821024465305600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一部の連中は人より物事を知ってる。偉いさんの個人的な会話を聞いたりしてな",
                    "maleVariant": null,
                    "stringId": 1925821108636598272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあな。だが俺は別のことを考えてた",
                    "maleVariant": null,
                    "stringId": 1925821496878153728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "面白い話だが、そういうことじゃねえ",
                    "maleVariant": null,
                    "stringId": 1925834178154971136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その情報をどうすると思う？ おとぎ話のフォーマットに落とし込んで、人心掌握に利用するんだよ",
                    "maleVariant": null,
                    "stringId": 1925865196459114496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "宇宙人のほうを信じたい",
                    "maleVariant": null,
                    "stringId": 1925865485715095556
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "確かに。時間の無駄だった",
                    "maleVariant": null,
                    "stringId": 1925865579415846912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "想像力ってものがないの？ 吸血鬼や宇宙人との戦争をかろうじて回避したって話のほうがいいでしょ",
                    "maleVariant": "想像力ってものがないのか？ 吸血鬼や宇宙人との戦争をかろうじて回避したって話のほうが面白い",
                    "stringId": 1925865651037782016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "確かに。完全に時間を無駄にしたね。くだらない作り話を必死に追いかけて…",
                    "maleVariant": "確かに。完全に時間を無駄にしたな。くだらない作り話を必死に追いかけて…",
                    "stringId": 1925865684575436800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その一方で、真実は残酷なほどつまらない。糸を引いてるのは企業だ。ひねくれた連中が権力の夢に酔いしれてるんだ",
                    "maleVariant": null,
                    "stringId": 1925865725025304576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺だって嫌いじゃないぜ？ 吸血鬼、宇宙人、爬虫類人… どれも興味をそそるし、ロマンチックですらある",
                    "maleVariant": null,
                    "stringId": 1925866001127948288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ、あれはコーポの目くらましだったってこと？",
                    "maleVariant": "じゃあ、あれはコーポの目くらましだったってことか？",
                    "stringId": 1925866725249368064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "間違いない。この世界で一番がめついのは、いつだってあいつらだ",
                    "maleVariant": null,
                    "stringId": 1925867087167471616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、でもくだらない話は魅力的だし、連中もそれを知ってる。夢に溢れる世界で頭をいっぱいにしとけば不満がでないだろ",
                    "maleVariant": null,
                    "stringId": 1925878081243561984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今は持ち合わせがない",
                    "maleVariant": null,
                    "stringId": 1945909127391911936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "悪いけど、今はお金がなくて",
                    "maleVariant": "悪いが、今はカネがなくて",
                    "stringId": 1945909325111402496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "かわいそうなギャリー様… 忘れ去られてしまう！",
                    "maleVariant": null,
                    "stringId": 1945909848979972096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ギャリーはどこ？",
                    "maleVariant": "ギャリーはどこに？",
                    "stringId": 1995483818980712452
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "このチップについて何か知ってる？",
                    "maleVariant": "このチップについて何か知ってるか？",
                    "stringId": 2003814750667653120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[謎のチップを見せる]",
                    "maleVariant": null,
                    "stringId": 2222782551538442240
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}