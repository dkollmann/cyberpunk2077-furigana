    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1924232964972453888,
            "femaleVariant": "あんたが運転手か。Vだな"
          },
          {
            "stringId": 1924234534665560068,
            "femaleVariant": "アロイス・ダカンね？",
            "maleVariant": "アロイス・ダカンだな？"
          },
          {
            "stringId": 1924235094856802304,
            "femaleVariant": "護衛よ。運転手じゃない",
            "maleVariant": "護衛だ。運転手じゃない"
          },
          {
            "stringId": 1924235349887262720,
            "femaleVariant": "アロイス・ダカンね？",
            "maleVariant": "アロイス・ダカンだな？"
          },
          {
            "stringId": 1924235539536912388,
            "femaleVariant": "護衛よ。運転手じゃない。あとは正解",
            "maleVariant": "護衛だ。運転手じゃない。あとは正解だ"
          },
          {
            "stringId": 1924235793745289216,
            "femaleVariant": "へえ、大したご観察眼だな。恐れ入ったよ。俺がアロイス・ダカンだ"
          },
          {
            "stringId": 1924237243313528832,
            "femaleVariant": "何でもいいさ。とにかく、このゴミ溜めから連れ出してもらえればな"
          },
          {
            "stringId": 1924237708008857604,
            "femaleVariant": "ええ、行きましょ",
            "maleVariant": "ああ、行こう"
          },
          {
            "stringId": 1924238300848562176,
            "femaleVariant": "行きましょ",
            "maleVariant": "よし行こう"
          },
          {
            "stringId": 1924239166519353344,
            "femaleVariant": "よし、わかった"
          },
          {
            "stringId": 1924240389242855424,
            "femaleVariant": "あばよナイトシティ… 未練はない"
          },
          {
            "stringId": 1924241776987054080,
            "femaleVariant": "あそこだ。そこでノーマッドと落ち合う。停めてくれ"
          },
          {
            "stringId": 1924243077405843468,
            "femaleVariant": "もうすぐラフェンの連中が来る。お前の姿を見られるとマズい"
          },
          {
            "stringId": 1937396216055537664,
            "femaleVariant": "ラフェン？ あんな連中と一緒に行くつもり？",
            "maleVariant": "ラフェン？ あんな連中と一緒に行くつもりか？"
          },
          {
            "stringId": 1937396770039209984,
            "femaleVariant": "まだいる気か？ あんたの仕事は終わりだ"
          },
          {
            "stringId": 1938515301040463872,
            "femaleVariant": "そういうんじゃ… まあ、超ハイリスクな投資の穴埋めに借りた、タチの悪い借金ってことにしとくよ"
          },
          {
            "stringId": 1938516868636418048,
            "femaleVariant": "試験的なサプリメントを流通させてた。バックにいたのはナイト・コープの人道支援部門だ"
          },
          {
            "stringId": 1938517696407478272,
            "femaleVariant": "サプリメント？ つまり…"
          },
          {
            "stringId": 1938517837034102784,
            "femaleVariant": "ビタミンじゃないぞ。それに、今となっちゃどうでもいい。それがオオゴケして、俺は借金まみれになっちまったわけだ"
          },
          {
            "stringId": 1938519297171988480,
            "femaleVariant": "ナイトシティは悪魔だ！ 言葉巧みに夢見る俺たちを誘い込み、チャンスを鼻先にぶら下げて引き付けておきながら… 最後にゃ、すべてをむしり取っていきやがる"
          },
          {
            "stringId": 1938528316167102464,
            "femaleVariant": "もったいぶるのはよして、ダカン",
            "maleVariant": "もったいぶるのはよせ、ダカン"
          },
          {
            "stringId": 1938528549454290944,
            "femaleVariant": "何から逃げ出すの？",
            "maleVariant": "何から逃げ出すんだ？"
          },
          {
            "stringId": 1938576812890988544,
            "femaleVariant": "街に残ってるよりマシだ。心配には及ばない"
          },
          {
            "stringId": 1938681630376665088,
            "femaleVariant": "ダカン… そんなこと言ったら、涙がこぼれちゃうじゃない",
            "maleVariant": "ダカン… そんなこと言ったら、涙がこぼれちまうだろ"
          },
          {
            "stringId": 1938682867075588096,
            "femaleVariant": "とにかく… 早いとこ連れ出してくれ"
          },
          {
            "stringId": 1938689334809796608,
            "femaleVariant": "近くに車を停めてる。行けるか？"
          }
        ]
      }
    }
  }
}