              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なあ、昨日入ってきたあいつはどうだ？ ワンワン泣いてたか？",
                    "maleVariant": null,
                    "stringId": 2168024955226607616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "弁護士が言ってた。最低でも10年の実刑、仮釈放はナシだとさ",
                    "maleVariant": null,
                    "stringId": 2211118142550925312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "10年？ たかがケンカでか…？",
                    "maleVariant": null,
                    "stringId": 2212855350437830656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ただのケンカじゃない。コーポの子供をボコボコにしたんだ。ありとあらゆる罪状で起訴された。しばらく陽の光は拝めないだろうな",
                    "maleVariant": null,
                    "stringId": 2212855557066022912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なるほど… 少し気をつけた方がいいな。コーポ嫌いの英雄気取りが助けにくるかもしれない",
                    "maleVariant": null,
                    "stringId": 2212856433155465216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハッ、地面にはいつくばっていじけてやがる。メシにも手をつけてない",
                    "maleVariant": null,
                    "stringId": 2212857128621400064
                  }
                }
              ]
