  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナム",
              "maleVariant": "",
              "stringId": "1512452623816089600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サスペンションの具合を見てたんだ。荒れた道を行くからね、油圧の調整をしとかないと",
              "maleVariant": "",
              "stringId": "1512453644088926208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "作戦はまとまった？",
              "maleVariant": "作戦はまとまったか？",
              "stringId": "1512454231358595076"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムはちゃんと休めたの？",
              "maleVariant": "パナムはちゃんと休めたのか？",
              "stringId": "1512455208597872640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手伝おうか？",
              "maleVariant": "",
              "stringId": "1512456170301448192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、もう終わるから。ラフェンの奴ら、この子を滅茶苦茶にしやがって…",
              "maleVariant": "",
              "stringId": "1523163298707271680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、乗りなよ",
              "maleVariant": "",
              "stringId": "1528477524086747136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、車に乗りなって！ 凍えちゃうよ…",
              "maleVariant": "",
              "stringId": "1528477996986134528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "乗って。寒いから早く",
              "maleVariant": "",
              "stringId": "1528607397304332288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よく眠れた？",
              "maleVariant": "",
              "stringId": "1539042075245015040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "準備はいい？",
              "maleVariant": "",
              "stringId": "1547526871483731968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあね",
              "maleVariant": "まあな",
              "stringId": "1547604425691418624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムはちゃんと休めたの？",
              "maleVariant": "パナムはちゃんと休めたのか？",
              "stringId": "1547610121220706304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたが何度か叫ぶから… 悪夢でも見た？",
              "maleVariant": "",
              "stringId": "1547610310450925568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろん",
              "maleVariant": "",
              "stringId": "1551827442671865856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘルマンは生け捕りにしたい。AVを木っ端みじんにするのはナシよ？ いい？",
              "maleVariant": "ヘルマンは生け捕りにしたい。AVを木っ端みじんにするのはナシだ。いいな",
              "stringId": "1675126255519395840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってるって。でもシートベルトはしてて欲しいね。これがうまくいくと… 間違いなく荒い着陸になるから",
              "maleVariant": "",
              "stringId": "1675126255519395852"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイクロ波発電所？",
              "maleVariant": "",
              "stringId": "1689356933433782272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんでAVをその場所で落とすの？",
              "maleVariant": "なぜAVをその場所で落とすんだ？",
              "stringId": "1689356933433782288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんでAVをその場所で落とすの？",
              "maleVariant": "なんでAVをその場所で落とすんだ？",
              "stringId": "1689356933500891136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "墜落させた後は、素早く場所を特定して、目標を回収する必要がある",
              "maleVariant": "",
              "stringId": "1689356933517668356"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マイクロ波… 発電所？",
              "maleVariant": "",
              "stringId": "1689356933718994944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それにここはカリフォルニアの典型的な砂漠地帯――平坦で周りには何もない。恰好の場所でしょ",
              "maleVariant": "",
              "stringId": "1689382094224617472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "施設でEMPを発生させ、AVを落とす",
              "maleVariant": "",
              "stringId": "1689385219182424064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AVが射程に入ったタイミングでEMPを発生させれば、機内の電子機器類は使い物にならなくなる",
              "maleVariant": "",
              "stringId": "1689387793629442048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほどね",
              "maleVariant": "なるほどな",
              "stringId": "1689388127294713856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それでエンジンも止まるの？",
              "maleVariant": "それでエンジンも止まるのか？",
              "stringId": "1689389486098214912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エンジンだけじゃなく、操縦、ナビ、通信システム、すべてが停止する",
              "maleVariant": "",
              "stringId": "1689389879876251648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[離れる] わかった、仕事にかかりましょ",
              "maleVariant": "[離れる] わかった、仕事にかかろう",
              "stringId": "1689393241157922820"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AVが発電所の上を通過するとは限らないんじゃない？",
              "maleVariant": "AVが発電所の上を通過するとは限らないんじゃないか？",
              "stringId": "1689393641026088960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V",
              "maleVariant": "",
              "stringId": "1689465749282353152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウソでしょ？",
              "maleVariant": "ウソだろ？",
              "stringId": "1689473120704524288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、気にしなくていいよ。そういうのには慣れてるから。ミッチとスコーピオンも、戦争から戻った時は数か月間、毎晩うなされてた",
              "maleVariant": "",
              "stringId": "1689477779603189760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、出発しましょ",
              "maleVariant": "わかった、出発しよう",
              "stringId": "1689543672756563968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう簡単にいく？ AVが発電所の上を通過するとは限らないでしょ",
              "maleVariant": "そう簡単にいくか？ AVが発電所の上を通過するとは限らないだろ",
              "stringId": "1689544924252991488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そ。そこに侵入する",
              "maleVariant": "",
              "stringId": "1689561976481116160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "侵入はいいけど、その後は？",
              "maleVariant": "侵入はいいが、その後は？",
              "stringId": "1689566232172949504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EMPを利用するつもり？",
              "maleVariant": "EMPを利用するつもりか？",
              "stringId": "1689574763991265280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "発電所？ まさか、EMPを使う気？",
              "maleVariant": "発電所？ まさか、EMPを使う気か？",
              "stringId": "1689575747354226688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ご名答",
              "maleVariant": "",
              "stringId": "1689576180458057728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも、どうやって？ そんな都合よくパルスを飛ばせないでしょ",
              "maleVariant": "だが、どうやって？ そんな都合よくパルスを飛ばせないだろ",
              "stringId": "1689576671258734592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私に任せなって",
              "maleVariant": "",
              "stringId": "1689577557095739392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "電力集積ユニットの制御をオーバーライドして、全てのリミッターを解除する",
              "maleVariant": "",
              "stringId": "1689578809330364416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AVが射程に入ったタイミングでEMPを発生させれば、AVのシステムは使い物にならなくなる",
              "maleVariant": "",
              "stringId": "1689580163251695616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまり操縦、ナビシステムも…",
              "maleVariant": "",
              "stringId": "1689580512234565632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…通信も停止する。連中はなす術もなく、地面に向かって真っ逆さまってわけ",
              "maleVariant": "",
              "stringId": "1689581211609591808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナムはちゃんと休めた？",
              "maleVariant": "パナムはちゃんと休めたか？",
              "stringId": "1692119039451197440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "赤ちゃんみたいに、ぐっすり",
              "maleVariant": "",
              "stringId": "1692119838952648704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさか、車のマシンガンで？",
              "maleVariant": "",
              "stringId": "1692129866157780992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あれは別のことに使う。あの程度の口径じゃ、こいつの重装甲は破れない",
              "maleVariant": "",
              "stringId": "1692130243527700480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこで登場するのが、このクランプ――マイクロ波発電所。これでAVを落とす",
              "maleVariant": "",
              "stringId": "1692130495437598720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大変な作戦になるとは思うけど、後悔はさせない。すべて計算済みだから",
              "maleVariant": "",
              "stringId": "1692173873382551552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあね。でもそれは関係ないんだ。まあ見てて",
              "maleVariant": "",
              "stringId": "1719592578722689024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナム",
              "maleVariant": "",
              "stringId": "1755958112958263300"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このレンチが私たち。カッターがAVで、ドライバーはAV墜落予想地点",
              "maleVariant": "",
              "stringId": "1755991070406586388"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、墜落させる方法なんだけど",
              "maleVariant": "",
              "stringId": "1755991070406586404"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちよ",
              "maleVariant": "",
              "stringId": "1755993001967464448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話を聞きたくないの？",
              "maleVariant": "",
              "stringId": "1755993718203592704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？ 時間がないんじゃなかったの？",
              "maleVariant": "",
              "stringId": "1755993769038557184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手伝おうか？",
              "maleVariant": "",
              "stringId": "1756016306829111296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘルマンの件だけど、作戦はまとまった？",
              "maleVariant": "ヘルマンの件だが、作戦はまとまったか？",
              "stringId": "1761870150437040128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うん、バッチリ",
              "maleVariant": "",
              "stringId": "1761870667410173952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "きっとそのうち収まる",
              "maleVariant": "",
              "stringId": "1764686089490657280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だといいんだけど",
              "maleVariant": "だといいんだがな",
              "stringId": "1764686362791505920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[パナムに近寄る] 聞かせて",
              "maleVariant": "[パナムに近寄る] 聞かせてくれ",
              "stringId": "1766037487967776772"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞かせて",
              "maleVariant": "聞かせてくれ",
              "stringId": "1766040499511603200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう一度説明して",
              "maleVariant": "もう一度説明してくれ",
              "stringId": "1790747127449251840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう一度いい？ レンチがあたしたちよね？",
              "maleVariant": "もう一度いいか？ レンチが俺たちだよな？",
              "stringId": "1790773519066984448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう。カッターがAV。ドライバーがAVを墜落させる地点",
              "maleVariant": "",
              "stringId": "1790774067178631168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、クランプがマイクロ波発電所。ここを使ってAVを引きずり落とすってわけ",
              "maleVariant": "",
              "stringId": "1790783562948669440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？ 聞いてる？",
              "maleVariant": "",
              "stringId": "1824162883682414592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何か意見ある？",
              "maleVariant": "",
              "stringId": "1824162933980508160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘルマンは生け捕りにするんだからね",
              "maleVariant": "ヘルマンは生け捕りにするんだぞ",
              "stringId": "1976311684333694976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、約束通り、夜にまた落ち合おう",
              "maleVariant": "",
              "stringId": "1986286032310104064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[パナムを待つ]",
              "maleVariant": "",
              "stringId": "2019869092333117444"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[乗る]",
              "maleVariant": "",
              "stringId": "2373619777174134788"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}