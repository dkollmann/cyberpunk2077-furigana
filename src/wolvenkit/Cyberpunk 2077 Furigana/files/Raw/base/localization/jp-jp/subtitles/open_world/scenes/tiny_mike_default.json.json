        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "力になれることがあったら言って",
                    "maleVariant": "力になれることがあったら言ってくれ",
                    "stringId": 1953435230038257668
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もし手を貸してほしいことがあったら、あたしに言いなよ",
                    "maleVariant": "もし手を貸してほしいことがあったら、俺に言えよな",
                    "stringId": 1953467192782221312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんまり気取んなって",
                    "maleVariant": null,
                    "stringId": 1953470293966712832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "傭兵ってのは何があるかわからねえ",
                    "maleVariant": null,
                    "stringId": 1953474289712361472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "次はお前がピンチの時に俺が助けてやるさ",
                    "maleVariant": null,
                    "stringId": 1953476366865920000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あくまで儲けありきの話でしょ？",
                    "maleVariant": "あくまで儲けありきの話だろ？",
                    "stringId": 1953476534117986304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺とレジーナはお前に恩がある。しばらくは足をむけて眠れない",
                    "maleVariant": null,
                    "stringId": 1953481437376802816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたの兄さんに会った",
                    "maleVariant": null,
                    "stringId": 1979390421684817920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そういえば、ビッグ・ピートに会ったよ。あんたたち2人とも、厄介ごとには事欠かないね",
                    "maleVariant": "そういえば、ビッグ・ピートに会ったぞ。お前ら2人とも、厄介ごとには事欠かないな",
                    "stringId": 1979391082170257408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "助けてやったらしいな。家族全員の緊急連絡先にお前を登録しとくか",
                    "maleVariant": null,
                    "stringId": 1979423195758170112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うちの母ちゃんにもしもの時があったら頼むわ",
                    "maleVariant": null,
                    "stringId": 1979423728585773056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、その時は家族割引してあげる",
                    "maleVariant": "ああ、その時は家族割引してやるよ",
                    "stringId": 1979424493073174528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "腕の具合はどう？",
                    "maleVariant": "腕の具合はどうだ？",
                    "stringId": 1979426136971907072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おい、V！ こっちで話そう！",
                    "maleVariant": null,
                    "stringId": 1979426523921616896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[飲む] 先に1杯飲ませて",
                    "maleVariant": "[飲む] 先に1杯飲ませてくれ",
                    "stringId": 1979429885924708352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、でも喉がカラカラ。先に1杯飲ませて",
                    "maleVariant": "ああ、でも喉がからからだ。先に1杯飲ませろ",
                    "stringId": 1979430827478847488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もちろんだ",
                    "maleVariant": null,
                    "stringId": 1979431584617828352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ははは、当然！",
                    "maleVariant": null,
                    "stringId": 1979434755880751104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "腕の具合はどう？",
                    "maleVariant": "腕の具合はどうだ？",
                    "stringId": 1979435639368945664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、お前が天使に見えてきたぜ",
                    "maleVariant": null,
                    "stringId": 1979436616373342208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "タイニー、真面目な話よ",
                    "maleVariant": "タイニー、真面目な話だ",
                    "stringId": 1979436965876305920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前とレジーナにツケを払った後でも、こいつを手に入れるぐらいの金はまだあった",
                    "maleVariant": null,
                    "stringId": 1979437006141624320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だからまあ、腕なら平気だ",
                    "maleVariant": null,
                    "stringId": 1979454178729496576
                  }
                }
              ]
            }
          }
        }
      }
    },
