    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "カメ！？ これって何かの冗談？",
                    "maleVariant": "カメ！？ これって何かの冗談か？",
                    "stringId": 1773708994622418944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バリーを放っておくのはまずい",
                    "maleVariant": null,
                    "stringId": 1773710302960054276
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "肩入れしすぎじゃない？",
                    "maleVariant": "肩入れしすぎじゃないか？",
                    "stringId": 1773710554668625920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バリーのことは放っておけない。カメが唯一の生きる理由だったんだとしたら…",
                    "maleVariant": null,
                    "stringId": 1773710833354960896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと肩入れしすぎじゃない？ そりゃ大切な存在だったってのはわかるけど、だからって何かするとは思えない",
                    "maleVariant": "ちょっと肩入れしすぎじゃないか？ そりゃ大切な存在だったってのはわかるが、だからって何かするとは思えない",
                    "stringId": 1773711948939161600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前が事実を無視するのは勝手だが、そうしたところで問題は消えてくれやしないぞ",
                    "maleVariant": null,
                    "stringId": 1773712301646573568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺には、バリーが冗談を言うような野郎には見えなかったがな",
                    "maleVariant": null,
                    "stringId": 1841757026116157440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やっとわかってきたか",
                    "maleVariant": null,
                    "stringId": 1841764984019468288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "友達に知らせなきゃ。手遅れじゃないといいけど",
                    "maleVariant": "友達に知らせよう。手遅れじゃないといいが",
                    "stringId": 1841765087266455552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺なら奴の仲間に教えてやるな。カメよりも些細なことが引き金となって起きた悲劇は山ほどある",
                    "maleVariant": null,
                    "stringId": 1841780719051587584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも、カメって…？ バリーは、唯一腹を割って話せる友達だとは言ってたけど…",
                    "maleVariant": null,
                    "stringId": 1896593619611795456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "納骨堂に金を払って、たまにお参りする理由としては十分じゃないか？",
                    "maleVariant": null,
                    "stringId": 1896593710628192256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかるだろ。奈落の底をずっと見つめてると、そのうち飲み込まれちまうんだよ",
                    "maleVariant": null,
                    "stringId": 1896621386508255232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "奴のこれまでの人生を考えりゃ、奈落に一歩踏み込むくらい大したことないさ",
                    "maleVariant": null,
                    "stringId": 1896621485242171392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "普通じゃない",
                    "maleVariant": null,
                    "stringId": 1967909408004096004
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "確かにそうね…",
                    "maleVariant": "確かにそうだな…",
                    "stringId": 1967909505781710848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "話し相手が欲しいがために、大金をはたいてカメを世話してたのよ？",
                    "maleVariant": "話し相手が欲しいがために、大金をはたいてカメを世話してたんだぞ？",
                    "stringId": 1967909587251871744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "確かに… あんたの言うとおりかもね",
                    "maleVariant": "確かに… お前の言うとおりかもな",
                    "stringId": 1967909895147339776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もしあいつが自分だったらと考えてみろ。大好きなばあちゃんはずっと前にあの世に旅立ち、他には誰もいなくて、唯一残されたのはカメだった",
                    "maleVariant": null,
                    "stringId": 1967910003075170304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それが普通に聞こえる？",
                    "maleVariant": "それが普通に聞こえるか？",
                    "stringId": 1967910338636267520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼はそれだけ追い詰められていた… 元同僚も話を聞いてくれなくて、どうしようもなかったのね",
                    "maleVariant": "あいつはそれだけ追い詰められていた… 元同僚も話を聞いてくれなくて、どうしようもなかったのか",
                    "stringId": 1967910609235984384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だが、ばあちゃんが人生についていろんな話をしてくれた時、カメはそこにいた。つらい経験をした時、ばあちゃんがくたばった時も、カメはそこにいたんだ",
                    "maleVariant": null,
                    "stringId": 1967910959795912704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ペットに納骨堂を買うほどだ、あの野郎はよっぽど孤独だったに違いない",
                    "maleVariant": null,
                    "stringId": 1967911212225904640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんな、大げさな…",
                    "maleVariant": null,
                    "stringId": 1967982955560943616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バリーを放っておくのはまずい",
                    "maleVariant": null,
                    "stringId": 2412963282462720000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "カメ！？ 冗談でしょ？",
                    "maleVariant": "カメ！？ 冗談だろ？",
                    "stringId": 2412964742667714560
                  }
                }
              ]
            }
          }
        }
      }
    },
