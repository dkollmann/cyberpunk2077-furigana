    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1154762624163786752,
            "femaleVariant": "仕事は進んでるか、ミスV？",
            "maleVariant": "仕事は進んでるか、ミスターV？"
          },
          {
            "stringId": 1215565923732811776,
            "femaleVariant": "紺碧プラザのBDを手に入れた"
          },
          {
            "stringId": 1215566598948646912,
            "femaleVariant": "エヴリン・パーカーと会った"
          },
          {
            "stringId": 1215568661657022464,
            "femaleVariant": "そうね。なかなか面白い話を聞けたけど…",
            "maleVariant": "そうだな。なかなか面白い話を聞けたが…"
          },
          {
            "stringId": 1215570546191364096,
            "femaleVariant": "アフターライフで落ち合おう。続きはそこで"
          },
          {
            "stringId": 1215590451166965760,
            "femaleVariant": "何か問題でも？"
          },
          {
            "stringId": 1215590970522464256,
            "femaleVariant": "エヴリンには色々驚かされた"
          },
          {
            "stringId": 1215594362170896384,
            "femaleVariant": "彼女はヨリノブと寝てる"
          },
          {
            "stringId": 1215597704108437504,
            "femaleVariant": "エヴリンにはいろいろ驚かされた"
          },
          {
            "stringId": 1215598318657859584,
            "femaleVariant": "だろうな"
          },
          {
            "stringId": 1215599321616609280,
            "femaleVariant": "秘密めいた女なのさ。この街で力を持つ者は皆そうだ"
          },
          {
            "stringId": 1215600472198082560,
            "femaleVariant": "問題は、奴の情報が使えるかどうかだ"
          },
          {
            "stringId": 1215614221579579408,
            "femaleVariant": "彼女はヨリノブ・アラサカと寝てた。でもそれだけじゃない… 目標のチップは奴が持ってる"
          },
          {
            "stringId": 1216693981575507968,
            "femaleVariant": "ほう、ヨリノブとエヴリンは仲良しこよしか。しかしアラサカのドラ息子は、よほどの構ってちゃんらしい。今度は一体、誰に喧嘩を売るつもりなんだか"
          },
          {
            "stringId": 1216694658536173568,
            "femaleVariant": "何がだ？"
          },
          {
            "stringId": 1216695553315434496,
            "femaleVariant": "それを聞いて気にならないの？",
            "maleVariant": "それを聞いて気にならないのか？"
          },
          {
            "stringId": 1216769627995631616,
            "femaleVariant": "ナイトシティのゴシップを嗅ぎ回るのは、メディアの連中に任せておけばいい"
          },
          {
            "stringId": 1216773214511427584,
            "femaleVariant": "俺たちは、俺たちの仕事に集中するだけだ"
          },
          {
            "stringId": 1451452170718801920,
            "femaleVariant": "わかった。すぐに向かう"
          },
          {
            "stringId": 1631817613672312832,
            "femaleVariant": "ではまた後で"
          },
          {
            "stringId": 1638588265817305088,
            "femaleVariant": "エヴリンのBDは見る価値があった"
          },
          {
            "stringId": 1638588265817305100,
            "femaleVariant": "そいつは素晴らしい。バグも作業に取り掛かるそうだ"
          },
          {
            "stringId": 1709714750776762376,
            "femaleVariant": "ボットを手に入れた"
          },
          {
            "stringId": 1709714750843871232,
            "femaleVariant": "ミリテクの協力なしで進めた。危険だと思ったから",
            "maleVariant": "ミリテクの協力なしで進めた。危険だと思ったんでね"
          },
          {
            "stringId": 1709714750860648448,
            "femaleVariant": "スタウトとは会った"
          },
          {
            "stringId": 1709714750877425664,
            "femaleVariant": "メレディス・スタウトは気の毒だな。失敗したデートのツケまで払わされるのか"
          },
          {
            "stringId": 1709714750894202880,
            "femaleVariant": "スタウトと会って、なんとか取引できた"
          },
          {
            "stringId": 1709714750927757312,
            "femaleVariant": "大した度胸だ"
          },
          {
            "stringId": 1709714750961311744,
            "femaleVariant": "でもチップにはウイルスが仕込まれてたのよ",
            "maleVariant": "だが、チップにはウイルスが仕込まれてたんだ"
          },
          {
            "stringId": 1709714750994866176,
            "femaleVariant": "でもあの女は、チップにウイルスを仕込んでやがったのよ",
            "maleVariant": "だがあの女は、チップにウイルスを仕込んでやがったんだ"
          },
          {
            "stringId": 1709714751028420608,
            "femaleVariant": "それをロイスに警告して、取引したってわけ",
            "maleVariant": "それをロイスに警告して、取引をした"
          },
          {
            "stringId": 1709714751045197824,
            "femaleVariant": "ウイルスはメイルストロームのシステムを攻撃して数人を焼き払い、残りはあたしが片付けた",
            "maleVariant": "ウイルスはメイルストロームのシステムを攻撃して数人を焼き払い、残りは俺が片付けた"
          },
          {
            "stringId": 1709714751112306688,
            "femaleVariant": "いいえ、代金は自分で出した",
            "maleVariant": "いや、代金は自分で出した"
          },
          {
            "stringId": 1709714751145861120,
            "femaleVariant": "あたしが知ってる唯一の方法――力ずくで",
            "maleVariant": "俺が知ってる唯一の方法――力ずくで"
          },
          {
            "stringId": 1709714751229747200,
            "femaleVariant": "それが代金を支払い済みだって言っても、ロイスは聞く耳を持たなくて、大変だった"
          },
          {
            "stringId": 1709714751229747208,
            "femaleVariant": "ではどうしたんだ？ コーポの女を利用したのか？"
          },
          {
            "stringId": 1709714751280078848,
            "femaleVariant": "企業の金でボットを買い取ったのか"
          },
          {
            "stringId": 1709767130352410624,
            "femaleVariant": "フラットヘッドを手に入れた"
          },
          {
            "stringId": 1709830280498077696,
            "femaleVariant": "やっぱりなんでもない"
          },
          {
            "stringId": 1709830665166086144,
            "femaleVariant": "やっぱり、なんでもない"
          },
          {
            "stringId": 1709835583440510976,
            "femaleVariant": "準備万端よ。次は？",
            "maleVariant": "準備万端だ。次は？"
          },
          {
            "stringId": 1709837325318184960,
            "femaleVariant": "準備は済んだ。で、次は？"
          },
          {
            "stringId": 1710018392968880128,
            "femaleVariant": "エヴリン・パーカーとは会ったのか？"
          },
          {
            "stringId": 1710019229933854720,
            "femaleVariant": "まだこれからよ",
            "maleVariant": "まだこれからだ"
          },
          {
            "stringId": 1710019622822699008,
            "femaleVariant": "フラットヘッドの件は？"
          },
          {
            "stringId": 1710019622822699016,
            "femaleVariant": "今から取り掛かるところよ",
            "maleVariant": "今から取り掛かるところだ"
          },
          {
            "stringId": 1715339194024980480,
            "femaleVariant": "そうか"
          },
          {
            "stringId": 1715342502106099712,
            "femaleVariant": "T-バグはそいつがなきゃチップは入手できんと言ってる。頼んだぞ"
          },
          {
            "stringId": 1715356835552817152,
            "femaleVariant": "ボットの支払い用にクレジットをくれたの",
            "maleVariant": "ボットの支払い用にクレジットをくれた"
          },
          {
            "stringId": 1715358211838177280,
            "femaleVariant": "ほう"
          },
          {
            "stringId": 1715358902119952384,
            "femaleVariant": "それで？"
          },
          {
            "stringId": 1715359223437193216,
            "femaleVariant": "相性が悪かったみたい",
            "maleVariant": "相性が悪かったみたいだ"
          },
          {
            "stringId": 1715366004083257344,
            "femaleVariant": "いや、それは自分で使おうと思って",
            "maleVariant": "いや、それは自分で使おうと思ってな"
          },
          {
            "stringId": 1715382763565740032,
            "femaleVariant": "企業のやりそうなことだ。ロイスは怒り心頭だったろうな"
          },
          {
            "stringId": 1715392757048668160,
            "femaleVariant": "そうか。パーカーと会わないことには始まらないからな"
          },
          {
            "stringId": 1783643374451879936,
            "femaleVariant": "ほう。問題なくいったのか？ それと、ミリテクはどうした？"
          },
          {
            "stringId": 1783738871623643136,
            "femaleVariant": "ミリテクの金でボットを買い取ったのか？"
          },
          {
            "stringId": 1783739671880069120,
            "femaleVariant": "ミリテクの金は、スキャンしてからじゃないと使う気になれない"
          },
          {
            "stringId": 1792362895505289216,
            "femaleVariant": "代金は自分で出した"
          },
          {
            "stringId": 1792365460724838400,
            "femaleVariant": "あのボットは安くなかっただろう"
          },
          {
            "stringId": 1792365962447482880,
            "femaleVariant": "まあ、そうね",
            "maleVariant": "まあ、そうだな"
          },
          {
            "stringId": 1792385695590838272,
            "femaleVariant": "ボットは譲ってもらった"
          },
          {
            "stringId": 1792387975950036992,
            "femaleVariant": "譲って…？"
          },
          {
            "stringId": 1792388065422929920,
            "femaleVariant": "ま、多少乱暴はしたけどね",
            "maleVariant": "ま、多少乱暴はしたがな"
          },
          {
            "stringId": 1792453099415072768,
            "femaleVariant": "ではどうやってロイスからフラットヘッドを手に入れたんだ？"
          },
          {
            "stringId": 1968888629290463232,
            "femaleVariant": "次はいよいよ本番だ… チップを手に入れる"
          },
          {
            "stringId": 1968899191705563136,
            "femaleVariant": "彼女はリジーズ・バーにいる。任せたぞ"
          }
        ]
      }
    }
  }
}