    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "皆さん、こんにちは！ 今日も“株価上昇・気分上々”の時間です",
                    "maleVariant": null,
                    "stringId": 1751942089780289536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今回のトピックはわが国の巨大軍事企業。そう… ミリテク社です",
                    "maleVariant": null,
                    "stringId": 1751945533823967232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一見、同社のポートフォリオには死角がありません。50万人以上の従業員を抱え、莫大な利益を上げており、時価総額はおよそ2兆ユーロドル…",
                    "maleVariant": null,
                    "stringId": 1751945658260578304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しかしこの驚くべき数字は、健全な競争を制した上での結果なのか… それとも、権威主義の新合衆国政府と結託して得られたものなのか？",
                    "maleVariant": null,
                    "stringId": 1751945807577800704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミリテクと政府との間には密接な繋がりがあります。現CEOのドナルド・ランディー氏は元国防長官、同社の元“プレジデント”であるエリザベス・クレス氏は、のちに新合衆国政府の大統領に就任しています",
                    "maleVariant": null,
                    "stringId": 1751945914951983104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そしてクレス氏の後を継いだのは、ロザリンド・マイヤーズ氏。驚くなかれ、ミリテクの元CEOです",
                    "maleVariant": null,
                    "stringId": 1751946079368699904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "新合衆国はミリテクにとって、全取引の6割を占める超大口の顧客であるのはよく知られています。しかしながら、その売り上げは助成金などの形を取った裏金なのです",
                    "maleVariant": null,
                    "stringId": 1751946198486933504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まともな投資家なら、これがいかに危険なことかおわかりでしょう。政府の甘い汁を吸い成長し続けるミリテクですが、もしワシントンの金が尽きると… そこで終わり！",
                    "maleVariant": null,
                    "stringId": 1751946306699976704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ではどうするべきか？ 売りましょう！ 今すぐ売り払いましょう！",
                    "maleVariant": null,
                    "stringId": 1751946446219304960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "代わりの投資先がわからないといった方は、“マネーマシン”ことアラサカ社を選んでおけば、まず間違いないでしょう",
                    "maleVariant": null,
                    "stringId": 1751946562820956160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「エディーはコツコツ堅実に増やすべし」をモットーに！ それでは、またお会いしましょう！",
                    "maleVariant": null,
                    "stringId": 1751946811123752960
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}