  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で？ 何か用があるんでしょ？",
                    "maleVariant": "で？ 何か用があるんだろ？",
                    "stringId": 1367770833858072576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほら… ループ、循環さ。1つの章が終われば、また次が始まる。生きるとはそういうことだ。まあ、俺の導師の受け売りだけどな",
                    "maleVariant": null,
                    "stringId": 1367772273326428160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us]Mm-hmmm... Hm-hmm... Hm-hmm, hm, uh-huh, uh-huhh, hm-mm...",
                    "maleVariant": null,
                    "stringId": 1367794050874216448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で、あたしに何か用があるの？",
                    "maleVariant": "で、俺に何か用があるのか？",
                    "stringId": 1380450451491254272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "聞き覚えのないリフね",
                    "maleVariant": "聞き覚えのないリフだ",
                    "stringId": 1380452762468872192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl][Kerry gra na gitarze lub wybija rytm na jakimś meblu (budżet :) ) próbuje różnych melodii, popijając piwo]",
                    "maleVariant": null,
                    "stringId": 1380464488098689024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "言ったろ。生の円環ってヤツさ",
                    "maleVariant": null,
                    "stringId": 1380499760031068160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "答えになってないんだけど",
                    "maleVariant": "答えになってないんだが",
                    "stringId": 1380501233792053248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なるほど。詳しく聞かせて",
                    "maleVariant": "なるほど。詳しく聞かせてくれ",
                    "stringId": 1380502947316232192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "導師？",
                    "maleVariant": null,
                    "stringId": 1380503684960088064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なるほど。もっと詳しく聞かせてよ",
                    "maleVariant": "なるほど。もっと詳しく聞かせてくれ",
                    "stringId": 1380504552577036288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl][zagrywa na gitarze ostrzejszą melodię i nie odpowiada od razu, drocząc się w ten sposób z V, z uśmiechem]",
                    "maleVariant": null,
                    "stringId": 1380506340524945408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんせ… 俺はあいつなしでも上手くやってきた。成功を掴んで富も得た。自力でな",
                    "maleVariant": null,
                    "stringId": 1380544312666353664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だがどうしても、何をやっても、あいつのことが頭から離れない… しつこい奴なんだ。どうにかしてひっぺがしてやりたいのに",
                    "maleVariant": null,
                    "stringId": 1380572124878073856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "気持ちはわかる",
                    "maleVariant": null,
                    "stringId": 1380579642278801408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "キーアンって言うんだが、タンガランに彼のユルトがあってな。数年おきに訪れてる。何もない僻地で、ネットも遮断されたド田舎だ",
                    "maleVariant": null,
                    "stringId": 1380587860916826112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で、前に行ったときに言われたんだ。“円環”を閉じ、けじめをつける時が来たってな。だから今、俺とお前はこうして話をしてるってわけだ",
                    "maleVariant": null,
                    "stringId": 1380593201758990336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "導師…？",
                    "maleVariant": null,
                    "stringId": 1380595149073674240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ。俺を導いてくれる師だ",
                    "maleVariant": null,
                    "stringId": 1380595296310521856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしに作曲を手伝えってこと？",
                    "maleVariant": "俺に作曲を手伝えってことか？",
                    "stringId": 1380606118738915328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最初に書いた曲の名前は？",
                    "maleVariant": null,
                    "stringId": 1380615522737582080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その曲、名前は何て？",
                    "maleVariant": null,
                    "stringId": 1380619160709099520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしに作曲を手伝えと？",
                    "maleVariant": "俺に作曲を手伝えと？",
                    "stringId": 1380619273653317632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us]Been thinkin' about that Us Cracks fiasco lately... 'Bout how you helped me. A lot.",
                    "maleVariant": "〈Us Cracks〉の件についてずっと考えててな… あの時はお前がいて助かったよ",
                    "stringId": 1380652911283531776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us]You shake things up, V. 'Fore you came along... I was stuck in the mud – scared to look at my own shadow, even.",
                    "maleVariant": "お前は現状を揺り動かす。お前と会うまでの俺は行き詰まってて… 自分の影を見るのも怖かったんだ",
                    "stringId": 1380655564499578880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で、今は？",
                    "maleVariant": null,
                    "stringId": 1380657028630441984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今はこうやって新曲に取り組んでる。アルバムだって作れそうだ。新たな魂を吹き込まれた… 生まれ変わった気分だ",
                    "maleVariant": null,
                    "stringId": 1380657241281654784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いわゆる“悟り”ってヤツを得たのさ",
                    "maleVariant": null,
                    "stringId": 1380659201514795008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "悟り？ 成長しただけでしょ",
                    "maleVariant": "悟り？ 成長しただけだろ",
                    "stringId": 1380659906946396160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "上手くいくか楽しみね",
                    "maleVariant": "上手くいくか楽しみだな",
                    "stringId": 1380660438415044608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "『Bleed the Beat』――クルーズ船で仕事をしてた時に書いた。デカいプールにビュッフェ… まあ、想像はつくだろ。豪華客船ってヤツだ",
                    "maleVariant": null,
                    "stringId": 1380664484827885568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "単に成長しただけじゃないの？",
                    "maleVariant": "単に成長しただけじゃないのか？",
                    "stringId": 1380665998736748544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "上手くいくか楽しみにしておく",
                    "maleVariant": null,
                    "stringId": 1380666085894385680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "とにかく――乱痴気騒ぎや、裏切り、裏切られのドラマとはオサラバするのさ",
                    "maleVariant": null,
                    "stringId": 1380668286394036224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあ… 明日からな",
                    "maleVariant": null,
                    "stringId": 1380753051465936896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいギターね",
                    "maleVariant": "いいギターだな",
                    "stringId": 1380758603449364480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "信じてないのか？ 乱痴気騒ぎや、裏切り、裏切られのドラマとはオサラバする。新たな人生を歩み始めるんだ",
                    "maleVariant": null,
                    "stringId": 1380761427373584384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なら応援するよ",
                    "maleVariant": null,
                    "stringId": 1380766130312441856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあ… それも明日からだけどな",
                    "maleVariant": null,
                    "stringId": 1380766641698762752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "弦に赤茶けた色がついてるだろ？  伝説のブルースマン、デニス・フォードの血だって話だ",
                    "maleVariant": null,
                    "stringId": 1380775665492160512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ランカスターのカスタムモデルさ。このシリーズはこの世に5本しか存在しない。1つがこれ、もう1つはミリテクのお偉いさんが持ってる。あとの3本は知らねえな",
                    "maleVariant": null,
                    "stringId": 1380775988000583680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リバースポラリティピックアップにエボニー製のフレット、カイマールのアームがスムーズなビブラートを可能にする",
                    "maleVariant": null,
                    "stringId": 1380779932122738688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オリジナル？ 聞き覚えがないけど",
                    "maleVariant": "オリジナルか？ 聞き覚えがないが",
                    "stringId": 1380788751435427840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうだな… これまでいろんな曲を書いてきたが、一番最初の曲は――海の上で書いたんだ",
                    "maleVariant": null,
                    "stringId": 1380798980671795200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そりゃ新曲だからな。ここんとこ、このリフが頭から離れなくて…",
                    "maleVariant": null,
                    "stringId": 1384791624028647424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こいつには… 可能性を感じてるんだ",
                    "maleVariant": null,
                    "stringId": 1384800059143090176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "“円環”っていうのは何なの？",
                    "maleVariant": "“円環”っていうのは何なんだ？",
                    "stringId": 1385137745376702464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「けじめ」って、ジョニーのこと？",
                    "maleVariant": "「けじめ」って、ジョニーのことか？",
                    "stringId": 1385138451445837824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で、今日また新しい曲が生まれる予感がする。円環ってのはそういうことだ",
                    "maleVariant": null,
                    "stringId": 1385142197563289600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そのけじめっていうのは… もしかしてジョニー絡み？ あいつの影にいたこと――",
                    "maleVariant": "そのけじめっていうのは… もしかしてジョニー絡みか？ あいつの影にいたこと――",
                    "stringId": 1405110024625844224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや… 違う。まあ… 多少は関係あるかもな…",
                    "maleVariant": null,
                    "stringId": 1405111600157421568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us]Huh.",
                    "maleVariant": null,
                    "stringId": 1405112701111889920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "世間の奴らは、企業のファシストどもに抗う歌だと思ってる",
                    "maleVariant": null,
                    "stringId": 1405123494498922496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "え…？ 違うの？",
                    "maleVariant": "え…？ 違うのか？",
                    "stringId": 1405124063582089216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺は、船の中でせっせと酒や料理を運びながら、残った時間で曲を作ってたんだ。それこそ、指から血が出るまでな",
                    "maleVariant": null,
                    "stringId": 1405128828479205376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "高そうな食器を血で汚さないよう、気をつけて働いてた。つまり、あれは単に金持ちに仕えながら血を流してるって歌なのさ。聴く側はそれを好きに解釈するわけだ",
                    "maleVariant": null,
                    "stringId": 1405133149266636800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハッ、しかしあれは売れたな… 〈Samurai〉の代表作の1つになった。こいつもそうなる。むき出しで… リアルな曲だ",
                    "maleVariant": null,
                    "stringId": 1405141831576580096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us]Hm-mm, hmm-mm... Hm hm hm... Ah, hm-hm-hm, ah, hm-hmmm...  Ah, hm-hm-hm-hmmm-mm-mm...",
                    "maleVariant": null,
                    "stringId": 1406653335459196928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいギターね",
                    "maleVariant": "いいギターだな",
                    "stringId": 1942991829555867648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そう嫌そうに言うなよ…",
                    "maleVariant": null,
                    "stringId": 2025562836908503040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": null,
                    "maleVariant": "そうじゃない。お前自身が必要なんだよ",
                    "stringId": 2025563592956964864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "〈Us Cracks〉の件についてずっと考えててな… あの時はお前がいて助かったよ",
                    "maleVariant": null,
                    "stringId": 2025567225895317504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前は現状を揺り動かす。お前と会うまで、俺は行き詰まってた",
                    "maleVariant": null,
                    "stringId": 2025568290258677760
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}