    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1643133016953864192,
            "femaleVariant": "座れ。昨日から何も食べてない"
          },
          {
            "stringId": 1643207201424162820,
            "femaleVariant": "[座る]"
          },
          {
            "stringId": 1643215852377489408,
            "femaleVariant": "おススメはあるか"
          },
          {
            "stringId": 1684082672072146944,
            "femaleVariant": "この野郎、アラサカの犬にしては思いのほか役に立つかもしれんな"
          },
          {
            "stringId": 1684083990409003008,
            "femaleVariant": "犬に要るのは命令だ、そうすりゃ喜んで動く"
          },
          {
            "stringId": 1684084744477753344,
            "femaleVariant": "目的を果たした後も生きてたら、始末すりゃいい"
          },
          {
            "stringId": 1684990129724211200,
            "femaleVariant": "ここのモノじゃなきゃ何でもいい"
          },
          {
            "stringId": 1711585098857463808,
            "femaleVariant": "あんたはこの街を知らない。手を貸すよ",
            "maleVariant": "あんたはこの街を知らないだろ。手を貸すよ"
          },
          {
            "stringId": 1711585098924572684,
            "femaleVariant": "わかった。その件は任せる"
          },
          {
            "stringId": 1711585098991681536,
            "femaleVariant": "この街じゃ右も左もわからないでしょ。一緒に行く",
            "maleVariant": "この街じゃ右も左もわからないだろ。一緒に行くよ"
          },
          {
            "stringId": 1711585099008458752,
            "femaleVariant": "あたしも手いっぱいだから、そっちの件は任せる",
            "maleVariant": "俺も手いっぱいだから、そっちの件は任せる"
          },
          {
            "stringId": 1711585099008458760,
            "femaleVariant": "心配ない"
          },
          {
            "stringId": 1711585099159453696,
            "femaleVariant": "では解散だ"
          },
          {
            "stringId": 1711585099159453704,
            "femaleVariant": "俺は偵察に行くとするよ"
          },
          {
            "stringId": 1711585099243339776,
            "femaleVariant": "おい、あいつを独りで行かせるな！"
          },
          {
            "stringId": 1711593175358914560,
            "femaleVariant": "おい、あんた！ ボリュームを上げてくれ！"
          },
          {
            "stringId": 1711596344860229632,
            "femaleVariant": "ヨリノブ…"
          },
          {
            "stringId": 1712750404300718080,
            "femaleVariant": "さて。手順を考えてきた"
          },
          {
            "stringId": 1712750701794312192,
            "femaleVariant": "1、アラサカ工業団地に侵入する。1.5、ハナコ様の山車にハッキングを仕掛ける"
          },
          {
            "stringId": 1712750925753368576,
            "femaleVariant": "3、俺が山車に乗り込む"
          },
          {
            "stringId": 1712751225629327360,
            "femaleVariant": "4、ハナコ様に真実を伝える"
          },
          {
            "stringId": 1714372251262783492,
            "femaleVariant": "ま、これより無謀な仕事もしてきたし…",
            "maleVariant": "ま、これより無謀な仕事もしてきたしな…"
          },
          {
            "stringId": 1714372329427832832,
            "femaleVariant": "5、オダに殺される"
          },
          {
            "stringId": 1714373979483803648,
            "femaleVariant": "かなり捨て身の作戦だけど、もっと無謀な仕事もしてきたしね…",
            "maleVariant": "かなり捨て身の作戦だが、もっと無謀な仕事もしてきたしな…"
          },
          {
            "stringId": 1714374060383539200,
            "femaleVariant": "5を忘れてる――オダに殺されて小便をかけられる"
          },
          {
            "stringId": 1714375202492518400,
            "femaleVariant": "2、式典が始まったら、お前が俺の指示に従い、狙撃手を排除する"
          },
          {
            "stringId": 1714377446310940672,
            "femaleVariant": "ハナコのすぐそばにあいつがいるはずよ",
            "maleVariant": "ハナコのすぐそばにあいつがいるはずだ"
          },
          {
            "stringId": 1714383853831720964,
            "femaleVariant": "様子を見て決めましょ",
            "maleVariant": "様子を見て決めよう"
          },
          {
            "stringId": 1714384050360029184,
            "femaleVariant": "タケムラは悪い奴じゃない"
          },
          {
            "stringId": 1714384699453739008,
            "femaleVariant": "まあ、必要があればね。でもまだわからない",
            "maleVariant": "まあ、必要があればな。だがまだわからない"
          },
          {
            "stringId": 1714384749953159168,
            "femaleVariant": "落ち着いてジョニー",
            "maleVariant": "落ち着けよジョニー"
          },
          {
            "stringId": 1714385176128000000,
            "femaleVariant": "同じ考えで何よりだ"
          },
          {
            "stringId": 1714386447438319616,
            "femaleVariant": "そう上手くいけばいいけど",
            "maleVariant": "そう上手くいけばいいが"
          },
          {
            "stringId": 1714387041234325504,
            "femaleVariant": "インプラントを失って万全の状態じゃないとはいえ、あたしくらいは余裕で倒せるかも",
            "maleVariant": "インプラントを失って万全の状態じゃないとはいえ、俺くらいは余裕で倒せるかもな"
          },
          {
            "stringId": 1714388095095148544,
            "femaleVariant": "はあ？ 奴はサブロウ・アラサカの元ボディガードなんだぞ！？"
          },
          {
            "stringId": 1714388319523966976,
            "femaleVariant": "つぶらな瞳だからその地位についたとでも？"
          },
          {
            "stringId": 1714389361321971712,
            "femaleVariant": "根拠は？ 奴の戦い振りを見たのか？"
          },
          {
            "stringId": 1714390172500365312,
            "femaleVariant": "いえ、そうじゃないけど… 歩き方や目つきでわかるの",
            "maleVariant": "いや、そうじゃないが… 歩き方や目つきでわかるんだ"
          },
          {
            "stringId": 1714391995747536896,
            "femaleVariant": "この件はまた後で話そう"
          },
          {
            "stringId": 1719857896152240128,
            "femaleVariant": "一人でふらついてたら、あっという間に死体袋におさまっちまうぞ"
          },
          {
            "stringId": 1719861335598960640,
            "femaleVariant": "たとえあんたには“アラサカの犬”でも、ゴロウは悪い奴じゃない",
            "maleVariant": "たとえお前には“アラサカの犬”でも、ゴロウは悪い奴じゃない"
          },
          {
            "stringId": 1719873748155109376,
            "femaleVariant": "オダは俺に襲い掛かるような真似はしないさ"
          },
          {
            "stringId": 1719989472978669568,
            "femaleVariant": "ふむ、確かにお前の技能や裏稼業の心得は役に立つかもしれん…"
          },
          {
            "stringId": 1720035864967254016,
            "femaleVariant": "ご満悦ね",
            "maleVariant": "ご満悦だな"
          },
          {
            "stringId": 1720036009653964800,
            "femaleVariant": "このままには… しておけない"
          },
          {
            "stringId": 1720072388161179648,
            "femaleVariant": "奴め… 絶対的権力を手に入れるため、まさかこれほどまでの蛮行に走るとは"
          },
          {
            "stringId": 1720074157150834688,
            "femaleVariant": "まんまと騙されてる"
          },
          {
            "stringId": 1724321730103480320,
            "femaleVariant": "一緒に行け、V"
          },
          {
            "stringId": 1724431394661076992,
            "femaleVariant": "だが気をつけろ。向こうも同じ腹積もりかもしれねえ"
          },
          {
            "stringId": 1724442673077424128,
            "femaleVariant": "今日は… 焼き鳥ですかね"
          },
          {
            "stringId": 1743088152589975552,
            "femaleVariant": "それじゃあまたな"
          },
          {
            "stringId": 1744582118726266880,
            "femaleVariant": "ありがとうございます。またのお越しを"
          },
          {
            "stringId": 1763515950433177600,
            "femaleVariant": "死にに行くつもりなの？",
            "maleVariant": "死にに行くつもりか？"
          },
          {
            "stringId": 1763516557969723392,
            "femaleVariant": "じゃああたしは？",
            "maleVariant": "じゃあ俺は？"
          },
          {
            "stringId": 1763516615196807168,
            "femaleVariant": "それはまた別の話だ。オダには近づかない方がいい"
          },
          {
            "stringId": 1763518283187621888,
            "femaleVariant": "手の込んだ切腹をするつもり？",
            "maleVariant": "手の込んだ切腹をするつもりか？"
          },
          {
            "stringId": 1763519094433124352,
            "femaleVariant": "切腹？ 何のことだ？"
          },
          {
            "stringId": 1763519154277453824,
            "femaleVariant": "どう考えたって死にに行くようなもんじゃない。わけを教えて",
            "maleVariant": "どう考えたって死にに行くようなもんだろ。わけを教えてくれ"
          },
          {
            "stringId": 1763519741597454336,
            "femaleVariant": "確かに危険は大きいが、他に手はない。ハナコ様が唯一の希望なんだ"
          },
          {
            "stringId": 1763523253840515072,
            "femaleVariant": "失うものは何もない。お前もだろ。目を見ればわかる"
          },
          {
            "stringId": 1763525503027646464,
            "femaleVariant": "アラサカ相手に強奪を仕掛けた以上に無謀なことを？"
          },
          {
            "stringId": 1763525697274253312,
            "femaleVariant": "まさか。あれでかなりハードルが上がっちゃったから",
            "maleVariant": "まさか。あれでかなりハードルが上がっちまったからな"
          },
          {
            "stringId": 1771875837534093312,
            "femaleVariant": "ずっと立ってる気か？"
          },
          {
            "stringId": 1771875855787704320,
            "femaleVariant": "お前は知らんが、俺は腹が減ってる。座ってくれ"
          },
          {
            "stringId": 1774983621225877508,
            "femaleVariant": "サブロウはただ指をくわえて見てただけなの？ 息子の手綱を握ることはできたはずよ",
            "maleVariant": "サブロウはただ指をくわえて見てただけなのか？ 息子の手綱を握ることはできたはずだ"
          },
          {
            "stringId": 1774983621242654720,
            "femaleVariant": "サブロウ様は強く厳格なお方だった。あれほど強い人間を俺は知らない"
          },
          {
            "stringId": 1774983621242654732,
            "femaleVariant": "だが決定的な時に弱みを見せてしまわれたのだ"
          },
          {
            "stringId": 1774983621242654744,
            "femaleVariant": "ヨリノブのためね",
            "maleVariant": "ヨリノブのためか"
          },
          {
            "stringId": 1774983621259431936,
            "femaleVariant": "娘のためだ"
          },
          {
            "stringId": 1774983621259431948,
            "femaleVariant": "ヨリノブが子分を従えてアラサカ家に宣戦布告した時、サブロウ様は一言命じるだけで反乱を潰せたはずだが――"
          },
          {
            "stringId": 1774983621259431960,
            "femaleVariant": "そうはなさらなかった。ハナコ様を苦しめないためにと"
          },
          {
            "stringId": 1774983621276209156,
            "femaleVariant": "かつてヨリノブが負け犬のようにアラサカ家に戻った時、もう一度機会を与えるよう頼み込んだのもハナコ様だ"
          },
          {
            "stringId": 1774983621276209168,
            "femaleVariant": "わかるか？ ヨリノブはハナコ様のおかげで、それまでの行いの報いを受けずに済んだんだ"
          },
          {
            "stringId": 1774983621326540800,
            "femaleVariant": "その兄妹愛もここまでね",
            "maleVariant": "その兄妹愛もここまでだな"
          },
          {
            "stringId": 1774983621326540816,
            "femaleVariant": "ハナコにも罪があると言いたいの？",
            "maleVariant": "ハナコにも罪があると言いたいのか？"
          },
          {
            "stringId": 1774983621527867396,
            "femaleVariant": "待って… 彼女のせいでもあると言いたいの？",
            "maleVariant": "待て… 彼女のせいでもあると言いたいのか？"
          },
          {
            "stringId": 1774983621527867408,
            "femaleVariant": "責任は、うかつにもヨリノブに騙された者、全員にある"
          },
          {
            "stringId": 1774983621578199040,
            "femaleVariant": "ヨリノブはどうしてそこまでして権力を？"
          },
          {
            "stringId": 1774983621578199052,
            "femaleVariant": "誰にもわからない"
          },
          {
            "stringId": 1774983621578199064,
            "femaleVariant": "だが、直々に問いただしてやるつもりだ"
          },
          {
            "stringId": 1774983621611753472,
            "femaleVariant": "サブロウはなぜ事態を防げなかったの？",
            "maleVariant": "サブロウはなぜ事態を防げなかったんだ？"
          },
          {
            "stringId": 1774983621611753488,
            "femaleVariant": "ヨリノブはどうしてそこまでして権力を？"
          },
          {
            "stringId": 1774983621611753504,
            "femaleVariant": "どんな派閥が存在するの？",
            "maleVariant": "どんな派閥が存在するんだ？"
          },
          {
            "stringId": 1774983621611753520,
            "femaleVariant": "トップが死んで、対立が激化してるのね",
            "maleVariant": "トップが死んで、対立が激化してるのか"
          },
          {
            "stringId": 1774983621645307908,
            "femaleVariant": "トップが死んで、内部派閥の対立が激化してるのね",
            "maleVariant": "トップが死んで、内部派閥の対立が激化してるのか"
          },
          {
            "stringId": 1774983621662085128,
            "femaleVariant": "力関係は今どうなってるの？",
            "maleVariant": "力関係は今どうなってるんだ？"
          },
          {
            "stringId": 1774983621695639556,
            "femaleVariant": "まず一つ目。〈キジ〉はハナコ様のもとに集う、保守的な旧体制派"
          },
          {
            "stringId": 1774983621712416768,
            "femaleVariant": "リベラル派の〈ハト〉はサブロウ様の孫娘、ミチコ様を支持しているが、よくは存じ上げない"
          },
          {
            "stringId": 1774983621712416780,
            "femaleVariant": "そしてヨリノブを支持する危険分子の集まり、〈タカ〉"
          },
          {
            "stringId": 1774983621712416792,
            "femaleVariant": "加えて小規模な派閥も数々存在し――"
          },
          {
            "stringId": 1774983621729193984,
            "femaleVariant": "あー、うん。もういい"
          },
          {
            "stringId": 1774983621829857284,
            "femaleVariant": "その兄妹愛もここまでね。ハナコがあたしたちを信じてくれればだけど…",
            "maleVariant": "その兄妹愛もここまでだな。ハナコが俺たちを信じてくれればだが…"
          },
          {
            "stringId": 1774983621880188932,
            "femaleVariant": "アラサカ内に派閥があるの？ 具体的には？",
            "maleVariant": "アラサカ内に派閥があるのか？ 具体的には？"
          },
          {
            "stringId": 1774983621880188944,
            "femaleVariant": "派閥は〈キジ〉、〈ハト〉、〈タカ〉の3つ"
          },
          {
            "stringId": 1776175116507287552,
            "femaleVariant": "なんだこれは？"
          },
          {
            "stringId": 1776178857709015040,
            "femaleVariant": "焼き鳥です"
          },
          {
            "stringId": 1779330942660902912,
            "femaleVariant": "アラサカは… 不滅の帝国だ"
          },
          {
            "stringId": 1779331499295375360,
            "femaleVariant": "ヨリノブはそこに、中から火を放った"
          },
          {
            "stringId": 1779331932818636800,
            "femaleVariant": "あいつが全ての元凶だ… 派閥も、権力闘争も… 全てあいつが… クソ"
          },
          {
            "stringId": 1779349656353505280,
            "femaleVariant": "だがこの過ちは必ず正す"
          },
          {
            "stringId": 1779356710082314240,
            "femaleVariant": "まずはお会いしなくてはな"
          },
          {
            "stringId": 1779374908529836032,
            "femaleVariant": "彼女に良心というものがあれば、希望があるかもしれない"
          },
          {
            "stringId": 1785176804160454656,
            "femaleVariant": "では、俺は偵察に向かう。連絡を待っていろ"
          },
          {
            "stringId": 1792518928464744448,
            "femaleVariant": "ああ、そのとおりだ"
          },
          {
            "stringId": 1863489970701672448,
            "femaleVariant": "とても食えたものじゃなかったぞ"
          },
          {
            "stringId": 1881960326313885700,
            "femaleVariant": "[食べる]"
          },
          {
            "stringId": 1911439759795646464,
            "femaleVariant": "一緒に来るか？"
          },
          {
            "stringId": 1911440410718072832,
            "femaleVariant": "そうする"
          },
          {
            "stringId": 1911440775756738560,
            "femaleVariant": "やめとく"
          },
          {
            "stringId": 1911440974029877248,
            "femaleVariant": "ええ、そうする",
            "maleVariant": "ああ、そうしよう"
          },
          {
            "stringId": 1911442384037441536,
            "femaleVariant": "大丈夫よ、自分で行く",
            "maleVariant": "大丈夫だ、自分で行く"
          },
          {
            "stringId": 1911486944037924864,
            "femaleVariant": "そうか。俺もすぐ出発する。じゃあ現地でな"
          },
          {
            "stringId": 1915626623637200896,
            "femaleVariant": "長居は無用だ"
          },
          {
            "stringId": 1915633618679308288,
            "femaleVariant": "よし。では共に動くとしよう"
          },
          {
            "stringId": 1956173676106825732,
            "femaleVariant": "[立ち上がって偵察を開始する]"
          },
          {
            "stringId": 1956268893132636160,
            "femaleVariant": "[立ち上がる]"
          },
          {
            "stringId": 1979277855036555264,
            "femaleVariant": "ええ",
            "maleVariant": "ああ"
          },
          {
            "stringId": 1979277855053332480,
            "femaleVariant": "遠慮する"
          },
          {
            "stringId": 1996730897998561280,
            "femaleVariant": "で？"
          },
          {
            "stringId": 1996732027088420868,
            "femaleVariant": "システムを乗っ取った"
          },
          {
            "stringId": 1996732933880168448,
            "femaleVariant": "言われたとおりシステムを乗っ取った"
          },
          {
            "stringId": 1996733363745996800,
            "femaleVariant": "チップは問題なしか。とはいえ… まだ安心はできん"
          },
          {
            "stringId": 1996733996851019776,
            "femaleVariant": "警備が厳重なアラサカ工業団地への侵入はそう簡単にはいかないだろう。だが、その話に移る前に…"
          },
          {
            "stringId": 2011305208242315264,
            "femaleVariant": "コソ泥には朝飯前よ",
            "maleVariant": "コソ泥には朝飯前だ"
          },
          {
            "stringId": 2011306311495262256,
            "femaleVariant": "ご期待通り。コソ泥には朝飯前よ",
            "maleVariant": "ご期待通り。コソ泥には朝飯前さ"
          },
          {
            "stringId": 2011307730327642112,
            "femaleVariant": "腕は確かなようだな"
          },
          {
            "stringId": 2401323113117728768,
            "femaleVariant": "[食べる]"
          }
        ]
      }
    }
  }
}