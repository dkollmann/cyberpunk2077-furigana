    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1756160274550509568,
            "femaleVariant": "じゃあ、階段だね"
          },
          {
            "stringId": 1756450648196141056,
            "femaleVariant": "コンボイの位置は監視してるの？",
            "maleVariant": "コンボイの位置は監視してるのか？"
          },
          {
            "stringId": 1756453581054824448,
            "femaleVariant": "うん、キャロルが通信チャンネルをモニターしてる"
          },
          {
            "stringId": 1774898536076611584,
            "femaleVariant": "心配事？",
            "maleVariant": "心配事か？"
          },
          {
            "stringId": 1774898997819150336,
            "femaleVariant": "[窓際に立つ] 話？"
          },
          {
            "stringId": 1774902379267035136,
            "femaleVariant": "[窓際に立つ] 街にいた時より楽しそう",
            "maleVariant": "[窓際に立つ] 街にいた時より楽しそうだな"
          },
          {
            "stringId": 1774903503877378048,
            "femaleVariant": "[窓際に立つ] 心配事でも？"
          },
          {
            "stringId": 1774904606811557888,
            "femaleVariant": "街にいるときよりこっちのほうが断然楽しそうね",
            "maleVariant": "街にいるときよりこっちのほうが断然楽しそうだ"
          },
          {
            "stringId": 1774909041046523904,
            "femaleVariant": "冗談でしょ。今からクランを危険にさらして、ソウルから大目玉を食らうどころか追い出されるかもしれないってのに、楽しそうって"
          },
          {
            "stringId": 1774925807323779072,
            "femaleVariant": "アルデカルドスにとって正しいと思うことをしてるからじゃない？",
            "maleVariant": "アルデカルドスにとって正しいと思うことをしてるからじゃないか？"
          },
          {
            "stringId": 1774930719793287168,
            "femaleVariant": "自分から頼んでおいて、聞くのは変かもしれないけど…"
          },
          {
            "stringId": 1774931022974357504,
            "femaleVariant": "なぜ来てくれたの？"
          },
          {
            "stringId": 1774934690339557376,
            "femaleVariant": "うん。でもコンボイのことじゃない"
          },
          {
            "stringId": 1774935286937358336,
            "femaleVariant": "じゃあ何？",
            "maleVariant": "じゃあ何だ？"
          },
          {
            "stringId": 1774936612974956544,
            "femaleVariant": "話って何？",
            "maleVariant": "話ってのは？"
          },
          {
            "stringId": 1774939929931223044,
            "femaleVariant": "パナムにとって大切なことだから",
            "maleVariant": "パナムにとって大切なことだからだ"
          },
          {
            "stringId": 1774941209244274688,
            "femaleVariant": "どんな展開になるか見たいから",
            "maleVariant": "どんな展開になるか見たいからだ"
          },
          {
            "stringId": 1774941920615010304,
            "femaleVariant": "タダではやらない"
          },
          {
            "stringId": 1774946988676419584,
            "femaleVariant": "大切なことなんでしょ",
            "maleVariant": "大切なことなんだろ"
          },
          {
            "stringId": 1774947339773218816,
            "femaleVariant": "また信用できないって言うの？",
            "maleVariant": "また信用できないって言うのか？"
          },
          {
            "stringId": 1774948141472485376,
            "femaleVariant": "どうなるか見てみたくって"
          },
          {
            "stringId": 1774948677303209984,
            "femaleVariant": "タダではやらないよ",
            "maleVariant": "タダではやらないぞ"
          },
          {
            "stringId": 1774949149128855552,
            "femaleVariant": "まだ信用できないの？ その話は終わったものかと",
            "maleVariant": "まだ信用できないのか？ その話は終わったものかと"
          },
          {
            "stringId": 1774949919303733248,
            "femaleVariant": "それが大変でも、不安でおしっこ漏らしそうでもね",
            "maleVariant": "それが大変でも、不安でおしっこ漏らしそうでもな"
          },
          {
            "stringId": 1774950624349458432,
            "femaleVariant": "やだな、お見通しなわけか"
          },
          {
            "stringId": 1774968509247590400,
            "femaleVariant": "クランのためを思ってやってるんでしょ",
            "maleVariant": "クランのためを思ってやってるんだろ"
          },
          {
            "stringId": 1774969466773639168,
            "femaleVariant": "で、話ってのは？"
          },
          {
            "stringId": 1774974751412129792,
            "femaleVariant": "全部クランのためなんでしょ？",
            "maleVariant": "全部クランのためなんだろ？"
          },
          {
            "stringId": 1774976323135279104,
            "femaleVariant": "ソウルを出し抜いて、クランを自分のものにしたいなんて考えてるわけじゃない"
          },
          {
            "stringId": 1774978592639619072,
            "femaleVariant": "そんなの当たり前でしょ"
          },
          {
            "stringId": 1774980810033926144,
            "femaleVariant": "で、あんたが手を貸す理由は？"
          },
          {
            "stringId": 1774981208123707392,
            "femaleVariant": "そりゃ頼んだのは私だけど… どうして来ようと思ったの？"
          },
          {
            "stringId": 1774985794595131392,
            "femaleVariant": "それだけで普通手を貸す？ あ、ごめん、そういう意味じゃなくて…"
          },
          {
            "stringId": 1774999854673891328,
            "femaleVariant": "どんなトラブルを起こすかって意味？ もしかして楽しんでる？"
          },
          {
            "stringId": 1775008137216331776,
            "femaleVariant": "まあ少しはね。それに、頑張ってるところを見たいし",
            "maleVariant": "まあ少しはな。それに、頑張ってるところを見たいし"
          },
          {
            "stringId": 1775100520520028160,
            "femaleVariant": "じゃあ定期的に会いにくるつもり？"
          },
          {
            "stringId": 1775101061333585920,
            "femaleVariant": "うん。見張っとかなきゃ",
            "maleVariant": "ああ。見張っとかないと"
          },
          {
            "stringId": 1775122043473563648,
            "femaleVariant": "もう1丁よこせっての？"
          },
          {
            "stringId": 1775123252087427072,
            "femaleVariant": "それもいいね",
            "maleVariant": "それもいいな"
          },
          {
            "stringId": 1775124449997426688,
            "femaleVariant": "どうかした？",
            "maleVariant": "どうかしたか？"
          },
          {
            "stringId": 1775124675751645184,
            "femaleVariant": "あんたが欲しいの",
            "maleVariant": "お前が欲しいんだ"
          },
          {
            "stringId": 1775130401127600128,
            "femaleVariant": "そうこなくっちゃ"
          },
          {
            "stringId": 1775130878825271296,
            "femaleVariant": "お好みに合うのをご用意しますよ"
          },
          {
            "stringId": 1775131500052025344,
            "femaleVariant": "気に障った？",
            "maleVariant": "気に障ったか？"
          },
          {
            "stringId": 1775132848218771456,
            "femaleVariant": "別に… 私、戻ったばかりなのに、もうナイトシティの常識を忘れてた"
          },
          {
            "stringId": 1775134863732838400,
            "femaleVariant": "どんなことにだって裏があるってことをね"
          },
          {
            "stringId": 1775145016683204608,
            "femaleVariant": "銃じゃない、パナムが欲しいの",
            "maleVariant": "銃じゃない、お前が欲しいんだ"
          },
          {
            "stringId": 1775146406776213504,
            "femaleVariant": "それって私が思ってる意味なら、悪いけどお断り"
          },
          {
            "stringId": 1775177777418670080,
            "femaleVariant": "頼りにしてるのはホント"
          },
          {
            "stringId": 1775178313417166848,
            "femaleVariant": "でも？"
          },
          {
            "stringId": 1775180615116984320,
            "femaleVariant": "あたしたち、このところずっと波長が合うと思ってたけど",
            "maleVariant": "俺たち、このところずっと波長が合うと思ってたけどな"
          },
          {
            "stringId": 1775183480548036608,
            "femaleVariant": "うん。波長は同じでも、チャンネルが違う"
          },
          {
            "stringId": 1775196218733170688,
            "femaleVariant": "でも… 何の見返りもなく、ここまでしてくれる人はいなかったから"
          },
          {
            "stringId": 1775203280615706624,
            "femaleVariant": "何言ってるの",
            "maleVariant": "何言ってんだ"
          },
          {
            "stringId": 1775203589031268352,
            "femaleVariant": "クランの家族以外で、でしょ",
            "maleVariant": "クランの家族以外で、だろ"
          },
          {
            "stringId": 1775205447997132800,
            "femaleVariant": "何言ってんの",
            "maleVariant": "何言ってんだ"
          },
          {
            "stringId": 1775205899606233088,
            "femaleVariant": "ごめん、ほんとに、感謝してる。ありがとう"
          },
          {
            "stringId": 1775208437948682240,
            "femaleVariant": "クランの家族以外では、でしょ",
            "maleVariant": "クランの家族以外では、だろ"
          },
          {
            "stringId": 1775209172924960768,
            "femaleVariant": "うん。あんたも似たような境遇だったね"
          },
          {
            "stringId": 1785152807674597380,
            "femaleVariant": "電車が好きだったの"
          },
          {
            "stringId": 1785152813227855872,
            "femaleVariant": "たくさん線路が集まる駅もね"
          },
          {
            "stringId": 1785156974799507460,
            "femaleVariant": "よし。着いた"
          },
          {
            "stringId": 1785158283237806080,
            "femaleVariant": "横長のカードで、穴が並んでるやつ！"
          },
          {
            "stringId": 1785167656953597952,
            "femaleVariant": "さてどうなるか…"
          },
          {
            "stringId": 1785185537640648704,
            "femaleVariant": "見て、うまくいった！"
          },
          {
            "stringId": 1785191216828928000,
            "femaleVariant": "V… 話したいことがある"
          },
          {
            "stringId": 1785416144266219524,
            "femaleVariant": "明かり要るか？"
          },
          {
            "stringId": 1785416144282996740,
            "femaleVariant": "よくやったね！ ありがと"
          },
          {
            "stringId": 1785532709527638016,
            "femaleVariant": "ないね",
            "maleVariant": "ないな"
          },
          {
            "stringId": 1785536373235404804,
            "femaleVariant": "あった！"
          },
          {
            "stringId": 1785538191969505284,
            "femaleVariant": "キャロル！ カードあったけど！？ どうすんの！？"
          },
          {
            "stringId": 1785538192724480000,
            "femaleVariant": "それを読み取り機に入れて！"
          },
          {
            "stringId": 1789834786336690176,
            "femaleVariant": "よし、みんなと合流しよう"
          },
          {
            "stringId": 1804105013097992192,
            "femaleVariant": "キャロル！ 何を探すの！？"
          },
          {
            "stringId": 1804149658846826496,
            "femaleVariant": "探してくれる？"
          },
          {
            "stringId": 1804187683987460096,
            "femaleVariant": "ジャンクションね",
            "maleVariant": "ジャンクションだろ"
          },
          {
            "stringId": 1804188433710911488,
            "femaleVariant": "そうそう。それを上から見るとさ…"
          },
          {
            "stringId": 1804201750793883648,
            "femaleVariant": "自分がどこにでも行けそうな気がして、自由を感じるとか？"
          },
          {
            "stringId": 1804203013262610432,
            "femaleVariant": "もっと単純に… わくわくするの"
          },
          {
            "stringId": 1804213521755738112,
            "femaleVariant": "私はそういう風に借りを返したくない"
          },
          {
            "stringId": 1804218925931892736,
            "femaleVariant": "全て計画どおり、今のところは"
          },
          {
            "stringId": 1804433293991235584,
            "femaleVariant": "おもちゃみたいだな"
          },
          {
            "stringId": 1804530071885336576,
            "femaleVariant": "先にカードを探して"
          },
          {
            "stringId": 1805306931644272640,
            "femaleVariant": "もう一度――"
          },
          {
            "stringId": 1805310038415577088,
            "femaleVariant": "パナム、一度座って、深呼吸でもしたらどうだ"
          },
          {
            "stringId": 1805450718811066368,
            "femaleVariant": "全て予定どおりだよ。コンボイはもうこっちに向かってる"
          },
          {
            "stringId": 1824226853946462208,
            "femaleVariant": "大昔の制御盤みたい",
            "maleVariant": "大昔の制御盤みたいだ"
          },
          {
            "stringId": 1842046176702779396,
            "femaleVariant": "手伝おうか？"
          },
          {
            "stringId": 1842046395880329216,
            "femaleVariant": "手伝おうか？"
          },
          {
            "stringId": 1842054801198436352,
            "femaleVariant": "あら、早かったね"
          },
          {
            "stringId": 1842055331157135360,
            "femaleVariant": "手伝ってくれるより、他の入り口を探してみてよ"
          },
          {
            "stringId": 1842057865741488128,
            "femaleVariant": "どうやって入り込んだの？ こっちはやっとドアを開けられたのに。今からそっちに行く"
          },
          {
            "stringId": 1842060211548606468,
            "femaleVariant": "こっちは私が開けたから、引き分けね"
          },
          {
            "stringId": 1842061931515236352,
            "femaleVariant": "すごいじゃない。私はドアのほうがいいけど。そっち行くから、ちょっと待って"
          },
          {
            "stringId": 1843037658913984512,
            "femaleVariant": "ミッチとボブが明かりをつけてくれたみたい"
          },
          {
            "stringId": 1843039537878290432,
            "femaleVariant": "誰かさんが手伝ったからね",
            "maleVariant": "誰かさんが手伝ったからな"
          },
          {
            "stringId": 1874852676838903808,
            "femaleVariant": "[窓際に立つ] 会いたかった"
          },
          {
            "stringId": 1874856673272418304,
            "femaleVariant": "ずっと会いたかった"
          },
          {
            "stringId": 1874937830269800448,
            "femaleVariant": "えっと… それって…"
          },
          {
            "stringId": 1882095463867109376,
            "femaleVariant": "ちょっと待ってよ。調子狂うじゃない"
          },
          {
            "stringId": 1882101384211091456,
            "femaleVariant": "はっきり言わせて",
            "maleVariant": "はっきり言わせてくれ"
          },
          {
            "stringId": 1882103001903812608,
            "femaleVariant": "そういう話じゃなかった？",
            "maleVariant": "そういう話じゃないのか？"
          },
          {
            "stringId": 1882105295567343616,
            "femaleVariant": "嫌ならいいの",
            "maleVariant": "嫌ならいいんだ"
          },
          {
            "stringId": 1882129382549245952,
            "femaleVariant": "はっきり言わせて",
            "maleVariant": "はっきり言わせてくれ"
          },
          {
            "stringId": 1882129702641750016,
            "femaleVariant": "そういう話じゃないの？",
            "maleVariant": "そういう話じゃないのか？"
          },
          {
            "stringId": 1882129978996051968,
            "femaleVariant": "嫌ならいいの",
            "maleVariant": "嫌ならいいんだ"
          },
          {
            "stringId": 1882133696793563136,
            "femaleVariant": "わかった、どうぞ"
          },
          {
            "stringId": 1882134151271567360,
            "femaleVariant": "冗談よ。V、何を言おうとしてるかはわかってる"
          },
          {
            "stringId": 1882144976065548288,
            "femaleVariant": "まあ、それもあったけど… 唐突すぎ"
          },
          {
            "stringId": 1882151172780609536,
            "femaleVariant": "聞きたくないの？",
            "maleVariant": "聞きたくないのか？"
          },
          {
            "stringId": 1882166138879365124,
            "femaleVariant": "いい作戦だけど、失敗したね",
            "maleVariant": "いい作戦だが、失敗したな"
          },
          {
            "stringId": 1882166666942238720,
            "femaleVariant": "問題なさそう",
            "maleVariant": "問題なさそうだ"
          },
          {
            "stringId": 1882167480469442560,
            "femaleVariant": "ちらちら外を窺って、何かあったらすぐに殻に戻ろうと思ってたのに"
          },
          {
            "stringId": 1882172966166089728,
            "femaleVariant": "いい作戦だけど、失敗に終わったね",
            "maleVariant": "いい作戦だが、失敗に終わったな"
          },
          {
            "stringId": 1882174074804523008,
            "femaleVariant": "それでも問題ないよ",
            "maleVariant": "それでも問題ないさ"
          },
          {
            "stringId": 1882178263353937920,
            "femaleVariant": "そうじゃないよ。私もその話はしたかったけど… 唐突すぎ"
          },
          {
            "stringId": 1882182303810867200,
            "femaleVariant": "他にご質問は？"
          },
          {
            "stringId": 1882184099744731136,
            "femaleVariant": "ない。とりあえず、知りたいことは全部わかった"
          },
          {
            "stringId": 1882192409684365312,
            "femaleVariant": "あんたを紹介してくれたお礼に、ローグに花でも贈ろうか"
          },
          {
            "stringId": 1882193035407413248,
            "femaleVariant": "そんなタイプじゃないでしょ",
            "maleVariant": "そんなタイプじゃないだろ"
          },
          {
            "stringId": 1882198092848844800,
            "femaleVariant": "お見通しだったんでしょ"
          },
          {
            "stringId": 1882203350962556928,
            "femaleVariant": "ごめん。こんなふうに自分の気持ちを話すのは得意じゃなくて"
          },
          {
            "stringId": 1882204591084032000,
            "femaleVariant": "カードにこう書くの。「親友をくれてありがとう、クソババア」って"
          },
          {
            "stringId": 1882212048774647808,
            "femaleVariant": "そうこなくっちゃ"
          },
          {
            "stringId": 1882229161300086784,
            "femaleVariant": "ここで台無しにしたくない"
          },
          {
            "stringId": 1882230691013087232,
            "femaleVariant": "どう反応するかなんてわからないし、あたしだって手探りだった",
            "maleVariant": "どう反応するかなんてわかるもんか。俺だって手探りだった"
          },
          {
            "stringId": 1882233164545150976,
            "femaleVariant": "もう外を窺う必要はない"
          },
          {
            "stringId": 1882238917184081920,
            "femaleVariant": "聞いて、私…"
          },
          {
            "stringId": 1882241194020065280,
            "femaleVariant": "台無しにしたくないの"
          },
          {
            "stringId": 1882250716935639044,
            "femaleVariant": "もう外を窺う必要はない。そうでしょ？",
            "maleVariant": "もう外を窺う必要はない。そうだろ？"
          },
          {
            "stringId": 1882254740145590272,
            "femaleVariant": "少しずつでいい"
          },
          {
            "stringId": 1882266437052813316,
            "femaleVariant": "少しずつでいい"
          },
          {
            "stringId": 1882267265041977344,
            "femaleVariant": "ここまでは上手くやってる"
          },
          {
            "stringId": 1882271414383038464,
            "femaleVariant": "ここまでは上手くやってる"
          },
          {
            "stringId": 1882302494326673408,
            "femaleVariant": "そりゃ大して喋ってないからね"
          },
          {
            "stringId": 1882302966756298752,
            "femaleVariant": "私って先に体が動くタイプでしょ"
          },
          {
            "stringId": 1882306410833530880,
            "femaleVariant": "ええ、知ってる",
            "maleVariant": "ああ、知ってる"
          },
          {
            "stringId": 1882308344491560960,
            "femaleVariant": "うん。そういう時はたいてい衝動に従ってる"
          },
          {
            "stringId": 1882314008731340800,
            "femaleVariant": "だから自分に正直に生きてるって思えるの"
          },
          {
            "stringId": 1882318221691604992,
            "femaleVariant": "でもあんたとは、慎重でいたいんだ"
          },
          {
            "stringId": 1882329999817330692,
            "femaleVariant": "あたしに衝動は感じないってこと？ 残念ね…",
            "maleVariant": "俺に衝動は感じないってことか？ 残念だな…"
          },
          {
            "stringId": 1882330579621138432,
            "femaleVariant": "どうしてあたしだけ違うの？",
            "maleVariant": "どうして俺だけ違うんだ？"
          },
          {
            "stringId": 1882332402532765696,
            "femaleVariant": "あたしに何の衝動も感じないの？ それは少し残念ね",
            "maleVariant": "俺に何の衝動も感じないのか？ そいつは少し残念だな"
          },
          {
            "stringId": 1882333123634286592,
            "femaleVariant": "なぜあたしだけ違うの？",
            "maleVariant": "どうして俺だけ違うんだ？"
          },
          {
            "stringId": 1882333620088885248,
            "femaleVariant": "本気で想ってるからよ。バカなことを言ったりして、急に捨てられるのが怖いの"
          },
          {
            "stringId": 1882337904822079488,
