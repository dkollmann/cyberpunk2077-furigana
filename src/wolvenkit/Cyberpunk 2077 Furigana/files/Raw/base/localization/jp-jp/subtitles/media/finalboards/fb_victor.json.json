  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや、もういい。こんなことしたってあいつは…",
                    "maleVariant": null,
                    "stringId": 1790637139848765444
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よう、V！ 砂漠での暮らしはどうだ？ 太陽の光と新鮮な空気を満喫してるか？",
                    "maleVariant": null,
                    "stringId": 1790637139882319876
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よう、V！ 元気か？ 最近はめっきり顔を出さなくなったじゃないか",
                    "maleVariant": null,
                    "stringId": 1790637139949428740
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V。お前さんが自由人なのはわかってる。好きな時に好きなことをやりゃいい",
                    "maleVariant": null,
                    "stringId": 1790637140016537604
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よう、V。外から見る地球はどうだ？ 噂どおりの美しさか？ それともやっぱりありゃウソなのか？",
                    "maleVariant": null,
                    "stringId": 1790637140050092036
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やるのよ… あなたのためでもあるの",
                    "maleVariant": null,
                    "stringId": 1790842572898619392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "…よう、V。死後の世界はどうだ？ 噂どおり、きれいなとこか？",
                    "maleVariant": null,
                    "stringId": 1790843297808568320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティのほうは相変わらずだよ。何なら、前よりひどくなってるような気もするな",
                    "maleVariant": null,
                    "stringId": 1790843733345095680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺がもっと力になってやれれば… そしたら、こんなことには",
                    "maleVariant": null,
                    "stringId": 1790843937104384000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ポジティブにね、ヴィクター",
                    "maleVariant": null,
                    "stringId": 1790844813143494656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺は… お前さんがいなくて寂しいよ",
                    "maleVariant": null,
                    "stringId": 1790845158452154368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "定期的に点検してもらうのを忘れるなよ。絶縁体に土やほこりが入りこんだらシャレにならないからな",
                    "maleVariant": null,
                    "stringId": 1790847472919433216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "とはいえ、そっちでまともなメンテができる腕の良いリパーを見つけるのは至難の業か…",
                    "maleVariant": null,
                    "stringId": 1790848030862528512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "街に来ることがあれば、俺んとこに来な。なんやかんやで、お前さんはお得意さんだからな――多少はまけてやるよ…",
                    "maleVariant": null,
                    "stringId": 1790848289349095424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあまたな、V",
                    "maleVariant": null,
                    "stringId": 1790848568538746880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アフターライフの奴らに“メジャーリーグ”ご用達のリパーでも紹介してもらったか？ きっとケツからクロームを垂れ流すような奴なんだろうな",
                    "maleVariant": null,
                    "stringId": 1790850364472610816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それでいいんだよ、V… 人生を目一杯楽しめ。お前さんには誰よりもその権利がある",
                    "maleVariant": null,
                    "stringId": 1790850574808567808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だが、今度近くまで来ることがあったら、立ち寄ってくれ。たまには思い出話をしようじゃないか… ついでに新しいリパーがヤブじゃないか見てやるよ",
                    "maleVariant": null,
                    "stringId": 1790850969962336256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあまたな、V",
                    "maleVariant": null,
                    "stringId": 1790851339446964224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だがせめて生きてるかどうかぐらい、教えてくれ",
                    "maleVariant": null,
                    "stringId": 1790852290262126592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "説教するつもりはないが――定期点検もすっぽかしやがって。何度も口をすっぱくして… ったく…",
                    "maleVariant": null,
                    "stringId": 1790852731150585856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一度、連絡してくれ。じゃあな",
                    "maleVariant": null,
                    "stringId": 1790853181971156992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいか… お前さんのインプラントのことだが――どれも宇宙での使用は想定されてない",
                    "maleVariant": null,
                    "stringId": 1790855627451715584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もしエラーが出たら絶対に無視するなよ。いいな？",
                    "maleVariant": null,
                    "stringId": 1790855888740077568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "戻ったら… もし戻ってきたら… 一通り点検してやる。何なら特別に割引してやってもいい",
                    "maleVariant": null,
                    "stringId": 1790856248024158208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "元気でな、V",
                    "maleVariant": null,
                    "stringId": 1790856425594212352
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}