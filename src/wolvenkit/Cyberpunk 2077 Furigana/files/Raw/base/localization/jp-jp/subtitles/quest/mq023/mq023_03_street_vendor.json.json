        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本物の音楽を探してる",
                    "maleVariant": null,
                    "stringId": 1854836846355161088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いい品揃えをしてるって聞いたんだけど",
                    "maleVariant": "いい品揃えをしてるって聞いたんだが",
                    "stringId": 1854838265338535936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「いい」の定義によるな。あるのは〈Cartesian Dualists〉、ユーロダインのが何枚か、それに〈Tainted Overlord〉…",
                    "maleVariant": null,
                    "stringId": 1854841406519910400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうとも。俺はそれを誇りに思ってる。シルヴァーハンドは、今のちゃちな“ロッカー”が夢にも見れないほどの何かを持ってた",
                    "maleVariant": null,
                    "stringId": 1854841515135606784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "んー、銀の手とか？",
                    "maleVariant": "あー、銀の手とか？",
                    "stringId": 1854841924415791104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "〈Samurai〉のヴィンテージ品を探してるの",
                    "maleVariant": "〈Samurai〉のヴィンテージ品を探してるんだ",
                    "stringId": 1854843511305543684
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "シルヴァーハンドの大ファンなの？",
                    "maleVariant": "シルヴァーハンドの大ファンなのか？",
                    "stringId": 1854846294175916032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんた、シルヴァーハンドの大ファンみたいね",
                    "maleVariant": "あんた、シルヴァーハンドの大ファンみたいだな",
                    "stringId": 1854848132841349120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "初回ライブだ…？ あんた、まさか本気で言ってんじゃないだろうな。それがどれだけ価値のあるものかわかってるのか！？",
                    "maleVariant": null,
                    "stringId": 1854849547630419968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカでジョニーがどう言われてるか教えてあげる",
                    "maleVariant": "アラサカでジョニーがどう言われてるか教えてやる",
                    "stringId": 1854850230161121284
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "レインボー・カデンザでのライブを覚えてる",
                    "maleVariant": null,
                    "stringId": 1854850486332432384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカの新人教育で、どんな脅し文句が使われるか知ってる",
                    "maleVariant": null,
                    "stringId": 1854853232477478912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "レインボー・カデンザがまだあった頃、よくあの店に行ったの。当時のライブ音源があればいい思い出になるんだけど",
                    "maleVariant": "レインボー・カデンザがまだあった頃、よくあの店に行ったんだ。当時のライブ音源があればいい思い出になるんだが",
                    "stringId": 1854865456810041344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "火事になってビールで消火したんでしょ",
                    "maleVariant": "火事になってビールで消火したんだろ",
                    "stringId": 1855925460627496964
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "デニーの髪に火が付いた",
                    "maleVariant": null,
                    "stringId": 1855926052510900224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "たしか… そう、火事があった。ビールで消そうとした奴らのこと覚えてる？",
                    "maleVariant": "たしか… そうだ、火事があった。ビールで消そうとした奴らのこと覚えてるか？",
                    "stringId": 1855927023777488896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待って… そう、火事があった。デニーの髪がチリチリになっちゃったのよね…",
                    "maleVariant": "待てよ… そうだ、火事があった。デニーの髪がチリチリになっちまったんだよな…",
                    "stringId": 1855927643813060608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "時は最良の師、なんて言うよな。前は俺もそう思ってたんだ",
                    "maleVariant": null,
                    "stringId": 1856026986524057600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「仕事の締め切りを守れないと、体が真っ二つに引き裂かれた男の幽霊に憑りつかれる」――もちろん、それがジョニーの幽霊だなんて誰も覚えてない",
                    "maleVariant": null,
                    "stringId": 1876319983446663168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これが欲しかったの",
                    "maleVariant": "これが欲しかったんだ",
                    "stringId": 1876320504798650368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "持ってけ。シルヴァーハンドの言葉を広めてくれよ。コーポには死を！",
                    "maleVariant": null,
                    "stringId": 1876320950418284544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "レーザーポップは置いてる？",
                    "maleVariant": "レーザーポップは置いてるか？",
                    "stringId": 1883552378951286784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ねえ、レーザーポップは置いてる？ 〈Us Cracks〉のやつとか。好きなんだよね",
                    "maleVariant": "なあ、レーザーポップは置いてるか？ 〈Us Cracks〉のやつとか。好きなんだよ",
                    "stringId": 1883552485050400768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなもん売ってない！ うちは本物の音楽しか取り扱ってないんだ！",
                    "maleVariant": null,
                    "stringId": 1883552802559213568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "落ち着いて… 冗談よ",
                    "maleVariant": "落ち着けって… 冗談だ",
                    "stringId": 1883552911929884672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "レーザーポップだ！？ よりによって、うちの店で、レーザーポップだって！？",
                    "maleVariant": null,
                    "stringId": 1883553011653656576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "〈Samurai〉はある？",
                    "maleVariant": "〈Samurai〉はあるか？",
                    "stringId": 1883578767146467328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おいおい、聞くまでもないだろ！？ 『Never Fade Away』、『Dancing with my Axe』、『Chippin' In』！ 何でもあるぞ！ 全曲な！",
                    "maleVariant": null,
                    "stringId": 1883578928325181440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニー・シルヴァーハンドはまさにロックの救世主だった。タダでレコードを配りたいとこだが、こっちも商売だからな",
                    "maleVariant": null,
                    "stringId": 1883579642363494400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほとばしる熱い魂に決まってるだろ！ あいつは企業中心の世界を変えようとし、理想のために命を捧げたんだ！",
                    "maleVariant": null,
                    "stringId": 1883594418376392704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "同時にテロリストでもあったと思うけど？",
                    "maleVariant": "同時にテロリストでもあったと思うがな？",
                    "stringId": 1883594511808708608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヴィンテージ品を探してる。例えば――〈Samurai〉の初回ライブ音源とか",
                    "maleVariant": null,
                    "stringId": 1883618222746091520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "音源の話をどこで聞いたのか知らないが、忘れるんだな。そこら辺の奴になんか絶対に渡さない",
                    "maleVariant": null,
                    "stringId": 1883618338693431296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほほう、そりゃアラサカの奴しか知らないネタだ。こりゃまいった！",
                    "maleVariant": null,
                    "stringId": 1883729768247705600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "『Chippin' In』は子供の頃から知ってる",
                    "maleVariant": null,
                    "stringId": 1883730042051870724
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニーはいつも一緒にいる",
                    "maleVariant": null,
                    "stringId": 1883730122465067008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "他の子が『Chrome Crayons』を習ってる時、あたしはもう『Chippin' In』を歌えてた",
                    "maleVariant": "他の子が『Chrome Crayons』を習ってる時、俺はもう『Chippin' In』を歌えてた",
                    "stringId": 1883730381874380800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしはいつもジョニーと一緒。どこへ行こうと、何をしようと、彼はあたしの生きる上での羅針盤みたいな存在になってる",
                    "maleVariant": "俺はいつもジョニーと一緒。どこへ行こうと、何をしようと、彼は俺の生きる上での羅針盤みたいな存在になってる",
                    "stringId": 1883730445879459840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はっ！ 気に入った。まいったね… おじさん、ちょいと涙ぐんじまったぜ",
                    "maleVariant": null,
                    "stringId": 1883731452411756544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "とにかく、違いのわかる若いもんには出血大サービスだ。テーブルの下にあるものを見てみな。ほら、遠慮するな",
                    "maleVariant": null,
                    "stringId": 1883739910829752320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニーが最後にケリーに贈った言葉を知ってる",
                    "maleVariant": null,
                    "stringId": 1883766702248542208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと、甘く見ないで。あたしはシルヴァーハンドマニアなの",
                    "maleVariant": "おい、甘く見るなよ。俺はシルヴァーハンドマニアなんだ",
                    "stringId": 1883767294870142976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "馬鹿な話はよせ。〈Samurai〉の初ライブを覚えてるような歳じゃないだろ。お前さんはまだ生まれてもなかったはずだ",
                    "maleVariant": null,
                    "stringId": 1883783333418876928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "外見で判断しないでよ。この顔はインプラントかもしれないでしょ？",
                    "maleVariant": "外見で判断しないほうがいい。この顔はインプラントかもしれないだろ？",
                    "stringId": 1883783426733752320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいだろう。なら証明してみろ。〈Samurai〉の3回目のライブはどう終わった？",
                    "maleVariant": null,
                    "stringId": 1883783523756392448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと助けて、ジョニー",
                    "maleVariant": "助けてくれ、ジョニー",
                    "stringId": 1883784703429558276
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニー、教えてよ",
                    "maleVariant": "ジョニー、教えてくれよ",
                    "stringId": 1883784759515791360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニー、あんたなら当然答えを知ってるでしょ。〈Samurai〉のことなんだから、助けてよ",
                    "maleVariant": "ジョニー、お前なら当然答えを知ってるだろ。〈Samurai〉のことなんだから、助けろよ",
                    "stringId": 1883784979548979200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニー！ 何て答えたらいいのか教えてよ！",
                    "maleVariant": "ジョニー！ 何て答えたらいいのか教えろよ！",
                    "stringId": 1883785047026941952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何とか言ったらどうなんだ？",
                    "maleVariant": null,
                    "stringId": 1883829558927147008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "3度目のライブはカデンザでやったんだが、ちょっとした火事になってな。酔っ払いがそれをビールで消そうとした",
                    "maleVariant": null,
                    "stringId": 1883830579116097536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハハ！ そうそう！ あれ、実はジョニーのタバコの火の不始末で起きた火事らしいぜ",
                    "maleVariant": null,
                    "stringId": 1883869859981017088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ざけんな。あれは絶対ケリーのだ。くそ、問題が起きれば何でも俺のせいかよ",
                    "maleVariant": null,
                    "stringId": 1883879709163110400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたがあの場にいたなんてまだ信じられないが… どうやら生粋のファンなのは間違いないみたいだな",
                    "maleVariant": null,
                    "stringId": 1884895243207950336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ならエピソードを話してみろよ",
                    "maleVariant": null,
                    "stringId": 1884936939874533376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最後にケリーに贈った言葉を知ってる",
                    "maleVariant": null,
                    "stringId": 1884937048188239872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でもこいつを見ろよ。この歳で、まだ2020年に生きてる",
                    "maleVariant": null,
                    "stringId": 1885000971679678464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたの大ファンよ。会えてうれしくないの？",
                    "maleVariant": "お前の大ファンだろ。会えてうれしくないのか？",
                    "stringId": 1885013542746963968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "吹っ飛ばしたはずのアラサカ・タワーが変わらずそびえ立ってる。それと一緒だ",
                    "maleVariant": null,
                    "stringId": 1885013626263945216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "延々と過去に囚われてちゃなんの意味もねえんだ。自分たちで、自分たちの手で世界を変えねえと",
                    "maleVariant": null,
                    "stringId": 1885013777527324672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "60年も経ってるのよ。何かしら変わってるはずでしょ",
                    "maleVariant": "60年も経ってるんだ。何かしら変わってるはずだろ",
                    "stringId": 1885047313688936448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "変わったのは、表面的なことだけだ。新しい接続プラグだの、わけわかんねえフルーツ味のクソ甘いシロップだの…",
                    "maleVariant": null,
                    "stringId": 1885047358383439872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何かしら変わってるはず",
                    "maleVariant": null,
                    "stringId": 1885047905605894148
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんな簡単に人は変えられない",
                    "maleVariant": null,
                    "stringId": 1885047953253187584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "人を変わらせたいのね。でもタワーみたいに壊せばいいってわけじゃない",
                    "maleVariant": "人を変わらせたいんだな。だがタワーみたいに壊せばいいってわけじゃない",
                    "stringId": 1885048100993351680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だったら音楽が答えだと思うか？ じいさんは歌詞は全部覚えてるだろうが、何一つ理解しちゃいねえ",
                    "maleVariant": null,
                    "stringId": 1885053996909707264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "〈燃やせ、反逆の道！〉",
                    "maleVariant": null,
                    "stringId": 1902512577208631296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「ビビるのをやめろ」ってアドバイスしたのよ",
                    "maleVariant": "「ビビるのをやめろ」ってアドバイスしたんだ",
                    "stringId": 1902615781397217280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「〈Samurai〉を抜けて、自分のやりたい音楽をやったらどうだ」って言ったのよ",
                    "maleVariant": "「〈Samurai〉を抜けて、自分のやりたい音楽をやったらどうだ」って言ったんだ",
                    "stringId": 1902615852180291584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふん。俺だって2人だけの時の会話ぐらい、いくらでもでっち上げられる。証明できないんだからな",
                    "maleVariant": null,
                    "stringId": 1902617875361546240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まさか、あんた正真正銘、本物のシルヴァーハンドファンなのか？",
                    "maleVariant": null,
                    "stringId": 1902709696544620544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これはどうだ？ “うるせえ、V。助手扱いすんな”",
                    "maleVariant": null,
                    "stringId": 1903741920060620800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうした？ 早く答えろ",
                    "maleVariant": null,
                    "stringId": 1903742095466414080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、3回目のライブな。昨日のことのように覚えてる。ステージでギターに火をつけてぶっ壊したんだ。客は大喜びだった",
                    "maleVariant": null,
                    "stringId": 1903753030805471232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "簡単よ。ジョニーがギターに火をつけてぶっ壊した",
                    "maleVariant": "簡単だ。ジョニーがギターに火をつけてぶっ壊した",
                    "stringId": 1903767086069170176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はずれ！ やっぱりな。駆け出しなのに楽器を壊す奴がいるか？ 買い替えるのにいくらかかんだよ",
                    "maleVariant": null,
                    "stringId": 1903767235034071040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう、ジョニー！",
                    "maleVariant": "おい、ジョニー！",
                    "stringId": 1903770626179186692
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニー、お願い",
                    "maleVariant": "ジョニー、頼む",
                    "stringId": 1903771068141387776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう、ジョニー！ なんで手を貸してくれないの！",
                    "maleVariant": "おい、ジョニー！ なんで手を貸してくれないんだ！",
                    "stringId": 1903771154510495744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ねえ、ジョニー、お願い。お互い協力し合わないとこの先やってけないでしょ？ 力を貸して",
                    "maleVariant": "なあ、ジョニー、頼むよ。お互い協力し合わないとこの先やってけないだろ？ 力を貸してくれ",
                    "stringId": 1903771218448465920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "面白いからさ。退屈だしな。誰がほいほい協力してやるかよ",
                    "maleVariant": null,
                    "stringId": 1903789698937765888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんなくだらない質問に、どうひねりを利かせた答えをしろってんだよ。俺にとっちゃ赤子の手をひねるようなもんだ",
                    "maleVariant": null,
                    "stringId": 1903802901633679360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "デニーのアフロが焦げちまって、しばらくかつらを被るハメになった。テックの髪は嫌なんだとよ",
                    "maleVariant": null,
                    "stringId": 1903809933803765760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なら、代わりに誰彼構わず、爆弾で殺したほうがいいって言うつもり？",
                    "maleVariant": "なら、代わりに誰彼構わず、爆弾で殺したほうがいいって言うのかよ？",
                    "stringId": 1903912835834691584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "中には変わりたくない人だっている。その自由を、あんたが勝手に奪う気？",
                    "maleVariant": "中には変わりたくない人だっている。その自由を、お前が勝手に奪うのか？",
                    "stringId": 1903912966277545984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうやって目を背けて、見て見ぬふりを決め込んだって、何も始まらねえんだ。目を覚まさせねえと。問題はどうやるか、だが",
                    "maleVariant": null,
                    "stringId": 1903913104253370368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカの連中も、ガワが違うだけで、中身は昔と全く変わらねえ",
                    "maleVariant": null,
                    "stringId": 1903915857025417216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなことない。あたしたちにはそれが見えないだけで、変化は常に起こってる",
                    "maleVariant": "そんなことはない。俺たちにはそれが見えないだけで、変化は常に起こってる",
                    "stringId": 1903915959165108224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、お前の言う通り人間は変わっちまったさ。機械が混ざった肉の塊にな",
                    "maleVariant": null,
                    "stringId": 1903916046255636480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと待った… お前の着てるそれ… まさか…",
                    "maleVariant": null,
                    "stringId": 1924395857093730304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うらやましい？ そう、シルヴァーハンドのジャケットよ",
                    "maleVariant": "うらやましいか？ そう、シルヴァーハンドのジャケットだ",
                    "stringId": 1927016159530172420
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "レプリカよ。懐かしいでしょ",
                    "maleVariant": "レプリカだ。懐かしいだろ",
                    "stringId": 1927016266686251008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うらやましい？ ジョニーが着てた本物よ。とあるルートで手に入れたの",
                    "maleVariant": "うらやましいか？ ジョニーが着てた本物だ。とあるルートで手に入れた",
                    "stringId": 1927016327537213440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そ、シルヴァーハンドのジャケットの完全レプリカ。どう？ 50年前がフラッシュバックしたんじゃない？",
                    "maleVariant": "そ、シルヴァーハンドのジャケットの完全レプリカ。どうだ、50年前がフラッシュバックしたんじゃないか？",
                    "stringId": 1927016364598083584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おお… さすがにたまげたな。前にそいつをこの目で見た時は、俺は青春の真っただ中だった",
                    "maleVariant": null,
                    "stringId": 1927016394847404032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ある時、ジョニーがそいつを椅子に掛けたまま、ミリテクかどこかの連中と喧嘩を始めてよ",
                    "maleVariant": null,
                    "stringId": 1927017068821725184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その時ジャケットをこっそり拝借して、ちょびーーっとだけ着てみたんだ。ああ… 懐かしいなあ",
                    "maleVariant": null,
                    "stringId": 1927017173159231488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[グッズを探す]",
                    "maleVariant": null,
                    "stringId": 1959217629940817924
                  }
                }
              ]
            }
          }
        }
      }
