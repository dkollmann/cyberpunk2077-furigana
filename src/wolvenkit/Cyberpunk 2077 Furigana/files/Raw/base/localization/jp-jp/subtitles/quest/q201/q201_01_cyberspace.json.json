      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やった… うまくいった",
                    "maleVariant": null,
                    "stringId": 1665156860480086016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハナコは、約束を守ってくれたのよ",
                    "maleVariant": "ハナコは、約束を守ってくれたんだ",
                    "stringId": 1665158049078730752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいえ、あたしは変わった",
                    "maleVariant": "いいや、俺は変わった",
                    "stringId": 1665160871258120196
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "嫌味を言わないと気が済まないの？",
                    "maleVariant": "嫌味を言わないと気が済まないのか？",
                    "stringId": 1665161449400983552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたの自意識過剰な“反逆者”気取りにはもうウンザリ",
                    "maleVariant": "お前の自意識過剰な“反逆者”気取りにはもうウンザリだ",
                    "stringId": 1665161892185268224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なに言ってるの。昔のあたしだったら、ここまで来れなかった",
                    "maleVariant": "なに言ってんだ。昔の俺だったら、ここまで来れなかった",
                    "stringId": 1665162370369478656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いちいち嫌味を言わないと気が済まないわけ？",
                    "maleVariant": "いちいち嫌味を言わないと気が済まないのかよ？",
                    "stringId": 1665162493480689664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "かもな。だが昔のお前は裏切り者でもなかった",
                    "maleVariant": null,
                    "stringId": 1665163494426173440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと。あたしがあんたを裏切ったって言うの？",
                    "maleVariant": "待てよ。俺がお前を裏切ったって言うのか？",
                    "stringId": 1665163747997016064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしはこれまで散々、ジョニー・シルヴァーハンドを見てきた",
                    "maleVariant": "俺はこれまで散々、ジョニー・シルヴァーハンドを見てきた",
                    "stringId": 1665165685597691904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いくら“世界を相手に戦う反逆者”を気取ってみせたって、もう何も響かない",
                    "maleVariant": null,
                    "stringId": 1665166211664076800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ただ話がしたいだけ。これが最後だから",
                    "maleVariant": "ただ話がしたいだけだ。これが最後だから",
                    "stringId": 1665170939047337984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "険悪なままじゃなく、友達として別れたいの",
                    "maleVariant": "険悪なままじゃなく、友達として別れたいんだ",
                    "stringId": 1665171643170316288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ぜんぶ説明させて",
                    "maleVariant": "ぜんぶ説明させてくれ",
                    "stringId": 1665180759238402048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お互い、話すことはもう何もないでしょ",
                    "maleVariant": "お互い、話すことはもう何もないだろ",
                    "stringId": 1665181711211192320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたとはそれなりに、長い付き合いだったしね。最後に話がしたかったの",
                    "maleVariant": "お前とはそれなりに、長い付き合いだったからな。最後に話がしたかった",
                    "stringId": 1665181982716878848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "険悪なままは嫌なの。友達として別れたい",
                    "maleVariant": "険悪なままは嫌なんだ。友達として別れたい",
                    "stringId": 1665182044524142592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "話を聞いてほしいの。ぜんぶ説明するから",
                    "maleVariant": "話を聞いてほしいんだ。ぜんぶ説明するから",
                    "stringId": 1665182198387990528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ話せよ",
                    "maleVariant": null,
                    "stringId": 1665187741630488576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[立ち上がる] そういう約束だったでしょ",
                    "maleVariant": "[立ち上がる] そういう約束だっただろ",
                    "stringId": 1665196199578722308
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "説明？ ああ、ぜひ頼む",
                    "maleVariant": null,
                    "stringId": 1665196792636530688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お互い、話すことなんてもう何もないでしょ",
                    "maleVariant": "お互い、話すことなんてもう何もないだろ",
                    "stringId": 1665198791272718336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これで終わりだからね… 最後にお別れを言いに来た、それだけよ",
                    "maleVariant": "これで終わりだからな… 最後にお別れを言いに来た、それだけだ",
                    "stringId": 1665199585640341504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしは今、手術台で頭を切り開かれてる。今まさに、アラサカの医者がチップを取り出そうとしてるの",
                    "maleVariant": "俺は今、手術台で頭を切り開かれてる。今まさに、アラサカの医者がチップを取り出そうとしてるんだ",
                    "stringId": 1665203495570530304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こうなることは、わかってたでしょ…",
                    "maleVariant": "こうなることは、わかってただろ…",
                    "stringId": 1665205892514603008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いつかあたしが、普通の人生を取り戻そうとするって",
                    "maleVariant": "いつか俺が、普通の人生を取り戻そうとするって",
                    "stringId": 1665207399444144128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "すべて元通りに戻れるとでも思ってんのか？ 連中は“俺たち”の一部を切り取ってる。その穴は、二度と埋めることはできねえんだよ",
                    "maleVariant": null,
                    "stringId": 1665220462100635648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[立ち上がる] 他にどうすればよかったって言うの？",
                    "maleVariant": "[立ち上がる] 他にどうすればよかったって言うんだ？",
                    "stringId": 1665220611786956800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[立ち上がる] “俺たち”じゃない！",
                    "maleVariant": null,
                    "stringId": 1665221715023126528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そういう約束だったでしょ。あたしが残り、あんたが消える",
                    "maleVariant": "そういう約束だっただろ。俺が残り、お前が消える",
                    "stringId": 1665222605087989760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こうするしかなかった",
                    "maleVariant": "こうするしかなかったんだ",
                    "stringId": 1665222670334582784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "“俺たち”じゃない！ あたしはあたしでしかない。あんたは、ただのコンストラクトよ",
                    "maleVariant": "“俺たち”じゃない！ 俺は俺でしかない。お前は、ただのコンストラクトだ",
                    "stringId": 1665222745278406656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本当にそうか？ 自分の心に忠実であればよかっただけだろ",
                    "maleVariant": null,
                    "stringId": 1665224166426042368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "支え合って共に戦う――それが約束だったはずだ",
                    "maleVariant": null,
                    "stringId": 1665226059550318592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ何なの？",
                    "maleVariant": "じゃあ何なんだ？",
                    "stringId": 1665226614960054276
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかってないのはそっちよ",
                    "maleVariant": "わかってないのはそっちだ",
                    "stringId": 1665227503380750336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自分自身を否定する気か？ お前は俺なんだぞ",
                    "maleVariant": null,
                    "stringId": 1668961775329501184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "またアラサカに捕らわれる心配をしてるの？ 大丈夫よ、あんたはちゃんと処分される",
                    "maleVariant": "またアラサカに捕らわれる心配をしてるのか？ 大丈夫だ、お前はちゃんと処分される",
                    "stringId": 1669203844367147008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふん、何もわかってねえな。これはアラサカの話でも、生きるか死ぬかの話でもない",
                    "maleVariant": null,
                    "stringId": 1669205245331791872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ何なの、ジョニー。教えてよ",
                    "maleVariant": "じゃあ何なんだ、ジョニー。教えろよ",
                    "stringId": 1669206690806390784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかってないのはそっちよ",
                    "maleVariant": "わかってないのはそっちだ",
                    "stringId": 1669209020373164032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これ以上、話しても無駄ね… さよなら、ジョニー",
                    "maleVariant": "これ以上、話しても無駄だ… じゃあな、ジョニー",
                    "stringId": 1669209091055575040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんた自身が迷子だから、身近な人が自分の道を見つけることが許せないんでしょ",
                    "maleVariant": "お前自身が迷子だから、身近な人が自分の道を見つけることが許せないんだろ",
                    "stringId": 1669210115170394112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "オルト、ローグ、ケリー… みんなそう",
                    "maleVariant": "オルト、ローグ、ケリー… みんなそうだ",
                    "stringId": 1669210645766627328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "昔のことだ。俺は変わった",
                    "maleVariant": null,
                    "stringId": 1669212681144918016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニー、あんたは不幸をまき散らす“汚い爆弾”よ。自分の自殺願望に周りの人間を巻き込んだあげく、破滅させる",
                    "maleVariant": "ジョニー、お前は不幸をまき散らす“汚い爆弾”だ。自分の自殺願望に周りの人間を巻き込んだあげく、破滅させる",
                    "stringId": 1669333421802414080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "巻き添えはご免よ。あたしは自分の人生を生きる",
                    "maleVariant": "巻き添えはご免だ。俺は自分の人生を生きる",
                    "stringId": 1669337879122829312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "信念の問題だ。いつだってそうさ",
                    "maleVariant": null,
                    "stringId": 1669349426377285632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "体をクロームと入れ替えようが、BDの幻想に逃げ込もうが、これだけは変わらない。そいつが何者なのかを決めるのは、結局何を信じ、何を守って生きてるかだ",
                    "maleVariant": null,
                    "stringId": 1669349835858796544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "迷った時には、それが帰り道を示し――",
                    "maleVariant": null,
                    "stringId": 1669351355740348416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "粉々に砕け散っても、自分を繋ぎ止めてくれる",
                    "maleVariant": null,
                    "stringId": 1669352973986717696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それが伝説のロッカーボーイ、シルヴァーハンドの信条ってわけ？",
                    "maleVariant": "それが伝説のロッカーボーイ、シルヴァーハンドの信条ってわけか？",
                    "stringId": 1669353294515429376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で、その人生に、意味はあんのか？",
                    "maleVariant": null,
                    "stringId": 1669360817637187584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さよなら、ジョニー",
                    "maleVariant": "じゃあな、ジョニー",
                    "stringId": 1669361569474568192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お別れよ、ジョニー。今までありがとう",
                    "maleVariant": "お別れだ、ジョニー。今までありがとう",
                    "stringId": 1669362061516759040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしの前から消えて",
                    "maleVariant": "俺の前から消えろ",
                    "stringId": 1669362289418461184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これ以上話しても何もいいことはなさそうね… さよなら、ジョニー。今まで… ありがとう",
                    "maleVariant": "これ以上話しても何もいいことはなさそうだ… じゃあな、ジョニー。今まで… ありがとう",
                    "stringId": 1669416608406724608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これ以上、話しても無駄ね。もうさっさと消えて",
                    "maleVariant": "これ以上、話しても無駄だな。もうさっさと消えてくれ",
                    "stringId": 1669416755928784896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "悪いが、良心を消すミュートボタンはねえんだ。俺の一部はお前と一緒にいる。ずっとな",
                    "maleVariant": null,
                    "stringId": 1669424715509817344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたとの旅もここまでよ、ジョニー… 後悔はしない。でも思い出すこともない",
                    "maleVariant": "お前との旅もここまでだ、ジョニー… 後悔はしない。だが思い出すこともない",
                    "stringId": 1669427067188961280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいや、後悔するさ。ダチを失って後悔しない奴はいねえ",
                    "maleVariant": null,
                    "stringId": 1669433403238801408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジャッキーがチップを渡した時の、死に際のセリフを覚えてるか？ 「あとは任せたぞ」って言ってたよな？",
                    "maleVariant": null,
                    "stringId": 1669442182269394944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ… そうだな",
                    "maleVariant": null,
                    "stringId": 1669445754843324416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これで終わりよ。お別れを言いに来たの",
                    "maleVariant": "これで終わりだ。別れを言いに来た",
                    "stringId": 1669484485465890816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "デクスに 「平穏な生涯と栄誉の死」、どっちを選ぶか聞かれたよな",
                    "maleVariant": null,
                    "stringId": 1669505130165723136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、覚えてる",
                    "maleVariant": "ああ、覚えてる",
                    "stringId": 1669505388652290048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前は選択を間違った。がっかりだよ",
                    "maleVariant": null,
                    "stringId": 1669505462287491072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたには命を救われた。何度もね",
                    "maleVariant": "お前には命を救われた。何度もな",
                    "stringId": 1669507009935335424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今のお前を見てると、初めて会った時のことを思い出す",
                    "maleVariant": null,
                    "stringId": 1694008916045225984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "傭兵としてはまだ駆け出しで、夢ばっか見てる、ちんけなコソ泥をな。お前ってやつは、そこからちっとも変わっちゃいない",
                    "maleVariant": null,
                    "stringId": 1694009401275867136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "きっと… すぐ死んでたはず。あたしは追いつめられて、強くなったのよ",
                    "maleVariant": "きっと… すぐ死んでたはずだ。俺は追いつめられて、強くなったのさ",
                    "stringId": 1694021086774583296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それどころじゃねえ。お前は自分自身を裏切った",
                    "maleVariant": null,
                    "stringId": 1694022471633100800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "昔の… 誰かさんを思い出すな",
                    "maleVariant": null,
                    "stringId": 1694194782667563008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺は人のことなんてどうでもよかった。簡単に裏切り、それを自慢にさえ思ってた",
                    "maleVariant": null,
                    "stringId": 1694195665065238528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと。あたしがあんたを裏切ったって言うの？",
                    "maleVariant": "待てよ。俺がお前を裏切ったって言うのか？",
                    "stringId": 1694202070237208576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや、それどころじゃねえ。自分自身を裏切ったんだよ",
                    "maleVariant": null,
                    "stringId": 1694206194261450752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まいったな。これじゃまるで昔の自分の姿を見てるみてえだ",
                    "maleVariant": null,
                    "stringId": 1694206576698089472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ローグ、オルト… 俺はみんな見捨てちまった。今度はお前が俺を見捨て、自分自身まで見捨てようとしてる",
                    "maleVariant": null,
                    "stringId": 1694207973686534144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[座る] あんたに言われる筋合いはない",
                    "maleVariant": "[座る] お前に言われる筋合いはない",
                    "stringId": 1694212410119315456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[座る] 何が言いたいわけ？",
                    "maleVariant": "[座る] 何が言いたいんだ？",
                    "stringId": 1694212927595765760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたに言われる筋合いはない",
                    "maleVariant": "お前に言われる筋合いはない",
                    "stringId": 1694214985774292992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あるに決まってんだろ。チップは俺たちの記憶を交わらせただけじゃない",
                    "maleVariant": null,
                    "stringId": 1694218091337637888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "Vとシルヴァーハンドを分け隔てる境界線は消え去った。それも、随分前にな",
                    "maleVariant": null,
                    "stringId": 1694219691816935424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何が言いたいわけ？",
                    "maleVariant": "何が言いたいんだ？",
                    "stringId": 1694222688747806720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だからあたしを罵る権利があるって？",
                    "maleVariant": "だから俺を罵る権利があるってか？",
                    "stringId": 1694223699239526400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺はVなんだよ。ジャッキーの死に打ちのめされ、ブリジットを憎み、罪の意識を抱えるVなんだ",
                    "maleVariant": null,
                    "stringId": 1694224445188104192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "チップは俺たちの記憶を交わらせただけじゃない",
                    "maleVariant": null,
                    "stringId": 1694229407267840000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だがお前は？ いったい誰だ？",
                    "maleVariant": null,
                    "stringId": 1694231683181076480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしはここにいる！ あたしの意志は、あたしだけのものよ",
                    "maleVariant": "俺はここにいる！ 俺の意志は、俺だけのものだ",
                    "stringId": 1694234354701066240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "Vとシルヴァーハンドを分け隔てる境界線は消え去った。それも、随分前にな",
                    "maleVariant": null,
                    "stringId": 1694235873357881344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アタシ、アタシ、アタシ――じゃあその“アタシ”ってのは誰なんだ？",
                    "maleVariant": "オレ、オレ、オレ――じゃあその“オレ”ってのは誰なんだ？",
                    "stringId": 1694239405985259520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ゴミ捨て場で死んだ、あの時のままのVよ",
                    "maleVariant": "ゴミ捨て場で死んだ、あの時のままのVだ",
                    "stringId": 1694240140978315264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "生きることを選んだVよ",
                    "maleVariant": "生きることを選んだVだ",
                    "stringId": 1694240576598728704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "悔しくて、悲しい。それが今のあたし",
                    "maleVariant": "悔しくて、悲しい。それが今の俺だ",
                    "stringId": 1694240837618655232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ゴミ捨て場で死んだあの時のあたしから、何も変わってない",
                    "maleVariant": "ゴミ捨て場で死んだあの時の俺から、何も変わってない",
                    "stringId": 1694243400858492928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "生きることを選んだVよ",
                    "maleVariant": "生きることを選んだVだ",
                    "stringId": 1694243478738329600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんというか… 内側に目を向けると、よくわからなくなる",
                    "maleVariant": null,
                    "stringId": 1694243580223709184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "始めたことをやり通した。あたしは勝者よ",
                    "maleVariant": "始めたことをやり通した。俺は勝者だ",
                    "stringId": 1694244238712659968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自分が誰だかわからない",
                    "maleVariant": null,
                    "stringId": 1694244799776317440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "過去のあたしはもういない",
                    "maleVariant": "過去の俺はもういない",
                    "stringId": 1694247754613153792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう… 自分が誰だかわからない",
                    "maleVariant": null,
                    "stringId": 1694247865963536384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その闘争心は？ 死神に中指を立てる度胸は？ その性格はどっから来た？",
                    "maleVariant": null,
                    "stringId": 1695640356705034240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしはもとからそういう人間よ",
                    "maleVariant": "俺はもとからそういう人間だ",
                    "stringId": 1695641615398887424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本当にそうか？ 記憶をたどってみろ。それがすべて自分のもんだと確信をもって言えるか？",
                    "maleVariant": null,
                    "stringId": 1695642590910111744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "生きること――それこそがシルヴァーハンドの原動力だった",
                    "maleVariant": null,
                    "stringId": 1695648521202982912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それと、2つのポリシー。決して振り返らない、決して周りを当てにしない――",
                    "maleVariant": null,
                    "stringId": 1695649859655720960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ただ突き進み、死神の目を真正面から見据えてたのが俺だ",
                    "maleVariant": null,
                    "stringId": 1695650526969487360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんただけが特別なわけじゃない",
                    "maleVariant": "お前だけが特別なわけじゃない",
                    "stringId": 1695650796260581376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その間違いに気付かなかったのも俺だけじゃないがな",
                    "maleVariant": null,
                    "stringId": 1695652038546317312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしは前よりも強くなった。でも… 頭の中はグチャグチャよ",
                    "maleVariant": "俺は前よりも強くなった。だが… 頭の中はグチャグチャだ",
                    "stringId": 1695655158353518592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今の気分は？",
                    "maleVariant": null,
                    "stringId": 1695656533581914112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "悔しくて… 悲しい",
                    "maleVariant": null,
                    "stringId": 1695656644697415680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そして失望してる。わかるよ",
                    "maleVariant": null,
                    "stringId": 1695656880383746048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "前よりずっと強くなった。自分の命を勝ち取ったのよ",
                    "maleVariant": "前よりずっと強くなった。自分の命を勝ち取ったんだぞ",
                    "stringId": 1695662089927417856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、気持ちはわかるぜ。なんせ、それは“俺の”気持ちだからな",
                    "maleVariant": null,
                    "stringId": 1695662765344579584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "知ったような口を利かないで！",
                    "maleVariant": "知ったような口を利くんじゃねえ！",
                    "stringId": 1695663884988866560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おっと、今のはいかにもシルヴァーハンドだな",
                    "maleVariant": null,
                    "stringId": 1695664874710392832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前は俺だ。そして俺はお前なんだよ",
                    "maleVariant": null,
                    "stringId": 1695687479374929920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺に何か言いたいなら、それはお前自身が聞きたがってるってことだ",
                    "maleVariant": null,
                    "stringId": 1695688487954046976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そいつはもう無理かもな",
                    "maleVariant": null,
                    "stringId": 1695693560343977984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "戦いはもう終わった",
                    "maleVariant": null,
                    "stringId": 1695724352524804096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "膝をついて生きるくらいなら、立ったまま死ぬ",
                    "maleVariant": null,
                    "stringId": 1695724536403091456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それに魂の一部を悪魔に売り渡した時点で、お前はお前じゃない。別の誰かだ",
                    "maleVariant": null,
                    "stringId": 1695737633134108672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニーがVから学んだことさ。昔のVからな",
                    "maleVariant": null,
                    "stringId": 1695751917423583232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前は自分の、俺たちの一部を企業に売り渡した。本当にそれでよかったのか？",
                    "maleVariant": null,
                    "stringId": 1699699311152488448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今のお前を見て、がっかりしてるだろうぜ",
                    "maleVariant": null,
                    "stringId": 1726090084152270848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[立ち上がる] 結局またアラサカのせいにする気？",
                    "maleVariant": "[立ち上がる] 結局またアラサカのせいにする気か？",
                    "stringId": 1790844682079068160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あれが嫌、これが嫌、望みどおりにならないのはいつも誰かのせい",
                    "maleVariant": null,
                    "stringId": 1790845828147470336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何もかもアラサカが悪いって話は、もういい加減、聞き飽きたのよ",
                    "maleVariant": "何もかもアラサカが悪いって話は、もういい加減、聞き飽きたんだよ",
                    "stringId": 1790846475496349696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカは関係ない。これは信頼の、そしてどんどん歪んでってるお前の問題だ",
                    "maleVariant": null,
                    "stringId": 1790846958411735040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[ジョニーに触れる] ハナコは約束を守ってくれた",
                    "maleVariant": null,
                    "stringId": 2115926178800558084
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "信じらんねえ…",
                    "maleVariant": null,
                    "stringId": 2534592891186393096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ったく…",
                    "maleVariant": null,
                    "stringId": 2534593761907126272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうなんだ？",
                    "maleVariant": null,
                    "stringId": 2534593910637146120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で？",
                    "maleVariant": null,
                    "stringId": 2534594130318012424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おい、どうした",
                    "maleVariant": null,
                    "stringId": 2534594286161571848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やれやれ",
                    "maleVariant": null,
                    "stringId": 2534594676701605896
                  }
                }
              ]
            }
          }
        }
