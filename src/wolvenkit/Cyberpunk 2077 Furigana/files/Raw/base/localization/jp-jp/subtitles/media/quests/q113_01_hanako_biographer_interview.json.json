    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティの皆さん、こんばんは！ 本日のゲストは『沈黙の花――ハナコ・アラサカの語られざる物語』の著者、マーク・ムラトヴィッチさんです。ようこそ、マーク",
                    "maleVariant": null,
                    "stringId": 1792180996126187520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "呼んでくれてありがとう、ジギー",
                    "maleVariant": null,
                    "stringId": 1795415353056804864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本を書くなら、もう少し安全なテーマにしようと思わなかったんですか？ スカベンジャーの密着とか、火山特集の方がまだいい",
                    "maleVariant": null,
                    "stringId": 1795415609815318528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうですね。愛娘に関する本を私が執筆すると聞かれた時、サブロウ・アラサカ氏は決して良い顔をされていませんでしたからね",
                    "maleVariant": null,
                    "stringId": 1795415967505563648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "覚えてない視聴者のためにおさらいすると、以前ハナコ・アラサカ氏の伝記を書いた人は“自殺”したんです。かわいそうに、自分の背中を10回も撃って",
                    "maleVariant": null,
                    "stringId": 1795416276072120320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、あれは悲惨でした… でもあの著者は同意を得ずに、秘密裏に書いていたんです",
                    "maleVariant": null,
                    "stringId": 1795416586786160640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まさか、今回はこっそり書いてませんよね？",
                    "maleVariant": null,
                    "stringId": 1795416855221616640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もちろんです",
                    "maleVariant": null,
                    "stringId": 1795417021232168960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よかった。では、お聞きします。『沈黙の花』というタイトル、どうやって思いつかれたんでしょう？",
                    "maleVariant": null,
                    "stringId": 1795417197443268608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "タイトルについては、「言わぬが花」という日本の古いことわざから着想を得ています",
                    "maleVariant": null,
                    "stringId": 1795417383603257344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "これは、ハナコ氏を完璧に表す言葉ではありませんか。年に数回しか自宅から出ず、公の場でもほとんど喋ることなく、いつだってボディガードの壁に囲まれている",
                    "maleVariant": null,
                    "stringId": 1795417608032075776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なるほど。一年中ガラスの城に閉じこもって、いったい何をしてるんでしょうね？ 折り紙？ クロスワードパズル？",
                    "maleVariant": null,
                    "stringId": 1795419225758351360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私にも謎です。でも自宅には300テラバイトの帯域幅を持つ光ファイバー回線が繋がっていますから… まあ",
                    "maleVariant": null,
                    "stringId": 1795419475789201408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まさか、彼女はネットランナー？",
                    "maleVariant": null,
                    "stringId": 1795419782912917504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや、私はそうは言ってませんよ",
                    "maleVariant": null,
                    "stringId": 1795419966086561792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "CMの時間ですが、チャンネルはどうぞそのまま！ まだまだマーク・ムラトヴィッチさんにお話を伺います！",
                    "maleVariant": null,
                    "stringId": 1795420188636332032
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}