    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1903987643713077248,
            "femaleVariant": "あのさ、俺は名前を覚えるのは苦手だが、顔はよく覚えてる方なんだよ！"
          },
          {
            "stringId": 1903988419156000768,
            "femaleVariant": "人違いだ"
          },
          {
            "stringId": 1903991552703741952,
            "femaleVariant": "V、よう来はったな"
          },
          {
            "stringId": 1903993144156897280,
            "femaleVariant": "ええやろ。ほなら用件言うてみ。電話ならいつでもできる"
          },
          {
            "stringId": 1903993925975162884,
            "femaleVariant": "ジャパンタウンのパレードについて情報が欲しい"
          },
          {
            "stringId": 1903995722294902784,
            "femaleVariant": "ジャパンタウンでパレードがある。その情報が欲しいの",
            "maleVariant": "ジャパンタウンでパレードがある。その情報が欲しいんだ"
          },
          {
            "stringId": 1903995723318312960,
            "femaleVariant": "またアラサカ絡みかいな"
          },
          {
            "stringId": 1903995723754520576,
            "femaleVariant": "仕事熱心なのは構わへんけど、なんでわざわざうちとこに？"
          },
          {
            "stringId": 1903995724257837056,
            "femaleVariant": "ミスター・デショーンは電話にでえへんかったんか？"
          },
          {
            "stringId": 1903995730431852544,
            "femaleVariant": "とまあ、こんなところや"
          },
          {
            "stringId": 1903995730884837376,
            "femaleVariant": "十分です"
          },
          {
            "stringId": 1903995731304267776,
            "femaleVariant": "ほんならもう帰ってんか"
          },
          {
            "stringId": 1903998593631535104,
            "femaleVariant": "ここまで聞いて、何をするか知りたくないの？",
            "maleVariant": "ここまで聞いて、何をするか知りたくないのか？"
          },
          {
            "stringId": 1904047836438216704,
            "femaleVariant": "オカダさん、恩に着ます"
          },
          {
            "stringId": 1904215148533399552,
            "femaleVariant": "あんた、どっかで見たんだよな。テレビか…？"
          },
          {
            "stringId": 1904219722086367232,
            "femaleVariant": "おお、いいところに来てくれた"
          },
          {
            "stringId": 1904221548135333888,
            "femaleVariant": "V、さっさと店の中に入るぞ"
          },
          {
            "stringId": 1904221766977339396,
            "femaleVariant": "ええ、行きましょ",
            "maleVariant": "ああ、行こう"
          },
          {
            "stringId": 1904222141528686592,
            "femaleVariant": "ヒデシ、せっかくのファンに悪いでしょ",
            "maleVariant": "ヒデシ、せっかくのファンに悪いだろ"
          },
          {
            "stringId": 1904222723580641280,
            "femaleVariant": "ヒデシ、せっかくのファンに悪いじゃない",
            "maleVariant": "ヒデシ、せっかくのファンに悪いだろ"
          },
          {
            "stringId": 1904223093132378112,
            "femaleVariant": "やめろ、今はふざけてる時ではない"
          },
          {
            "stringId": 1904223451057504256,
            "femaleVariant": "頼むって！ 久し振りに聞かせてくれよ！"
          },
          {
            "stringId": 1904223843040378880,
            "femaleVariant": "ええ、行きましょ",
            "maleVariant": "ああ、行こう"
          },
          {
            "stringId": 1904253663501631488,
            "femaleVariant": "これで肝心な情報は大体手に入ったと言えるが――"
          },
          {
            "stringId": 1904255041364049920,
            "femaleVariant": "念のため、もう少し調べてみる。ここに残って、いくつかツテを当たってみるとしよう"
          },
          {
            "stringId": 1904255307652022272,
            "femaleVariant": "何か新しいことがわかったら、すぐに連絡する"
          },
          {
            "stringId": 1904256366093025280,
            "femaleVariant": "なんだって？"
          },
          {
            "stringId": 1904256637951033344,
            "femaleVariant": "あとは神頼みしかないってくらい、思い詰めた顔してるからさ"
          },
          {
            "stringId": 1904258241215348736,
            "femaleVariant": "そう見えるのか？"
          },
          {
            "stringId": 1904258646921986048,
            "femaleVariant": "運命はルーレットと同じ。勇気を出してボタンを押せば、道が開けることもある"
          },
          {
            "stringId": 1904303405732478976,
            "femaleVariant": "本題の前に… サンドラ・ドーセットの件の報酬は？"
          },
          {
            "stringId": 1904303938123874304,
            "femaleVariant": "本題の前に… サンドラ・ドーセットの件の報酬がまだなんだけど？",
            "maleVariant": "本題の前に… サンドラ・ドーセットの件の報酬がまだなんだが？"
          },
          {
            "stringId": 1904304511636226048,
            "femaleVariant": "今までほったらかしにしとったやろ。何や、急にせっついてからに"
          },
          {
            "stringId": 1904304649091956736,
            "femaleVariant": "V、今はよせ。それよりここに来た理由を話すんだ"
          },
          {
            "stringId": 1949140527975841792,
            "femaleVariant": "俺を別の誰かと勘違いしてくれて助かった"
          },
          {
            "stringId": 1950228564071759876,
            "femaleVariant": "聡明で、魅力的な女性だ"
          },
          {
            "stringId": 1978123618848096256,
            "femaleVariant": "このチップに入っとる"
          },
          {
            "stringId": 1978125649344217088,
            "femaleVariant": "ジャパンタウン――"
          },
          {
            "stringId": 1978128300127567872,
            "femaleVariant": "パレードの間は地区全体が封鎖される"
          },
          {
            "stringId": 1978128482277801984,
            "femaleVariant": "メインとなるのは山車の行列や。通りの上空をこっちに向けて通過する"
          },
          {
            "stringId": 1978128790844358656,
            "femaleVariant": "主賓のハナコ・アラサカは、一番大きな山車からスピーチを行う予定になっとる。せやな、具体的な場所は… ここや"
          },
          {
            "stringId": 1978128951804968960,
            "femaleVariant": "警備についての情報はあらへん。まあ、ゆうても最近色々あったし、それなりに厳重やろな"
          },
          {
            "stringId": 1978129704615428096,
            "femaleVariant": "前回のパレードでは、アラサカの奴らが身分を偽って群衆に紛れとったわ"
          },
          {
            "stringId": 1978130012393455616,
            "femaleVariant": "そこら中に狙撃手やドローンがおるのは当たり前。それだけでも十分厄介やけど、少なくとも1人はネットランナーがおるやろな"
          },
          {
            "stringId": 1978131476138442752,
            "femaleVariant": "山車の行列がジャパンタウンを出たら、パレードは終いや。後に残るんは、誰も片づけへんゴミの山っちゅう話やな"
          },
          {
            "stringId": 1996740137278521344,
            "femaleVariant": "[チップを受け取る]"
          },
          {
            "stringId": 2004126178845990912,
            "femaleVariant": "ローグ、落ち着きなはれ。あんた何やえらい誤解しとるようやな…"
          },
          {
            "stringId": 2004127993167683584,
            "femaleVariant": "あんたのチームを襲ったんが誰や知らんけど、クロウズやないのは確かや。うちの子らは分をわきまえとる"
          },
          {
            "stringId": 2004131298379898880,
            "femaleVariant": "かんにん、もう切るで。お客さんが、来はってん"
          },
          {
            "stringId": 2004141114494099456,
            "femaleVariant": "そっちのええ男は誰や…？"
          },
          {
            "stringId": 2004142566830579716,
            "femaleVariant": "仕事相手よ",
            "maleVariant": "仕事相手だ"
          },
          {
            "stringId": 2004142835333144576,
            "femaleVariant": "コメディアンのヒデシ・ヒノよ",
            "maleVariant": "コメディアンのヒデシ・ヒノだ"
          },
          {
            "stringId": 2004143907766345728,
            "femaleVariant": "たまたま一緒に仕事をしてるだけの相手よ",
            "maleVariant": "たまたま一緒に仕事をしてるだけの相手さ"
          },
          {
            "stringId": 2004144025626288128,
            "femaleVariant": "コメディアンのヒデシ・ヒノよ",
            "maleVariant": "コメディアンのヒデシ・ヒノだ"
          },
          {
            "stringId": 2004147944096079872,
            "femaleVariant": "ゴロウ・タケムラです。お会いできて光栄です"
          },
          {
            "stringId": 2004157714089713664,
            "femaleVariant": "あら… えらい紳士的やないの！"
          },
          {
            "stringId": 2004158060891545600,
            "femaleVariant": "気の毒やな。聞いた話やと、アラサカの兵隊はんに追われとるんやろ？"
          },
          {
            "stringId": 2004159007277522944,
            "femaleVariant": "V、なんでうちに連れてきたんや？"
          },
          {
            "stringId": 2004160816415055872,
            "femaleVariant": "今すぐ然るべきとこに“通報”せんほうがええ理由があるんやったら、言うてみ"
          },
          {
            "stringId": 2004162517977079808,
            "femaleVariant": "仕事を持ってきたの。エディーは好きでしょ？",
            "maleVariant": "仕事を持ってきた。エディーは好きだろ？"
          },
          {
            "stringId": 2004162517977079824,
            "femaleVariant": "そんなこと言って、興味があるんでしょ",
            "maleVariant": "そんなこと言って、興味があるんだろ"
          },
          {
            "stringId": 2004164679318708224,
            "femaleVariant": "その気があればとっくに通報してるはずよ",
            "maleVariant": "その気があればとっくに通報してるはずだ"
          },
          {
            "stringId": 2004165303666024448,
            "femaleVariant": "つまりあんたは、あたしたちに興味がある",
            "maleVariant": "つまりあんたは、俺たちに興味がある"
          },
          {
            "stringId": 2004166166031704064,
            "femaleVariant": "金よ。エディーは大好きでしょ、ワカコ",
            "maleVariant": "金さ。エディーは大好きだろ、ワカコ"
          },
          {
            "stringId": 2004168505715138560,
            "femaleVariant": "たんまり儲かる仕事の話がある。単純でしょ",
            "maleVariant": "たんまり儲かる仕事の話がある。単純だろ"
          },
          {
            "stringId": 2004179920530251776,
            "femaleVariant": "あんたもグルだと思われるのがオチよ",
            "maleVariant": "あんたもグルだと思われるのがオチだ"
          },
          {
            "stringId": 2004180476309086208,
            "femaleVariant": "冷静になって",
            "maleVariant": "冷静になれよ"
          },
          {
            "stringId": 2004183841835393024,
            "femaleVariant": "なんやて？"
          },
          {
            "stringId": 2004183977797951488,
            "femaleVariant": "日本人の逃亡犯がナイトシティに潜伏してるってだけで、あんたの関与が疑われるのは間違いない"
          },
          {
            "stringId": 2004184860313067520,
            "femaleVariant": "通報なんてしたら、あたしたちごとあんたも消されることくらい、考えなくてもわかるでしょ",
            "maleVariant": "通報なんてしたら、俺たちごとあんたも消されることくらい、考えなくてもわかるだろ"
          },
          {
            "stringId": 2004242184151048192,
            "femaleVariant": "あんたはこの界隈を熟知してる"
          },
          {
            "stringId": 2004242184167825416,
            "femaleVariant": "アラサカに孫を殺されたそうね",
            "maleVariant": "アラサカに孫を殺されたそうだな"
          },
          {
            "stringId": 2004242341655552004,
            "femaleVariant": "ゴロウ。デクスは今どうしてるんだっけ？",
            "maleVariant": "ゴロウ。デクスは今どうしてるんだった？"
          },
          {
            "stringId": 2004243285038407680,
            "femaleVariant": "ねえ、ゴロウ、裏切り者のデクスとはどうして連絡がつかないんだっけ？",
            "maleVariant": "なあ、ゴロウ、裏切り者のデクスとはどうして連絡がつかないんだった？"
          },
          {
            "stringId": 2004244253117325312,
            "femaleVariant": "奴は死んだ"
          },
          {
            "stringId": 2004244644781432832,
            "femaleVariant": "ゴミ捨て場でアラサカの刺客に始末されたの。ほんと、お気の毒",
            "maleVariant": "ゴミ捨て場でアラサカの刺客に始末されたんだ。ほんと、気の毒に"
          },
          {
            "stringId": 2004250263722729472,
            "femaleVariant": "タイガークロウズは、さんざんアラサカにいいようにこき使われてきたんでしょ？",
            "maleVariant": "タイガークロウズは、さんざんアラサカにいいようにこき使われてきたんだろ？"
          },
          {
            "stringId": 2004250368144121856,
            "femaleVariant": "この界隈の全てを把握してるワカコを見込んで、頼んでるの",
            "maleVariant": "この界隈の全てを把握してるワカコを見込んで、頼んでるんだ"
          },
          {
            "stringId": 2004253370577920000,
            "femaleVariant": "それは、うちを脅してるんか？"
          },
          {
            "stringId": 2004259661295722496,
            "femaleVariant": "事実を伝えたまでよ",
            "maleVariant": "事実を伝えたまでだ"
          },
          {
            "stringId": 2004261099640004608,
            "femaleVariant": "そんな、まさか。なぜデクスに頼まないんだって聞くから、事実を答えたまでよ",
            "maleVariant": "そんな、まさか。なぜデクスに頼まないんだって聞くから、事実を答えたまでだ"
          },
          {
            "stringId": 2004261778513272832,
            "femaleVariant": "脅すなんて、そんな！"
          },
          {
            "stringId": 2004263529400311808,
            "femaleVariant": "オカダさん… 俺たちに悪意はありません。どうしてもこの情報が必要なだけなんです。俺たちの命がかかっている"
          },
          {
            "stringId": 2004269437345931264,
            "femaleVariant": "サブロウ・アラサカを偲ぶ大規模なパレードなら、あんたも何かしら絡んでるんでしょ？",
            "maleVariant": "サブロウ・アラサカを偲ぶ大規模なパレードなら、あんたも何かしら絡んでるんだろ？"
          },
          {
            "stringId": 2004271140082360320,
            "femaleVariant": "孫を殺した連中を、好きになれるはずがない"
          },
          {
            "stringId": 2004271955505389568,
            "femaleVariant": "遠い昔のことや"
          },
          {
            "stringId": 2004272043501887488,
            "femaleVariant": "でもそんな心の傷は、時間が経ったからって…"
          },
          {
            "stringId": 2004272695800045568,
            "femaleVariant": "そら、癒えへんよ"
          },
          {
            "stringId": 2005264550398337024,
            "femaleVariant": "わかった！ ヒデシ・ヒノだろ！ こんな有名人に会えるとは！"
          },
          {
            "stringId": 2005292913204731904,
            "femaleVariant": "知ってるか？ 深夜のコメディ番組で司会をやってたヒデシ・ヒノだよ！ 一時はかなり勢いあったよな！"
          },
          {
            "stringId": 2005294358343786496,
            "femaleVariant": "しかしコメディアンとは"
          },
          {
            "stringId": 2005303863274184704,
            "femaleVariant": "あー…"
          },
          {
            "stringId": 2005304026466164736,
            "femaleVariant": "東京へ… 行きたいか…"
          },
          {
            "stringId": 2005306551588167680,
            "femaleVariant": "うわあ…"
          },
          {
            "stringId": 2005307167748202496,
            "femaleVariant": "ヒノさん… あんた、どうしちゃったんだ？"
          },
          {
            "stringId": 2005311642600808448,
            "femaleVariant": "あれやってくれよ、“東京へ行きたいかー！”って"
          },
          {
            "stringId": 2005315512215347200,
            "femaleVariant": "頼むよ。あんたの持ちネタだろ？ ほら、1回だけさ！"
          },
          {
            "stringId": 2005320029463977984,
            "femaleVariant": "断る"
          },
          {
            "stringId": 2005322291770892288,
            "femaleVariant": "さあな、知らん。もうかつての俺じゃないんだ"
          },
          {
            "stringId": 2005325154400149504,
            "femaleVariant": "あの伝説のヒデシ・ヒノも落ちぶれたもんだな… ガッカリだ…"
          },
          {
            "stringId": 2005326924765536256,
            "femaleVariant": "もういいだろ？ 行くぞ"
          },
          {
            "stringId": 2005329702116204544,
            "femaleVariant": "この野郎"
          },
          {
            "stringId": 2005353636475772928,
            "femaleVariant": "V、アラサカ相手に何をすんのか知らへんけど… 今度は上手にやるんやで"
          },
          {
            "stringId": 2005385803247460352,
            "femaleVariant": "なあ、運試しにスロットでもやったらどうだい？"
          },
          {
            "stringId": 2005394577060339712,
            "femaleVariant": "転職を考えてみてもいいんじゃない？",
            "maleVariant": "転職を考えてみてもいいんじゃないか？"
          },
          {
            "stringId": 2005417269503270912,
            "femaleVariant": "気をつけて",
            "maleVariant": "気をつけてな"
          },
          {
            "stringId": 2005417269503270928,
            "femaleVariant": "目立たないようにね",
            "maleVariant": "目立たないようにな"
          },
          {
            "stringId": 2005418698536198144,
            "femaleVariant": "じゃあね、ゴロウ",
            "maleVariant": "じゃあな、ゴロウ"
          },
          {
            "stringId": 2005421380273512448,
            "femaleVariant": "とにかく目立たないようにね。サインを配ったりしないでよ",
            "maleVariant": "とにかく目立たないようにな。サインを配ったりするなよ"
          },
          {
            "stringId": 2005422732819111936,
            "femaleVariant": "ああ。では達者でな、V"
          },
          {
            "stringId": 2005422956694282240,
            "femaleVariant": "それを言ったら、お前のほうが有名人だろう。気をつけろよ、V"
          },
          {
            "stringId": 2005428212073308160,
            "femaleVariant": "ありがとう、ワカコ"
          },
          {
            "stringId": 2005428212090085396,
            "femaleVariant": "何をするか知りたくないの？",
            "maleVariant": "何をするか知りたくないのか？"
          },
          {
            "stringId": 2005429814146441216,
            "femaleVariant": "知らぬが仏っちゅうもんや。それに… ほっといても、いずれニュースでわかるんとちゃうか？"
          },
          {
            "stringId": 2005450788568899584,
            "femaleVariant": "ありがとう、ワカコ"
          },
          {
            "stringId": 2005476014740545536,
            "femaleVariant": "街の監視カメラを使うて、隅々まで見張りよるやろ。メインアクセスポイントは… ここ。自動化された制御センターや。うまいこと利用できるかもしれへんな"
          },
          {
            "stringId": 2005487192309047296,
            "femaleVariant": "パレードのルート沿いにはバルコニーや歩道橋、他にも見晴らしのいいポイントがある"
          },
          {
            "stringId": 2005518919668670464,
            "femaleVariant": "あんな奴、誰が偲ぶかいな…"
          },
          {
            "stringId": 2005520525332127744,
            "femaleVariant": "いま何と？"
          },
          {
            "stringId": 2005521856520331264,
            "femaleVariant": "ま、絡んではいる"
          },
          {
            "stringId": 2005523994541629440,
            "femaleVariant": "本当に… タダで？"
          },
          {
            "stringId": 2005526457285943296,
            "femaleVariant": "しつこいな。気が変わらへんうちに受け取りいや"
          },
          {
            "stringId": 2005528539640438784,
            "femaleVariant": "それをいくらで？ 今はその… 持ち合わせが"
          },
          {
            "stringId": 2005528767709913088,
            "femaleVariant": "せやろな"
          },
          {
            "stringId": 2005529895910580224,
            "femaleVariant": "ええわ。タダでやる"
          },
          {
            "stringId": 2005532010259226624,
            "femaleVariant": "オカダさん… いくらなんでも、さすがにタダというわけには"
          },
          {
            "stringId": 2005533767051845632,
            "femaleVariant": "要るん？ 要らへんの？"
          },
          {
            "stringId": 2005557858748145664,
            "femaleVariant": "せやったらええねん。勘違いさせんといてえや"
          },
          {
            "stringId": 2014223467040022528,
            "femaleVariant": "この音… 日本を思い出すな"
          },
          {
            "stringId": 2014229129434308608,
            "femaleVariant": "確かに… そうかもしれないな。助言に感謝する"
          },
          {
            "stringId": 2391138054867861508,
            "femaleVariant": "[降りる]"
          },
