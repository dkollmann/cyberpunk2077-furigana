                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[調べる]",
                    "maleVariant": null,
                    "stringId": 1811026156216668164
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "調子はどう、ミッチ？",
                    "maleVariant": "調子はどうだ、ミッチ？",
                    "stringId": 1811026489244405760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、元気にやってるさ。でもな…",
                    "maleVariant": null,
                    "stringId": 1811034571802431488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なあ、それはみんな、クランのもんなんだ。その、控えてくれねえか",
                    "maleVariant": null,
                    "stringId": 1811036563509628928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "仲間を大勢失っちまった。いい奴らだったってのによ",
                    "maleVariant": null,
                    "stringId": 1811049077349830656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "手伝うよ",
                    "maleVariant": null,
                    "stringId": 1811052590331088900
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今は無理",
                    "maleVariant": "今は無理だ",
                    "stringId": 1811053327740063744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった、手伝うよ",
                    "maleVariant": null,
                    "stringId": 1811053525124009984
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今は忙しいの",
                    "maleVariant": "今は忙しい",
                    "stringId": 1811053690346033152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ならいいんだ。用事が済むまで待ってるぜ",
                    "maleVariant": null,
                    "stringId": 1811054273773719552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうだ、話があったんだ。手を借りたくてよ",
                    "maleVariant": null,
                    "stringId": 1811066095687979008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なに？",
                    "maleVariant": "なんだ？",
                    "stringId": 1811066744899129344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最後の手向けってやつをやり残しててな。一人でやりたかねえからさ",
                    "maleVariant": null,
                    "stringId": 1811067471788789760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なら行こう",
                    "maleVariant": null,
                    "stringId": 1811069143101493248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あそこの車はスコーピオンのだったが俺が頂いた。こいつは、俺の元愛車だ",
                    "maleVariant": null,
                    "stringId": 1811080391100747776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "へえ、どっちもいい車ね",
                    "maleVariant": "へえ、どっちもいい車だな",
                    "stringId": 1811081032594378752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、そうだろ。ここじゃ、車が俺たちの全てだからな",
                    "maleVariant": null,
                    "stringId": 1811081236521439232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。出発しましょ",
                    "maleVariant": "わかった。出発しよう",
                    "stringId": 1811081898298724356
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "目的地は？",
                    "maleVariant": null,
                    "stringId": 1811082275031109632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "で、目的地はどこなの？",
                    "maleVariant": "で、目的地はどこなんだよ？",
                    "stringId": 1811082529054937088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "スコーピオンの野郎が好きだった場所さ",
                    "maleVariant": null,
                    "stringId": 1811083369660567552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "借りを返しに行く",
                    "maleVariant": null,
                    "stringId": 1811084284337606656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それじゃ行きましょ",
                    "maleVariant": "それじゃ行こうか",
                    "stringId": 1811085494025211904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前、運転は得意だよな？",
                    "maleVariant": null,
                    "stringId": 1811112445314093056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ。で？",
                    "maleVariant": "ああ。で？",
                    "stringId": 1811113195557638144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "2人で1台に乗ればいいでしょ？",
                    "maleVariant": "2人で1台に乗ればいいだろ？",
                    "stringId": 1811713540197470208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうして1台で行かないの？",
                    "maleVariant": "どうして1台で行かないんだ？",
                    "stringId": 1811713938404691968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "2台必要だからさ",
                    "maleVariant": null,
                    "stringId": 1811714415984922624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おい、V！ こっちだ！",
                    "maleVariant": null,
                    "stringId": 1844282496732909568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ごめん。そういうつもりじゃ…",
                    "maleVariant": null,
                    "stringId": 1862091728060993536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいんだ、気にするな。声を荒げる場じゃねえからな",
                    "maleVariant": null,
                    "stringId": 1862093709232099328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、お前は親友を失くしたことはあるか？",
                    "maleVariant": null,
                    "stringId": 1862102324953272320
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "死者の持ち物はクランに戻る。お前もノーマッドだったんなら、わかってるだろ",
                    "maleVariant": null,
                    "stringId": 1862109188277456896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おい… よせ、V",
                    "maleVariant": null,
                    "stringId": 1862110024051249152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ごめん。そういうつもりじゃ…",
                    "maleVariant": "すまない。そういうつもりじゃ…",
                    "stringId": 1862113154411319296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいんだ、気にするな。声を荒げる場じゃねえからな",
                    "maleVariant": null,
                    "stringId": 1862113695627530240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ。ジャッキーって友達を",
                    "maleVariant": "ああ。ジャッキーって友達を",
                    "stringId": 1863156279544172548
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "幸い、そういう経験はしていない",
                    "maleVariant": null,
                    "stringId": 1863156611212955648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "友達なんていない",
                    "maleVariant": null,
                    "stringId": 1863156856562962432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ。ジャッキーって友達をね",
                    "maleVariant": "ああ。ジャッキーって友達をな",
                    "stringId": 1863157385011712000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ないよ。でも… 気持ちはわかる",
                    "maleVariant": "ない。だが… 気持ちはわかる",
                    "stringId": 1863158163558420480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "友達なんていない",
                    "maleVariant": null,
                    "stringId": 1863158412985290752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうか… 何があったんだ？",
                    "maleVariant": null,
                    "stringId": 1863159149421187072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティではよくある話よ",
                    "maleVariant": "ナイトシティではよくある話だ",
                    "stringId": 1863159371299868672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "最悪だよ",
                    "maleVariant": null,
                    "stringId": 1863159870086500352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ん？ そいつはどうしてだよ。お前は別に性格が悪いわけでもねえのに",
                    "maleVariant": null,
                    "stringId": 1863160808503300096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "友達を失うのが… 耐えられないだけ",
                    "maleVariant": "友達を失うのが… 耐えられないだけさ",
                    "stringId": 1863161245516222464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今からドライブに行くぞ",
                    "maleVariant": null,
                    "stringId": 1863179823699251200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ドライブって、どこへ？",
                    "maleVariant": null,
                    "stringId": 1863180596139053056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前、都会モンだろ。そうやって何でも知りたがるのが、その証拠だ",
                    "maleVariant": null,
                    "stringId": 1863181000486735872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前、さては街に来て長いんだろ？ そうやって何でも知りたがるのが、その証拠だ",
                    "maleVariant": null,
                    "stringId": 1863181300379471872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まあ、そう焦るなって",
                    "maleVariant": null,
                    "stringId": 1863184247431680000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アルデカルドスじゃ全てはクランの持ち物なんだ",
                    "maleVariant": null,
                    "stringId": 1863188012842930176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、お前のクランがどうだったかは知らんが… アルデカルドスじゃ全ての所有権はクランに帰属する",
                    "maleVariant": null,
                    "stringId": 1863188727384559616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺はスコーピオンので行く。お前はもう一台を使え",
                    "maleVariant": null,
                    "stringId": 1863191775100063744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと、なによそれ。禅問答でもやってるつもり？",
                    "maleVariant": "おい、なんだよそれ。禅問答でもやってるつもりか？",
                    "stringId": 1863200466100604928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まぁまぁ。今にわかるって",
                    "maleVariant": null,
                    "stringId": 1863201013440499712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "先導する。ついて来い",
                    "maleVariant": null,
                    "stringId": 1863201822404300800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "スコーピオンのバカ野郎…！",
