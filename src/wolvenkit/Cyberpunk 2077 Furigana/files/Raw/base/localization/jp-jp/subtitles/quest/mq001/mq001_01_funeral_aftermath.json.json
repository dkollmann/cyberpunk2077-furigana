              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やあ、ミッチ。調子はどう？",
                    "maleVariant": "やあ、ミッチ。調子はどうだ？",
