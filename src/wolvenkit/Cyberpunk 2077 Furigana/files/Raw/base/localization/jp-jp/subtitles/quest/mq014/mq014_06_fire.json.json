    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1815550918190403588,
            "femaleVariant": "今からそなたに教えるのは火の瞑想だ"
          },
          {
            "stringId": 1816922686184460288,
            "femaleVariant": "ここは森の中、太陽の光が樹々の合間から差し込み、下で燃え盛る炎の上で踊っている"
          },
          {
            "stringId": 1822765850526978048,
            "femaleVariant": "我々は一心同体。そなたなしに命は存在しえない。我はそなたから作られ、そなたに戻るのだから"
          },
          {
            "stringId": 1822767135779164160,
            "femaleVariant": "根源たる火、太陽の精霊よ。我の身体と魂から穢れを全て焼き払い、害ある人々、社会、次元から害ある気を焼き払いたまえ"
          },
          {
            "stringId": 1822770179350695936,
            "femaleVariant": "全細胞を浄化したまえ。命の気、健康、若さ、知恵、知識、強さ、喜び、そして幸せを与えたまえ"
          },
          {
            "stringId": 1822774555922370560,
            "femaleVariant": "この目を通し、火と太陽の気は我々の頭に触れ、次に全ての内臓の中を流れ、やがては足先まで達しよう"
          },
          {
            "stringId": 1822775532222124032,
            "femaleVariant": "そして我々に、光が宿るのだ"
          },
          {
            "stringId": 1822781310043656192,
            "femaleVariant": "太陽の精霊、火の元素よ、精神、身体、魂をもって感謝申し上げる"
          },
          {
            "stringId": 1925616428986109952,
            "femaleVariant": "火の精霊、太陽の精霊よ、ご挨拶申し上げる… そなたは他の全てのものに美を与えてくださった"
          },
          {
            "stringId": 1925617208791109632,
            "femaleVariant": "我に内なる安らぎを与え、我を神聖なる平静から守りたまえ"
          },
          {
            "stringId": 1925617534923411456,
            "femaleVariant": "我々は目を細め、太陽の神聖な気を吸収する"
          },
          {
            "stringId": 1925619216168226816,
            "femaleVariant": "我々の肉体の全細胞が、その隅々に至るまで、再び太陽の気で満たされていく"
          },
          {
            "stringId": 1925620860083736576,
            "femaleVariant": "我らは太陽"
          }
        ]
      }
    }
  }
