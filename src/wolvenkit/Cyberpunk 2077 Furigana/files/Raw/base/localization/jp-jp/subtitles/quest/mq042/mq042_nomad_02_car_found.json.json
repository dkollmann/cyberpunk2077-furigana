    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ずいぶん久しぶりね、相棒",
              "maleVariant": "ずいぶん久しぶりだな、相棒",
              "stringId": 1828421667858030592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あれから、元気にしてたかしら…？",
              "maleVariant": "あれから、元気にしてたか…？",
              "stringId": 1828425331951673344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと！ 私の車に触らないで！",
              "maleVariant": "",
              "stringId": 1828425863470653440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "壊れたから",
              "maleVariant": "壊れたからだ",
              "stringId": 1828442120475516932
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "関係ないでしょ！",
              "maleVariant": "関係ないだろ！",
              "stringId": 1828442340693254144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてって、壊れたからで――",
              "maleVariant": "",
              "stringId": 1828443462669574144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "関係ないでしょ",
              "maleVariant": "関係ないだろ",
              "stringId": 1828444680443797504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "壊れたから乗り捨てて、今頃になってそれを思い出したってわけ？ 調子がよすぎる",
              "maleVariant": "",
              "stringId": 1828446233108029440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "関係ない！？",
              "maleVariant": "",
              "stringId": 1828450037643747328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここ1週間、どうにかしてこのポンコツを動かそうと頑張ったのは、あんたじゃなく私なんだから！",
              "maleVariant": "",
              "stringId": 1828452000628350976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なかなかいい仕事よ",
              "maleVariant": "なかなかいい仕事だ",
              "stringId": 1828454367742533636
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へたくそな整備士ね",
              "maleVariant": "へたくそな整備士だな",
              "stringId": 1828454799000870912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全体的には… 悪くない出来ね。もうこの車はダメだと思ってたけど… なかなかやるね",
              "maleVariant": "全体的には… 悪くない出来だ。もうこの車はダメだと思ってたが… なかなかやるな",
              "stringId": 1828456749922656256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1週間もかかったの？ お粗末な整備士ね。あたしが本気を出せば、1日で直せる",
              "maleVariant": "1週間もかかったのか？ お粗末な整備士だな。俺が本気を出せば、1日で直せる",
              "stringId": 1828459668285825024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもやらなかった。なら権利はないわ",
              "maleVariant": "",
              "stringId": 1828466710857785344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ははは！ ご機嫌ね！",
              "maleVariant": "ははは！ ご機嫌だな！",
              "stringId": 1828497559393280000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私のほうが愛情を注いでるってことね",
              "maleVariant": "",
              "stringId": 1828512707877150720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "車を買い取るよ",
              "maleVariant": "車を買い取りたい",
              "stringId": 1828516532243537924
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかくあたしの車よ",
              "maleVariant": "とにかく俺の車だ",
              "stringId": 1828516976655212544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "車はあげる",
              "maleVariant": "車はやる",
              "stringId": 1828517313608818688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この車、買い取るよ。別のを買えるだけ払えばいいでしょ",
              "maleVariant": "この車、買い取るよ。別のを買えるだけ払えばいいだろ",
              "stringId": 1828518863957794816
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも… これ、ポンコツよ…",
              "maleVariant": "",
              "stringId": 1828520330236141568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…話はもういい。さっさと消えて",
              "maleVariant": "…話はもういい。さっさと消えろ",
              "stringId": 1828522153248432128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "元はあたしの車なのに、ひどい言われようね。ほら、気が変わる前に金を受け取って",
              "maleVariant": "元は俺の車なのに、ひどい言われようだな。ほら、気が変わる前に金を受け取ってくれ",
              "stringId": 1828523318392528896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "タイミングチェーンのさびがひどい",
              "maleVariant": "",
              "stringId": 1828530463540387840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で？",
              "maleVariant": "",
              "stringId": 1828531189205311488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "交換したほうがいいってこと。でないと、30以上出した時にチェーンが切れちゃうよ",
              "maleVariant": "交換したほうがいいってことだ。でないと、30以上出した時にチェーンが切れちまうぞ",
              "stringId": 1828531519229927424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと待ってよ… つまり、返さなくていいの？",
              "maleVariant": "",
              "stringId": 1828532628271013888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "男も女も寄ってこないのは確かだが… 死んではいねえようだな",
              "maleVariant": "",
              "stringId": 1872242108275761152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "序の口よ。こいつはまだまだ走れる",
              "maleVariant": "序の口だ。こいつはまだまだ走れる",
              "stringId": 1872243923067215872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "事故っても知らねえぞ… って、俺はもう死んでるか",
              "maleVariant": "",
              "stringId": 1872244276345053184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おやおや… V、ノーマッドの掟はどこにいったんだよ？",
              "maleVariant": "",
              "stringId": 1872245231203524608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあね。頭に時限爆弾があると、あまり考え事ができないの",
              "maleVariant": "さあな。頭に時限爆弾があると、あまり考え事ができないんだ",
              "stringId": 1872246049680007168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コイルとプラグが新調されてる… カムシャフトも改造されてるみたいね… こっちは…",
              "maleVariant": "コイルとプラグが新調されてる… カムシャフトも改造されてるみたいだな… こっちは…",
              "stringId": 1875030682833833984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの？ これはあたしの車よ！",
              "maleVariant": "あんたの？ これは俺の車だ！",
              "stringId": 1875036734023323648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だったら、どうして乗り捨てたのよ！？",
              "maleVariant": "",
              "stringId": 1875037002089680896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってないのね。車はノーマッドにとって神聖なものなの。それをいじるってのは、家族の墓を掘り起こされるのと同じなのよ",
              "maleVariant": "わかってないんだな。車はノーマッドにとって神聖なものなんだ。それをいじるってのは、家族の墓を掘り起こされるのと同じなんだよ",
              "stringId": 1875058238823911424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなの私は知ったこっちゃない。車は返さないからね。何を言っても無駄！",
              "maleVariant": "",
              "stringId": 1875059673124884480
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティから出るために、どうしてもこの車が必要なの",
              "maleVariant": "",
              "stringId": 1875070171300024320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてよ？",
              "maleVariant": "どうしてだ？",
              "stringId": 1875071642611535872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私はこの車でナイトシティから出ていくの。そのために修理した",
              "maleVariant": "",
              "stringId": 1875071774161686528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何をそんなに急いでるの？",
              "maleVariant": "何をそんなに急いでんだ？",
              "stringId": 1875074241184190464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ずいぶん必死だけど、どうして急いでるの？",
              "maleVariant": "ずいぶん必死だが、どうして急いでるんだ？",
              "stringId": 1875074641555673088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言わなきゃわかんないようじゃ、絶対理解できないよ",
              "maleVariant": "",
              "stringId": 1875075399785172992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいから、話してみて。知りたいの",
              "maleVariant": "いいから、話してみろよ。知りたいんだ",
              "stringId": 1875075937578831872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはこの街が嫌にならないの？ どこも汚れてて、犯罪者やクズばっかり…",
              "maleVariant": "",
              "stringId": 1875076763688947712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その… 何て言ったらいいか… ありがと",
              "maleVariant": "",
              "stringId": 1875087525887135744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいの、気にしないで。じゃ、この車はもらってくよ",
              "maleVariant": "いいんだ、気にするな。じゃ、この車はもらってくぞ",
              "stringId": 1875088709788160000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何度も言わせないで。これは――",
              "maleVariant": "",
              "stringId": 1875090592040800256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなにあたしの銃を見たい？ 生きて街を出たいんでしょ？ 車から離れて。早く！",
              "maleVariant": "そんなに俺の銃が見たいのか？ 生きて街を出たいんだろ？ 車から離れろ。早く！",
              "stringId": 1875090882320191488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このクズ！ 人でなし！ 地獄に落ちろ！",
              "maleVariant": "",
              "stringId": 1875093389742231552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "直してくれて、ありがとね",
              "maleVariant": "直してくれて、ありがとな",
              "stringId": 1875093846099283968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ま、一言でいえばそういうこと。どうせここじゃ使い道ないし。都会で乗るには向いてないの",
              "maleVariant": "ま、一言でいえばそういうことだ。どうせここじゃ使い道ないしな。都会で乗るには向いてない",
              "stringId": 1875099313810755584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この子のことは、あんたに任せる",
              "maleVariant": "こいつのことは、あんたに任せる",
              "stringId": 1875101482752462848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんなところに住みたくないの… 絶対に",
              "maleVariant": "",
              "stringId": 1875105427797364736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は人の良心を問うような真似は好きじゃないんだが… あんまりじゃねえか、V",
              "maleVariant": "",
              "stringId": 1875110844623540224
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ノーマッドじゃないなら、理解できないでしょうね。ギターを盗まれたらどう思う？",
              "maleVariant": "ノーマッドじゃないなら、理解できないだろうな。ギターを盗まれたらどう思う？",
              "stringId": 1875112373380239360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの女にとっちゃ、この車が最後のチャンスだったのかもな",
              "maleVariant": "",
              "stringId": 1875112888256221184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "せいぜい楽しめ、V",
              "maleVariant": "",
              "stringId": 1875115676075872256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "思いっきり踏み込め",
              "maleVariant": "",
              "stringId": 1875118168113205248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とはいえ、時限爆弾の立場になるのもオススメはしないがな",
              "maleVariant": "",
              "stringId": 1875125058448707584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でしょうね。さあ、そろそろ行きましょ、ジョニー。この場所にいると気分が悪くなる",
              "maleVariant": "だろうな。さあ、そろそろ行こう、ジョニー。この場所にいると気分が悪くなる",
              "stringId": 1875125684708626432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パルスモーターから手を付けるべきね",
              "maleVariant": "パルスモーターから手を付けるべきだ",
              "stringId": 1881961806684278784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パルスモーターから始めれば早かったのに",
              "maleVariant": "",
              "stringId": 1881964272582709248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ？ なんでそう言い切れるわけ？",
              "maleVariant": "",
              "stringId": 1881967289042259968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "モーターが不安定だと噛み込みが起こるからね。でもカーボンデポジットを掃除すれば息を吹き返す",
              "maleVariant": "モーターが不安定だと噛み込みが起こるからな。だがカーボンデポジットを掃除すれば息を吹き返す",
              "stringId": 1881970561958334464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あーもう… スロットルを丸ごと変えちゃったよ…",
              "maleVariant": "",
              "stringId": 1881979761811836928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほらね？ 車のことはオーナーが一番よく知ってるの",
              "maleVariant": "ほらな？ 車のことはオーナーが一番よく知ってる",
              "stringId": 1881981099962585088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“元”オーナーね。今は私の車よ。とにかくこの車の修理が済んだら、私はさっさとこんな街から出ていくの",
              "maleVariant": "",
              "stringId": 1881981663022731264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ボンネットを開ける]",
              "maleVariant": "",
              "stringId": 1927350610698313732
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[エンジンを調べる]",
              "maleVariant": "",
              "stringId": 1927362745708978176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "温かい。さっきまでエンジンがついてたみたいね",
              "maleVariant": "温かい。さっきまでエンジンがついてたみたいだ",
              "stringId": 1927363726471471104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フェンダーとトランクはサビだらけ… ボディは何とか形を保ってる… エンジンは…",
              "maleVariant": "フェンダーとトランクはサビだらけ… ボディは何とか形を保ってるな… エンジンは…",
              "stringId": 1927364794123165696
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ラジエーターを調べる]",
              "maleVariant": "",
              "stringId": 1927385042561933316
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ボンネットを閉める] これはあたしの車よ",
              "maleVariant": "[ボンネットを閉める] これは俺の車だ",
              "stringId": 1927385760794550272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[オイルパンを調べる]",
              "maleVariant": "",
              "stringId": 1927394303417393152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クーリングファンはキレイだし、ブレーキ液も足してある… って… 入れてあるのは水じゃない",
              "maleVariant": "クーリングファンはキレイだし、ブレーキ液も足してある… って… 入れてあるのは水かよ",
              "stringId": 1927399774719188992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "交換されてるけど… 別の車から取った中古品ね",
              "maleVariant": "交換されてるけど… 別の車から取った中古品か",
              "stringId": 1927400107814035456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし。もう十分ね",
              "maleVariant": "よし。もう十分だ",
              "stringId": 1933092941841354752
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}