  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[乗る]",
              "maleVariant": "",
              "stringId": 1145790880853725184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミスV、会えて光栄だ",
              "maleVariant": "ミスターV、会えて光栄だ",
              "stringId": 1172322775523405824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本題に入る前にひとつ質問がある",
              "maleVariant": "",
              "stringId": 1172323745498148864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがちな話さ。2週間前に、イカれた連中がミリテク社のコンボイを襲撃して兵器を強奪した。ミリテクはメイルストロームが一枚噛んでたことすら知らないがな",
              "maleVariant": "",
              "stringId": 1172327136458715136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "女の詳細は？",
              "maleVariant": "",
              "stringId": 1172332148182233088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その女の詳細は？",
              "maleVariant": "",
              "stringId": 1172332275437416448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "盗まれた兵器はプロトタイプの戦闘ボット、通称“フラットヘッド”だ。まずはそいつを入手する必要がある",
              "maleVariant": "",
              "stringId": 1172350021990625280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "内政担当の企業エージェントだ。例のコンボイの件で躍起になって嗅ぎ回ってやがる。唯一掴んだ手掛かりは、トランクの中で丁重に“もてなし”てるらしい",
              "maleVariant": "",
              "stringId": 1172369506965401600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのロイスはどんな奴？",
              "maleVariant": "そのロイスはどんな奴だ？",
              "stringId": 1174963773897818112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[チップを受け取る]",
              "maleVariant": "",
              "stringId": 1175126700094685204
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、出せ",
              "maleVariant": "",
              "stringId": 1176642317162434560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待って。こっちも聞きたいことがある。なぜあたしを呼んだの？",
              "maleVariant": "待て。こっちにも聞きたいことがある。なぜ俺を呼んだ？",
              "stringId": 1186596413243969536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "詳しく聞かせて",
              "maleVariant": "詳しく聞かせてくれ",
              "stringId": 1186596661513211904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜあたしなの？",
              "maleVariant": "なぜ俺なんだ？",
              "stringId": 1186596672972050432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このチップを挿せ",
              "maleVariant": "",
              "stringId": 1187749962556428288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大体わかった。仕事にかかりましょ",
              "maleVariant": "大体わかった。仕事にかかろう",
              "stringId": 1187768799460904960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "凡人としてなんの面白みもないしみったれた平穏な生涯を過ごすか、名を上げて華々しく早死にするか。お前ならどちらの生き方を選ぶ？",
              "maleVariant": "",
              "stringId": 1187780010600615936
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "古い考えかもしれんが、仕事をする相手は直に見極めたい性質でね",
              "maleVariant": "",
              "stringId": 1196607822107447296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジャッキーとは知った仲だ。T-バグには2年前に手を貸してもらった。今度はお前の番ってわけだ",
              "maleVariant": "",
              "stringId": 1196610813350842368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼もしいな。エヴリン・パーカーとの面会は手配しておく。フラットヘッドの件は任せたぞ",
              "maleVariant": "",
              "stringId": 1244940493912739840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞かせて。仕事の内容は？",
              "maleVariant": "聞かせてくれ。仕事の内容は？",
              "stringId": 1569251174210752512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ある、試作型のテック… 正確に言うと、バイオチップがあってな。そいつを手に入れたい",
              "maleVariant": "",
              "stringId": 1569254923566206976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メイルストロームの問題？",
              "maleVariant": "",
              "stringId": 1569297342290612228
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "依頼主のことを聞きたい",
              "maleVariant": "",
              "stringId": 1569305716872859648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メイルストロームの問題というと？ 揉め事でもあるの？",
              "maleVariant": "メイルストロームの問題というと？ 揉め事でもあるのか？",
              "stringId": 1569306134575206400
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、問題ない",
              "maleVariant": "",
              "stringId": 1569308812336320512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "依頼主ってのは、どんな奴？ なぜ会いたがってるの？",
              "maleVariant": "依頼主ってのは、どんな奴だ？ なぜ会いたがってる？",
              "stringId": 1569310770858184704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "名前はエヴリン・パーカー。一筋縄じゃいかない女だ。素性を探ってみたが…",
              "maleVariant": "",
              "stringId": 1569311546334662656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それともうひとつ",
              "maleVariant": "",
              "stringId": 1569316152838082560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジャッキーやT-バグでも良かったはずよね。わざわざ会う必要だって…",
              "maleVariant": "ジャッキーやT-バグでも良かったはずだろ。わざわざ会う必要だって…",
              "stringId": 1630085098826035200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それに、このヤマの前にひとつやってもらいたいことがある。まあ、その話はまた後にしよう",
              "maleVariant": "",
              "stringId": 1630097934570680320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "デクスター・デショーン。噂どおりの体格ね",
              "maleVariant": "デクスター・デショーン。噂通りの体格だな",
              "stringId": 1630100503145672704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうせ企業から盗めって言うんでしょ？",
              "maleVariant": "どうせ、企業から盗めって言うんだろ？",
              "stringId": 1630147450661433344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカからだ",
              "maleVariant": "",
              "stringId": 1630147646115999744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何か問題でもあるか？",
              "maleVariant": "",
              "stringId": 1630148399865982976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別に",
              "maleVariant": "",
              "stringId": 1630149136318656516
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自殺行為よ",
              "maleVariant": "自殺行為だ",
              "stringId": 1630149888978120704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "相手が企業だろうと関係ない",
              "maleVariant": "",
              "stringId": 1630150368487731200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "問題どころか、ただの自殺行為よ。この街でアラサカに手を出すようなバカは、ひとりもいない",
              "maleVariant": "問題どころか、ただの自殺行為だ。この街でアラサカに手を出すようなバカは、ひとりもいない",
              "stringId": 1630150437811187712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何か、プランがあるの？",
              "maleVariant": "何か、プランがあるのか？",
              "stringId": 1630150523928637440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほう。大した度胸じゃないか",
              "maleVariant": "",
              "stringId": 1630164702521651200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リスクを取って利益を狙う。それがアフターライフの仕事の鉄則だ",
              "maleVariant": "",
              "stringId": 1630165934573285376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それに俺もバカじゃない。痕跡を残さず仕事を済ませれば、奴らに追われることもない。違うか？",
              "maleVariant": "",
              "stringId": 1630166955164889088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前とは、金になる良好な関係が築いていけそうだ",
              "maleVariant": "",
              "stringId": 1630168531703099392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やるべきは2つ。まずはメイルストロームの連中の問題を片付けてほしい",
              "maleVariant": "",
              "stringId": 1630177638040399872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "次に、このヤマを持ち掛けてきた依頼主と会え。簡単だろ。直接会って話したいそうだ",
              "maleVariant": "",
              "stringId": 1630181679705288704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボットなしではアラサカのチップを手に入れることができない。つまり俺もお前も報酬なしってことになる。とはいえ、ボット自体は使い捨てのオモチャだ",
              "maleVariant": "",
              "stringId": 1630235385486307328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "締め上げても暖簾に腕押しで、相当焦ってるようだ。付け入る隙があるとすればそこだな",
              "maleVariant": "",
              "stringId": 1630240516613271552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのロイスって、どんな奴なの？",
              "maleVariant": "そのロイスって、どんな奴なんだ？",
              "stringId": 1630241912058212352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クローム狂いの、サイコパス野郎だ",
              "maleVariant": "",
              "stringId": 1630313775652335616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この街じゃ珍しくもない",
              "maleVariant": "",
              "stringId": 1630314951651291136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かにな。だがロイスの奴は、かつての仲間を業務用の電子乾燥機にかけるほど気合が入ってやがる",
              "maleVariant": "",
              "stringId": 1630319093778149376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業エージェントが一体何を？",
              "maleVariant": "",
              "stringId": 1630328689775276032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業エージェントが一体何をしてるの？",
              "maleVariant": "企業エージェントが一体何をしてるんだ？",
              "stringId": 1630329263186964480
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあな。例のコンボイの件で躍起になって嗅ぎ回ってる。唯一掴んだ手掛かりを、トランクの中で丁重に“もてなし”てるらしい",
              "maleVariant": "",
              "stringId": 1630336251451076608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふむ… 差し詰めこの女はコンボイに待ちぼうけを食らって、ひとまず怪しいとふんだスーツ野郎を捕まえたのね",
              "maleVariant": "ふむ… 差し詰めこの女はコンボイに待ちぼうけを食らって、ひとまず怪しいとふんだスーツ野郎を捕まえたってところか",
              "stringId": 1630336730004385792
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何も情報が得られず、念のため身柄を確保してるだけ。盗まれた品を早く見つけなきゃ、上の連中に合わす顔がないってところかしら",
              "maleVariant": "何も情報が得られず、念のため身柄を確保してるだけだ。盗まれた品を早く見つけなきゃ、上の連中に合わす顔がないんだろうな",
              "stringId": 1630337177100414976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつは驚いたな、ミスV。大した観察眼だ。この情報を上手く利用するといい",
              "maleVariant": "こいつは驚いたな、ミスターV。大した観察眼だ。この情報を上手く利用するといい",
              "stringId": 1630338374758756352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は後方待機。T-バグは愛想ってものを知らない。ジャッキーは… まあ、わかるだろ。残るはお前しかいないってわけだ",
              "maleVariant": "",
              "stringId": 1630341567462961152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ともかく、依頼主は危険に身をさらす本人との面会を希望してる。つまり、実行役とな",
              "maleVariant": "",
              "stringId": 1630342993559875584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みたが？",
              "maleVariant": "",
              "stringId": 1630355721997447168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パシフィカの連中から連絡があってな。「余計な詮索をするな」と言われておしまいだ",
              "maleVariant": "",
              "stringId": 1630356056149258240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何を成し遂げるか。大事なのはそれだけ",
              "maleVariant": "何を成し遂げるか。大事なのはそれだけだ",
              "stringId": 1630449162265751552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "凡人はナイトシティじゃ生きていけない",
              "maleVariant": "",
              "stringId": 1630449287558000640
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これは何かのテスト？",
              "maleVariant": "これは何かのテストか？",
              "stringId": 1630449408337178624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "のし上がらなきゃ無意味。何を犠牲にしようとね",
              "maleVariant": "のし上がらなきゃ無意味だ。何を犠牲にしようとな",
              "stringId": 1630451353118502912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何かを成し遂げなきゃ、虚しく消えていくだけ。この街に凡人の居場所はない",
              "maleVariant": "何かを成し遂げなきゃ、虚しく消えていくだけだ。この街に凡人の居場所はない",
              "stringId": 1630451995014787072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これは何かのテスト？",
              "maleVariant": "これは何かのテストか？",
              "stringId": 1630452298615287808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こういう… 謎かけが？",
              "maleVariant": "",
              "stringId": 1630454359411060736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この手の話が好きなだけだ",
              "maleVariant": "",
              "stringId": 1630454957820801024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミスV。存在論と言ってくれ",
              "maleVariant": "ミスターV。存在論と言ってくれ",
              "stringId": 1630459997847482368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あるさ。この俺様も優しさしか取り柄のない、心底平凡な男だ。だがそんな男でも、裏切り者には容赦しないがな…",
              "maleVariant": "",
              "stringId": 1630462637205549056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本題に入ろう。今、デカい仕事に目をつけている。スカベンジャーのヤマがママゴトに思えるほどのな",
              "maleVariant": "",
              "stringId": 1630464120311443456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん… 言うね。新参者はそれぐらいの意気込みでないとな。気に入った",
              "maleVariant": "",
              "stringId": 1630465234687684608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "平穏な生涯と栄誉の死、どちらを選ぶ？",
              "maleVariant": "",
              "stringId": 1653396512059617280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また会おう",
              "maleVariant": "",
              "stringId": 1653417621756768256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最初に爆発するのは目玉らしい。「ポンッ！」ってな。残りはドロドロになるそうだ",
              "maleVariant": "",
              "stringId": 1667862752152973312
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろんそのためには、女の情報が必要だ。今送る",
              "maleVariant": "",
              "stringId": 1667865798945730560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろんそのためには、女の情報が必要だ。今送る",
              "maleVariant": "",
              "stringId": 1667867078074232832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく、俺はすでにボットを購入した。だが問題は、話した相手がブリックだったってことだ",
              "maleVariant": "",
              "stringId": 1672282882077290496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのブリックはメイルストロームのリーダーだったんだが… 同じギャング仲間のサイモン・ランドール、通称ロイスに引きずり降ろされた。俺と取引をした3日後にな",
              "maleVariant": "",
              "stringId": 1672283756606451712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今じゃロイスがリーダーだ。すんなりとボットを引き渡してくれるとは限らない",
              "maleVariant": "",
              "stringId": 1672284141995880448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おまけにもうひとつ。ミリテクのメレディス・スタウトって女が、“関心”を示している",
              "maleVariant": "",
              "stringId": 1995112637703057408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[降りる]",
              "maleVariant": "",
              "stringId": 2017298064277725188
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}