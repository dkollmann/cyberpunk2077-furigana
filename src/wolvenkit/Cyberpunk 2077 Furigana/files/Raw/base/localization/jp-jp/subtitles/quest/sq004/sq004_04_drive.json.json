        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、そうだ… ラジオもあるから",
                    "maleVariant": null,
                    "stringId": 1375164841633607680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よし、降りて",
                    "maleVariant": null,
                    "stringId": 1376237517701738496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よし、作戦を確認するよ",
                    "maleVariant": null,
                    "stringId": 1376253101084602368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと… あれ見て！",
                    "maleVariant": null,
                    "stringId": 1376328672795783168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "頑張ってね",
                    "maleVariant": null,
                    "stringId": 1700875168543301632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "車を隠さないと。岩場のそばに停めて",
                    "maleVariant": null,
                    "stringId": 1764683611163996160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、時間がない。嵐が迫ってきてる",
                    "maleVariant": null,
                    "stringId": 1764770380677001216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いい場所があるんだ。そこからなら、完璧に援護できる",
                    "maleVariant": null,
                    "stringId": 1776252189428297728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、バッチリよ",
                    "maleVariant": "ああ、バッチリだ",
                    "stringId": 1776255147251478532
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、こっち",
                    "maleVariant": null,
                    "stringId": 1776260259839619072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一緒に乗ってくれば良かったのに！",
                    "maleVariant": null,
                    "stringId": 1783732414881869824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと、何考えてるの！？",
                    "maleVariant": null,
                    "stringId": 1822633049249165312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私が援護する！ あんたはとにかくソウルを探して",
                    "maleVariant": null,
                    "stringId": 1822836259452760064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃ、後でね",
                    "maleVariant": null,
                    "stringId": 1822836525354856448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私は離れた場所から援護する",
                    "maleVariant": null,
                    "stringId": 1847678184782229504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、バッチリよ。そっちは？",
                    "maleVariant": "ああ、バッチリだ。そっちは？",
                    "stringId": 1847679575848316928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バカでっかい嵐だ！",
                    "maleVariant": null,
                    "stringId": 1886342008745566208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ペトロケムのタンクローリーを奪ったときは、そのまま燃料をばらまいた後、火柱をあげて子供たちを喜ばせてた",
                    "maleVariant": null,
                    "stringId": 1886495717622358016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "昔はすごかったんだ",
                    "maleVariant": null,
                    "stringId": 1886496439394967552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "爆撃のなか、みんなを引き連れて州境を越えたり…",
                    "maleVariant": null,
                    "stringId": 1886497324510552064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "憧れの人か…",
                    "maleVariant": null,
                    "stringId": 1886498802029633536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "やるなら今しかないよ",
                    "maleVariant": null,
                    "stringId": 1886515716231602176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヤバそうね…",
                    "maleVariant": "ヤバそうだな…",
                    "stringId": 1886519217166819328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あれに巻き込まれたらおしまいだ。急がないと",
                    "maleVariant": null,
                    "stringId": 1886519378613968896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "よりによってソウルが捕まるとは…",
                    "maleVariant": null,
                    "stringId": 1893697359283298304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "他の人だったら、まだわかるけど",
                    "maleVariant": null,
                    "stringId": 1893697941083594752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今は鈍ってるって、言ってなかったっけ",
                    "maleVariant": "今は鈍ってるって、言ってなかったか",
                    "stringId": 1893700340762353664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうだけどさ…",
                    "maleVariant": null,
                    "stringId": 1893700992943071232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "聴きたければ、どうぞ",
                    "maleVariant": null,
                    "stringId": 1893707732317184000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "くれぐれも慎重にね",
                    "maleVariant": null,
                    "stringId": 1893714389029953536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "万が一騒ぎが起きても、私がバックアップしてあげるから",
                    "maleVariant": null,
                    "stringId": 1893755141609037824
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リンクは常時オン、あんたがソウルを見つけたら私が車を出して、脱出する",
                    "maleVariant": null,
                    "stringId": 1893756179799941120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたはバレないようにレイスのキャンプに侵入する…",
                    "maleVariant": null,
                    "stringId": 1893758231032700928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "準備はいい？",
                    "maleVariant": null,
                    "stringId": 1895149296348372992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "嵐がこっちに迫ってる",
                    "maleVariant": null,
                    "stringId": 1895152071132127232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なかなかの嵐が近づいてるようね",
                    "maleVariant": "なかなかの嵐が近づいてるようだ",
                    "stringId": 1895152305593720832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なかなかどころか、あんなの食らったらひとたまりもないよ！ 早く片付けなきゃ",
                    "maleVariant": null,
                    "stringId": 1895152816325730304
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さ、降りて",
                    "maleVariant": null,
                    "stringId": 1895166198789513216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私はあそこから援護する。あんたはソウルを探して",
                    "maleVariant": null,
                    "stringId": 1895166775120437248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クソ、もう引き返せない",
                    "maleVariant": null,
                    "stringId": 1895170199299891200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃ、後でね",
                    "maleVariant": null,
                    "stringId": 1895171037623820288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかってる。ただ…",
                    "maleVariant": null,
                    "stringId": 1895181027650641920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待って。作戦をおさらいしたい",
                    "maleVariant": "待ってくれ。作戦をおさらいしたい",
                    "stringId": 1957602384160468992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もう一度説明して",
                    "maleVariant": "もう一度説明してくれるか",
                    "stringId": 2277749476720746496
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
