  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レディース・アンド・ジェントルメン！ 〈Us Cracks〉アンド… ケリィーーー・ユーロダイン！",
              "maleVariant": "",
              "stringId": "1685057871779360768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よう",
              "maleVariant": "",
              "stringId": "1685058629019004928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんばんは！ ナイトシティのみんな、盛り上がってる？",
              "maleVariant": "",
              "stringId": "1685058884200460288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大盛り上がりじゃない、ケリー",
              "maleVariant": "大盛り上がりじゃないか、ケリー",
              "stringId": "1805308170825494528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すべてコヴァチェクの思惑通りだったってわけね",
              "maleVariant": "すべてコヴァチェクの思惑通りだったってわけか",
              "stringId": "1805308322592190464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ、笑えるな",
              "maleVariant": "",
              "stringId": "1805316570791669760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とはいえ、奴を許すわけにもいかねえ。今日明日どうこうする気はないが、あの野郎には契約の件をたっぷり後悔させてやる",
              "maleVariant": "",
              "stringId": "1805317543501099008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レーベルと契約は解消するの？",
              "maleVariant": "レーベルと契約は解消するのか？",
              "stringId": "1805345964507656192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴は俺の味方だと思ってたが、とんだ見当違いだった",
              "maleVariant": "",
              "stringId": "1805352733023793152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうにかできないの？",
              "maleVariant": "どうにかできないのか？",
              "stringId": "1805355364681117696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴を始末できても、他は打つ手なしだ。企業に逆らうのは無謀だからな。連中は俺の曲や歌詞の権利も握ってる…",
              "maleVariant": "",
              "stringId": "1805356442214592512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やろうと思えば、子供用のオモチャに俺の顔を印刷することだってできる。名声の代償ってヤツさ",
              "maleVariant": "",
              "stringId": "1805358562066165760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや… 現実的に考えて――契約を破棄したらどうなると思う？",
              "maleVariant": "",
              "stringId": "1805366413467709440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新曲はリリースできない。販売、宣伝、インフルエンサーやメディア対応は誰がやるんだ？",
              "maleVariant": "",
              "stringId": "1805366911549698048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自由にはなれる",
              "maleVariant": "",
              "stringId": "1805368302599008256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本気か？ 声が枯れるまで自由に歌ったところで、誰の耳にも届かない。それじゃ意味がないだろ",
              "maleVariant": "",
              "stringId": "1805368378281029632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V！",
              "maleVariant": "",
              "stringId": "1805524730646323204"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "来いよ。会わせたい相手がいるんだ",
              "maleVariant": "",
              "stringId": "1805524730679877648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「ひと目ぼれ」、ねえ",
              "maleVariant": "",
              "stringId": "1805524730948313092"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "愛には色んな形がある。始まり方も色々よ",
              "maleVariant": "",
              "stringId": "1805524730948313100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "取引の話は後にしろ。来いよ、見せたいものがある",
              "maleVariant": "",
              "stringId": "1805524730998644756"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケリー、最高のパフォーマンスでした！ 〈Us Cracks〉とのまさかのコラボ！ どうやって実現したんです？",
              "maleVariant": "",
              "stringId": "1805524731048976388"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "簡単なことさ。ロックは死んでない。生まれ変わったんだ。その瞬間に、俺は立ち会った",
              "maleVariant": "",
              "stringId": "1805524731048976396"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しかし――とても元気そうですね、ケリー！ 少し前まで、うつ病の患いや引退の話もありましたが、何か心境の変化があったんですか？",
              "maleVariant": "",
              "stringId": "1805524731048976404"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて、〈Us Cracks〉の皆さんは、伝説的アーティストとのコラボ、いかがでした？",
              "maleVariant": "",
              "stringId": "1805524731048976412"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケリー！ こっちだ！ こっち見て！",
              "maleVariant": "",
              "stringId": "1805524731065753604"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "息をのむような人だわ、ケリー！",
              "maleVariant": "",
              "stringId": "1805524731065753612"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも、どうも。あんたこそ息をのむようだぜ",
              "maleVariant": "",
              "stringId": "1805524731065753620"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ悪いが次の予定があるんで、ここらで失礼するよ",
              "maleVariant": "",
              "stringId": "1805618578298986496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行くぞ。それともまだインタビューを続けるか？",
              "maleVariant": "",
              "stringId": "1805618581100781568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…こうやって街を見下ろすのが好きなんだ。いくら見ても飽きない",
              "maleVariant": "",
              "stringId": "1807175567395217412"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうだ？",
              "maleVariant": "",
              "stringId": "1807175567395217424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ケリーにキスする] ええ",
              "maleVariant": "[ケリーにキスする] ああ",
              "stringId": "1807175567479103488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたなら大丈夫。あたしがいなくてもね",
              "maleVariant": "あんたなら大丈夫だ。俺がいなくてもな",
              "stringId": "1807175567479103504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、悪い、V。お前はいい奴だが、そういう関係は、ちとな…",
              "maleVariant": "",
              "stringId": "1807175567529435152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ごめん。今のは気にしないで",
              "maleVariant": "すまん。今のは気にしないでくれ",
              "stringId": "1807175567529435164"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そろそろ戻らなきゃ。下でみんなが待ってるよ… あんたをね",
              "maleVariant": "そろそろ戻ったほうがいい。下でみんなが待ってる… あんたをな",
              "stringId": "1807175567546212356"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。今夜はこの街と、この街にいる俺に乾杯してやろう",
              "maleVariant": "",
              "stringId": "1807175567562989576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今日はもう店じまいにしてもらった",
              "maleVariant": "",
              "stringId": "1807181463915552768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "まるで夢みたいだ",
              "stringId": "1808136513764679680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よう！",
              "maleVariant": "",
              "stringId": "1811346933804068864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る]",
              "maleVariant": "",
              "stringId": "1827237053803606020"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "座れよ。紹介したい相手がいるんだ",
              "maleVariant": "",
              "stringId": "1827238069949247488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] じゃあね、スペクター",
              "maleVariant": "[立ち上がる] じゃあな、スペクター",
              "stringId": "1827255321792688128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあね、スペクター",
              "maleVariant": "じゃあな、スペクター",
              "stringId": "1827255915840352260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、座れって",
              "maleVariant": "",
              "stringId": "1827361329907052548"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんなものを扱ってるの？",
              "maleVariant": "どんなものを扱ってるんだ？",
              "stringId": "1833096823026561028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺のお気に入りスポットを教えてやるよ",
              "maleVariant": "",
              "stringId": "1833134127199891460"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう！ みんな、ありがとう！",
              "maleVariant": "",
              "stringId": "1834645726675202048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつはV… 俺の… 右腕さ",
              "maleVariant": "",
              "stringId": "1834653185020129280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ワイルドでクレイジーだけど、悪くない",
              "maleVariant": "ワイルドでクレイジーだが、悪くない",
              "stringId": "1834653454747430916"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この子たちはエネルギーに満ちてる。新曲に参加してくれって頼まれて、断れるわけがないだろ？ 後は知ってのとおりさ",
              "maleVariant": "",
              "stringId": "1834667204716130304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなこと言ったか？ この口から出る言葉は信用しちゃいけねえ。俺は小さなウソでしか、真実を語れない",
              "maleVariant": "",
              "stringId": "1834675889408438272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケリーさんはね、世界一優しい最高の人だよ！ ステキ！ カッコイイ！ カワイイ！",
              "maleVariant": "",
              "stringId": "1834680753844776960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケリーさんとは楽屋で初めて会ったんだけど、すごくドキドキしちゃって… 何ていうかもう、ひと目ぼれ！",
              "maleVariant": "",
              "stringId": "1834684999671607296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "繊細なのに、それでいてすごくワイルドなの！ そのギャップが、もうたまらない！",
              "maleVariant": "",
              "stringId": "1834688297770283008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケツがあるときにこうやって来てくれるんだ。ってわけで、失礼するよ",
              "maleVariant": "",
              "stringId": "1834693233996328960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、V！ 一ついいですか？ ずばり、生きるレジェンドのそばにいるご気分は？",
              "maleVariant": "",
              "stringId": "1834694243481415680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしがいなきゃ、彼は今ごろ牢屋に入ってたのよ",
              "maleVariant": "俺がいなきゃ、彼は今ごろ牢屋に入ってた",
              "stringId": "1834695516150689792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ノーコメント",
              "maleVariant": "",
              "stringId": "1834695666591985664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ワイルドでクレイジーって感じ",
              "maleVariant": "ワイルドでクレイジーって感じだ",
              "stringId": "1834697098107940864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね、言葉にするのも躊躇うけど…",
              "maleVariant": "そうだな、言葉にするのも躊躇うが…",
              "stringId": "1834697209307328512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ノーコメントよ",
              "maleVariant": "ノーコメントだ",
              "stringId": "1834697310507495424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼は… まさにレジェンドよ。言えるのはそれだけ",
              "maleVariant": "彼は… まさにレジェンドだ。言えるのはそれだけさ",
              "stringId": "1834698909627838464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね。彼はみんなのイメージどおり、まさにレジェンドよ",
              "maleVariant": "そうだな。彼はみんなのイメージどおり、まさにレジェンドだ",
              "stringId": "1834699454216269824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「ひと目ぼれ」？",
              "maleVariant": "",
              "stringId": "1834711909067780100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "みんな素敵よ",
              "maleVariant": "みんなイケてるな",
              "stringId": "1834714291482783744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい曲だった",
              "maleVariant": "",
              "stringId": "1834714794379833344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケリーは味方につけたようだけど、あたしはまだ納得してない",
              "maleVariant": "ケリーは味方につけたようだが、俺はまだ納得してない",
              "stringId": "1834714948310790144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "素敵ね",
              "maleVariant": "イケてるな",
              "stringId": "1834716681783078912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新曲、すごく良かった",
              "maleVariant": "新曲、すごく良かったぞ",
              "stringId": "1834716769410478080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケリーは騙せても… あたしは、あのシングルの件でまだあんたたちを疑ってるからね",
              "maleVariant": "ケリーは騙せても… 俺は、あのシングルの件でまだあんたらを疑ってるからな",
              "stringId": "1834716965032816640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "古い友人のスペクターだ。ずいぶん前に、メンフィスのビールストリートで出会った",
              "maleVariant": "",
              "stringId": "1834719738877046784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しょっちゅう来るわけじゃないが、特別な場所なんだ",
              "maleVariant": "",
              "stringId": "1834734088295673856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは特別ってこと？",
              "maleVariant": "俺は特別ってことか？",
              "stringId": "1834734859946946560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "調子に乗るな",
              "maleVariant": "かもな",
              "stringId": "1834735620827246592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こうしてると、自分が特別だって思える。たとえそれが幻でもな",
              "maleVariant": "",
              "stringId": "1834740624011161600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話して",
              "maleVariant": "話せよ",
              "stringId": "1834755541237886976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は自分が、企業の奴隷に成り下がったと思われるのが嫌なんだと思ってた。でも本当は、また誰かの影になるのが怖かったんだ",
              "maleVariant": "",
              "stringId": "1834756306278936576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう恐れる必要はないんじゃない？",
              "maleVariant": "もう恐れる必要はないんじゃないか？",
              "stringId": "1834762513832079360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはすでに頂点に立ってる。誰に力を示す必要もない",
              "maleVariant": "",
              "stringId": "1834763324255498240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "恐れるのをやめにすればいい。その必要はない",
              "maleVariant": "",
              "stringId": "1834764028344922112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[寄りかかる] 最高の眺めね",
              "maleVariant": "[寄りかかる] 最高の眺めだな",
              "stringId": "1835530933611655172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[寄りかかる] 何か悩んでるようね",
              "maleVariant": "[寄りかかる] 何か悩んでるんだろ",
              "stringId": "1835538655912521728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[寄りかかる] 二人になれてよかった",
              "maleVariant": "",
              "stringId": "1835539221522804736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これより美しい光景はそうないでしょうね…",
              "maleVariant": "これより美しい光景はそうないだろうな…",
              "stringId": "1835559903367393280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケリー、何か悩み事？",
              "maleVariant": "ケリー、何か悩み事か？",
              "stringId": "1835559987555463168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "二人になれてよかった",
              "maleVariant": "",
              "stringId": "1835560082044743680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あるさ。でもナイトシティはまた違う。忘れようとしても、忘れることができないんだ",
              "maleVariant": "",
              "stringId": "1835562405890486272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかる気がする",
              "maleVariant": "",
              "stringId": "1835566328000933892"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうこと？",
              "maleVariant": "どういうことだ？",
              "stringId": "1835566605596749824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カリエンテで話したことを、覚えてるか？",
              "maleVariant": "",
              "stringId": "1835567217797361664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうこと？ 話してよ。何か様子が変よ",
              "maleVariant": "どういうことだ？ 話せよ。何か様子が変だぞ",
              "stringId": "1835567332469633024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺は怖かったのさ。〈Us Cracks〉の一件で俺の正体が暴かれるのが… 俺がただの商品で、企業の歯車に過ぎないんだって思われるのが",
              "maleVariant": "",
              "stringId": "1835572324446367744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈Us Cracks〉とあんた自身のこと、企業の奴隷――",
              "maleVariant": "",
              "stringId": "1835574392489897984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。あの時はお前にウソをついてた。俺自分に対してもな",
              "maleVariant": "",
              "stringId": "1835575061699489792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話して",
              "maleVariant": "話してくれ",
              "stringId": "1835575790216540164"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今は相談に乗れない",
              "maleVariant": "",
              "stringId": "1835576060799479808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実際はどうなの？",
              "maleVariant": "実際はどうなんだ？",
              "stringId": "1835579742140100608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その通りさ。でも重要なのはそこじゃねえ。問題はもっと、ずっと深いところにある",
              "maleVariant": "",
              "stringId": "1835580069346144256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケリー・ユーロダインが金欲しさにアジアのポップスターに擦り寄った… スポットライトを浴びるためにプライドを捨てて自らを売った、とかってな",
              "maleVariant": "",
              "stringId": "1835581802935873536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかる気がする",
              "maleVariant": "",
              "stringId": "1835583667136229376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この街は選択肢をくれない。生きたまま焼かれるか、もともと存在しない者として消えるだけだ",
              "maleVariant": "",
              "stringId": "1835584030396510208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケリー、どうしたの？ 話なら聞くよ",
              "maleVariant": "ケリー、どうしたんだ？ 話なら聞くぞ",
              "stringId": "1835587036621959168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。実は、話がしたくて呼んだんだ。街の喧騒から離れてな",
              "maleVariant": "ああ。お前とは何となく… 馬が合う気がしてな。少し話がしたかったんだ",
              "stringId": "1835588366300213248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしたの？",
              "maleVariant": "どうしたんだ？",
              "stringId": "1835592156726292480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また？",
              "maleVariant": "",
              "stringId": "1835599499677528064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺はずっとジョニーの影で生きてた。黒子として、奴の引き立て役としてな",
              "maleVariant": "",
              "stringId": "1835599671979536384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも成功したじゃない",
              "maleVariant": "だが成功しただろ",
              "stringId": "1835602472331767812"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーはあんたが怖気づいたんだと思ってた",
              "maleVariant": "",
              "stringId": "1835603713174663168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで〈Samurai〉を抜けたのか。クソ…",
              "maleVariant": "",
              "stringId": "1835604423874310144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも成功したじゃない。コアなファン以外が〈Samurai〉を忘れ去った今、ナイトシティ中の誰もがあんたの名前を知ってる",
              "maleVariant": "だが成功しただろ。コアなファン以外が〈Samurai〉を忘れ去った今、ナイトシティ中の誰もがあんたの名前を知ってる",
              "stringId": "1835606284450787328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーは、あんたがプレッシャーに負けて〈Samurai〉を抜けたと思ってた",
              "maleVariant": "",
              "stringId": "1835606371960745984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それが何だって言うんだ？",
              "maleVariant": "",
              "stringId": "1835608283657400320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、でもそれは間違いだ。自分の力で戦ってみようと思ったのさ。今も戦い続けてる",
              "maleVariant": "",
              "stringId": "1835613993564762112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティは影の街だ。俺はそこでずっと、影を振り払おうと必死でもがいてきた。わかるか…？",
              "maleVariant": "",
              "stringId": "1835614825647566848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう簡単にいくと思うか？",
              "maleVariant": "ハッ、お前が影を追い払ってくれるのか？",
              "stringId": "1835622707147436032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしはこの街でまだ駆け出しよ。でもあんたは、街の頂点に立ってる。何を恐れる必要があるの？",
              "maleVariant": "俺はこの街でまだ駆け出しだ。だがあんたは、街の頂点に立ってる。何を恐れる必要があるんだ？",
              "stringId": "1835625446699036672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話の続きは、お前が俺と同じところに立ったときにしよう",
              "maleVariant": "",
              "stringId": "1835628046328655872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしにできると思う？",
              "maleVariant": "俺にできると思うか？",
              "stringId": "1835628915203895296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前は強い。この街に食われることはないさ",
              "maleVariant": "もちろんだ",
              "stringId": "1835629362752909312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[キスする] その言葉、忘れないでよ",
              "maleVariant": "[キスする] その言葉、忘れるなよ",
              "stringId": "1835630340696829956"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そろそろ戻ったほうがいい",
              "maleVariant": "",
              "stringId": "1835630646495145984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ケリーにキスする] 影はきっと消える",
              "maleVariant": "",
              "stringId": "1835632311868715008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしがあんたを守るよ",
              "maleVariant": "俺があんたを守ってやるよ",
              "stringId": "1835633879783436288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "影は消えるさ。俺にはわかる",
              "stringId": "1835633977527496704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたなら大丈夫… あたしがいなくてもね",
              "maleVariant": "あんたなら大丈夫だ… 俺がいなくてもな",
              "stringId": "1835636158230032384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その言葉、忘れないでよ",
              "maleVariant": "その言葉、忘れるなよ",
              "stringId": "1835637668246581248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ここでお前と一緒に、街を見下ろしてるなんてな。この瞬間が永遠に続いてほしい。凍りついた時間のなかで、ネオンの光をずっと浴びていたい",
              "stringId": "1835663543730040832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "でもそうはいかない",
              "stringId": "1835665149443829760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ああ。混沌が俺たちを呼んでるからな",
              "stringId": "1835666814230196224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "ケリーは昔からわかってなかった。叶わぬ夢より嫌なモンは――",
              "stringId": "1835668535824216064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "叶っちまった夢さ",
              "stringId": "1835670279480274944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺はもう少しここにいる",
              "maleVariant": "",
              "stringId": "1835683260230397952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前は下に戻りな。この街と、この街にいる俺に乾杯してくれ",
              "maleVariant": "",
              "stringId": "1835684089628848128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "行こう。混沌が待ってる",
              "stringId": "1835691919656218628"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "色々ありがとな。感謝してる",
              "maleVariant": "",
              "stringId": "1835693156439027712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ… だといいんだが",
              "maleVariant": "",
              "stringId": "1835696247842402304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが俺はもう少しここにいる。お前は先に戻りな",
              "maleVariant": "",
              "stringId": "1835697426743816192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。じゃあね",
              "maleVariant": "わかった。じゃあな",
              "stringId": "1835697974318592000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "にゃあ！ あなたもね！",
              "maleVariant": "",
              "stringId": "1835707784795648000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえねえねえ、次のMVに、Vも出てもらおうよ！",
              "maleVariant": "",
              "stringId": "1835709014011932672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボディガードをやってもらった方がよくない？",
              "maleVariant": "",
              "stringId": "1835709891812651008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はちょっと忙しいの。でも仕事に困ったら連絡する",
              "maleVariant": "今はちょっと忙しいんだ。だが仕事に困ったら連絡する",
              "stringId": "1835710362128347136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケリーさんのロッカーボーイの魅力とバイブスが加わったら、向かうとこ敵なしって感じ",
              "maleVariant": "",
              "stringId": "1835711322019983360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケリーさんのおかげでナイトシティをたっぷり味わえた。超神ってた！ マジ卍！",
              "maleVariant": "",
              "stringId": "1835711850082856960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "えー、萎えポヨ！ ちょっとー、そんなこと言わないでよ、V！",
              "maleVariant": "",
              "stringId": "1835717488552833024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私たちがそんな酷いことするわけないでしょ！",
              "maleVariant": "",
              "stringId": "1835717833190404096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら、嫌なことは忘れて、みんな笑顔でいこーよ",
              "maleVariant": "",
              "stringId": "1835718806336040960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V！",
              "maleVariant": "",
              "stringId": "1835731210822348800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも… 悪くはないかな",
              "maleVariant": "だが… 悪くはないかな",
              "stringId": "1835731492948013056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、V！",
              "maleVariant": "",
              "stringId": "1835732934345748480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしがいなかったら、ケリーは今ごろ牢屋の中にいるんじゃない",
              "maleVariant": "俺がいなかったら、ケリーは今ごろ牢屋の中にいるんじゃないか",
              "stringId": "1835733401004011520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもとっておきの話は、内緒にしとく",
              "maleVariant": "だがとっておきの話は、内緒にしておく",
              "stringId": "1835735159138807808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろんです、ユーロダインさん。ご友人には特別価格で",
              "maleVariant": "",
              "stringId": "1835747583204126720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "あいつは…？",
              "stringId": "1836983073412608000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "気になるのか？",
              "stringId": "1836983420247994368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "",
              "maleVariant": "いや、今はもう",
              "stringId": "1836983669708419072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも",
              "maleVariant": "",
              "stringId": "1851622350087909376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつの商品は一級品ばっかだ。身内には割引もしてくれるんだろ？",
              "maleVariant": "",
              "stringId": "1851622626861641728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ後で話をさせて",
              "maleVariant": "じゃあ後で話をさせてくれ",
              "stringId": "1851625353494786048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんなものがあるの？",
              "maleVariant": "どんなものがあるんだ？",
              "stringId": "1851625965393408000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ後で話をさせて",
              "maleVariant": "じゃあ後で話をさせてくれ",
              "stringId": "1851627732118138880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ぜひ",
              "maleVariant": "",
              "stringId": "1851628375826362368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "来いよ、見せたいものがあるんだ",
              "maleVariant": "",
              "stringId": "1851628609365209088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これでひと段落ね。で、次はどうするの？ コヴァチェクを暗殺でもする？",
              "maleVariant": "これでひと段落だな。で、次はどうする？ コヴァチェクを暗殺でもするか？",
              "stringId": "1851630351377100800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんなものがあるの？",
              "maleVariant": "どんなものがあるんだ？",
              "stringId": "1851697876383166468"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "取引の話は後にしろ。来いよ、見せたいものがある",
              "maleVariant": "",
              "stringId": "1851697876399943688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんなものを扱ってるの？",
              "maleVariant": "どんなものを扱ってるんだ？",
              "stringId": "1851697876467052544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ後で話をさせて",
              "maleVariant": "じゃあ後で話をさせてくれ",
              "stringId": "1851697876467052560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ずいぶん空いてるのね",
              "maleVariant": "ずいぶん空いてるんだな",
              "stringId": "1851697876534161412"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈Us Cracks〉の件はどうするの？ もう近づけないなら、コヴァチェクを暗殺でもする？",
              "maleVariant": "〈Us Cracks〉の件はどうするんだ？ もう近づけないなら、コヴァチェクを暗殺でもするか？",
              "stringId": "1851697876534161424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ、笑えるな",
              "maleVariant": "",
              "stringId": "1851697876550938624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とはいえ、奴を許すわけにもいかねえ。今日明日どうこうする気はないが、あの野郎には契約の件をたっぷり後悔させてやる",
              "maleVariant": "",
              "stringId": "1851697876550938636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すべてコヴァチェクの思惑通りだったってわけね",
              "maleVariant": "すべてコヴァチェクの思惑通りだったってわけか",
              "stringId": "1851697876601270276"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴は俺の味方だと思ってたが、とんだ見当違いだった",
              "maleVariant": "",
              "stringId": "1851697876618047496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうにかできないの？",
              "maleVariant": "どうにかできないのか？",
              "stringId": "1851697876618047508"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴を始末できても、他は打つ手なしだ。企業に逆らうのは無謀だからな。連中は俺の曲や歌詞の権利も握ってる…",
              "maleVariant": "",
              "stringId": "1851697876634824704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やろうと思えば、子供用のオモチャに俺の顔を印刷することだってできる。名声の代償ってヤツさ",
              "maleVariant": "",
              "stringId": "1851697876634824716"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "古い友人のスペクターだ。ずいぶん前に、メンフィスのビールストリートで出会った",
              "maleVariant": "",
              "stringId": "1851697876685156356"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも",
              "maleVariant": "",
              "stringId": "1851697876685156368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつの商品は一級品ばっかだ。身内には割引もしてくれるんだろ？",
              "maleVariant": "",
              "stringId": "1851697876685156380"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろんです、ユーロダインさん。ご友人には特別価格で",
              "maleVariant": "",
              "stringId": "1851697876701933572"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ後で話をさせて",
              "maleVariant": "じゃあ後で話をさせてくれ",
              "stringId": "1851697876769042436"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ぜひ",
              "maleVariant": "",
              "stringId": "1851697876785819656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "来いよ、見せたいものがあるんだ",
              "maleVariant": "",
              "stringId": "1851697876785819668"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今日はもう店じまいだ。静かに話したくてな",
              "maleVariant": "",
              "stringId": "1851708361237196800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケリー、悪いけど今はあんたの相談に乗れない",
              "maleVariant": "ケリー、悪いが今はあんたの相談に乗れない",
              "stringId": "1867648834275504128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他に大事な用でもあるのか？",
              "maleVariant": "",
              "stringId": "1867650285001371648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ずっと大事な用事がね",
              "maleVariant": "ずっと大事な用事がな",
              "stringId": "1867651026688536576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、そうか。わかったよ",
              "maleVariant": "",
              "stringId": "1867651343694032896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたなら何が起きても大丈夫",
              "maleVariant": "",
              "stringId": "1867653960990060548"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そろそろ戻ったほうがいいかも",
              "maleVariant": "",
              "stringId": "1867654162467647488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何が起きたって、あんたなら耐えられる。大丈夫よ",
              "maleVariant": "何が起きたって、あんたなら耐えられる。大丈夫だ",
              "stringId": "1867654844025270272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だといいけどな",
              "maleVariant": "",
              "stringId": "1867655553701507072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この業界じゃレーベルが実権を握ってる。どうにもならねえよ",
              "maleVariant": "",
              "stringId": "1928499671415255040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一人じゃ何もできないって思われてたのさ",
              "maleVariant": "",
              "stringId": "1928507540114108416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "毎朝起きるたび、俺はこの街の蒸気や悪臭、暗闇に紛れて、いつかそのまま消えちまうんじゃないかって思うんだ",
              "maleVariant": "",
              "stringId": "1928508055526961152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。じゃあね",
              "maleVariant": "わかった。じゃあな",
              "stringId": "1928528361377361920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ… そう",
              "maleVariant": "ああ… そうか",
              "stringId": "1928529589469573120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[離れる] わかった、じゃあね",
              "maleVariant": "[離れる] わかった、じゃあな",
              "stringId": "2188034697242947584"
            },
