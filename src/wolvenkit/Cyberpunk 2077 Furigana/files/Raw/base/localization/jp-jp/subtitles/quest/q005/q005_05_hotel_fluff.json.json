    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1675099609341243392,
            "femaleVariant": "ナイトシティのVIPがわんさか来る――俺のヨットにな"
          },
          {
            "stringId": 1675105775957979136,
            "femaleVariant": "<kiroshi l=\"rus\" o=\"Ты можешь нормально сказать, врал он или нет?\" t=\"もったいぶらないで、教えてよ。結局あいつは嘘をついてたの？\" b=\"\" a=\"\"/>"
          },
          {
            "stringId": 1714374171429842944,
            "femaleVariant": "あの女たち… さすがに可哀そうじゃないか…？"
          },
          {
            "stringId": 1714378792529440768,
            "femaleVariant": "最高級のドールたちだ。何も記憶に残らない"
          },
          {
            "stringId": 1714380025436712960,
            "femaleVariant": "ルールその1――何事にも驚くな、驚かされるな"
          },
          {
            "stringId": 1730089146722955264,
            "femaleVariant": "俺がなめたくなるくらいにきれいにしろ。いいな？"
          },
          {
            "stringId": 1730091539707604992,
            "femaleVariant": "客が白トリュフを望むなら、白トリュフを出すんだ！ 黒でも青でもなく、白だ！"
          },
          {
            "stringId": 1730093034305892352,
            "femaleVariant": "いいか、金を払う客が神様なら、お前らの給料を払ってる俺も同様だ！"
          },
          {
            "stringId": 1730101480812064768,
            "femaleVariant": "明日になれば休める！ 客が延泊することになったら別だが…"
          },
          {
            "stringId": 1776493427839348736,
            "femaleVariant": "こんばんは"
          },
          {
            "stringId": 1776494213415710720,
            "femaleVariant": "どうも"
          },
          {
            "stringId": 1776496925586894848,
            "femaleVariant": "どうも"
          },
          {
            "stringId": 1776497293913894916,
            "femaleVariant": "何があるの？",
            "maleVariant": "何があるんだ？"
          },
          {
            "stringId": 1776497710710272000,
            "femaleVariant": "嬢ちゃん？ 何様のつもり？",
            "maleVariant": "兄ちゃん？ 何様のつもりだ？"
          },
          {
            "stringId": 1776498046204260352,
            "femaleVariant": "注文はいい"
          },
          {
            "stringId": 1776498593007284224,
            "femaleVariant": "メニューを見せて",
            "maleVariant": "メニューを見せてくれ"
          },
          {
            "stringId": 1776499336305061888,
            "femaleVariant": "はいよ"
          },
          {
            "stringId": 1776501622938939392,
            "femaleVariant": "今はやめとく"
          },
          {
            "stringId": 1776502084413042688,
            "femaleVariant": "いつでも来なよ"
          },
          {
            "stringId": 1776529643423019008,
            "femaleVariant": "…首が飛ぶっていうのは単なるたとえ話じゃないぞ"
          },
          {
            "stringId": 1776540477192810500,
            "femaleVariant": "痛い目を見るって？"
          },
          {
            "stringId": 1776541417169891328,
            "femaleVariant": "バーテンダーの言うとおり、飲みすぎよ",
            "maleVariant": "バーテンダーの言うとおり、飲みすぎだ"
          },
          {
            "stringId": 1776541898474663936,
            "femaleVariant": "痛い目を見るって？"
          },
          {
            "stringId": 1776545646219505664,
            "femaleVariant": "死神だよ、死神が来たんだ"
          },
          {
            "stringId": 1776554122337464320,
            "femaleVariant": "バーテンダーの言うとおり、飲みすぎよ",
            "maleVariant": "バーテンダーの言うとおり、飲みすぎだ"
          },
          {
            "stringId": 1780592327177154560,
            "femaleVariant": "バーにいた奴だ… 被害妄想じゃなかったようね",
            "maleVariant": "バーにいた奴だ… 被害妄想じゃなかったようだな"
          },
          {
            "stringId": 1780623436967456768,
            "femaleVariant": "あっ、もしかして君は…！ 紺碧プラザのバーで会ったよな？"
          },
          {
            "stringId": 1783821656799367168,
            "femaleVariant": "<kiroshi l=\"rus\" o=\"А ты сама как думаешь?\" t=\"君はどう思うんだ？\" b=\"\" a=\"\"/>"
          },
          {
            "stringId": 1783821967966392320,
            "femaleVariant": "<kiroshi l=\"rus\" o=\"Я думаю, что не врал. По крайней мере, насчёт «Кан тао».\" t=\"少なくともカン・タオについては本当のこと言ってたと思うけど\" b=\"\" a=\"\"/>"
          },
          {
            "stringId": 1783822360670687232,
            "femaleVariant": "<kiroshi l=\"rus\" o=\"Зая, ты же помнишь, как мы тестировали эту технику на наших.\" t=\"おいおい… うちでやった実験を覚えてないのか？\" b=\"\" a=\"\"/>"
          },
          {
            "stringId": 1783822608184954880,
            "femaleVariant": "<kiroshi l=\"rus\" o=\"То есть он тоже? Охренеть. Он так натурально смеялся.\" t=\"じゃあ、あいつも？ あんなにこやかに笑ってたのに？\" b=\"\" a=\"\"/>"
          },
          {
            "stringId": 1783822789664100352,
            "femaleVariant": "<kiroshi l=\"rus\" o=\"А вот его мозговые волны вообще не смеялись.\" t=\"脳波は笑ってなかった。まったくな\" b=\"\" a=\"\"/>"
          },
          {
            "stringId": 1783842792618643456,
            "femaleVariant": "俺はどうなる？ あの光景が頭から離れないんだ！"
          },
          {
            "stringId": 1783842868887867392,
            "femaleVariant": "クソ… 余計な詮索なんかするんじゃなかった…"
          },
          {
            "stringId": 1783843059879694336,
            "femaleVariant": "そうは言ってもな… あいつら、ひと晩でお前のひと月分を稼いじまうんだぞ"
          },
          {
            "stringId": 1783847170264059904,
            "femaleVariant": "どこかの金持ち女が飼い犬を何匹もプールに飛び込ませても、じろじろ見たり俺に報告したりするな"
          },
          {
            "stringId": 1783847318188773376,
            "femaleVariant": "嵐の後みたいに部屋をめちゃくちゃにした客がいても…"
          },
          {
            "stringId": 1783847534933626880,
            "femaleVariant": "何も聞かない、じろじろ見ない、いちいち報告しない"
          },
          {
            "stringId": 1783847646317563904,
            "femaleVariant": "飲み込みが早いな、気に入った。客の子供が窓からテレビを落とそうとしててもだぞ。黙って見てろ"
          },
          {
            "stringId": 1783847692136140800,
            "femaleVariant": "もし落とそうとしてるのが、テレビではなく人だったら…？"
          },
          {
            "stringId": 1783847800047194112,
            "femaleVariant": "その時は報告しろ。ただし目立たないようにな"
          },
          {
            "stringId": 1784825328530411520,
            "femaleVariant": "アナが？ なんでまたナイトシティに？"
          },
          {
            "stringId": 1784825425435611136,
            "femaleVariant": "主演映画のプロモーションだ。興行収入はもう20億に達したらしい…"
          },
          {
            "stringId": 1784825707712270336,
            "femaleVariant": "あの話の原案は俺だったんだ。球形のジッグラトさ"
          },
          {
            "stringId": 1789301703534497792,
            "femaleVariant": "ちょっと、飲みすぎじゃないか？ 他のお客さんもいるんだから…"
          },
          {
            "stringId": 1789302368969216000,
            "femaleVariant": "聞かせてやれ！ ここにいるみんなが痛い目を見るんだ！"
          },
          {
            "stringId": 1789306519115583488,
            "femaleVariant": "そうかもな。それに俺一人に何ができるってんだ"
          },
          {
            "stringId": 1789307101066874880,
            "femaleVariant": "そろそろ寝るか…"
          },
          {
            "stringId": 1789312666253971460,
            "femaleVariant": "悪いけど、聞いてる暇はない",
            "maleVariant": "悪いが、聞いてる暇はない"
          },
          {
            "stringId": 1789314390062583808,
            "femaleVariant": "ふん、あとでほえ面かくなよ"
          },
          {
            "stringId": 1789316991269261312,
            "femaleVariant": "大声出さないで。サブロウ・アラサカの娘なのよ",
            "maleVariant": "大声出すな。サブロウ・アラサカの娘だぞ"
          },
          {
            "stringId": 1789317355200630784,
            "femaleVariant": "ちょっと、相手は帝王の娘よ？",
            "maleVariant": "おい、相手は帝王の娘だぞ？"
          },
          {
            "stringId": 1789317409307152388,
            "femaleVariant": "だから何？",
            "maleVariant": "だから何だ"
          },
          {
            "stringId": 1789317720558063616,
            "femaleVariant": "だから何よ",
            "maleVariant": "だから何だ"
          },
          {
            "stringId": 1789317763440627712,
            "femaleVariant": "聞いてる暇はない"
          },
          {
            "stringId": 1789320265141637120,
            "femaleVariant": "あー… 君の言う通りだ"
          },
          {
            "stringId": 1789322186602631168,
            "femaleVariant": "何もわかってないんだな…"
          },
          {
            "stringId": 1790687663526563844,
            "femaleVariant": "人違いよ",
            "maleVariant": "人違いだ"
          },
          {
            "stringId": 1790688788506005504,
            "femaleVariant": "人違いじゃない？",
            "maleVariant": "人違いじゃないか？"
          },
          {
            "stringId": 1790688837159931908,
            "femaleVariant": "ええ、覚えてる",
            "maleVariant": "ああ、覚えてるよ"
          },
          {
            "stringId": 1790688968710082560,
            "femaleVariant": "覚えてる",
            "maleVariant": "覚えてるよ"
          },
          {
            "stringId": 1790692121903943680,
            "femaleVariant": "そんな、覚えてないのか？ ほら、アラサカの空母の… 俺もかなり酔ってたからな…"
          },
          {
            "stringId": 1790692219228573696,
            "femaleVariant": "じゃあ、そういうことね",
            "maleVariant": "じゃあ、そういうことだろ"
          },
          {
            "stringId": 1790692271841923072,
            "femaleVariant": "そういうことって？"
          },
          {
            "stringId": 1790692391144706048,
            "femaleVariant": "酔ってたってこと",
            "maleVariant": "酔ってたってことだ"
          },
          {
            "stringId": 1790692932176367616,
            "femaleVariant": "あー、うん… そうだな。悪い、人違いだったみたいだ…"
          },
          {
            "stringId": 1790693483123363840,
            "femaleVariant": "聞いたかい？ 君が出てった後、大事件があったのさ…"
          },
          {
            "stringId": 1790694435549138944,
            "femaleVariant": "まだ生きてるのは君のおかげだ…"
          },
          {
            "stringId": 1790696880627044352,
            "femaleVariant": "借りができたな"
          },
          {
            "stringId": 1966318080072949760,
            "femaleVariant": "じゃあなおさら来たらいい。彼女の成功に貢献した人物としてな。ただし銃は家に置いてこいよ"
          },
          {
            "stringId": 1966318649038675968,
            "femaleVariant": "ふん。うるせえ"
          },
          {
            "stringId": 1966417520695779328,
            "femaleVariant": "いいえ、ダメよ。キューバの件はまだしも、ナイロビに関しては折れるわけにいかない"
          },
          {
            "stringId": 1966418751505899520,
            "femaleVariant": "おいおい、ファリダ… 俺は関係ない――"
          },
          {
            "stringId": 1966419597899669504,
            "femaleVariant": "関係あるわ。あんたはあの街を実験場にしようとしてる。私たちがこれまで散々投資した街をね"
          },
          {
            "stringId": 1966420714205290496,
            "femaleVariant": "利益が望めないなら、撤退するしかない。でもその時、ついでに街のあちこちを爆破するかもね。そうなってもいいわけ？"
          },
          {
            "stringId": 1966421103352815616,
            "femaleVariant": "あー、いや… わかった、工場は別の場所に建てよう。時間はどれくらいもらえる？"
          },
          {
            "stringId": 1966425161627148288,
            "femaleVariant": "この酒を飲み終わるまでよ。私は飲むのが早いの――だからさっさと電話をすることね"
          },
          {
            "stringId": 1983624045050798080,
            "femaleVariant": "レイチェル・ザッカリア！？"
          },
          {
            "stringId": 1983624965062991872,
            "femaleVariant": "すごいだろう"
          },
          {
            "stringId": 1983625060441464832,
            "femaleVariant": "世界一のモデルをどうやって口説いたんだ？"
          },
          {
            "stringId": 1983625335604584448,
            "femaleVariant": "口説いてなんかいない"
          },
          {
            "stringId": 1983625604996341760,
            "femaleVariant": "拉致したのさ。ブエノス・シックスのショーの直後にな"
          },
          {
            "stringId": 1983626449360068608,
            "femaleVariant": "いいのか、それ？ で、ザッカリアは？ 嫌がってないのか？"
          },
          {
            "stringId": 1983626842987110400,
            "femaleVariant": "一緒に食事して話せばすぐモノにできるだろう"
          },
          {
            "stringId": 1983629691355734016,
            "femaleVariant": "ダメなら、今夜飛行機で送り返せばいい"
          },
          {
            "stringId": 1983760646787637248,
            "femaleVariant": "泥酔しながら、誰が聞いてるかもわからない場所でする話じゃない"
          },
          {
            "stringId": 1983782865710424064,
            "femaleVariant": "その調子だと、今に大変なことになる",
            "maleVariant": "その調子だと、今に大変なことになるぞ"
          },
          {
            "stringId": 1983789867379531776,
            "femaleVariant": "それに… お前が別れたあいつも来るぞ"
          },
          {
            "stringId": 1983797723998789632,
            "femaleVariant": "今日、誰の運転手をしたと思う？"
          },
          {
            "stringId": 1983800262425124864,
            "femaleVariant": "市長だ"
          },
          {
            "stringId": 1983802023479156736,
            "femaleVariant": "へえ… だから？"
          },
          {
            "stringId": 1983803199645884416,
            "femaleVariant": "は？ だからって何だよ？"
          },
          {
            "stringId": 1983803884542173184,
            "femaleVariant": "俺は昨日、ティナ・カルソを乗せた。バイオテクニカの西海岸支社長だ"
          },
          {
            "stringId": 1983810889130516480,
            "femaleVariant": "2台目の車には彼女の理学療法士…"
          },
          {
            "stringId": 1983811973207105536,
            "femaleVariant": "3台目にはピューマが乗ってた"
          },
          {
            "stringId": 1983814138961141760,
            "femaleVariant": "ピューマ？"
          },
          {
            "stringId": 1983814219491778560,
            "femaleVariant": "そうさ。絶滅したヤツだ。彼女の服の色に合うからって、復活させたらしい"
          },
          {
            "stringId": 1983820027210309632,
            "femaleVariant": "市長程度じゃ自慢にもならないな"
          },
          {
            "stringId": 1983865895867195392,
            "femaleVariant": "最高だ…！ ん？ あれ… 飲み物に何か入れたのか？"
          },
          {
            "stringId": 1983870070407634944,
            "femaleVariant": "どういたしまして"
          },
          {
            "stringId": 1983871138243878912,
            "femaleVariant": "だからこんなに… こんなに… へへ…"
          },
          {
            "stringId": 1983872313303310336,
            "femaleVariant": "まだ手始めよ。もう少ししたら、全ての感覚が倍増するから…"
          },
          {
            "stringId": 1983873852931645440,
            "femaleVariant": "特に痛みがね"
          },
          {
            "stringId": 1983874580123295744,
            "femaleVariant": "い… 痛み？"
          },
          {
            "stringId": 1983874679712849920,
            "femaleVariant": "今日は新しいことを試したいの。いいでしょう？"
          },
          {
            "stringId": 1983875249215447040,
            "femaleVariant": "え？ ああ… もちろんさ"
          },
          {
            "stringId": 1983882708231462912,
            "femaleVariant": "わざわざシアトルからプライベートジェットを手配してくれるなんて。相当金がかかっただろう"
          },
          {
            "stringId": 1983911631178768384,
            "femaleVariant": "あなたにはその価値があるから"
          },
          {
            "stringId": 1986532688020709376,
            "femaleVariant": "ボッシュやマネ、バスキアのミニチュア2点… どれも名作ばかりよ…"
          },
          {
            "stringId": 1986538281947176960,
            "femaleVariant": "そのすべてがステーションの穴から宇宙の暗闇へ吸い込まれていったの"
          },
          {
            "stringId": 1986539748963721216,
            "femaleVariant": "そんな… ギャラリーはどうなるの？"
          },
          {
            "stringId": 1986540185993420800,
            "femaleVariant": "ああ、そうじゃなくて――そういう企画だったのよ！ それがオープニングの目玉！"
          },
          {
            "stringId": 1986540631160070144,
            "femaleVariant": "待って… もしかして… アブラモフスキーの新作？"
          },
          {
            "stringId": 1986541318270951424,
            "femaleVariant": "そう、『無題3』よ。テーマは確か… なんだっけ。神への報い… だったかしら"
          },
          {
            "stringId": 1986542014458306560,
            "femaleVariant": "野心的ね。それで、いくらだったの？"
          },
          {
            "stringId": 1986542473617154048,
            "femaleVariant": "彼の作品に、値段なんてつけようがないわ"
          },
          {
            "stringId": 1988089095635410944,
            "femaleVariant": "気分はどう？"
          },
          {
            "stringId": 1988092836367376384,
            "femaleVariant": "長さは400ヤード以上、排水量は10万トン、原子力で動く死神が、いま港に停泊してる"
          },
          {
            "stringId": 1990825294426873856,
            "femaleVariant": "嬢ちゃん、ご注文は？",
            "maleVariant": "兄ちゃん、ご注文は？"
          },
          {
            "stringId": 1990832635415175168,
            "femaleVariant": "いつもそんなに馴れ馴れしいの？",
            "maleVariant": "いつもそんなに馴れ馴れしいのか？"
          },
          {
            "stringId": 1990835064034631680,
            "femaleVariant": "嬢ちゃん？",
            "maleVariant": "兄ちゃん？"
          },
          {
            "stringId": 1990835360236380160,
            "femaleVariant": "ずいぶん慣れ慣れしいのね",
            "maleVariant": "ずいぶん慣れ慣れしいんだな"
          },
          {
            "stringId": 1990836061322682372,
            "femaleVariant": "ウソばっかり…",
            "maleVariant": "ウソつけ…"
          },
          {
            "stringId": 1990836821112463360,
            "femaleVariant": "面白い話ね",
            "maleVariant": "面白い話だな"
          },
          {
            "stringId": 1990837164089090048,
            "femaleVariant": "デタラメね…",
            "maleVariant": "デタラメだな…"
          },
          {
            "stringId": 1990837388836675584,
            "femaleVariant": "面白かったよ。作り話にしては"
          },
          {
            "stringId": 1990837708660744192,
            "femaleVariant": "あー…"
          },
          {
            "stringId": 1990839443710091264,
            "femaleVariant": "デタラメか… それはお互い様じゃないか？"
          },
          {
            "stringId": 1990846393873145856,
            "femaleVariant": "どうして俺が従業員だと？"
          },
          {
            "stringId": 1990847082242318336,
            "femaleVariant": "あなた… バーテンダーでしょ？",
            "maleVariant": "あんた… バーテンダーだろ？"
          },
          {
            "stringId": 1990847557188526080,
            "femaleVariant": "お客さん… 誰が紺碧を所有してるか知ってるかい？"
          },
          {
            "stringId": 1990848218596712448,
            "femaleVariant": "いいえ",
            "maleVariant": "いいや"
          },
          {
            "stringId": 1990848532431314944,
            "femaleVariant": "おかしなことに、誰も知らないのさ。まるで幽霊だ"
          },
          {
            "stringId": 1990855558544826368,
            "femaleVariant": "だがその謎のオーナーは、とにかくサービスの品質にうるさいらしくてね"
          },
          {
            "stringId": 1990856177104003072,
            "femaleVariant": "ここだけの話なんだが… 時々バーテンダーのふりをしてこっそり働いてるっていう噂がある"
          },
          {
            "stringId": 1990858213723807744,
            "femaleVariant": "母親譲りの性格でな"
          },
          {
            "stringId": 1990858481488175104,
            "femaleVariant": "母乳は半分タンパク質、もう半分はハイオクのCHOOH2"
          },
          {
            "stringId": 1990859249767227392,
            "femaleVariant": "何の話よ…？",
            "maleVariant": "何を言ってるんだ…？"
          },
          {
            "stringId": 1990859488943218688,
            "femaleVariant": "風になびく髪の毛は、ほこりまみれだった"
          },
          {
            "stringId": 1990859969056808960,
            "femaleVariant": "アルデカルドスだったのさ。戦争の前の話だが"
          },
          {
            "stringId": 1990860909973413888,
            "femaleVariant": "「どこへ行っても、自分らしくありなさい、デイビッド」って言い残して死んでった"
          },
          {
            "stringId": 1990861698049581056,
            "femaleVariant": "だから俺はここで、俺らしさを貫いてる"
          },
          {
            "stringId": 1990874238951432192,
            "femaleVariant": "嘘も方便――嬢ちゃんもそう思うだろ？",
            "maleVariant": "嘘も方便――兄ちゃんもそう思うだろ？"
          },
          {
            "stringId": 1990875926001164288,
            "femaleVariant": "で、何にする？ せめて… 水くらいはどうだ？"
          },
          {
            "stringId": 1990891200314155008,
            "femaleVariant": "あなたの名前は？ 後で上司に報告させてもらう",
            "maleVariant": "あんたの名前は？ 後で上司に報告させてもらう"
          },
          {
            "stringId": 1990916268847079424,
            "femaleVariant": "ハナコ・アラサカが… ひっく… 遊びにいらっしゃったのさ！"
          },
          {
            "stringId": 1990963118786551808,
            "femaleVariant": "わかった。忠告どおりにさせてもらう！"
          },
          {
            "stringId": 1990970582802178048,
            "femaleVariant": "よかった。じゃあ、15分後に部屋に来て。鏡とカメラは隠すのよ"
          },
          {
            "stringId": 1993562117683093504,
            "femaleVariant": "それはまた、何のために？"
          },
          {
            "stringId": 1993562451482583040,
            "femaleVariant": "さあ… 現場を確認するため… それか小遣い稼ぎのためかもな"
          },
          {
            "stringId": 1993573715000762368,
            "femaleVariant": "気づく頃には――ひっく――手遅れなんだ"
          },
          {
            "stringId": 1993669380900986880,
            "femaleVariant": "やってられっか！"
          },
          {
            "stringId": 1993671854852481024,
            "femaleVariant": "ほら、さっさと行きな。大ごとになる前にね",
            "maleVariant": "ほら、さっさと行けよ。大ごとになる前にな"
          },
          {
            "stringId": 1996532066647089152,
            "femaleVariant": "ははは！"
          },
          {
            "stringId": 1999709244159238144,
            "femaleVariant": "どうも"
          },
          {
            "stringId": 1999709806917394432,
            "femaleVariant": "どうも"
          },
          {
            "stringId": 1999712966268493824,
            "femaleVariant": "また来たのか、嬢ちゃん。何にする？",
            "maleVariant": "また来たのか、兄ちゃん。何にする？"
          },
          {
            "stringId": 2012989603718176768,
            "femaleVariant": "禁輸措置？ それが何だと言うんだ？"
          },
          {
            "stringId": 2012990879407349760,
            "femaleVariant": "例のガレージには、最新モデルのダイカニ20台、徹甲弾のコンテナ10個、対人地雷1トン分が用意してある"
          },
          {
            "stringId": 2012992502821101568,
            "femaleVariant": "何とかしろ。さもなくば品物はソ連かゼロ・シンジケートに送る。なんなら軌道上から落としてやってもいい"
          },
          {
            "stringId": 2012993619613261824,
            "femaleVariant": "だがその前に、まずはお前の口に地雷を突っ込んでやるからな。このハジメ・タキはお前と違って、言ったことを必ず守る"
          },
          {
            "stringId": 2012994324390551552,
            "femaleVariant": "クルド人は今ペトロケムと“交渉”中だ。もし連中にダイカニを渡せば、どうなるかわかるか？ バイオ農場から出る煙で、ナイトシティ市民の目がしみることになる"
          },
          {
            "stringId": 2012997319358488576,
            "femaleVariant": "ああ… そうだ大臣、それでいい。ようやく話が通じたようだ"
          },
          {
            "stringId": 2012997914765107200,
            "femaleVariant": "奴もこれで立場がわかっただろう"
          },
          {
            "stringId": 2013007420819578880,
            "femaleVariant": "ああ。で、次に会うミリテクの連中はどこだ？ 約束の時間は過ぎてるだろう"
          },
          {
            "stringId": 2013008087126712320,
            "femaleVariant": "美しい奥さんによろしく言ってくれ"
          },
          {
            "stringId": 2013021042425798656,
            "femaleVariant": "<kiroshi l=\"rus\" o=\"Про заражение под Новоиркутском они врали, про аварию на «Бакунинской-3», врали. Они всегда врут.\" t=\"ニュー・イルクーツク州の化学汚染に、バクニン・スリーの爆発――全員が嘘をついてた。人間は嘘をつく生き物なのさ\" b=\"\" a=\"\"/>"
          },
          {
            "stringId": 2014459769329098752,
            "femaleVariant": "今のままじゃ、ブレインダンスはダメだと思うんだよな。単純な表現しかできてないから"
          },
          {
            "stringId": 2014460849899249664,
            "femaleVariant": "笑いといえばコメディだし、恐怖はホラーだし、恋愛はラブロマンスじゃん？ ジャンル映画の方法を使ってるだけでしょう？"
          },
          {
            "stringId": 2014461638847832064,
            "femaleVariant": "感情表現って、そうじゃないんじゃないかな？ だから今、東京のチームと実験をしてるんだよ"
          },
          {
            "stringId": 2014462673398079488,
            "femaleVariant": "たとえば、嬉しいけど、もの哀しいとか、落ち着いてるけど、何か不安を感じるとか。そういう感覚ってあるでしょう？"
          },
          {
            "stringId": 2025737534300942336,
            "femaleVariant": "人間の感情がもっと豊かな作品を創ろうと思ってね"
          },
          {
            "stringId": 2028846827841712128,
            "femaleVariant": "はい、なんです？"
          },
          {
            "stringId": 2028846881277145088,
            "femaleVariant": "新しい企画のオファー？"
          },
          {
            "stringId": 2028846897383272448,
            "femaleVariant": "僕の作品、知ってるの？"
          },
          {
            "stringId": 2028847596691189760,
            "femaleVariant": "ああ、すいません。いま、時間ないんで"
          },
          {
            "stringId": 2030120029425917956,
            "femaleVariant": "どうも"
          },
          {
            "stringId": 2069124038754377728,
            "femaleVariant": "僕はリアルを感じてもらいたいんだよ。もちろん、現実には体験できない物語を通してね"
          },
          {
            "stringId": 2539069666311262208,
            "femaleVariant": "ねえ",
            "maleVariant": "やあ"
          }
        ]
      }
    }
  }
