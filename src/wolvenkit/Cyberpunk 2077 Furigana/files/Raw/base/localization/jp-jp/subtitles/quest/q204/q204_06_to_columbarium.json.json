    },
    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1306592593676619792,
            "femaleVariant": "さて。行こうか"
          },
          {
            "stringId": 1306596955182354432,
            "femaleVariant": "親父は、また企業戦争が始まるって言ってた。今にもどっちかがしびれを切らすって"
          },
          {
            "stringId": 1306604025017290772,
            "femaleVariant": "もう戦争は起きない"
          },
          {
            "stringId": 1306605983539154944,
            "femaleVariant": "わからない"
          },
          {
            "stringId": 1306606769115516928,
            "femaleVariant": "心配しなくても、もう戦争は起きない。アラサカは見た目より弱ってる"
          },
          {
            "stringId": 1306607026159243264,
            "femaleVariant": "俺に聞くな。そういう話題には興味ねえ。正直、どうなってもいいと思ってる"
          },
          {
            "stringId": 1315740078154932224,
            "femaleVariant": "OK。どこまで？"
          },
          {
            "stringId": 1315740524261105664,
            "femaleVariant": "墓地までだ"
          },
          {
            "stringId": 1315740729362571264,
            "femaleVariant": "了解"
          },
          {
            "stringId": 1315755141075001344,
            "femaleVariant": "というわけで、お届けしたのは『Perilous Futur』"
          },
          {
            "stringId": 1315756886777880576,
            "femaleVariant": "…とここで不穏なニュースが入ってきたぜ。アラサカがミリテクとの交渉を打ち切ったらしい"
          },
          {
            "stringId": 1315760779209433088,
            "femaleVariant": "アラサカはあれで経済的打撃をくらった。しかし“魂の救済”へのバッシングで受けた被害に比べたら微々たるもんだ。メディアがあんな風にアラサカの事業をこき下ろすとはな！"
          },
          {
            "stringId": 1315763332450050048,
            "femaleVariant": "ホントかな？ また戦争が始まると思う？"
          },
          {
            "stringId": 1315770269895974912,
            "femaleVariant": "俺もだ。人々から搾取するだけじゃ腹が満たされず、企業同士が互いを餌食にしようとしてやがる"
          },
          {
            "stringId": 1315778445131341824,
            "femaleVariant": "なぜかわかるか？"
          },
          {
            "stringId": 1315778613104828416,
            "femaleVariant": "さあ？"
          },
          {
            "stringId": 1315778708936286208,
            "femaleVariant": "気にする意味がないことに気づいたからさ"
          },
          {
            "stringId": 1315781805037281280,
            "femaleVariant": "俺、戦争なんて嫌だな…"
          },
          {
            "stringId": 1315795029375803392,
            "femaleVariant": "でももし親父が送り込まれるなら、それは構わないや"
          },
          {
            "stringId": 1315795062007488512,
            "femaleVariant": "戦争はクソだし企業はバカだ"
          },
          {
            "stringId": 1319806003434971136,
            "femaleVariant": "うわ！ 銃なんか持ってたのか… しかも本物だ。昔、親父も売ってたよ"
          },
          {
            "stringId": 1319807838107426816,
            "femaleVariant": "理由があればな"
          },
          {
            "stringId": 1319857034457997312,
            "femaleVariant": "これは形見だ"
          },
          {
            "stringId": 1319881814020153344,
            "femaleVariant": "いや、これは形見だ"
          },
          {
            "stringId": 1319894456105394176,
            "femaleVariant": "もうそういうことはしない。理由があれば話は別だが"
          },
          {
            "stringId": 1319898519966539776,
            "femaleVariant": "てことは… 大事な人のものなの？"
          },
          {
            "stringId": 1319898684668469248,
            "femaleVariant": "そうだな… 「大事」だけじゃ… 言い表せない"
          },
          {
            "stringId": 1319963666634465280,
            "femaleVariant": "ちょっと前まで、命なんて大して価値のないもんだと思ってた"
          },
          {
            "stringId": 1319964109569744896,
            "femaleVariant": "自分の命もそうだが… 他人の命はなおさらな"
          },
          {
            "stringId": 1319964724521820160,
            "femaleVariant": "それ、どうするの？"
          },
          {
            "stringId": 1319964724538597376,
            "femaleVariant": "持ち主に返すのさ"
          },
          {
            "stringId": 1319985180008210432,
            "femaleVariant": "もう、って…？"
          },
          {
            "stringId": 1324194862440603648,
            "femaleVariant": "鬼軍曹の下に回されて… 根性を叩き直してもらえばいい"
          },
          {
            "stringId": 1678066081898414080,
            "femaleVariant": "一緒に行ってもいい？"
          },
          {
            "stringId": 1678117113860964352,
            "femaleVariant": "ダメだ。ここで待ってろ。すぐに戻る"
          },
          {
            "stringId": 1695620305884241920,
            "femaleVariant": "わかった"
          },
          {
            "stringId": 1695625530175086592,
            "femaleVariant": "「平和的手段による問題解決の可能性はもはや皆無だ」"
          },
          {
            "stringId": 1695627438080090112,
            "femaleVariant": "こりゃいよいよだな。匿名の情報筋によると、ヨリノブ・アラサカは今日、こんなことを言ったらしいぞ…"
          },
          {
            "stringId": 1695631235149946880,
            "femaleVariant": "企業は嫌いだ"
          },
          {
            "stringId": 1723017670488186880,
            "femaleVariant": "誰かを撃つの？"
          },
          {
            "stringId": 1725928290669780992,
            "femaleVariant": "だがちょっと待ってくれ。いつも聴いてくれてるリスナーなら、何か月か前にアラサカ・タワーに突入した根性ある奴らのことを覚えてるよな！"
          },
          {
            "stringId": 1730255380177182720,
            "femaleVariant": "この辺にツレが住んでるんだ"
          },
          {
            "stringId": 1730258487653130240,
            "femaleVariant": "どこだ？"
          },
          {
            "stringId": 1730258580783456256,
            "femaleVariant": "いや、ここからじゃ見えないよ"
          },
          {
            "stringId": 1730261268040474624,
            "femaleVariant": "そいつとは仲いいのか？"
          },
          {
            "stringId": 1730261493710807040,
            "femaleVariant": "一度、家に呼んでくれた"
          },
          {
            "stringId": 1730262871724220416,
            "femaleVariant": "そいつのお母さんが昼飯を作ってくれてさ"
          },
          {
            "stringId": 1730268947274227712,
            "femaleVariant": "ここを通るのは好きだ。いい所だよ"
          },
          {
            "stringId": 1730287627680833536,
            "femaleVariant": "そうだな。確かに、ここはいい所だ"
          },
          {
            "stringId": 1730287656202100736,
            "femaleVariant": "この街は… 生きてる"
          },
          {
            "stringId": 1730296529889746944,
            "femaleVariant": "ここに来たのは初めて？"
          },
          {
            "stringId": 1730296696000962560,
            "femaleVariant": "いいや。でも初めて気づいた"
          },
          {
            "stringId": 1730297565060751360,
            "femaleVariant": "まるで、街が指の隙間からすり抜けてったみたいだ"
          },
          {
            "stringId": 1730298136626946048,
            "femaleVariant": "ゆっくり眺めたことなんてなかったからな…"
          },
          {
            "stringId": 1730300930184404992,
            "femaleVariant": "いいペンダントだ"
          },
          {
            "stringId": 1730300930184405008,
            "femaleVariant": "似合ってる"
          },
          {
            "stringId": 1730300930234736640,
            "femaleVariant": "俺のじゃない"
          },
          {
            "stringId": 1730300930234736656,
            "femaleVariant": "形見なんだ"
          },
          {
            "stringId": 1730300931073597440,
            "femaleVariant": "実は俺のじゃない"
          },
          {
            "stringId": 1730300931073597456,
            "femaleVariant": "じゃあ、誰の？"
          },
          {
            "stringId": 1730300931073597472,
            "femaleVariant": "昔知り合いだった… イカれた女のだ",
            "maleVariant": "昔知り合いだった… イカれた男のだ"
          },
          {
            "stringId": 1730300931090374660,
            "femaleVariant": "俺はそいつの代わりにここにいる"
          },
          {
            "stringId": 1730300931107151872,
            "femaleVariant": "形見なんだ"
          },
          {
            "stringId": 1730300931123929100,
            "femaleVariant": "てことは… 大事な人のものなの？"
          },
          {
            "stringId": 1730300931123929116,
            "femaleVariant": "ああ… 誰よりも大事な奴のだ"
          },
          {
            "stringId": 1730300931157483520,
            "femaleVariant": "それどうするんだい？"
          },
          {
            "stringId": 1730300931157483536,
            "femaleVariant": "持ち主に返すのさ"
          },
          {
            "stringId": 1730307983728664576,
            "femaleVariant": "死んだの？"
          },
          {
            "stringId": 1730308090549198848,
            "femaleVariant": "ああ。こいつが殺したんだ"
          },
          {
            "stringId": 1730315530086866944,
            "femaleVariant": "足元や頭上を通り過ぎようが、それに気づくことすらできなかった"
          },
          {
            "stringId": 1730318307705970688,
            "femaleVariant": "そりゃ、みんな歩きまわってるし？"
          },
          {
            "stringId": 1730319162253467648,
            "femaleVariant": "そういうことじゃない"
          },
          {
            "stringId": 1730320283877466112,
            "femaleVariant": "人間が創り出した化けもんが、いつの間にか人間を食い物にするようになっちまった"
          },
          {
            "stringId": 1730327073365123072,
            "femaleVariant": "それを曲にしたら？"
          },
          {
            "stringId": 1730327767975419904,
            "femaleVariant": "考えとく"
          },
          {
            "stringId": 1730330836360454144,
            "femaleVariant": "フランケンシュタインみたいだね"
          },
          {
            "stringId": 1730331496040587264,
            "femaleVariant": "フランケンシュタインの怪物、な"
          },
          {
            "stringId": 1730334243393593344,
            "femaleVariant": "そうだな。いい所だ"
          },
          {
            "stringId": 1730334524713951232,
            "femaleVariant": "この街は生き物みたいなもんだ"
          },
          {
            "stringId": 1804046445232324612,
            "femaleVariant": "[ギターを車に積む]"
          },
          {
            "stringId": 1872277745848508416,
            "femaleVariant": "リスナーのみんなも忘れちゃいないよな？"
          },
          {
            "stringId": 1872278182626549760,
            "femaleVariant": "人格の強奪、記憶痕跡の虐待などなど、アラサカは猛烈な非難を浴びた。なかでもとびきり強烈だったのが、「神の領域」に踏み込んだことだ"
          },
          {
            "stringId": 1872280176112132096,
            "femaleVariant": "しかしそう考えると、今回のミリテクとのいざこざも、違う見方ができる。専門家によると、アラサカは別の問題から世間の目をごまかそうとしてるだけだって話だ"
          },
          {
            "stringId": 1872280681391546368,
            "femaleVariant": "どう思う？ 日本の“巨人”は、この危機を乗り越えられるのだろうか？"
          },
          {
            "stringId": 2027344929925169156,
            "femaleVariant": "[消す]"
          },
          {
            "stringId": 2028943422464786436,
            "femaleVariant": "[降りる] ここで待ってろ"
          },
          {
            "stringId": 2301321051489656832,
            "femaleVariant": "あの時はシルヴァーハンドの時代を思い出したぜ！"
          }
        ]
      }
    }
  }
}