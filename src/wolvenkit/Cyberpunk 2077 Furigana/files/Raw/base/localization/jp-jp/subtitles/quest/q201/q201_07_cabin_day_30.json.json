  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんた名前は？",
              "maleVariant": "",
              "stringId": "1673415053142888448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おはよう。起きてくれ",
              "maleVariant": "",
              "stringId": "1680502215467884544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おはよう",
              "maleVariant": "",
              "stringId": "1680502340407812096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まさかここであんたに会うとは思わなかった",
              "maleVariant": "",
              "stringId": "1680502455298187264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[体を起こす] おはよう",
              "maleVariant": "",
              "stringId": "1680502559769911300"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[体を起こす] アンダース・ヘルマン。なんであんたが…",
              "maleVariant": "",
              "stringId": "1680504270441963520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[体を起こす] 今は朝なの？",
              "maleVariant": "[体を起こす] 今は朝なのか？",
              "stringId": "1680507014959620096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あれ？ 今は朝なの？",
              "maleVariant": "あれ？ 今は朝なのか？",
              "stringId": "1680507434171916288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人生は驚きの連続だな",
              "maleVariant": "",
              "stringId": "1680508892833406976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょうど2時間前に、ナイトシティで日が昇った",
              "maleVariant": "",
              "stringId": "1680509711293112320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やれやれ、ずいぶんと散らかしたな",
              "maleVariant": "",
              "stringId": "1680512680709349376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何の用？",
              "maleVariant": "何の用だ？",
              "stringId": "1680513650231107584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "君と話をするよう頼まれた",
              "maleVariant": "",
              "stringId": "1680513909522980864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしに話って、いったい何？",
              "maleVariant": "俺に話って、いったい何だ？",
              "stringId": "1680514399786786816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカに戻ったのね",
              "maleVariant": "アラサカに戻ったんだな",
              "stringId": "1680514555311579136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またアラサカに戻ったのね。今度は定年までいるつもり？",
              "maleVariant": "アラサカに戻ったんだな。今度は定年までいるつもりか？",
              "stringId": "1680517539256885248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで？ 何の話があるっていうの？",
              "maleVariant": "それで？ 何の話があるっていうんだ？",
              "stringId": "1680518018196070400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのイカれた一家には愛想を尽かしたのかと思ってたけど",
              "maleVariant": "あのイカれた一家には愛想を尽かしたのかと思ってたが",
              "stringId": "1680520431497928704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカにかなう者などいやしない。それに私の能力を活かすのにふさわしい、新たなプロジェクトを用意してもらった",
              "maleVariant": "",
              "stringId": "1680520748234989568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よりによって、なんであんたを寄こしたの？",
              "maleVariant": "よりによって、なんであんたを寄こしたんだ？",
              "stringId": "1680527260915359744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よりによって、なんであんたが？",
              "maleVariant": "",
              "stringId": "1680527578273177600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪い報せがあってな。せめて顔見知りの口から伝えてやろうという配慮だよ",
              "maleVariant": "",
              "stringId": "1680529463747043328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悲しいかな、この辺りにいる君の知人は私だけだったということだ",
              "maleVariant": "",
              "stringId": "1680530578274279424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残念だが、決していい報せではない",
              "maleVariant": "",
              "stringId": "1680531684731674624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だがその結果、体が深刻なダメージを受けてしまったんだ。DNAそのものが損傷している… 放射線被ばくの患者に見られるようなレベルでね",
              "maleVariant": "",
              "stringId": "1680532169777766400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よく聞いてくれ…",
              "maleVariant": "",
              "stringId": "1680557721662283776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残念だが、手の施しようがない",
              "maleVariant": "",
              "stringId": "1680558369749360640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは死ぬのね",
              "maleVariant": "俺は死ぬのか",
              "stringId": "1680558764685025284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなの信じられるわけがない",
              "maleVariant": "",
              "stringId": "1680561833170722816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ",
              "maleVariant": "",
              "stringId": "1680562012116508672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまりあたしは死ぬってこと？",
              "maleVariant": "つまり俺は死ぬってことか？",
              "stringId": "1680562450521939968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウソよ。そんなわけない",
              "maleVariant": "ウソだ。そんなわけない",
              "stringId": "1680562590091599872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ… クソ！",
              "maleVariant": "",
              "stringId": "1680562800159121408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "手術は成功し、生体チップは無事摘出された",
              "maleVariant": "",
              "stringId": "1680571882219790336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "検査結果に疑う余地はなかった",
              "maleVariant": "",
              "stringId": "1680574338890752000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それでも絶対じゃない",
              "maleVariant": "",
              "stringId": "1680575583290417152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気持ちはわかるが、別の道に希望を託すべきだ",
              "maleVariant": "",
              "stringId": "1680577772566110208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気持ちはわかる。だが我々も諦めたわけではない",
              "maleVariant": "",
              "stringId": "1680579236445315072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういうことよ",
              "maleVariant": "どういうことだ",
              "stringId": "1680579692231942144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしに残された時間は？",
              "maleVariant": "俺に残された時間は？",
              "stringId": "1680580143404834816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハナコ・アラサカと話したい",
              "maleVariant": "",
              "stringId": "1680580334178557952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしに残された時間は？",
              "maleVariant": "俺に残された時間は？",
              "stringId": "1680580627259744256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハナコ・アラサカと話をさせて",
              "maleVariant": "ハナコと話をさせろ",
              "stringId": "1680580859775180800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "選択肢？ 何があるっていうの？ また手術でもするわけ？",
              "maleVariant": "選択肢？ 何があるっていうんだ？ また手術でもするのか？",
              "stringId": "1680581072258621440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "およそ6ヶ月といったところだ。それ以降は悪化していく。それも急激に",
              "maleVariant": "",
              "stringId": "1680593767544414208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死の直前には… 大きな苦しみが待っているだろう",
              "maleVariant": "",
              "stringId": "1680593916677087232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残念だがそれはできない",
              "maleVariant": "",
              "stringId": "1680596951826456576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "取引したのよ",
              "maleVariant": "取引したんだ",
              "stringId": "1680597106512388096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "義務は果たされた。君や、君が抱える問題は彼女にとって、もはや些事でしかない",
              "maleVariant": "",
              "stringId": "1680597241099214848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの命を救うと約束したのに",
              "maleVariant": "俺の命を救うと約束したんだぞ",
              "stringId": "1680599349827493888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから私がここにいるんだ。彼女の命令でね",
              "maleVariant": "",
              "stringId": "1680599667202088960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカ社には死神を欺くノウハウがある。君は我々を信じてくれさえすればいい",
              "maleVariant": "",
              "stringId": "1680602697687392256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたたちを信じたからこそ、今こんなことになってるんだけど",
              "maleVariant": "あんたらを信じたからこそ、今こんなことになってるんだが",
              "stringId": "1680665358676549632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "我々の試験プログラム、“魂の救済”に加わるんだ",
              "maleVariant": "",
              "stringId": "1680667781675638784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "適合の問題はまだ解決できていないんだ",
              "maleVariant": "",
              "stringId": "1680671550895316992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカの魂の監獄には入らない",
              "maleVariant": "",
              "stringId": "1680679322756079616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "罠じゃないという保証は？",
              "maleVariant": "",
              "stringId": "1680679500024143872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これ以上、あんたたちの罠に引っ掛かるつもりはない",
              "maleVariant": "これ以上、あんたらの罠に引っ掛かるつもりはない",
              "stringId": "1680680165492416512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ならあんたの個人的復讐のためね。生涯を捧げてサブロウを神にしてやったのに、目の前にいるあんたは、今もただの人間でしかない",
              "maleVariant": "ならあんたの個人的復讐のためか。生涯を捧げてサブロウを神にしてやったのに、目の前にいるあんたは、今もただの人間でしかない",
              "stringId": "1680680642636439552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私の雇い主は君に選択肢を与えた。その話に戻ろう",
              "maleVariant": "",
              "stringId": "1680681333790629888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "同意したら、あたしは魂の監獄に入れられて、アラサカの言いなりになるんでしょ？ そんなの呑めるわけがない",
              "maleVariant": "同意したら、俺は魂の監獄に入れられてアラサカの言いなりになるんだろ？ そんなの呑めるわけがない",
              "stringId": "1680682277307703296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "復讐を求めるのは君も同じだろう。相棒である君を、私が苦しめる必要はあるまい？",
              "maleVariant": "",
              "stringId": "1680682789432221696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "細かい条件はすべて契約書に記載されている。君はサインするだけでいい。生きることを望むなら、ではあるが",
              "maleVariant": "",
              "stringId": "1680686627824361472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悲しいかな、あたしはもう地獄に落ちてたってことね",
              "maleVariant": "悲しいかな、俺はもう地獄に落ちてたってことか",
              "stringId": "1681156544390520832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハジメです",
              "maleVariant": "",
              "stringId": "1682654691327500288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハジメ、ひとつ聞かせて。死にたくない一心で、宿敵の監獄にまで来た人間に何て声をかける？",
              "maleVariant": "ハジメ、ひとつ聞かせてくれ。死にたくない一心で、宿敵の監獄にまで来た人間に、何て声をかける？",
              "stringId": "1682654873494511616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "えーっと、そうですね。「帰り道だけは忘れるな」、でしょうか",
              "maleVariant": "",
              "stringId": "1682660766273527808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サブロウ・アラサカが蘇った",
              "maleVariant": "",
              "stringId": "1683580861397098496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サブロウ・アラサカが蘇った。ヨリノブの体を使ってね",
              "maleVariant": "サブロウ・アラサカが蘇った。ヨリノブの体を使ってな",
              "stringId": "1683592745470943232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奇妙な時代が来たものだ",
              "maleVariant": "",
              "stringId": "1683597848311521280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "驚かないの？ ずいぶん冷静ね",
              "maleVariant": "驚かないのか？ ずいぶん冷静だな",
              "stringId": "1683598185181241344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私は、サブロウ・アラサカに永遠の命を与えることに半生を捧げた。彼の復活は… いわば必然だ。もはや驚くはずもない",
              "maleVariant": "",
              "stringId": "1683598328106344448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうにも理解できない",
              "maleVariant": "",
              "stringId": "1683599745445552132"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サブロウ・アラサカに生きる価値なんてない",
              "maleVariant": "",
              "stringId": "1683600081426079744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブが気の毒ね",
              "maleVariant": "ヨリノブが気の毒だな",
              "stringId": "1683600290319196160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それだけじゃない。サブロウは自分の息子の体を奪ったのよ？",
              "maleVariant": "それだけじゃない。サブロウは自分の息子の体を奪ったんだぞ？",
              "stringId": "1683601778156265472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サブロウ・アラサカに生きる価値なんてない。地獄で腐り果てるべきよ",
              "maleVariant": "サブロウ・アラサカに生きる価値なんてない。地獄で腐り果てるべきだ",
              "stringId": "1683602090095042560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブは気の毒ね。あれほど憎んだ相手に返り討ちにあうどころか、体を乗っ取られるなんて",
              "maleVariant": "ヨリノブは気の毒だな。あれほど憎んだ相手に返り討ちにあうどころか、体を乗っ取られるなんて",
              "stringId": "1683602331670175744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サブロウは神の領域に挑戦した… いや、達した最初の人間だ。善悪などという価値観を超越している",
              "maleVariant": "",
              "stringId": "1683604917777358848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん。犬が吠えても、キャラバンは進むものさ",
              "maleVariant": "",
              "stringId": "1683605859046285312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、かわいそうなお方ではあった",
              "maleVariant": "",
              "stringId": "1683608278773817344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "嫌ってたんでしょ",
              "maleVariant": "嫌ってたんだろ",
              "stringId": "1683608485133574144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "故人を悪く言うのは、私の主義に反する",
              "maleVariant": "",
              "stringId": "1683608618327891968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] じゃあね",
              "maleVariant": "[立ち上がる] じゃあな",
              "stringId": "1715818197974069248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[契約にサインする] あたしは生きたい",
              "maleVariant": "[契約にサインする] 俺は生きたい",
              "stringId": "1720143812227313664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[サインを拒否する] 地球に帰る",
              "maleVariant": "",
              "stringId": "1720143812227313680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "即答できないのはよくわかる。よく考えてから決めるといい",
              "maleVariant": "",
              "stringId": "1721002059368837120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "選択肢はまだ残されている",
              "maleVariant": "",
              "stringId": "1721037999588061184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[パズルを手に取る]",
              "maleVariant": "",
              "stringId": "1721559632979505152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "抜け穴や不自然な条項など一切ない。何も心配しなくていい",
              "maleVariant": "",
              "stringId": "1722569427203207168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "賢明な判断だ",
              "maleVariant": "",
              "stringId": "1722725833151688704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[アラサカに魂を売る]",
              "maleVariant": "",
              "stringId": "1730085601029730304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "違う。オルト・カニンガムが最初よ",
              "maleVariant": "違う。オルト・カニンガムが最初だ",
              "stringId": "1730422819343851520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "面白い見解だ。確かに多くの点で言い得ている",
              "maleVariant": "",
              "stringId": "1730424218379116544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残念だがその体はもう使えない。捨ててもらうことになる",
              "maleVariant": "",
              "stringId": "1730449312849616896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうと決まったら、今すぐ取り掛かるとしよう",
              "maleVariant": "",
              "stringId": "1730457598378401792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃあ… また未来で会えたら",
              "maleVariant": "",
              "stringId": "1730458234016784384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あとはスタッフに任せておけばいい",
              "maleVariant": "",
              "stringId": "1730458434554847232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしは… 生きたい",
              "maleVariant": "俺は… 生きたい",
              "stringId": "1730459257846390784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、決めた",
              "maleVariant": "",
              "stringId": "1730463623076667392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "地球に… 家に帰る",
              "maleVariant": "",
              "stringId": "1730465133344874496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死ぬだけだぞ",
              "maleVariant": "",
              "stringId": "1730465375758868480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さよなら、ヘルマン。あんたが持ち掛けた話はクソ以下だったけど、おかげで得られるものがあった",
              "maleVariant": "じゃあな、ヘルマン。あんたが持ち掛けた話はクソ以下だったが、おかげで得られるものがあった",
              "stringId": "1730465824029302784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死からは逃れられない、ずっとそう思ってた。実際にそうだったし。でも今回は違う。これは自分の意志で受け入れる死よ",
              "maleVariant": "死からは逃れられない、ずっとそう思っていた。実際にそうだったしな。だが今回は違う。これは自分の意志で受け入れる死だ",
              "stringId": "1730466095182667776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いったい何のことだ？",
              "maleVariant": "",
              "stringId": "1730470807835533312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "実は、君もそれに関係している",
              "maleVariant": "",
              "stringId": "1730478544665468928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "罠とは？",
              "maleVariant": "",
              "stringId": "1732729487985500160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "君の意識から記憶痕跡を作り、それを“神輿”に保管する",
              "maleVariant": "",
              "stringId": "1732965978498162688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "保管？ 新しい体に移してくれないの？",
              "maleVariant": "保管？ 新しい体に移してくれないのか？",
              "stringId": "1732966308673773568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サブロウ・アラサカは何の問題もなくやってたじゃない",
              "maleVariant": "サブロウ・アラサカは、何の問題もなくやってただろ",
              "stringId": "1732968765126631424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "君には頼りになる“家族”もいなければ、自分専用の医療施設もないだろう",
              "maleVariant": "",
              "stringId": "1732968880285442048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "酷い場所だ。虫唾が走る。人を狂気に追いやりかねない",
              "maleVariant": "",
              "stringId": "1735919949344956416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカ家は、もはや神となった。彼らにとって、君は路傍の石に過ぎない",
              "maleVariant": "",
              "stringId": "1736037829772337152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "目障りなコソ泥に罰を与えたいんでしょ？ それか、サブロウのコレクションに新しい魂を加えたいとかね",
              "maleVariant": "目障りなコソ泥に罰を与えたいんだろ？ それか、サブロウのコレクションに新しい魂を加えたいとかな",
              "stringId": "1736039672296529920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。そうだな",
              "maleVariant": "",
              "stringId": "1736051116790538240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[体を起こす]",
              "maleVariant": "",
              "stringId": "1741995088799293444"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[文書を手に取る]",
              "maleVariant": "",
              "stringId": "1744472205147463684"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "デジタル化された精神は法的に個人とみなされない。そのためこの契約では、必然的に君の諸権利を放棄することになる。ああ、もちろん一時的な措置だ",
              "maleVariant": "",
              "stringId": "1744473385072287784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いやよ。アラサカ社の所有物になる気はない",
              "maleVariant": "いやだね。アラサカ社の所有物になる気はない",
              "stringId": "1744473385072287800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "現段階のプログラム参加者は皆、知名度と影響力を持った者ばかりだ。彼らからは何の不満も出ていない",
              "maleVariant": "",
              "stringId": "1744473385089064960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "断ると言ったら？",
              "maleVariant": "",
              "stringId": "1744473385089064976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "君の荷物はまとめてある。シャトルで地球まで帰れるが、冬が来る頃には死んでいるだろう",
              "maleVariant": "",
              "stringId": "1744473385089064992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[中指を立てる]",
              "maleVariant": "",
              "stringId": "1760502584401276944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[地球に帰る」",
              "maleVariant": "",
              "stringId": "1760519992440598528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "1862341356661989376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈美しきものは消え去らない〉",
              "maleVariant": "",
              "stringId": "1865108574663565312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "〈お前の目、いつも俺を見てる、どこにでも現れるお前はまるでゴースト〉",
              "maleVariant": "",
              "stringId": "1865138045219201024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[荷物を取る]",
              "maleVariant": "",
              "stringId": "2066574925973393412"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "起きてくれないかね",
              "maleVariant": "",
              "stringId": "2221730121913823232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？",
              "maleVariant": "",
              "stringId": "2221730236955193344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "内容を確認してくれ",
              "maleVariant": "",
              "stringId": "2221730355335229440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "心は決まったかね？",
              "maleVariant": "",
              "stringId": "2221730490693808128"
            }
          ]
        }
      }
    },
