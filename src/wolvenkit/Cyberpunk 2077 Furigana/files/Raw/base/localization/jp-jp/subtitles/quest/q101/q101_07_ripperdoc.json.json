        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "早く、中へ！",
                    "maleVariant": null,
                    "stringId": 1419533616736813056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "う、力が…",
                    "maleVariant": null,
                    "stringId": 1419543039408521216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "少し落ち着け",
                    "maleVariant": null,
                    "stringId": 1419543700212727808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "こうなったら後頭骨を切るしかないだろう… それしか手はない",
                    "maleVariant": null,
                    "stringId": 1424364956879601664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "神経性ショック、危機的状況です",
                    "maleVariant": null,
                    "stringId": 1642906261335363584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "血圧が下がってる！",
                    "maleVariant": null,
                    "stringId": 1652051273440612352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "少し… 休む…",
                    "maleVariant": null,
                    "stringId": 1652064332020240384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そいつはお前の血か？",
                    "maleVariant": null,
                    "stringId": 1653054588420935680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミスティ！",
                    "maleVariant": null,
                    "stringId": 1653061175676030976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しかし、リスクが――",
                    "maleVariant": null,
                    "stringId": 1654541687611510784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "少し黙ってろ！",
                    "maleVariant": null,
                    "stringId": 1654542046492938240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "具合はどうだ？",
                    "maleVariant": null,
                    "stringId": 1677773336796160000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前さんほどじゃないが、日に日に良くはなってる",
                    "maleVariant": null,
                    "stringId": 1677775299378110464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ミスティ！",
                    "maleVariant": null,
                    "stringId": 1714273080247373840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほら、家まで送るわ",
                    "maleVariant": null,
                    "stringId": 1714273080683581440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんな簡単に生き返ったりできるわけがない",
                    "maleVariant": null,
                    "stringId": 1714273081119789056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まるでフランケンシュタインね",
                    "maleVariant": "まるでフランケンシュタインだな",
                    "stringId": 1714273081119789076
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "つまりあたしはヴィクター博士の怪物ってわけね",
                    "maleVariant": "つまり俺は… ヴィクター博士の怪物ってことか",
                    "stringId": 1714273081304338432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "死んだ人間が、そんな簡単に生き返れるわけがない",
                    "maleVariant": null,
                    "stringId": 1714273081455333376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それも昨日までの話だ。今や企業の極秘チップさえあれば、死から蘇ることも可能になったのさ",
                    "maleVariant": null,
                    "stringId": 1714273081455333388
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ、あたしの精神はどうなるの？ わざわざ他人に乗っ取られるために、あの世から引き戻されたわけ？",
                    "maleVariant": "じゃあ、俺の精神はどうなる？ わざわざ他人に乗っ取られるために、あの世から引き戻されたって言うのか？",
                    "stringId": 1714273081455333400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "チップを作ったアラサカのエンジニアに聞いてくれ。確実なのは、お前さんの精神が消えてなくなっちまうってことだけだ",
                    "maleVariant": null,
                    "stringId": 1714273081455333412
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "生体チップからしてみれば、お前さんの脳細胞は切除すべき腫瘍で、その体はコンストラクトを入れとくための容れ物なんだ",
                    "maleVariant": null,
                    "stringId": 1714273081505665024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "つまりそのテロリストは、あたしを消し去ってこの体を奪おうとしてるのね",
                    "maleVariant": "つまりそのテロリストは、俺を消し去ってこの体を奪おうとしてるのか",
                    "stringId": 1714273081505665060
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その生体チップは… 導火線に火のついた爆弾みたいなもんだ。もう長くは… 生きられない。持って数週間だろう",
                    "maleVariant": null,
                    "stringId": 1714273081589551104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "シルヴァーハンドのコンストラクトは、意識を上書きしていく。最終的には体を乗っ取られ、お前さんは… 消えちまう",
                    "maleVariant": null,
                    "stringId": 1714273081589551116
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、俺の言ってることがわかるか？",
                    "maleVariant": null,
                    "stringId": 1714273081589551128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "治せるんでしょ？",
                    "maleVariant": "治せるんだろ？",
                    "stringId": 1714273081757323264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなのウソよ",
                    "maleVariant": "そんなのウソだ",
                    "stringId": 1714273081757323280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "治せるんでしょ、ヴィクター",
                    "maleVariant": "治せるんだろ、ヴィクター",
                    "stringId": 1714273081975427072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺だって治してやりたい。だがこいつは… とても俺の手には負えない",
                    "maleVariant": null,
                    "stringId": 1714273081992204296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなふざけた話、信じれるわけがない。だってそうでしょ？",
                    "maleVariant": "そんなふざけた話、信じれるわけがない。だってそうだろ？",
                    "stringId": 1714273082059313152
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いくらなんでも、現実味がなさすぎる！",
                    "maleVariant": null,
                    "stringId": 1714273082076090376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "他人の記憶を追体験したっていうの？ そんなのありえない",
                    "maleVariant": "他人の記憶を追体験したっていうのか？ ありえないだろ",
                    "stringId": 1714273082227085312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何なの？",
                    "maleVariant": "何だ？",
                    "stringId": 1714284557725388800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヴィクがビビってるとこなんて初めて見た",
                    "maleVariant": null,
                    "stringId": 1714285082818695168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "頭の中にテロリストがいることより深刻だって言うの…？",
                    "maleVariant": "頭の中にテロリストがいることより深刻だって言うのか…？",
                    "stringId": 1714285317313843200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何なの？",
                    "maleVariant": "何だ？",
                    "stringId": 1714286572618698752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたがそんな顔するの初めて見たよ。何なの？",
                    "maleVariant": "あんたがそんな顔するの初めて見たな。何なんだ？",
                    "stringId": 1714286663685427200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "頭の中のテロリストより深刻？",
                    "maleVariant": "頭の中のテロリストより深刻か？",
                    "stringId": 1714286764717821952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前さんには… つまり… 時間があまり残されてない",
                    "maleVariant": null,
                    "stringId": 1714296322630668288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どういうことよ？",
                    "maleVariant": "どういうことだ？",
                    "stringId": 1714296467837472768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なぜあたしが死ななきゃいけないの？",
                    "maleVariant": "なぜ俺が死ななきゃいけない？",
                    "stringId": 1714303736650190852
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "全部くわしく説明して",
                    "maleVariant": "全部くわしく説明してくれ",
                    "stringId": 1714304238976815104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "クソッ！ なんでなの？ あんたは名医なんでしょ？",
                    "maleVariant": "クソッ！ なんでだよ？ あんた名医なんだろ？",
                    "stringId": 1714306590706290688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本当に聞きたいのか…？",
                    "maleVariant": null,
                    "stringId": 1714307138935377920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしは死ぬ、死ぬしかない… そう言いたいの？ ちゃんと説明してよ",
                    "maleVariant": "俺は死ぬ、死ぬしかない… そう言いたいのか？ ちゃんと説明してくれ",
                    "stringId": 1714307766772355072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何も端折らず、全部聞かせて",
                    "maleVariant": "何も端折らず、全部聞かせてくれ",
                    "stringId": 1714307874515636224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "チップスロットに差した時は、何も起きなかったんだよな？ お前さんが死ぬまでは",
                    "maleVariant": null,
                    "stringId": 1714308711312838668
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしはデクスター・デショーンに頭を撃たれた… 普通ならそれで死んでおしまいでしょ？",
                    "maleVariant": "俺はデクスター・デショーンに頭を撃たれた… 普通ならそれで死んでおしまいだろ？",
                    "stringId": 1714308711329615880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "幸いデショーンが使ったのは小口径の銃だった。奴さんの間抜けさに感謝するんだな",
                    "maleVariant": null,
                    "stringId": 1714308711329615892
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった… デクスに撃たれたろ？ そん時に、お前さんのチップスロットと脳が損傷した",
                    "maleVariant": null,
                    "stringId": 1714316245490782208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そこでチップが蘇り、お前さんをショートさせ… 脳にデータをアップロードし始めたんだ",
                    "maleVariant": null,
                    "stringId": 1714321524408242176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいだろう。チップにはコンストラクトが保存されてる。ジョニー・シルヴァーハンドの精神だ",
                    "maleVariant": null,
                    "stringId": 1714325676785979392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしはどうしたらいいの？",
                    "maleVariant": "俺はどうしたらいいんだ？",
                    "stringId": 1714329873724002308
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "チップを取り出せば済むことじゃないの？",
                    "maleVariant": "チップを取り出せば済むことじゃないのか？",
                    "stringId": 1714330172861763584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヴィク、あんたは何度もあたしを救ってくれた",
                    "maleVariant": "ヴィク、あんたは何度も俺を救ってくれた",
                    "stringId": 1714331494168186880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "チップを取り出すか、停止させればいいんじゃないの？",
                    "maleVariant": "チップを取り出すか、停止させればいいんじゃないのか？",
                    "stringId": 1714331602062462976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どちらもダメだ。その瞬間に死んじまう",
                    "maleVariant": null,
                    "stringId": 1714367287603548160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺にもわからないんだ",
                    "maleVariant": null,
                    "stringId": 1714369806635421696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V？ 大丈夫か…？",
                    "maleVariant": null,
                    "stringId": 1715423139194261504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うう… 頭が",
                    "maleVariant": null,
                    "stringId": 1715423139194261516
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "気分はどうだ？",
                    "maleVariant": null,
                    "stringId": 1715423139194261528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと待って。じゃあ、あたしの頭の中に本物のテロリストがいるってこと？",
                    "maleVariant": "ちょっと待て。じゃあ、俺の頭の中に本物のテロリストがいるってことか？",
                    "stringId": 1715423139311702016
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ。ジョニー・シルヴァーハンドがな",
                    "maleVariant": null,
                    "stringId": 1715423139311702028
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "活躍してたのは50年ほど前のことだ",
                    "maleVariant": null,
                    "stringId": 1715423139328479252
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アラサカ社爆破事件、知ってるか？ 奴の仕業だ。がれきに埋もれて死んだとされてたが…",
                    "maleVariant": null,
                    "stringId": 1715423139328479264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "良くはない… 耳鳴りがするし、幻覚も見える",
                    "maleVariant": null,
                    "stringId": 1715423139395588096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その… 幻覚では… どんなものが見えるんだ？",
                    "maleVariant": null,
                    "stringId": 1715423139412365332
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まばゆい光、鳴り響く爆音…あたしはステージ上にいて… 息が詰まりそうで、胸に… 憎しみが満ちてる",
                    "maleVariant": "まばゆい光、鳴り響く爆音… 俺はステージ上にいて… 息が詰まりそうで、胸に… 憎しみが満ちてる",
                    "stringId": 1715423139412365344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その気持ちをマイクにぶつけるんだけど、結局そんなことしても意味はないって気付くの",
                    "maleVariant": "その気持ちをマイクにぶつけるんだが、結局そんなことをしても意味はないって気付くんだ",
                    "stringId": 1715423139412365360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それから… 笑わないでよ――アラサカ・タワーに爆弾を仕掛けた",
                    "maleVariant": "それから… 笑うなよ――アラサカ・タワーに爆弾を仕掛けた",
                    "stringId": 1715423139429142536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや、笑えないな…",
                    "maleVariant": null,
                    "stringId": 1715423139429142552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その後、殺された",
                    "maleVariant": null,
                    "stringId": 1715423139479474176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティが… いつもと違った",
                    "maleVariant": null,
                    "stringId": 1715423139496251400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その後、殺されたの",
                    "maleVariant": "その後、殺されたんだ",
                    "stringId": 1715423139764686860
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんな恐怖は感じたことがない",
                    "maleVariant": null,
                    "stringId": 1715423139764686876
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ナイトシティが… いつもと違って見えた",
                    "maleVariant": null,
                    "stringId": 1715423139798241280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "街を憎んでた。人も、何もかもが嫌だった",
                    "maleVariant": null,
                    "stringId": 1715423139815018504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[座る] 何か… おかしな感じがする",
                    "maleVariant": null,
                    "stringId": 1715423139865350144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "明晰夢にしても、リアル過ぎる",
                    "maleVariant": null,
                    "stringId": 1715423140066676736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、そいつは夢じゃない。記憶だ",
                    "maleVariant": null,
                    "stringId": 1715423140066676748
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "チップには、人格コンストラクトが入っていたんだ。お前さんはそいつの記憶を見たんだよ",
                    "maleVariant": null,
                    "stringId": 1715423140083453976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ありえないでしょ",
                    "maleVariant": "ありえないだろ",
                    "stringId": 1715423140133785600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしの頭の中にテロリストがいるってこと？",
                    "maleVariant": "俺の頭の中にテロリストがいるってことか？",
                    "stringId": 1715423140133785620
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたに為す術がないって言われたら… あたしはどうしたらいいの？ 教えてよ",
                    "maleVariant": "あんたに為す術がないって言われたら… 俺はどうしたらいいんだ？ 教えてくれよ",
                    "stringId": 1715455847383486464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヴィク？",
                    "maleVariant": null,
                    "stringId": 1715457323442950144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "テレビで宣伝してるチップね",
                    "maleVariant": "テレビで宣伝してるチップか",
                    "stringId": 1725569191054462976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "このチップ、テレビで宣伝してたヤツでしょ… あのふざけたスローガン、何だっけ？ 魂を…",
                    "maleVariant": "このチップ、テレビで宣伝してたヤツだろ… ふざけたスローガンがあったはずだ。 魂を…",
                    "stringId": 1725570104976527360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それよ！ コンストラクトと会話するだけのツールでしょ",
                    "maleVariant": "それだ！ コンストラクトと会話するだけのツールだろ",
                    "stringId": 1725573453104861184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、だがどうやらこれはプロトタイプだ。アラサカの研究所から流出した代物だろう",
                    "maleVariant": null,
                    "stringId": 1725579290904940544
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前さんたち2人は、理解できない方法で繋がってる",
                    "maleVariant": null,
                    "stringId": 1772317707660189696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "2人って？ あたしと、もうひとりは誰のこと？",
                    "maleVariant": "2人？ 俺と、もうひとりは誰のことだ？",
                    "stringId": 1772320213253513216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジョニー・シルヴァーハンド。俺の時代じゃ有名だったテロリストさ",
                    "maleVariant": null,
                    "stringId": 1772320290009276416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "とにかく今は、それよりもっと深刻な問題がある…",
                    "maleVariant": null,
                    "stringId": 1772321542361341952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "チップはお前さんの脳を、空の容れ物と認識したのさ。保持してた記憶痕跡を流し込むためのな",
                    "maleVariant": null,
                    "stringId": 1772339636152590336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "空の容れ物？ じゃあ、今ここにいるあたしは何なのよ！？",
                    "maleVariant": "空の容れ物？ じゃあ、今ここにいる俺は何なんだ！？",
                    "stringId": 1772341580195717120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "チップは書き込むだけで、読み込みはしない。頭痛がするのは、チップが神経回路を書き換えてるからだ。神経構造を新しいものに作り替えてる",
                    "maleVariant": null,
                    "stringId": 1772341789827031040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "チップにとっちゃ、お前さんの存在は、切除すべき腫瘍に過ぎないんだよ",
                    "maleVariant": null,
                    "stringId": 1772344391520247808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そいつの意志でやってるわけじゃない。自動プログラムだ。誰にも止めることはできない",
                    "maleVariant": null,
                    "stringId": 1772406218664992768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "弾丸を受けた後、チップのナノマシンが傷を修復し、お前さんをこの世に連れ戻してくれたのさ",
                    "maleVariant": null,
                    "stringId": 1772412155165433856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "体はしょせん器に過ぎない。改造することも、クロームと入れ替えることもできる… 全身丸ごとだってな",
                    "maleVariant": null,
                    "stringId": 1772440314195763200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「魂を救済しよう」",
                    "maleVariant": null,
                    "stringId": 1772453879581532160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だが精神となると… 話がまったく変わってくる",
                    "maleVariant": null,
                    "stringId": 1772461340761808896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ヴィクターももう年なんだから、あまり無茶言わないで",
                    "maleVariant": null,
                    "stringId": 1772467106117869568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何が起きてるのかは知らないけど、もっとまともな説明があるはずよ",
                    "maleVariant": "何が起きてるのかは知らないが、もっとまともな説明があるはずだ",
                    "stringId": 1772478113431183360
                  }
                },
                {
