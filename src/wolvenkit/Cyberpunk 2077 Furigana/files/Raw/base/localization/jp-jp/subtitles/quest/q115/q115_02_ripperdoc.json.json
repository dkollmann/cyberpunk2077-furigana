    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1648957578149761024,
            "femaleVariant": "お前の思ってるとこじゃない。今はまだな"
          },
          {
            "stringId": 1648958390989099008,
            "femaleVariant": "落ち着け、V。まだ動くなよ"
          },
          {
            "stringId": 1648958992435515392,
            "femaleVariant": "ヴィク"
          },
          {
            "stringId": 1648961674424487936,
            "femaleVariant": "痛むだろ。意識が朦朧とした状態で来たもんだから、ベータハロペリドールを使ったんだ"
          },
          {
            "stringId": 1682515973600108548,
            "femaleVariant": "どれくらい深刻なの？",
            "maleVariant": "どれくらい深刻なんだ？"
          },
          {
            "stringId": 1683606889656147968,
            "femaleVariant": "包み隠さずはっきり言って",
            "maleVariant": "包み隠さずはっきり言え"
          },
          {
            "stringId": 1683606971277303808,
            "femaleVariant": "次はもっと弱い酒にする"
          },
          {
            "stringId": 1683608974426562560,
            "femaleVariant": "ヴィクター、はっきり言って",
            "maleVariant": "ヴィクター、はっきり言えよ"
          },
          {
            "stringId": 1683609018433200128,
            "femaleVariant": "次はもっと弱い酒にしとく"
          },
          {
            "stringId": 1683619156418289664,
            "femaleVariant": "ゆっくり、体を起こしてみろ"
          },
          {
            "stringId": 1683620842713047040,
            "femaleVariant": "[体を起こそうとする] 少し感覚が戻って来たみたい…",
            "maleVariant": "[体を起こそうとする] 少し感覚が戻って来たみたいだ…"
          },
          {
            "stringId": 1683621626376806400,
            "femaleVariant": "[体を起こそうとする] やってみるけど、期待しないで",
            "maleVariant": "[体を起こそうとする] やってみるけど、期待するなよ"
          },
          {
            "stringId": 1683622106859495424,
            "femaleVariant": "やってみる。少し感覚が戻って来たみたい…",
            "maleVariant": "やってみる。少し感覚が戻って来たようだ…"
          },
          {
            "stringId": 1683622179404177408,
            "femaleVariant": "わかった、でも期待はしないで…",
            "maleVariant": "わかった、でも期待はするなよ…"
          },
          {
            "stringId": 1683625477788065792,
            "femaleVariant": "うう…"
          },
          {
            "stringId": 1683627065986441216,
            "femaleVariant": "ふむ…"
          },
          {
            "stringId": 1683634579192864768,
            "femaleVariant": "いつまでこんなことを続けるつもりだ？"
          },
          {
            "stringId": 1683634695492526084,
            "femaleVariant": "どう？ 体は起こせたけど",
            "maleVariant": "どうだ？ 体は起こせたぞ"
          },
          {
            "stringId": 1683636309930467328,
            "femaleVariant": "何かあるならはっきり言って",
            "maleVariant": "何かあるならはっきり言えよ"
          },
          {
            "stringId": 1683637187160760320,
            "femaleVariant": "[体を起こす] ええ、わかってる",
            "maleVariant": "[体を起こす] ああ、わかってる"
          },
          {
            "stringId": 1683637187160760336,
            "femaleVariant": "[体を起こす] どうしたらいいと思う？"
          },
          {
            "stringId": 1683639974611312640,
            "femaleVariant": "[体を起こす] あたしが遊んでるとでも思ってるの！？",
            "maleVariant": "[体を起こす] 俺が遊んでるとでも思ってるのか！？"
          },
          {
            "stringId": 1683640430129504256,
            "femaleVariant": "ええ、わかってる。早めに何とかするよ",
            "maleVariant": "ああ、わかってる。早めに何とかするよ"
          },
          {
            "stringId": 1683640536379613184,
            "femaleVariant": "わかってる。何かいい案はない？",
            "maleVariant": "わかってる。何かいい案はないか？"
          },
          {
            "stringId": 1683640564498227200,
            "femaleVariant": "言われなくてもわかってる！ あたしが遊んでたとでも思ってるの！？",
            "maleVariant": "言われなくてもわかってる！ 俺が遊んでたとでも思ってるのか！？"
          },
          {
            "stringId": 1683660001037180928,
            "femaleVariant": "V、そこのテーブルを見ろ"
          },
          {
            "stringId": 1683660772218691584,
            "femaleVariant": "黙ってて、ジョニー。あたしが決めることよ",
            "maleVariant": "黙ってろ、ジョニー。俺が決めることだ"
          },
          {
            "stringId": 1683660910530060288,
            "femaleVariant": "いい加減にして。少し考えさせてよ",
            "maleVariant": "いい加減にしろ。少し考えさせてくれ"
          },
          {
            "stringId": 1683726677300666368,
            "femaleVariant": "どう？ 体は起こせたよ",
            "maleVariant": "どうだ？ 体は起こせたぞ"
          },
          {
            "stringId": 1683726782695137280,
            "femaleVariant": "ヴィク、何かあるならはっきり言って",
            "maleVariant": "ヴィク、何かあるならはっきり言えよ"
          },
          {
            "stringId": 1683733734770794496,
            "femaleVariant": "エンドトライジン、ローグ、アラサカ・タワー"
          },
          {
            "stringId": 1683738725908668416,
            "femaleVariant": "よし、これで少しはマシになったろ"
          },
          {
            "stringId": 1689476912391262208,
            "femaleVariant": "どれくらい深刻なの？",
            "maleVariant": "どれくらい深刻なんだ？"
          },
          {
            "stringId": 1689476912408039432,
            "femaleVariant": "そんな悪いのね",
            "maleVariant": "そんなに悪いのか"
          },
          {
            "stringId": 1689494856294862848,
            "femaleVariant": "ゆっくりだ"
          },
          {
            "stringId": 1689497413763338240,
            "femaleVariant": "歴史に残る… 医学の勝利ね…",
            "maleVariant": "歴史に残る…　医学の勝利だな…"
          },
          {
            "stringId": 1689517781974650880,
            "femaleVariant": "冗談言ってる場合か"
          },
          {
            "stringId": 1689553667516928000,
            "femaleVariant": "見たところ、長くは持たないぞ"
          },
          {
            "stringId": 1689556979591024640,
            "femaleVariant": "次に発作が起きたら――どっかの路地裏で、野垂れ死ぬのがオチだ"
          },
          {
            "stringId": 1689561404296531968,
            "femaleVariant": "自分の手で解決したいなら、これが最後のチャンスだ。わかったな？"
          },
          {
            "stringId": 1689565193850195968,
            "femaleVariant": "こっちが聞きたい"
          },
          {
            "stringId": 1689574067739168768,
            "femaleVariant": "毎度ぶっ倒れてるくせに、よく言う"
          },
          {
            "stringId": 1689585438748971008,
            "femaleVariant": "でもその様子だと、ずいぶん悪そうね",
            "maleVariant": "でもその様子だと、随分悪そうだな"
          },
          {
            "stringId": 1689632916860964864,
            "femaleVariant": "いや、今すぐにだ"
          },
          {
            "stringId": 1689636587514499072,
            "femaleVariant": "おいおい、何回同じ事を言わせる気だ？"
          },
          {
            "stringId": 1689637429344866304,
            "femaleVariant": "すべてはお前さん次第なんだよ。俺にも、頭ん中のそいつにも決められない"
          },
          {
            "stringId": 1689680205625298944,
            "femaleVariant": "あれがどうかしたの？",
            "maleVariant": "あれがどうかしたのか？"
          },
          {
            "stringId": 1689680788465782784,
            "femaleVariant": "1回分の疑似エンドトライジン。ミスティからのプレゼントだ"
          },
          {
            "stringId": 1689684056415801344,
            "femaleVariant": "頭ん中の声に屈したいなら好きにしろ。薬で自分を眠らせ、体を明け渡すんだ"
          },
          {
            "stringId": 1689685496739794944,
            "femaleVariant": "…そうでなければ、自分のやり方で片をつけるんだな"
          },
          {
            "stringId": 1689689061612982272,
            "femaleVariant": "あのテーブルまで自力で歩け… 選ぶのは、誰でもない。お前さんだ"
          },
          {
            "stringId": 1689713830303502336,
            "femaleVariant": "取るべき道はそれしかねえ"
          },
          {
            "stringId": 1690664218099847168,
            "femaleVariant": "[楽な体勢を取る] あたしはどうやってここに？",
            "maleVariant": "[楽な体勢を取る] 俺はどうやってここに？"
          },
          {
            "stringId": 1690664218166956032,
            "femaleVariant": "あたしは… どうやってここに？",
            "maleVariant": "俺は… どうやってここに？"
          },
          {
            "stringId": 1690664218183733252,
            "femaleVariant": "よろめきながら自力で来たんだよ。ミスティが腰を抜かすとこだった"
          },
          {
            "stringId": 1690664218183733268,
            "femaleVariant": "それから診察台にいた患者を押しのけてだな…"
          },
          {
            "stringId": 1690664218183733284,
            "femaleVariant": "今すぐ自分を診るよう要求したってとこだ"
          },
          {
            "stringId": 1690664218250842128,
            "femaleVariant": "それは… あたしじゃないっていうか…",
            "maleVariant": "それは… 俺じゃないっていうか…"
          },
          {
            "stringId": 1690664218250842140,
            "femaleVariant": "わかってる。そこが心配なんだ…"
          },
          {
            "stringId": 1690678024892977152,
            "femaleVariant": "聞いたろ。最後のチャンスだ。ハンドルを俺に譲れ"
          },
          {
            "stringId": 1690688205827739648,
            "femaleVariant": "俺のおかげだってちゃんと言ってやれよ"
          },
          {
            "stringId": 1690702447284981760,
            "femaleVariant": "こんな状態じゃ、もう次はないぞ"
          },
          {
            "stringId": 1690703160031449088,
            "femaleVariant": "運命の分かれ目に来たってことだ"
          },
          {
            "stringId": 1690723658685616128,
            "femaleVariant": "俺からのプレゼントとして、遮断薬も用意した"
          },
          {
            "stringId": 1693957206083379200,
            "femaleVariant": "ええ… わかった…",
            "maleVariant": "ああ… わかった…"
          },
          {
            "stringId": 1693962112580198404,
            "femaleVariant": "[取る]"
          },
          {
            "stringId": 1703720226720190464,
            "femaleVariant": "だが感覚過敏があるってことは、視神経は損傷してないようだな"
          },
          {
            "stringId": 1703721871692664832,
            "femaleVariant": "[楽な体勢を取る] 頭が破裂しそうに痛い…"
          },
          {
            "stringId": 1703722424988471296,
            "femaleVariant": "頭が… うう…"
          },
          {
            "stringId": 1703722838832058368,
            "femaleVariant": "すぐに生体チップの安定化が完了する"
          },
          {
            "stringId": 1703723075608907776,
            "femaleVariant": "まるで…"
          },
          {
            "stringId": 1703723390768910336,
            "femaleVariant": "頭に無数のナイフが刺さってるみたいか？"
          },
          {
            "stringId": 1703840397506830340,
            "femaleVariant": "あたし…",
            "maleVariant": "俺…"
          },
          {
            "stringId": 1704345085306134532,
            "femaleVariant": "[立ち上がる] 考えさせて",
            "maleVariant": "[立ち上がる] 考えさせてくれ"
          },
          {
            "stringId": 1712547418393468928,
            "femaleVariant": "グロマツキーの猛攻が続く… まったく容赦を見せない！"
          },
          {
            "stringId": 1712553750467108864,
            "femaleVariant": "そして強烈な右フック！ ウォルシュをロープに叩きつけた！"
          },
          {
            "stringId": 1712577304319418368,
            "femaleVariant": "立て！ それで終わりか！"
          },
          {
            "stringId": 1712577965928931328,
            "femaleVariant": "グロマツキーのキツい一発！ 勝負はついたか？ そしてウォルシュのキャリアに終止符を打つことになるのか！"
          },
          {
            "stringId": 1742954123806773248,
            "femaleVariant": "ウォルシュ、スタミナは残っているのか！ 最後の力を振り絞れるか！"
          },
          {
            "stringId": 1742955377954000896,
            "femaleVariant": "しかしレフェリーの判断は――試合続行だ！"
          },
          {
            "stringId": 1742969025548128260,
            "femaleVariant": "色々ありがとう、ヴィクター"
          },
          {
            "stringId": 1742969278682763264,
            "femaleVariant": "それで…？ 試合はどっちが勝ったの？",
            "maleVariant": "それで…？ 試合はどっちが勝ったんだ？"
          },
          {
            "stringId": 1742970467918942208,
            "femaleVariant": "それで… どっちが勝ったの？",
            "maleVariant": "それで… どっちが勝った？"
          },
          {
            "stringId": 1742970612890865664,
            "femaleVariant": "ヴィク、あんたには感謝してもしきれない"
          },
          {
            "stringId": 1742972770591838208,
            "femaleVariant": "あぁ、聞こえてたのか"
          },
          {
            "stringId": 1742973177422548992,
            "femaleVariant": "ウォルシュは立ち上がれた？",
            "maleVariant": "ウォルシュは立ち上がれたか？"
          },
          {
            "stringId": 1742973383027331072,
            "femaleVariant": "いいや、何度見てもな"
          },
          {
            "stringId": 1742973505433899008,
            "femaleVariant": "どういうこと？",
            "maleVariant": "どういうことだ？"
          },
          {
            "stringId": 1742973569036324864,
            "femaleVariant": "あれは… 再放送だ。好きな試合でね"
          },
          {
            "stringId": 1742974199054340096,
            "femaleVariant": "結果を知ってるなら、なんであんなに熱くなってたのよ？",
            "maleVariant": "結果を知ってるなら、なんであんなに熱くなってたんだ？"
          },
          {
            "stringId": 1742974449252962304,
            "femaleVariant": "さあね。ウォルシュがどこでしくじったのか、それをずっと考えてるのかもな"
          },
          {
            "stringId": 1742975753815724032,
            "femaleVariant": "結論は？"
          },
          {
            "stringId": 1742978040600596480,
            "femaleVariant": "勝ち目の薄い戦いに挑んだのが間違いだった…"
          },
          {
            "stringId": 1742980802231013376,
            "femaleVariant": "力不足ですまない"
          },
          {
            "stringId": 1742981963197583360,
            "femaleVariant": "さあ行け。もう店じまいだ"
          },
          {
            "stringId": 1742982852725575680,
            "femaleVariant": "薬はやる。やるべきことをやれ、V"
          },
          {
            "stringId": 1742983494504419328,
            "femaleVariant": "ありがとう。あんたも頑張ってね",
            "maleVariant": "ありがとう。あんたも頑張れよ"
          },
          {
            "stringId": 1743041574793515008,
            "femaleVariant": "だがそれじゃ、余りにもやり切れない"
          },
          {
            "stringId": 1743042172079181824,
            "femaleVariant": "幸運を祈る"
          },
          {
            "stringId": 1744825070818877440,
            "femaleVariant": "…は…"
          },
          {
            "stringId": 1744825127156768768,
            "femaleVariant": "…死んだの？",
            "maleVariant": "…死んだのか？"
          },
          {
            "stringId": 1751739992458006528,
            "femaleVariant": "おい、V。声に出てるぞ！"
          },
          {
            "stringId": 1751741005768298496,
            "femaleVariant": "声？ 何のこと？",
            "maleVariant": "声？ 何のことだ？"
          },
          {
            "stringId": 1751767548062208000,
            "femaleVariant": "[立ち上がる] 黙ってて、あたしが決めることよ",
            "maleVariant": "[立ち上がる] 黙ってろ、俺が決めることだ"
          },
          {
            "stringId": 1751781623743778816,
            "femaleVariant": "手遅れになる前に決断しろよ！"
          },
          {
            "stringId": 1751799806622044160,
            "femaleVariant": "少し考えさせて",
            "maleVariant": "少し考えたい"
          },
          {
            "stringId": 1751830949765451776,
            "femaleVariant": "え… ここは…？"
          },
          {
            "stringId": 1754883812151189504,
            "femaleVariant": "あたしはどうやってここに？",
            "maleVariant": "俺はどうやってここに？"
          },
          {
            "stringId": 1754888388103299072,
            "femaleVariant": "頭が破裂しそうに痛い…"
          },
          {
            "stringId": 1760509687722020864,
            "femaleVariant": "何よ",
            "maleVariant": "何だよ"
          },
          {
            "stringId": 1760509687738798108,
            "femaleVariant": "お前さんのやり方で片をつけるのに、役に立つものを用意した"
          },
          {
            "stringId": 1760509687738798132,
            "femaleVariant": "…あそこまでたどり着くことができたら、そっから先はお前さん次第だ"
          },
          {
            "stringId": 1760535757301092352,
            "femaleVariant": "「自分のやり方」ね"
          },
          {
            "stringId": 1760535757301092376,
            "femaleVariant": "上手いこと言いやがるじゃねえか"
          },
          {
            "stringId": 1760545801736425472,
            "femaleVariant": "脳みそをストレッチしたいってんなら、せめて外でやったらどうだ"
          },
          {
            "stringId": 1760546718493839360,
            "femaleVariant": "脳みそをストレッチしたいってんなら、せめて外でやったらどうだ"
          },
          {
            "stringId": 1760558559399907328,
            "femaleVariant": "どっちを選ぶにしても、お前が歩けなきゃ始まらねえ"
          }
        ]
      }
    }
  }
}