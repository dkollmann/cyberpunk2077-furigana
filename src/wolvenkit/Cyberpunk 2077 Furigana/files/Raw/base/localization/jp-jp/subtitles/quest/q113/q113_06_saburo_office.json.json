    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1753439384113266688,
            "femaleVariant": "忠誠心のある、全ての兵を招集するの"
          },
          {
            "stringId": 1753439567807004672,
            "femaleVariant": "準備の上、待機します"
          },
          {
            "stringId": 1821289547171762176,
            "femaleVariant": "防諜にいたことがあるけど、こんな場所、聞いたことがない",
            "maleVariant": "防諜にいたことがあるが、こんな場所、聞いたことがない"
          },
          {
            "stringId": 1821321276662091776,
            "femaleVariant": "もちろん、そうでしょうね"
          },
          {
            "stringId": 1821323272496484352,
            "femaleVariant": "ここは我がアラサカ社の中でも、最高機密に分類される場所なの"
          },
          {
            "stringId": 1821324270522732544,
            "femaleVariant": "父のオフィス。東京本社にあるオフィスの精密なコピーよ"
          },
          {
            "stringId": 1821324549947265024,
            "femaleVariant": "父は世界中の支社に、この部屋を作らせたの"
          },
          {
            "stringId": 1821327312986968064,
            "femaleVariant": "小物を含め隅々まで再現されている。目を閉じていても仕事ができるくらいにね"
          },
          {
            "stringId": 1821329848645062656,
            "femaleVariant": "父はそういう、細部にこだわる人なのよ"
          },
          {
            "stringId": 1821433581064294400,
            "femaleVariant": "どこへ行くの？",
            "maleVariant": "どこへ行くんだ？"
          },
          {
            "stringId": 1821449271318024192,
            "femaleVariant": "こっちよ"
          },
          {
            "stringId": 1822868108110336000,
            "femaleVariant": "“だった”"
          },
          {
            "stringId": 1822876060477165568,
            "femaleVariant": "今なんて？"
          },
          {
            "stringId": 1822876116865388544,
            "femaleVariant": "サブロウはもういない"
          },
          {
            "stringId": 1822885497711497216,
            "femaleVariant": "タケムラ、戦力を集めて"
          },
          {
            "stringId": 1841690843489898496,
            "femaleVariant": "で、見込みはあるの…？",
            "maleVariant": "で、見込みはあるのか…？"
          },
          {
            "stringId": 1842859818361950208,
            "femaleVariant": "V、あなたは私と一緒に"
          },
          {
            "stringId": 1842861158895063040,
            "femaleVariant": "象徴は、そう簡単に消え去らないものよ"
          },
          {
            "stringId": 1842997280702861312,
            "femaleVariant": "あなたが思っている以上にね"
          },
          {
            "stringId": 1844343836416806912,
            "femaleVariant": "問題なのは、各派閥が今もアラサカの理念に忠実かどうか"
          },
          {
            "stringId": 1844346672991047680,
            "femaleVariant": "そして、過ちを認める勇気があるかどうか"
          },
          {
            "stringId": 1844347581762506756,
            "femaleVariant": "あたしの証言に耳を貸すとは思えない",
            "maleVariant": "俺の証言に耳を貸すとは思えない"
          },
          {
            "stringId": 1844347722036809728,
            "femaleVariant": "見込みはあるの…？",
            "maleVariant": "見込みはあるのか…？"
          },
          {
            "stringId": 1844350650868961280,
            "femaleVariant": "正直言って、望み薄ね",
            "maleVariant": "正直言って、望み薄だな"
          },
          {
            "stringId": 1844350926099189760,
            "femaleVariant": "そうする他にない。アラサカの未来が懸かってるんだから",
            "maleVariant": "そうする他にない。アラサカの未来が懸かってるからな"
          },
          {
            "stringId": 1844351183243579392,
            "femaleVariant": "そうする他ないでしょ。アラサカ社の未来が懸かってるんだから",
            "maleVariant": "そうする他ないだろ。アラサカ社の未来が懸かってるんだから"
          },
          {
            "stringId": 1844373897060683776,
            "femaleVariant": "ヨリノブが亀裂を生み、内部は分裂状態に陥ってる。ゴロウはそう言ってた"
          },
          {
            "stringId": 1844376056338714624,
            "femaleVariant": "亀裂… そのとおりね"
          },
          {
            "stringId": 1844376513484296192,
            "femaleVariant": "誰もそれを塞ごうとはしなかった"
          },
          {
            "stringId": 1844378741246922752,
            "femaleVariant": "あんたのせいじゃない"
          },
          {
            "stringId": 1844379212602806272,
            "femaleVariant": "残念ながら、そうは思わない人もいるわ"
          },
          {
            "stringId": 1844386383554023424,
            "femaleVariant": "あたしの証言なんて屁とも思わないだろうし、あんたの影響力もたかが知れてる",
            "maleVariant": "俺の証言なんて屁とも思わないだろうし、あんたの影響力もたかが知れてる"
          },
          {
            "stringId": 1844387415688351744,
            "femaleVariant": "じきにわかるわ"
          },
          {
            "stringId": 1844402972579991556,
            "femaleVariant": "クソ…"
          },
          {
            "stringId": 1844402978619789316,
            "femaleVariant": "ゆっくりとだ…"
          },
          {
            "stringId": 1844402981555802112,
            "femaleVariant": "ありがとう"
          },
          {
            "stringId": 1844402984424706048,
            "femaleVariant": "踏ん張れ。あともう少しだ"
          },
          {
            "stringId": 1844402990481281028,
            "femaleVariant": "彼女は大丈夫？",
            "maleVariant": "彼は大丈夫？"
          },
          {
            "stringId": 1844402993903833088,
            "femaleVariant": "おそらくは"
          },
          {
            "stringId": 1844402996923731968,
            "femaleVariant": "平気よ",
            "maleVariant": "平気だ"
          },
          {
            "stringId": 1863349118742372352,
            "femaleVariant": "これってもしかして…"
          },
          {
            "stringId": 1895369521335713792,
            "femaleVariant": "[立ち上がる]"
          },
          {
            "stringId": 1913787885628243972,
            "femaleVariant": "ほら、掴め"
          },
          {
            "stringId": 1913787886030897152,
            "femaleVariant": "おい、どうした"
          },
          {
            "stringId": 1913787886098006020,
            "femaleVariant": "時間があまりないのよ、V"
          },
          {
            "stringId": 1917011090325192708,
            "femaleVariant": "[降りる]"
          },
          {
            "stringId": 1954422090967891972,
            "femaleVariant": "うう… ぐっ…"
          },
          {
            "stringId": 2008472534385500160,
            "femaleVariant": "これが最後の機会かもしれない"
          },
          {
            "stringId": 2008472613892726784,
            "femaleVariant": "何の？"
          },
          {
            "stringId": 2008472678736666624,
            "femaleVariant": "作戦を聞く機会よ",
            "maleVariant": "作戦を聞く機会だ"
          },
          {
            "stringId": 2008472798274330624,
            "femaleVariant": "もし、ないと言ったら？"
          },
          {
            "stringId": 2008472950863110144,
            "femaleVariant": "ハナコ、はぐらかさないで",
            "maleVariant": "ハナコ、はぐらかすな"
          },
          {
            "stringId": 2008473060435107840,
            "femaleVariant": "こう考えて… パレードの日のあなたとタケムラには、それなりの“作戦”があった"
          },
          {
            "stringId": 2008473505316544512,
            "femaleVariant": "でもそれは一瞬で覆り、計画は水の泡"
          },
          {
            "stringId": 2008474449538260992,
            "femaleVariant": "それでも私たちは今ここにいる。なぜかわかる？"
          },
          {
            "stringId": 2008482695556702208,
            "femaleVariant": "時に人は、真実をまっすぐ見据える必要がある"
          },
          {
            "stringId": 2008483086230953984,
            "femaleVariant": "あの時、私はそうしたの。あなたのおかげよ"
          },
          {
            "stringId": 2008484156399230976,
            "femaleVariant": "あなたは… とても誠実な目をしていた"
          },
          {
            "stringId": 2008484371785129984,
            "femaleVariant": "じゃあ役員たちも、この誠実な目で見つめてやればいい？",
            "maleVariant": "じゃあ役員たちも、この誠実な目で見つめてやればいいと？"
          },
          {
            "stringId": 2008488133924823040,
            "femaleVariant": "武器を携えてね"
          },
          {
            "stringId": 2008488681113722880,
            "femaleVariant": "あたしの目で魔法にかからなかったら、もっと確実な方法を取る… そういうことね",
            "maleVariant": "俺の目で魔法にかからなかったら、もっと確実な方法を取る… そういうことだな"
          },
          {
            "stringId": 2008503592434868224,
            "femaleVariant": "いいえ。連中も予想だにしない展開が待ってるわ"
          },
          {
            "stringId": 2013916409912696832,
            "femaleVariant": "来て"
          },
          {
            "stringId": 2025833805022052352,
            "femaleVariant": "[手を掴む]"
          },
          {
            "stringId": 2042827347669893120,
            "femaleVariant": "[立ち上がる]"
          },
          {
            "stringId": 2239262414837489664,
            "femaleVariant": "V？ 遅れるわけにはいかないのよ"
          },
          {
            "stringId": 2239262732799287300,
            "femaleVariant": "何をしてる？ 早く降りたまえ"
          },
          {
            "stringId": 2239263043983089664,
            "femaleVariant": "V、ここまで来たんだ。もう後戻りはできないぞ。来い"
          },
          {
            "stringId": 2239272875649769480,
            "femaleVariant": "グズグズしてる時間はないわ"
          },
          {
            "stringId": 2239273052565512200,
            "femaleVariant": "どうした？ まさかエレベーターが怖いのかね？"
          }
        ]
      }
    }
  }
}