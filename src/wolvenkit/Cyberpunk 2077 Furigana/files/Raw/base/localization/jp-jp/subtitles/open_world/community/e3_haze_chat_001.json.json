          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もうどれが自分の感情で、どれが他人のものかわからなくなって… へへっ、そしたら銃声がたくさん聞こえたんだ",
                    "maleVariant": null,
                    "stringId": 1202335763952881664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "メキシコでな… コーポのクズどもにいろんな薬をぶち込まれて… 他にもいっぱい俺のような兵士がいて、みーんなリンクされたのさ",
                    "maleVariant": null,
                    "stringId": 1202375424788189184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかるか？ つまり俺は生ける死体になったのさ！ 死を超越した死体だ！",
                    "maleVariant": null,
                    "stringId": 1202384527786823680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "死体がどこに帰りゃいいってんだ… でも俺は兵士だからな！ これは名誉除隊ってやつに違いねえ！",
                    "maleVariant": null,
                    "stringId": 1202387735775072256
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "とにかく、俺はもう大丈夫のはずだ… あの訓練は… いやいやいやいや、あんなの訓練じゃねえ… 訓練であってたまるか！",
                    "maleVariant": null,
                    "stringId": 1202391425571409920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "他の奴らの感情、痛みが俺の中に入ってくるんだ… 10回も、100回も… とても耐えられなかった",
                    "maleVariant": null,
                    "stringId": 1264722296966520832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "奴らは死んだ… そして俺も奴らと一緒に死んだんだ… 奴らの頭の中にいる俺は死んだ",
                    "maleVariant": null,
                    "stringId": 1264723518750498816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もうわけがわからなくなった… そしたら連中が急に、もう帰ってもいいぞって言うんだよ。「解散！」ってな",
                    "maleVariant": null,
                    "stringId": 1264723906622955520
                  }
                }
              ]
            }
          }
        }
      }
    },
