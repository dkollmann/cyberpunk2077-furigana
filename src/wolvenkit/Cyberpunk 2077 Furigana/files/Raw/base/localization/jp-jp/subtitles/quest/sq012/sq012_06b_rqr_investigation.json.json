  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ひどい有様ね… 何としてでも手がかりを見つけられたくないってことか",
              "maleVariant": "ひどい有様だな… 何としてでも手がかりを見つけられたくないようだ",
              "stringId": "1750217206870429696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何を探せばいいの？",
              "maleVariant": "何を探せばいいんだ？",
              "stringId": "1750222544558477312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだな… 事務所… というかその残骸を探すんだ",
              "maleVariant": "",
              "stringId": "1750245085117272064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最近亡くなった、ルシアス・ライン元市長は――死んだ日ここにいたんでしょ？",
              "maleVariant": "最近亡くなった、ルシアス・ライン元市長は――死んだ日ここにいたんだよな？",
              "stringId": "1750267897013981184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあね。いたかもしれないし、いなかったかもしれない",
              "maleVariant": "",
              "stringId": "1750268381338652672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "事務所ね… 多分",
              "maleVariant": "事務所だ… 多分",
              "stringId": "1750351613727985664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも、お前は今日ここで死ぬかもしれない… なんてね、ヘヘヘ",
              "maleVariant": "",
              "stringId": "1750370804111626240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あれ？ この男、どこかで会ったような…",
              "maleVariant": "",
              "stringId": "1750373986145521664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よせ、今は冗談を言う気分じゃない",
              "maleVariant": "",
              "stringId": "1750374853426925568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう少し見て回る",
              "maleVariant": "",
              "stringId": "1750376491638181892"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[クラブを出る] わかった。行きましょ",
              "maleVariant": "[クラブを出る] わかった。行こう",
              "stringId": "1750376851358470144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう少し見て回る",
              "maleVariant": "",
              "stringId": "1750377398413152256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アニマルズの仕業じゃない",
              "maleVariant": "",
              "stringId": "1786615442178691072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BDヘッドセットは、内側から焼かれたのよ",
              "maleVariant": "BDヘッドセットは、内側から焼かれたんだ",
              "stringId": "1786616695050850304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああああっ…",
              "maleVariant": "",
              "stringId": "1786617723510968320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V！",
              "maleVariant": "",
              "stringId": "1786619068959805440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインの死因は… 心臓発作なんかじゃない",
              "maleVariant": "",
              "stringId": "1786621327642841088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺も最初からそう言ってるが… まずは――",
              "maleVariant": "",
              "stringId": "1786621831378751488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインはウイルス入りのBDで死んだのよ",
              "maleVariant": "ラインはウイルス入りのBDで死んだんだ",
              "stringId": "1786622050002653184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[接続する]",
              "maleVariant": "",
              "stringId": "1805437241020502020"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "退屈すぎて寝ちまいそうだ",
              "maleVariant": "",
              "stringId": "1856187583873798144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それより一発キメてえよ",
              "maleVariant": "",
              "stringId": "1856242173193342976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンに連絡して、チャビー・バッファローに呼び出す",
              "maleVariant": "",
              "stringId": "1869299955903614976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね",
              "maleVariant": "そうだな",
              "stringId": "1869300585284096004"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰の命令でラインの死を隠蔽したのか――直接、問い詰めてやる",
              "maleVariant": "",
              "stringId": "1869301493384466432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "現地で落ち合いましょ",
              "maleVariant": "現地で落ち合おう",
              "stringId": "1869306435197325312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、行きましょ",
              "maleVariant": "わかった、行こう",
              "stringId": "1869317665194627072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リバー、記録を見つけた。見たほうがいい",
              "maleVariant": "",
              "stringId": "1915636498792304640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今そっちに送る。あんたの相棒がラインの死を隠蔽してたのよ",
              "maleVariant": "今そっちに送る。あんたの相棒がラインの死を隠蔽してたんだ",
              "stringId": "1915637243163824128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺の… 相棒？ ハンのことか？",
              "maleVariant": "",
              "stringId": "1915637496348790784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その反応からすると、本当に何も知らなかったようね",
              "maleVariant": "その反応からすると、本当に何も知らなかったようだな",
              "stringId": "1915638010016813056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウソだ… そんな… まさか…",
              "maleVariant": "",
              "stringId": "1915638309708222464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンに連絡して、チャビー・バッファローに呼び出す。すぐに向かうぞ",
              "maleVariant": "",
              "stringId": "1915638431896686592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。すぐに出る",
              "maleVariant": "",
              "stringId": "1915668167465226240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、とっくに冷たくなってる。死後1時間はたってるな",
              "maleVariant": "",
              "stringId": "1932759772839923712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、トラウマカードはない。忘れたわけじゃないのは確かだ",
              "maleVariant": "",
              "stringId": "1932869497342513152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こういうクラブだ。盛り上がってる時に、トラウマ・チームに来てほしくはないだろう…",
              "maleVariant": "",
              "stringId": "1932869720076832768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "たぶん心臓だ。ちょっと張り切りすぎたんだろう",
              "maleVariant": "",
              "stringId": "1932870793097236480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった… ああ、自宅に移動させてから検視官を呼ぶ。報告書には「睡眠中に死亡」って書かせるさ",
              "maleVariant": "",
              "stringId": "1932871398805065728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いま運び出すところだ… ああ、人に見られる心配はない",
              "maleVariant": "",
              "stringId": "1932872165758717952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へへへへ…",
              "maleVariant": "",
              "stringId": "1940136860020596736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "『お熱いのがお好き』のリメイク観たか？",
              "maleVariant": "",
              "stringId": "1940148413902053376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BD版と普通の方、どっちのことだ？",
              "maleVariant": "",
              "stringId": "1940148677841215488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "普通の方だよ",
              "maleVariant": "",
              "stringId": "1940148833332453376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全然よくなかった。やっぱカーティスとレモンじゃねえと",
              "maleVariant": "",
              "stringId": "1940149091701579776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「完璧な人なんていない」、そうだろ",
              "maleVariant": "",
              "stringId": "1940149296115179520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ… わかった。また連絡する",
              "maleVariant": "",
              "stringId": "1940191378422849536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コンピュータ―だ、調べてみろ",
              "maleVariant": "",
              "stringId": "1940195276474769408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一緒に来るか？",
              "maleVariant": "",
              "stringId": "1940207980501491712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ひとりで行く",
              "maleVariant": "",
              "stringId": "1940242179245694980"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": "1940242194580070400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、乗ってく",
              "maleVariant": "ああ、乗ってく",
              "stringId": "1940242224510623748"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "1940253322051805188"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見るべきものはすべて見た。行くぞ",
              "maleVariant": "",
              "stringId": "1940272961091756032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうか。好きにしろ。時間の無駄だと思うがな",
              "maleVariant": "",
              "stringId": "1940277309393821696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どこかに別の出口があったはずだ",
              "maleVariant": "",
              "stringId": "1940277793718493184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V！",
              "maleVariant": "",
              "stringId": "1940324248906850304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞こえるか？",
              "maleVariant": "",
              "stringId": "1940325410192187392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、V！ 大丈夫か？",
              "maleVariant": "",
              "stringId": "1940336659416178688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインが死んだのはウイルスのせいよ",
              "maleVariant": "ラインはウイルスで死んだんだ",
              "stringId": "1940344516555530244"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死ぬかと思った…",
              "maleVariant": "",
              "stringId": "1940347415155138560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ひどい目に遭った…",
              "maleVariant": "",
              "stringId": "1940352153544253440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "焦ったぞ。大丈夫か？",
              "maleVariant": "",
              "stringId": "1940353448409784320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、なんとかね",
              "maleVariant": "まあ、なんとかな",
              "stringId": "1940353762445713408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何が起きたんだ？",
              "maleVariant": "",
              "stringId": "1940353899733671936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ… そういうことか",
              "maleVariant": "",
              "stringId": "1940366242563710976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラインはウイルスで死んだのよ",
              "maleVariant": "ラインはウイルスで死んだんだ",
              "stringId": "1940369362790342656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんでここに？",
              "maleVariant": "なぜここに？",
              "stringId": "1940369362790342672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ！ 目を覚ませ…",
              "maleVariant": "",
              "stringId": "1940375083854553088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんでここに？ 令状がなかったんじゃないの",
              "maleVariant": "なんでここに？ 令状がなかったんじゃないのか",
              "stringId": "1940376312030650368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中が俺に手を出してきたんだ。やり合ってたら、いつの間にかここにいた",
              "maleVariant": "",
              "stringId": "1940377334299975680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ… なるほどね",
              "maleVariant": "",
              "stringId": "1940377715427991552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、何が起きたんだ？",
              "maleVariant": "",
              "stringId": "1940377870063591424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "テックに詳しいのか？",
              "maleVariant": "",
              "stringId": "1940382345218187264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうでもないけど、リセットくらいならできそう",
              "maleVariant": "そうでもないが、リセットくらいならできそうだ",
              "stringId": "1940382485609930752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、戻ってこい",
              "maleVariant": "",
              "stringId": "1940395943252418560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あん？",
              "maleVariant": "",
              "stringId": "1940399883851358208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつと話せ。ここで何してるのか、ラインについて知ってるか聞くんだ",
              "maleVariant": "",
              "stringId": "1940412745684574208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしが？ 刑事はそっちでしょ",
              "maleVariant": "俺が？ 刑事はそっちだろ",
              "stringId": "1940414077862633472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも俺には――",
              "maleVariant": "",
              "stringId": "1940414375960207360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はいはい、令状ね",
              "maleVariant": "はいはい、令状な",
              "stringId": "1940414478838095872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜこんな場所を見張ってるの？ 何もないのに",
              "maleVariant": "なぜこんな場所を見張ってるんだ？ 何もないだろ",
              "stringId": "1940418068910661632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "依頼人は誰？ もしかしたら力になれるかも",
              "maleVariant": "依頼人は誰だ？ もしかしたら力になれるかもしれない",
              "stringId": "1940418493944651780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウェルドン・ホルトだ",
              "maleVariant": "",
              "stringId": "1940424180431020036"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ルシアス・ラインの右腕… 今度の市長選挙の候補者の？",
              "maleVariant": "ルシアス・ラインの右腕… 今度の市長選挙の候補者のか？",
              "stringId": "1940424181336989696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、そのクズ野郎だ",
              "maleVariant": "",
              "stringId": "1940424182159073280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがと",
              "maleVariant": "助かるよ",
              "stringId": "1940424183065042944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんでここに？",
              "maleVariant": "なぜここに？",
              "stringId": "1940436943849086980"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ルシアス・ラインは、死んだ日ここにいたのね",
              "maleVariant": "ルシアス・ラインは、死んだ日ここにいたんだな",
              "stringId": "1940437918286569472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見張ってなんかいない。待ってるだけだ",
              "maleVariant": "",
              "stringId": "1940441622242877440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何を？",
              "maleVariant": "何をだ？",
              "stringId": "1940441772700950528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "報酬の送金を、だ。言われた通り、ここを盛大に荒らしてやった",
              "maleVariant": "",
              "stringId": "1940441851839078400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "にもかかわらず、まだ金を寄越してこない",
              "maleVariant": "",
              "stringId": "1940447922288812032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "情報をくれたら、半分払ってあげる",
              "maleVariant": "情報をくれたら、半分払ってやるよ",
              "stringId": "1940448768179265536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "依頼人の情報をくれたら、あたしが報酬の半分を払ってあげる",
              "maleVariant": "依頼人の情報をくれたら、俺が報酬の半分を払ってやる",
              "stringId": "1940449197189455872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰に依頼されたの？",
              "maleVariant": "誰に依頼されたんだ？",
              "stringId": "1940449214788755456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前には関係ないだろ",
              "maleVariant": "",
              "stringId": "1940456105090703360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしが支払いを早めてあげる",
              "maleVariant": "俺が支払いを早めてやるよ",
              "stringId": "1940459286117965824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ、バカバカしい。デマカセもほどほどにしな！",
              "maleVariant": "",
              "stringId": "1940459626443792384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
