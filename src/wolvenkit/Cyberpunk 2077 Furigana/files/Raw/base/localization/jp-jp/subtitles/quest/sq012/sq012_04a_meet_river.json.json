  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リバー・ウォード？",
              "maleVariant": "リバー・ウォードか？",
              "stringId": 1700890536825180160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウォード刑事？",
              "maleVariant": "ウォード刑事か？",
              "stringId": 1700891001688281092
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お客さんが来たみたいだぞ",
              "maleVariant": "",
              "stringId": 1700982118597382144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。お前はVだな？",
              "maleVariant": "",
              "stringId": 1700983760499625984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "相棒のハン刑事だ",
              "maleVariant": "",
              "stringId": 1700983916276076544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レッド・クイーンズ・レースって知ってる？ 聞いたことは？",
              "maleVariant": "レッド・クイーンズ・レースって知ってるか？ 聞いたことは？",
              "stringId": 1701115711139205120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ… あるかもな。ラインと関係あるのか？",
              "maleVariant": "",
              "stringId": 1701116952049209344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死ぬ前日、そこに向かってた",
              "maleVariant": "",
              "stringId": 1701118020220997632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん… いいだろう。情報屋に会いに行くぞ",
              "maleVariant": "",
              "stringId": 1701118796418895872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お高くとまったクラブだってだけで、場所もわからないが… そいつなら知ってるかもしれない",
              "maleVariant": "",
              "stringId": 1745803853716639744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "上司に会いにいきましょ",
              "maleVariant": "上司に会いにいこう",
              "stringId": 1745805693204156420
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "情報屋と会いましょ",
              "maleVariant": "情報屋と会おう",
              "stringId": 1745806603166797824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "上司が先ね。あれだけのクロームを買えるってことは、高給取りでしょ。サイバーサイコを抑える薬だって持ってたはず",
              "maleVariant": "上司が先だ。あれだけのクロームを買えるってことは、高給取りだろ。サイバーサイコを抑える薬だって持ってたはずだ",
              "stringId": 1745806924517593088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "情報屋と会いましょ。クラブが気になる",
              "maleVariant": "情報屋に会いにいこう。クラブが気になる",
              "stringId": 1745807684760358912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": 1756154294396637184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいだろう",
              "maleVariant": "",
              "stringId": 1806954913600229376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "会ってくれてありがとう",
              "maleVariant": "",
              "stringId": 1933016127373008900
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ決めたわけじゃない。とりあえず、ラインの死について知ってることを聞くだけだ",
              "maleVariant": "",
              "stringId": 1933028122226810916
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おいおい、またそれか？ こないだボスに手を引けと言われたばかりだろ。しかも、「二度は言わせるな」のおまけつきでな",
              "maleVariant": "",
              "stringId": 1933028122226810928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ボスの言葉が聞こえてなかったんなら、せめて友人である俺のアドバイスを聞け。その件は忘れろ",
              "maleVariant": "",
              "stringId": 1933028122243588100
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別に何もしてないだろ？ コーヒーを飲んで、うわさ話を聞いてるだけだ",
              "maleVariant": "",
              "stringId": 1933028122243588112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もういい。好きにしろ。俺は関わらないからな。娘を迎えにいかなきゃいけないし",
              "maleVariant": "",
              "stringId": 1933028122243588124
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あと、せめて… もっと人の少ない所で話せよ",
              "maleVariant": "",
              "stringId": 1933028122243588136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、話せ",
              "maleVariant": "",
              "stringId": 1933028122260365312
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく、会ってくれてありがとう",
              "maleVariant": "",
              "stringId": 1933030116433813504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当に傭兵と協力するのか？",
              "maleVariant": "",
              "stringId": 1933040774646816768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "襲撃の前、ラインの護衛に何か伝えようとしてたけど――",
              "maleVariant": "襲撃の前、ラインの護衛に何か伝えようとしてたが――",
              "stringId": 1933076392877711360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エリザベス・ペラレスと夫のジェファーソンに雇われたの",
              "maleVariant": "エリザベス・ペラレスと夫のジェファーソンに雇われたんだ",
              "stringId": 1933076428931948544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] 相棒は傭兵が嫌いみたいね",
              "maleVariant": "[座る] 相棒は傭兵が嫌いみたいだな",
              "stringId": 1933081205556338688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハン刑事は、傭兵が嫌いみたいね",
              "maleVariant": "ハン刑事は、傭兵が嫌いみたいだな",
              "stringId": 1933081855757344768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気を悪くするな。あいつはみんなが嫌いなんだ。娘以外はな",
              "maleVariant": "",
              "stringId": 1933105064720424960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何だったの？",
              "maleVariant": "何だったんだ？",
              "stringId": 1933112870672490496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "所轄の連中が、アロヨ付近でゴミ箱を撃ったり、市長に会わせろって叫んでるホルヴァースを見かけたんだ。それで連行した",
              "maleVariant": "",
              "stringId": 1933130031331762176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "逆に聞くが、なぜこの件に首を突っ込む？ 誰に雇われた？",
              "maleVariant": "",
              "stringId": 1933130075220959232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、ペテル・ホルヴァース――市長を襲ったサイバーサイコだよ。連行したはいいが、事情聴取する前に“消えた”んだ",
              "maleVariant": "",
              "stringId": 1933175609424044032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから市長に警告しようとしたの？",
              "maleVariant": "だから市長に警告しようとしたのか？",
              "stringId": 1933181626421567488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "消えたって、どういう意味？",
              "maleVariant": "消えたって、どういう意味だ？",
              "stringId": 1933182033587822592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪いけど、依頼主の名前は言えない",
              "maleVariant": "悪いが、依頼主の名前は言えないな",
              "stringId": 1933187025396785152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ホルヴァースが“消えた”って、どういうこと？ 何が起きたの？",
              "maleVariant": "ホルヴァースが“消えた”って、どういうことだ？ 何が起きた？",
              "stringId": 1933187066484187136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "たったそれだけで、わざわざ市長に警告しようとしたの？",
              "maleVariant": "たったそれだけで、わざわざ市長に警告しようとしたのかよ",
              "stringId": 1933187169647288320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "正義感が強いにも、程があるんじゃない？",
              "maleVariant": "正義感が強いにも、程があるんじゃないか？",
              "stringId": 1934314850573049856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まるで恥じるべきことだと言わんばかりだな",
              "maleVariant": "",
              "stringId": 1934315107281231872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうじゃないけど… あんたみたいなタイプは珍しいから",
              "maleVariant": "そうじゃないが… あんたみたいなタイプは珍しいからな",
              "stringId": 1934316634897080320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰かが、奴を留置所から出してやったってことだ",
              "maleVariant": "",
              "stringId": 1934317625608462336
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "先に言っておくが、それが誰なのかはわからない",
              "maleVariant": "",
              "stringId": 1934325079020888064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ホルヴァース？",
              "maleVariant": "",
              "stringId": 1934388765618036736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今度は俺が質問する番だ。なぜペラレスは今さら、このことを陰でこっそり調べている？",
              "maleVariant": "",
              "stringId": 1934441210188619776
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうか。ではお引き取り願おう。俺もお前に教えることは何ひとつない。他をあたるんだな",
              "maleVariant": "",
              "stringId": 1934460958263697408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言っておくが、詮索しようってわけでも、お前が嫌いってわけでもない。俺も俺で、自分の身を守らないといけないんだ",
              "maleVariant": "",
              "stringId": 1934469901190914048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ペラレス夫妻よ",
              "maleVariant": "ペラレス夫妻だ",
              "stringId": 1934480952829632516
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "依頼主の名前は言えない",
              "maleVariant": "",
              "stringId": 1934482865432256512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰の耳に入るかわからないまま、ベラベラと喋るつもりはない",
              "maleVariant": "",
              "stringId": 1934493319701422080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "市長候補の？ はっ！ なるほど、合点がいった…",
              "maleVariant": "",
              "stringId": 1934494484358979584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンの言うとおりだ。ここは声が響きすぎるな",
              "maleVariant": "",
              "stringId": 1934502426844028928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "外に車がある",
              "maleVariant": "",
              "stringId": 1934503676427853824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立つ] わかった",
              "maleVariant": "",
              "stringId": 1934504246668648448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": 1934505629161578496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レッド・クイーンズ・レースって知ってる？",
              "maleVariant": "レッド・クイーンズ・レースって知ってるか？",
              "stringId": 1934615348849106944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ホルトを陥れるネタを探してるんだと思う",
              "maleVariant": "",
              "stringId": 1934617133357686784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ホルトの評判を落とすつもりなのかも",
              "maleVariant": "ホルトの評判を落とすつもりなのかもな",
              "stringId": 1934667874822742016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ペラレスは怖がってるのかも",
              "maleVariant": "",
              "stringId": 1934669997157343232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ペラレスはきっと怖いのよ",
              "maleVariant": "ペラレスはきっと怖いんだ",
              "stringId": 1934670431485911040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ホルトが関係してると？",
              "maleVariant": "",
              "stringId": 1934673191354720256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ。でも市長が殺されたなんて、補佐役としては十分失態でしょ",
              "maleVariant": "さあな。だが市長が殺されたなんて、補佐役としては十分失態だろ",
              "stringId": 1934673357449158656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "前任者が死ぬような仕事には就きたくないってことか？",
              "maleVariant": "",
              "stringId": 1934675882218840064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしなら嫌ね",
              "maleVariant": "俺なら嫌だな",
              "stringId": 1934676423586045952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その後、ホルヴァースの上司と話をする",
              "maleVariant": "",
              "stringId": 1934694753197842432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや… 先に上司に会ってもいいな。お前次第だ",
              "maleVariant": "",
              "stringId": 1934696305140654080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一緒に行動するの？",
              "maleVariant": "一緒に行動するのか？",
              "stringId": 1934699234828443648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "一緒に行動するの？",
              "maleVariant": "一緒に行動するのか？",
              "stringId": 1934699516199133184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ。この件は何かがおかしい。捜査の打ち切りも早すぎた",
              "maleVariant": "",
              "stringId": 1934703094309543936
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前はペラレスのため、俺は捜査再開につながる証拠を見つけるため――",
              "maleVariant": "",
              "stringId": 1934708604987465728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うまくいけば、二人とも目的を果たせる",
              "maleVariant": "",
              "stringId": 1934711079727489024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも腑に落ちない",
              "maleVariant": "",
              "stringId": 1944734753104814080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話があるんだろ？",
              "maleVariant": "",
              "stringId": 1948748049721679876
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シャイなのかな？",
              "maleVariant": "",
              "stringId": 1948748050006892548
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、どうしたんだ",
              "maleVariant": "",
              "stringId": 1948748050040446980
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] 市長に何を伝えようとしてたの？",
              "maleVariant": "[座る] 市長に何を伝えようとしてたんだ？",
              "stringId": 1948873165340237824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ん。何だろう…",
              "maleVariant": "",
              "stringId": 1957872228313358336
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話がしたいの",
              "maleVariant": "話がしたい",
              "stringId": 1957873563561648128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話があるの。今いい？",
              "maleVariant": "話があるんだ。今いいか？",
              "stringId": 1957874101640519680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前がVか",
              "maleVariant": "",
              "stringId": 1957875741361725440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "相棒のハン刑事だ",
              "maleVariant": "",
              "stringId": 1957876038519775232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "二人だけで話せる？",
              "maleVariant": "二人だけで話せるか？",
              "stringId": 1957878086950739968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "二人きりで話したいんだけど",
              "maleVariant": "二人きりで話したいんだが",
              "stringId": 1957878347165360128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハンと俺は一心同体だ。隠し事はない",
              "maleVariant": "",
              "stringId": 1957878865698775040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座って待つ]",
              "maleVariant": "",
              "stringId": 2104079250819997700
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[乗る]",
              "maleVariant": "",
              "stringId": 2341373769146523652
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}