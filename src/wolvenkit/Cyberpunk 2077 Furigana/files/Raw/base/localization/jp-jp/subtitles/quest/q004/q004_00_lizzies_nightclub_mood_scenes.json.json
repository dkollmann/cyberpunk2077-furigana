  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あなたの好み次第よ。何でもしてあげる… 一杯おごってくれたらね",
                    "maleVariant": null,
                    "stringId": 1487994350681862144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おい！ こっちにおかわり！",
                    "maleVariant": null,
                    "stringId": 1487994350681862160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いやいやいや、バーチャルって言っても、感触はマジでリアルなんだ。ムチで打たれた感じがまだ残ってるし、女の子もすげえからな",
                    "maleVariant": null,
                    "stringId": 1487994350799302656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうか？ 俺も何度か試したが、頭が痛くなっただけだ",
                    "maleVariant": null,
                    "stringId": 1487994350799302672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ここのは違うんだって。きっとカスタムされてんだ。たった数秒で、すぐに現実と区別がつかなくなる。リアルと変わらねえんだよ",
                    "maleVariant": null,
                    "stringId": 1487994350816079884
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "見ろよ。ヴィッキーとバックでAFだ。いいケツだろ？ 場所も選べるぞ。ワイハの海小屋に、ウエストブルックのペントハウスだって…",
                    "maleVariant": null,
                    "stringId": 1487994350816079900
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺はもっと普通のヤツでいい。こういうのとか",
                    "maleVariant": null,
                    "stringId": 1487994350816079916
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "え、“ムカデ”か？ まあ… 好きにしな。ヴィッキーは俺がもらうぜ",
                    "maleVariant": null,
                    "stringId": 1487994350816079932
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほら、景気づけだ。飲んだらボックスに直行するぞ",
                    "maleVariant": null,
                    "stringId": 1487994350832857096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "昨日、朝4時にジーナとバーにいたんだけど",
                    "maleVariant": null,
                    "stringId": 1487994350899965952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの子、また銀行の話をするわけ。ベラベラ、延々と",
                    "maleVariant": null,
                    "stringId": 1487994350916743180
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "暴れ出したの？",
                    "maleVariant": null,
                    "stringId": 1487994350916743196
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それがさ… バーチャルじゃない生身の相手とヤラせろ、とか言うの。禁止なのはわかってるけど3倍払うから、って",
                    "maleVariant": null,
                    "stringId": 1487994350916743212
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そしたらジーナが… どうしたと思う？",
                    "maleVariant": null,
                    "stringId": 1487994350916743228
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そいつをボックスにブチ込んだの。でもハイになってるからBD用の電極をオモチャと勘違いしちゃって。そのまま、最後のとこでイッちゃったままループするヤツをロードしてやったの",
                    "maleVariant": null,
                    "stringId": 1487994350933520396
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ウソ… バレなかったの？",
                    "maleVariant": null,
                    "stringId": 1487994350950297600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "当然よ。シラフの奴だって、あのブッカケを本物と勘違いするんだから",
                    "maleVariant": null,
                    "stringId": 1487994350950297616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "モール、ちょっと相談したいことがあるんだけど…",
                    "maleVariant": null,
                    "stringId": 1487994351134846976
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "客とのトラブルかい？ 金を払わず逃げたのなら、入口の女たちに言いな",
                    "maleVariant": null,
                    "stringId": 1487994351151624196
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうじゃなくて… 私の友達がさ、8番街で働いてるの",
                    "maleVariant": null,
                    "stringId": 1487994351151624212
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うちらのシマじゃない。管轄外だ",
                    "maleVariant": null,
                    "stringId": 1487994351151624228
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それはわかってるんだけど、彼女、そこを離れてこっちで働きたいって。フルタイムで",
                    "maleVariant": null,
                    "stringId": 1487994351168401420
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でもパパが許さなくて。逃げようとしたら、売りモンだろうと容赦しないって言われたみたい",
                    "maleVariant": null,
                    "stringId": 1487994351185178624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "パパの名前は？",
                    "maleVariant": null,
                    "stringId": 1487994351185178640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本名はわかんない。でも誰かが“ラテックス・フレッド”って呼んでた。イタいよね",
                    "maleVariant": null,
                    "stringId": 1487994351185178656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。その友達に、ラテックス・フレッドのことはもう心配しなくていいって言っときな",
                    "maleVariant": null,
                    "stringId": 1487994351201955844
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それで… 今日はどんなプレイをしにきたの？",
                    "maleVariant": null,
                    "stringId": 1487994351269064704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと激しめのノーマル？ もっと変態っぽいヤツならワックスに電流とか？ それともパワーインプラント？",
                    "maleVariant": null,
                    "stringId": 1487994351269064720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや… そうだな、君の友達にも来てもらおう。ヒールに目隠し、猿ぐつわをつけてね",
                    "maleVariant": null,
                    "stringId": 1487994351269064736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ええ、いいわよ。購入？ それともレンタル？",
                    "maleVariant": null,
                    "stringId": 1487994351269064752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "レンタルで。いくらだ？",
                    "maleVariant": null,
                    "stringId": 1487994351285841920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大丈夫、つけておくから。あなたにぴったりのヤツがあるの",
                    "maleVariant": null,
                    "stringId": 1487994351285841936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私とキャンディスのよ。カラダの隅々まで私たち二人が満足させてあげる",
                    "maleVariant": null,
                    "stringId": 1487994351336173568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それじゃ、向こうで会いましょ",
                    "maleVariant": null,
                    "stringId": 1487994351336173584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前のちんけな妄想なんかよりずっと凄いのが体験できるぜ。ほら、このカタログ――",
                    "maleVariant": null,
                    "stringId": 1631443522876469248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そしたらそこに、変な男がフラフラ入ってきたの。ドアのところでモールとぶつかりながら",
                    "maleVariant": null,
                    "stringId": 1631451954937458688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でもさ、保育園に娘を迎えに行くのに、午後のシフト代わってほしかったから… 言い出すタイミング見つつ、まあ我慢してたわけ",
                    "maleVariant": null,
                    "stringId": 1631704311361302528
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}