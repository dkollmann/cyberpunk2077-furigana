        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "1919年は歴史的な変革の年でした。アメリカ禁酒法の成立、第一次世界大戦の終戦…",
                    "maleVariant": null,
                    "stringId": 1792116760528199680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "…そしてサブロウ・アラサカ氏の誕生",
                    "maleVariant": null,
                    "stringId": 1792544797840097280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サブロウ氏は若い頃から父の興した事業に積極的に関わっていました",
                    "maleVariant": null,
                    "stringId": 1792546671368585216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "若くしてサブロウ氏は仕事への情熱を見せ、優れた経営者としての才覚を発揮し始めました。しかしながら、輝かしい彼のキャリアも、戦争勃発により中断されてしまいます",
                    "maleVariant": null,
                    "stringId": 1792547934642618368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "開戦を受け、サブロウ氏は大日本帝国海軍に入隊。兵科は航空兵でした。23歳になる頃には、彼は大尉にまで進級しました",
                    "maleVariant": null,
                    "stringId": 1792549241235423232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "勝ち星は記録にあるだけで20を数え、日本軍における伝説のパイロットの一人として、後世に語り継がれることになります",
                    "maleVariant": null,
                    "stringId": 1792549875028312064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しかし1942年、任務中に搭乗機が激しい損傷を受けます",
                    "maleVariant": null,
                    "stringId": 1792550483554713600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "サブロウ氏自身も重傷を負いましたが、故障した機体をなんとか操り、基地へと帰還したのでした… 続きはCMの後、チャンネルはそのまま",
                    "maleVariant": null,
                    "stringId": 1792557854389682176
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
