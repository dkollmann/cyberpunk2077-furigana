  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "来ないのかと思い始めてた",
              "maleVariant": "",
              "stringId": 1792432330482032640
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった… 要件は？",
              "maleVariant": "",
              "stringId": 1792531005510164484
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "驚いた。あんたは…",
              "maleVariant": "",
              "stringId": 1792531476882825220
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう。リズィー・ウィズィーよ",
              "maleVariant": "",
              "stringId": 1792531866852433920
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしにどうしろと？",
              "maleVariant": "俺にどうしろと？",
              "stringId": 1792532344852094980
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのリアムってのは何者？",
              "maleVariant": "そのリアムってのは何者だ？",
              "stringId": 1792532709353889792
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "浮気をしてるって根拠は？",
              "maleVariant": "",
              "stringId": 1792532872998854656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、あたしに何をしてほしいわけ？",
              "maleVariant": "で、俺にどうしてしてほしいんだ？",
              "stringId": 1792550454095159296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "証拠がほしい。それでこの関係は終わりにする",
              "maleVariant": "",
              "stringId": 1792554907976245248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "任せておいて",
              "maleVariant": "任せておけ",
              "stringId": 1792570683911217156
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "考えさせて",
              "maleVariant": "考えさせてくれ",
              "stringId": 1792581826901876740
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。慎重なのはいいことよ。後で後悔するのはご免だものね",
              "maleVariant": "",
              "stringId": 1792582527954624512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よかった",
              "maleVariant": "",
              "stringId": 1792588537251405824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] 任せて",
              "maleVariant": "[立ち上がる] 任せてくれ",
              "stringId": 1792591127787773952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] 考えておく",
              "maleVariant": "",
              "stringId": 1792591127787773968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "具体的な情報を掴んで。確たる証拠が欲しいの。できれば映像や写真",
              "maleVariant": "",
              "stringId": 1803970679038451712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "来ないほうが良かったかもね",
              "maleVariant": "来ないほうが良かったかもな",
              "stringId": 1945752777125056512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして？",
              "maleVariant": "",
              "stringId": 1945753901483741184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フィクサーを通さない仕事は避けてる。直の依頼は厄介なことが多いから",
              "maleVariant": "フィクサーを通さない仕事は避けてる。直の依頼は厄介なことが多いからな",
              "stringId": 1945754033268772864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "プライバシーを守りたかったの",
              "maleVariant": "",
              "stringId": 1945754887849824256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰だってそう",
              "maleVariant": "誰だってそうだ",
              "stringId": 1945756282439127044
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なぜ？",
              "maleVariant": "",
              "stringId": 1945756490006843392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰だってプライバシーは守りたい",
              "maleVariant": "",
              "stringId": 1945757412200075264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "かもね。でも私ほどじゃない",
              "maleVariant": "",
              "stringId": 1945757827889156096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふうん。で、そいつはどうしてなの？",
              "maleVariant": "ふうん。で、そいつはどうしてなんだ？",
              "stringId": 1945758169540382720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これが理由",
              "maleVariant": "",
              "stringId": 1945758797780013056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "掛けて。話をしましょ",
              "maleVariant": "",
              "stringId": 1945760336301051904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わからない？ 問題を抱えてるの",
              "maleVariant": "",
              "stringId": 1945762786714112000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "問題ってどんな？",
              "maleVariant": "",
              "stringId": 1945763534004867072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん、いい質問ね",
              "maleVariant": "",
              "stringId": 1945763845071228928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "恋をしたの",
              "maleVariant": "",
              "stringId": 1945764910088900608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはお気の毒に",
              "maleVariant": "",
              "stringId": 1945768400655798272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはお気の毒",
              "maleVariant": "それはお気の毒に",
              "stringId": 1945768620236001284
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それの何が問題なの？",
              "maleVariant": "それの何が問題なんだ？",
              "stringId": 1945768840269189120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "恋は悪いことじゃない",
              "maleVariant": "",
              "stringId": 1945769062382751744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あら、どうして？",
              "maleVariant": "",
              "stringId": 1945770376240091136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だって、絶対に実ることのない恋でしょ",
              "maleVariant": "だって、絶対に実ることのない恋だろ",
              "stringId": 1945770936079011840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私は別",
              "maleVariant": "",
              "stringId": 1945774347776217088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私は表現者なの。恋は… その邪魔になるのよ。集中できなくなるの",
              "maleVariant": "",
              "stringId": 1945774460351336448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで？ 相手は男？ 女？",
              "maleVariant": "",
              "stringId": 1945774857551925248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かにそうね",
              "maleVariant": "",
              "stringId": 1945776029490794496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、それはともかく。相手はリアム・ノーソムという男性よ。付き合って何ヶ月かなんだけど、浮気をしてるみたいで",
              "maleVariant": "",
              "stringId": 1945776580991438848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼とは直接話したの？",
              "maleVariant": "そいつとは直接話したのか？",
              "stringId": 1945779616879005696
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リアム・ノーソム… 知らない名前ね。どんな人なの？",
              "maleVariant": "リアム・ノーソム… 知らない名前だな。どんな男だ？",
              "stringId": 1945780041543897088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "40前後。繊細で、まったくの無欲。そんな人と出会ったことある？",
              "maleVariant": "",
              "stringId": 1945781606505828352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "絶滅したかと。出会いは？",
              "maleVariant": "",
              "stringId": 1945782725294477312
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "えっと… 私のマネージャーなの",
              "maleVariant": "",
              "stringId": 1945786056729481216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どう聞こえるかはわかってる。彼は私のタイプですらないんだけど…",
              "maleVariant": "",
              "stringId": 1945786467284733952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だけど？",
              "maleVariant": "",
              "stringId": 1945789721930420224
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いくら体をクロームにしても… 心は変わらない。本能には抗えないでしょ",
              "maleVariant": "",
              "stringId": 1945789868529733632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、浮気を疑う根拠は？",
              "maleVariant": "",
              "stringId": 1945792317936160768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その… 恥ずかしいんだけど、ありきたりな話",
              "maleVariant": "",
              "stringId": 1945792787345887232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "トイレでコソコソ電話してたり、怪しい着信があったり、夜中にこっそりメッセージを送ってたり",
              "maleVariant": "",
              "stringId": 1945793051503153152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "特に記憶に残ってる出来事は？",
              "maleVariant": "",
              "stringId": 1945796130055180288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "記憶… ね。それは… あまり話したくない",
              "maleVariant": "",
              "stringId": 1945797027401355264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうして？",
              "maleVariant": "どうしてだ？",
              "stringId": 1945797593414291456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私たち、まだ会ったばかりでしょ",
              "maleVariant": "",
              "stringId": 1945797729561399296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼とは面と向かって話し合ったの？",
              "maleVariant": "そいつとは面と向かって話し合ったのか？",
              "stringId": 1945800422103240704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何も知らないのね！",
              "maleVariant": "",
              "stringId": 1945806120400769024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2人っきりになれることなんてないの。常にアシスタントやらPRやらメイクやらに取り囲まれてるのよ？",
              "maleVariant": "",
              "stringId": 1945806690289242112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼の前で一瞬でも顔を曇らせたら、スクリームシートに書き立てられる",
              "maleVariant": "",
              "stringId": 1945807364347449344
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど",
              "maleVariant": "",
              "stringId": 1945807904976457728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいえ、これは実際に経験しないとわからない。無数の目が常に自分に向けられているの。それは美しくもあるけど… 恐ろしくもある",
              "maleVariant": "",
              "stringId": 1945808367104872448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リアムはライオットに入り浸ってる。きっと誰かに会ってるのよ。その相手を知りたい",
              "maleVariant": "",
              "stringId": 1945811742361964544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "調べてくれる？",
              "maleVariant": "",
              "stringId": 1945812165785341952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "報酬の話が先よ",
              "maleVariant": "報酬の話が先だ",
              "stringId": 1945817915605700608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "焦らないで。報酬は？",
              "maleVariant": "焦るなって。報酬は？",
              "stringId": 1945818408889405440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろん出す",
              "maleVariant": "",
              "stringId": 1945823364694462464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でないと困る。聞きたいのは金額よ",
              "maleVariant": "でないと困る。聞きたいのは金額だ",
              "stringId": 1945823933089763328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "下らないわね。大金よ、たっぷり払う。それで満足？",
              "maleVariant": "",
              "stringId": 1945825037533892608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのね、あたしたち一般人にとっては、その下らない金額の話が、時に生き死にさえ左右するのよ",
              "maleVariant": "あのな、俺たち一般人にとっては、その下らない金額の話が、時に生き死にさえ左右するんだ",
              "stringId": 1945825602741522432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お説教は結構。引き受けるの？",
              "maleVariant": "",
              "stringId": 1945826667088105472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、わかった",
              "maleVariant": "ああ、わかった",
              "stringId": 1945829471550754816
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃあ、もう行って",
              "maleVariant": "",
              "stringId": 1945829651771609088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "嫌になる… この手。軽くなでたつもりでも、火花が飛んだりするんだから…",
              "maleVariant": "",
              "stringId": 1946062477636980736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今年は色々と調子が出なくて。もうずいぶん長い間、まともな作品を作れてない。さらに厄介なことに…",
              "maleVariant": "",
              "stringId": 1963309036221026304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る]",
              "maleVariant": "",
              "stringId": 1973597722804920336
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？",
              "maleVariant": "",
              "stringId": 2483716571303682048
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}