  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エヴ、待ってたよ",
              "maleVariant": "",
              "stringId": "1095391017180213264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっとやめてよ。ムズムズする",
              "maleVariant": "",
              "stringId": "1095392272904499200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る]",
              "maleVariant": "",
              "stringId": "1101183555417813012"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まずは感覚プロフィールの作成ね",
              "maleVariant": "",
              "stringId": "1185246544197988352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[BDを開始] OK、始めましょ",
              "maleVariant": "[BDを開始] OK、始めよう",
              "stringId": "1185247331603066880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ダイブ開始まで、3… 2… 1…",
              "maleVariant": "",
              "stringId": "1185253220120670208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BD自体は経験済みだけど、ナマモノは未知の領域ね",
              "maleVariant": "BD自体は経験済みだが、ナマモノは… 未知の領域だな",
              "stringId": "1205294610376437760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫、ちゃんと説明するから。意外と簡単よ",
              "maleVariant": "",
              "stringId": "1205295480744849408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女はV。例のBDの件で連れてきたの。こっちはジュディ。最高のブレインダンスエディターよ",
              "maleVariant": "彼はV。例のBDの件で連れてきたの。こっちはジュディ。最高のブレインダンスエディターよ",
              "stringId": "1207971280065261568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よろしく、ジュディ",
              "maleVariant": "",
              "stringId": "1207999930718351360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よろしく、ジュディ",
              "maleVariant": "",
              "stringId": "1208001059908874240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すごい機材ね。ブレインダンスの処理にこんなに必要なの？",
              "maleVariant": "すごい機材だな。ブレインダンスの処理にこんなに必要なのか？",
              "stringId": "1208002337594535952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあね。分析機器に感覚信号増幅器、音波と感情波モニター、表情変換器…",
              "maleVariant": "",
              "stringId": "1208003389576310784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジュディ…",
              "maleVariant": "",
              "stringId": "1209711106959470592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はいはい…",
              "maleVariant": "",
              "stringId": "1209712251467591680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃ、さっそく調整から始めようか",
              "maleVariant": "",
              "stringId": "1209713636141559824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "たまに舞い込んでくるジグジグ・ストリートのどぎついポルノに比べたら、全然余裕だから",
              "maleVariant": "",
              "stringId": "1212303062025760768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうぞ、ご自由に",
              "maleVariant": "",
              "stringId": "1212468254000934912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "感覚プロフィール？",
              "maleVariant": "",
              "stringId": "1212468809863655424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうぞ、ご自由に。でも痛いのは勘弁してね",
              "maleVariant": "どうぞ、ご自由に。でも痛いのは勘弁してくれ",
              "stringId": "1212469212550393856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫。これは痛くないから",
              "maleVariant": "",
              "stringId": "1212469756400627712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃ、プロフィールのテストから始めるよ。サンプルBDを使って、エディターに慣れてもらう",
              "maleVariant": "",
              "stringId": "1212551858844463104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "感情反応と認知機能のモデル化。そのうえで調整を加えないと、ナマモノのBDには耐えられない",
              "maleVariant": "",
              "stringId": "1212556388374016000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これをやっとかないと、起動した途端に気分が悪くなったり、吐く可能性もあるから。それに最悪の場合…",
              "maleVariant": "",
              "stringId": "1212600646619492352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なに、もったいぶらないでよ",
              "maleVariant": "なんだ、もったいぶるなよ",
              "stringId": "1212607428473516032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたのシナプスがカリッカリに焦げる",
              "maleVariant": "",
              "stringId": "1212607527190654976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ま、そんなことは起こさせないけど",
              "maleVariant": "",
              "stringId": "1212607851292913664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サンプルの中身は何？",
              "maleVariant": "サンプルの中身は何だ？",
              "stringId": "1299453107841675264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BDはコンパイル済みよ",
              "maleVariant": "",
              "stringId": "1307947014497628160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃ、じっと座って私を見て。分析ソフトを実行するよ。ちょっとヒリヒリするから…",
              "maleVariant": "",
              "stringId": "1308071991855149056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "毎回この調整をやるの？",
              "maleVariant": "毎回この調整をやるのか？",
              "stringId": "1308090225199280128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "毎回、このめんどくさい調整をやんなきゃいけないの？",
              "maleVariant": "毎回、このめんどくさい調整をやんなきゃいけないのか？",
              "stringId": "1308092577599844352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういうわけじゃないけど、やる価値はある。見てればわかるから。BD分析の凄さがね",
              "maleVariant": "",
              "stringId": "1308135852968898560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "えーと、あとは痛覚リミッターを入れて…",
              "maleVariant": "",
              "stringId": "1308150917147258880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、準備完了",
              "maleVariant": "",
              "stringId": "1308163796160344064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだナマに近いけど… ま、大丈夫でしょ",
              "maleVariant": "",
              "stringId": "1451519348570714112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあさっそく潜って、直に見てもらおうかしら。いいわね？",
              "maleVariant": "",
              "stringId": "1451525201973604352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vはどう？ ナマモノのブレインダンス… これが初体験でしょ？",
              "maleVariant": "",
              "stringId": "1451525849540587520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "初体験",
              "maleVariant": "",
              "stringId": "1451526554485649408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうとでもなる",
              "maleVariant": "",
              "stringId": "1451526858706907136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうとでもなる。さっさと始めて",
              "maleVariant": "どうとでもなる。さっさと始めてくれ",
              "stringId": "1451529165674770432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "了解",
              "maleVariant": "",
              "stringId": "1451529803057983488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ座って。さっそく始めるから",
              "maleVariant": "",
              "stringId": "1451536527181606912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "始めましょ",
              "maleVariant": "始めてくれ",
              "stringId": "1461687719782117376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "中身は何？",
              "maleVariant": "中身は何だ？",
              "stringId": "1461687988653780992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ… 先に調整済みのモノで慣れてもらって、それから編集モードに入りましょ",
              "maleVariant": "",
              "stringId": "1461688549952319488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘイウッドで起きた強盗――",
              "maleVariant": "",
              "stringId": "1461689039561814016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK。次は視覚とその他感覚信号の設定ね。視力検査の時みたいに、この2つの画面をよく見て",
              "maleVariant": "",
              "stringId": "1498105181783138304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった…",
              "maleVariant": "",
              "stringId": "1498106194472673280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私を見て。すぐ済むから",
              "maleVariant": "",
              "stringId": "1498117272988491776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと。目線はこっち",
              "maleVariant": "",
              "stringId": "1498117352109842432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほら座って。始めるよ",
              "maleVariant": "",
              "stringId": "1508076081622228992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？ なにモタモタしてるの？",
              "maleVariant": "",
              "stringId": "1508076118448218112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どう？ 何とかなりそう？",
              "maleVariant": "",
              "stringId": "1531376649791729664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく、Vが深く潜り込めるならいいわ",
              "maleVariant": "",
              "stringId": "1531378377056448512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナマモノってどういうこと？ 既製品とどう違うの？",
              "maleVariant": "ナマモノってどういうことだ？ 既製品とどう違う？",
              "stringId": "1531419102473220096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね。既製のものでも… ああ、もちろんちゃんとしたものであればだけど… 現実では到底味わえないような感情や印象を与えてはくれるの",
              "maleVariant": "",
              "stringId": "1531420289780019200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ところがナマモノは… まるで仮想空間にいるような体験ができる。動き回って細部を調べたり",
              "maleVariant": "",
              "stringId": "1531440494648356864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつもこういう仕事を？",
              "maleVariant": "",
              "stringId": "1531468059127705600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こういう依頼は多いの？",
              "maleVariant": "こういう依頼は多いのか？",
              "stringId": "1531468470203052032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあね。でもいつもはクラブの子たちが使うヤツだけど。要はポルノ用の調整ばっかり",
              "maleVariant": "",
              "stringId": "1531469144143818752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "というより、不要な情報を遮断するため。客にとってセックスはもはや日常の一部だから、気が散っちゃうの… 買い物のこととか、すぐ雑念が浮かぶ",
              "maleVariant": "",
              "stringId": "1531469922354982912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて、どれがいいかな…",
              "maleVariant": "",
              "stringId": "1531474389238411264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私の記録を使えば？ 時間の無駄じゃない",
              "maleVariant": "",
              "stringId": "1531474913123758080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも感情の組み合わせ次第で、忘れがたい特別なひと時に仕立てることはできる。そこが腕の見せ所ってわけ",
              "maleVariant": "",
              "stringId": "1531566322107412480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "より深いオーガズムのため？",
              "maleVariant": "",
              "stringId": "1531570673328713728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すごい機材ね",
              "maleVariant": "すごい機材だな",
              "stringId": "1531771899394842644"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪くない。一級品のハードウェアね",
              "maleVariant": "悪くない。一級品のハードウェアだ",
              "stringId": "1558808213139877888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "感覚信号増幅器、音波に感情波モニター… どれも一級品ね",
              "maleVariant": "感覚信号増幅器、音波に感情波モニター… どれも一級品だ",
              "stringId": "1558864370424475648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "当たり前でしょ？",
              "maleVariant": "",
              "stringId": "1558875011860262912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこにある変換器は… フユツキ製ね。そのシリーズのマトリクスは、使い物にならないって聞いたけど",
              "maleVariant": "そこにある変換器は… フユツキ製だな。そのシリーズのマトリクスは使い物にならないって聞いたが",
              "stringId": "1558880559045406720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そう。でもマトリクスの交換なんて難しくないし、スキャンウェアを追加できるモデルはこれしかないから",
              "maleVariant": "",
              "stringId": "1558897533242699776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1558905175264587776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はいはい…",
              "maleVariant": "",
              "stringId": "1558905760973975552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リスクヘッジよ。すぐ終わるから。たしかここらへんに… あった",
              "maleVariant": "",
              "stringId": "1631578857614733312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ほとんどは改造済み。既製品は外装ぐらい",
              "maleVariant": "",
              "stringId": "1631732358839922688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分で改造したの？",
              "maleVariant": "自分で改造したのか？",
              "stringId": "1631733124937936896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナマモノって？",
              "maleVariant": "",
              "stringId": "1631769761059303424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただし大量生産品だから、管理できるように制限をかけてハードコードされてる",
              "maleVariant": "",
              "stringId": "1631772718144286720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "市場に出回るBDでも、エディターがまずナマモノのレイヤーにある感情や欲求の発露を見つけ、切り出してるわけ",
              "maleVariant": "",
              "stringId": "1631792460582219776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "感覚プロフィールってなに？",
              "maleVariant": "感覚プロフィールってなんだ？",
              "stringId": "1664671191184121856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": "1719836086107840512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とんだ素人連中だし、出所も不明だけど、調整にはまさにうってつけ。準備はいい？",
              "maleVariant": "",
              "stringId": "1964742450583564288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V？",
              "maleVariant": "",
              "stringId": "1970361249193877504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞いてるの？",
              "maleVariant": "",
              "stringId": "1970361281708122112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、集中して",
              "maleVariant": "",
              "stringId": "1970361312964075520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[ブレインダンスのチュートリアルをスキップする]",
              "maleVariant": "",
              "stringId": "3043160229220868096"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}