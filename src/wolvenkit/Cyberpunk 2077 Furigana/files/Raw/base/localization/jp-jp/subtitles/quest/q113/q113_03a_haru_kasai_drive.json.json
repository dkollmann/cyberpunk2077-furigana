  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "急いでるんです！ ハナコ・アラサカと約束があって！",
              "maleVariant": "",
              "stringId": "1721215632973774848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっぱりダメだ！",
              "maleVariant": "",
              "stringId": "1721263440690954240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この警備の中、どうやってたどり着く！？",
              "maleVariant": "",
              "stringId": "1724079058729320448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちくしょおおお！",
              "maleVariant": "",
              "stringId": "1781044245499494400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハナコ様のことか？ 邸宅の通信が遮断されたのかもしれん",
              "maleVariant": "",
              "stringId": "1804133045293301760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何…？ ハナコのこと？",
              "maleVariant": "何だ…？ ハナコのことか？",
              "stringId": "1804133996242681856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハナコ・アラサカと打ち合わせで…",
              "maleVariant": "",
              "stringId": "1804234795803230208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "通して。IOCのアバナシーの使いよ",
              "maleVariant": "通せ。IOCのアバナシーの使いだ",
              "stringId": "1804234795803230240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[攻撃する] Vよ",
              "maleVariant": "[攻撃する] Vだ",
              "stringId": "1804236419888070656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "Vよ",
              "maleVariant": "Vだ",
              "stringId": "1804237573908873216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこを動くな",
              "maleVariant": "",
              "stringId": "1804269961082847232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー… 何か問題でも…？",
              "maleVariant": "",
              "stringId": "1805271425938898944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やるな、なかなか見事な手際だ",
              "maleVariant": "",
              "stringId": "1805301516899205120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いったい何を考えてるんだ！？",
              "maleVariant": "",
              "stringId": "1805302056706129920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ、もう知るか！",
              "maleVariant": "",
              "stringId": "1805302953733537792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘルマン、お前はAVを回してくるんだ。退路を確保しておく必要がある",
              "maleVariant": "",
              "stringId": "1805305959908655104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、質問に答えろ",
              "maleVariant": "",
              "stringId": "1821038332974546944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞こえないのか？",
              "maleVariant": "",
              "stringId": "1821038335457574916"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何か問題が起きたんだ…！",
              "maleVariant": "",
              "stringId": "1825487404368007168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今は余計なことを考えずに、運転に集中しろ",
              "maleVariant": "",
              "stringId": "1825487611885391872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんなの聞いてないぞ…",
              "maleVariant": "",
              "stringId": "1825489469458747392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "文句を言いたいのはあんただけじゃない",
              "maleVariant": "",
              "stringId": "1825497457292611584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて、お楽しみはここからだ",
              "maleVariant": "",
              "stringId": "1825502554579931136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "武器を準備しておけ",
              "maleVariant": "",
              "stringId": "1825505270341439488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "君の運転手になった覚えはない！",
              "maleVariant": "",
              "stringId": "1825540085916766208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、起きたか",
              "maleVariant": "",
              "stringId": "1825544003950350336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "肝心の君がそんなぼんやりしてるようじゃ困るよ",
              "maleVariant": "",
              "stringId": "1825545128258703360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "運転してるのだね",
              "maleVariant": "",
              "stringId": "1825547272487571456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そもそもなぜ私が――",
              "maleVariant": "",
              "stringId": "1825548010668298240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "乱暴な運転ね。タイヤをダメにするよ…",
              "maleVariant": "乱暴な運転だな。タイヤをダメにするぞ…",
              "stringId": "1825549802223308800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まったく… なんなんだ！ 私は君の運転手じゃないんだぞ！",
              "maleVariant": "",
              "stringId": "1825551601562947584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつの間にか眠ってたのね",
              "maleVariant": "いつの間にか眠ってたようだ",
              "stringId": "1825554878690103296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカ社のリムジンとはいえ、検問の先には警備がいる",
              "maleVariant": "",
              "stringId": "1825560028188119040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるべく自然に振る舞うんだ",
              "maleVariant": "",
              "stringId": "1825560518804246528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく… 問題を起こしたくないならじっとしていろ。いいな",
              "maleVariant": "",
              "stringId": "1825560650455060480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく… 問題を起こしたくないならじっとしてろ。全員だぞ",
              "maleVariant": "",
              "stringId": "1825562622583246848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう着くぞ。彼女はなぜ連絡してこない？",
              "maleVariant": "",
              "stringId": "1825567536545927168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハナコ様と約束がある。いいから通してくれ。面倒なことになるのはご免だろ？",
              "maleVariant": "",
              "stringId": "1835608612879077376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "当面の間、この敷地は立ち入り禁止となっている",
              "maleVariant": "",
              "stringId": "1835642902975221760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブ・アラサカ様直々の指示だ",
              "maleVariant": "",
              "stringId": "1835644364824383488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知っている。だがその女は知らん",
              "maleVariant": "知っている。だがその男は知らん",
              "stringId": "1835644997174431744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "知っている。だが残りの連中は知らん",
              "maleVariant": "",
              "stringId": "1835645337399595008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最悪の展開ね… すぐに応援が駆け付けるはずよ",
              "maleVariant": "最悪の展開だな… すぐに応援が駆け付けるはずだ",
              "stringId": "1835649838072336384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何のつもりだ！",
              "maleVariant": "",
              "stringId": "1835666170255904768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと、ヘルマン。自然に振る舞うんじゃなかったの？",
              "maleVariant": "おいおい、ヘルマン。自然に振る舞うんじゃなかったのか？",
              "stringId": "1835671531968925696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やるじゃないか、運転手さん",
              "maleVariant": "",
              "stringId": "1835671602718445568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今度は何？",
              "maleVariant": "今度は何だ？",
              "stringId": "1835696967167905792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "力づくで突破するだけのことよ",
              "maleVariant": "力づくで突破するだけのことだ",
              "stringId": "1835697224899497984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何者だ？ 名を名乗れ",
              "maleVariant": "",
              "stringId": "1837435033293295616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何者だ？ 名を名乗れ",
              "maleVariant": "",
              "stringId": "1837435268442755072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やめろ！ 騒ぎを起こしにきたわけじゃない",
              "maleVariant": "",
              "stringId": "1841652185277796352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "IOCの者だ。アバナシーがプロトコル4を発令した。ナイトシティにある全施設の安全確認を行っている",
              "maleVariant": "",
              "stringId": "1841654617789566976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アバナシー？",
              "maleVariant": "",
              "stringId": "1841655046380326912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2日前に自殺した、あのアバナシーのことか？",
              "maleVariant": "",
              "stringId": "1841656351110860800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え？",
              "maleVariant": "何！？",
              "stringId": "1841656504605609984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "文句を言いたいのはあんただけじゃない",
              "maleVariant": "",
              "stringId": "1845795264662474752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "黙ってろ！",
              "maleVariant": "",
              "stringId": "1845799435058941952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うるさい！",
              "maleVariant": "",
              "stringId": "1845799776005525504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "連中の思惑など我々には関係ない",
              "maleVariant": "",
              "stringId": "1845828019257479168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、お前は俺と来い",
              "maleVariant": "",
              "stringId": "1845831056453668864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "勝手にしろ。私はAVの準備をする。ハナコは君に任せた",
              "maleVariant": "",
              "stringId": "1846005625500078080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいから黙って運転して",
              "maleVariant": "いいから黙って運転しろ",
              "stringId": "1846008199007571968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しっかり目を開けておけ。もうじき着く",
              "maleVariant": "",
              "stringId": "1846008792786800640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヨリノブ・アラサカ様直々の指示だと言ってるだろう！",
              "maleVariant": "",
              "stringId": "2000945336351006720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おいおい、私が誰だか知っているのか？",
              "maleVariant": "",
              "stringId": "2000945884512985088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[攻撃する]",
              "maleVariant": "",
              "stringId": "2079645422272462852"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}