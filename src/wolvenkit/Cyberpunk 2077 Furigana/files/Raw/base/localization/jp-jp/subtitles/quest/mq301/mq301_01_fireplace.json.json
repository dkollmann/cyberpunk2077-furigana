  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Ej ej, zakaz szwendania się!",
                    "maleVariant": null,
                    "stringId": 2959949919278239744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Wyluzuj, najpierw rozmawiamy, jak ludzie. A to pewnie też jakiś nowy rekrut.",
                    "maleVariant": null,
                    "stringId": 2959950872156999680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Tak, kazali mi się tu stawić.",
                    "maleVariant": null,
                    "stringId": 2959951428640477188
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Nie, z wojskiem mi nie do twarzy. Ale trzeba wiedzieć, kto rządzi w okolicy.",
                    "maleVariant": null,
                    "stringId": 2959952308655788032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]No chyba tak, kazali mi się tu stawić.",
                    "maleVariant": null,
                    "stringId": 2959952394404139012
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Nie, z wojskiem mi nie do twarzy.",
                    "maleVariant": null,
                    "stringId": 2959952845375705088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Pierdoli, już przecież podpadł.",
                    "maleVariant": null,
                    "stringId": 2959960470268055552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Dobra, bo nie wiesz o co chodzi w ogóle. Najpierw po łyku!",
                    "maleVariant": null,
                    "stringId": 2959964926078521344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Ale daj przynajmniej coś na wczutę!",
                    "maleVariant": null,
                    "stringId": 2959965284959948800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Uuu, tak, totalnie musisz się sztachnąć.",
                    "maleVariant": null,
                    "stringId": 2959965931771957248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]To jest niezły szajs, zobaczysz!",
                    "maleVariant": null,
                    "stringId": 2959966100970180608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl][Weź Smasha] Dobra.",
                    "maleVariant": null,
                    "stringId": 2959966727095881732
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl][Odmów] Nie, dzięki.",
                    "maleVariant": null,
                    "stringId": 2959967513880203264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Nie, dzięki, szajsu nie biorę.",
                    "maleVariant": null,
                    "stringId": 2959967773356625920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]E, to nie ma sensu. Nie wkręcisz się.",
                    "maleVariant": null,
                    "stringId": 2959968686406275072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][db_db]Until the player decides to take a whiff of Smash, the scene does not progress. Player can also walk away and leave them.",
                    "maleVariant": null,
                    "stringId": 2959969261848006656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Dobra, dawaj.",
                    "maleVariant": null,
                    "stringId": 2959972231432015872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Najlepsza decyzja, przekonasz się.",
                    "maleVariant": null,
                    "stringId": 2959973918750183432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Co to jest za towar...?",
                    "maleVariant": null,
                    "stringId": 2959974463707713536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Ale ja jeszcze żyję i nie zamierzam umierać, więc słuchaj: najlepszy dzień. Normalnie miękkie nogi i czekam. I teraz wyobraź sobie, że jesteś Kurtem, samym, kurwa, Kurtem, wchodzisz do lobby, a tam ten dupek Jurij...",
                    "maleVariant": null,
                    "stringId": 2959975399977029632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Służbista pierdolony. To co, nowa?",
                    "maleVariant": null,
                    "stringId": 2970723334339506176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]No, kurwa, zdecydowanie. I komu nie podpadać.",
                    "maleVariant": null,
                    "stringId": 2970728809684611072
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Taaa, tylko to pewnie już nie dzisiaj.",
                    "maleVariant": null,
                    "stringId": 2970740231764258816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Ale przypatrz się, jak wygląda twarz szczęściarza.",
                    "maleVariant": null,
                    "stringId": 2970740358398685184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Nie najlepiej.",
                    "maleVariant": null,
                    "stringId": 2970742509539766272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Ale w oczy się patrz! Tak wygląda ulga, że się wyrwałeś z gówna. Teraz droga jest już tylko w górę.",
                    "maleVariant": null,
                    "stringId": 2970743042786799616
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Dobra tam, będzie spoko, na razie siedzimy, fajnie jest, a jak się sytuacja rozwinie, to się zobaczy.",
                    "maleVariant": null,
                    "stringId": 2970846769434669056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Mhm, módl się, żeby nie skończyć gorzej niż Jurij. A pewnie się na ciebie uweźmie, żeby jego samego nie dojebali.",
                    "maleVariant": null,
                    "stringId": 2970870007992758272
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Paco, ale to chyba nie o ciebie chodzi. Nie nakręcaj się tak.",
                    "maleVariant": null,
                    "stringId": 3014401619711569920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Ale jestem ważną częścią zespołu, sama widziałaś.",
                    "maleVariant": null,
                    "stringId": 3014405277765414912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Zaraz zobaczysz. Tylko słuchaj mnie dobrze. To był najlepszy dzień w moim życiu, rozumiesz.",
                    "maleVariant": null,
                    "stringId": 3014410663369191424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[en_us][pl_pl]Mhm, znam jedną taką historię. Nie kończy się dobrze. Mój przyjaciel ginie.",
                    "maleVariant": null,
                    "stringId": 3014410895700078592
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}