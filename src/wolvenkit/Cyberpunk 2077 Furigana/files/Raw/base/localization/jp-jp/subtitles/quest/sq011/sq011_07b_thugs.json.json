      "Properties": {
        "entries": [
          {
            "stringId": 1801600868656308224,
            "femaleVariant": "ボスが誰だろうが、なんとかなる"
          },
          {
            "stringId": 1801605526548340736,
            "femaleVariant": "ロイス時代も生き抜いた。次はあの女ってだけだ"
          },
          {
            "stringId": 1801605828521451520,
            "femaleVariant": "こき使われるのはごめんだぜ、チューマ"
          },
          {
            "stringId": 1801606125864050688,
            "femaleVariant": "毎日ビクビクしながら生きてられるかって"
          },
          {
            "stringId": 1801606309608120320,
            "femaleVariant": "ああ、ただパトリシアは…"
          },
          {
            "stringId": 1801607085386588160,
            "femaleVariant": "おい！ ウソだろ！"
          },
          {
            "stringId": 1801607368149786624,
            "femaleVariant": "オールフーズの映像、お前も見たよな？"
          },
          {
            "stringId": 1801607657741312000,
            "femaleVariant": "あいつ、ロイスを殺った女だ！",
            "maleVariant": "あいつ、ロイスを殺った野郎だ！"
          },
          {
            "stringId": 1801608230398025732,
            "femaleVariant": "そうよ。ロイスが死んでせいせいしたでしょ？",
            "maleVariant": "そうだ。ロイスが死んでせいせいしただろ？"
          },
          {
            "stringId": 1801608578391040000,
            "femaleVariant": "何のことかさっぱり",
            "maleVariant": "何のことかさっぱりだ"
          },
          {
            "stringId": 1801609595929190400,
            "femaleVariant": "そうよ。ロイスが死んで、せいせいしたでしょ？",
            "maleVariant": "そうだ。ロイスが死んで、せいせいしただろ？"
          },
          {
            "stringId": 1801610345300320260,
            "femaleVariant": "いったい何の話よ",
            "maleVariant": "いったい何の話だ"
          },
          {
            "stringId": 1801610346457948160,
            "femaleVariant": "間違いねえ、こいつだ"
          },
          {
            "stringId": 1801610386488385540,
            "femaleVariant": "ベス・アイシスはどこ",
            "maleVariant": "ベス・アイシスはどこだ"
          },
          {
            "stringId": 1801610387780231168,
            "femaleVariant": "質問してんのはこっちだ"
          },
          {
            "stringId": 1801611910899785728,
            "femaleVariant": "お前ら、どきな"
          },
          {
            "stringId": 1801618118352265216,
            "femaleVariant": "何の用だ？"
          },
          {
            "stringId": 1801620272530022404,
            "femaleVariant": "ベス・アイシスという名に心当たりは？"
          },
          {
            "stringId": 1801620548330676224,
            "femaleVariant": "あんたがここのボス？",
            "maleVariant": "あんたがここのボスか？"
          },
          {
            "stringId": 1801621581589078016,
            "femaleVariant": "そいつには用心しろ"
          },
          {
            "stringId": 1801622689673863168,
            "femaleVariant": "ベス・アイシス――心当たりは？"
          },
          {
            "stringId": 1801623233020780544,
            "femaleVariant": "女の仲間か？"
          },
          {
            "stringId": 1801623386448420864,
            "femaleVariant": "話があるだけ。ここにいるんでしょ",
            "maleVariant": "話があるだけだ。ここにいるんだろ"
          },
          {
            "stringId": 1801623933888978948,
            "femaleVariant": "今はあんたがここのボス？",
            "maleVariant": "今はあんたがここのボスか？"
          },
          {
            "stringId": 1801623933888978964,
            "femaleVariant": "ああ。驚いたか？"
          },
          {
            "stringId": 1801626200188870656,
            "femaleVariant": "話、ねえ"
          },
          {
            "stringId": 1801630897238925312,
            "femaleVariant": "ついてきな"
          },
          {
            "stringId": 1801646908088475648,
            "femaleVariant": "このハコで演るのもいいな"
          },
          {
            "stringId": 1801657186012323840,
            "femaleVariant": "だから言ってるでしょ…"
          },
          {
            "stringId": 1801657865590235136,
            "femaleVariant": "音楽メディアだろ。聞いたよ"
          },
          {
            "stringId": 1801658062957404160,
            "femaleVariant": "質問を変えるか。いったい誰の差し金だ？"
          },
          {
            "stringId": 1801660247518699520,
            "femaleVariant": "トンダー、女はちゃんと調べたのか？"
          },
          {
            "stringId": 1801662583611809796,
            "femaleVariant": "ひでえことしやがる"
          },
          {
            "stringId": 1801667891922952192,
            "femaleVariant": "殺せ！"
          },
          {
            "stringId": 1801693663538667520,
            "femaleVariant": "逃がすな！"
          },
          {
            "stringId": 1801698978946572292,
            "femaleVariant": "そこのエレベーターへ。早く！"
          },
          {
            "stringId": 1801700446986526720,
            "femaleVariant": "ああ、もう"
          },
          {
            "stringId": 1801702090147061764,
            "femaleVariant": "ナンシーだ"
          },
          {
            "stringId": 1801702995428859908,
            "femaleVariant": "で、あなたは？"
          },
          {
            "stringId": 1801702995445637124,
            "femaleVariant": "V。ケリー・ユーロダインの使いよ",
            "maleVariant": "V。ケリー・ユーロダインの使いだ"
          },
          {
            "stringId": 1801702995495968780,
            "femaleVariant": "ケリー？ もう、頭がおかしくなりそう"
          },
          {
            "stringId": 1801706033765900288,
            "femaleVariant": "きっと妄想癖のあるパトリシアのせいね"
          },
          {
            "stringId": 1801707350005604356,
            "femaleVariant": "ここから出るぞ"
          },
          {
            "stringId": 1801713286036283396,
            "femaleVariant": "外に車が"
          },
          {
            "stringId": 1801713286237609988,
            "femaleVariant": "このエレベーター、遅すぎでしょ…"
          },
          {
            "stringId": 1801713286640263168,
            "femaleVariant": "大丈夫？",
            "maleVariant": "大丈夫か？"
          },
          {
            "stringId": 1801713286640263176,
            "femaleVariant": "冷や冷やしたわ"
          },
          {
            "stringId": 1801713286824812548,
            "femaleVariant": "行きましょ。車が停めた場所にまだあるといいけど"
          },
          {
            "stringId": 1801751340436381700,
            "femaleVariant": "「オルタナティブ」ってのが、他人がどう思おうがクソくらえって意味なら間違っちゃいねえ"
          },
          {
            "stringId": 1801751340436381708,
            "femaleVariant": "それなら… エクストリームミュージックはどう？"
          },
          {
            "stringId": 1801751340453158916,
            "femaleVariant": "何かピンとくる？"
          },
          {
            "stringId": 1801751340553822212,
            "femaleVariant": "ブリック。また客だよ"
          },
          {
            "stringId": 1801763933716701184,
            "femaleVariant": "おい、Vじゃねえか。急にどうした？"
          },
          {
            "stringId": 1801765974816022532,
            "femaleVariant": "ケリー・ユーロダインに頼まれたの。ベスに話がある",
            "maleVariant": "ケリー・ユーロダインに頼まれた。ベスに話がある"
          },
          {
            "stringId": 1801765974816022544,
            "femaleVariant": "ケリー？ 今さら私に何の用よ"
          },
          {
            "stringId": 1801765974866354180,
            "femaleVariant": "行っていいぞ"
          },
          {
            "stringId": 1801770804372529156,
            "femaleVariant": "ナンシーだ"
          },
          {
            "stringId": 1801773722920247296,
            "femaleVariant": "ありがとう、ブリック。とても… 面白かったわ"
          },
          {
            "stringId": 1801773884283510784,
            "femaleVariant": "行きましょ"
          },
          {
            "stringId": 1801776045860020224,
            "femaleVariant": "さっさと出るぞ"
          },
          {
            "stringId": 1807306642497953792,
            "femaleVariant": "一体どうやって助けを呼んだんだ？"
          },
          {
            "stringId": 1807318513636564992,
            "femaleVariant": "またあたしらを罠にはめようってんだろ？ 無駄だよ"
          },
          {
            "stringId": 1807413588576133124,
            "femaleVariant": "今はブリックがしきってるのね",
            "maleVariant": "今はブリックがしきってるのか"
          },
          {
            "stringId": 1807413588576133132,
            "femaleVariant": "ああ。オールフーズでの話は聞いた"
          },
          {
            "stringId": 1807413588576133140,
            "femaleVariant": "ブリックに感謝されると思うなよ？"
          },
          {
            "stringId": 1807413588592910336,
            "femaleVariant": "何か不満でもあるの？",
            "maleVariant": "何か不満でもあるのか？"
          },
          {
            "stringId": 1807436070615998464,
            "femaleVariant": "は？ 何言ってんだ！"
          },
          {
            "stringId": 1807436636847038464,
            "femaleVariant": "文句あんのか？"
          },
          {
            "stringId": 1807437353485819904,
            "femaleVariant": "ないよな、ならいい"
          },
          {
            "stringId": 1810061699215745024,
            "femaleVariant": "わかった。まずは出ましょ。私についてきて"
          },
          {
            "stringId": 1810102237851770880,
            "femaleVariant": "仕事の邪魔をするな"
          },
          {
            "stringId": 1810103935437594624,
            "femaleVariant": "ブリックはまともでしょ",
            "maleVariant": "ブリックはまともだろ"
          },
          {
            "stringId": 1810104934252371968,
            "femaleVariant": "そうね。音楽をまるでわかってないのが残念だけど"
          },
          {
            "stringId": 1810105550009753600,
            "femaleVariant": "彼とそんなに話がしたかったの？",
            "maleVariant": "奴とそんなに話がしたかったのか？"
          },
          {
            "stringId": 1810108275770482688,
            "femaleVariant": "本当の目的は何だったの？",
            "maleVariant": "本当の目的は何だったんだ？"
          },
          {
            "stringId": 1810110179128860672,
            "femaleVariant": "トーテンタンツの音楽を特集したかったのよ"
          },
          {
            "stringId": 1810110487611531264,
            "femaleVariant": "それじゃパトリシアが疑うのも無理ないね",
            "maleVariant": "それじゃパトリシアが疑うのも無理ないな"
          },
          {
            "stringId": 1844437918495297536,
            "femaleVariant": "お前らがどう言おうと、少なくともロイスにはカリスマがある"
          },
          {
            "stringId": 1844440466719526912,
            "femaleVariant": "そのカリスマで、何か得したか？"
          },
          {
            "stringId": 1844448816320499712,
            "femaleVariant": "うーん… さあな…"
          },
          {
            "stringId": 1844455912562774016,
            "femaleVariant": "俺があのイカれたボスから貰ったもんと言えば――"
          },
          {
            "stringId": 1844456393079017472,
            "femaleVariant": "この膝くらいだ。前にロイスに撃たれた。大損だよ"
          },
          {
            "stringId": 1844472931421016064,
            "femaleVariant": "いや、ロイスよかマシか"
          },
          {
            "stringId": 1844473446666096640,
            "femaleVariant": "そうか？ 俺からしたらロイス以上だけどな"
          },
          {
            "stringId": 1844491914606039040,
            "femaleVariant": "まさか。自分からノコノコやって来るバカがいるかよ"
          },
          {
            "stringId": 1844492986066161664,
            "femaleVariant": "なあ、お前…"
          },
          {
            "stringId": 1844502773273219072,
            "femaleVariant": "ベス・アイシスを探してる"
          },
          {
            "stringId": 1844524425327431680,
            "femaleVariant": "飛んで火にいる夏の虫じゃねえか"
          },
          {
            "stringId": 1844526337426739200,
            "femaleVariant": "こいつはオールフーズの…"
          },
          {
            "stringId": 1844528301568970752,
            "femaleVariant": "わかってるよ"
          },
          {
            "stringId": 1845761333227442180,
            "femaleVariant": "おい"
          },
          {
            "stringId": 1845761333260996612,
            "femaleVariant": "どうなんだ？"
          },
          {
            "stringId": 1845761333294551044,
            "femaleVariant": "お前に聞いてんだよ"
          },
          {
            "stringId": 1845789547303817216,
            "femaleVariant": "今はあんたがここのボス？",
            "maleVariant": "今はあんたがここのボスか？"
          },
          {
            "stringId": 1845796312045080576,
            "femaleVariant": "いるけど、ブリックに話を通すんだね"
          },
          {
            "stringId": 1845838784842452992,
            "femaleVariant": "いや、違う。ロイスが死んで上は変わったけどね"
          },
          {
            "stringId": 1845857388006756352,
            "femaleVariant": "すべてはボス次第だ"
          },
          {
            "stringId": 1845867117718294528,
            "femaleVariant": "ヴァレンティーノズ、それともコーポか"
          },
          {
            "stringId": 1845867372060889088,
            "femaleVariant": "私に何を頼むってのよ？"
          },
          {
            "stringId": 1845867582027747328,
            "femaleVariant": "それを聞いてんだ"
          },
          {
            "stringId": 1845874729054654464,
            "femaleVariant": "はあ？ 助けなんか呼んでない！ 知らない女よ！",
            "maleVariant": "はあ？ 助けなんか呼んでない！ 知らない男よ！"
          },
          {
            "stringId": 1845875595698524160,
            "femaleVariant": "どうだかな"
          },
          {
            "stringId": 1845886349273223168,
            "femaleVariant": "ありがとう"
          },
          {
            "stringId": 1845896226120945664,
            "femaleVariant": "〈Samurai〉のことよ。後で話す",
            "maleVariant": "〈Samurai〉のことだ。後で話す"
          },
          {
            "stringId": 1845903407088160768,
            "femaleVariant": "それで… 取材はおしまい？",
            "maleVariant": "それで… 取材はしまいか？"
          },
          {
            "stringId": 1845905835187523584,
            "femaleVariant": "あの妄想女、私をスパイだと思ったのね"
          },
          {
            "stringId": 1845914907114754048,
            "femaleVariant": "ブリックは安牌だった"
          },
          {
            "stringId": 1845932246333935616,
            "femaleVariant": "別に。あの男は、あんたのことを何とも思っちゃいないよ"
          },
          {
            "stringId": 1845940711261491200,
            "femaleVariant": "あんた、よっぽど俺たちにレッテルを貼りたいんだな"
          },
          {
            "stringId": 1845941791579983872,
            "femaleVariant": "理解しようとしてるのよ。カオスに、ノイズ、それにディストーション…"
          },
          {
            "stringId": 1845942621330755584,
            "femaleVariant": "そこには何らかのスタイルがあるはず"
          },
          {
            "stringId": 1845989793107562500,
            "femaleVariant": "ケリー・ユーロダインの使いよ",
            "maleVariant": "ケリー・ユーロダインの使いでな"
          },
          {
            "stringId": 1845990839502524416,
            "femaleVariant": "取り込み中みたいね",
            "maleVariant": "取り込み中みたいだな"
          },
          {
            "stringId": 1845991305489698816,
            "femaleVariant": "あんたの手下はまだ怒ってるみたい",
            "maleVariant": "あんたの手下はまだ怒ってるみたいだ"
          },
          {
            "stringId": 1845994443265298432,
            "femaleVariant": "まだ恨まれてるみたいね",
            "maleVariant": "まだ恨まれてるみたいだな"
          },
          {
            "stringId": 1845995083886514176,
            "femaleVariant": "ハッ… 銃を持つより“根に”持つのが得意だからな"
          },
          {
            "stringId": 1845995595658711040,
            "femaleVariant": "話の邪魔をしたようね",
            "maleVariant": "話の邪魔をしたようだな"
          },
          {
            "stringId": 1846081803302752256,
            "femaleVariant": "もう切り上げるところだ。さすがに十分だろ"
          },
          {
            "stringId": 1847228034058047488,
            "femaleVariant": "でもまだ聞きたいことが山ほどあるのよ"
          },
          {
            "stringId": 1847231540227756032,
            "femaleVariant": "適当にでっちあげろよ。ジャーナリストならお手の物だろ"
          },
          {
            "stringId": 1847231929996038144,
            "femaleVariant": "“狂乱のノイズ”とか、なんでもいい"
          },
          {
            "stringId": 1847312521148256256,
            "femaleVariant": "本命はブリックじゃなくて〈Tinnitus〉、トーテンタンツの音楽集団よ"
          },
          {
            "stringId": 1847319086005768192,
            "femaleVariant": "メイルストロームの音楽性に迫ってたんでしょ？",
            "maleVariant": "メイルストロームの音楽性に迫ってたんだろ？"
          },
          {
            "stringId": 1847320634190483456,
            "femaleVariant": "映像をたっぷり撮ったから、それを見返して、繋ぎ合わせるの"
          },
          {
            "stringId": 1847324286271639552,
            "femaleVariant": "〈Tinnitus〉の連中と話したかったのに"
          },
          {
            "stringId": 1847325994192228352,
            "femaleVariant": "ツテでもあったの？",
            "maleVariant": "ツテでもあったのか？"
          },
          {
            "stringId": 1847326255111491584,
            "femaleVariant": "ブリックがライブに招待してくれたの"
          },
          {
            "stringId": 1847327066340216832,
            "femaleVariant": "でもメンバーが変わるのよ… 毎回ね"
          },
          {
            "stringId": 1912761110846263296,
            "femaleVariant": "お前もあまり… 挑発しないようにしておけよ"
          },
          {
            "stringId": 1912799069431681024,
            "femaleVariant": "エブニケの警備を強化した方がいい"
          },
          {
            "stringId": 1912799571556978688,
            "femaleVariant": "エブニケの警備、甘すぎよ",
            "maleVariant": "エブニケの警備、甘すぎないか？"
          },
          {
            "stringId": 1912800479271473152,
            "femaleVariant": "この前お邪魔したんだけど… おたくの連中は話にならなかった",
            "maleVariant": "この前お邪魔したんだが… おたくの連中は話にならなかった"
          },
          {
            "stringId": 1912801037097127936,
            "femaleVariant": "だったら、また行ってみるといい"
          },
          {
            "stringId": 1912811616876646400,
            "femaleVariant": "丁重におもてなししてやるからよ"
          },
          {
            "stringId": 1912813304530358272,
            "femaleVariant": "落ち着いて、ただの老婆心よ",
            "maleVariant": "落ち着けって、ただの老婆心だ"
          },
          {
            "stringId": 1912813562362613760,
            "femaleVariant": "そりゃどうも"
          },
          {
            "stringId": 1976576730003689492,
            "femaleVariant": "あんたのことはよく知らない"
          },
          {
            "stringId": 1976576730020466700,
            "femaleVariant": "これから知ることになる"
          },
          {
            "stringId": 1976577915548880896,
            "femaleVariant": "いや、あんたの噂は聞いてたから",
            "maleVariant": "いや、あんたの噂は聞いてたしな"
          },
          {
            "stringId": 1976578721526337536,
            "femaleVariant": "さぞかし良い噂だったろうね"
          },
          {
            "stringId": 1976580088567451648,
            "femaleVariant": "ロイスが死んだ？"
          },
          {
            "stringId": 1976580679226118144,
            "femaleVariant": "ミリテクさ。あんたのおかげだ"
          },
          {
            "stringId": 1976651223074099204,
            "femaleVariant": "おいおい！ ウソだろ！"
          },
          {
            "stringId": 1976651223074099216,
            "femaleVariant": "オールフーズの映像、見ただろ？"
          },
          {
            "stringId": 1976651223074099228,
            "femaleVariant": "スパイダーボットを盗んだ女だ！",
            "maleVariant": "スパイダーボットを盗んだ野郎だ！"
          },
          {
            "stringId": 1976651223090876432,
            "femaleVariant": "まさか。自分からノコノコやって来るバカがいるかよ"
          },
          {
            "stringId": 1976651223107653632,
            "femaleVariant": "なあ、お前…"
          },
          {
            "stringId": 1976655141694885888,
            "femaleVariant": "ええ。もう1つをくれるの？",
            "maleVariant": "ああ。もう1つくれるのか？"
          },
          {
            "stringId": 1976655976797589504,
            "femaleVariant": "ええ。なに、もう1つくれるの？",
            "maleVariant": "ああ。なんだ、もう1つくれるのか？"
          },
          {
            "stringId": 1976656812319723520,
            "femaleVariant": "前のは捨てちゃったから",
            "maleVariant": "前のは捨てちまってな"
          },
          {
            "stringId": 1976657583803224064,
            "femaleVariant": "ナメやがって…"
