    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1704108437404663816,
            "femaleVariant": "V、今いいか？ 話がある"
          },
          {
            "stringId": 1704108437421441024,
            "femaleVariant": "そいつは今、ナイトシティにいる。大物と言われる連中にツテを持つ男さ"
          },
          {
            "stringId": 1705567568355409920,
            "femaleVariant": "幸運にも、アラサカ時代の古い友人と接触できたんだ"
          },
          {
            "stringId": 1712357015848620032,
            "femaleVariant": "わかった。どこで落ち合えばいい？"
          },
          {
            "stringId": 1712357529952849920,
            "femaleVariant": "信用できる相手なの？",
            "maleVariant": "信用できる相手なのか？"
          },
          {
            "stringId": 1712358218322022400,
            "femaleVariant": "この街じゃ、人を不用意に信用すると痛い目を見る。それが“古い友人”だとしてもね",
            "maleVariant": "この街じゃ、人を不用意に信用すると痛い目を見る。それが“古い友人”だとしてもな"
          },
          {
            "stringId": 1712358437197582336,
            "femaleVariant": "わかった。いつどこで落ち合うの？",
            "maleVariant": "わかった。いつどこで落ち合う？"
          },
          {
            "stringId": 1712358983581175808,
            "femaleVariant": "日没後、ジャパンタウンで。今、具体的な場所の情報を送る"
          },
          {
            "stringId": 1712359226481709056,
            "femaleVariant": "では現地で会おう"
          },
          {
            "stringId": 1712360217528635392,
            "femaleVariant": "そいつとは、何度も命を取り合おうとしたことがある"
          },
          {
            "stringId": 1712363658518859776,
            "femaleVariant": "大丈夫だ… 信用できる"
          },
          {
            "stringId": 1712366752489148416,
            "femaleVariant": "どんな話？",
            "maleVariant": "どんな話だ？"
          },
          {
            "stringId": 1712366985994440704,
            "femaleVariant": "例の件だが… 解決策が見つかったかもしれない"
          },
          {
            "stringId": 1712371151290634240,
            "femaleVariant": "それが何の役に立つの？",
            "maleVariant": "それが何の役に立つ？"
          },
          {
            "stringId": 1712371151290634256,
            "femaleVariant": "もったいぶらずに要点を言って",
            "maleVariant": "もったいぶらずに要点を言え"
          },
          {
            "stringId": 1712371927404646400,
            "femaleVariant": "それがいったい何の役に立つの？",
            "maleVariant": "それがいったい何の役に立つんだ？"
          },
          {
            "stringId": 1712372580776546304,
            "femaleVariant": "大物へのツテがある古い友人ね…"
          },
          {
            "stringId": 1712372923233079296,
            "femaleVariant": "もったいぶらずに要点を言って",
            "maleVariant": "もったいぶらずに要点を言え"
          },
          {
            "stringId": 1712373188162097152,
            "femaleVariant": "前にも言った通り、俺はあらゆる選択肢を探り続けていたんだ"
          },
          {
            "stringId": 1712375251809996800,
            "femaleVariant": "話し合いの場を設ける、その友人と俺たち2人のな"
          },
          {
            "stringId": 1712378750832611328,
            "femaleVariant": "お前は紺碧プラザで起こった出来事の真相を、もう一度初めから話してくれ"
          },
          {
            "stringId": 1712380843790307328,
            "femaleVariant": "そいつは本当に信用して大丈夫なの？",
            "maleVariant": "そいつは本当に信用して大丈夫なのか？"
          },
          {
            "stringId": 1712382423767846912,
            "femaleVariant": "信じてくれる可能性は低いが、何はともあれ足がかりにはなるだろう"
          }
        ]
      }
    }
  }
}