      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見せて",
              "maleVariant": "見せてくれ",
              "stringId": "1003746643229827072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V！ お前のかわいい45口径がママをお待ちかねだ！ 見てかないか？",
              "maleVariant": "V！ お前のかわいい45口径がパパをお待ちかねだ！ 見てかないか？",
              "stringId": "1066441592860774400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうだ、すごい改造銃だろ。ん？ 試しに撃ってみるか？",
              "maleVariant": "",
              "stringId": "1729862937493008384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろん",
              "maleVariant": "",
              "stringId": "1729872039636004868"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "遠慮しとく",
              "maleVariant": "",
              "stringId": "1729872524833091584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろん、やるに決まってる",
              "maleVariant": "",
              "stringId": "1729876797939675136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうこなくっちゃ！ “ホットプレートがつけっぱなしで”とか“ドールハウスを予約してて”みたいなくだらない言い訳はなしだ！",
              "maleVariant": "",
              "stringId": "1729877965365473280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今は遠慮しとく",
              "maleVariant": "",
              "stringId": "1729878191639785472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいのか？ いい練習になるし、ストレスも解消できるぞ… 気が向いたらいつでも来てくれよな",
              "maleVariant": "",
              "stringId": "1729878573371781120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ルールに軽く目を通したら、あとはぶっ放し放題だ",
              "maleVariant": "",
              "stringId": "1729884885413425152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よーう、Vじゃねえか！",
              "maleVariant": "",
              "stringId": "1735505249602531328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "新しい銃があんたのお気に召すといいんだがな！",
              "maleVariant": "",
              "stringId": "1744351249255526400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "売り物を見せて",
              "maleVariant": "売り物を見せてくれ",
              "stringId": "1744352629651304452"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "射撃場は使える？",
              "maleVariant": "射撃場は使えるか？",
              "stringId": "1744363878506196992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここ1、2… いや、数週間見かけなかったな。街を出たのかと思ったぜ",
              "maleVariant": "",
              "stringId": "1764867286410747904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただのトラブルよ。よくあるね",
              "maleVariant": "ただのトラブルだ。よくあるな",
              "stringId": "1764869259478458368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティはそう簡単に離れられないよ",
              "maleVariant": "ナイトシティはそう簡単に離れられないさ",
              "stringId": "1764869333499535360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、見せてちょうだい",
              "maleVariant": "ああ、見せてもらおうか",
              "stringId": "1764870677958516736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また今度ね。今日はちょっと急いでるの",
              "maleVariant": "また今度な。今日はちょっと急いでるんだ",
              "stringId": "1764870853129428992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい装備がありゃ片付くんじゃねえか？ どうだい？",
              "maleVariant": "",
              "stringId": "1764877314085642240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ズルズル続く恋愛みたいなもんか",
              "maleVariant": "",
              "stringId": "1764878794238746624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはそうと、買い物でもどうだ？ 新しく仕入れた品を見てってくれよ",
              "maleVariant": "",
              "stringId": "1764879548844367872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっとトラブルでね",
              "maleVariant": "ちょっとトラブルでな",
              "stringId": "1886483207627632640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティを離れるのは簡単じゃない",
              "maleVariant": "",
              "stringId": "1886483207627632656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ見せて",
              "maleVariant": "じゃあ見せてくれ",
              "stringId": "1886483207677964288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はいい",
              "maleVariant": "",
              "stringId": "1886483207677964304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見せてよ",
              "maleVariant": "見せてくれ",
              "stringId": "1983595561550663680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それじゃ、何があるか見せて",
              "maleVariant": "それじゃ、何があるか見せてくれ",
              "stringId": "1983606839463694336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その一言を待ってたよ",
              "maleVariant": "",
              "stringId": "1983607286173847552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "射撃場を使わせてもらってもいい？",
              "maleVariant": "射撃場を使わせてもらってもいいか？",
              "stringId": "1983608114095902720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろんだ。お得意様はタダで使ってくれ",
              "maleVariant": "",
              "stringId": "1983609112038264832"
            }
          ]
        }
      }
    },
