  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「二度と会えないかも」なんて書くなよ。バカじゃないのか？",
              "maleVariant": "",
              "stringId": 1967644505896931328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "彼女、コロラドの仕事をやめてこっちに来たがってるんだ。でも明日には“こっち”なんてなくなってるかもしれない！",
              "maleVariant": "",
              "stringId": 1967655489554370560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだわかんないでしょ。でもそんな手紙を送ったら、彼女は最悪の事態を考える。それでいいわけ？",
              "maleVariant": "",
              "stringId": 1967655795587567616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "センサーは高度が上がると誤作動を起こす",
              "maleVariant": "",
              "stringId": 1967659495014027264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺が一番たくさんの星を見たのはどこだと思う？ シナイ山の頂上だ",
              "maleVariant": "",
              "stringId": 1967668889063804932
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このフレーズはいいな。「野蛮な者は旅をするのではなく、ただひたすらに目的地を目指し、その土地から奪う」",
              "maleVariant": "",
              "stringId": 1967669232594079748
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "嘘じゃない、確実に聞こえた",
              "maleVariant": "",
              "stringId": 1967670909074165764
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "終わるまで待っててくれ",
              "maleVariant": "",
              "stringId": 1967678719925178372
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうは言っても、低空を飛ばすわけにもいかないしな",
              "maleVariant": "",
              "stringId": 1967717873014820864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "センサーは私が見るから、スラスターをお願い。何か緩んでるのかも",
              "maleVariant": "",
              "stringId": 1967718278604017664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "差動装置は大丈夫？ また軋んだら困るよ",
              "maleVariant": "",
              "stringId": 1967721786552111108
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫だよ、油を差したから",
              "maleVariant": "",
              "stringId": 1967721788951252992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ、位置について…",
              "maleVariant": "",
              "stringId": 1967721791216177152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やった！",
              "maleVariant": "",
              "stringId": 1967722596891643904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから確認したって！ いちいちうるさいな",
              "maleVariant": "",
              "stringId": 1967723580237828096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レディ…",
              "maleVariant": "",
              "stringId": 1967729477798350848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゴー！",
              "maleVariant": "",
              "stringId": 1967729535461642240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ、天の川銀河には最低1千億の星があるって言われてるからな",
              "maleVariant": "",
              "stringId": 1967740485078110208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "著者はよくわかってる。本音を書いてるんだ",
              "maleVariant": "",
              "stringId": 1967742566845403136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まあ… 歴史とは皮肉なものだ。そうだろ？ 今のこの国を見てみろ",
              "maleVariant": "",
              "stringId": 1967745052893270016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だから俺たちは、この荒野にいる。あそこじゃなくてな",
              "maleVariant": "",
              "stringId": 1967746559940251648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ラフェンの無線でも拾ったんじゃないか？ ミリテクはモジュラー周波数を使う。やるだけ無駄だ",
              "maleVariant": "",
              "stringId": 1967773149982117888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴らのジャマーを突破して、向こうのランナーに無線を焼かれず、こっちの位置も特定されずに済んだら、酒をおごってやるよ",
              "maleVariant": "",
              "stringId": 1967773661167112192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ポンコツにいくら手を加えたって、ポンコツのままだと思うけど",
              "maleVariant": "",
              "stringId": 1967774098146480128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そこ、少しずれてるぞ…",
              "maleVariant": "",
              "stringId": 1967774813560524800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "邪魔をするなら、よそをあたってくれないか？",
              "maleVariant": "",
              "stringId": 1967776311715581952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ何を書けって言うんだよ？",
              "maleVariant": "",
              "stringId": 1997880898559275008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "寂しい、愛してる、もうすぐ会える、とか色々あるだろ",
              "maleVariant": "",
              "stringId": 1997882319673356288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "先のことなんてわからないんだから、嘘にはならないでしょ",
              "maleVariant": "",
              "stringId": 1997882995862269952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "明日、建設現場の上空を飛ばすのよ。万が一は許されない",
              "maleVariant": "",
              "stringId": 1997887911100018688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ベアリングは？",
              "maleVariant": "",
              "stringId": 1997913449210441728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エジプトに行ったことあるの？",
              "maleVariant": "",
              "stringId": 1997920382193627136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昔タンカーで働いてて、あちこち行かせてもらった。でもシナイ山の夜空は格別さ… 無数の星に手が届きそうだった",
              "maleVariant": "",
              "stringId": 1997921092205408256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "宇宙人って… いると思う？",
              "maleVariant": "",
              "stringId": 1997923697119850496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちの銀河だけで考えても、生命が存在できる可能性のある星はいくらでもある。宇宙全体で考えりゃもっとだ",
              "maleVariant": "",
              "stringId": 1997927042681270272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあなぜ誰もコンタクトしてこないの？",
              "maleVariant": "",
              "stringId": 1997927934994280448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちなんかに興味ないんだろ。周りを見ろ。こんなとこ来たいか？",
              "maleVariant": "",
              "stringId": 1997928268911210496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあもし俺が連中の信号をつかまえて、現場の詳細がわかったらどうする？",
              "maleVariant": "",
              "stringId": 1997933306958180352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハッ、約束だぞ",
              "maleVariant": "",
              "stringId": 1997935529872191488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これはどうだ？ 「危険を顧みず動くものは、留まるものに負けはしない。変化には、停滞に勝る気高さがあるからだ」",
              "maleVariant": "",
              "stringId": 1997960977687638016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "著者の世界が私たちの置かれた境遇と似てたからよ。断片化し、バラバラに分裂してた",
              "maleVariant": "",
              "stringId": 2003821069955887104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昔はヨーロッパ人をあざ笑う人が多かった。国と言語に加えて、文化もバラバラだって… 一方、こっちは“人種のるつぼ”だったからね",
              "maleVariant": "",
              "stringId": 2003823088607293440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この国って、どの国だよ？",
              "maleVariant": "",
              "stringId": 2003825544842047488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういうことだ",
              "maleVariant": "",
              "stringId": 2003825669983301632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "「流れさすらう心と、幻想を持ってこそ、文明に生きる者だ」",
              "maleVariant": "",
              "stringId": 2003828711373795328
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}