    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "BDってすごいでしょ？ ここで扱うヤツは、残念ながらほとんど“おひとり様”用だけどね…",
                    "maleVariant": null,
                    "stringId": 1185263910797586432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "んじゃ、そろそろ本番に行くよ？",
                    "maleVariant": null,
                    "stringId": 1213716969789452288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうね… よし… じゃあT-バグとリンクして始めましょ",
                    "maleVariant": "そうだな… よし… じゃあT-バグとリンクして始めよう",
                    "stringId": 1451548901049495552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと、誰よそれ？",
                    "maleVariant": null,
                    "stringId": 1451549633173647360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "うちのランナー。セキュリティ専門。分析に協力してもらう。別に構わないでしょ？",
                    "maleVariant": "仲間のランナーだ。セキュリティ専門。分析に協力してもらう。別に構わないだろ？",
                    "stringId": 1451549807153377280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いや、構うんだけど",
                    "maleVariant": null,
                    "stringId": 1451550335568572416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "T-バグはプロよ。それも意識が高すぎて困るくらいの。信用できる",
                    "maleVariant": "T-バグはプロだ。それも意識が高すぎて困るくらいのな。信用できる",
                    "stringId": 1451552074929664000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大丈夫よ。何も問題ないから",
                    "maleVariant": "大丈夫だ。何も問題ない",
                    "stringId": 1451563456525889536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと、ジュディ…",
                    "maleVariant": null,
                    "stringId": 1451574859127189504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あのね、あなたをここに入れるだけでも、私は大きなリスクを冒してるのよ",
                    "maleVariant": null,
                    "stringId": 1451583776116830208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "リスクがあるのはこっちも同じ。それが仕事でしょ。バグを繋いでくれないなら、あたしは降りる",
                    "maleVariant": "リスクがあるのはこっちも同じだ。それが仕事だろ。T-バグを繋いでくれないなら、俺は降りる",
                    "stringId": 1451592411366682624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジュディ、お願い",
                    "maleVariant": null,
                    "stringId": 1451593349481492480
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、どうしたの？",
                    "maleVariant": null,
                    "stringId": 1451624189292834816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バグ、聞いて。紺碧プラザの情報が手に入った… ブレインダンスよ",
                    "maleVariant": "バグ、聞いてくれ。紺碧プラザの情報が手に入った… ブレインダンスだ",
                    "stringId": 1451624522488344576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "紺碧？ そんなことかと思った",
                    "maleVariant": null,
                    "stringId": 1451634783836303360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "誰かと一緒なの？",
                    "maleVariant": null,
                    "stringId": 1451636251909812224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジュディとエヴリン。大丈夫よ、BDのことで手伝ってもらってるの",
                    "maleVariant": "ジュディとエヴリン。大丈夫だ、BDのことで手伝ってもらってる",
                    "stringId": 1451638633754390528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたの力を借りたいの。ダイブしたら何に注意すべきか、助言して",
                    "maleVariant": "お前の力を借りたいんだ。ダイブしたら何に注意すべきか、助言してくれ",
                    "stringId": 1451639974774042624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それは関係ない。とにかく、バグの力が必要なの。ダイブしたら何に注意すべきか、アドバイスして",
                    "maleVariant": "それは関係ない。とにかく、お前の力が必要なんだ。ダイブしたら何に注意すべきか、アドバイスしてくれ",
                    "stringId": 1451640636836540416
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。やってみようか",
                    "maleVariant": null,
                    "stringId": 1451641395955232768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ふーん、そう… で、肝心の用件は？",
                    "maleVariant": null,
                    "stringId": 1451641916367695872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "接続開始",
                    "maleVariant": null,
                    "stringId": 1451648277465919488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "T-バグは信用できる",
                    "maleVariant": null,
                    "stringId": 1461630767458807808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大丈夫よ。何も問題ないから",
                    "maleVariant": "大丈夫だ。何も問題ない",
                    "stringId": 1461631500237271040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だからなんなのよ",
                    "maleVariant": null,
                    "stringId": 1461648515689738240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あなたを危険にさらすようなことはしない。わかるでしょ…",
                    "maleVariant": null,
                    "stringId": 1461649517104984064
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "エヴリン… T-バグがいないと役に立つ情報は得られない",
                    "maleVariant": null,
                    "stringId": 1461650627622465536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジュディ、お願い…",
                    "maleVariant": null,
                    "stringId": 1461651038429376512
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "エヴリン、ランナーを加えようとしてるのよ？ 問題があるのは明らかでしょ？",
                    "maleVariant": null,
                    "stringId": 1461651220780937216
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この記録以外に入り込まない保証はない",
                    "maleVariant": null,
                    "stringId": 1461651586977230848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それは… 私が保証する",
                    "maleVariant": null,
                    "stringId": 1461652075781419008
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうだか…",
                    "maleVariant": null,
                    "stringId": 1461652470851301376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "助けを借りるのはこれで最後。必ず上手くいくから… 計画どおりにね",
                    "maleVariant": null,
                    "stringId": 1461654095355572224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。連絡しなよ",
                    "maleVariant": null,
                    "stringId": 1461667773148688384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかった。そのランナーに連絡したらダイブするよ",
                    "maleVariant": null,
                    "stringId": 1461681294947250176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私のアクセスを許可してくれる？ ポート1779を開放… セキュアプロトコル。よし、今リクエストを…",
                    "maleVariant": null,
                    "stringId": 1461698828446593024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "…送った",
                    "maleVariant": null,
                    "stringId": 1461699620482183168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "来た。準備はいい？",
                    "maleVariant": "来た。準備はいいか？",
                    "stringId": 1461700466641072128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "待って… OK、接続を確認。あとは簡易ICEを… よし",
                    "maleVariant": null,
                    "stringId": 1461700797470994432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジュディ、始めて",
                    "maleVariant": "ジュディ、始めてくれ",
                    "stringId": 1461703015116959744
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "エヴリン…",
                    "maleVariant": null,
                    "stringId": 1507021223309811716
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "とにかく準備はいい？",
                    "maleVariant": "とにかく準備はいいか？",
                    "stringId": 1521198299341254656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジュディとエヴリン",
                    "maleVariant": null,
                    "stringId": 1521198299341254676
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[T-バグに連絡する]",
                    "maleVariant": null,
                    "stringId": 1522726524790124548
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "踊りましょうか",
                    "maleVariant": null,
                    "stringId": 1531547792007884800
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大丈夫。コルチゾールとアドレナリンが急上昇したけど、ホルモンブロッカーが効いてくれた。あなたの体は何ともないよ",
                    "maleVariant": null,
                    "stringId": 1564862123393867780
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "すごく… 強烈だった",
                    "maleVariant": null,
                    "stringId": 1564862123444199424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "先に忠告してほしかった",
                    "maleVariant": null,
                    "stringId": 1564862123444199440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "すごく… 強烈だった。男の… 痛みやストレス、それにあれは… 希望？ 色々と混じってる感じだった",
                    "maleVariant": "すごく… 強烈だった。男の… 痛みやストレス、それにあれは… 希望か？ 色々と混じってる感じだった",
                    "stringId": 1564862123460976640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの前にブースターでも使ってたのかもね",
                    "maleVariant": null,
                    "stringId": 1564862123460976652
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ま、心配はいらないよ。設定は完璧だし、編集モードに切り替えるね",
                    "maleVariant": null,
                    "stringId": 1564862123494531084
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "強烈な… ショックだった。まだ感覚が残ってる… あの最後の瞬間… 死ぬのがどれだけ痛いか、忠告してくれてもよかったのに",
                    "maleVariant": "強烈な… ショックだった。まだ感覚が残ってる… あの最後の瞬間… 死ぬのがどれだけ痛いか、忠告してくれてもよかっただろ",
                    "stringId": 1564862123511308288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "本当の死の痛みは、あんなもんじゃないよ",
                    "maleVariant": null,
                    "stringId": 1564862123511308300
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "話が違うよ、エヴリン",
                    "maleVariant": null,
                    "stringId": 1631585921812525056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジュディ、お願い…",
                    "maleVariant": null,
                    "stringId": 1720103593901289492
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ダメよ。外のランナーを入れるなんて、考えられない",
                    "maleVariant": null,
                    "stringId": 1720103593918066696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "この記録以外に入り込まない保証はない",
                    "maleVariant": null,
                    "stringId": 1720103593918066716
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それは… 私が保証する",
                    "maleVariant": null,
                    "stringId": 1720103593918066736
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうだか…",
                    "maleVariant": null,
                    "stringId": 1720103593918066756
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "約束を、思い出して",
                    "maleVariant": null,
                    "stringId": 1720103593934843912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "助けを借りるのはこれで最後。必ず上手くいくから… 計画どおりにね",
                    "maleVariant": null,
                    "stringId": 1720103593934843952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あああ！",
                    "maleVariant": null,
                    "stringId": 1949099408325750784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんなもん、ね…",
                    "maleVariant": null,
                    "stringId": 1966367485685526528
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その… ちょっと休憩させてくれない？ まだあの瞬間の感覚が… クソ… 消えないの",
                    "maleVariant": "いや… ちょっと休憩させてくれないか？ まだあの瞬間の感覚が… クソ… 消えないんだ",
                    "stringId": 1966377491969470464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、時間がないのよ。しっかりして",
                    "maleVariant": null,
                    "stringId": 1966380490896830464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ねえ、 V？",
                    "maleVariant": null,
                    "stringId": 2231727234287333376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと、何か言ってよ",
                    "maleVariant": null,
                    "stringId": 2231727689352540160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ゆっくり息を吸って。大丈夫よ。何ともない",
                    "maleVariant": null,
                    "stringId": 2231727962435284992
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}