  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "仕事を探してるって言ってたじゃない",
              "maleVariant": "",
              "stringId": 1853325216020525060
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "戦いに来た",
              "maleVariant": "",
              "stringId": 1853325216054079488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シーザーね？ 戦いに来た",
              "maleVariant": "シーザーだな？ 戦いに来た",
              "stringId": 1853325216154742784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いかにも。グレン最強の男だ",
              "maleVariant": "",
              "stringId": 1853325216238628868
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ついでにナイトシティ最悪の父親ね",
              "maleVariant": "",
              "stringId": 1853325216238628876
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、同額を出す",
              "maleVariant": "",
              "stringId": 1853325216272183296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんな額は賭けられない",
              "maleVariant": "",
              "stringId": 1853325216272183328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんな車？",
              "maleVariant": "どんな車だ？",
              "stringId": 1853325216288960520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんな額、今すぐは無理ね",
              "maleVariant": "そんな額、今すぐは無理だ",
              "stringId": 1853325216322514948
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、そうか… まあ、気が変わったら言ってくれよな",
              "maleVariant": "",
              "stringId": 1853325216322514956
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "同額を出す。それでいい？",
              "maleVariant": "同額を出す。それでいいか？",
              "stringId": 1853325216389623812
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "素手だけだ。いいな？ モノワイヤーやマンティスブレードはナシだぞ？",
              "maleVariant": "",
              "stringId": 1853325216473509888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気をつけろ。手加減しないぞ",
              "maleVariant": "",
              "stringId": 1853325216523841540
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やめて！ もうやめてよ！ 十分でしょ！？",
              "maleVariant": "",
              "stringId": 1853325216674836484
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミカエラ… ごめん… 俺――",
              "maleVariant": "",
              "stringId": 1853325216725168128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう聞きたくない、このバカ。キーを渡したら家に帰るよ",
              "maleVariant": "",
              "stringId": 1853325216725168136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この辺でやめとくか？",
              "maleVariant": "",
              "stringId": 1853325216758722564
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし… 再戦ならいつでも受けてやるぜ。エディーはいくらあっても、困らないからな",
              "maleVariant": "",
              "stringId": 1853325216758722572
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[試合を開始する] いいよ",
              "maleVariant": "[試合を開始する] いいぞ",
              "stringId": 1853325216926494720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、試合開始ね",
              "maleVariant": "ああ、試合開始だな",
              "stringId": 1853325216960049156
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "先に教えて。どんな車なの？",
              "maleVariant": "先に教えてくれ。どんな車だ？",
              "stringId": 1853325216993603588
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フルカスタムの2056年モデルだ。なのにまだ2万マイルしか走ってない。へこみも継ぎはぎもないぞ",
              "maleVariant": "",
              "stringId": 1853325216993603596
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "へえ。盗難車？",
              "maleVariant": "へえ。盗難車か？",
              "stringId": 1853325217010380804
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし。やろうか",
              "maleVariant": "",
              "stringId": 1853325217027158020
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見つけただろ！",
              "maleVariant": "",
              "stringId": 1853337697765912576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんなの仕事じゃない。あなたが死んだら、お金が全部なくなったら、私たちがどうなるか考えたことある？",
              "maleVariant": "",
              "stringId": 1853337966151036928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シーザー、もうちょっとちゃんとしてよ…",
              "maleVariant": "",
              "stringId": 1853338688292745216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今すぐ大金が必要なんだ。だから大きな賭けしかするつもりはない",
              "maleVariant": "",
              "stringId": 1853342056000090112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミカエラ、頼むよ… 大丈夫だから――",
              "maleVariant": "",
              "stringId": 1853342756901842944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あなたとは話してない。クロ－ムに大金つぎこんだかと思えば、次は… ああもう、やってらんないわ",
              "maleVariant": "",
              "stringId": 1853342789265092608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちは車を賭ける。それに見合う金を出すつもりがないなら、忘れてくれ",
              "maleVariant": "",
              "stringId": 1853343621029130240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いやいや、まさか。別にやましい車じゃないって",
              "maleVariant": "",
              "stringId": 1853355857843941376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いい車をありがとう",
              "maleVariant": "",
              "stringId": 1853392040812703748
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エディーはもらうけど、車は取っておいて",
              "maleVariant": "エディーはもらうが、車は取っておけ",
              "stringId": 1853392460930969600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうも。乗り回すのが楽しみよ",
              "maleVariant": "どうも。乗り回すのが楽しみだ",
              "stringId": 1853392981276323840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミエルダ… いい車だったのに…",
              "maleVariant": "",
              "stringId": 1853393262428909568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エディーはもらうけど、車は取っておいて",
              "maleVariant": "エディーはもらう。だが車は取っておけ",
              "stringId": 1853394404118458368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え… 本気なのか？",
              "maleVariant": "",
              "stringId": 1853395192278511616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんな名前にしよう？",
              "maleVariant": "",
              "stringId": 1853403160197820416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、白馬の王子様… どうやって家に帰るわけ？ 歩くっての！？",
              "maleVariant": "",
              "stringId": 1853404198355169280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エディーも車も取っておいて",
              "maleVariant": "エディーも車も取っておけ",
              "stringId": 1862161572718198784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "全部取っておきなよ。こっちは何もいらないから",
              "maleVariant": "全部取っておけって。こっちは何もいらないから",
              "stringId": 1862162730748755968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミカエラ！ ミカエラ、待ってくれよ！",
              "maleVariant": "",
              "stringId": 1908167778851147776
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なあ… この条件が気に入らないなら断ってくれ。こちとら、時間も金も余裕がないんだ",
              "maleVariant": "",
              "stringId": 1908174881552650240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うっ… ほら。大事に乗れよ",
              "maleVariant": "",
              "stringId": 1908188269016043520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ… そうね",
              "maleVariant": "ああ… そうだな",
              "stringId": 1908189732324823040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんな冗談は言わない",
              "maleVariant": "",
              "stringId": 1908198852754984960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ、気が変わる前にエディーは渡してよ",
              "maleVariant": "さあ、気が変わる前にエディーは渡してくれ",
              "stringId": 1908200371344691200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あのさ… その… なんてお礼言っていいのか…",
              "maleVariant": "",
              "stringId": 1908200482493747200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別にお礼はいらない",
              "maleVariant": "",
              "stringId": 1908201560849305600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残ったものを大切にするのね",
              "maleVariant": "残ったものを大切にするんだな",
              "stringId": 1908201718639022080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気にしないで",
              "maleVariant": "気にするな",
              "stringId": 1908203471723876352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく残ったものを大切にして",
              "maleVariant": "とにかく残ったものを大切にしろ",
              "stringId": 1908203576162045952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふう… ラッキーだったな",
              "maleVariant": "",
              "stringId": 1908203741283405824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのつもりだ",
              "maleVariant": "",
              "stringId": 1908205221637836800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "愚か者は運だけはいいのよね",
              "maleVariant": "",
              "stringId": 1908206784737497088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うーん… “V”が付く名前は？",
              "maleVariant": "",
              "stringId": 1908208811207086080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何か考えるから。とにかく落ち着けよ！",
              "maleVariant": "",
              "stringId": 1908210066545496064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この人にわからせてやってくれる？ 負けないと理解できないのよ",
              "maleVariant": "",
              "stringId": 1932970634292547584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": 1932975713745686528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "前に戦った奴のロングスパーズには驚かされたからな。とにかく… 準備はいいか？",
              "maleVariant": "",
              "stringId": 1932976908015030272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "エディーはいらない。車だけでいい",
              "maleVariant": "",
              "stringId": 2096711922941444096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう。でも賭けたエディーは別にいらない",
              "maleVariant": "ありがとう。だが賭けたエディーは別にいらない",
              "stringId": 2252011877682626560
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}