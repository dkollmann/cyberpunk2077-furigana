    "ArchiveFileName": "C:\\Users\\Dan\\cyberpunk2077-furigana\\src\\wolvenkit\\Cyberpunk 2077 Furigana\\files\\Raw\\base\\localization\\jp-jp\\subtitles\\quest\\mq009\\mq009_02_second_meeting.json"
  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Hey! I know you! We had a deal!",
              "maleVariant": "",
              "stringId": 1810907847150505984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Hey, Ken. Is it time for the next episode \"Daddy-Daughter Dilemmas?\"",
              "maleVariant": "",
              "stringId": 1810914493008416768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Sit. I'll tell you the rest.",
              "maleVariant": "",
              "stringId": 1810914990402539520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us][Sit by Ken.]",
              "maleVariant": "",
              "stringId": 1810916276510363652
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]And she had just called me... a few days before. If only I'd answered...",
              "maleVariant": "",
              "stringId": 1810944077481349120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": 1810945063964872708
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Fuck... OK. I mean, no one should die alone.",
              "maleVariant": "",
              "stringId": 1810945283041759232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]What, suddenly realize your priorities ruined your life? No, this is on you. Don't drag me into it.",
              "maleVariant": "",
              "stringId": 1810945305556783104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Don't rope me into this.",
              "maleVariant": "",
              "stringId": 1810945491028905984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Thank you... But can we do it elsewhere? I have a place.",
              "maleVariant": "",
              "stringId": 1810950577662693376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Yes.",
              "maleVariant": "",
              "stringId": 1812768080516845572
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]No.",
              "maleVariant": "",
              "stringId": 1812768768164597760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]All right.",
              "maleVariant": "",
              "stringId": 1821258003523801092
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また後で",
              "maleVariant": "",
              "stringId": 1821259069229338624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Yeah, sure.",
              "maleVariant": "",
              "stringId": 1821265006283427840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Can't right now. In a bit of a hurry.",
              "maleVariant": "",
              "stringId": 1821265033680621572
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]And you just had to know everything about me... Had you pegged for a flake.",
              "maleVariant": "",
              "stringId": 1821269297006088192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I'm back. Not hurryin' off anywhere else for now.",
              "maleVariant": "",
              "stringId": 1821336626238820356
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]You're just lucky no one took your spot. Have a seat.",
              "maleVariant": "",
              "stringId": 1821337795577221120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I'm back.",
              "maleVariant": "",
              "stringId": 1821339843370651652
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]You, come here!",
              "maleVariant": "",
              "stringId": 1847227078311395328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Last time, I...I wasn't quite myself. It's starting to happen more often... But that's neither here nor there.",
              "maleVariant": "",
              "stringId": 1847242209078525952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Our first conversation made me realize a few things. Strange as it sounds, I want to thank you for being nosy.",
              "maleVariant": "",
              "stringId": 1847253026389868544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I'd give it all just to hear her raise her voice again.",
              "maleVariant": "",
              "stringId": 1847253153460502528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I'll be back. Maybe.",
              "maleVariant": "",
              "stringId": 1847264318680973312
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I'll still be here. Maybe.",
              "maleVariant": "",
              "stringId": 1847264685112147968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Enid, my baby girl...",
              "maleVariant": "",
              "stringId": 1847505636703322112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]You deserved it.",
              "maleVariant": "",
              "stringId": 1847531502388887556
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはお気の毒",
              "maleVariant": "それはお気の毒に",
              "stringId": 1847531765455634432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Tell me in the car.",
              "maleVariant": "",
              "stringId": 1847581332901445632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Happy to be of service.",
              "maleVariant": "",
              "stringId": 1867796902854770688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Got a quick tongue, don't you? Just like my Enid... You know, there was a time I thought she was incapable of using a normal tone.",
              "maleVariant": "",
              "stringId": 1867796976389308416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Enid's dead, isn't she?",
              "maleVariant": "",
              "stringId": 1867801913336774660
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Why'd you bite my head off before?",
              "maleVariant": "",
              "stringId": 1867802130249400320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Why'd you bite my head off before?",
              "maleVariant": "",
              "stringId": 1867804019481362432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Look, you and Enid... You remind me a lot of her.",
              "maleVariant": "",
              "stringId": 1867817689825394688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I fooled myself into thinking if you gave me any words of sympathy, forgiveness, it'd be like it came from her.",
              "maleVariant": "",
              "stringId": 1867817771178115072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Noticed a lot past tense before. She...she's dead, isn't she?",
              "maleVariant": "",
              "stringId": 1867818370493825024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]She called me right after she signed her contract. Explained to me that Biotechnica had the DNA for extinct animals she wanted to resurrect.",
              "maleVariant": "",
              "stringId": 1867856189996122112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]\"Platypuses, dad! Platypuses and axolotls! You know I've dreamed of this since elementary school! Ever since you got me that book.\"",
              "maleVariant": "",
              "stringId": 1867856318442487808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I said...I said never wanted to see her again. And hung up.",
              "maleVariant": "",
              "stringId": 1867856431034384384
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]You're a dick.",
              "maleVariant": "",
              "stringId": 1867856835415621636
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I understand... in a way.",
              "maleVariant": "",
              "stringId": 1867857067259969536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Dick move, but I understand the instinct. She knew she picked the enemy over you.",
              "maleVariant": "",
              "stringId": 1867857134754709504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Thanks for the reminder all corpos are massive dickheads.",
              "maleVariant": "",
              "stringId": 1867859364547379200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I broke off contact, not wanting to know what she did, where she lived. She betrayed the company... and, by extension, me.",
              "maleVariant": "",
              "stringId": 1867859465428779008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I know...I know! I was just so furious, I lost myself... How could I know it would be the last time I'd hear her voice?",
              "maleVariant": "",
              "stringId": 1867880104290140160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]That call still haunts my sleep... How could I know it would be the last time I'd hear her voice?",
              "maleVariant": "",
              "stringId": 1867898013414572032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Whoever isn't with us is against us... Classic.",
              "maleVariant": "",
              "stringId": 1867914514561482752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Don't you mock me. Not right now.",
              "maleVariant": "",
              "stringId": 1867914613882601472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Then, one day the CEO called me in to talk. I thought he wanted to talk strategy for the upcoming quarter...",
              "maleVariant": "",
              "stringId": 1867926200647401472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]But as soon as I stepped in, he told me he had Enid  killed.",
              "maleVariant": "",
              "stringId": 1867926278879559680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Why do it, have her killed?",
              "maleVariant": "",
              "stringId": 1867927744201605120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Competition? To test my loyalty, maybe?",
              "maleVariant": "",
              "stringId": 1867928895991037952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Enid focused on artificial tissues, regenerative work... heart muscle.",
              "maleVariant": "",
              "stringId": 1867928995228270592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]If Biotechnica took it to market, Kendachi pacemaker sales... they would have plummeted.",
              "maleVariant": "",
              "stringId": 1867929105790124032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Jesus, Ken, I'm sorry... You fucked up, but not enough to deserve this.",
              "maleVariant": "",
              "stringId": 1867930808123899904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Don't know what you expected after sellin' yourself to a corp. One day you're destroying lives, next it's yours. Circle of life.",
              "maleVariant": "",
              "stringId": 1867930875333427200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Why have Enid killed?",
              "maleVariant": "",
              "stringId": 1867939937294991360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Your words won't change anything. Still... thank you. I mean it.",
              "maleVariant": "",
              "stringId": 1871833510218285056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I didn't deserve that. I did everything for her! Everything!",
              "maleVariant": "",
              "stringId": 1871848029808214016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]You really so fuckin' certain?",
              "maleVariant": "",
              "stringId": 1871848133826953216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Guessin' we're at the part we agreed on. What is it you want from me for your story? Revenge?",
              "maleVariant": "",
              "stringId": 1871848682676797440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I would've killed him.",
              "maleVariant": "",
              "stringId": 1871858202455031808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I'd have gutted the bastard. Dont' care if his security flatlined me after.",
              "maleVariant": "",
              "stringId": 1871858361905692672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]When the initial shock passed, I thought of nothing else. I had to wipe that arrogant smirk from his face.",
              "maleVariant": "",
              "stringId": 1871888298112958464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]So I threw myself at him in a rage. If only I had mantis blades instead of my bare hands...",
              "maleVariant": "",
              "stringId": 1871888323899539456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Finally, a good decision. Only a shame it came too late.",
              "maleVariant": "",
              "stringId": 1871888440501190656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]They didn't kill me, though. Would've been a bad PR move. Instead they waited – waited until I wound up here.",
              "maleVariant": "",
              "stringId": 1871888500664287232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Just... kill me. That's it.",
              "maleVariant": "",
              "stringId": 1871895077014192128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I have no strength left, and I don't want it back. I don't even have enough to pull the trigger.",
              "maleVariant": "",
              "stringId": 1871905031573979136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Wait, please! You can't leave me like this now!",
              "maleVariant": "",
              "stringId": 1871921707791138816
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]No? Watch me.",
              "maleVariant": "",
              "stringId": 1871921796509057024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Last one had quite the dramatic conclusion.",
              "maleVariant": "",
              "stringId": 1898229783527669760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us][Offer Ken a cigarette.]",
              "maleVariant": "",
              "stringId": 1898253983889997828
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I don't smoke.",
              "maleVariant": "",
              "stringId": 1898254042325041152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Here. To cool your nerves?",
              "maleVariant": "",
              "stringId": 1898254155856461824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Out of luck. Don't smoke.",
              "maleVariant": "",
              "stringId": 1898254277088624640
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Got a cigarette? It's a perfect day for a puff.",
              "maleVariant": "",
              "stringId": 1898255140913922048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]For our history lesson.",
              "maleVariant": "",
              "stringId": 1898262313324756992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]New day, new disappointments... It's fine.",
              "maleVariant": "",
              "stringId": 1898264426046013440
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}