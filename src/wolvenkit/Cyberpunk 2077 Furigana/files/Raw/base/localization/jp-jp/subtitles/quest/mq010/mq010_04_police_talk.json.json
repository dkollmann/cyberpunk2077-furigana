  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アンドリューのお墓に行った",
                    "maleVariant": null,
                    "stringId": 1775014363328049156
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、お前か。バリーと話したか？ 引き籠るのをやめてくれりゃいいんだが",
                    "maleVariant": null,
                    "stringId": 1793827762354679808
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バリーはふさぎ込んでる",
                    "maleVariant": null,
                    "stringId": 1793849555874377728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "死んだ友達について、バリーは具体的に何て言ってたの？",
                    "maleVariant": "死んだ友達について、バリーは具体的に何て言ってた？",
                    "stringId": 1841843375376678912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バリーは死んだ友達について何て？",
                    "maleVariant": null,
                    "stringId": 1841843452082110464
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "詳しいことは何も。彼の話はほとんどしてなかった。唯一理解してくれる相手とだけ",
                    "maleVariant": null,
                    "stringId": 1841855909919965184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そういう人がいるといいよね",
                    "maleVariant": null,
                    "stringId": 1841856078480654336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いい加減にしてよ、メンデス！",
                    "maleVariant": null,
                    "stringId": 1895457101287505920
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "少し話をした。彼、相当思い詰めてるみたいよ。メンデスに対して不満があったみたい",
                    "maleVariant": "少し話をした。あいつ、相当思い詰めてるみたいだぞ。メンデスに対して不満があったらしい",
                    "stringId": 1896484015133409280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあね。自分で聞きなよ",
                    "maleVariant": "さあな。自分で聞けよ",
                    "stringId": 1896484102794362880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあね",
                    "maleVariant": "さあな",
                    "stringId": 1896487074458746884
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼の問題に無関心だったでしょ",
                    "maleVariant": "あいつの問題に無関心だっただろ",
                    "stringId": 1896487124236746752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バリーは仕事に耐えられなくて、話をできる相手もいなかった",
                    "maleVariant": null,
                    "stringId": 1896487404768575488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はぁ…？ じゃあどうして俺に言わないんだ！？",
                    "maleVariant": null,
                    "stringId": 1896489395251036160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、聞くさ！ お前と話したんなら、もう駄々をこねるのはやめたんだろうし",
                    "maleVariant": null,
                    "stringId": 1896499997243588608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ガツンと一発食らわせてやる。酒と拳、どっちにするか",
                    "maleVariant": null,
                    "stringId": 1896500213988442112
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "手を貸してくれてありがとう。これをどうぞ",
                    "maleVariant": null,
                    "stringId": 1896500346343899136
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "理解しようともしなかったんだってね",
                    "maleVariant": "理解しようともしなかったんだってな",
                    "stringId": 1896513332529840128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほらね？ 言ったでしょ？",
                    "maleVariant": null,
                    "stringId": 1896513540147888128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ちょっと待て。バリーは大げさに騒ぎ立ててるだけだ。ほとぼりが冷めて話をすりゃ大丈夫だって",
                    "maleVariant": null,
                    "stringId": 1896514249236922368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうかな… 後で様子を見にいかないと",
                    "maleVariant": null,
                    "stringId": 1896514356074233856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バリーは人を信じられなくなってる。あんたたちよりカメと話すことを選んだくらいにね",
                    "maleVariant": "バリーは人を信じられなくなってる。あんたたちよりカメと話すことを選んだくらいにな",
                    "stringId": 1896658787603562496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "冗談のつもりか？",
                    "maleVariant": null,
                    "stringId": 1896808866679341056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいえ。残念ながら本当の話よ",
                    "maleVariant": "いいや。残念ながら本当の話だ",
                    "stringId": 1896809022103470080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そしてあんたたち以外には… おばあさんのことを思い出させてくれる、親友のカメしかいなかった。それも死んじゃったけど",
                    "maleVariant": "そしてあんたら以外には… おばあさんのことを思い出させてくれる、親友のカメしかいなかった。それも死んじまったけどな",
                    "stringId": 1896809763119546368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "嘘だろ… そんなくだらない話、信じられるか！",
                    "maleVariant": null,
                    "stringId": 1896809888629899264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自分で見に行きなよ",
                    "maleVariant": "なら自分で見に行け",
                    "stringId": 1896811439331196932
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "警官をからかってる暇なんてない",
                    "maleVariant": null,
                    "stringId": 1896811502698741760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "証拠が欲しかったら、納骨堂にあるから見に行けばいい",
                    "maleVariant": null,
                    "stringId": 1896811799739351040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "警官をからかうのが趣味の暇人に見える？",
                    "maleVariant": "警官をからかうのが趣味の暇人に見えるか？",
                    "stringId": 1896811958082715648
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうして彼は私たちにウソを？ そのカメのこと、普通の人間みたいに話してたのよ！",
                    "maleVariant": null,
                    "stringId": 1896825827169648640
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "真実を話したら、メンデスがどんな反応したと思う？",
                    "maleVariant": null,
                    "stringId": 1896859868392570880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バリーの奴… 大げさに騒ぎ立ててるだけだと思ってたが、こりゃ話をしにいかないといけないな",
                    "maleVariant": null,
                    "stringId": 1896860616002093056
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうね",
                    "maleVariant": null,
                    "stringId": 1897940019079528448
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "バリーは警官として苦しんでたのに、それを茶化されたから、殻に閉じこもっちゃったのよ",
                    "maleVariant": "バリーは警官として苦しんでたのに、それを茶化されたから、殻に閉じこもっちまったんだよ",
                    "stringId": 1967956602883129344
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}