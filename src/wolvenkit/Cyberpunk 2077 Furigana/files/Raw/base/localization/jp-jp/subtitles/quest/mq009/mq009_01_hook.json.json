    "ArchiveFileName": "C:\\Users\\Dan\\cyberpunk2077-furigana\\src\\wolvenkit\\Cyberpunk 2077 Furigana\\files\\Raw\\base\\localization\\jp-jp\\subtitles\\quest\\mq009\\mq009_01_hook.json"
  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Agh! W-what's that?  Legs getting... tired?",
              "maleVariant": "",
              "stringId": "1807048818263298048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Quit cryin', your majesty! Or I'll cut our your tongue!",
              "maleVariant": "",
              "stringId": "1807066462811033600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Where's the cash?! Don't play me!",
              "maleVariant": "",
              "stringId": "1807068544527994880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]The fuck're you?",
              "maleVariant": "",
              "stringId": "1807070911893835776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]The hell d'you want? Jealous of his majesty's busted ribs?",
              "maleVariant": "",
              "stringId": "1807072612214345728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Agreed.",
              "maleVariant": "",
              "stringId": "1807077953878593540"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I need details first.",
              "maleVariant": "",
              "stringId": "1807079593465581568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Kendachi's infamous for its suicides.",
              "maleVariant": "",
              "stringId": "1821739065848860676"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I'm supposed to explain her decision?",
              "maleVariant": "",
              "stringId": "1821739556599205888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Is that--agh! T-that all you got?",
              "maleVariant": "",
              "stringId": "1845995562256285696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Heard you talkin' in your sleep!",
              "maleVariant": "",
              "stringId": "1846000027797536768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Plenty more to go 'round!",
              "maleVariant": "",
              "stringId": "1846005943863328768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Enid's dream project?",
              "maleVariant": "",
              "stringId": "1846109968826875904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Corpo-prick drifted off. Musta really drilled it into him, the corp's glorious vision. Till he grew a heart, anyway.",
              "maleVariant": "",
              "stringId": "1846110267075444736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Think this daughter issue was what wrecked the guy?",
              "maleVariant": "",
              "stringId": "1846197228469469184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]That was rhetorical right? You really gotta work on your intonation.",
              "maleVariant": "",
              "stringId": "1846197380538155008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Shame he didn't finish his story. Actually had me curious what he wanted to ask â€“ and what happened to Enid.",
              "maleVariant": "",
              "stringId": "1846198376450473984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]On the edge of your seat, huh?",
              "maleVariant": "",
              "stringId": "1846198815879315456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Ugh... Bravo. What an altruist. What do ya want now? Warm hug? Handful of ennies?",
              "maleVariant": "",
              "stringId": "1865016786344984576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]You needed help.",
              "maleVariant": "",
              "stringId": "1865016961532674052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]You owe me.",
              "maleVariant": "",
              "stringId": "1865017054847549440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]That why you were hollerin'? 'Cause you didn't \"have a stake in it?\"",
              "maleVariant": "",
              "stringId": "1865017146870579200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]A few ennies sounds nice, actually. Your choombas mentioned a hidden stash.",
              "maleVariant": "",
              "stringId": "1865017190659112960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]How 'bout some advice instead. You see someone not asking for help... you keep walkin'.",
              "maleVariant": "",
              "stringId": "1865017888893288448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Neither side... had any real stake in it.",
              "maleVariant": "",
              "stringId": "1865018031851945984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]The body... doesn't always do what it's told. Not even implants always help... Pain is pain.",
              "maleVariant": "",
              "stringId": "1865018331157479424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]My \"choombas\" think they're smarter than me. But they're easy to mislead... to manipulate.",
              "maleVariant": "",
              "stringId": "1865018714349092864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Not your average homeless guy, are you?",
              "maleVariant": "",
              "stringId": "1865020923489009668"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]You were provoking them?",
              "maleVariant": "",
              "stringId": "1865021037238534144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Hold up... You wanted to provoke them? On purpose?",
              "maleVariant": "",
              "stringId": "1865021152649003008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Does it matter? Why do you care?",
              "maleVariant": "",
              "stringId": "1865035571139428352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Normally, I'd say \"'cause I saved your ass,\" but I'm guessin' that won't do the trick.",
              "maleVariant": "",
              "stringId": "1865035672859688960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]You're starting to catch on.",
              "maleVariant": "",
              "stringId": "1865035777146863616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Not a man of the streets, are you? Where'd you drop in from?",
              "maleVariant": "",
              "stringId": "1865036623104425984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Actually, think you do owe me.",
              "maleVariant": "",
              "stringId": "1865054748856270848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Two bums kick your ass to the curb, you're insulted that I'd dare help, and now you try to chew me out with perfect diction.",
              "maleVariant": "",
              "stringId": "1865054796100911104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Actually, think I am owed.",
              "maleVariant": "",
              "stringId": "1865055699017121796"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Didn't think anyone would notice?",
              "maleVariant": "",
              "stringId": "1865055799881744384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]What is it to you? I owe you shit. Now, leave me alone.",
              "maleVariant": "",
              "stringId": "1865056573646946304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I dunno how you ruffled their rags, but seemed pretty clear to me you weren't gettin' kicked by choice.",
              "maleVariant": "",
              "stringId": "1866137874747604992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]If you thought none of that'd seem odd, ya didn't quite think it through.",
              "maleVariant": "",
              "stringId": "1866139086448476160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Hm. You're stubborn â€“ just like my daughter. She loved to call people out on their gaps in logic.",
              "maleVariant": "",
              "stringId": "1866139655179321344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Actually... You even look her age...",
              "maleVariant": "",
              "stringId": "1866162543731130368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]All right, fine. I'll satisfy your curiosity provided you do something for me later on. Are we agreed?",
              "maleVariant": "",
              "stringId": "1866162716335128576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]C'mon, out with it. Who are you really?",
              "maleVariant": "",
              "stringId": "1866171141114806272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]A deal with details in the dark? Consider me interested.",
              "maleVariant": "",
              "stringId": "1866172214252650496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Nice try. You spin some tall tale and then I have to do... Jesus, I don't even wanna think what.",
              "maleVariant": "",
              "stringId": "1866172392762228736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I'll hold you to your word. In the meantime, I invite you to my cozy abode. Need to get off my feet â€“ I can barely stand.",
              "maleVariant": "",
              "stringId": "1866187647043223552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]No, you tell me you story and what it is you want from me, then I make my decision.",
              "maleVariant": "",
              "stringId": "1866205464933154816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Got a real negotiator on my hands, huh... So be it, follow me. Need to get off my feet , after all. I can barely stand.",
              "maleVariant": "",
              "stringId": "1866206496681607168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Now? Nobody. But a few months ago, a seat on the Kendachi board had my name on it â€“ Ken Lee.",
              "maleVariant": "",
              "stringId": "1866339301650579456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]My, my! What an honor! Got someone you owe to this spectacular downfall?",
              "maleVariant": "",
              "stringId": "1866339369279537152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]No, no one. I sacrificed my life for the company. And I wanted my daughter... Enid... to do the same.",
              "maleVariant": "",
              "stringId": "1866339438418444288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Daddy plannin' out his kid's career? Rings of domestic despot to me...",
              "maleVariant": "",
              "stringId": "1866339514368901120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Wrong again. Biotech was Enid's passion. And at Kendachi, she could make her living immersed in it.",
              "maleVariant": "",
              "stringId": "1866339585923727360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I just wanted her to be happy. Does that make me a rotten father? Does it?!",
              "maleVariant": "",
              "stringId": "1866339678634622976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]If my dad micromanaged my whole life, I'd've bail first chance I got and never looked back.",
              "maleVariant": "",
              "stringId": "1866340465502830592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Just matched her own plans with your expectations. Nothin' wrong in that.",
              "maleVariant": "",
              "stringId": "1866340532729135104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Wouldn't want a dad like you.",
              "maleVariant": "",
              "stringId": "1866340610004992004"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]You did nothing wrong.",
              "maleVariant": "",
              "stringId": "1866340684479053824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I gave her everything! Toys, books, even a dog! Do you know how steep the taxes on those fleabags are?",
              "maleVariant": "",
              "stringId": "1866357096505393152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]And this gives you the right to control her life?",
              "maleVariant": "",
              "stringId": "1866357200524132352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Think we got off subject...",
              "maleVariant": "",
              "stringId": "1866377860893884416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I'm sure your family history's a real page-turner, but what's it got to do with your hero-to-zero story?",
              "maleVariant": "",
              "stringId": "1866378031232958464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I'll get to that. But first, you answer my question.",
              "maleVariant": "",
              "stringId": "1866383229770776576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]What, whether you're a shitty dad? What do you care what I think?",
              "maleVariant": "",
              "stringId": "1866383299832430592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]What do you care about my story?",
              "maleVariant": "",
              "stringId": "1866383478878879744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]She'd have equipment, resources... One month and I could give her an entire lab at Kendachi to lead.",
              "maleVariant": "",
              "stringId": "1866405782677602304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Everything she could need to carry out her dream project.... So why'd she choose Biotechnica?",
              "maleVariant": "",
              "stringId": "1866405911778279424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Exactly! I gave her everything! Toys, books, even a dog! Do you know how steep the taxes on those fleabags are?",
              "maleVariant": "",
              "stringId": "1866445394926092288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]A good dad, and a corpo no less.",
              "maleVariant": "",
              "stringId": "1866445501897621504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]What's this dream project of hers?",
              "maleVariant": "",
              "stringId": "1866470468089528320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Well... Enid always loved nature's strangest creatures. As a girl, she wanted to bring the platypus back from extinction.",
              "maleVariant": "",
              "stringId": "1866470713993183232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Not foxes or bears, but platypuses specifically. She was a special kid.",
              "maleVariant": "",
              "stringId": "1866471181070876672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Platy--what? That alien or animal?",
              "maleVariant": "",
              "stringId": "1866471287119659008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]It's like a cross between a duck and a beaver. Can you imagine? I'd love to see one with own two eyes.",
              "maleVariant": "",
              "stringId": "1866471467491508224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Maybe she didn't want to be a martyr for some deadline deity. Kendashi's got a nasty rep for staff suicides.",
              "maleVariant": "",
              "stringId": "1866473151856885760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Wait, you... you want me to explain why your daugher jumped ship for the competition?",
              "maleVariant": "",
              "stringId": "1866473562009485312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Eighteen-hour workdays can break a man's soul... Or his family's, at the very least.",
              "maleVariant": "",
              "stringId": "1866504430358872064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]You think think it's that simple? That...that you just quit and live happily ever after?",
              "maleVariant": "",
              "stringId": "1866504603617181696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I did. Dunno if it extended my life any, but I like to think my mood's taken a turn for the better.",
              "maleVariant": "",
              "stringId": "1867525818964230144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Don't have any kids, do you? Well, without my money, Enid could only ever dream of becoming a bioengineer!",
              "maleVariant": "",
              "stringId": "1867526361606504448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Didn't consider that, did you? Like the eddies just fell from the sky, and I spent my nights in meetings by choice! I did it all for her!",
              "maleVariant": "",
              "stringId": "1867526467772727296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Hey, Ken... Maybe get off my back?",
              "maleVariant": "",
              "stringId": "1867541748142292992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I did everything to convince her. The problem was with her... Right? It must've been!",
              "maleVariant": "",
              "stringId": "1867592561397055488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Take it easy, man. How could I know...",
              "maleVariant": "",
              "stringId": "1867592634545717248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]I had to the right to feel betrayed. Who keeps traitors close? Suicidal fools, that's who!",
              "maleVariant": "",
              "stringId": "1867592702208229376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]S-she made... her decision... She knew... Kendachi is my life. So why... why would...",
              "maleVariant": "",
              "stringId": "1867592763830943744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Ugh... Think my kidneys got the worst of it.",
              "maleVariant": "",
              "stringId": "1867594126107328512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Yo, Ken. You alive?",
              "maleVariant": "",
              "stringId": "1867599455926861824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Look like a gonk when you smile. Nah, I just love seein' bottomed-out corpos â€“ balm for the soul. Curious how he wound up here.",
              "maleVariant": "",
              "stringId": "1867635690384900096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Guess we gotta wait for the exciting conclusion.",
              "maleVariant": "",
              "stringId": "1867635822102822912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Did she know anyone at Biotechnica?",
              "maleVariant": "",
              "stringId": "1898207197469065216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Guessin' she didn't know anyone at Biotechnica, did she?",
              "maleVariant": "",
              "stringId": "1898207350829596672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Not that I know of. Why?",
              "maleVariant": "",
              "stringId": "1898222228445315072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]If I were her, wouldn't want anyone thinkin' I got my job 'cause I'm daddy's pet.",
              "maleVariant": "",
              "stringId": "1898222352949035008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[en_us]Anywhere else she has to rely on herself. An uphill climb, sure, but all the more satisfying.",
              "maleVariant": "",
              "stringId": "1898222659938533376"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}