    "RootChunk": {
      "Type": "JsonResource",
      "Properties": {
        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "乾杯だ！ お前の目の前にいる、新たな警部補様にな！ ハッ！",
                    "maleVariant": null,
                    "stringId": 1976726136215146496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "黙っててほしかったらもっと金を寄越して",
                    "maleVariant": "黙っててほしかったらもっと金を寄越せ",
                    "stringId": 1976727311140360196
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[座って飲む] ところで、あれは何の映像だったの？",
                    "maleVariant": "[座って飲む] ところで、あれは何の映像だったんだ？",
                    "stringId": 1976727439972601860
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたなんかと飲む気はない",
                    "maleVariant": null,
                    "stringId": 1976727544511434752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "遠慮しとく",
                    "maleVariant": null,
                    "stringId": 1976727646013591552
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "はっ… 俺に見え透いた嘘は…",
                    "maleVariant": null,
                    "stringId": 1976728589664882688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、俺に過去を忘れさせ、未来をくれたお前に乾杯だ！ ハッ！",
                    "maleVariant": null,
                    "stringId": 1976728667578273792
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "アーロン？",
                    "maleVariant": null,
                    "stringId": 1976735608312532992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おい、何を突っ立ってる！ 一緒に飲めよ！",
                    "maleVariant": null,
                    "stringId": 1976736251517440000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V！ V！ 驚いたな！",
                    "maleVariant": null,
                    "stringId": 1976736632125362176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[座って飲む] 愛に乾杯",
                    "maleVariant": null,
                    "stringId": 1976744216249077760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でしょうね",
                    "maleVariant": "だろうな",
                    "stringId": 1976751346146447360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "昇進するの？ なら、口止め料を値上げしてもらおうかしら",
                    "maleVariant": "昇進するのか？ なら、口止め料を値上げしてもいいよな",
                    "stringId": 1976754986148007936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "映像はもうねえ。残念だったな！",
                    "maleVariant": null,
                    "stringId": 1976755703407546368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "コピーがあったら？",
                    "maleVariant": null,
                    "stringId": 1976756210750558208
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "俺のキャリアに乾杯だ！ それと“はったり”にな！ ハハハ！",
                    "maleVariant": null,
                    "stringId": 1976757582740951040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あんたを祝う？ 無理な相談ね",
                    "maleVariant": "あんたを祝う？ 無理な相談だな",
                    "stringId": 1976769824253382656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハハハ！ ならお前の健康にでもいい！",
                    "maleVariant": null,
                    "stringId": 1976770641622568960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前と話せて楽しかったぜ。だがよう、今は一人で酔っ払いてえんだ",
                    "maleVariant": null,
                    "stringId": 1976790868838158336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ乾杯させて。愛にね",
                    "maleVariant": "乾杯させてくれ。愛にな",
                    "stringId": 1976792861333884928
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ハハハ！ 言い寄る気か？ 俺と結ばれんのはやめておけ",
                    "maleVariant": null,
                    "stringId": 1976797098369454080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ろくなもんじゃねえぞ",
                    "maleVariant": null,
                    "stringId": 1976797677636390912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "気にしないで",
                    "maleVariant": "気にするなって",
                    "stringId": 1976799184901476352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "水くせえだろ。ああ？ 警官となんてごめんってか？ ハハッ、別にいいだろ？",
                    "maleVariant": null,
                    "stringId": 1976799462715396096
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "警部補に乾杯",
                    "maleVariant": null,
                    "stringId": 1976803239367380992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "乾杯！",
                    "maleVariant": null,
                    "stringId": 1976803372209377280
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ところで、あれは何の映像だったの？ ずっと気になってて",
                    "maleVariant": "ところで、あれは何の映像だったんだ？ ずっと気になってる",
                    "stringId": 1976804612649619456
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おいおい、V！ 未来を祝ってるんだぞ！ 過去は忘れろって！",
                    "maleVariant": null,
                    "stringId": 1976806128789852160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だが言わせてくれ… お前がいなきゃ、今の俺はなかった。V、ありがとよ",
                    "maleVariant": null,
                    "stringId": 1976806773001392128
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "昇進おめでとう。でも飲むのはやめとく",
                    "maleVariant": null,
                    "stringId": 1976808074124824576
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほんとに結構よ",
                    "maleVariant": "ほんとに結構だ",
                    "stringId": 1976808599453011968
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ああ、わかったよ。だが、お前に乾杯させてくれ！ でっかい借りができたな",
                    "maleVariant": null,
                    "stringId": 1976809377647398912
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どうも",
                    "maleVariant": null,
                    "stringId": 1976810177635389440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうか、からかってんだな！",
                    "maleVariant": null,
                    "stringId": 1976891925861863424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "レジーナの犬の間違いでしょ",
                    "maleVariant": "レジーナの犬の間違いだろ",
                    "stringId": 1995305104266596352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "レジーナの犬の間違いでしょ。たまには縄を緩めてくれてる？",
                    "maleVariant": "レジーナの犬の間違いだろ。たまには縄を緩めてくれてるか？",
                    "stringId": 1995305821962342400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お前だってこき使われてるんだろ！ ハハハ！",
                    "maleVariant": null,
                    "stringId": 1995453688341434368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ、彼女にあたしの駐禁をツケといて。手の空いた時にでもよろしく",
                    "maleVariant": "じゃあ、彼女に俺の駐禁をツケといてくれ。手の空いた時にでもよろしく",
                    "stringId": 1995459372730650624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "おもしれえ！ そのユーモアに乾杯だ！",
                    "maleVariant": null,
                    "stringId": 1995459793369010176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[座る]",
                    "maleVariant": null,
                    "stringId": 2156300654734385156
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[立ち上がる]",
                    "maleVariant": null,
                    "stringId": 2157501507474464768
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
}