          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "V、よく来てくれたね",
                    "maleVariant": null,
                    "stringId": 1701015549985595392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "お悔やみを… 彼はとてもいい子だった",
                    "maleVariant": null,
                    "stringId": 1701036886678478848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ボクシングクラブのナイトシティ・デビルズには、こういう言い回しがあるんだ",
                    "maleVariant": null,
                    "stringId": 1701194367459303424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "パドレ、どうぞあの子に乾杯を。飲んでいって",
                    "maleVariant": null,
                    "stringId": 1701228292315455488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしはよく… 初めてジャッキーに会った時のことを思い出す",
                    "maleVariant": "えー、俺はよく… 初めてジャッキーに会った時のことを思い出す",
                    "stringId": 1709616265364938752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "大昔の… いえ、それどころか、まるで別世界で起きたことのように感じるけど、ジャッキーは、かつてあたしの命を救ってくれた",
                    "maleVariant": "大昔の… いや、それどころか、まるで別世界で起きたことのように感じるけど、ジャッキーは、かつて俺の命を救ってくれた",
                    "stringId": 1709617872420904960
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あいつはアラサカの盗品を密輸しようとしてたの。超極秘の高級品… トカゲをね",
                    "maleVariant": "あいつはアラサカの盗品を密輸しようとしてたんだ。超極秘の高級品… トカゲをな",
                    "stringId": 1709618813656276992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今日は、来てくれてありがとう",
                    "maleVariant": null,
                    "stringId": 1709710379691450368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "代わりに、ある出来事を紹介するわ",
                    "maleVariant": null,
                    "stringId": 1709710379691450376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[飲む]",
                    "maleVariant": null,
                    "stringId": 1725623255585169412
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジャッキーがどんな人間だったかとか、あの子を失ってどれだけ寂しいかとか、そんな話はしない",
                    "maleVariant": null,
                    "stringId": 1725638389305090048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「自分をノックアウトできるファイターを一番の親友にしろ」",
                    "maleVariant": null,
                    "stringId": 1725639810939265024
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l="mex" o="Me mandó Campo Orta." t="カンポ・オルタの代理で来た" b="" a=""/>",
                    "maleVariant": null,
                    "stringId": 1725640208240517120
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[炎に酒を注ぐ]",
                    "maleVariant": null,
                    "stringId": 1741399012064272384
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "来てくれて嬉しかった。いいオフレンダができたわ",
                    "maleVariant": null,
                    "stringId": 1742986048802037768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l="mex" o="De acuerdo." t="ああ。そうさせてもらおう" b="" a=""/>",
                    "maleVariant": null,
                    "stringId": 1856038944331583488
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もうすぐ始まるわ",
                    "maleVariant": null,
                    "stringId": 1856039595723771904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あれは… ジャッキーが19の頃だったかしら",
                    "maleVariant": null,
                    "stringId": 1856156412475658240
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの子はヴァレンティーノズの一員で、当時メイルストローム相手に日々戦ってた",
                    "maleVariant": null,
                    "stringId": 1856158365041610752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんな時に… 病院から連絡があってね。あの子が… 3発も撃たれたって言うの。それも、心臓の近くに",
                    "maleVariant": null,
                    "stringId": 1856158506087665664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "私が病院に行くと、あの子は意識を取り戻してた。体中から色んな管が伸びてて、まるでタコみたいだったのを覚えてる",
                    "maleVariant": null,
                    "stringId": 1856158611717017600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そしてこんなことを言ったの。「心配すんな、オフクロ。俺は防弾仕様なんだ。だから絶対死なないんだ」って",
                    "maleVariant": null,
                    "stringId": 1856158787005370368
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それから私の帰り際に、ヴァレンティーノズの指輪を外したの。もう必要ないからって",
                    "maleVariant": null,
                    "stringId": 1856158884732653568
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの子は怖気づいたんじゃない。ただ私を安心させたかったの",
                    "maleVariant": null,
                    "stringId": 1856158994120101888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l="mex" o="Descansa en paz, mi hijo." t="我が子よ、どうか安らかに" b="" a=""/>",
                    "maleVariant": null,
                    "stringId": 1856159204825157632
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジャッキーの腕はまるで大砲だった。あんな強烈なのは食らったことがない… 今でも感触を覚えてる",
                    "maleVariant": null,
                    "stringId": 1856161373750087680
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あの12ラウンドを思い出すぜ",
                    "maleVariant": null,
                    "stringId": 1856161567308828672
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l="mex" o="Lamenta no estar aquí, todavía le quedan cuatro años que cumplir. Me pidió que leyera esto." t="彼は今日来れないことを残念に思ってる。刑期がまだ4年あるからな。だから手紙を読む" b="" a=""/>",
                    "maleVariant": null,
                    "stringId": 1856163297492783104
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l="mex" o="«Recuerdo cuando Jackie se nos unió. Era un niño fuerte»." t="「俺たちのもとへ初めてやって来た時、ジャッキーはタフなガキだった」" b="" a=""/>",
                    "maleVariant": null,
                    "stringId": 1856163459107704832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l="mex" o="«Cuando decía que iba a hacer algo, cumplía. Tenía honor. Podría haber llegado lejos»." t="「何かをやると言ったら、必ずそれをやり遂げる男だ。人を敬い、大成する資質を持っていた」" b="" a=""/>",
                    "maleVariant": null,
                    "stringId": 1856163547557187584
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l="mex" o="«Mis condolencias a la familia»." t="「心からの哀悼の意を」" b="" a=""/>",
                    "maleVariant": null,
                    "stringId": 1856163713181863936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l="mex" o="«Que descanse en paz»." t="「どうか安らかに」" b="" a=""/>",
                    "maleVariant": null,
                    "stringId": 1856163837064826880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジャッキー・ウェルズ",
                    "maleVariant": null,
                    "stringId": 1856342198181482496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[ボールを置く] ヘイウッドの申し子に",
                    "maleVariant": null,
                    "stringId": 1856343144634568704
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[テキーラを置く] 常に上を目指してた",
                    "maleVariant": null,
                    "stringId": 1856343144634568720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "安らかに、チューマ",
                    "maleVariant": null,
                    "stringId": 1856352075163422724
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "優しい言葉をありがとう、V",
                    "maleVariant": null,
                    "stringId": 1856384043812966400
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あなたは、良い心の持ち主よ",
                    "maleVariant": null,
                    "stringId": 1856384113891397636
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[鍵を取る] ありがとう",
                    "maleVariant": null,
                    "stringId": 1856384414287450116
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いいの？",
                    "maleVariant": "いいのか？",
                    "stringId": 1856384516628467712
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ありがとう。大事にするよ",
                    "maleVariant": null,
                    "stringId": 1856384596387352580
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ほんとに… いいの？",
                    "maleVariant": "ほんとに… いいのか？",
                    "stringId": 1856384635746701316
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "それから何年も経って、あいつはナイトシティでまたあたしを救ってくれたの。何もかも失って、もう終わりだと思ってたのに",
                    "maleVariant": "それから何年も経って、あいつはナイトシティでまた俺を救ってくれたんだ。何もかも失って、もう終わりだと思ってたのに",
                    "stringId": 1856404934617780224
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そしてあたしは自由になれた。それは全部、ジャッキーがいてくれたおかげよ",
                    "maleVariant": "そして俺は自由になれた。それは全部、ジャッキーがいてくれたおかげだ",
                    "stringId": 1856405046538588160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その30分後にはママ・ウェルズの店で夕飯を食べてて、あたしたちには意外と共通点が多いことがわかった。生き方のね",
                    "maleVariant": "その30分後にはママ・ウェルズの店で夕飯を食べてて、俺たちには意外と共通点が多いことがわかった。生き方のな",
                    "stringId": 1856405814951858176
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "その時に、あいつが言ったことを覚えてる。「俺は町全体が家族みたいなところの出身なんだ」って。それで… あたしと似たような境遇だと感じたの",
                    "maleVariant": "その時に、あいつが言ったことを覚えてる。「俺は町全体が家族みたいなところの出身なんだ」って。それで… 俺と似たような境遇だと感じたんだ",
                    "stringId": 1856408139200253952
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "乾杯をいいかしら",
                    "maleVariant": null,
                    "stringId": 1857268243663474688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l="mex" o="Por mi hijo." t="我が息子に" b="" a=""/>",
                    "maleVariant": null,
                    "stringId": 1857270968400793600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "息子の代わりに持っていて",
                    "maleVariant": null,
                    "stringId": 1857288359176359936
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "<kiroshi l="mex" o="Cuídate, V." t="自分を大事にするのよ、V" b="" a=""/>",
                    "maleVariant": null,
                    "stringId": 1857291401774813184
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "傷でもつけたら、きっとジャッキーが怒る",
                    "maleVariant": null,
                    "stringId": 1857291779614494720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "じゃあ傷つけないように気をつけて",
                    "maleVariant": null,
                    "stringId": 1857291857326559232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そこに警官がやって来て、あたしたちは叩きのめされた。半分はあたしのせいで、半分はあいつのせい。一緒に大慌てで逃げ出したっけ",
                    "maleVariant": "そこに警官がやって来て、俺たちは叩きのめされた。半分は俺のせいで、半分はあいつのせい。一緒に大慌てで逃げ出したっけ",
                    "stringId": 1892238155099983872
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "頭に銃を向けられた。それがあいつとの出会い",
                    "maleVariant": "頭に銃を向けられた。それがあいつとの出会いだ",
                    "stringId": 1892238198183874560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あたしはジャッキーの運転手として雇われて、密輸品の運搬を手伝ってた",
                    "maleVariant": "俺はジャッキーの運転手として雇われて、密輸品の運搬を手伝ってた",
                    "stringId": 1892239992356130816
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼女はジャッキーにとって大事な存在だった",
                    "maleVariant": null,
                    "stringId": 1892438629442965508
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "名前は… ミスティ",
                    "maleVariant": null,
                    "stringId": 1892438716969701376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もういいでしょ、ママ・ウェルズ",
                    "maleVariant": "もういいだろ、ママ・ウェルズ",
                    "stringId": 1892438754332561412
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "え…？",
                    "maleVariant": null,
                    "stringId": 1892438849962692608
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もういいでしょ、ママ・ウェルズ",
                    "maleVariant": "もういいだろ、ママ・ウェルズ",
                    "stringId": 1892439027532746752
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "なんですって？",
                    "maleVariant": null,
                    "stringId": 1892441958294278144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "悪い子じゃない。ジャッキーは彼女を愛してたのよ？",
                    "maleVariant": "悪い子じゃない。ジャッキーは彼女を愛してたんだぞ？",
                    "stringId": 1892442026611101696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "絶対に裏切ることのない、最高の友達だった",
                    "maleVariant": null,
                    "stringId": 1938725270667063296
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "友達を大事にする奴だった",
                    "maleVariant": null,
                    "stringId": 1938725290984271876
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "生き方を曲げない男だった",
                    "maleVariant": null,
                    "stringId": 1938725478167670784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何があっても自分で決めた生き方を曲げない、立派な男だった",
                    "maleVariant": "何があっても自分が決めた生き方を曲げない、立派な男だった",
                    "stringId": 1938726244114690048
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "一度目標を定めたら、あいつは死ぬ覚悟でそれを成し遂げようとする",
                    "maleVariant": null,
                    "stringId": 1938727557200277504
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "恐れずに人生を生きた",
                    "maleVariant": null,
                    "stringId": 1938738185449172996
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "前にこんなことを言われた。「チーカ、誰かを恐れるな。恐怖は最大の敵だ」",
                    "maleVariant": "前にこんなことを言われた。「ブロダー、誰かを恐れるな。恐怖は最大の敵だ」",
                    "stringId": 1938738266701230080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あいつは周りが思うような人間じゃなかった",
                    "maleVariant": null,
                    "stringId": 1938739476137177088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジャッキーは人生を愛し、決して恐怖を見せなかった。考え過ぎず、思ったことを率直に伝え、精一杯に生きたはず",
                    "maleVariant": null,
                    "stringId": 1938741214927843328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "みんなジャッキーをこういう風に思ってたはず。人や物事のポジティブな面ばかりを見る、前向きで気のいい奴だって",
                    "maleVariant": null,
                    "stringId": 1938741228836155392
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自分は銃弾を食らっても絶対に倒れない、アフターライフの伝説になると息巻いてた",
                    "maleVariant": null,
                    "stringId": 1938741526161977344
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そしてあいつは、目的を成し遂げた… 権力を握る連中に一矢報いてやったのよ",
                    "maleVariant": "そしてあいつは、目的を成し遂げた… 権力を握る連中に一矢報いてやったんだ",
                    "stringId": 1938741913866661888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "もしあいつがここにいるなら、きっと今ごろ… 中指を突き立ててるでしょうね",
                    "maleVariant": "もしあいつがここにいるなら、きっと今ごろ… 中指を突き立ててるだろうな",
                    "stringId": 1938742316402405376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも、それだけじゃない。本当のジャッキー・ウェルズは不屈の精神と野心にあふれたチューマ。そしてそのために命を落とした",
                    "maleVariant": null,
                    "stringId": 1938743697335054336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "あいつは、やると言ったことを最後まで貫き、がむしゃらに富と名声を追い求めた。そこには一切の恐怖はなかったの",
                    "maleVariant": "あいつは、やると言ったことを最後まで貫き、がむしゃらに富と名声を追い求めた。そこには一切の恐怖はなかったんだ",
                    "stringId": 1938745003156107264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "今日はたくさんの人が来てくれた… ジャッキーをたぶらかしてたあの子もね",
                    "maleVariant": null,
                    "stringId": 1938881424185552896
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "彼女の名前。ジャッキーはミスティのことを大事に思ってた",
                    "maleVariant": "彼女の名前だ。ジャッキーはミスティのことを大事に思ってた",
                    "stringId": 1938885004292452352
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まずは夕飯に誘ってあげたらどう？ それくらいならいいでしょ",
                    "maleVariant": "まずは夕飯に誘ってあげたらどうだ？ それくらいならいいだろ",
                    "stringId": 1938890485123706880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[ベルトを置く] 誰にも恐れを見せることがなかった",
                    "maleVariant": null,
                    "stringId": 1942891821798432768
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[本の一節を朗読する]",
                    "maleVariant": null,
                    "stringId": 1942891950764892160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "他には…？",
                    "maleVariant": null,
                    "stringId": 1943191058796945408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "野心あふれる、ヘイウッドの申し子に",
                    "maleVariant": null,
                    "stringId": 1954616294136279040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "満足することなく、常に上を目指してた",
                    "maleVariant": null,
                    "stringId": 1954616317926371328
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "誰のことも恐れず、誰にも恐れを見せず、精一杯に人生を生き抜いた",
                    "maleVariant": null,
                    "stringId": 1954616331381698560
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "「新兵は不死の幻想を抱いて戦地に赴く。周りで仲間が殺されようと、自分は死なないと信じている… ところが初めて重傷を負った途端、その幻想は崩れ去る」",
                    "maleVariant": null,
                    "stringId": 1954616339669643264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "野心を持っていた",
                    "maleVariant": null,
                    "stringId": 1954666752167047168
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "常に上を目指して野心を燃やし、絶対に諦めない男だった",
                    "maleVariant": null,
                    "stringId": 1954666960372297728
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自分のルーツを決して忘れなかった",
                    "maleVariant": null,
                    "stringId": 1954671769041170436
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "何よりも愛する人を大事にしていた",
                    "maleVariant": null,
                    "stringId": 1954671940386877440
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "いつだって自分が望むものを理解していた",
                    "maleVariant": null,
                    "stringId": 1954672623152459776
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジャッキーの心には常にヘイウッドがあって、それがあいつを形作っていた",
                    "maleVariant": null,
                    "stringId": 1954675017496064000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "だけど何よりあいつは…",
                    "maleVariant": null,
                    "stringId": 1954675031639257088
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "愛する人のためなら、自分の命さえ迷わず犠牲にできる奴だった",
                    "maleVariant": null,
                    "stringId": 1954675040547958784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "自分が望むものをはっきり理解し、真っすぐそれに向かう奴だった",
                    "maleVariant": null,
                    "stringId": 1954675454391545856
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "来てくれて本当に… 本当にありがとう",
                    "maleVariant": null,
                    "stringId": 1959312160644308992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "ジャッキーはいい友を持ったわね",
                    "maleVariant": null,
                    "stringId": 1959312875957690372
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[座る]",
                    "maleVariant": null,
                    "stringId": 1969073723778596868
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[立ち上がる] 初めて会った時のことをよく思い出す",
                    "maleVariant": null,
                    "stringId": 1969265292137791492
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "[立ち上がり、静かに別れを告げる]",
                    "maleVariant": null,
                    "stringId": 1969265620501463040
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "さあ、座って",
                    "maleVariant": null,
                    "stringId": 1970780234074619904
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "受け取れない",
                    "maleVariant": null,
                    "stringId": 2037366793477095424
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "気持ちは嬉しいけど… これは受け取れない",
                    "maleVariant": "気持ちは嬉しいが… これは受け取れない",
                    "stringId": 2037367489530232832
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "気を悪くしないでね",
                    "maleVariant": "気を悪くしないでくれ",
                    "stringId": 2262244236776882176
                  }
                }
              ]
            }
          }
        }
      }
