        "cookingPlatform": "PLATFORM_PC",
        "root": {
          "HandleId": "0",
          "Data": {
            "Type": "localizationPersistenceSubtitleEntries",
            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "まず入れなきゃいけないのは、記憶をブロックする神経遮断装置。これが無いと、この仕事はコンマ2秒と耐えれない",
                    "maleVariant": null,
                    "stringId": 1806884847720456192
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも、装置をつけたらそのまま昏睡状態になることもあるって…",
                    "maleVariant": null,
                    "stringId": 1806886123308965888
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そんなのウソよ。トラブった話なんか聞いたことない",
                    "maleVariant": null,
                    "stringId": 1806886452612161536
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どっかで読んだんだ。日本の女の子が使ったインプラントは記憶どころか――",
                    "maleVariant": null,
                    "stringId": 1806886643167780864
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "起きた瞬間に、それまで見てた夢を忘れちゃうときってあるでしょ？ それと同じ",
                    "maleVariant": null,
                    "stringId": 1806886818875564032
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "でも、どうやって目覚めるんだ？",
                    "maleVariant": null,
                    "stringId": 1806887058856861696
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "しばらく意識をなくしたあと… パッと現実に戻ったら、ポケットの中のお金が増えてるのよ",
                    "maleVariant": null,
                    "stringId": 1824543639562719232
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そうね… アラームをセットしたような感じ。一瞬で意識をなくして、次の瞬間には戻ってる。私なんか時々休みたい時にも使っちゃうの",
                    "maleVariant": null,
                    "stringId": 1824544161149587456
                  }
                }
              ]
            }
          }
        }
      }
    },
    "EmbeddedFiles": []
  }
