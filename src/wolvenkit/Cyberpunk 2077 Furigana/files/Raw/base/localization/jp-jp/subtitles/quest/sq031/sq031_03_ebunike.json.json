      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メイルストロームと遭遇したら援護は頼んだよ",
              "maleVariant": "",
              "stringId": 1750509373849559048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあ、行こう",
              "maleVariant": "",
              "stringId": 1750509373849559056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウェイランドがグレイソンを見た時、誰かと一緒にいた",
              "maleVariant": "",
              "stringId": 1751957451383201792
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スマッシャーね",
              "maleVariant": "スマッシャーか",
              "stringId": 1751958386327117824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だといいが。まずはメイルストロームのデータ端末だ",
              "maleVariant": "",
              "stringId": 1751973563986567168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "で、作戦は？",
              "maleVariant": "",
              "stringId": 1752060664715370496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "準備は？",
              "maleVariant": "",
              "stringId": 1752952913720467456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あった。エブニケ号",
              "maleVariant": "",
              "stringId": 1753741636431208448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "冴えない船ね",
              "maleVariant": "冴えない船だな",
              "stringId": 1753742316982198272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんな所を隠れ家にしてるとは…",
              "maleVariant": "",
              "stringId": 1753745583606816768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スマッシャーが表札でも出してると？",
              "maleVariant": "",
              "stringId": 1753745583606816776
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "甲板を調べよう",
              "maleVariant": "",
              "stringId": 1753761192021286912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "了解",
              "maleVariant": "",
              "stringId": 1753761565549223936
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつは任せた",
              "maleVariant": "",
              "stringId": 1758078778762551296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気をつけな",
              "maleVariant": "",
              "stringId": 1758079373548412928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もっと気合い入れな",
              "maleVariant": "",
              "stringId": 1758079394788368384
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "片付けよう",
              "maleVariant": "",
              "stringId": 1758079415910883328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "野郎はこの辺りに隠れているはずだ",
              "maleVariant": "",
              "stringId": 1779365959497900032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メイルストロームの倉庫に向かう",
              "maleVariant": "",
              "stringId": 1779371727420874752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "データ端末はそこにある。スマッシャーの情報がないか調べるんだ",
              "maleVariant": "",
              "stringId": 1779372922478747648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグはいつも無愛想なの？",
              "maleVariant": "ローグはいつも無愛想なのか？",
              "stringId": 1779388340320821252
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "倉庫を目指すとしよう",
              "maleVariant": "",
              "stringId": 1790608732934856704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メイルストロームと仕事してたとは",
              "maleVariant": "",
              "stringId": 1790619154236792832
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "データ端末の場所は、いったいどうやって調べたの？",
              "maleVariant": "データ端末の場所は、いったいどうやって調べたんだ？",
              "stringId": 1790619330145902596
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK、始めましょ",
              "maleVariant": "OK、始めよう",
              "stringId": 1790619351721402372
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰とでもやるよ。こっちの条件に合えばね",
              "maleVariant": "",
              "stringId": 1790621711050956800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "50年も経てば、いろいろ変わるんだな",
              "maleVariant": "",
              "stringId": 1790621919373647872
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "私が仕入れたのさ",
              "maleVariant": "",
              "stringId": 1790622720385048576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ついてきな。ここはよく知ってる",
              "maleVariant": "",
              "stringId": 1790628138519846912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "緊張してるな",
              "maleVariant": "",
              "stringId": 1790629297389264896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "早く",
              "maleVariant": "",
              "stringId": 1790692867535757312
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "敵がいる",
              "maleVariant": "",
              "stringId": 1790730040242823168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "船に隠れてるようね",
              "maleVariant": "船に隠れてるようだな",
              "stringId": 1790787977690701828
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし。調べてみよう",
              "maleVariant": "",
              "stringId": 1790789883163009024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし。あれだ",
              "maleVariant": "",
              "stringId": 1791157074513321984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スキャンして、使えそうなものを探して",
              "maleVariant": "",
              "stringId": 1791157840460341248
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "居場所はとっくにわかってるくせに",
              "maleVariant": "",
              "stringId": 1791176008641765376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "開けられるかい？",
              "maleVariant": "",
              "stringId": 1791421008340279296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…まるで処刑ね",
              "maleVariant": "まるで… 処刑だな",
              "stringId": 1792091878675144704
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ホローポイント弾で至近距離から撃たれてる",
              "maleVariant": "",
              "stringId": 1792092740956938240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こいつは、スマッシャーのやり口だ",
              "maleVariant": "",
              "stringId": 1792099513382166528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "録音データみたいね",
              "maleVariant": "録音データみたいだな",
              "stringId": 1792141059758448644
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "再生できるかい？",
              "maleVariant": "",
              "stringId": 1792141059775225860
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、やってみる",
              "maleVariant": "ああ、やってみる",
              "stringId": 1792141059775225868
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんなことして何になる？",
              "maleVariant": "",
              "stringId": 1792141060026884100
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰が後ろについてるか知らないが、マロリーに何かあったら…",
              "maleVariant": "",
              "stringId": 1792266351822987264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それまでにナイト・コープとの取引から手を引かなければ、女は死ぬ",
              "maleVariant": "",
              "stringId": 1792270501449261056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、わからないのか！？ 俺一人を脅迫したところで、ミリテク役員全員を操れるはずが――",
              "maleVariant": "",
              "stringId": 1792270850650234880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "24時間だぞ",
              "maleVariant": "",
              "stringId": 1792271201864474624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これが企業の日常",
              "maleVariant": "",
              "stringId": 1792272104310923264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残り時間は？",
              "maleVariant": "",
              "stringId": 1792275223900020740
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1時間。計画は失敗だな。思ったとおりだ",
              "maleVariant": "",
              "stringId": 1792275223900020748
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあ… 何のために女を…",
              "maleVariant": "",
              "stringId": 1792275223900020756
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "後1時間で死ぬ。それだけのこと",
              "maleVariant": "",
              "stringId": 1792275223900020764
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何でこんなのを残したんだ？",
              "maleVariant": "",
              "stringId": 1792275223950352388
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マロリーってのはさっきの遺体かもね",
              "maleVariant": "",
              "stringId": 1792279675616845824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いつ殺されたのかもわからない",
              "maleVariant": "",
              "stringId": 1792280478624735232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "カン・タオを襲った連中はこいつらか",
              "maleVariant": "",
              "stringId": 1792501006958551044
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "プロの仕事だった",
              "maleVariant": "",
              "stringId": 1792501006975328260
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "本当にその時の品なの？",
              "maleVariant": "本当にその時の品なのか？",
              "stringId": 1792501006975328268
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、もうそろそろいいだろ",
              "maleVariant": "",
              "stringId": 1792501007143100420
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "中はもっとひどいよ",
              "maleVariant": "",
              "stringId": 1796915671132868608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "行こう",
              "maleVariant": "",
              "stringId": 1796915842361135104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はぐれるんじゃないよ",
              "maleVariant": "",
              "stringId": 1796916600724852736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "念のため援護してやれ",
              "maleVariant": "",
              "stringId": 1796922193829236736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "静かにデータ端末まで行くんだ",
              "maleVariant": "",
              "stringId": 1796924355405746176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "グレイソンとスマッシャーを一度に片付けられたら、ツイてるね",
              "maleVariant": "",
              "stringId": 1796925320699645952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "外部データベース、アテナD-III",
              "maleVariant": "",
              "stringId": 1796941254759878656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ドアを見張ってる",
              "maleVariant": "",
              "stringId": 1812872038536515584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ためらってるな。やっぱりまだあの野郎が怖いんだ",
              "maleVariant": "",
              "stringId": 1814213857690062848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さっさと野郎を見つけて片付けるぞ",
              "maleVariant": "",
              "stringId": 1814318375987646464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "24時間やろう",
              "maleVariant": "",
              "stringId": 1816931139113521152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だからお前を脅迫してるんだ",
              "maleVariant": "",
              "stringId": 1816945094452121600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "表では笑顔で握手し、互いに尻尾を振り合ってるが――",
              "maleVariant": "",
              "stringId": 1817010448704540672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "裏ではスマッシャーに銃を突きつけられてる",
              "maleVariant": "",
              "stringId": 1817010751751393280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "グレイソンが、スマッシャーをゆするためにやったのかも",
              "maleVariant": "",
              "stringId": 1817012601808556032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "グレイソンを買いかぶりすぎだと思うぞ",
              "maleVariant": "",
              "stringId": 1817013630990094336
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スマッシャーには、これが日常なんだろ",
              "maleVariant": "",
              "stringId": 1817014774541914112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "679275-PN1H。私も行方探しを依頼されたんだ",
              "maleVariant": "",
              "stringId": 1817037794107002880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それでも隠し通すなんて、なかなかやるじゃないか",
              "maleVariant": "",
              "stringId": 1817038599581143040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいかげん、クソ野郎を倒しに行くぞ",
              "maleVariant": "",
              "stringId": 1817042427185532928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "最初のうちはな",
              "maleVariant": "",
              "stringId": 1817046671401750528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "トリフェットだ",
              "maleVariant": "",
              "stringId": 1869862073709510656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見ろ、ご丁寧に「サプリメント」って書いてある！",
              "maleVariant": "",
              "stringId": 1869862334108680196
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "小分けにして街で売る前に、ここで保管してるってわけか",
              "maleVariant": "",
              "stringId": 1894108944321499136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてつきあってくれるの？",
              "maleVariant": "どうしてつきあってくれるんだ？",
              "stringId": 1897947614879174656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ケリをつけたいからさ",
              "maleVariant": "",
              "stringId": 1897953148457996288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニーの仇？",
              "maleVariant": "",
              "stringId": 1897954773683687424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それもある",
              "maleVariant": "",
              "stringId": 1897959263115694080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグはタワー以来、あいつにビビっちまってた",
              "maleVariant": "",
              "stringId": 1897959622701764608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっと向き合う気になったんだな",
              "maleVariant": "",
              "stringId": 1897970598608130048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今の見たか？",
              "maleVariant": "",
              "stringId": 1898025977564123136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "考えがある",
              "maleVariant": "",
              "stringId": 1898026287539965952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "右に向かうんだ",
              "maleVariant": "",
              "stringId": 1898035855301038080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ご飯の時間みたい",
              "maleVariant": "飯の時間みたいだな",
              "stringId": 1898042369843679232
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "静かに片付けるんだ",
              "maleVariant": "",
              "stringId": 1898042622592438272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シーッ、起こす気かい？",
              "maleVariant": "",
              "stringId": 1898042885474635776
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あれくらいあたしだって…",
              "maleVariant": "あれくらい俺だって…",
              "stringId": 1898044829769420800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの身のこなしにはいつも惚れ惚れする",
              "maleVariant": "",
              "stringId": 1898046673736454144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少し待ってからついてきな",
              "maleVariant": "",
              "stringId": 1898050120464486400
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "音を立てるな",
              "maleVariant": "",
              "stringId": 1898055222264877056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これは… 触らないでおこう",
              "maleVariant": "",
              "stringId": 1908683275202482176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ここにはいろいろありそうだね",
              "maleVariant": "",
              "stringId": 1908691953418784768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "前に来た時より警備が強化されたかも",
              "maleVariant": "",
              "stringId": 1911495907236265984
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何てことない",
              "maleVariant": "",
              "stringId": 1911497156987863040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どういう作戦なの？",
              "maleVariant": "どういう作戦なんだ？",
              "stringId": 1916908206649241600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたがメイルストロームと仕事をしてたなんて",
              "maleVariant": "",
              "stringId": 1916908206649241616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "データ端末の場所をなぜ知ってるの？",
              "maleVariant": "データ端末の場所をなぜ知ってるんだ？",
              "stringId": 1916908206649241636
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "OK、始めましょ",
              "maleVariant": "OK、始めよう",
              "stringId": 1916908206649241668
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃ、ぶっ放すか",
              "maleVariant": "",
              "stringId": 1960994083227713536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…まるで処刑ね",
              "maleVariant": "…まるで処刑だな",
              "stringId": 1961039143893876740
