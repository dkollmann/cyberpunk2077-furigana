    "1": {
      "Type": "localizationPersistenceSubtitleEntries",
      "ParentIndex": -1,
      "Properties": {
        "entries": [
          {
            "stringId": 1730251372773777408,
            "femaleVariant": "もうすぐ… 着く"
          },
          {
            "stringId": 1764773498032504832,
            "femaleVariant": "こっちもよ"
          },
          {
            "stringId": 1764773972458618880,
            "femaleVariant": "ただ見張るだけにして。わかったわね"
          },
          {
            "stringId": 1764777519766061056,
            "femaleVariant": "俺は残ろう"
          },
          {
            "stringId": 1764788193162444800,
            "femaleVariant": "あんたに何かを変えることなんかできない"
          },
          {
            "stringId": 1764793118164533248,
            "femaleVariant": "あんたに何かを変えることなんかできない"
          },
          {
            "stringId": 1764796995983130624,
            "femaleVariant": "なに、探し出して、今度こそきっちりととどめを刺すつもり？",
            "maleVariant": "なんだ、探し出して、今度こそきっちりととどめを刺すつもりか？"
          },
          {
            "stringId": 1764798658403909632,
            "femaleVariant": "お前さえいなければ… あいつは家で、何事もなく過ごせたのに"
          },
          {
            "stringId": 1764809843236945920,
            "femaleVariant": "ハナコ"
          },
          {
            "stringId": 1764812022580527104,
            "femaleVariant": "助かったわ、V。これで終わりよ"
          },
          {
            "stringId": 1764812290730770432,
            "femaleVariant": "心配しないで、約束は守る。あなたはヘルマンと行きなさい"
          },
          {
            "stringId": 1764812556632866816,
            "femaleVariant": "私はまだ兄に用がある"
          },
          {
            "stringId": 1764818464779812864,
            "femaleVariant": "わかってる"
          },
          {
            "stringId": 1764821320329084928,
            "femaleVariant": "だが… もう… 疲れた…"
          },
          {
            "stringId": 1764822155062689792,
            "femaleVariant": "運命の女神は実に気まぐれだ"
          },
          {
            "stringId": 1764822519379935232,
            "femaleVariant": "一生をかけて抗ってきたその相手に…"
          },
          {
            "stringId": 1764822812914106368,
            "femaleVariant": "まさか、自分自身がなってしまうとは"
          },
          {
            "stringId": 1811303377379393536,
            "femaleVariant": "このような結末を迎えようとは…"
          },
          {
            "stringId": 1811333724326461440,
            "femaleVariant": "お前は…"
          },
          {
            "stringId": 1812519762868699140,
            "femaleVariant": "敗北者"
          },
          {
            "stringId": 1812520064506265600,
            "femaleVariant": "恐怖"
          },
          {
            "stringId": 1812521791066341376,
            "femaleVariant": "失敗したクーデター"
          },
          {
            "stringId": 1812534040866717696,
            "femaleVariant": "恐怖"
          },
          {
            "stringId": 1812535291994046464,
            "femaleVariant": "失敗に終わった… クーデター…"
          },
          {
            "stringId": 1812537057108164608,
            "femaleVariant": "これだけは認めざるを得ない。あいつは人に恐怖を抱かせる術を心得ていた"
          },
          {
            "stringId": 1812537330341904384,
            "femaleVariant": "今でもだ。見ただろう！ 死人の言葉にバカどもは震え上がっていた！"
          },
          {
            "stringId": 1812547933827629056,
            "femaleVariant": "かつて教えられたよ。旗は風があるからこそはためく"
          },
          {
            "stringId": 1812568124099788800,
            "femaleVariant": "あんたの部下はまだ意味もなく血を流してる"
          },
          {
            "stringId": 1812574171212414976,
            "femaleVariant": "そして無用の流血"
          },
          {
            "stringId": 1812574324472283136,
            "femaleVariant": "無用？"
          },
          {
            "stringId": 1812574425437569024,
            "femaleVariant": "あんたの負けよ",
            "maleVariant": "あんたの負けだ"
          },
          {
            "stringId": 1812629191756742656,
            "femaleVariant": "物心ついてからずっと、俺は恐れていた"
          },
          {
            "stringId": 1812632513611956224,
            "femaleVariant": "違う。負けたのは彼らだ"
          },
          {
            "stringId": 1812633876190330880,
            "femaleVariant": "サブロウね…",
            "maleVariant": "サブロウか…"
          },
          {
            "stringId": 1812686126195539968,
            "femaleVariant": "あいつの思惑は外れたがな"
          },
          {
            "stringId": 1812695621126717440,
            "femaleVariant": "それも終わりよ",
            "maleVariant": "それも終わりだ"
          },
          {
            "stringId": 1812720544704495616,
            "femaleVariant": "哀れな"
          },
          {
            "stringId": 1812732321957806084,
            "femaleVariant": "[武器を回収する] これはもう要らない"
          },
          {
            "stringId": 1812733448849850368,
            "femaleVariant": "これはもう要らないでしょ",
            "maleVariant": "これはもう要らないだろ"
          },
          {
            "stringId": 1812735471896887296,
            "femaleVariant": "弾は残り1発"
          },
          {
            "stringId": 1812756784984227840,
            "femaleVariant": "何が見える？"
          },
          {
            "stringId": 1812760281825804288,
            "femaleVariant": "俺は守ろうとしたんだ"
          },
          {
            "stringId": 1812773702457753600,
            "femaleVariant": "妹はどこだ？"
          },
          {
            "stringId": 1812774610776227844,
            "femaleVariant": "[座る] 動かないで。彼女もすぐに来る",
            "maleVariant": "[座る] 動くな。彼女もすぐに来る"
          },
          {
            "stringId": 1812774896710320128,
            "femaleVariant": "すぐに来るはずよ",
            "maleVariant": "すぐに来るはずだ"
          },
          {
            "stringId": 1812775257001033728,
            "femaleVariant": "[座る] あんたが殺しかけた妹のこと？",
            "maleVariant": "[座る] あんたが殺しかけた妹のことか？"
          },
          {
            "stringId": 1812776755692961792,
            "femaleVariant": "それまであんたが変な真似をしないよう、見張らせてもらう"
          },
          {
            "stringId": 1812777300348502016,
            "femaleVariant": "今さら… 俺が何をするというんだ。すべて無に帰した"
          },
          {
            "stringId": 1812783472283615232,
            "femaleVariant": "京都"
          },
          {
            "stringId": 1812786215761084416,
            "femaleVariant": "ドバイ… パリ…"
          },
          {
            "stringId": 1812787177565323264,
            "femaleVariant": "忘れることができたのに"
          },
          {
            "stringId": 1812789993453256704,
            "femaleVariant": "もう叶わない"
          },
          {
            "stringId": 1812791346921603072,
            "femaleVariant": "忘れるって何をよ？",
            "maleVariant": "忘れるって何をだ？"
          },
          {
            "stringId": 1812791501154549760,
            "femaleVariant": "恐怖を、だ"
          },
          {
            "stringId": 1813920561503010816,
            "femaleVariant": "俺は結果を恐れたりしない。一度も恐れたことはない"
          },
          {
            "stringId": 1813922483182108672,
            "femaleVariant": "いいのよ、兄さん…"
          },
          {
            "stringId": 1813923509444411392,
            "femaleVariant": "もうお休みなさい"
          },
          {
            "stringId": 1813976407452811264,
            "femaleVariant": "何の話？",
            "maleVariant": "何の話だ？"
          },
          {
            "stringId": 1814006021453643776,
            "femaleVariant": "敗北者"
          },
          {
            "stringId": 1814022075638743040,
            "femaleVariant": "お前の分じゃない"
          },
          {
            "stringId": 1814022848195985408,
            "femaleVariant": "馬鹿め"
          },
          {
            "stringId": 1814023095995465728,
            "femaleVariant": "負けたのは俺じゃない"
          },
          {
            "stringId": 1814029022865784832,
            "femaleVariant": "バカげてる"
          },
          {
            "stringId": 1814029563192803328,
            "femaleVariant": "理解できない"
          },
          {
            "stringId": 1814030775447322624,
            "femaleVariant": "バカげてる"
          },
          {
            "stringId": 1814030937230016512,
            "femaleVariant": "つまり恐怖を忘れるためには、死ななきゃいけないってこと？",
            "maleVariant": "つまり恐怖を忘れるためには、死ななきゃいけないってことか？"
          },
          {
            "stringId": 1814034734081769472,
            "femaleVariant": "悪いけど、まったく理解できない",
            "maleVariant": "悪いが、まったく理解できないな"
          },
          {
            "stringId": 1814063613945184256,
            "femaleVariant": "いいえ、まだよ",
            "maleVariant": "いいや、まだだ"
          },
          {
            "stringId": 1814067567210921984,
            "femaleVariant": "あんたは？"
          },
          {
            "stringId": 1814067861667840000,
            "femaleVariant": "二人にして"
          },
          {
            "stringId": 1814069810307923968,
            "femaleVariant": "行こう。次は君のチップの件だ"
          },
          {
            "stringId": 1814138702556430336,
            "femaleVariant": "もうすぐ着く"
          },
          {
            "stringId": 1814140077969375232,
            "femaleVariant": "忘れないで。 髪の毛一本、傷つけてはダメよ"
          },
          {
            "stringId": 1814148897718272000,
            "femaleVariant": "はいはい。あんたのお楽しみは取っとく"
          },
          {
            "stringId": 1814153083969208320,
            "femaleVariant": "中に入れば、きっと奴を殺してしまうからな"
          },
          {
            "stringId": 1814172819629146112,
            "femaleVariant": "それを変えるはずだった。お前が邪魔をするまではな"
          },
          {
            "stringId": 1814177778235998208,
            "femaleVariant": "勘違いするな… 巻き込むつもりはなかった"
          },
          {
            "stringId": 1814178425433882624,
            "femaleVariant": "ハナコはどこだ！？"
          },
          {
            "stringId": 1814179058656346112,
            "femaleVariant": "見ろ"
          },
          {
            "stringId": 1814188731157577728,
            "femaleVariant": "風は恐怖だ"
          },
          {
            "stringId": 1814192978427248640,
            "femaleVariant": "あんたは父親と一緒よ。ただ権力に飢えてるだけ",
            "maleVariant": "あんたは父親と一緒――ただ権力に飢えてるだけだ"
          },
          {
            "stringId": 1814193124456136704,
            "femaleVariant": "サブロウ以上のクズね",
            "maleVariant": "サブロウ以上のクズだな"
          },
          {
            "stringId": 1814194290925633536,
            "femaleVariant": "サブロウ以上のクズね",
            "maleVariant": "サブロウ以上のクズだな"
          },
          {
            "stringId": 1814195428957749248,
            "femaleVariant": "あいつは恐怖で支配こそしても、権力のために粛清するようなことはしなかった"
          },
          {
            "stringId": 1815436412748169216,
            "femaleVariant": "自分が爆弾になるんだ"
          },
          {
            "stringId": 1815701927408304128,
            "femaleVariant": "権力？"
          },
          {
            "stringId": 1815702212755193856,
            "femaleVariant": "俺が、そんなものを求めていたとでも"
          },
          {
            "stringId": 1815703471197388800,
            "femaleVariant": "権力だと？ ハッ… くだらん。権力など"
          },
          {
            "stringId": 1815771519770034176,
            "femaleVariant": "ふん"
          },
          {
            "stringId": 1815773885844029440,
            "femaleVariant": "そして父は… 俺の顔に息を… 恐怖を吹きかけた"
          },
          {
            "stringId": 1815779097468076032,
            "femaleVariant": "だが他の奴らは… まんまと、恐怖に駆られた"
          },
          {
            "stringId": 1816876981856894976,
            "femaleVariant": "それで？ 何か変わったか？"
          },
          {
            "stringId": 1817103687813300224,
            "femaleVariant": "文字通りにな"
          },
          {
            "stringId": 1837104310493515776,
            "femaleVariant": "半世紀前、テロリストはこの建物を吹き飛ばした"
          },
          {
            "stringId": 1837105396835028992,
            "femaleVariant": "爆弾を仕掛けても足りないとしたら、一体どうすればいい？"
          },
          {
            "stringId": 1837117112767614980,
            "femaleVariant": "ありがとう。まさかホントにうまくいくとは"
          },
          {
            "stringId": 1837117917822324736,
            "femaleVariant": "ありがとう、ゴロウ。まさかホントにうまくいくとはね",
            "maleVariant": "ありがとう、ゴロウ。まさかホントにうまくいくとはな"
          },
          {
            "stringId": 1837277160848363520,
            "femaleVariant": "それは知ってる"
          },
          {
            "stringId": 1837277790312730624,
            "femaleVariant": "いいえ",
            "maleVariant": "いいや"
          },
          {
            "stringId": 1837281272977559552,
            "femaleVariant": "そうだろう！"
          },
          {
            "stringId": 1837283129477173248,
            "femaleVariant": "だが俺は学んだ！"
          },
          {
            "stringId": 1842988048553218048,
            "femaleVariant": "そうなればハナコ様の協力を得られなくなるのだろう？"
          },
          {
            "stringId": 1842988447750295552,
            "femaleVariant": "お前のためだ。遠慮しよう"
          },
          {
            "stringId": 1843009456649932800,
            "femaleVariant": "行け、V。ケリをつけてくるんだ"
          },
          {
            "stringId": 1843243291010535424,
            "femaleVariant": "自分の心配をした方がいい。早く… ここから逃げて",
            "maleVariant": "自分の心配をした方がいい。早く… ここから逃げろ"
          },
          {
            "stringId": 1843243631034372096,
            "femaleVariant": "なぜだ…？"
          },
          {
            "stringId": 1843243683362508800,
            "femaleVariant": "アラサカのことは忘れて",
            "maleVariant": "アラサカのことは忘れるんだ"
          },
          {
            "stringId": 1843244422902190080,
            "femaleVariant": "サブロウのボディガードとして、職務を全うできなかったのよ？",
            "maleVariant": "サブロウのボディガードとして、職務を全うできなかったんだぞ？"
          },
          {
            "stringId": 1843245068271357952,
            "femaleVariant": "それは… わからない…"
          },
          {
            "stringId": 1843246683950813184,
            "femaleVariant": "今さら戻って、歓迎されると思う？",
            "maleVariant": "今さら戻って、歓迎されると思うか？"
          },
          {
            "stringId": 1843249540372500480,
            "femaleVariant": "街で働くか、猫の世話でもすればいい。アラサカとは距離を置くのね",
            "maleVariant": "街で働くか、猫の世話でもすりゃいい。アラサカとは距離を置け"
          },
          {
            "stringId": 1843267840070520832,
            "femaleVariant": "あんたのために言うけど、逃げたほうがいい",
            "maleVariant": "あんたのために言うが、逃げたほうがいい"
          },
          {
            "stringId": 1843270350713786368,
            "femaleVariant": "それは叶わない話だ"
          },
          {
            "stringId": 1843271391387717632,
            "femaleVariant": "だが、助言には感謝する"
          },
          {
            "stringId": 1863433761340596224,
            "femaleVariant": "やめて。人の前よ"
          },
          {
            "stringId": 1863438340077723648,
            "femaleVariant": "待っていたぞ… 世界が崩れ去る時は、必ずお前がそこにいる"
          },
          {
            "stringId": 1863444095904325632,
            "femaleVariant": "兄さん"
          },
          {
            "stringId": 1886410598805225472,
            "femaleVariant": "[立ち上がる]"
          },
          {
            "stringId": 1886553994928668676,
            "femaleVariant": "二人にしてやろう"
          },
          {
            "stringId": 1886553994962223108,
            "femaleVariant": "どうした。立ってるのもやっとじゃないか"
          },
          {
            "stringId": 1925677343180931072,
            "femaleVariant": "すぐに来る。動かないで",
            "maleVariant": "すぐに来る。動くんじゃない"
          },
          {
            "stringId": 1925677343180931088,
            "femaleVariant": "あんたが殺しかけた妹のこと？",
            "maleVariant": "あんたが殺しかけた妹のことか？"
          },
          {
            "stringId": 2089643690079371264,
            "femaleVariant": "あんたは？"
          },
          {
            "stringId": 2121504880296280064,
            "femaleVariant": "[最後の旅へと向かう]"
          },
          {
            "stringId": 2228675042219499520,
            "femaleVariant": "あんたは自らすべてを捨てただけ",
            "maleVariant": "あんたは自らすべてを捨てただけだ"
          },
          {
            "stringId": 2228676119048331264,
            "femaleVariant": "あんたもかつてはギャングのリーダーだったんでしょ…",
            "maleVariant": "あんたもかつてはギャングのリーダーだったんだろ…"
          },
          {
            "stringId": 2228677970162458624,
            "femaleVariant": "でも権力のために、夢も理想も――すべてを捨てた",
            "maleVariant": "だが権力のために、夢も理想も――すべてを捨てた"
          },
          {
            "stringId": 2230339558707666944,
            "femaleVariant": "どうしたと言うんだ？ さっさとボタンを押せ"
          },
          {
            "stringId": 2230345571896762376,
            "femaleVariant": "どこに行く気だ？"
          },
          {
            "stringId": 2230347837039026176,
            "femaleVariant": "V、一体いつまでここにいるつもり？ 早くヘルマンと行きなさい"
          }
        ]
      }
    }
  }
}