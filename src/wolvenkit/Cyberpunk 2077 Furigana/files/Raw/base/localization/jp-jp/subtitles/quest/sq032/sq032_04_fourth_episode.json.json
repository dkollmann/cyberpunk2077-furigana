  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おいおい、あれ見ろよ",
              "maleVariant": "",
              "stringId": "1813916428740239364"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信じらんねえ…",
              "maleVariant": "",
              "stringId": "1813916636224069636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゴキブリみてえな連中だ",
              "maleVariant": "",
              "stringId": "1813916784232669184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "笑えねえよ。周りを、連中がここに何したかを見てみろ",
              "maleVariant": "",
              "stringId": "1813923430308683776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいや、俺がほかにもゴミ認定してんのは、アフターライフで仕事した途端、立派な反乱者を気取るコソ泥傭兵だよ",
              "maleVariant": "",
              "stringId": "1814232836967669760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分だってアラサカに殺されたくせに",
              "maleVariant": "",
              "stringId": "1867797275593957380"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのアラサカに殺された身分で何言ってんの。デジタルゾンビの助言なんていらない",
              "maleVariant": "そのアラサカに殺された身分で何言ってんだ。デジタルゾンビの助言なんていらない",
              "stringId": "1867801604132462596"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "近頃のアラサカは社員教育がなってねえようだな。都合の悪い真実は見て見ぬふりか？",
              "maleVariant": "",
              "stringId": "1867801604132462608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪いけど、お喋りにつきあってる暇はない",
              "maleVariant": "悪いが、お喋りにつきあってる暇はない",
              "stringId": "1867803431506829316"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "信用もなきゃ金もない。かわいそうな奴だよ、お前は",
              "maleVariant": "肝っ玉もねえ、かわいそうな奴め",
              "stringId": "1867803431506829328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "無名の傭兵で、相棒になってくれる奴もいない",
              "maleVariant": "",
              "stringId": "1867803431506829344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言いたいことがあるならはっきり言って。何か思ってることがあるんでしょ",
              "maleVariant": "言いたいことがあるならはっきり言え。何か思ってることがあるんだろ",
              "stringId": "1867803623018749956"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前がどんな奴なのか… それを知るために、俺はずっとお前を観察してきた",
              "maleVariant": "",
              "stringId": "1867803623035527172"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "初めはただの不運な奴だと思ってたが、しばらく見てるうちに、お前の問題がわかったよ",
              "maleVariant": "",
              "stringId": "1867803623035527200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] あたしの過去にずいぶん詳しいのね",
              "maleVariant": "[座る] 俺の過去にずいぶん詳しいんだな",
              "stringId": "1867808446703230976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの過去に詳しいのね",
              "maleVariant": "俺の過去に詳しいんだな",
              "stringId": "1867808861184352260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ま、記憶の断片を見たからな。お前も俺のを見たんだろ",
              "maleVariant": "",
              "stringId": "1867808861184352272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分が自分じゃなくなるようで怖い",
              "maleVariant": "",
              "stringId": "1867822643952508928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんなの、このチップ",
              "maleVariant": "なんなんだ、このチップ",
              "stringId": "1867822643952508944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2人分の記憶が混じり合うなんて信じられない。このチップのせいで、あたしの人生は無茶苦茶",
              "maleVariant": "2人分の記憶が混じり合うなんて信じられない。このチップのせいで、俺の人生は無茶苦茶だ",
              "stringId": "1867876421422571524"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺じゃなくてチップのせいでよかったよ",
              "maleVariant": "",
              "stringId": "1867876421439348736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろんあんたにも嫌気はさすけど、命が脅かされることのほうが重大ね",
              "maleVariant": "もちろんお前にも嫌気はさすが、命が脅かされることのほうが重大だ",
              "stringId": "1867876421439348748"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "別の誰かになって死んでいく… それほど理不尽な運命もないよな",
              "maleVariant": "",
              "stringId": "1867876421439348760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "嫌な奴ね",
              "maleVariant": "嫌な奴だ",
              "stringId": "1867876578742525956"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奇遇だな、お前もだ。逆に相性がいいんじゃないか？",
              "maleVariant": "",
              "stringId": "1867876578759303176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いずれあんたの記憶と自分のを区別できなくなるのね…",
              "maleVariant": "いずれお前の記憶と自分のを区別できなくなるのか…",
              "stringId": "1867876737639538692"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "待てよ。てことは、デショーン風情に撃ち殺された記憶が俺のものになっちまう、ってことか",
              "maleVariant": "",
              "stringId": "1867876737639538704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、その変化は認識できるの？ それとも気づかないうちにあんたになっちゃうのかな？",
              "maleVariant": "なあ、その変化は認識できるのか？ それとも気づかないうちにお前になっちまうのかな",
              "stringId": "1867876737639538716"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "人間はアイデンティティを奪われることをもっとも嫌がる… 俺にわかるのはそれだけだ",
              "maleVariant": "",
              "stringId": "1867876737656315904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしにどうしてほしいの？",
              "maleVariant": "俺にどうしてほしいんだ？",
              "stringId": "1867877243137056768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "他人の頭のなかにいるって、どんな感じ？",
              "maleVariant": "他人の頭のなかにいるって、どんな感じだ？",
              "stringId": "1867877520145670144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[会話を終える] あんたの同情なんていらない",
              "maleVariant": "[会話を終える] お前の同情なんていらない",
              "stringId": "1867877520145670160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしにどうしてほしいわけ？ 何をどうしたいの？",
              "maleVariant": "俺にどうしてほしいんだ？ 何をどうしたいんだよ？",
              "stringId": "1867877778112143364"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカとケリをつけるのに、手を貸してほしい。それだけだ",
              "maleVariant": "",
              "stringId": "1867877778128920584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしの記憶は… その… どんな感じがした？",
              "maleVariant": "俺の記憶は… その… どんな感じがした？",
              "stringId": "1867877984270573572"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんな感じって？",
              "maleVariant": "",
              "stringId": "1867877984287350792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見たり聞いたりした部分以外はどうだったかってこと。あたしの感情も… 追体験したの？",
              "maleVariant": "見たり聞いたりした部分以外はどうだったかってことだ。俺の感情も… 追体験したのか？",
              "stringId": "1867877984287350804"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "怒りや後悔、苦しみに恐怖。だが正直それが自分のなのか区別がつかなかった",
              "maleVariant": "",
              "stringId": "1867877984287350816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしのなかは快適？",
              "maleVariant": "俺のなかは快適か？",
              "stringId": "1867882978965217300"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "んー、なんつうか、いろんな感覚が極端なんだ。それとお前のホルモンの状態は滅茶苦茶だ",
              "maleVariant": "",
              "stringId": "1867882978965217312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "同情なんていらない",
              "maleVariant": "",
              "stringId": "1867883393513447428"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何だよ！ 人が力になろうとしてやってんのに！",
              "maleVariant": "",
              "stringId": "1867883393530224648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死が怖いの？",
              "maleVariant": "死が怖いのか？",
              "stringId": "1867883393530224660"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チャンスを無駄にするのが怖いんだ",
              "maleVariant": "",
              "stringId": "1867883393530224672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカ・タワーみたいに？",
              "maleVariant": "",
              "stringId": "1867883393547001856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺はこうやって戻ってきた。決着はまだついてねえ",
              "maleVariant": "",
              "stringId": "1867883393547001868"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "すべて自分の手で解決しないと気が済まないタイプでしょ",
              "maleVariant": "すべて自分の手で解決しないと気が済まないタイプだろ",
              "stringId": "1867883730651602944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "自分が何をしたいかわかってないんでしょ",
              "maleVariant": "自分が何をしたいかわかってないんだろ",
              "stringId": "1867883730668380160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしにはわかる… 自分で自分の主導権を握れない、そんなのをあんたがよしとするはずがない",
              "maleVariant": "俺にはわかる… 自分で自分の主導権を握れない、そんなのをお前がよしとするはずがない",
              "stringId": "1867884570082828292"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、何を言ってる",
              "maleVariant": "",
              "stringId": "1867884570099605504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こんな話をして何になるの？",
              "maleVariant": "こんな話をして何になるんだ？",
              "stringId": "1867887071230189572"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "力になろうとしてるだけだ！",
              "maleVariant": "",
              "stringId": "1867887071230189584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはテレビよ。50年前にはなかった？",
              "maleVariant": "それはテレビだ。50年前にはなかったか？",
              "stringId": "1869149005199990784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん… 50年前と言えばな――昔この辺りは一面畑だったんだ。農家がみんな企業に刈られる前はな",
              "maleVariant": "",
              "stringId": "1869149324369747968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう黙ってて",
              "maleVariant": "もう黙ってろ",
              "stringId": "1869150967479951360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コーポの奴らはじわじわとこの世界を蝕んでんだ。そんなの、指をくわえて見過ごせるかよ",
              "maleVariant": "",
              "stringId": "1869151271365664768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なに、また打倒アラサカ計画でも披露してくれるの？",
              "maleVariant": "なんだ、また打倒アラサカ計画でも披露するのか？",
              "stringId": "1869152073719242752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "消えて",
              "maleVariant": "失せろ",
              "stringId": "1869156824758378496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "言いたいことがあるなら、はっきり言って",
              "maleVariant": "言いたいことがあるなら、はっきり言え",
              "stringId": "1869157901419438080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヘボテロリストがよく言えたもんね。そのアラサカに負けて、どこぞの頭に追いやられたのは誰だっけ？ ここのことよ",
              "maleVariant": "ヘボテロリストがよく言えたもんだな。そのアラサカに負けて、どこぞの頭に追いやられたのは誰だっけ？ ここのことだ",
              "stringId": "1869161927666065408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はん！ こっちは連中に戦争を仕掛けてやったんだ。お前はどうだ？ ただの車泥棒が大口叩くな",
              "maleVariant": "",
              "stringId": "1869163013034500096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "街のために命を懸けて戦う意味は、ノーマッドにはわかんねえだろうな",
              "maleVariant": "",
              "stringId": "1869164514243010560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカに殺されたのはあんたも同じでしょ。ま、あたしは魂を抜かれて板切れにされたりはしてないけど",
              "maleVariant": "アラサカに殺されたのはお前も同じだろ。ま、俺は魂を抜かれて板切れにされたりはしてないがな",
              "stringId": "1869175938839240704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それでも、チップに閉じ込められた、古臭いロッカーボーイの亡霊よりは見込みがある",
              "maleVariant": "",
              "stringId": "1869184056545533952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、そうかい。生体チップをくすねて金持ちになるっていう、こないだの“見込み”は結局どうなった？",
              "maleVariant": "",
              "stringId": "1869185640599937024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前は企業の歯車、ただの部品だった",
              "maleVariant": "",
              "stringId": "1869193201000669184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前はバッドランズでは仲間に守られ、支えられて生きていた",
              "maleVariant": "",
              "stringId": "1869193369527803904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前はヘイウッドで生まれ育ち、ようやく家から数ブロック先まで足を踏み出した",
              "maleVariant": "",
              "stringId": "1869193548154822656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカに見限られてすべてを失った結果、お前がなれたのは三流の傭兵だ",
              "maleVariant": "",
              "stringId": "1869195665724694528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だがナイトシティじゃ何者でもねえ。せいぜいスカベンジャーを襲う程度の下っ端傭兵だ",
              "maleVariant": "",
              "stringId": "1869197027464208384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はした金を追い回す三流の傭兵に落ち着いただけだがな",
              "maleVariant": "",
              "stringId": "1869233101515284480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "視覚と触覚の間にラグがあるのもイラつくな。お前がピザを食べた時、口のなかを火傷としたと俺が気づくのはずっと後だ",
              "maleVariant": "",
              "stringId": "1869269295204687872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あとは煙草か。5分と置かずに新しいのを吸ってたからな。ニコチンが欲しくて仕方ない",
              "maleVariant": "",
              "stringId": "1869270972641075200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いろいろ慣れてはきてるが、快適には程遠い",
              "maleVariant": "",
              "stringId": "1869274334275067904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "常に誰か敵がいないとダメなのね",
              "maleVariant": "常に誰か敵がいないとダメなんだな",
              "stringId": "1911515621115846656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしに言わせれば、あんたは常に宿敵がいないとダメなのよ",
              "maleVariant": "俺に言わせれば、お前は常に宿敵がいないとダメなんだよ",
              "stringId": "1911517157774614528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それもなるべく強大な相手がいい。決して倒せない何かに戦いを挑めば、自殺行為も伝説にできる",
              "maleVariant": "",
              "stringId": "1911517756016582656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん、面白れえじゃねえか",
              "maleVariant": "",
              "stringId": "1911518220393144320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その宿敵は、あたしなんじゃない？ あんたの運命を握り、あんたの計画をぶち壊せる存在なんだから",
              "maleVariant": "その宿敵は、俺なんじゃないか？ お前の運命を握り、お前の計画をぶち壊せる存在なんだからな",
              "stringId": "1911518741426364416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうせまた、アラサカをぶっ潰すって話でしょ",
              "maleVariant": "どうせまた、アラサカをぶっ潰すって話だろ",
              "stringId": "1927170022239318020"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今はそんな話をする気分じゃない",
              "maleVariant": "",
              "stringId": "1927170936564035584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いったい何の話よ？",
              "maleVariant": "いったい何の話だ？",
              "stringId": "1927171184027971584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、どこ行くんだ！",
              "maleVariant": "",
              "stringId": "1927251424083390464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたがあたしになるその瞬間を、あたしは認識できるの？",
              "maleVariant": "お前が俺になるその瞬間を、俺は認識できるのか？",
              "stringId": "1927455427866079232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしのなかは快適？",
              "maleVariant": "俺のなかは快適か？",
              "stringId": "1927469805336211456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "死ぬのが怖いの？",
              "maleVariant": "死ぬのが怖いのか？",
              "stringId": "1927544599826083840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカのこと？ それとも環境破壊ドキュメンタリーでも見てるの？",
              "maleVariant": "アラサカのことか？ それとも環境破壊ドキュメンタリーでも見てんのかよ",
              "stringId": "1948704099670712320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アラサカに立ち向かった気でいるのかしれねえが、お前なんか奴らに踏み潰された、ただの虫けらじゃねえか",
              "maleVariant": "",
              "stringId": "1948787588902445056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう放っといてくれない？",
              "maleVariant": "もう放っといてくれないか？",
              "stringId": "1949254606365679616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは自分でもどうしたいのかわかってないのよ。だから倒すべき相手をでっち上げてる… それがアラサカ",
              "maleVariant": "お前は自分でもどうしたいのかわかってないんだ。だから倒すべき相手をでっち上げてる… それがアラサカ",
              "stringId": "1949313415759028224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはアラサカを倒したい、あたしは生き延びたい",
              "maleVariant": "お前はアラサカを倒したい、俺は生き延びたい",
              "stringId": "1949325025726717952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうでもいい。あんたがなんでそんなにしつこいのかは知りたいけど",
              "maleVariant": "どうでもいい。お前がなんでそんなにしつこいのかは知りたいがな",
              "stringId": "1949327220287234048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたはアラサカを倒したい。徹底的に。かたやあたしは生き延びたい。どちらも明確な目標があるわけね",
              "maleVariant": "お前はアラサカを倒したい。徹底的に。かたや俺は生き延びたい。どちらも明確な目標があるわけか",
              "stringId": "1949342221567811584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "昔はシルヴァーハンドが世界の中心だったのに、今や誰も気にかけてくれない",
              "maleVariant": "",
              "stringId": "1949345169140113408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふん、面白れえじゃねえか",
              "maleVariant": "",
              "stringId": "1949355607252819968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "生き返ったはいいけど、あんたは何をすればいいのか途方に暮れている",
              "maleVariant": "生き返ったはいいが、お前は何をすればいいのか途方に暮れている",
              "stringId": "1949355925063622656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メガビルディングの一室にいる、フィーダーに繋がれた哀れな連中と、何ひとつ違わない",
              "maleVariant": "",
              "stringId": "1949357846004523008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった。俺がアラサカを倒したい理由を説明してやる。だが一度しか言わない。いいか？",
              "maleVariant": "",
              "stringId": "1949359773354319872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞きたい",
              "maleVariant": "",
              "stringId": "1949362426603921412"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[会話を終える] いや、興味ない",
              "maleVariant": "",
              "stringId": "1949362584645296128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうぞ",
              "maleVariant": "",
              "stringId": "1949362736596541440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、別に興味ない",
              "maleVariant": "",
              "stringId": "1949362810718281728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業は農民から水を奪い… しまいには土地まで奪っちまう",
              "maleVariant": "",
              "stringId": "1949368890747805696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "余計なお世話なのよ！",
              "maleVariant": "余計なお世話なんだよ！",
              "stringId": "1949373863011311616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "話は終わり、檻に戻って",
              "maleVariant": "話は終わりだ、檻に戻れ",
              "stringId": "1949374227831873536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなこと頼んでないし、必要ない。いい？",
              "maleVariant": "そんなこと頼んでないし、必要ない。いいな？",
              "stringId": "1949375411061809152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう消えて… ウンザリよ…",
              "maleVariant": "もう消えろ… ウンザリだ…",
              "stringId": "1949375817506643968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう行かないと。じゃあね",
              "maleVariant": "もう行かないと。じゃあな",
              "stringId": "1949378856783208448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "しかもその二つは同じ直線上にあるってわけだ。お前が〈神輿〉で命をつないだ後、俺はそいつを破壊する",
              "maleVariant": "",
              "stringId": "1949382442829242368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "結局その話に戻るのね。〈神輿〉を見つけて、システムを破壊する",
              "maleVariant": "結局その話に戻るんだな。〈神輿〉を見つけて、システムを破壊する",
              "stringId": "1949383517930020864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティは、破れた夢、打ち砕かれた希望、そして巻き上げられたカネを燃料に動く歪なマシンだ",
              "maleVariant": "",
              "stringId": "1949396936045387776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業は俺たちからすべてを奪い去りやがる… 遂には魂までもな！",
              "maleVariant": "",
              "stringId": "1953187046008315904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのとおりね",
              "maleVariant": "そのとおりだな",
              "stringId": "1953696401145225220"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おおげさよ",
              "maleVariant": "おおげさだ",
              "stringId": "1953696524961079296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その通りね。反論の余地はない",
              "maleVariant": "その通りだ。反論の余地はない",
              "stringId": "1953696599787462656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おおげさに言いすぎよ",
              "maleVariant": "おおげさに言いすぎだろ",
              "stringId": "1953696690837413888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V… 俺はな、資本主義が気に入らないとか、古き良きアメリカを取り戻そうなんつって戦争を仕掛けたんじゃない",
              "maleVariant": "",
              "stringId": "1953702887300816896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これは手に負えないとこまで暴走したシステムに抗う民衆の戦い、増大するエントロピーに抗う戦いなんだよ。わかるか！？",
              "maleVariant": "",
              "stringId": "1953708288104751104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー、ちょっと落ち着きなよ。今は――",
              "maleVariant": "ジョニー、ちょっと落ち着けって。今は――",
              "stringId": "1953714988002074624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今は理解できなくても、いつかわかるさ",
              "maleVariant": "",
              "stringId": "1953715769954557952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "奴らの暴走を止め、叩きのめすためなら何だってする。殺す必要がある奴がいれば殺し、お前の体が必要なら遠慮なく奪ってやる！",
              "maleVariant": "",
              "stringId": "1953718580960321536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー…",
              "maleVariant": "",
              "stringId": "1953720328240259072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たち、助け合えるんじゃないか？",
              "maleVariant": "",
              "stringId": "1954646283813150720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でもあんたの計画は滅茶苦茶よ。アラサカを倒す？ そんなことをして何になるの！",
              "maleVariant": "でもお前の計画は滅茶苦茶だ。アラサカを倒す？ そんなことをして何になる！",
              "stringId": "1954646420430020608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのうちわかるさ",
              "maleVariant": "",
              "stringId": "1954648710989766656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゴミどもが…",
              "maleVariant": "",
              "stringId": "1957568895006564352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、これを見ろよ。クソが",
              "maleVariant": "",
              "stringId": "1957604208747765760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "見ろよ！ これでもまだわかんねえか？",
              "maleVariant": "",
              "stringId": "1957605376441999360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "また問題から目をそらす気か？",
              "maleVariant": "",
              "stringId": "1957627679200534528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうやって俺のことも、ジャッキーみたいに見捨てるっていうのかよ！？",
              "maleVariant": "",
              "stringId": "1957628159431565312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "この自分勝手なひとでなしめ！",
              "maleVariant": "",
              "stringId": "1957630700760358912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もう行きましょ",
              "maleVariant": "[立ち上がる] もう行こう",
              "stringId": "1965101554514067456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー…",
              "maleVariant": "",
              "stringId": "1965113673804480516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またアラサカの話？",
              "maleVariant": "またアラサカの話か？",
              "stringId": "1974599145618092032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "1974694226010255364"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[会話を終える] あんたのくだらない精神分析に興味はない",
              "maleVariant": "[会話を終える] お前のくだらない精神分析に興味はない",
              "stringId": "2038691136727101456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "嫌な奴ね",
              "maleVariant": "嫌な奴だな",
              "stringId": "2038691136743878656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたと違って、あたしはまだ生きてる",
              "maleVariant": "お前と違って、俺はまだ生きてる",
              "stringId": "2039126618090975232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "今のあんたは何者でもない。それが気に入らないんでしょ",
              "maleVariant": "今のお前は何者でもない。それが気に入らないんだろ",
              "stringId": "2049188448075198468"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[会話を終える] 話は終わりよ",
              "maleVariant": "[会話を終える] 話は終わりだ",
              "stringId": "2049197202711719936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "2134901960272982016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "2134921321146691584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる]",
              "maleVariant": "",
              "stringId": "2134923345049366528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "残念ながら、その宿敵はあたしのようね",
              "maleVariant": "残念ながら、その宿敵は俺のようだな",
              "stringId": "2139096048031444992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[会話を終える] よし、もうこの話は終わり",
              "maleVariant": "[会話を終える] よし、もうこの話は終わりだ",
              "stringId": "2139096048048222208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたは自分が何者なのか、何をすればいいのかわかってない",
              "maleVariant": "お前は自分が何者なのか、何をすればいいのかわかってない",
              "stringId": "2139121533327192064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[会話を終える] もうこの話は終わりね",
              "maleVariant": "[会話を終える] もうこの話は終わりだ",
              "stringId": "2139121533327192080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "またその話？",
              "maleVariant": "またその話かよ？",
              "stringId": "2139121743797366784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[会話を終える] もう終わりにしましょ",
              "maleVariant": "[会話を終える] もう終わりにしよう",
              "stringId": "2139121743797366800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "被害妄想もほどほどにして",
              "maleVariant": "被害妄想もほどほどにしろ",
              "stringId": "2169476719585210368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ただのTV番組でしょ",
              "maleVariant": "ただのTV番組だろ",
              "stringId": "2169476977081921536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[会話を終える] 相手をしてられない",
              "maleVariant": "[会話を終える] 相手をしてられるか",
              "stringId": "2180810862947639300"
            }
          ]
        }
      }
    },
