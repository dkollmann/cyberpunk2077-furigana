  },
  "Data": {
    "Version": 195,
    "BuildVersion": 0,
    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっとごめんよ… ブラッド！ おい、ブラッド！",
              "maleVariant": "",
              "stringId": 1792286477788389376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大将、助けてくれないか？ 近所のチンピラどもに困ってる。あんたは随分強そうだ",
              "maleVariant": "",
              "stringId": 1792287336278532096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] わかった",
              "maleVariant": "",
              "stringId": 1792288099692191744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これも何かの縁ね。行きましょ",
              "maleVariant": "これも何かの縁だ。行こう",
              "stringId": 1792288185104998400
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしのバイクじゃないし、助ける義理もない",
              "maleVariant": "俺のバイクじゃないし、助ける義理もない",
              "stringId": 1792288192822517760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[立ち上がる] 自分でどうにかして",
              "maleVariant": "[立ち上がる] 自分でどうにかしろ",
              "stringId": 1792288341468651520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "助かるよ、大将。こっちだ",
              "maleVariant": "",
              "stringId": 1792294771504455680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうか、仕方ない。自分の身くらい自分で守ってやる",
              "maleVariant": "",
              "stringId": 1792295223918862336
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、何してる？ 汚い手で俺のバイクに触るな！",
              "maleVariant": "",
              "stringId": 1792295436049981440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うるせえ！ こうなって当然だ！ 裏切りの代償には安いもんだろ！？",
              "maleVariant": "",
              "stringId": 1792296376211611648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何だてめえ？ 首を突っ込むんじゃねえ！",
              "maleVariant": "",
              "stringId": 1792297738957758464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "その人に手を出さないで",
              "maleVariant": "その人に手を出すな",
              "stringId": 1792298430313275396
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "聞こえねえのか？",
              "maleVariant": "",
              "stringId": 1792298641790083072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気にしないで。もう行くから",
              "maleVariant": "気にするな。もう行くから",
              "stringId": 1792299007113961472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "喋り方を忘れたか？",
              "maleVariant": "",
              "stringId": 1792300793518366720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうした？ 黙ってんなら消えな",
              "maleVariant": "",
              "stringId": 1792300802729058304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "通りの店主を痛めつけたくらいで、企業に歯向かってるつもり？ ガキの考えそうなことね。ほら、離れて",
              "maleVariant": "通りの店主を痛めつけたくらいで、企業に歯向かってるつもりか？ ガキの考えそうなことだ。ほら、離れろ",
              "stringId": 1792302949172518912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "確かにあたしには関係ない。どうぞ、好き勝手にやって",
              "maleVariant": "確かに俺には関係ない。ほら、好き勝手やってくれ",
              "stringId": 1792302957678567424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "嫌だと言ったら？",
              "maleVariant": "",
              "stringId": 1792353471359361024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誰がてめえに許可してくれって言った！？",
              "maleVariant": "",
              "stringId": 1792353855255617536
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頼むよ、大将！ 見捨てないでくれ！",
              "maleVariant": "",
              "stringId": 1792353964777283584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだ言ってんのか！ シェンナは自分で決めたんだよ。誇りに思ってやったらどうだ！",
              "maleVariant": "",
              "stringId": 1792357434003562496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭を撃ち抜く",
              "maleVariant": "",
              "stringId": 1792401080300085252
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "頭をかち割ってもいいし、もちろん撃ち殺してもいい… 選択肢はいくらでもある",
              "maleVariant": "",
              "stringId": 1792401223963385856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "気が変わった。好きにすればいい",
              "maleVariant": "",
              "stringId": 1792401305920086016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やっぱ、あたしには関係ない。好きにして",
              "maleVariant": "やっぱ、俺には関係ない。好きにしろ",
              "stringId": 1792401592290385920
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなにこの裏切り者が気に入ったなら、一緒に死にやがれ！",
              "maleVariant": "",
              "stringId": 1792401920117186560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それが身のためだ。流れ弾を食らう前にとっとと失せな",
              "maleVariant": "",
              "stringId": 1792402224724320256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "平気さ。このくらいどうってことない。ったく、怠け者がえらそうに！",
              "maleVariant": "",
              "stringId": 1792403399666311168
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく… いつでもまた寄ってくれ。うんと安くしてやるからさ",
              "maleVariant": "",
              "stringId": 1792404035371802624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "助かるよ。ありがとう",
              "maleVariant": "",
              "stringId": 1792404350414364672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいか、ダレル。お前がどうなろうと誰も気に留めねえ。これでよくわかったろ",
              "maleVariant": "",
              "stringId": 1792404632238039040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シックス・ストリートが来る前にズラかるぞ",
              "maleVariant": "",
              "stringId": 1792425399059226624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[殴る]",
              "maleVariant": "",
              "stringId": 1793728856487632896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫？",
              "maleVariant": "大丈夫か？",
              "stringId": 1795368961405100036
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "危なかったね。大丈夫？",
              "maleVariant": "危なかったな。大丈夫か？",
              "stringId": 1795370919725637632
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "何があったの？",
              "maleVariant": "何があったんだ？",
              "stringId": 1796398932378525696
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "落ち着いて。“まだ”あんたとやりあう気はないよ。何があったの？",
              "maleVariant": "落ち着け。“まだ”お前とやりあう気はない。何があったんだ？",
              "stringId": 1796399427172179968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そいつは妹を丸め込んでゼータテクの仕事に就かせたんだ。あの女、挨拶もなしに出ていきやがった！",
              "maleVariant": "",
              "stringId": 1796399749076623360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ブラッド、あいつは… 噂になってる奴じゃねえか。あの、一度死んで生き返ったって！",
              "maleVariant": "",
              "stringId": 1796403835167580160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "危なかったね。大丈夫？",
              "maleVariant": "危なかったな。大丈夫か？",
              "stringId": 1796418047751233540
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺か？ ピンピンしてるよ！ 手は出されてないからな。あいつら、まともな仕事でも見つけろってんだ！ 寄生虫め…",
              "maleVariant": "",
              "stringId": 1796418047751233548
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とにかく… いつでもまたこの店に寄ってくれ。うんと安くしてやるからさ",
              "maleVariant": "",
              "stringId": 1796418047751233556
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうするよ。ありがとう",
              "maleVariant": "",
              "stringId": 1796418047751233564
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大丈夫？",
              "maleVariant": "大丈夫か？",
              "stringId": 1796418047952560128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "助かったよ、大将。これで連中も懲りたはずだ",
              "maleVariant": "",
              "stringId": 1796421886898348032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ！ やりやがったな！",
              "maleVariant": "",
              "stringId": 1796426860470476800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあまたな！ 妹さんにもよろしく伝えてくれ！",
              "maleVariant": "",
              "stringId": 1796437432029159424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "誇りだあ！？ 企業に寝返ったんだぞ！ こいつはその報いだ！",
              "maleVariant": "",
              "stringId": 1821108533206958080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "企業のネズミか？",
              "maleVariant": "",
              "stringId": 1821110340851318784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "妹の新しい仕事って？",
              "maleVariant": "",
              "stringId": 1821141893191024640
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "俺たちはガキの頃からここらのストリートを仕切ってきた！ 自販機をハックし、コーポを襲い… そうやって生きてきたんだ！",
              "maleVariant": "",
              "stringId": 1821175551859867648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それがどうだ？ もう住む世界が違うってか？ あの裏切り者！",
              "maleVariant": "",
              "stringId": 1821175728624615424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前はそれでいいのか？ 俺のような貧乏人からモノを盗んで、それで満足か？",
              "maleVariant": "",
              "stringId": 1821176177180262400
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつが今まで何人殺したか知ってるか？ しかも、タイマンでじゃねえぞ…",
              "maleVariant": "",
              "stringId": 1821221780136378368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ… 追って来ねえよな？",
              "maleVariant": "",
              "stringId": 1821230634999988224
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シェンナがお前らと違って、企業でまともな仕事を手に入れたのがそんなに気に食わないか",
              "maleVariant": "",
              "stringId": 1821396608927645696
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "上等だ、気が変わったよ。この企業の犬をぶちのめしてやれ！",
              "maleVariant": "",
              "stringId": 1821397052886335488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "お前の妹には、二度とここをうろつくなって言っとけ。でないと兄貴が痛い目見るぞってな！",
              "maleVariant": "",
              "stringId": 1824394338327314432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "少なくともあいつは前を見て生きてる！ もう放っといてくれ！",
              "maleVariant": "",
              "stringId": 1824394741114716160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シェンナはな、もう大人なんだよ。お前らも見習ったらどうだ！",
              "maleVariant": "",
              "stringId": 1824431494391382016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ま、これでシェンナも仕事に集中できる。あいつは家族で一番の出世頭なんだ",
              "maleVariant": "",
              "stringId": 1824450156980584448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ま、これでシェンナも仕事に集中できる。あいつは家族で一番の出世頭なんだ",
              "maleVariant": "",
              "stringId": 1824471451310997504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヴァンス！ おい、一体何しようってんだ！？",
              "maleVariant": "",
              "stringId": 1834160374299738112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "会えたらな！ 仕事が忙しいみたいで、顔を出してくれないんだよ",
              "maleVariant": "",
              "stringId": 1834169533552377856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちなみになんだけど… ゼータテクに入った妹のこと、本気で誇りに思ってるの？",
              "maleVariant": "ちなみになんだが… ゼータテクに入った妹のこと、本気で誇りに思ってるのか？",
              "stringId": 1834184192040878080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "妹の新しい仕事って？",
              "maleVariant": "",
              "stringId": 1834184465979260928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたの言いたいことはわかる。「企業は民衆から搾取してる」とか、「企業の奴隷に未来はない」ってんだろ？",
              "maleVariant": "",
              "stringId": 1834210439391330304
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だが俺たちにとって肝心なのは、毎日三度の飯にありつくことだ。あいつはその心配がなくなった",
              "maleVariant": "",
              "stringId": 1834210597751472128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも、働き過ぎで心が病んじゃったらどうするの？",
              "maleVariant": "だが、働き過ぎで心が病んじまったらどうするんだ？",
              "stringId": 1834210720057376768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そりゃ全て思い通りとはいかないさ。もしそうなっちまったら、人生退屈で仕方ないだろ！",
              "maleVariant": "",
              "stringId": 1834210870918103040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まぁ、そりゃそうだけど…",
              "maleVariant": "まぁ、そりゃそうだが…",
              "stringId": 1834211028741373952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あいつらには慣れたもんだと思ってたが… まさかナイフを向けてくるとはな",
              "maleVariant": "",
              "stringId": 1834214721943486464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] クロマンティコアをくれる？",
              "maleVariant": "[座る] クロマンティコアをもらえるか？",
              "stringId": 1864744166132555780
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっと元気が出ないの。クロマンティコアはある？",
              "maleVariant": "ちょっと元気が出なくてな。クロマンティコアはあるか？",
              "stringId": 1864949453790363648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "水が欲しい。サン・ルイソーはある？",
              "maleVariant": "水が欲しい。サン・ルイソーはあるか？",
              "stringId": 1864949521117331456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] サン・ルイソーをお願い",
              "maleVariant": "[座る] サン・ルイソーを頼む",
              "stringId": 1864949721672171520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "[座る] ロズブロークをちょうだい",
              "maleVariant": "[座る] ロズブロークをくれ",
              "stringId": 1864984420041867264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ビールはある？ ロズブロークとか",
              "maleVariant": "ビールはあるか？ ロズブロークとか",
              "stringId": 1864984476329426944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "大将、変わった趣味してんねぇ。 ありゃ暗闇で不気味に光るらしいよ？",
              "maleVariant": "",
              "stringId": 1864989304728305664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんなのはやめといて、代わりにスパンキーモンキーを飲むといい。ずっと――",
              "maleVariant": "",
              "stringId": 1864989430356099072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい、大将。水だって？ 味のあるものが欲しくないのか？",
              "maleVariant": "",
              "stringId": 1864991732022362112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "水なんか動物が飲むもんだ！ 人間が砂糖を発明したのは――",
              "maleVariant": "",
              "stringId": 1864991883268964352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "悪いな。ビールはない",
              "maleVariant": "",
              "stringId": 1864993113659654144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おすすめは？",
              "maleVariant": "",
              "stringId": 1864993221688147968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだな… アプリコット味のがよく売れてる。しゃっくりが出るって言う奴もいるが、そいつはウソだ。だって――",
              "maleVariant": "",
              "stringId": 1864993318861783040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ねえ、もしもし？",
              "maleVariant": "おい？ もしもし？",
              "stringId": 1865004367849701376
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}