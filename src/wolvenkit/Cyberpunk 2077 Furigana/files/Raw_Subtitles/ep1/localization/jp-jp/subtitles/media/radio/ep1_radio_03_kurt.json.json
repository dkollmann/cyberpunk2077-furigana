            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "70571Z^所{しょ}属{ぞく}不{ふ}明{めい}の航{こう}空{くう}機{き}がこの地{ち}区{く}に墜{つい}落{らく}した",
              "maleVariant": "",
              "stringId": "3065195960630448128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A4571Z^墜{つい}落{らく}現{げん}場{ば}は当{とう}面{めん}立入禁止{たちいりきんし}とさせてもらう",
              "maleVariant": "",
              "stringId": "3065202296579624960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "65571Z^安{あん}全{ぜん}確{かく}認{にん}が済{す}むまで、住{じゅう}民{みん}たちは自{じ}宅{たく}で待{たい}機{き}するように",
              "maleVariant": "",
              "stringId": "3065203029962067968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "90571Z^追{お}って通{つう}告{こく}があるまで、ドッグタウンの非{ひ}常{じょう}事{じ}態{たい}を宣{せん}言{げん}する",
              "maleVariant": "",
              "stringId": "3065215056591917056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4A45AZ^状{じょう}況{きょう}に進{しん}展{てん}があり次{し}第{だい}、続{ぞく}報{ほう}を伝{つた}える",
              "maleVariant": "",
              "stringId": "3336323385528786944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7D45AZ^流{なが}れ弾{だん}を食{く}らいたくなければ従{したが}うことだ",
              "maleVariant": "",
              "stringId": "3336327947639693312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "368AEZ^ドッグタウンに“不{ふ}法{ほう}入{にゅう}国{こく}”したサイバーサイコの件{けん}について続{ぞく}報{ほう}だ。懸{けん}賞{しょう}金{きん}を2倍{ばい}に引{ひ}き上{あ}げることにした",
              "maleVariant": "",
              "stringId": "3365151643775197184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "688AE004^皆{みな}の者{もの}に朗{ろう}報{ほう}だ。先{せん}日{じつ}、病{びょう}院{いん}の改{かい}修{しゅう}が完{かん}了{りょう}した",
              "maleVariant": "",
              "stringId": "3365265242204069892"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E78AE008^火{か}曜{よう}日{び}に、スカベンジャーの代{だい}表{ひょう}者{しゃ}らがドッグタウンを訪{ほう}問{もん}する",
              "maleVariant": "",
              "stringId": "3365265768320786440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "728AE008^皆{みな}の者{もの}に朗{ろう}報{ほう}だ。とある大{だい}統{とう}領{りょう}が、無{ぶ}事{じ}ワシントンにいることが判{はん}明{めい}した",
              "maleVariant": "",
              "stringId": "3365266578106671112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "758AE008^もうすぐ、我々{われわれ}がここに新{あら}たな故{こ}郷{きょう}を築{きづ}いてから七{なな}年{ねん}が経{へ}つ",
              "maleVariant": "",
              "stringId": "3365266578157002760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F88AE008^ドッグタウンの住{じゅう}民{みん}たち。こちらはカート・ハンセンだ",
              "maleVariant": "",
              "stringId": "3365301507028869128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "998AEZ^ドッグタウンの全{ぜん}住{じゅう}民{みん}に告{つ}ぐ。こちらはカート・ハンセンだ",
              "maleVariant": "",
              "stringId": "3365301866212286464"
            },
            {
