            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "466F7Z^[ドッグタグを渡{わた}す] 仕{し}事{ごと}はこなした",
              "maleVariant": "",
              "stringId": "3346030007258411008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6E6F7Z^そう。当{とう}然{ぜん}の報{むく}いよ",
              "maleVariant": "",
              "stringId": "3346033242039873536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "26559Z^終{お}わったの？ ラインダーは？ 約{やく}束{そく}は守{まも}ってくれたんでしょうね",
              "maleVariant": "",
              "stringId": "3348880108362764288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7C559Z^ええ。もう奴{やつ}を気{き}にする必{ひつ}要{よう}はない",
              "maleVariant": "7C559Z^ああ。もう奴{やつ}を気{き}にする必{ひつ}要{よう}はない",
              "stringId": "3348884288808783872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D559Z^スタックスのみんなを代{だい}表{ひょう}して… ありがとう",
              "maleVariant": "",
              "stringId": "3348884639133831168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4A559Z^恐{きょう}怖{ふ}に震{ふる}えながら死{し}んだことを祈{いの}るわ",
              "maleVariant": "",
              "stringId": "3349031107131969536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "93559Z^大{たい}したことじゃない",
              "maleVariant": "",
              "stringId": "3349033835660939264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2B559Z^歓{かん}喜{き}に震{ふる}えてなかったことは確{たし}かね…",
