          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2575F004^リードは信{しん}用{よう}できるの？",
              "maleVariant": "2575F004^リードは信{しん}用{よう}できるのか？",
              "stringId": "3388446304856567812"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1B75FZ^ソミとはどうやって出{で}会{あ}ったの？",
              "maleVariant": "1B75FZ^ソミとはどうやって出{で}会{あ}ったんだ？",
              "stringId": "3388449620403548160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C875FZ^アラサカが動{うご}く可{か}能{のう}性{せい}は？",
              "maleVariant": "",
              "stringId": "3388449739270123520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4275FZ^帰{き}還{かん}兵{へい}は社{しゃ}会{かい}に復帰{ふっき}できない人{にん}が多{おお}い",
              "maleVariant": "",
              "stringId": "3388450093504262144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7F75FZ^ねえ… そのリードとかいう男{おとこ}は本{ほん}当{とう}に信{しん}用{よう}できるの？",
              "maleVariant": "7F75FZ^なあ… そのリードとかいう男{おとこ}は本{ほん}当{とう}に信{しん}用{よう}できるのか？",
              "stringId": "3388476457036935168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B275FZ^ソミとはどうやって出{で}会{あ}ったの？",
              "maleVariant": "B275FZ^ソミとはどうやって出{で}会{あ}ったんだ？",
              "stringId": "3388476500842246144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "275FZ^ミリテクは今{いま}、墜{つい}落{らく}事{じ}件{けん}に相{そう}当{とう}気{き}を取{と}られてるはず。つまりアラサカにとっては恰{かっ}好{こう}のチャンスよ",
              "maleVariant": "275FZ^ミリテクは今{いま}、墜{つい}落{らく}事{じ}件{けん}に相{そう}当{とう}気{き}を取{と}られてるはず。つまりアラサカにとっては恰{かっ}好{こう}のチャンスだ",
              "stringId": "3388476510774358016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F75FZ^あんたの戦{せん}争{そう}を経{けい}験{けん}した帰{き}還{かん}兵{へい}たちのことだけど、ノーマッドになる人{にん}が多{おお}いのは知{し}ってる？ そのうちの何{なん}人{にん}かと会{あ}った",
              "maleVariant": "F75FZ^あんたの戦{せん}争{そう}を経{けい}験{けん}した帰{き}還{かん}兵{へい}たちのことだが、ノーマッドになる人{にん}が多{おお}いのは知{し}ってるか？ そのうちの何{なん}人{にん}かと会{あ}った",
              "stringId": "3388476523877363712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AD75FZ^そう思{おも}うか？ まあ、確{たし}かにそうかもしれないな。だが、見{み}方{かた}を変{か}えてみたらどうだ？",
              "maleVariant": "",
              "stringId": "3388495029247275008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BA75FZ^彼{かの}女{じょ}が私{わたし}の家{いえ}に押{お}し入{いっ}ったのさ",
              "maleVariant": "",
              "stringId": "3388497640805494784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6575FZ^二{に}社{しゃ}とも、その両{りょう}方{ほう}の結{けっ}果{か}に備{そな}えてる",
              "maleVariant": "",
              "stringId": "3388498713121255424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DA75FZ^自{じ}由{ゆう}、開{かい}放{ほう}感{かん}――そんなものは所{しょ}詮{せん}まやかしよ。兵{へい}士{し}たちはそれを誰{だれ}よりも知{し}ってる。他{ほか}に選{せん}択{たく}肢{し}がないだけ",
              "maleVariant": "DA75FZ^自{じ}由{ゆう}、開{かい}放{ほう}感{かん}――そんなものは所{しょ}詮{せん}まやかしだ。兵{へい}士{し}たちはそれを誰{だれ}よりも知{し}ってる。他{ほか}に選{せん}択{たく}肢{し}がないだけだ",
              "stringId": "3388503375123705856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9B45AZ^七{なな}年{ねん}も経{へ}ってるんでしょ… 人{にん}を変{か}えるには十{じゅう}分{ぶん}すぎる",
              "maleVariant": "9B45AZ^七{なな}年{ねん}も経{へ}ってるんだろ… 人{にん}を変{か}えるには十{じゅう}分{ぶん}すぎる",
              "stringId": "3397653827657506816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4E45AZ^七{なな}年{ねん}もスリーパーエージェントでい続{つづ}けた男{おとこ}だぞ。他{ほか}の者{もの}ならとっくに去{さ}っているが… リードは違{ちが}う",
              "maleVariant": "",
              "stringId": "3397656450590679040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "え、ウソでしょ",
              "maleVariant": "え、ウソだろ",
              "stringId": "3397661936119554048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D45AZ^ミリテクのデータ要{よう}塞{さい}のことだ。ギリギリでソミの信{しん}号{ごう}を特{とく}定{てい}できた",
              "maleVariant": "",
              "stringId": "3397662471044308992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F245AZ^殺{ころ}すはずだったが… 少{しょう}女{じょ}がおいたをしたくらいで、命{いのち}を奪{うば}うべきではない",
              "maleVariant": "",
              "stringId": "3397664996032094208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8C45AZ^むしろ迎{むか}え入{い}れ、才{さい}能{のう}を活{い}かすチャンスを与{あた}えてやるべきだ",
              "maleVariant": "",
              "stringId": "3397666918466166784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6645AZ^リードはそれを理{り}解{かい}し、ソミは期{き}待{たい}に応{こた}えた",
              "maleVariant": "",
              "stringId": "3397667729577451520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DE453Z^かもな。しかし今{いま}の私{わたし}はシュレディンガーの大{だい}統{とう}領{りょう}だ――生{せい}死{し}は不{ふ}明{めい}",
              "maleVariant": "",
              "stringId": "3398370560039006208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "つまり…？",
              "maleVariant": "",
              "stringId": "3398482542922641408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A1453Z^いつもと変{か}わらないということだ",
              "maleVariant": "",
              "stringId": "3398563622090518528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7C453Z^フム… 言{い}いたいことはわかる",
              "maleVariant": "",
              "stringId": "3399970545381748736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FC453Z^多{おお}くの帰{き}還{かん}兵{へい}たちが、国{くに}から見{み}捨{す}てられたと感{かん}じていることについては知{し}っているよ",
              "maleVariant": "",
              "stringId": "3399973231883792384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "80453Z^我々{われわれ}は様々{さまざま}な形{かたち}で手{て}を差{さ}し伸{の}べている。それを掴{つか}むかどうかは彼{かれ}ら次{し}第{だい}だ",
              "maleVariant": "",
              "stringId": "3400059494221557760"
            }
          ]
        }
