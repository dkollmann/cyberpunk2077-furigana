          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい！ こっちだ！",
              "maleVariant": "",
              "stringId": "2759203503488331776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E36F7008^あんた、ゼータテクに雇{こ}われたんだろ？ 理{り}由{ゆう}はだいたい想{そう}像{ぞう}がつく",
              "maleVariant": "",
              "stringId": "2759209976859684872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D06F7Z^唯{ゆい}一{いつ}の生{せい}存{ぞん}者{しゃ}？ どうも怪{あや}しい…",
              "maleVariant": "",
              "stringId": "2764878371298963456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7D6F7Z^いいから、さっさと試{し}作{さく}品{ひん}の場{ば}所{しょ}を教{おし}えて",
              "maleVariant": "7D6F7Z^いいから、さっさと試{し}作{さく}品{ひん}の場{ば}所{しょ}を教{おし}えろ",
              "stringId": "2764879473713049600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "696F7Z^たかがインプラントに大{おお}げさじゃない？",
              "maleVariant": "696F7Z^たかがインプラントに大{おお}げさじゃないか？",
              "stringId": "2764886723282300928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2B6F7Z^ゴホン… サントドミンゴのクズどもがこいつをアラサカ工{こう}業{ぎょう}団{だん}地{ち}から盗{ぬす}んだって話{はなし}が入{いっ}ってな。我々{われわれ}は調{ちょう}査{さ}に向{む}かい――",
              "maleVariant": "",
              "stringId": "2764887478156357632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "236F7Z^人質{ひとじち}については何{なに}も聞{き}いてない。試{し}作{さく}品{ひん}は持{も}ってるの？",
              "maleVariant": "236F7Z^人質{ひとじち}については何{なに}も聞{き}いてない。試{し}作{さく}品{ひん}は持{も}ってるのか？",
              "stringId": "2764917280800206848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9E6F7Z^しょうがない。連{つ}れ出{だ}してあげる",
              "maleVariant": "9E6F7Z^しょうがない。連{つ}れ出{だ}してやるよ",
              "stringId": "2764925924338003968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1C6F7Z^逃{に}がしてあげる",
              "maleVariant": "1C6F7Z^逃{に}がしてやる",
              "stringId": "2774864085637885952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D76F7Z^わかった。でも妙{みょう}な真似{まね}はしないで",
              "maleVariant": "D76F7Z^わかった。だが妙{みょう}な真似{まね}はするなよ",
              "stringId": "2774865252711362560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "596F7004^さて… ここら辺{へん}か？ 絶{ぜっ}対{たい}にあるはず… どこだ…",
              "maleVariant": "",
              "stringId": "2776523589010026500"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あった！",
              "maleVariant": "",
              "stringId": "2776532047629348864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "446F7Z^この先{さき}に二{ふた}人{り}いる。頼{たの}めるか？",
              "maleVariant": "",
              "stringId": "2806851246087958528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EC6F7Z^捕{ほ}虜{りょ}は取{と}らない、ってか。すごいな…",
              "maleVariant": "",
              "stringId": "2806866981371736064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3C6F7Z^よし、タブレットはここにあるはずだ。その前{まえ}に… 見{み}張{は}りを片{かた}付{づ}けてくれ",
              "maleVariant": "",
              "stringId": "2806881104566382592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8C6F7Z^あ、ああ… 行{い}こう",
              "maleVariant": "",
              "stringId": "2808471682622124032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DE6F7004^どこへ行{い}く気{き}だ！？",
              "maleVariant": "",
              "stringId": "2809685385921130500"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FC6F7Z^た、頼{たの}む！ 置{お}いてかないでくれ！",
              "maleVariant": "",
              "stringId": "2809686129453789184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "406F7008^静{しず}かに、手{て}際{ぎわ}よく。見{み}事{ごと}だな",
              "maleVariant": "",
              "stringId": "2921624445891080200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "566F7Z^それが一{いち}番{ばん}安{あん}全{ぜん}だと思{おも}ってね。未{み}発{はつ}売{ばい}のテックなら検問所{けんもんじょ}のスキャンにも引{ひ}っかからない",
              "maleVariant": "",
              "stringId": "2922833878985961472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FF6F7Z^さて、扉{とびら}を開{ひら}けるには…",
              "maleVariant": "",
              "stringId": "2922853823354466304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FC6F7Z^あんたが来{き}てくれなかったら、僕{ぼく}もここで焼{や}かれてたのか",
              "maleVariant": "",
              "stringId": "2927329209291337728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D46F7Z^そんな！ これは… ハリーか？ 嫌{いや}な奴{やつ}だったが… これは… あんまりだ…",
              "maleVariant": "",
              "stringId": "2947577539879464960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C86F7Z^へえ。ゼータテクはケチらず本{ほん}物{もの}のプロを雇{やと}ったようだな",
              "maleVariant": "",
              "stringId": "3008439305026629632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "486F7Z^ええ。次{つぎ}はあんたの番{ばん}よ。なぜ設計図{せっけいず}の場{ば}所{しょ}を知{し}ってるの？",
              "maleVariant": "486F7Z^ああ。次{つぎ}はあんたの番{ばん}だぞ。なぜ設計図{せっけいず}の場{ば}所{しょ}を知{し}ってるんだ？",
              "stringId": "3008570514130038784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BE6F7Z^まずい、見{み}つかったぞ！ 早{はや}く撃{う}ってくれ！",
              "maleVariant": "",
              "stringId": "3011374975135215616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3D6F7Z^片{かた}付{づ}いたね。大{だい}丈{じょう}夫{ぶ}？",
