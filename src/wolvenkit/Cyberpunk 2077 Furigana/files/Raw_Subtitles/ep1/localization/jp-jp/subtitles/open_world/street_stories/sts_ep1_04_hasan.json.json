            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "806F7004^片{かた}付{づ}いた",
              "maleVariant": "",
              "stringId": "3288190851887362052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EB6F7Z^今{いま}のあんたが？ ありがとう… でも…",
