          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7955AZ^ハンセンは数{すう}百{ひゃく}人{にん}くらい殺{ころ}してる殺{さつ}人{じん}鬼{き}でしょ。哀{あわ}れむ気{き}にはなれない",
              "maleVariant": "",
              "stringId": "3578031531418296320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AF55AZ^前{まえ}にここらへんで働{はたら}いてたんだけど… ある日{にち}、バーゲストの男{おとこ}が来{き}たと思{おも}ったら、商{しょう}品{ひん}が高{たか}すぎるとか、色々{いろいろ}言{い}ってきてさ",
              "maleVariant": "",
              "stringId": "3579484292127236096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "15559Z^どれが好{この}み？",
              "maleVariant": "",
              "stringId": "3588041994700951552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "97559Z^聞{き}いたところで運{うん}転{てん}できるわけじゃないでしょ。まさかハンセンの部{ぶ}下{か}たちが私{わたし}たちにくれるとでも思{おも}ってるわけ？",
              "maleVariant": "",
              "stringId": "3588052128709840896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3D559Z^思{おも}ってないけど… 別{べつ}に夢{ゆめ}を見{み}てもいいじゃない",
              "maleVariant": "",
              "stringId": "3588052260343877632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A8559Z^私{わたし}だってドッグタウンに来{く}る前{まえ}は、こういうのを買{か}えたのよ",
              "maleVariant": "",
              "stringId": "3588053061002956800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E1559Z^そしたら突{とつ}然{ぜん}ハンセン大{たい}佐{さ}が現{あらわ}れて、「何{なに}事{ごと}だ」って聞{き}いてきたの。でもバーゲストの奴{やつ}は彼{かれ}に気{き}づかず、ずっと私{わたし}に怒{ど}鳴{な}り続{つづ}けてて… で、次{つぎ}の瞬{しゅん}間{かん}… ハンセンが銃{じゅう}を取{と}り出{だ}して、バン！ その男{おとこ}を撃{う}って「ご迷{めい}惑{わく}をおかけして申{もう}し訳{わけ}ない」って言{い}って立{た}ち去{さ}ったのよ",
              "maleVariant": "",
              "stringId": "3588075881120501760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B7559Z^やっぱり殺{さつ}人{じん}鬼{き}じゃない",
              "maleVariant": "",
              "stringId": "3588076670689841152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "96559Z^人{ひと}殺{ごろ}しなんて別{べつ}に珍{めずら}しくもないでしょ。少{すく}なくともハンセンは秩{ちつ}序{じょ}を保{たも}つためにやってた。彼{かれ}がいた方{ほう}が安{あん}全{ぜん}だったのよ",
              "maleVariant": "",
              "stringId": "3588473576653950976"
            }
          ]
        }
      }
