          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B64EAZ^私{わたし}のエース… よく来{き}たな",
              "maleVariant": "",
              "stringId": "2928832545452630016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おや、Vじゃないか…",
              "maleVariant": "",
              "stringId": "3021453322456309760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C66EAZ^どうしたんだい。パパはお仕{し}事{ごと}中{ちゅう}だよ…",
              "maleVariant": "",
              "stringId": "3021532826327490560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うん？ なるほど。ママじゃダメってどんなことかな？",
              "maleVariant": "",
              "stringId": "3021539073357422592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7A521Z^大{だい}丈{じょう}夫{ぶ}。家{いえ}に帰{かえ}ったらパパがすぐ見{み}つけてあげよう。それでいいかい？",
              "maleVariant": "",
              "stringId": "3080970752905842688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "51521Z^もちろんだよ、子{こ}グマちゃん。ああ、愛{いと}してるよ。バイバイ",
              "maleVariant": "",
              "stringId": "3080972255456530432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "876FCZ^政{せい}治{じ}の勉{べん}強{きょう}は今{こん}度{ど}にして。本{ほん}題{だい}に入{い}りたい",
              "maleVariant": "876FCZ^政{せい}治{じ}の勉{べん}強{きょう}は今{こん}度{ど}にしろ。本{ほん}題{だい}に入{い}りたい",
              "stringId": "3205570052190355456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "フム… まあいいだろう…",
              "maleVariant": "",
              "stringId": "3205571854516338688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "316FCZ^チップにはブラック・サファイアの情{じょう}報{ほう}と詳{しょう}細{さい}な図{ず}面{めん}が入{いっ}っている",
              "maleVariant": "",
              "stringId": "3205613812169293824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1A521004^[チップを受{う}け取{と}る] ありがとう",
              "maleVariant": "",
              "stringId": "3210514747423330308"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう、ハンズ",
              "maleVariant": "",
              "stringId": "3210516364042309632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2521Z^今{こん}後{ご}の働{はたら}きにも大{おお}いに期{き}待{たい}しているぞ。実{み}のところ、お前{まえ}の力{ちから}を借{か}りたい案{あん}件{けん}がいくつか差{さ}し迫{せま}っている…",
              "maleVariant": "",
              "stringId": "3210521391335084032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D3521Z^へえ… また地{ち}域{いき}の勢{せい}力{りょく}争{あらそ}い？",
              "maleVariant": "D3521Z^へえ… また地{ち}域{いき}の勢{せい}力{りょく}争{あらそ}いか？",
              "stringId": "3210522344029294592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "58521Z^V… ブラック・サファイアで何{なに}があろうと、必{かなら}ず生{い}きて戻{もど}るんだぞ。いいな？",
              "maleVariant": "",
              "stringId": "3210525305493131264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BC521004^政{せい}治{じ}には興{きょう}味{み}ない",
              "maleVariant": "",
              "stringId": "3298322135282487300"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "53521Z^それじゃあパパはお仕{し}事{ごと}に戻{もど}るからね… ああ",
              "maleVariant": "",
              "stringId": "3300311214480035840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9A521Z^私{わたし}にもお前{まえ}ほどの才{さい}能{のう}があれば、今{いま}頃{ごろ}事{じ}業{ぎょう}を拡{かく}大{だい}していたところだろうな…",
              "maleVariant": "",
              "stringId": "3300412645618880512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A2521Z^あんたの依{い}頼{らい}とハンセンに何{なん}の関{かん}係{けい}があるの？",
              "maleVariant": "A2521Z^あんたの依{い}頼{らい}とハンセンに何{なん}の関{かん}係{けい}があるんだ？",
              "stringId": "3300433914431148032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9A521Z^あたしのやった仕{し}事{ごと}がハンセンにどう関{かん}係{けい}してるの？",
              "maleVariant": "9A521Z^俺{おれ}のやった仕{し}事{ごと}がハンセンにどう関{かん}係{けい}してるんだ？",
              "stringId": "3300435774017769472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A521Z^たとえばボクサーが相{あい}手{て}をノックアウトできないときは、どうする？ まずボディーを攻{せ}めるだろう",
              "maleVariant": "",
              "stringId": "3300437038617202688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C3521Z^そして相{あい}手{て}のスタミナが落{お}ちたとき、あごに渾{こん}身{しん}の一{いち}撃{げき}を叩{たた}き込{こ}む",
              "maleVariant": "",
              "stringId": "3300437823405035520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "10521Z^苦{く}労{ろう}が無{む}駄{だ}にならないといいけど",
              "maleVariant": "10521Z^苦{く}労{ろう}が無{む}駄{だ}にならないといいがな",
              "stringId": "3300439740252295168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "22521Z^無{む}駄{だ}？ 私{わたし}の利{り}益{えき}は、お前{まえ}の利{り}益{えき}にもなる",
              "maleVariant": "",
              "stringId": "3300441187958263808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "12521Z^安{あん}心{しん}したまえ。私{わたし}との仕{し}事{ごと}を後{こう}悔{かい}する者{もの}は存{そん}在{ざい}しない。一{ひと}人{り}もだ",
              "maleVariant": "",
              "stringId": "3300441909244334080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "98521Z^仕{し}事{ごと}をこなした甲斐{かい}があるといいんだけど",
              "maleVariant": "98521Z^仕{し}事{ごと}をこなした甲斐{かい}があるといいんだがな",
              "stringId": "3300444166350311424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AD521Z^ブラック・サファイアだが… ハンセンの本{ほん}拠{きょ}地{ち}であり、精{せい}鋭{えい}部{ぶ}隊{たい}が警{けい}備{び}を担{たん}当{とう}。建物{たてもの}の下{か}層{そう}は兵{へい}隊{たい}の居{きょ}住{じゅう}区{く}域{いき}、上{じょう}層{そう}は富{ふ}裕{ゆう}層{そう}向{む}けの社{しゃ}交{こう}場{ば}となっている",
              "maleVariant": "",
              "stringId": "3300449037195546624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "97521Z^ちなみに、図{ず}面{めん}の入{にゅう}手{しゅ}は決{けっ}して簡{かん}単{たん}ではなかったぞ…",
              "maleVariant": "",
              "stringId": "3300450501410295808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "90521Z^ハンセンはホテルに関{かん}するあらゆる情{じょう}報{ほう}を厳{げん}重{じゅう}に守{まも}っているんでね…",
              "maleVariant": "",
              "stringId": "3300451265797033984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7B521Z^だが方{ほう}法{ほう}は必{かなら}ずあるものだ… お前{まえ}が助{たす}けた依{い}頼{らい}人{にん}が、借{か}りを返{かえ}してくれた。わかるかね？ この世{よ}は常{つね}に持{も}ちつ持{も}たれつだ",
              "maleVariant": "",
              "stringId": "3300451793725689856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "75521Z^建物{たてもの}の図{ず}面{めん}はお前{まえ}にやる。脆{ぜい}弱{じゃく}性{せい}についても教{おし}えてやろう。ほんの感{かん}謝{しゃ}の印{いん}と思{おも}ってくれたまえ",
              "maleVariant": "",
              "stringId": "3300453296863580160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2E521Z^そして… 言{い}うまでもないが、お前{まえ}がこの図{ず}面{めん}とホテルの情{じょう}報{ほう}を持{も}っていることを知{し}る者{もの}は誰{だれ}もいない。追{つい}跡{せき}は不{ふ}可{か}能{のう}だ",
              "maleVariant": "",
              "stringId": "3300465398890237952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "40521Z^私{わたし}は決{けっ}して取{とり}引{ひき}相{あい}手{て}を危{き}険{けん}に晒{さら}さない。例{たと}えば… そう、故{ゆえ}ミスター・デショーンと違{ちが}ってな",
              "maleVariant": "",
              "stringId": "3300465858753728512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もらう",
              "maleVariant": "",
              "stringId": "3302934803291336708"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B9747Z^さて、茶{ちゃ}を飲{の}むか？ 私{わたし}の特{とく}製{せい}ブレンドだ",
              "maleVariant": "",
              "stringId": "3302934990239854592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いらない",
              "maleVariant": "",
              "stringId": "3302937047143313408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、いただく",
              "maleVariant": "ああ、もらおう",
              "stringId": "3302937572119179264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7A747Z^いえ、結{けっ}構{こう}よ",
              "maleVariant": "7A747Z^いや、結{けっ}構{こう}だ",
              "stringId": "3302938180343590912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C7521Z^そんなことより頼{たの}んでたものは？",
              "maleVariant": "",
              "stringId": "3305589817132322816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5E521018^お話{はなし}もいいけど、頼{たの}んでたものは？ 次{つぎ}はそっちが約{やく}束{そく}を果{は}たす番{ばん}よ、ハンズ",
              "maleVariant": "5E521018^お話{はなし}もいいが、頼{たの}んでたものは？ 次{つぎ}はそっちが約{やく}束{そく}を果{は}たす番{ばん}だぞ、ハンズ",
              "stringId": "3305593126790500376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、そうだな…いいだろう",
              "maleVariant": "",
              "stringId": "3305593126790500392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D4521Z^お前{まえ}の働{はたら}きのおかげで… ハンセンにまつわる状{じょう}況{きょう}が大{おお}きく変{か}わった。私{わたし}の指{し}示{じ}通{とう}りこなしてくれて何{なに}よりだ",
              "maleVariant": "",
              "stringId": "3346430687137894400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E9521004^あんたはハンセンを潰{つぶ}したいの？",
              "maleVariant": "E9521004^あんたはハンセンを潰{つぶ}したいのか？",
              "stringId": "3346453145874206724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D521Z^汚{きたな}い政{せい}治{じ}の臭{くさ}いがする",
              "maleVariant": "",
              "stringId": "3346453571679948800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D521Z^ハンセンを引{ひ}きずり下{くだ}ろすのが、あんたの目{もく}的{てき}？",
              "maleVariant": "D521Z^ハンセンを引{ひ}きずり下{くだ}ろすのが、あんたの目{もく}的{てき}か？",
              "stringId": "3346454306119356416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8C521Z^奴{やつ}は強{きょう}大{だい}な権{けん}力{りょく}を有{ゆう}している。強{ごう}欲{よく}で、なおかつ頭{あたま}も切{き}れる男{おとこ}だ。どちらも評{ひょう}価{か}に値{あたい}する才{さい}覚{かく}であることは認{みと}めよう",
              "maleVariant": "",
              "stringId": "3346455287502606336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "74521Z^しかし、地{ち}区{く}一{ひと}つを牛{ぎゅう}耳{じ}る者{もの}がその両{りょう}方{ほう}を揃{そろ}えているとなると話{はなし}は別{べつ}だ。私{わたし}の商{しょう}売{ばい}に… 悪{あく}影{えい}響{きょう}を及{およ}ぼす",
              "maleVariant": "",
              "stringId": "3346456962137198592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F7521Z^地{ち}域{いき}一{いっ}帯{たい}の政{せい}治{じ}に手{て}を入{い}れたいってわけね。ハンズだけに",
              "maleVariant": "F7521Z^地{ち}域{いき}一{いっ}帯{たい}の政{せい}治{じ}に手{て}を入{い}れたいってわけか。ハンズだけに",
              "stringId": "3346477253760520192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9E521Z^これさえあれば、問{もん}題{だい}なく目{もく}的{てき}を果{は}たすことができるだろう。ただし… どれだけ準{じゅん}備{び}を整{ととの}えようが、蛇{へび}の巣{す}に飛{と}び込{こ}むことに変{か}わりはない。足{あし}元{もと}には気{き}をつけるんだな",
              "maleVariant": "",
              "stringId": "3346481268061769728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A8521Z^まあな。ドッグタウンも所{しょ}詮{せん}政{せい}治{じ}の場{ば}だ。ここにはここの関{かん}係{けい}性{せい}や取{と}り決{き}め、権{けん}力{りょく}闘{とう}争{そう}がある…",
              "maleVariant": "",
              "stringId": "3347685598534176768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E8521Z^不{ふ}用{よう}意{い}に動{うご}くと、泥{どろ}沼{ぬま}に沈{しず}む。そして私{わたし}は… 靴{くつ}を汚{よご}したくない",
              "maleVariant": "",
              "stringId": "3347688245307772928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8F521Z^ドッグタウンは活発{かっぱつ}な街{まち}だが、裏{うら}では策{さく}略{りゃく}が静{しず}かに渦{うず}巻{まき}いている",
              "maleVariant": "",
              "stringId": "3347713588416614400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9C521Z^いずれ大{おお}きな変{へん}化{か}が訪{おとず}れた際{きわ}は…",
              "maleVariant": "",
              "stringId": "3347714808405430272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2F521Z^誰{だれ}かが新{あら}たなレールを敷{し}いてやらねばなるまい…",
              "maleVariant": "",
              "stringId": "3347716004872589312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "24521004^[立{た}ち上{あ}がる] 新{あたら}しい仕{し}事{ごと}を楽{たの}しみにしてる",
              "maleVariant": "",
              "stringId": "3347717065544962052"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "48521Z^[立{た}ち上{あ}がる] 直{すぐ}に会{あ}えてよかった",
              "maleVariant": "",
              "stringId": "3347717517120507904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "44521Z^新{あたら}しい仕{し}事{ごと}を楽{たの}しみにしてる",
              "maleVariant": "",
              "stringId": "3347718367456923648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BF521Z^会{あ}えてよかったよ。やっと顔{かお}と名{な}前{まえ}が一{いっ}致{ち}した",
              "maleVariant": "",
              "stringId": "3347718446829932544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5B521Z^またすぐに連{れん}絡{らく}するさ。必{かなら}ずな",
              "maleVariant": "",
              "stringId": "3347719789476974592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E9521Z^お前{まえ}は幸{こう}運{うん}な奴{やつ}だ。私{わたし}の顔{かお}を見{み}て生{い}きている者{もの}はそういない",
              "maleVariant": "",
              "stringId": "3347721200608612352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B9521Z^あそこは地{じ}元{もと}と世{せ}界{かい}の政{せい}治{じ}が絡{から}み合{あ}う場{ば}所{しょ}だ… 潮{しお}の流{なが}れが激{はげ}しい",
              "maleVariant": "",
              "stringId": "3347722363739443200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "45521Z^流{なが}されないようにするよ。じゃあね、ハンズ",
              "maleVariant": "45521Z^流{なが}されないようにするさ。じゃあな、ハンズ",
              "stringId": "3347724251578896384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6F521Z^ところで… このコンバット・ゾーンが“ドッグタウン”になった経{けい}緯{い}は知{し}っているか？",
              "maleVariant": "",
              "stringId": "3353389181002977280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A7521004^さあ。統一戦{とういつせん}争{そう}？",
              "maleVariant": "A7521004^さあ。統一戦{とういつせん}争{そう}か？",
              "stringId": "3353391647253729284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうでもいい",
              "maleVariant": "",
              "stringId": "3353393114119278592"
            },
