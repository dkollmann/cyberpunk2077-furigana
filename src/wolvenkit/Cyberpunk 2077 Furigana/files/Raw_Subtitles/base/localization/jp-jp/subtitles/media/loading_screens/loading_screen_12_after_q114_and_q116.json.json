          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E24EAZ^皆{みな}さん、こんばんは。まずは、あー… えー…",
              "maleVariant": "",
              "stringId": "1672054587723259904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B444DZ^市{し}場{じょう}は混{こん}乱{らん}に陥{おち}っており… この株{かぶ}価{か}急{きゅう}落{らく}を受{う}けた損{そん}失{しつ}は5千{せん}億{おく}ユーロドルを超{こ}えると言{い}われています",
              "maleVariant": "",
              "stringId": "1680732233364983808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F444DZ^先{せん}日{じつ}発{はっ}生{せい}したアラサカ・タワーに対{たい}する襲{しゅう}撃{げき}を受{う}け、同{どう}社{しゃ}は17パーセントという株{かぶ}価{か}の大{おお}下{した}落{らく}に見{み}舞{まい}われる形{かたち}となりました。アナリストの予{よ}測{そく}では、この下{げ}落{らく}はまだ続{つづ}くとのこと",
              "maleVariant": "",
              "stringId": "1680734240188452864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E444DZ^NCPDの公式発表{こうしきはっぴょう}によりますと、襲{しゅう}撃{げき}を行{い}ったのはノーマッドの一{いち}団{だん}で、リニア用{よう}のトンネルからタワーに侵{しん}入{にゅう}したとのことです",
              "maleVariant": "",
              "stringId": "1680734635057008640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7544DZ^この悲{ひ}惨{さん}な事{じ}件{けん}を受{う}けて、私{わたし}から言{い}えることは一{ひと}つしかありません。売{う}りましょう",
              "maleVariant": "",
              "stringId": "1680735393403949056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7D651Z^今{こん}夜{や}はまず… いつもどおり、株{かぶ}式{しき}市{し}場{じょう}から",
              "maleVariant": "",
              "stringId": "1741544287841357824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AC651Z^アラサカ社{しゃ}のCEO、ヨリノブ・アラサカ氏{し}は、ナイトシティのオフィスから東{とう}京{きょう}に避{ひ}難{なん}しました",
              "maleVariant": "",
              "stringId": "1741554562191659008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B5651Z^今{いま}すぐにです。売{う}れるうちにね",
              "maleVariant": "",
              "stringId": "1741555189407879168"
            }
          ]
        }
