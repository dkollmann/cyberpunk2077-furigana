          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9A5F9Z^今{いま}からそなたに教{おし}えるのは大{だい}地{ち}の瞑{めい}想{そう}だ",
              "maleVariant": "",
              "stringId": "1815424808219807744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CE5F9Z^大{おお}いなる大{だい}自{し}然{ぜん}の精{せい}霊{れい}、母{はは}なる大{だい}地{ち}よ。我々{われわれ}は常{つね}に一{ひと}つの共{きょう}同{どう}体{たい}。我{われ}はそなたであり、そなたは我{われ}",
              "maleVariant": "",
              "stringId": "1816921764108668928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "235F9Z^我{われ}の身{しん}体{たい}と魂{たましい}から苦{く}痛{つう}を、病{びょう}を、穢{けが}れを取{と}り除{のぞ}き、害{がい}ある人々{ひとびと}、社{しゃ}会{かい}、次{じ}元{げん}から害{がい}ある気{き}を取{と}り除{のぞ}きたまえ",
              "maleVariant": "",
              "stringId": "1822658939899056128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BE5F9Z^命{いのち}の気{き}を与{あた}え、健{けん}康{こう}、若{わか}さ、知{ち}恵{え}、知{ち}識{しき}、強{つよ}さ、喜{よろこ}び、幸{しあわ}せ、そして内{ない}なる安{やす}らぎを与{あた}えたまえ",
              "maleVariant": "",
              "stringId": "1822659844442656768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "735F9Z^我々{われわれ}の心{こころ}と魂{たましい}は、喜{よろこ}び、平{へい}穏{おん}、安{あん}心{しん}、純{じゅん}潔{けつ}を感{かん}じるだろう",
              "maleVariant": "",
              "stringId": "1822667226233147392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C15F9Z^我々{われわれ}はぬくもりと喜{よろこ}びに包{つつ}み込{こ}まれる。大{だい}地{ち}の気{き}が脚{あし}を伝{つだ}い、全{ぜん}細胞{さいぼう}を満{み}たしていく。我々{われわれ}の魂{たましい}と精{せい}神{しん}が清{きよ}められていく",
              "maleVariant": "",
              "stringId": "1822667455175036928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "345F9Z^母{はは}なる大{だい}地{ち}よ、心{こころ}と、身{しん}体{たい}と、魂{たましい}をもって感{かん}謝{しゃ}申{もう}し上{あ}げる。四{よん}大{だい}元{げん}素{そ}なくして、命{いのち}は存{そん}在{ざい}しえない",
              "maleVariant": "",
              "stringId": "1822684701398110208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FE5F9Z^我々{われわれ}は豊{ゆた}かな大{だい}地{ち}を歩{あゆ}み、草{そう}原{げん}を駆{か}け抜{ぬ}ける",
              "maleVariant": "",
              "stringId": "1918338054580310016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "275F9Z^母{はは}なる大{だい}地{ち}よ、我{われ}も他{ほか}の生{い}き物{もの}も、全{すべ}てはそなたが生{う}み出{だ}し存{そん}在{ざい}",
              "maleVariant": "",
              "stringId": "1925539654566645760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "435F9Z^我々{われわれ}は靴{くつ}を脱{ぬ}ぎ、裸足{はだし}になることで土{つち}を直{ちょく}接{せつ}感{かん}じ、大{だい}地{ち}の気{き}を吸{きゅう}収{しゅう}する",
              "maleVariant": "",
              "stringId": "1925541063785680896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A45F9Z^我々{われわれ}は、咲{さ}き誇{ほこ}る花{はな}にチョウが休{やす}み、ハチが蜜{みつ}を集{あつ}める大{だい}地{ち}を歩{あゆ}んでいく",
              "maleVariant": "",
              "stringId": "1925541516384636928"
            }
          ]
        }
