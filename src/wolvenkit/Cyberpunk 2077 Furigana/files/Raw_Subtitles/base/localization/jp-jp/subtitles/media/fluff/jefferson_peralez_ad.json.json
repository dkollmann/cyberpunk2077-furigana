            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "803BCZ^例{れい}を挙{あ}げると、カン・タオ、アラサカ、ムーア・テクノロジー、ダイナラーです",
              "maleVariant": "",
              "stringId": 1803789058476589056
            },
