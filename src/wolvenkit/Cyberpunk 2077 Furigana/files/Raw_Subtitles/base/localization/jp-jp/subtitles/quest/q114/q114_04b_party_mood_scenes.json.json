      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CA3C5Z^「二{に}度{ど}と会{あ}えないかも」なんて書{か}くなよ。バカじゃないのか？",
              "maleVariant": "",
              "stringId": "1967644505896931328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1F3C5Z^彼{かの}女{じょ}、コロラドの仕{し}事{ごと}をやめてこっちに来{き}たがってるんだ。でも明日{あした}には“こっち”なんてなくなってるかもしれない！",
              "maleVariant": "",
              "stringId": "1967655489554370560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "603C5Z^まだわかんないでしょ。でもそんな手紙{てがみ}を送{おく}ったら、彼{かの}女{じょ}は最{さい}悪{あく}の事{じ}態{たい}を考{かんが}える。それでいいわけ？",
              "maleVariant": "",
              "stringId": "1967655795587567616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B73C5Z^センサーは高{こう}度{ど}が上{あ}がると誤{ご}作{さ}動{どう}を起{お}こす",
              "maleVariant": "",
              "stringId": "1967659495014027264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F03C5004^俺{おれ}が一{いち}番{ばん}たくさんの星{ほし}を見{み}たのはどこだと思{おも}う？ シナイ山{やま}の頂{ちょう}上{じょう}だ",
              "maleVariant": "",
              "stringId": "1967668889063804932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EC3C5004^このフレーズはいいな。「野{や}蛮{ばん}な者{もの}は旅{たび}をするのではなく、ただひたすらに目{もく}的{てき}地{ち}を目指{めざ}し、その土{と}地{ち}から奪{うば}う」",
              "maleVariant": "",
              "stringId": "1967669232594079748"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "423C5004^嘘{うそ}じゃない、確{かく}実{じつ}に聞{き}こえた",
              "maleVariant": "",
              "stringId": "1967670909074165764"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DD3C5004^終{お}わるまで待{ま}っててくれ",
              "maleVariant": "",
              "stringId": "1967678719925178372"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E73C5Z^そうは言{い}っても、低{てい}空{くう}を飛{と}ばすわけにもいかないしな",
              "maleVariant": "",
              "stringId": "1967717873014820864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "563C5Z^センサーは私{わたし}が見{み}るから、スラスターをお願{ねが}い。何{なに}か緩{ゆる}んでるのかも",
              "maleVariant": "",
              "stringId": "1967718278604017664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "183C5004^差{さ}動{どう}装{そう}置{ち}は大{だい}丈{じょう}夫{ぶ}？ また軋{あつ}んだら困{こま}るよ",
              "maleVariant": "",
              "stringId": "1967721786552111108"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A73C5Z^大{だい}丈{じょう}夫{ぶ}だよ、油{あぶら}を差{さ}したから",
              "maleVariant": "",
              "stringId": "1967721788951252992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2E3C5Z^じゃあ、位{い}置{ち}について…",
              "maleVariant": "",
              "stringId": "1967721791216177152"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やった！",
              "maleVariant": "",
              "stringId": "1967722596891643904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B83C5Z^だから確{かく}認{にん}したって！ いちいちうるさいな",
              "maleVariant": "",
              "stringId": "1967723580237828096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レディ…",
              "maleVariant": "",
              "stringId": "1967729477798350848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゴー！",
              "maleVariant": "",
              "stringId": "1967729535461642240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AF3C5Z^まあ、天{てん}の川{かわ}銀河{ぎんが}には最{さい}低{てい}1千{せん}億{おく}の星{ほし}があるって言{い}われてるからな",
              "maleVariant": "",
              "stringId": "1967740485078110208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "623C5Z^著{ちょ}者{しゃ}はよくわかってる。本{ほん}音{ね}を書{か}いてるんだ",
              "maleVariant": "",
              "stringId": "1967742566845403136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "363C5Z^まあ… 歴{れき}史{し}とは皮{ひ}肉{にく}なものだ。そうだろ？ 今{いま}のこの国{くに}を見{み}てみろ",
              "maleVariant": "",
              "stringId": "1967745052893270016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "193C5Z^だから俺{おれ}たちは、この荒{こう}野{や}にいる。あそこじゃなくてな",
              "maleVariant": "",
              "stringId": "1967746559940251648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "133C5Z^ラフェンの無{む}線{せん}でも拾{ひろ}ったんじゃないか？ ミリテクはモジュラー周{しゅう}波{は}数{すう}を使{つか}う。やるだけ無{む}駄{だ}だ",
              "maleVariant": "",
              "stringId": "1967773149982117888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "183C5Z^奴{やつ}らのジャマーを突破{とっぱ}して、向{む}こうのランナーに無{む}線{せん}を焼{や}かれず、こっちの位{い}置{ち}も特{とく}定{てい}されずに済{す}んだら、酒{さけ}をおごってやるよ",
              "maleVariant": "",
              "stringId": "1967773661167112192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D63C5Z^ポンコツにいくら手{て}を加{くわ}えたって、ポンコツのままだと思{おも}うけど",
              "maleVariant": "",
              "stringId": "1967774098146480128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "683C5Z^そこ、少{すこ}しずれてるぞ…",
              "maleVariant": "",
              "stringId": "1967774813560524800"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "393C5Z^邪{じゃ}魔{ま}をするなら、よそをあたってくれないか？",
              "maleVariant": "",
              "stringId": "1967776311715581952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8E29FZ^じゃあ何{なに}を書{か}けって言{い}うんだよ？",
              "maleVariant": "",
              "stringId": "1997880898559275008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6F29FZ^寂{さび}しい、愛{いと}してる、もうすぐ会{あ}える、とか色々{いろいろ}あるだろ",
              "maleVariant": "",
              "stringId": "1997882319673356288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DF29FZ^先{さき}のことなんてわからないんだから、嘘{うそ}にはならないでしょ",
              "maleVariant": "",
              "stringId": "1997882995862269952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4A29FZ^明日{あした}、建{けん}設{せつ}現{げん}場{ば}の上{じょう}空{くう}を飛{と}ばすのよ。万{まん}が一{いち}は許{ゆる}されない",
              "maleVariant": "",
              "stringId": "1997887911100018688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ベアリングは？",
              "maleVariant": "",
              "stringId": "1997913449210441728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8E29FZ^エジプトに行{い}ったことあるの？",
              "maleVariant": "",
              "stringId": "1997920382193627136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DE29FZ^昔{むかし}タンカーで働{はたら}いてて、あちこち行{い}かせてもらった。でもシナイ山{やま}の夜空{よぞら}は格{かく}別{べつ}さ… 無{む}数{すう}の星{ほし}に手{て}が届{とど}きそうだった",
              "maleVariant": "",
              "stringId": "1997921092205408256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5F29FZ^宇{う}宙{ちゅう}人{じん}って… いると思{おも}う？",
              "maleVariant": "",
              "stringId": "1997923697119850496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5229FZ^俺{おれ}たちの銀河{ぎんが}だけで考{かんが}えても、生{せい}命{めい}が存{そん}在{ざい}できる可{か}能{のう}性{せい}のある星{ほし}はいくらでもある。宇{う}宙{ちゅう}全{ぜん}体{たい}で考{かんが}えりゃもっとだ",
              "maleVariant": "",
              "stringId": "1997927042681270272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1429FZ^じゃあなぜ誰{だれ}もコンタクトしてこないの？",
              "maleVariant": "",
              "stringId": "1997927934994280448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D329FZ^俺{おれ}たちなんかに興{きょう}味{み}ないんだろ。周{まわ}りを見{み}ろ。こんなとこ来{き}たいか？",
              "maleVariant": "",
              "stringId": "1997928268911210496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D629FZ^じゃあもし俺{おれ}が連{れん}中{ちゅう}の信{しん}号{ごう}をつかまえて、現{げん}場{ば}の詳{しょう}細{さい}がわかったらどうする？",
              "maleVariant": "",
              "stringId": "1997933306958180352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6629FZ^ハッ、約{やく}束{そく}だぞ",
              "maleVariant": "",
              "stringId": "1997935529872191488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6E29FZ^これはどうだ？ 「危{き}険{けん}を顧{かえり}みず動{うご}くものは、留{と}まるものに負{ま}けはしない。変{へん}化{か}には、停{てい}滞{たい}に勝{まさ}る気{け}高{たか}さがあるからだ」",
              "maleVariant": "",
              "stringId": "1997960977687638016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7C29FZ^著{ちょ}者{しゃ}の世{せ}界{かい}が私{わたし}たちの置{お}かれた境{きょう}遇{ぐう}と似{に}てたからよ。断片化{だんぺんか}し、バラバラに分{ぶん}裂{れつ}してた",
              "maleVariant": "",
              "stringId": "2003821069955887104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7D29FZ^昔{むかし}はヨーロッパ人{にん}をあざ笑{わら}う人{にん}が多{おお}かった。国{くに}と言{げん}語{ご}に加{くわ}えて、文{ぶん}化{か}もバラバラだって… 一方{いっぽう}、こっちは“人{じん}種{しゅ}のるつぼ”だったからね",
              "maleVariant": "",
              "stringId": "2003823088607293440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6029FZ^この国{くに}って、どの国{くに}だよ？",
              "maleVariant": "",
              "stringId": "2003825544842047488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そういうことだ",
              "maleVariant": "",
              "stringId": "2003825669983301632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A429FZ^「流{なが}れさすらう心{こころ}と、幻{げん}想{そう}を持{も}ってこそ、文{ぶん}明{めい}に生{い}きる者{もの}だ」",
              "maleVariant": "",
              "stringId": "2003828711373795328"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}