          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E502Z^いい車{くるま}ね",
              "maleVariant": "E502Z^いい車{くるま}だな",
              "stringId": 1734409387521351680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "18502Z^来{き}たよ、クレア。すごいマシンね",
              "maleVariant": "18502Z^来{き}たぞ、クレア。すごいマシンだな",
              "stringId": 1734409387689123840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "18502010^あら、なかなか見{み}る目{め}があるのね",
              "maleVariant": "",
              "stringId": 1734409387689123856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1950200C^冷{れい}蔵{ぞう}庫{こ}にビールがあるから",
              "maleVariant": "",
              "stringId": 1734409387705901068
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スケジュールは？",
              "maleVariant": "",
              "stringId": 1734409388192440320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "37502008^自{じ}分{ぶん}の車{くるま}を使{つか}いたい",
              "maleVariant": "",
              "stringId": 1734409388209217544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "37502018^賞{しょう}品{ひん}はあるの？",
              "maleVariant": "37502018^賞{しょう}品{ひん}はあるのか？",
              "stringId": 1734409388209217560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5B502Z^前{まえ}は夫{おっと}のディーンと乗{の}ってたんだけど、一{いち}年{ねん}ぐらい前{まえ}に…",
              "maleVariant": "",
              "stringId": 1734409388813197312
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": 1734409388829974528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5C50200C^事{じ}故{こ}があってね",
              "maleVariant": "",
              "stringId": 1734409388829974540
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5C502018^苦{くる}しまずに即{そく}死{し}したのが、不{ふ}幸{こう}中{ちゅう}の幸{さいわ}いよ",
              "maleVariant": "",
              "stringId": 1734409388829974552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5C502024^それは気{き}の毒{どく}ね、クレア…",
              "maleVariant": "5C502024^それは気{き}の毒{どく}だな、クレア…",
              "stringId": 1734409388829974564
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5C502030^でも、あんたは走{はし}り続{つづ}けてる",
              "maleVariant": "",
              "stringId": 1734409388829974576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5D502008^人{にん}も車{くるま}も、立{た}ち止{と}まったらお終{しま}いだから",
              "maleVariant": "",
              "stringId": 1734409388846751752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "77502Z^何{なに}か賞{しょう}品{ひん}はあるの？",
              "maleVariant": "77502Z^何{なに}か賞{しょう}品{ひん}はあるのか？",
              "stringId": 1734409389282959360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "78502008^1位{くらい}には賞{しょう}金{きん}が出{で}る。その時{とき}は半々{はんはん}ってことでいい？",
              "maleVariant": "",
              "stringId": 1734409389299736584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "96502Z^ドライバーが要{い}るの。レースのね",
              "maleVariant": "",
              "stringId": 1785290523034001408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "37502Z^どうせなら、自{じ}分{ぶん}の車{くるま}を使{つか}いたい",
              "maleVariant": "",
              "stringId": 1785292613089239040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "12502Z^装{そう}備{び}は十{じゅう}分{ぶん}なの？ これは単{たん}なるスピード勝負{しょうぶ}じゃない、命{いのち}懸{か}けのぶつかり合{あ}いなのよ",
              "maleVariant": "",
              "stringId": 1785292805742010368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レースはいつなの？",
              "maleVariant": "レースはいつなんだ？",
              "stringId": 1785293402457251840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7B502Z^まずは予{よ}選{せん}を突破{とっぱ}しないと、決勝{けっしょう}に参{さん}加{か}できない",
              "maleVariant": "",
              "stringId": 1785293533353091072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4E502Z^2レース以{い}上{じょう}で3位{くらい}以{い}内{ない}に入{い}ること",
              "maleVariant": "",
              "stringId": 1785293605612560384
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FE502Z^じゃ、シティ・センターで会{あ}いましょ。詳{くわ}しくはそこで話{はな}すから。エンジンのうなり声{こえ}を辿{たど}れば、レース会{かい}場{じょう}に着{つ}くはずよ",
              "maleVariant": "",
              "stringId": 1786416755398156288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F8502Z^大{だい}丈{じょう}夫{ぶ}よ。ソープボックスに乗{の}ってるわけじゃないから",
              "maleVariant": "F8502Z^大{だい}丈{じょう}夫{ぶ}だ。ソープボックスに乗{の}ってるわけじゃないからな",
              "stringId": 1827282789298348032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6F521004^あんたはナビ役{やく}？",
              "maleVariant": "6F521004^あんたはナビ役{やく}か？",
              "stringId": 1854851610842370052
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4F521Z^あたしがドライバーってことは、あんたはナビ役{やく}？",
              "maleVariant": "4F521Z^俺{おれ}がドライバーってことは、あんたはナビ役{やく}か？",
              "stringId": 1854851691909877760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E6521Z^どんなレースなの？ 特{とく}殊{しゅ}なルールがあるとか？",
              "maleVariant": "E6521Z^どんなレースなんだ？ 特{とく}殊{しゅ}なルールがあるとか？",
              "stringId": 1854851728802975744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どんなレースをするの？",
              "maleVariant": "どんなレースをするんだ？",
              "stringId": 1854852433966141440
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1E521Z^ちょっと違{ちが}う。このレースにはドライバーとガンナーが必{ひつ}要{よう}なの",
              "maleVariant": "",
              "stringId": 1854854663205163008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C8521Z^むしろルールがないのが特{とく}徴{ちょう}ね。ドライバーとガンナーのペアで出{で}るの",
              "maleVariant": "",
              "stringId": 1854857732663939072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D0521Z^あんたの車{くるま}だし、あんたが運{うん}転{てん}すればいいんじゃないの？",
              "maleVariant": "D0521Z^あんたの車{くるま}だし、あんたが運{うん}転{てん}すればいいんじゃないのか？",
              "stringId": 1854859025583312896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "60521Z^私{わたし}は整{せい}備{び}士{し}よ。銃{じゅう}もそれなりに使{つか}えるけど、ラリーレーサーにはなれない",
              "maleVariant": "",
              "stringId": 1854860587072360448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B8521Z^あなた、運{うん}転{てん}は得{とく}意{い}なんでしょ？",
              "maleVariant": "",
              "stringId": 1854861507671756800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "41521004^わかった、引{ひ}き受{う}ける",
              "maleVariant": "",
              "stringId": 1854862729740947460
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9521Z^よし、わかった。ドライバーは任{まか}せて",
              "maleVariant": "9521Z^よし、わかった。ドライバーは任{まか}せろ",
              "stringId": 1854862801815867392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "58521Z^まさか初{はつ}出{しゅつ}場{じょう}ってわけじゃないよね？",
              "maleVariant": "58521Z^まさか初{はつ}出{しゅつ}場{じょう}ってわけじゃないよな？",
              "stringId": 1854862854680875008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C9521Z^前{まえ}にもレースに出{で}たことはあるのよね？",
              "maleVariant": "C9521Z^前{まえ}にもレースに出{で}たことはあるんだよな？",
              "stringId": 1854863822944342016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F7521Z^心{しん}配{ぱい}しないで。それなりに経{けい}験{けん}は積{つ}んでる",
              "maleVariant": "",
              "stringId": 1854864674119618560
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C9521Z^前{まえ}のドライバーは？ やめたの？",
              "maleVariant": "C9521Z^前{まえ}のドライバーは？ やめたのか？",
              "stringId": 1854867430716870656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BF521Z^ある意{い}味{み}ね。レース中{なか}の事{じ}故{こ}で死{し}んだの",
              "maleVariant": "",
              "stringId": 1854867825686089728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー… ごめん…",
              "maleVariant": "あー… すまん…",
              "stringId": 1854868629398622208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "...",
              "maleVariant": "",
              "stringId": 1854870345674264576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "21521Z^レースは全{ぜん}部{ぶ}で四{よ}つ――まずはシティ・センターから",
              "maleVariant": "",
              "stringId": 1854871048555728896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "33521Z^そこまで言{い}うなら… 運{うん}転{てん}手{しゅ}のあなたに任{まか}せる",
              "maleVariant": "",
              "stringId": 1854910158829916160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EC521Z^待{ま}ちきれないね",
              "maleVariant": "EC521Z^待{ま}ちきれないな",
              "stringId": 1854910866308337664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5521Z^条{じょう}件{けん}は？",
              "maleVariant": "",
              "stringId": 1854912558944882688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "11521Z^バッドランズ、サントと続{つづ}いて、最{さい}後{ご}はワトソンよ",
              "maleVariant": "",
              "stringId": 1863062461453832192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A514Z^ほら、頑{がん}張{ば}って…",
              "maleVariant": "",
              "stringId": 1935548074635116544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FD514Z^言{い}われたとおり来{き}たよ",
              "maleVariant": "FD514Z^言{い}われたとおり来{き}たぞ",
              "stringId": 1935549826763669504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "73514Z^クレア――来{き}たよ",
              "maleVariant": "73514Z^クレア――来{き}たぞ",
              "stringId": 1935550558887821312
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8A514Z^この子{こ}は“ビースト”よ。私{わたし}の自{じ}慢{まん}のマシン",
              "maleVariant": "",
              "stringId": 1935552135526694912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DF521Z^うん、ちょっと待{ま}っててくれる？",
              "maleVariant": "",
              "stringId": 1935927010288340992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "18521Z^この子{こ}は“ビースト”よ",
              "maleVariant": "",
              "stringId": 1935927380611829760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D3521Z^ありがと。で、用{よう}件{けん}は？",
              "maleVariant": "D3521Z^どうも。で、用{よう}件{けん}は？",
              "stringId": 1935931751730909184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": 1944782724081934336
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、やればいいんでしょ",
              "maleVariant": "わかった、やればいいんだろ",
              "stringId": 1962021937652703236
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C7502Z^遠{えん}慮{りょ}しとく",
              "maleVariant": "",
              "stringId": 1962022230448676864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "73502Z^せっかくだけど、遠{えん}慮{りょ}しとく",
              "maleVariant": "73502Z^せっかくだが、遠{えん}慮{りょ}しとく",
              "stringId": 1962022598406578176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "76502Z^本{ほん}当{とう}にいいの？ あなたは適{てき}任{にん}だと思{おも}うけど",
              "maleVariant": "",
              "stringId": 1962022942054293504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "93502Z^ええ。あんたは楽{たの}しんで",
              "maleVariant": "93502Z^ああ。あんたは楽{たの}しんでくれ",
              "stringId": 1962023161584164864
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "15502Z^ええ。でも、あんたはどうぞ楽{たの}しんで",
              "maleVariant": "15502Z^ああ。だが、あんたはどうぞ楽{たの}しんでくれ",
              "stringId": 1962023296909189120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まったく… しょうがないな",
              "maleVariant": "",
              "stringId": 1962023312948207616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "78521Z^エンジンと銃{じゅう}声{せい}のハーモニー、あふれ出{で}るアドレナリン… こんな楽{たの}しいことを見{み}逃{のが}していいわけ？",
              "maleVariant": "",
              "stringId": 1983919625960820736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C9521Z^残{ざん}念{ねん}ね…",
              "maleVariant": "",
              "stringId": 1983927096267902976
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クレア？",
              "maleVariant": "",
              "stringId": 2003779718919307268
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あー… クレア？",
              "maleVariant": "",
              "stringId": 2003782011391655936
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
