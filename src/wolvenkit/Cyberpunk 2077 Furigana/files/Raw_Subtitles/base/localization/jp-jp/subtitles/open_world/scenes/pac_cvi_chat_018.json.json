          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4B647Z^みんな、我{われ}先{さき}に逃{に}げ出{だ}すのね",
              "maleVariant": "",
              "stringId": "2179412543517519872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9B3BCZ^母{かあ}さん、よして。大{おお}げさよ",
              "maleVariant": "",
              "stringId": "2189631659328651264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CC3BCZ^事{じ}実{じつ}でしょ。街{まち}から人{にん}が消{き}えていってる。特{とく}に若{わか}い人{にん}たちがね",
              "maleVariant": "",
              "stringId": "2189631853424263168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C3BCZ^ここにいても、未{み}来{らい}がないからよ。街{まち}に行{い}けば仕{し}事{ごと}があるし、まともな暮{く}らしができるかもしれない",
              "maleVariant": "",
              "stringId": "2189631944692318208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BA3BCZ^待{ま}ってて。私{わたし}がヘイウッドで稼{かせ}ぐから。お金{きん}が貯{た}まったら母{かあ}さんも一{いっ}緒{しょ}に住{す}もう。きっとその方{ほう}がいいって",
              "maleVariant": "",
              "stringId": "2189632076460572672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BB3BCZ^人{にん}もその根{ね}から切{き}り離{はな}されると、枯{か}れてしまう。私{わたし}の故{ふる}郷{さと}はここだけよ",
              "maleVariant": "",
              "stringId": "2189632295520681984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7B3BCZ^でも… はあ、もういい。また今{こん}度{ど}話{はな}そう",
              "maleVariant": "",
              "stringId": "2189632479130533888"
            }
          ]
        }
