              "stringId": "1802298368609030144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "98351Z^じゃあ二{ふた}人{り}で楽{たの}しんで",
              "maleVariant": "",
              "stringId": "1802299005874802688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "20351Z^俺{おれ}はそういう役{やく}回{まわ}りを求{もと}められてると思{おも}ってる",
