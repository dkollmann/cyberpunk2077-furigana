            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "95405004^あたしのせいだって言{い}いたいの？",
              "maleVariant": "95405004^俺{おれ}のせいだって言{い}いたいのか？",
              "stringId": "1660729509290201092"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AE405Z^落{お}ち着{つ}いて。こういうこともある",
              "maleVariant": "AE405Z^落{お}ち着{つ}け。こういうこともある",
              "stringId": "1660729930616426496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6D405Z^落{お}ち着{つ}いて。こういうこともある",
              "maleVariant": "6D405Z^落{お}ち着{つ}け。こういうこともある",
              "stringId": "1660731196541259776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FB405Z^[イグアナに触{ふ}る] 売{う}ろう",
              "maleVariant": "FB405Z^[イグアナに触{ふ}る] 売{う}るべきだ",
              "stringId": "1660740068031090688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FB405010^イグアナに詳{くわ}しいの？",
              "maleVariant": "FB405010^イグアナに詳{くわ}しいのか？",
              "stringId": "1660740068031090704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BF405Z^うお！ 本{ほん}物{もの}のイグアナだ！ たしか、レッサーアンティルとかいうやつじゃねえか？",
              "maleVariant": "",
              "stringId": "1660740835823603712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "31405Z^売{う}れば金{きん}になると思{おも}う？",
              "maleVariant": "31405Z^売{う}れば金{きん}になると思{おも}うか？",
              "stringId": "1660741391786987520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5E405Z^相{あい}棒{ぼう}だろ。金{きん}は山{やま}分{わ}けだ",
              "maleVariant": "",
              "stringId": "1660742685327118336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レッサーアンティル？",
              "maleVariant": "",
              "stringId": "1660743425755992064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C3405008^ああ。テレビで見{み}た。小{しょう}アンティル諸{しょ}島{しま}に生{せい}息{そく}してて、30年{ねん}前{まえ}に絶{ぜつ}滅{めつ}したヤツだ",
              "maleVariant": "",
              "stringId": "1660743425755992072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A2405Z^ホントは飼{か}いたいけどな… ペットが欲{ほっ}しくてよ",
              "maleVariant": "",
              "stringId": "1660747702989770752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "76405Z^マニーって名{な}前{まえ}まで考{かんが}えてたのに",
              "maleVariant": "",
              "stringId": "1660749381583785984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CA405Z^ところで… ナイトシティで何{なに}するかはもう決{き}めてんのか？",
              "maleVariant": "",
              "stringId": "1660749876914311168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CC405Z^特{とく}に決{き}めてない",
              "maleVariant": "",
              "stringId": "1660750834725572608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CC405010^どうして聞{き}くの？",
              "maleVariant": "CC405010^どうして聞{き}く？",
              "stringId": "1660750834725572624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DC405Z^どうして聞{き}くの？",
              "maleVariant": "DC405Z^なぜ聞{き}くんだ？",
              "stringId": "1660751711167336448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C72EFZ^あたしのせいだと思{おも}ってるなら、そう言{い}えばいいでしょ",
              "maleVariant": "C72EFZ^俺{おれ}のせいだと思{おも}ってるなら、そう言{い}えばいいだろ",
              "stringId": "1731663961628012544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんだそりゃ？  フォローになってねえぞ！",
              "maleVariant": "",
              "stringId": "1731681828289310720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1C2EFZ^お前{まえ}自{じ}身{しん}はどう思{おも}ってんだ？",
              "maleVariant": "",
              "stringId": "1731687851662176256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "592EFZ^ああ、二{ふた}人{り}で一{いち}儲{もう}けしてやろう",
              "maleVariant": "",
              "stringId": "1731711088458657792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BE2EFZ^「二{ふた}人{り}」？",
              "maleVariant": "",
              "stringId": "1731711236182044672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C42EFZ^特{とく}に決{き}めてない。ここ数{すう}年{ねん}は、ノーマッドのファミリーと州{しゅう}を行{い}き来{き}して過{す}ごしてたけど",
              "maleVariant": "C42EFZ^特{とく}に決{き}めてない。ここ数{すう}年{ねん}は、ノーマッドのファミリーと州{しゅう}を行{い}き来{き}して過{す}ごしてたが",
              "stringId": "1732774790444281856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AD2EFZ^よう、お前{まえ}、よく生{い}きてたな",
              "maleVariant": "",
              "stringId": "1732872998780596224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AE2EFZ^どこかで帰{かえ}りを待{ま}っててくれてんのか",
              "maleVariant": "",
              "stringId": "1732906860319535104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8A2EFZ^いえ。別{べつ}の道{みち}を歩{あゆ}むことにしたの",
              "maleVariant": "8A2EFZ^いや。別{べつ}の道{みち}を歩{あゆ}むことにしたんだ",
              "stringId": "1732906958499803136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "152EFZ^帰{かえ}る先{さき}がないってのは、つれえよな",
              "maleVariant": "",
              "stringId": "1732907265774514176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C72EFZ^なんつうか、行{い}くだけ行{い}って、ダラダラすんのももったいないと思{おも}ってな",
              "maleVariant": "",
              "stringId": "1732910034719797248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "982EFZ^それにナイトシティは一{ひと}人{り}で生{い}きていける場{ば}所{しょ}じゃねえ",
              "maleVariant": "",
              "stringId": "1732910686766297088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "222EFZ^気{き}にすんな。これも何{なに}かの縁{へり}だ。それに、お前{まえ}とは気{き}が合{あ}いそうだしな… 相{あい}棒{ぼう}",
              "maleVariant": "",
              "stringId": "1732935380848537600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FC2EFZ^助{たす}けを頼{たの}んだ覚{おぼ}えはない",
              "maleVariant": "",
              "stringId": "1740238259133280256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう",
              "maleVariant": "",
              "stringId": "1740238272840265732"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3E2EFZ^手助{てだす}けはいらない",
              "maleVariant": "",
              "stringId": "1740238689737306112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7C2EFZ^いいじゃねえか。これも何{なに}かの縁{へり}だ。それに、お前{まえ}とは気{き}が合{あ}いそうだしな… 相{あい}棒{ぼう}",
              "maleVariant": "",
              "stringId": "1740239352202457088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "12EFZ^ありがとう、助{たす}かる",
              "maleVariant": "",
              "stringId": "1740240449650487296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "692EFZ^まあ、心{しん}配{ぱい}すんな。俺{おれ}が住{す}む場{ば}所{しょ}探{さが}すのを手{て}伝{つだ}ってやるからよ",
              "maleVariant": "",
              "stringId": "1740241310388776960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "612EFZ^あたしたちが荷{に}物{もつ}を持{も}ってるって、税{ぜい}関{かん}が企{き}業{ぎょう}にたれ込{こ}んだのよ",
              "maleVariant": "612EFZ^俺{おれ}たちが荷{に}物{もつ}を持{も}ってるって、税{ぜい}関{かん}が企{き}業{ぎょう}にたれ込{こ}んだんだ",
              "stringId": "1812670217100128256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "252EFZ^クソ野{や}郎{ろう}どもが、賄{わい}賂{ろ}を受{う}け取{と}ったうえで、俺{おれ}たちを売{う}りやがったのか！ 信{しん}じられねえ！",
              "maleVariant": "",
              "stringId": "1812683710880739328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F22EFZ^賭{か}けに出{で}たのよ。あたしたちにクランがついてないと見{み}破{やぶ}られたのね",
              "maleVariant": "F22EFZ^賭{か}けに出{で}たんだ。俺{おれ}たちにクランがついてないと見{み}破{やぶ}られたんだろ",
              "stringId": "1812686329955151872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウソでしょ",
              "maleVariant": "ウソだろ",
              "stringId": "1812693221314064384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DB2EFZ^ケッ… 納{なっ}得{とく}いかねえ。で、どうすんだ？",
              "maleVariant": "",
              "stringId": "1812693583769038848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A62EFZ^開{かい}封{ふう}済{す}みの荷{に}物{もつ}を受{う}け取{と}って喜{よろこ}ぶ客{きゃく}がいる？ トラブルの元{もと}よ",
              "maleVariant": "A62EFZ^開{かい}封{ふう}済{す}みの荷{に}物{もつ}を受{う}け取{と}って喜{よろこ}ぶ客{きゃく}がいるか？ トラブルの元{もと}だ",
              "stringId": "1812700626626211840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FF2EFZ^[開{ひら}ける] わかった",
              "maleVariant": "",
              "stringId": "1812701465638006784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "792EFZ^客{きゃく}なんか知{し}るか。これからは自{じ}分{ぶん}の好{す}きなようにやる。手{て}数{すう}料{りょう}も仲{ちゅう}介{かい}人{にん}もクソ食{く}らえだ",
              "maleVariant": "",
              "stringId": "1812723075665293312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "332EFZ^アラサカって書{か}いてある… まさか相{あい}手{て}がこんな大{おお}物{もの}だとは",
              "maleVariant": "",
              "stringId": "1812804648804741120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A82EFZ^その分{ふん}、手{て}に入{い}る金{きん}もデカいはずだ",
              "maleVariant": "",
              "stringId": "1812805170458718208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "122EFZ^だが、売{う}り物{もの}が何{なに}かも知{し}らずに、新{あたら}しい買{か}い手{て}は探{さが}せねぇ",
              "maleVariant": "",
              "stringId": "1815449295838441472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "572EFZ^それなりのフィクサーなら、こいつを欲{ほっ}しがる奴{やつ}を見{み}つけてくれる",
              "maleVariant": "",
              "stringId": "1815639482442903552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "18405Z^捕{つか}まるところだったじゃねえか！ これのどこが密{みつ}輸{ゆ}だ！",
              "maleVariant": "",
              "stringId": "1834150743188197376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "チンガード！",
              "maleVariant": "",
              "stringId": "1834150743624404992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ、すべてスムーズにいくはずだったのによ",
              "maleVariant": "",
              "stringId": "1834150744127721472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9C2EF004^報{ほう}酬{しゅう}を払{はら}って",
              "maleVariant": "9C2EF004^報{ほう}酬{しゅう}を払{はら}ってくれ",
              "stringId": "1885092012469710852"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5B2EFZ^州境{しゅうざかい}は越{こ}えたでしょ。約{やく}束{そく}の報{ほう}酬{しゅう}をもらい次{し}第{だい}、解{かい}散{さん}よ",
              "maleVariant": "5B2EFZ^州境{しゅうざかい}は越{こ}えただろ。約{やく}束{そく}の報{ほう}酬{しゅう}をもらい次{し}第{だい}、解{かい}散{さん}だ",
              "stringId": "1885092071508733952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F12EFZ^それはこっちが聞{き}きたい。報{ほう}酬{しゅう}も払{はら}わないまま、ずっと文{もん}句{く}を言{い}い続{つづ}ける気{き}？",
              "maleVariant": "F12EFZ^それはこっちが聞{き}きたい。報{ほう}酬{しゅう}も払{はら}わないまま、ずっと文{もん}句{く}を言{い}い続{つづ}ける気{き}か？",
              "stringId": "1885092082615250944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B42EFZ^それはこっちが聞{き}きたい",
              "maleVariant": "",
              "stringId": "1885092317815042048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B52EFZ^実{み}を言{い}うとよ――いま金{きん}欠{けつ}でな。すぐには払{はら}えない",
              "maleVariant": "",
              "stringId": "1885122683250601984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "442EFZ^あたしが黙{だま}って待{ま}つと思{おも}ったわけ？",
              "maleVariant": "442EFZ^俺{おれ}が黙{だま}って待{ま}つと思{おも}ったのか？",
              "stringId": "1885141424592056320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5C2EFZ^企{き}業{ぎょう}のオモチャを届{とど}けて、このヤマを片{かた}付{づ}けりゃ、お前{まえ}に渡{わた}す分{ふん}ができる",
              "maleVariant": "",
              "stringId": "1885141691282681856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B62EFZ^いや、本{ほん}当{とう}は一{いっ}切{さい}払{はら}わずに、州境{しゅうざかい}を越{こ}えたらトンズラするつもりだったんだが…",
              "maleVariant": "",
              "stringId": "1885144037844774912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あたしをだましたのね",
              "maleVariant": "5C2EFZ^俺{おれ}をだましたのか",
              "stringId": "1885144826608807936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "だましたのね",
              "maleVariant": "だましたのか",
              "stringId": "1885144847731322880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3A2EFZ^正{しょう}直{じき}なのはいいことね",
              "maleVariant": "3A2EFZ^正{しょう}直{じき}なのはいいことだ",
              "stringId": "1885144997837074432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D22EFZ^正{しょう}直{じき}なのはいいことね",
              "maleVariant": "D22EFZ^正{しょう}直{じき}なのはいいことだ",
              "stringId": "1885145593092698112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B22EFZ^そう怒{ど}んなって。密{みつ}輸{ゆ}犯{はん}のピュアなハートを傷{きず}つけちまったか",
              "maleVariant": "",
              "stringId": "1885162312863510528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おうよ",
              "maleVariant": "",
              "stringId": "1885171940603129856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて、どうしようか",
              "maleVariant": "さて、どうするか",
              "stringId": "1885176987525246976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3F2EFZ^中{なか}身{み}を拝{はい}んでみようぜ",
              "maleVariant": "",
              "stringId": "1885178090643976192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D92EFZ^お前{まえ}を気{き}に入{い}った",
              "maleVariant": "",
              "stringId": "1885197235896905728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "70449Z^開{ひら}けてくれ",
              "maleVariant": "",
              "stringId": "1898158607917420544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6B449Z^あたしもあんたが気{き}に入{い}った",
              "maleVariant": "6B449Z^俺{おれ}もお前{まえ}が気{き}に入{い}った",
