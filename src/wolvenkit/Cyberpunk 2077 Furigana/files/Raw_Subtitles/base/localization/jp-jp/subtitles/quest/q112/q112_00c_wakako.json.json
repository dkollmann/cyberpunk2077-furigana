          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "98404Z^あのさ、俺{おれ}は名{な}前{まえ}を覚{おぼ}えるのは苦{にが}手{て}だが、顔{かお}はよく覚{おぼ}えてる方{ほう}なんだよ！",
              "maleVariant": "",
              "stringId": "1903987643713077248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "24404Z^人{ひと}違{ちが}いだ",
              "maleVariant": "",
              "stringId": "1903988419156000768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BA404Z^V、よう来{らい}はったな",
              "maleVariant": "",
              "stringId": "1903991552703741952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "44404Z^ええやろ。ほなら用{よう}件{けん}言{い}うてみ。電{でん}話{わ}ならいつでもできる",
              "maleVariant": "",
              "stringId": "1903993144156897280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4C404004^ジャパンタウンのパレードについて情{じょう}報{ほう}が欲{ほ}しい",
              "maleVariant": "",
              "stringId": "1903993925975162884"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "89404Z^ジャパンタウンでパレードがある。その情{じょう}報{ほう}が欲{ほ}しいの",
              "maleVariant": "89404Z^ジャパンタウンでパレードがある。その情{じょう}報{ほう}が欲{ほ}しいんだ",
              "stringId": "1903995722294902784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C6404Z^またアラサカ絡{から}みかいな",
              "maleVariant": "",
              "stringId": "1903995723318312960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E0404Z^仕{し}事{ごと}熱心{ねっしん}なのは構{かま}わへんけど、なんでわざわざうちとこに？",
              "maleVariant": "",
              "stringId": "1903995723754520576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FE404Z^ミスター・デショーンは電{でん}話{わ}にでえへんかったんか？",
              "maleVariant": "",
              "stringId": "1903995724257837056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "とまあ、こんなところや",
              "maleVariant": "",
              "stringId": "1903995730431852544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "89404Z^十{じゅう}分{ぶん}です",
              "maleVariant": "",
              "stringId": "1903995730884837376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A2404Z^ほんならもう帰{かえ}ってんか",
              "maleVariant": "",
              "stringId": "1903995731304267776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "12404Z^ここまで聞{き}いて、何{なに}をするか知{し}りたくないの？",
              "maleVariant": "12404Z^ここまで聞{き}いて、何{なに}をするか知{し}りたくないのか？",
              "stringId": "1903998593631535104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4E404Z^オカダさん、恩{おん}に着{ちゃく}ます",
              "maleVariant": "",
              "stringId": "1904047836438216704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B0404Z^あんた、どっかで見{み}たんだよな。テレビか…？",
              "maleVariant": "",
              "stringId": "1904215148533399552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8D404Z^おお、いいところに来{き}てくれた",
              "maleVariant": "",
              "stringId": "1904219722086367232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B6404Z^V、さっさと店{みせ}の中{なか}に入{い}るぞ",
              "maleVariant": "",
              "stringId": "1904221548135333888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AA404004^ええ、行{い}きましょ",
              "maleVariant": "AA404004^ああ、行{い}こう",
              "stringId": "1904221766977339396"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DF404Z^ヒデシ、せっかくのファンに悪{わる}いでしょ",
              "maleVariant": "DF404Z^ヒデシ、せっかくのファンに悪{わる}いだろ",
              "stringId": "1904222141528686592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "64404Z^ヒデシ、せっかくのファンに悪{わる}いじゃない",
              "maleVariant": "64404Z^ヒデシ、せっかくのファンに悪{わる}いだろ",
              "stringId": "1904222723580641280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6F404Z^やめろ、今{いま}はふざけてる時{とき}ではない",
              "maleVariant": "",
              "stringId": "1904223093132378112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C5404Z^頼{たの}むって！ 久{ひさ}し振{ぶ}りに聞{き}かせてくれよ！",
              "maleVariant": "",
              "stringId": "1904223451057504256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9404Z^ええ、行{い}きましょ",
              "maleVariant": "9404Z^ああ、行{い}こう",
              "stringId": "1904223843040378880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "27404Z^これで肝心{かんじん}な情{じょう}報{ほう}は大{だい}体{たい}手{て}に入{いっ}ったと言{い}えるが――",
              "maleVariant": "",
              "stringId": "1904253663501631488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F6404Z^念{ねん}のため、もう少{すこ}し調{しら}べてみる。ここに残{のこ}って、いくつかツテを当{あ}たってみるとしよう",
              "maleVariant": "",
              "stringId": "1904255041364049920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F6404Z^何{なに}か新{あたら}しいことがわかったら、すぐに連{れん}絡{らく}する",
              "maleVariant": "",
              "stringId": "1904255307652022272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんだって？",
              "maleVariant": "",
              "stringId": "1904256366093025280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B2404Z^あとは神頼{かみだの}みしかないってくらい、思{おも}い詰{つ}めた顔{かお}してるからさ",
              "maleVariant": "",
              "stringId": "1904256637951033344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FC404Z^そう見{み}えるのか？",
              "maleVariant": "",
              "stringId": "1904258241215348736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "72404Z^運{うん}命{めい}はルーレットと同{おな}じ。勇{ゆう}気{き}を出{だ}してボタンを押{お}せば、道{みち}が開{ひら}けることもある",
              "maleVariant": "",
              "stringId": "1904258646921986048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AB404Z^本{ほん}題{だい}の前{まえ}に… サンドラ・ドーセットの件{けん}の報{ほう}酬{しゅう}は？",
              "maleVariant": "",
              "stringId": "1904303405732478976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A0404Z^本{ほん}題{だい}の前{まえ}に… サンドラ・ドーセットの件{けん}の報{ほう}酬{しゅう}がまだなんだけど？",
              "maleVariant": "A0404Z^本{ほん}題{だい}の前{まえ}に… サンドラ・ドーセットの件{けん}の報{ほう}酬{しゅう}がまだなんだが？",
              "stringId": "1904303938123874304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "28404Z^今{いま}までほったらかしにしとったやろ。何{なに}や、急{きゅう}にせっついてからに",
              "maleVariant": "",
              "stringId": "1904304511636226048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "29404Z^V、今{いま}はよせ。それよりここに来{き}た理{り}由{ゆう}を話{はな}すんだ",
              "maleVariant": "",
              "stringId": "1904304649091956736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B3514Z^俺{おれ}を別{べつ}の誰{だれ}かと勘{かん}違{ちが}いしてくれて助{たす}かった",
              "maleVariant": "",
              "stringId": "1949140527975841792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D8514004^聡{そう}明{めい}で、魅{み}力{りょく}的{てき}な女{じょ}性{せい}だ",
              "maleVariant": "",
              "stringId": "1950228564071759876"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "67404Z^このチップに入{いっ}っとる",
              "maleVariant": "",
              "stringId": "1978123618848096256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジャパンタウン――",
              "maleVariant": "",
              "stringId": "1978125649344217088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "59404Z^パレードの間{かん}は地{ち}区{く}全{ぜん}体{たい}が封{ふう}鎖{さ}される",
              "maleVariant": "",
              "stringId": "1978128300127567872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C2404Z^メインとなるのは山車{だし}の行{ぎょう}列{れつ}や。通{とう}りの上{じょう}空{くう}をこっちに向{む}けて通{つう}過{か}する",
              "maleVariant": "",
              "stringId": "1978128482277801984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9A404Z^主{しゅ}賓{ひん}のハナコ・アラサカは、一{いち}番{ばん}大{おお}きな山車{だし}からスピーチを行{おこな}う予{よ}定{てい}になっとる。せやな、具{ぐ}体{たい}的{てき}な場{ば}所{しょ}は… ここや",
              "maleVariant": "",
              "stringId": "1978128790844358656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "14404Z^警{けい}備{び}についての情{じょう}報{ほう}はあらへん。まあ、ゆうても最{さい}近{きん}色々{いろいろ}あったし、それなりに厳{げん}重{じゅう}やろな",
              "maleVariant": "",
              "stringId": "1978128951804968960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5B404Z^前{ぜん}回{かい}のパレードでは、アラサカの奴{やつ}らが身{み}分{ぶん}を偽{にせ}って群{ぐん}衆{しゅう}に紛{まぎ}れとったわ",
              "maleVariant": "",
              "stringId": "1978129704615428096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4404Z^そこら中{なか}に狙{そ}撃{げき}手{て}やドローンがおるのは当{あ}たり前{まえ}。それだけでも十{じゅう}分{ぶん}厄介{やっかい}やけど、少{すく}なくとも一{ひと}人{り}はネットランナーがおるやろな",
              "maleVariant": "",
              "stringId": "1978130012393455616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D2404Z^山車{だし}の行{ぎょう}列{れつ}がジャパンタウンを出{で}たら、パレードは終{しま}いや。後{のち}に残{のこ}るんは、誰{だれ}も片{かた}づけへんゴミの山{やま}っちゅう話{はなし}やな",
              "maleVariant": "",
              "stringId": "1978131476138442752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5F404Z^[チップを受{う}け取{と}る]",
              "maleVariant": "",
              "stringId": "1996740137278521344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2F2D2Z^ローグ、落{お}ち着{つ}きなはれ。あんた何{なに}やえらい誤{ご}解{かい}しとるようやな…",
              "maleVariant": "",
              "stringId": "2004126178845990912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9D2D2Z^あんたのチームを襲{おそ}ったんが誰{だれ}や知{し}らんけど、クロウズやないのは確{たし}かや。うちの子{こ}らは分{ふん}をわきまえとる",
              "maleVariant": "",
              "stringId": "2004127993167683584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2B2D2Z^かんにん、もう切{き}るで。お客{きゃく}さんが、来{らい}はってん",
              "maleVariant": "",
              "stringId": "2004131298379898880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A92D2Z^そっちのええ男{おとこ}は誰{だれ}や…？",
              "maleVariant": "",
              "stringId": "2004141114494099456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CF2D2004^仕{し}事{ごと}相{あい}手{て}よ",
              "maleVariant": "CF2D2004^仕{し}事{ごと}相{あい}手{て}だ",
              "stringId": "2004142566830579716"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コメディアンのヒデシ・ヒノよ",
              "maleVariant": "コメディアンのヒデシ・ヒノだ",
              "stringId": "2004142835333144576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "52D2Z^たまたま一{いっ}緒{しょ}に仕{し}事{ごと}をしてるだけの相{あい}手{て}よ",
              "maleVariant": "52D2Z^たまたま一{いっ}緒{しょ}に仕{し}事{ごと}をしてるだけの相{あい}手{て}さ",
              "stringId": "2004143907766345728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コメディアンのヒデシ・ヒノよ",
              "maleVariant": "コメディアンのヒデシ・ヒノだ",
              "stringId": "2004144025626288128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CD2D2Z^ゴロウ・タケムラです。お会{あ}いできて光{こう}栄{えい}です",
              "maleVariant": "",
              "stringId": "2004147944096079872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8E2D2Z^あら… えらい紳{しん}士{し}的{てき}やないの！",
              "maleVariant": "",
              "stringId": "2004157714089713664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4D2D2Z^気{き}の毒{どく}やな。聞{き}いた話{はなし}やと、アラサカの兵{へい}隊{たい}はんに追{お}われとるんやろ？",
              "maleVariant": "",
              "stringId": "2004158060891545600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A62D2Z^V、なんでうちに連{つ}れてきたんや？",
              "maleVariant": "",
              "stringId": "2004159007277522944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DF2D2Z^今{いま}すぐ然{しか}るべきとこに“通{つう}報{ほう}”せんほうがええ理{り}由{ゆう}があるんやったら、言{い}うてみ",
              "maleVariant": "",
              "stringId": "2004160816415055872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C2D2Z^仕{し}事{ごと}を持{も}ってきたの。エディーは好{す}きでしょ？",
              "maleVariant": "C2D2Z^仕{し}事{ごと}を持{も}ってきた。エディーは好{す}きだろ？",
              "stringId": "2004162517977079808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C2D2010^そんなこと言{い}って、興{きょう}味{み}があるんでしょ",
              "maleVariant": "C2D2010^そんなこと言{い}って、興{きょう}味{み}があるんだろ",
              "stringId": "2004162517977079824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "462D2Z^その気{き}があればとっくに通{つう}報{ほう}してるはずよ",
              "maleVariant": "462D2Z^その気{き}があればとっくに通{つう}報{ほう}してるはずだ",
              "stringId": "2004164679318708224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A42D2Z^つまりあんたは、あたしたちに興{きょう}味{み}がある",
              "maleVariant": "A42D2Z^つまりあんたは、俺{おれ}たちに興{きょう}味{み}がある",
              "stringId": "2004165303666024448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6D2D2Z^金{きん}よ。エディーは大{だい}好{す}きでしょ、ワカコ",
              "maleVariant": "6D2D2Z^金{きん}さ。エディーは大{だい}好{す}きだろ、ワカコ",
              "stringId": "2004166166031704064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2D2D2Z^たんまり儲{もう}かる仕{し}事{ごと}の話{はなし}がある。単{たん}純{じゅん}でしょ",
              "maleVariant": "2D2D2Z^たんまり儲{もう}かる仕{し}事{ごと}の話{はなし}がある。単{たん}純{じゅん}だろ",
              "stringId": "2004168505715138560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E52D2Z^あんたもグルだと思{おも}われるのがオチよ",
              "maleVariant": "E52D2Z^あんたもグルだと思{おも}われるのがオチだ",
              "stringId": "2004179920530251776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4C2D2Z^冷{れい}静{せい}になって",
              "maleVariant": "4C2D2Z^冷{れい}静{せい}になれよ",
              "stringId": "2004180476309086208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なんやて？",
              "maleVariant": "",
              "stringId": "2004183841835393024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8D2D2Z^日{に}本{ほん}人{じん}の逃{とう}亡{ぼう}犯{はん}がナイトシティに潜伏{せんぷく}してるってだけで、あんたの関{かん}与{よ}が疑{うたが}われるのは間{ま}違{ちが}いない",
              "maleVariant": "",
              "stringId": "2004183977797951488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "72D2Z^通{つう}報{ほう}なんてしたら、あたしたちごとあんたも消{け}されることくらい、考{かんが}えなくてもわかるでしょ",
              "maleVariant": "72D2Z^通{つう}報{ほう}なんてしたら、俺{おれ}たちごとあんたも消{け}されることくらい、考{かんが}えなくてもわかるだろ",
              "stringId": "2004184860313067520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C62D2Z^あんたはこの界{かい}隈{わい}を熟{じゅく}知{ち}してる",
              "maleVariant": "",
              "stringId": "2004242184151048192"
            },
            {
