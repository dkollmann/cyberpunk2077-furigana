          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5E4EAZ^V、よく来{き}てくれたね",
              "maleVariant": "",
              "stringId": "1701015549985595392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "344EAZ^お悔{く}やみを… 彼{かれ}はとてもいい子{こ}だった",
              "maleVariant": "",
              "stringId": "1701036886678478848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8E4EAZ^ボクシングクラブのナイトシティ・デビルズには、こういう言{い}い回{まわ}しがあるんだ",
              "maleVariant": "",
              "stringId": "1701194367459303424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4D4EAZ^パドレ、どうぞあの子{こ}に乾杯{かんぱい}を。飲{の}んでいって",
              "maleVariant": "",
              "stringId": "1701228292315455488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "774EAZ^あたしはよく… 初{はじ}めてジャッキーに会{あ}った時{とき}のことを思{おも}い出{だ}す",
              "maleVariant": "774EAZ^えー、俺{おれ}はよく… 初{はじ}めてジャッキーに会{あ}った時{とき}のことを思{おも}い出{だ}す",
              "stringId": "1709616265364938752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A34EAZ^大{おお}昔{むかし}の… いえ、それどころか、まるで別世界{べっせかい}で起{お}きたことのように感{かん}じるけど、ジャッキーは、かつてあたしの命{いのち}を救{すく}ってくれた",
              "maleVariant": "A34EAZ^大{おお}昔{むかし}の… いや、それどころか、まるで別世界{べっせかい}で起{お}きたことのように感{かん}じるけど、ジャッキーは、かつて俺{おれ}の命{いのち}を救{すく}ってくれた",
              "stringId": "1709617872420904960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C94EAZ^あいつはアラサカの盗{とう}品{ひん}を密{みつ}輸{ゆ}しようとしてたの。超{ちょう}極{ごく}秘{ひ}の高{こう}級{きゅう}品{ひん}… トカゲをね",
              "maleVariant": "C94EAZ^あいつはアラサカの盗{とう}品{ひん}を密{みつ}輸{ゆ}しようとしてたんだ。超{ちょう}極{ごく}秘{ひ}の高{こう}級{きゅう}品{ひん}… トカゲをな",
              "stringId": "1709618813656276992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2A4EAZ^今{こん}日{にち}は、来{き}てくれてありがとう",
              "maleVariant": "",
              "stringId": "1709710379691450368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2A4EA008^代{か}わりに、ある出{で}来{き}事{ごと}を紹{しょう}介{かい}するわ",
              "maleVariant": "",
              "stringId": "1709710379691450376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4D4EA004^[飲{の}む]",
              "maleVariant": "",
              "stringId": "1725623255585169412"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E54EAZ^ジャッキーがどんな人{にん}間{げん}だったかとか、あの子{こ}を失{う}ってどれだけ寂{さび}しいかとか、そんな話{はなし}はしない",
              "maleVariant": "",
              "stringId": "1725638389305090048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E54EAZ^「自{じ}分{ぶん}をノックアウトできるファイターを一{いち}番{ばん}の親{しん}友{ゆう}にしろ」",
              "maleVariant": "",
              "stringId": "1725639810939265024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Me mandó Campo Orta.\" t=\"664EAZ^カンポ・オルタの代{だい}理{り}で来{き}た\" b=\"\" a=\"\" />",
              "maleVariant": "",
              "stringId": "1725640208240517120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D64EAZ^[炎{ほのお}に酒{さけ}を注{そそ}ぐ]",
              "maleVariant": "",
              "stringId": "1741399012064272384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9F4EA008^来{き}てくれて嬉{うれ}しかった。いいオフレンダができたわ",
              "maleVariant": "",
              "stringId": "1742986048802037768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"De acuerdo.\" t=\"ああ。そうさせてもらおう\" b=\"\" a=\"\"/>",
              "maleVariant": "",
              "stringId": "1856038944331583488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B82CDZ^もうすぐ始{はじ}まるわ",
              "maleVariant": "",
              "stringId": "1856039595723771904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3D2CDZ^あれは… ジャッキーが19の頃{ごろ}だったかしら",
              "maleVariant": "",
              "stringId": "1856156412475658240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DB2CDZ^あの子{こ}はヴァレンティーノズの一{いち}員{いん}で、当{とう}時{じ}メイルストローム相{あい}手{て}に日々{ひび}戦{たたか}ってた",
              "maleVariant": "",
              "stringId": "1856158365041610752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B22CDZ^そんな時{とき}に… 病{びょう}院{いん}から連{れん}絡{らく}があってね。あの子{こ}が… 三{さん}発{はつ}も撃{う}たれたって言{い}うの。それも、心{しん}臓{ぞう}の近{ちか}くに",
              "maleVariant": "",
              "stringId": "1856158506087665664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4A2CDZ^私{わたし}が病{びょう}院{いん}に行{い}くと、あの子{こ}は意{い}識{しき}を取{と}り戻{もど}してた。体{からだ}中{じゅう}から色{いろ}んな管{かん}が伸{の}びてて、まるでタコみたいだったのを覚{おぼ}えてる",
              "maleVariant": "",
              "stringId": "1856158611717017600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1A2CDZ^そしてこんなことを言{い}ったの。「心{しん}配{ぱい}すんな、オフクロ。俺{おれ}は防{ぼう}弾{だん}仕{し}様{よう}なんだ。だから絶{ぜっ}対{たい}死{し}なないんだ」って",
              "maleVariant": "",
              "stringId": "1856158787005370368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DB2CDZ^それから私{わたし}の帰{かえ}り際{ぎわ}に、ヴァレンティーノズの指{ゆび}輪{わ}を外{はず}したの。もう必{ひつ}要{よう}ないからって",
              "maleVariant": "",
              "stringId": "1856158884732653568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "532CDZ^あの子{こ}は怖気{おじけ}づいたんじゃない。ただ私{わたし}を安{あん}心{しん}させたかったの",
              "maleVariant": "",
              "stringId": "1856158994120101888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Descansa en paz, mi hijo.\" t=\"622CDZ^我{わ}が子{こ}よ、どうか安{やす}らかに\" b=\"\" a=\"\" />",
              "maleVariant": "",
              "stringId": "1856159204825157632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "602CDZ^ジャッキーの腕{うで}はまるで大{たい}砲{ほう}だった。あんな強{きょう}烈{れつ}なのは食{く}らったことがない… 今{いま}でも感{かん}触{しょく}を覚{おぼ}えてる",
              "maleVariant": "",
              "stringId": "1856161373750087680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "712CDZ^あの12ラウンドを思{おも}い出{だ}すぜ",
              "maleVariant": "",
              "stringId": "1856161567308828672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"Lamenta no estar aquí, todavía le quedan cuatro años que cumplir. Me pidió que leyera esto.\" t=\"482CDZ^彼{かれ}は今日{きょう}来{こ}れないことを残{ざん}念{ねん}に思{おも}ってる。刑{けい}期{き}がまだ四{よ}年{ねん}あるからな。だから手紙{てがみ}を読{よ}む\" b=\"\" a=\"\" />",
              "maleVariant": "",
              "stringId": "1856163297492783104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"«Recuerdo cuando Jackie se nos unió. Era un niño fuerte».\" t=\"E92CDZ^「俺{おれ}たちのもとへ初{はじ}めてやって来{き}た時{とき}、ジャッキーはタフなガキだった」\" b=\"\" a=\"\" />",
              "maleVariant": "",
              "stringId": "1856163459107704832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "<kiroshi l=\"mex\" o=\"«Cuando decía que iba a hacer algo, cumplía. Tenía honor. Podría haber llegado lejos».\" t=\"812CDZ^「何{なに}かをやると言{い}ったら、必{かなら}ずそれをやり遂{と}げる男{おとこ}だ。人{にん}を敬{たかし}い、大{たい}成{せい}する資{し}質{しつ}を持{も}っていた」\" b=\"\" a=\"\" />",
              "maleVariant": "",
