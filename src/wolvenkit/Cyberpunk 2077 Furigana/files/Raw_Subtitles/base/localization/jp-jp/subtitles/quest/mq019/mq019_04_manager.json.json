            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "334EAZ^シャンパンをご注{ちゅう}文{もん}されました？",
              "maleVariant": "",
              "stringId": "1944525214101446656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C04EAZ^ああ。できれば静{しず}かに味{あじ}わいたいんだが",
              "maleVariant": "",
              "stringId": "1944526277323956224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "794EAZ^そう。じゃあ払{はら}って",
              "maleVariant": "794EAZ^そうか。じゃあ払{はら}え",
              "stringId": "1944532327741693952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "84EAZ^じゃあ払{はら}って。それなりの額{ひたい}なんでしょうね",
              "maleVariant": "84EAZ^じゃあ払{はら}え。それなりの額{ひたい}なんだろうな",
              "stringId": "1944532428925083648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
