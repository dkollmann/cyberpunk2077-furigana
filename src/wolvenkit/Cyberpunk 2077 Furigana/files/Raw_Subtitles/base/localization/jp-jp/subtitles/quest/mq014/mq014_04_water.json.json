          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B25F9Z^今{いま}からそなたに教{おし}えるのは水{みず}の瞑{めい}想{そう}だ",
              "maleVariant": "",
              "stringId": "1815549813645611008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E15F9Z^ここは森{もり}の中{なか}、目{め}の前{まえ}には滝{たき}がある",
              "maleVariant": "",
              "stringId": "1816922335658086400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2A5F9Z^若{わか}さ、知{ち}恵{え}、知{ち}識{しき}、健{けん}康{こう}、強{つよ}さ、喜{よろこ}び、幸{しあわ}せ、そして内{ない}なる安{やす}らぎを与{あた}えたまえ",
              "maleVariant": "",
              "stringId": "1822695898210078720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AB5F9Z^水{みず}の精{せい}霊{れい}よ、我々{われわれ}に宿{やど}りたまえ。我々{われわれ}の血{ち}と肉{にく}になりたまえ",
              "maleVariant": "",
              "stringId": "1822699929053663232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FB5F9Z^我々{われわれ}の身{しん}体{たい}と魂{たましい}から病{びょう}を、苦{く}痛{つう}を、穢{けが}れを取{と}り除{のぞ}き、害{がい}ある人々{ひとびと}、社{しゃ}会{かい}、次{じ}元{げん}から害{がい}ある気{き}を取{と}り除{のぞ}きたまえ",
              "maleVariant": "",
              "stringId": "1822701562483412992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "395F9Z^水{みず}の精{せい}霊{れい}よ、心{こころ}と身{しん}体{たい}と魂{たましい}からお礼{れい}を申{もう}し上{あ}げる。そなたなしで命{いのち}は存{そん}在{ざい}しえない",
              "maleVariant": "",
              "stringId": "1822702594315751424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A35F9Z^水{みず}の精{せい}霊{れい}よ、我々{われわれ}の血{ち}と肉{にく}の8割{わり}は、そなたが司{つかさど}る水{みず}より創{つく}られている",
              "maleVariant": "",
              "stringId": "1925545639536463872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B85F9Z^液{えき}体{たい}の時{とき}もあれば、氷{こおり}の時{とき}もあり、雪{ゆき}の時{とき}もあれば、蒸{じょう}気{き}や霧{きり}の時{とき}もある。癒{いや}しを与{あた}える時{とき}もあれば、溺{おぼ}れさせる時{とき}もある",
              "maleVariant": "",
              "stringId": "1925547349285769216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "965F9Z^水{みず}の精{せい}霊{れい}よ、そなたは、何{なに}よりも謎{なぞ}めいた存{そん}在{ざい}だ！",
              "maleVariant": "",
              "stringId": "1925547988665470976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7A5F9Z^我々{われわれ}は水{みず}。滝{たき}から流{なが}れ落{お}ちるのは我々{われわれ}自{じ}身{しん}だ",
              "maleVariant": "",
              "stringId": "1925549289570799616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "275F9Z^そなたら四{よん}大{だい}元{げん}素{そ}が形{かた}作{づく}る美{び}に、敬{けい}意{い}を表{あらわ}さん…",
              "maleVariant": "",
              "stringId": "1925614429947269120"
            }
          ]
        }
