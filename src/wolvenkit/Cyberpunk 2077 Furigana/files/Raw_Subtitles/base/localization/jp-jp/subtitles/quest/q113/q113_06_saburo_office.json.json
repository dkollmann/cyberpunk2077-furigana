        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "69404Z^忠{ちゅう}誠{せい}心{しん}のある、全{すべ}ての兵{へい}を招{しょう}集{しゅう}するの",
              "maleVariant": "",
              "stringId": 1753439384113266688
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2E404Z^準{じゅん}備{び}の上{うえ}、待{たい}機{き}します",
              "maleVariant": "",
              "stringId": 1753439567807004672
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8C4E1Z^防{ぼう}諜{ちょう}にいたことがあるけど、こんな場{ば}所{しょ}、聞{き}いたことがない",
              "maleVariant": "8C4E1Z^防{ぼう}諜{ちょう}にいたことがあるが、こんな場{ば}所{しょ}、聞{き}いたことがない",
              "stringId": 1821289547171762176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もちろん、そうでしょうね",
              "maleVariant": "",
              "stringId": 1821321276662091776
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D64E1Z^ここは我{わ}がアラサカ社{しゃ}の中{なか}でも、最{さい}高{こう}機{き}密{みつ}に分{ぶん}類{るい}される場{ば}所{しょ}なの",
              "maleVariant": "",
              "stringId": 1821323272496484352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "354E1Z^父{ちち}のオフィス。東{とう}京{きょう}本{ほん}社{しゃ}にあるオフィスの精{せい}密{みつ}なコピーよ",
              "maleVariant": "",
              "stringId": 1821324270522732544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "444E1Z^父{ちち}は世{せ}界{かい}中{じゅう}の支{し}社{しゃ}に、この部屋{へや}を作{つく}らせたの",
              "maleVariant": "",
              "stringId": 1821324549947265024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "964E1Z^小{こ}物{もの}を含{ふく}め隅々{すみずみ}まで再{さい}現{げん}されている。目{め}を閉{と}じていても仕{し}事{ごと}ができるくらいにね",
              "maleVariant": "",
              "stringId": 1821327312986968064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F74E1Z^父{ちち}はそういう、細{さい}部{ぶ}にこだわる人{にん}なのよ",
              "maleVariant": "",
              "stringId": 1821329848645062656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D4E1Z^どこへ行{い}くの？",
              "maleVariant": "D4E1Z^どこへ行{い}くんだ？",
              "stringId": 1821433581064294400
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "こっちよ",
              "maleVariant": "",
              "stringId": 1821449271318024192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "“だった”",
              "maleVariant": "",
              "stringId": 1822868108110336000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "764E1Z^今{いま}なんて？",
              "maleVariant": "",
              "stringId": 1822876060477165568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "サブロウはもういない",
              "maleVariant": "",
              "stringId": 1822876116865388544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BD4E1Z^タケムラ、戦{せん}力{りょく}を集{あつ}めて",
              "maleVariant": "",
              "stringId": 1822885497711497216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "804E1Z^で、見{み}込{こ}みはあるの…？",
              "maleVariant": "804E1Z^で、見{み}込{こ}みはあるのか…？",
              "stringId": 1841690843489898496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AC4E1Z^V、あなたは私{わたし}と一{いっ}緒{しょ}に",
              "maleVariant": "",
              "stringId": 1842859818361950208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CA4E1Z^象{しょう}徴{ちょう}は、そう簡{かん}単{たん}に消{き}え去{さ}らないものよ",
              "maleVariant": "",
              "stringId": 1842861158895063040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1E4E1Z^あなたが思{おも}っている以{い}上{じょう}にね",
              "maleVariant": "",
              "stringId": 1842997280702861312
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8D4E1Z^問{もん}題{だい}なのは、各{かく}派{は}閥{ばつ}が今{いま}もアラサカの理{り}念{ねん}に忠{ちゅう}実{じつ}かどうか",
              "maleVariant": "",
              "stringId": 1844343836416806912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FE4E1Z^そして、過{あやま}ちを認{みと}める勇{ゆう}気{き}があるかどうか",
              "maleVariant": "",
              "stringId": 1844346672991047680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "954E1004^あたしの証{しょう}言{げん}に耳{みみ}を貸{か}すとは思{おも}えない",
              "maleVariant": "954E1004^俺{おれ}の証{しょう}言{げん}に耳{みみ}を貸{か}すとは思{おも}えない",
              "stringId": 1844347581762506756
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3E4E1Z^見{み}込{こ}みはあるの…？",
              "maleVariant": "3E4E1Z^見{み}込{こ}みはあるのか…？",
              "stringId": 1844347722036809728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2A4E1Z^正{しょう}直{じき}言{い}って、望{のぞ}み薄{はく}ね",
              "maleVariant": "2A4E1Z^正{しょう}直{じき}言{い}って、望{のぞ}み薄{はく}だな",
              "stringId": 1844350650868961280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3F4E1Z^そうする他{ほか}にない。アラサカの未{み}来{らい}が懸{か}かってるんだから",
              "maleVariant": "3F4E1Z^そうする他{ほか}にない。アラサカの未{み}来{らい}が懸{か}かってるからな",
              "stringId": 1844350926099189760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1E4E1Z^そうする他{ほか}ないでしょ。アラサカ社{しゃ}の未{み}来{らい}が懸{か}かってるんだから",
              "maleVariant": "1E4E1Z^そうする他{ほか}ないだろ。アラサカ社{しゃ}の未{み}来{らい}が懸{か}かってるんだから",
              "stringId": 1844351183243579392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "974E1Z^ヨリノブが亀{き}裂{れつ}を生{う}み、内{ない}部{ぶ}は分{ぶん}裂{れつ}状{じょう}態{たい}に陥{おち}ってる。ゴロウはそう言{い}ってた",
              "maleVariant": "",
              "stringId": 1844373897060683776
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "564E1Z^亀{き}裂{れつ}… そのとおりね",
              "maleVariant": "",
              "stringId": 1844376056338714624
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C64E1Z^誰{だれ}もそれを塞{ふさ}ごうとはしなかった",
              "maleVariant": "",
              "stringId": 1844376513484296192
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたのせいじゃない",
              "maleVariant": "",
              "stringId": 1844378741246922752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "364E1Z^残{ざん}念{ねん}ながら、そうは思{おも}わない人{にん}もいるわ",
              "maleVariant": "",
              "stringId": 1844379212602806272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D44E1Z^あたしの証{しょう}言{げん}なんて屁{へ}とも思{おも}わないだろうし、あんたの影{えい}響{きょう}力{りょく}もたかが知{し}れてる",
              "maleVariant": "D44E1Z^俺{おれ}の証{しょう}言{げん}なんて屁{へ}とも思{おも}わないだろうし、あんたの影{えい}響{きょう}力{りょく}もたかが知{し}れてる",
              "stringId": 1844386383554023424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じきにわかるわ",
              "maleVariant": "",
              "stringId": 1844387415688351744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ…",
              "maleVariant": "",
              "stringId": 1844402972579991556
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ゆっくりとだ…",
              "maleVariant": "",
              "stringId": 1844402978619789316
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ありがとう",
              "maleVariant": "",
              "stringId": 1844402981555802112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "54E1Z^踏{ふ}ん張{ば}れ。あともう少{すこ}しだ",
              "maleVariant": "",
              "stringId": 1844402984424706048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6E4E1004^彼{かの}女{じょ}は大{だい}丈{じょう}夫{ぶ}？",
              "maleVariant": "6E4E1004^彼{かれ}は大{だい}丈{じょう}夫{ぶ}？",
              "stringId": 1844402990481281028
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おそらくは",
              "maleVariant": "",
              "stringId": 1844402993903833088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EE4E1Z^平{へい}気{き}よ",
              "maleVariant": "EE4E1Z^平{へい}気{き}だ",
              "stringId": 1844402996923731968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これってもしかして…",
              "maleVariant": "",
              "stringId": 1863349118742372352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9D514Z^[立{た}ち上{あ}がる]",
              "maleVariant": "",
              "stringId": 1895369521335713792
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3A514004^ほら、掴{つか}め",
              "maleVariant": "",
              "stringId": 1913787885628243972
