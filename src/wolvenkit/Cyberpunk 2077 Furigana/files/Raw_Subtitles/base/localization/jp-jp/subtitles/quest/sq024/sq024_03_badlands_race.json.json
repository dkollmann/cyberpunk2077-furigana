          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7E502Z^さあいくぞ！ パンツを履{は}いてベルトを締{し}めたら、エンジンをかけろ！",
              "maleVariant": "",
              "stringId": "1395316292542537728"
