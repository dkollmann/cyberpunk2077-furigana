        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、そうだ… ラジオもあるから",
              "maleVariant": "",
              "stringId": "1375164841633607680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "112D7Z^よし、降{お}りて",
              "maleVariant": "",
              "stringId": "1376237517701738496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5B2D7Z^よし、作{さく}戦{せん}を確{かく}認{にん}するよ",
              "maleVariant": "",
              "stringId": "1376253101084602368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C42D7Z^ちょっと… あれ見{み}て！",
              "maleVariant": "",
              "stringId": "1376328672795783168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "444E6Z^頑{がん}張{ば}ってね",
              "maleVariant": "",
              "stringId": "1700875168543301632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8623Z^車{くるま}を隠{かく}さないと。岩{いわ}場{ば}のそばに停{と}めて",
              "maleVariant": "",
              "stringId": "1764683611163996160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A2623Z^V、時{じ}間{かん}がない。嵐{あらし}が迫{せま}ってきてる",
              "maleVariant": "",
              "stringId": "1764770380677001216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8E623Z^いい場{ば}所{しょ}があるんだ。そこからなら、完璧{かんぺき}に援{えん}護{ご}できる",
              "maleVariant": "",
              "stringId": "1776252189428297728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、バッチリよ",
              "maleVariant": "ああ、バッチリだ",
              "stringId": "1776255147251478532"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V、こっち",
              "maleVariant": "",
              "stringId": "1776260259839619072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "43623Z^一{いっ}緒{しょ}に乗{の}ってくれば良{よ}かったのに！",
              "maleVariant": "",
              "stringId": "1783732414881869824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "623Z^ちょっと、何{なに}考{かんが}えてるの！？",
              "maleVariant": "",
              "stringId": "1822633049249165312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "90623Z^私{わたし}が援{えん}護{ご}する！ あんたはとにかくソウルを探{さが}して",
              "maleVariant": "",
              "stringId": "1822836259452760064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "79623Z^じゃ、後{あと}でね",
              "maleVariant": "",
              "stringId": "1822836525354856448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "72EFZ^私{わたし}は離{はな}れた場{ば}所{しょ}から援{えん}護{ご}する",
              "maleVariant": "",
              "stringId": "1847678184782229504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ、バッチリよ。そっちは？",
              "maleVariant": "ああ、バッチリだ。そっちは？",
              "stringId": "1847679575848316928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C2D2Z^バカでっかい嵐{あらし}だ！",
              "maleVariant": "",
              "stringId": "1886342008745566208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2F2D2Z^ペトロケムのタンクローリーを奪{うば}ったときは、そのまま燃{ねん}料{りょう}をばらまいた後{のち}、火柱{ひばしら}をあげて子{こ}供{ども}たちを喜{よろこ}ばせてた",
              "maleVariant": "",
              "stringId": "1886495717622358016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3C2D2Z^昔{むかし}はすごかったんだ",
              "maleVariant": "",
              "stringId": "1886496439394967552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "512D2Z^爆{ばく}撃{げき}のなか、みんなを引{ひ}き連{つ}れて州境{しゅうざかい}を越{こ}えたり…",
              "maleVariant": "",
              "stringId": "1886497324510552064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "542D2Z^憧{あこが}れの人{にん}か…",
              "maleVariant": "",
              "stringId": "1886498802029633536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "792D2Z^やるなら今{いま}しかないよ",
              "maleVariant": "",
              "stringId": "1886515716231602176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ヤバそうね…",
              "maleVariant": "ヤバそうだな…",
              "stringId": "1886519217166819328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "302D2Z^あれに巻{ま}き込{こ}まれたらおしまいだ。急{いそ}がないと",
              "maleVariant": "",
              "stringId": "1886519378613968896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C2D2Z^よりによってソウルが捕{つか}まるとは…",
              "maleVariant": "",
              "stringId": "1893697359283298304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "822D2Z^他{ほか}の人{にん}だったら、まだわかるけど",
              "maleVariant": "",
              "stringId": "1893697941083594752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3A2D2Z^今{いま}は鈍{にぶ}ってるって、言{い}ってなかったっけ",
              "maleVariant": "3A2D2Z^今{いま}は鈍{にぶ}ってるって、言{い}ってなかったか",
              "stringId": "1893700340762353664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうだけどさ…",
              "maleVariant": "",
              "stringId": "1893700992943071232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "352D2Z^聴{き}きたければ、どうぞ",
              "maleVariant": "",
              "stringId": "1893707732317184000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "182D2Z^くれぐれも慎{しん}重{ちょう}にね",
              "maleVariant": "",
              "stringId": "1893714389029953536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8B2D2Z^万{まん}が一{いち}騒{さわ}ぎが起{お}きても、私{わたし}がバックアップしてあげるから",
              "maleVariant": "",
              "stringId": "1893755141609037824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "442D2Z^リンクは常{じょう}時{じ}オン、あんたがソウルを見{み}つけたら私{わたし}が車{くるま}を出{だ}して、脱出{だっしゅつ}する",
              "maleVariant": "",
              "stringId": "1893756179799941120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DB2D2Z^あんたはバレないようにレイスのキャンプに侵{しん}入{にゅう}する…",
              "maleVariant": "",
              "stringId": "1893758231032700928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7D2D2Z^準{じゅん}備{び}はいい？",
              "maleVariant": "",
              "stringId": "1895149296348372992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8B2D2Z^嵐{あらし}がこっちに迫{せま}ってる",
              "maleVariant": "",
              "stringId": "1895152071132127232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "222D2Z^なかなかの嵐{あらし}が近{ちか}づいてるようね",
              "maleVariant": "222D2Z^なかなかの嵐{あらし}が近{ちか}づいてるようだ",
              "stringId": "1895152305593720832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C2D2Z^なかなかどころか、あんなの食{く}らったらひとたまりもないよ！ 早{はや}く片{かた}付{づ}けなきゃ",
              "maleVariant": "",
              "stringId": "1895152816325730304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E52D2Z^さ、降{お}りて",
              "maleVariant": "",
              "stringId": "1895166198789513216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "152D2Z^私{わたし}はあそこから援{えん}護{ご}する。あんたはソウルを探{さが}して",
              "maleVariant": "",
              "stringId": "1895166775120437248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "562D2Z^クソ、もう引{ひ}き返{かえ}せない",
              "maleVariant": "",
              "stringId": "1895170199299891200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "862D2Z^じゃ、後{あと}でね",
              "maleVariant": "",
              "stringId": "1895171037623820288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってる。ただ…",
              "maleVariant": "",
              "stringId": "1895181027650641920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1F57BZ^待{ま}って。作{さく}戦{せん}をおさらいしたい",
              "maleVariant": "1F57BZ^待{ま}ってくれ。作{さく}戦{せん}をおさらいしたい",
              "stringId": "1957602384160468992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BF45AZ^もう一{いち}度{ど}説{せつ}明{めい}して",
              "maleVariant": "BF45AZ^もう一{いち}度{ど}説{せつ}明{めい}してくれるか",
              "stringId": "2277749476720746496"
            }
          ]
        }
      }
    },
