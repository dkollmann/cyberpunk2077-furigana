        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、もういい。こんなことしたってあいつは…",
              "maleVariant": "",
              "stringId": 1790637139848765444
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D44EA004^よう、V！ 砂{さ}漠{ばく}での暮{く}らしはどうだ？ 太{たい}陽{よう}の光{ひか}と新{しん}鮮{せん}な空{くう}気{き}を満{まん}喫{きつ}してるか？",
              "maleVariant": "",
              "stringId": 1790637139882319876
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D84EA004^よう、V！ 元{げん}気{き}か？ 最{さい}近{きん}はめっきり顔{かお}を出{だ}さなくなったじゃないか",
              "maleVariant": "",
              "stringId": 1790637139949428740
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DC4EA004^V。お前{まえ}さんが自{じ}由{ゆう}人{じん}なのはわかってる。好{す}きな時{とき}に好{す}きなことをやりゃいい",
              "maleVariant": "",
              "stringId": 1790637140016537604
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DE4EA004^よう、V。外{そと}から見{み}る地{ち}球{きゅう}はどうだ？ 噂{うわさ}どおりの美{うつく}しさか？ それともやっぱりありゃウソなのか？",
              "maleVariant": "",
              "stringId": 1790637140050092036
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やるのよ… あなたのためでもあるの",
              "maleVariant": "",
              "stringId": 1790842572898619392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B644DZ^…よう、V。死{し}後{ご}の世{せ}界{かい}はどうだ？ 噂{うわさ}どおり、きれいなとこか？",
              "maleVariant": "",
              "stringId": 1790843297808568320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1E44DZ^ナイトシティのほうは相{あい}変{か}わらずだよ。何{なに}なら、前{まえ}よりひどくなってるような気{き}もするな",
              "maleVariant": "",
              "stringId": 1790843733345095680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8F44DZ^俺{おれ}がもっと力{ちから}になってやれれば… そしたら、こんなことには",
              "maleVariant": "",
              "stringId": 1790843937104384000
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ポジティブにね、ヴィクター",
              "maleVariant": "",
              "stringId": 1790844813143494656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ED44DZ^俺{おれ}は… お前{まえ}さんがいなくて寂{さび}しいよ",
              "maleVariant": "",
              "stringId": 1790845158452154368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CE44DZ^定{てい}期{き}的{てき}に点{てん}検{けん}してもらうのを忘{わす}れるなよ。絶{ぜつ}縁{えん}体{たい}に土{つち}やほこりが入{い}りこんだらシャレにならないからな",
              "maleVariant": "",
              "stringId": 1790847472919433216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B644DZ^とはいえ、そっちでまともなメンテができる腕{うで}の良{よ}いリパーを見{み}つけるのは至{し}難{なん}の業{ぎょう}か…",
              "maleVariant": "",
              "stringId": 1790848030862528512
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E544DZ^街{まち}に来{く}ることがあれば、俺{おれ}んとこに来{こ}な。なんやかんやで、お前{まえ}さんはお得{とく}意{い}さんだからな――多{た}少{しょう}はまけてやるよ…",
              "maleVariant": "",
              "stringId": 1790848289349095424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあまたな、V",
              "maleVariant": "",
              "stringId": 1790848568538746880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C44DZ^アフターライフの奴{やつ}らに“メジャーリーグ”ご用{よう}達{たつ}のリパーでも紹{しょう}介{かい}してもらったか？ きっとケツからクロームを垂{た}れ流{なが}すような奴{やつ}なんだろうな",
              "maleVariant": "",
              "stringId": 1790850364472610816
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "544DZ^それでいいんだよ、V… 人{じん}生{せい}を目一杯{めいっぱい}楽{たの}しめ。お前{まえ}さんには誰{だれ}よりもその権{けん}利{り}がある",
              "maleVariant": "",
              "stringId": 1790850574808567808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "644DZ^だが、今{こん}度{ど}近{ちか}くまで来{く}ることがあったら、立{た}ち寄{よ}ってくれ。たまには思{おも}い出話{でばなし}をしようじゃないか… ついでに新{あたら}しいリパーがヤブじゃないか見{み}てやるよ",
              "maleVariant": "",
              "stringId": 1790850969962336256
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあまたな、V",
              "maleVariant": "",
              "stringId": 1790851339446964224
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6E44DZ^だがせめて生{い}きてるかどうかぐらい、教{おし}えてくれ",
              "maleVariant": "",
              "stringId": 1790852290262126592
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1544DZ^説教{せっきょう}するつもりはないが――定{てい}期{き}点{てん}検{けん}もすっぽかしやがって。何{なん}度{ど}も口{くち}をすっぱくして… ったく…",
              "maleVariant": "",
              "stringId": 1790852731150585856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C44DZ^一{いち}度{ど}、連{れん}絡{らく}してくれ。じゃあな",
              "maleVariant": "",
              "stringId": 1790853181971156992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6E44DZ^いいか… お前{まえ}さんのインプラントのことだが――どれも宇{う}宙{ちゅう}での使{し}用{よう}は想{そう}定{てい}されてない",
              "maleVariant": "",
              "stringId": 1790855627451715584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4444DZ^もしエラーが出{で}たら絶{ぜっ}対{たい}に無{む}視{し}するなよ。いいな？",
              "maleVariant": "",
              "stringId": 1790855888740077568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EB44DZ^戻{もど}ったら… もし戻{もど}ってきたら… 一{ひと}通{とお}り点{てん}検{けん}してやる。何{なに}なら特{とく}別{べつ}に割引{わりびき}してやってもいい",
              "maleVariant": "",
              "stringId": 1790856248024158208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4344DZ^元{げん}気{き}でな、V",
              "maleVariant": "",
              "stringId": 1790856425594212352
            }
          ]
        }
      }
