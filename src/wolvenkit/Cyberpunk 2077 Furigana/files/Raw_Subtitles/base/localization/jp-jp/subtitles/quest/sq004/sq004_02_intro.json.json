            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F4623Z^よし。ミッチ、バンの準{じゅん}備{び}は？",
              "maleVariant": "",
              "stringId": 1776193869778661376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
