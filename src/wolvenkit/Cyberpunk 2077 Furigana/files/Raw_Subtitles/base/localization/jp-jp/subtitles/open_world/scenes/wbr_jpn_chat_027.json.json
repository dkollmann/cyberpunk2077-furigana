            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6670AZ^目{め}を入{い}れ替{か}えようとも、ものの見{み}方{かた}を知{し}らず",
              "maleVariant": "",
              "stringId": "2160361409076043776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EC559Z^脚{あし}を入{い}れ替{か}えようとも、走{はし}り向{む}かう場{ば}所{しょ}を持{も}たぬ",
              "maleVariant": "",
              "stringId": "2179730189113987072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DC559Z^心{しん}臓{ぞう}を入{い}れ替{か}えようとも、何{なに}も感{かん}じることはできず",
              "maleVariant": "",
              "stringId": "2179730455133523968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7D559Z^あらゆる問{もん}題{だい}に対{たい}する答{こた}えを、硬{こう}貨{か}に求{もと}めるようになる",
              "maleVariant": "",
              "stringId": "2179733249563398144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B559Z^だが己{おのれ}の内{ない}にある空{くう}白{はく}は、何{なん}十{じゅう}億{おく}のユーロドルでも埋{う}めることはできぬ。よいか、その道{みち}は間{ま}違{ちが}っている",
              "maleVariant": "",
              "stringId": "2179744788227919872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2B559Z^肉{にく}体{たい}の卑{いや}しさに屈{く}してはならぬ。表{ひょう}層{そう}を捨{す}て心{こころ}の奥{おく}を見{み}つめるのだ。言葉{ことば}を交{まじ}わし、互{たが}いに愛{いと}し合{あ}うのだ",
              "maleVariant": "",
              "stringId": "2179745037872893952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2B559Z^恐{おそ}れるべきは敵{てき}対{たい}者{しゃ}ではなく、快{かい}楽{らく}の源{げん}だ。己{おのれ}を満{み}たすのはそばにいる者{もの}たちだ",
              "maleVariant": "",
              "stringId": "2179745209671585792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A559Z^その者{もの}との交{こう}流{りゅう}は幸{こう}福{ふく}を意{い}味{み}する。幸{こう}運{うん}にも、私{わたし}たちは他{た}者{しゃ}を思{おも}いやる気{き}持{も}ちを持{も}ち合{あ}わせているのだから",
              "maleVariant": "",
              "stringId": "2179754241434161152"
            }
          ]
        }
