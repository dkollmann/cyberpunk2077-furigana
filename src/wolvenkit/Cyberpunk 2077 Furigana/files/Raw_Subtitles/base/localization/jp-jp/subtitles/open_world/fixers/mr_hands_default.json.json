        "entries": [
          {
            "stringId": 1948875546946367492,
            "femaleVariant": "いつ会{あ}える？"
          },
          {
            "stringId": 1948877390007431168,
            "femaleVariant": "あんたの噂{うわさ}は聞{き}いてる"
          },
          {
            "stringId": 1948877911728517120,
            "femaleVariant": "ネットウォッチがパシフィカで問題{もんだい}を起{お}こしてるって聞{き}いた"
          },
          {
            "stringId": 1948878544061788160,
            "femaleVariant": "直接{ちょくせつ}会{あ}って話{はなし}をしたいんだけど",
            "maleVariant": "直接{ちょくせつ}会{あ}って話{はなし}をしたいんだが"
          },
          {
            "stringId": 1948878551728975872,
            "femaleVariant": "ネットウォッチがこの辺{へん}をうろついてるって聞{き}いた"
          },
          {
            "stringId": 1948878567499558912,
            "femaleVariant": "噂{うわさ}は聞{き}いてる。パシフィカの“最{サイ}後{ゴ}のコーポ”だって",
            "maleVariant": "噂{うわさ}は聞{き}いてる。パシフィカの“最{サイ}後{ゴ}のコーポ”だと"
          },
          {
            "stringId": 1948881248414789632,
            "femaleVariant": "断{ことわ}る"
          },
          {
            "stringId": 1948881417394909184,
            "femaleVariant": "名{ナ}前{マエ}も顔{かお}も晒{さら}す気{き}はない"
          },
          {
            "stringId": 1948881485460074496,
            "femaleVariant": "ミスター・ハンズとは、そういう男{おとこ}だ。気に入{きにい}らないなら他{ほか}をあたるんだな"
          },
          {
            "stringId": 1948882202920939520,
            "femaleVariant": "私{わたし}はあくまで、需{ジュ}要{ヨウ}と供給{きょうきゅう}のバランスを保{たも}っているだけさ"
          },
          {
            "stringId": 1948882289340379136,
            "femaleVariant": "ところで、その噂{うわさ}はどこで聞{き}いた？"
          },
          {
            "stringId": 1948882630958051328,
            "femaleVariant": "前世{ぜんせ}かな"
          },
          {
            "stringId": 1948882711488688128,
            "femaleVariant": "よくある話{はなし}だ"
          },
          {
            "stringId": 1948887939134976000,
            "femaleVariant": "奴{やつ}らはGIMで地{ジ}獄{ゴク}の門{もん}を開{ひら}いた。ヴードゥー・ボーイズのランナーが次々{つぎつぎ}倒{たお}され、地{チ}区{ク}の電{デン}力{リョク}消{ショウ}費{ヒ}が半{ハン}分{ブン}に落{お}ちたほどだ"
          },
          {
            "stringId": 1948888053807247360,
            "femaleVariant": "大{タイ}変{ヘン}そうね",
            "maleVariant": "大{タイ}変{ヘン}そうだな"
          },
          {
            "stringId": 1948888195423727616,
            "femaleVariant": "大{タイ}変{ヘン}どころじゃない。秩序{ちつじょ}が乱{みだ}されたんだ。秩序{ちつじょ}があってこそ、商売{しょうばい}が安{アン}定{テイ}する"
          },
          {
            "stringId": 1948888289007038464,
            "femaleVariant": "それと、この手{て}の話{はなし}をホロでするな"
          },
          {
            "stringId": 1956136150203559936,
            "femaleVariant": "どうしてパシフィカを拠点{きょてん}にしたの？",
            "maleVariant": "なぜパシフィカを拠点{きょてん}にしたんだ？"
          },
          {
            "stringId": 1956136552286318592,
            "femaleVariant": "どうしてパシフィカで仕事{しごと}を仕切{しき}るようになったの？",
            "maleVariant": "どうしてパシフィカで仕事{しごと}を仕切{しき}るようになったんだ？"
          },
          {
            "stringId": 1956137565042962432,
            "femaleVariant": "パシフィカと私{わたし}は、何{なん}というか似{に}たもの同{ドウ}士{シ}でね"
          },
          {
            "stringId": 1956138252657160192,
            "femaleVariant": "あえて目立{めだ}たないようにしているだけで、裏{うら}では色々{いろいろ}と動{うご}いているのさ"
          },
          {
            "stringId": 1956145242598993920,
            "femaleVariant": "もう行{い}かなきゃ",
            "maleVariant": "もう行{い}かないと"
          },
          {
            "stringId": 1956145349838958592,
            "femaleVariant": "もう行{い}かないと"
          },
          {
            "stringId": 1956145623089475584,
            "femaleVariant": "時{とき}は金{きん}なり"
          },
          {
            "stringId": 1959157768110665728,
            "femaleVariant": "私{わたし}はミスター・ハンズ。パシフィカで仕事{しごと}が欲{ほっ}しければ連{レン}絡{ラク}しろ"
          },
          {
            "stringId": 1959158450356154368,
            "femaleVariant": "Vよ。なに、“初{はじ}めまして”もなし？",
            "maleVariant": "Vだ。なんだ、“初{はじ}めまして”もなしか？"
          },
          {
            "stringId": 1959159171071799296,
            "femaleVariant": "知り合{しりあ}えて大{タイ}変{ヘン}光栄{こうえい}だ"
          },
          {
            "stringId": 1959164695188271104,
            "femaleVariant": "V、パシフィカへようこそ"
          },
          {
            "stringId": 1959168581915570176,
            "femaleVariant": "用{よう}があれば連{レン}絡{ラク}しろ"
          },
          {
            "stringId": 1959172823598428160,
            "femaleVariant": "ずいぶんと情{ジョウ}報{ホウ}が早{はや}いのね",
