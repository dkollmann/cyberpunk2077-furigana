        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F54EA004^自{じ}殺{さつ}か… まあ、当{とう}然{ぜん}といえばそうなのかもしれない",
              "maleVariant": "",
              "stringId": "1790661351166615556"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F84EA004^よう、V… ようやく写{しゃ}真{しん}が届{とど}いたぞ",
              "maleVariant": "",
              "stringId": "1790661351216947204"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FB4EA004^よう、V！ 来{らい}週{しゅう}ランディの誕生日{たんじょうび}にジョスが夕{ゆう}飯{はん}を作{つく}るんだ。あー、何{なん}日{にち}だったかな…",
              "maleVariant": "",
              "stringId": "1790661351267278852"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FD4EA004^よう、V… ちょっと言{い}いにくいんだが… 身{み}元{もと}引{ひき}受{うけ}人{にん}になってくれないか？",
              "maleVariant": "",
              "stringId": "1790661351300833284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もしもし、V",
              "maleVariant": "",
              "stringId": "1790661351317610500"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4EA004^よう、V。実{じつ}はな――",
              "maleVariant": "",
              "stringId": "1790661351351164932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "63BCZ^ナイトシティの半{はん}分{ぶん}の人{にん}間{げん}が選{えら}ぶ道{みち}だ。特{とく}にいい奴{やつ}はな",
              "maleVariant": "",
              "stringId": "1801109546592878592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "173BCZ^俺{おれ}の最{さい}初{しょ}の相{あい}棒{ぼう}、ジェイミー・シーンも… 自{じ}分{ぶん}の頭{あたま}を撃{う}ったんだ",
              "maleVariant": "",
              "stringId": "1801110212598022144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EE3BCZ^遺{い}書{しょ}すら残{のこ}さなかったが、署{しょ}の上{うえ}の連{れん}中{ちゅう}が原{げん}因{いん}なのは明{あき}らかだった",
              "maleVariant": "",
              "stringId": "1801110478198128640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4D3BCZ^結{けっ}局{きょく}、奴{やつ}の死{し}で何{なに}かが変{か}わるわけもなく――まっとうな奴{やつ}が一{ひと}人{り}減{へ}っただけだ",
              "maleVariant": "",
              "stringId": "1801110664475557888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "473BCZ^とにかく、お前{まえ}が安{やす}らかに眠{ねむ}れてることを願{ねが}ってる",
              "maleVariant": "",
              "stringId": "1801111231310577664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D63BCZ^バッドランズがこんなに綺{き}麗{れい}だとは思{おも}わなかったよ",
              "maleVariant": "",
              "stringId": "1801112788487880704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5D3BCZ^落{お}ち着{つ}いたら遊{あそ}びに行{い}こうかと思{おも}う。その… ジョスの具{ぐ}合{あい}が良{よ}くなったら、な",
              "maleVariant": "",
              "stringId": "1801112928191758336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "763BCZ^ときどき考{かんが}えるんだ、もし俺{おれ}たちが…",
              "maleVariant": "",
              "stringId": "1801113151949488128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EE3BCZ^いや、やっぱ何{なん}でもない。俺{おれ}の愚{ぐ}痴{ち}を聞{き}くほどお前{まえ}もヒマじゃないだろう",
              "maleVariant": "",
              "stringId": "1801113458905432064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DF3BCZ^頑{がん}張{ば}れよ、V。思{おも}うがままに生{い}きるんだ",
              "maleVariant": "",
              "stringId": "1801113965459914752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E63BCZ^21日{にち}！",
              "maleVariant": "",
              "stringId": "1801115176758132736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1E3BCZ^そうそう、21日{にち}だ。だから、もし予{よ}定{てい}が空{あ}いてて… 近{ちか}くでヒマしてるようだったらだな…",
              "maleVariant": "",
              "stringId": "1801118678096003072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C63BCZ^来{こ}なよ、楽{たの}しいよ！",
              "maleVariant": "",
              "stringId": "1801119153360977920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うるさい、ドリアン！",
              "maleVariant": "",
              "stringId": "1801125979657846784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "463BCZ^…ほら、みんな会{あ}いたがってる",
              "maleVariant": "",
              "stringId": "1801126353873649664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B73BCZ^V！ 車{くるま}に乗{の}せてくれる？",
              "maleVariant": "",
              "stringId": "1801126686481956864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4E3BCZ^うるさいって言{い}っただろ",
              "maleVariant": "",
              "stringId": "1801126946713354240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C63BCZ^ちょっと二{ふた}人{り}とも！ 静{しず}かにしなさい！",
              "maleVariant": "",
              "stringId": "1801127142000148480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "453BCZ^えっーと… じゃあまた連{れん}絡{らく}する。絶{ぜっ}対{たい}来{き}てくれよ。あと、無{む}茶{ちゃ}はほどほどにな",
              "maleVariant": "",
              "stringId": "1801127358879219712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "63BCZ^昨日{きのう}の夜{よる}、バーで飲{の}んでた時{とき}に、あの農{のう}場{じょう}の子{こ}供{ども}たちについて話{はな}してる奴{やつ}と出{で}くわしてな",
              "maleVariant": "",
              "stringId": "1801129982047272960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "63BCZ^騙{だま}されたのは自{じ}業{ごう}自{じ}得{とく}で、いい経{けい}験{けん}になっただろうってぬかしやがった",
              "maleVariant": "",
              "stringId": "1801130179615768576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5E3BCZ^もちろん最{さい}初{しょ}に黙{だま}れと警{けい}告{こく}はしたが――",
              "maleVariant": "",
              "stringId": "1801130421610332160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B83BCZ^案{あん}の定{じょう}、言{い}うことを聞{き}かなかった。外{そと}に出{で}ろってなって… あとはお決{き}まりの展{てん}開{かい}だ",
              "maleVariant": "",
              "stringId": "1801130633573679104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "763BCZ^俺{おれ}はリトルチャイナの留{りゅう}置{ち}場{じょう}にいる――場{ば}所{しょ}はわかるよな。他{ほか}に頼{たよ}れる奴{やつ}がいないんだ。連{れん}絡{らく}をくれ、頼{たの}んだぞ…",
              "maleVariant": "",
              "stringId": "1801130812855009280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C63BCZ^街{まち}の全{すべ}ての警{けい}察{さつ}署{しょ}と病{びょう}院{いん}、リパードクのクリニックまで… 一{ひと}つ残{のこ}らず探{さが}したが――",
              "maleVariant": "",
              "stringId": "1801131827809468416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "63BCZ^…誰{だれ}もお前{まえ}の行方{なめがた}を知{し}らない。神{かみ}隠{かく}しみたいだ",
              "maleVariant": "",
              "stringId": "1801131979207065600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B63BCZ^例{れい}の仕{し}事{ごと}が失敗{しっぱい}したのなら… もう会{あ}えないのかもしれない",
              "maleVariant": "",
              "stringId": "1801132166843449344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CD3BCZ^だが、もし上手{うま}くいって… もし生{い}きてるなら… 連{れん}絡{らく}してくれ、V。それぐらいしてくれてもいいだろ",
              "maleVariant": "",
              "stringId": "1801132386272657408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3F3BCZ^もう切{き}るよ… じゃあな",
              "maleVariant": "",
              "stringId": "1801132568573886464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "403BCZ^第{だい}五{ご}分{ぶん}署{しょ}の連{れん}中{ちゅう}が、違{い}法{ほう}武{ぶ}器{き}店{てん}を摘{てき}発{はつ}した",
              "maleVariant": "",
              "stringId": "1801133388929417216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AE3BCZ^ショットガンにスタンガン、マスタードガスとか、色々{いろいろ}だ",
              "maleVariant": "",
              "stringId": "1801133566868570112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A63BCZ^上{うえ}には全{ぜん}部{ぶ}処{しょ}分{ぶん}するよう言{い}われたんだが… 有{ゆう}効{こう}活{かつ}用{よう}できる人{にん}間{げん}に渡{わた}したほうが、世{よ}のためになるんじゃないか… と思{おも}ってな",
              "maleVariant": "",
              "stringId": "1801133764302848000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D3BCZ^お前{まえ}が俺{おれ}とランディのためにしてくれたことは、忘{わす}れない",
              "maleVariant": "",
              "stringId": "1801133989369200640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "673BCZ^こういう装{そう}備{び}は市{し}民{みん}を守{まも}ってくれる人{にん}間{げん}にこそ、提{てい}供{きょう}すべきなんだ",
              "maleVariant": "",
              "stringId": "1801134725318557696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C73BCZ^礼{れい}はいらない。じゃあな、V",
              "maleVariant": "",
              "stringId": "1801135126361128960"
            }
          ]
        }
      }
    },
