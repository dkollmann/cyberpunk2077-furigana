        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "89623Z^「私{わたし}は現{げん}場{ば}に到{とう}着{ちゃく}した。ピンクのジャケットを着{き}た、不{ふ}貞{てい}を働{はたら}く女{おんな}を捜{さが}しにきたのだ」",
              "maleVariant": "",
              "stringId": "1805320966090403840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "74623Z^「女{おんな}はどいつも同{おな}じだとよく言{い}うが、この女{おんな}はどこか違{ちが}った…」",
              "maleVariant": "",
              "stringId": "1805395303032041472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2E623Z^「ジャケットがピンクではなく、後{うし}ろめたさがない。違{ちが}う女{おんな}のようだ」",
              "maleVariant": "",
              "stringId": "1805395340512342016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CB623Z^「ついに見{み}つけた。タバコの煙{けむり}に包{つつ}まれ、ピンクの服{ふく}を着{き}た女{おんな}。私{わたし}は彼{かの}女{じょ}の後{のち}をつけ…」",
              "maleVariant": "",
              "stringId": "1805395373211136000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5F623Z^…ふざけたナレーションをいい加{か}減{げん}やめることにした",
              "maleVariant": "",
              "stringId": "1805397948379574272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "72623Z^「私{わたし}は女{おんな}に近{ちか}づきすぎて、怖{こわ}がらせた。注{ちゅう}意{い}しなくては…」",
              "maleVariant": "",
              "stringId": "1805461926531182592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "99623Z^「またしても近{ちか}づきすぎた。まるで磁{じ}石{しゃく}のような力{ちから}で、女{おんな}に引{ひ}き寄{よ}せられているかのようだ」",
              "maleVariant": "",
              "stringId": "1805461952955297792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AC623Z^「私{わたし}は女{おんな}を怒{いか}らせてしまった。探{たん}偵{てい}という仕{し}事{ごと}は、存{ぞん}外{がい}難{むずか}しいものである」",
              "maleVariant": "",
              "stringId": "1805461974748901376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8A623Z^「周{しゅう}囲{い}を見{み}回{まわ}す女{おんな}。何{なに}かを感{かん}じていた。私{わたし}の視{し}線{せん}かもしれない…」",
              "maleVariant": "",
              "stringId": "1806780176951029760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B7623Z^「女{おんな}は距{きょ}離{り}を取{と}って浮{ふ}浪{ろう}者{しゃ}のそばを通{とう}り過{す}ぎた。服{ふく}を汚{よご}したくはない。ただでさえ頭{あたま}の中{なか}は汚{よご}れているのだから」",
              "maleVariant": "",
              "stringId": "1806780482648682496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4F44DZ^場{ば}末{すえ}のバーに、浮気{うわき}、そして魅{み}力{りょく}的{てき}な女{おんな}… まるでノワールもんの脚{きゃく}本{ほん}みたいだな",
              "maleVariant": "",
              "stringId": "1913994035820023808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4744DZ^「ぺぺは客{きゃく}の愚{ぐ}痴{ち}を聞{き}くのには慣{な}れていた。しかし今{こん}度{ど}は自{じ}分{ぶん}が胸{むね}の内{ない}を告{こく}白{はく}する番{ばん}だった」",
              "maleVariant": "",
              "stringId": "1913996165989584896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A044DZ^「どこにでもある、ありふれた話{はなし}である。何{なに}もかもが不{ふ}確{たし}かな街{まち}で、不{ふ}貞{てい}をはたらく妻{つま}…」",
              "maleVariant": "",
              "stringId": "1913997318533992448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6F44DZ^もういい？ 集{しゅう}中{ちゅう}させてよ",
              "maleVariant": "6F44DZ^もういいか？ 集{しゅう}中{ちゅう}させてくれ",
              "stringId": "1913998344209092608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やめてよね",
              "maleVariant": "やめろって",
              "stringId": "1914000258590101504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7044DZ^楽{たの}しい？",
              "maleVariant": "7044DZ^楽{たの}しいか？",
              "stringId": "1914000959860953088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8744DZ^あんたの歌{うた}を聴{き}いてるほうがまだマシ",
              "maleVariant": "8744DZ^お前{まえ}の歌{うた}を聴{き}いてるほうがまだマシだ",
              "stringId": "1914003498572500992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4644DZ^吐{は}きそうになってきた。それもナレーションしてよ",
              "maleVariant": "4644DZ^吐{は}きそうになってきた。それもナレーションしろよ",
              "stringId": "1914008350795026432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9D44DZ^これ、何{なに}かの罰{ばつ}ゲーム？",
              "maleVariant": "9D44DZ^これ、何{なに}かの罰{ばつ}ゲームか？",
              "stringId": "1914009898442870784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってるって",
              "maleVariant": "",
              "stringId": "1914010873081679872"
            },
            {
