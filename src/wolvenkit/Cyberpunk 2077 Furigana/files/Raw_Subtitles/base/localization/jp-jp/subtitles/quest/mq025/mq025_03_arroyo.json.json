          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E02D7004^ロストビッチだって！？ 目{め}が見{み}えねえのか？ テクトロニカのロゴだぞ！",
              "maleVariant": "",
              "stringId": 1852094905156726788
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E32D7Z^一{いっ}戦{せん}やりにきた",
              "maleVariant": "",
              "stringId": 1852094905207058432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E52D7004^一{いっ}戦{せん}やりにきた。あんたがバック？",
              "maleVariant": "E52D7004^一{いっ}戦{せん}やりにきた。あんたがバックか？",
              "stringId": 1852094905240612868
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E52D700C^ああ、俺{おれ}様{さま}がそうだ",
              "maleVariant": "",
              "stringId": 1852094905240612876
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E52D7024^へっ。お前{まえ}を見{み}ただけで財{さい}布{ふ}が濡{ぬ}れてきた。有{あ}り金{きん}全{ぜん}部{ぶ}賭{か}けてやる",
              "maleVariant": "",
              "stringId": 1852094905240612900
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E72D7Z^受{う}けて立{た}つ",
              "maleVariant": "",
              "stringId": 1852094905274167296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E82D7004^スナイパーライフルも入{い}れてよ",
              "maleVariant": "E82D7004^スナイパーライフルも入{い}れてくれよ",
              "stringId": 1852094905290944516
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E82D7014^また後{あと}で",
              "maleVariant": "",
              "stringId": 1852094905290944532
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいライフルね",
              "maleVariant": "いいライフルだな",
              "stringId": 1852094905307721728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EA2D7004^また後{あと}で",
              "maleVariant": "",
              "stringId": 1852094905324498948
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EA2D700C^何{なん}だ、怖{こわ}いのか？ はっ！ そりゃ当{とう}然{ぜん}だ",
              "maleVariant": "",
              "stringId": 1852094905324498956
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EE2D7004^やりましょ。だからその口{くち}を閉{と}じてて",
              "maleVariant": "EE2D7004^やるさ。だからその口{くち}を閉{と}じてろ",
              "stringId": 1852094905391607812
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F02D7004^提{てい}案{あん}がある。賭{か}けを増{ふ}やすってのはどう？",
              "maleVariant": "F02D7004^提{てい}案{あん}がある。賭{か}けを増{ふ}やすってのはどうだ？",
              "stringId": 1852094905425162244
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F02D700C^言{い}ったろ… 持{も}ち金{がね}を全{ぜん}額{がく}賭{か}けるって",
              "maleVariant": "",
              "stringId": 1852094905425162252
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F22D7004^準{じゅん}備{び}はいいか？ 怖{こわ}いなら逃{に}げ出{だ}していいんだぜ？",
              "maleVariant": "",
              "stringId": 1852094905458716676
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F72D7004^へえ？ ボコボコにされるのが好{す}きらしいな",
              "maleVariant": "",
              "stringId": 1852094905542602756
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "このっ、クソアマ！",
              "maleVariant": "B2D7004^このっ、クソ野{や}郎{ろう}！",
              "stringId": 1852094905878147076
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C2D7004^ほら、ライフルを渡{わた}して",
              "maleVariant": "C2D7004^ほら、ライフルを渡{わた}せよ",
              "stringId": 1852094905894924292
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D2D7004^思{おも}ったとおり、てんで雑{ざつ}魚{ぎょ}じゃねえか",
              "maleVariant": "",
              "stringId": 1852094905911701508
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E2D7004^またやりたけりゃ言{い}ってくれ。サンドバッグとして使{つか}ってやる",
              "maleVariant": "",
              "stringId": 1852094905928478724
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1D2D7Z^[試{し}合{あい}を開{かい}始{し}する] いつでも",
              "maleVariant": "",
              "stringId": 1852094906180136960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "202D7004^相{あい}手{て}になってあげる",
              "maleVariant": "202D7004^相{あい}手{て}になってやるよ",
              "stringId": 1852094906230468612
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "242D7Z^そのライフル… 「グラッド」だっけ？ どこで手{て}に入{い}れたの？",
              "maleVariant": "242D7Z^そのライフル… 「グラッド」だっけ？ どこで手{て}に入{い}れたんだ？",
              "stringId": 1852094906297577472
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B02D7Z^これはこれは… どうやら銃{じゅう}に詳{くわ}しいようだな",
              "maleVariant": "",
              "stringId": 1852103318192353280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E02D7Z^しーっ… 負{ま}け犬{いぬ}は黙{だま}ってて",
              "maleVariant": "E02D7Z^しーっ… 負{ま}け犬{いぬ}は黙{だま}ってろ",
              "stringId": 1852111028463955968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "もらうべきものはもらっとく",
              "maleVariant": "",
              "stringId": 1853240499753545728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "43404Z^どうかしたのか？ 哺{ほ}乳{にゅう}瓶{びん}を武{ぶ}器{き}にしたきゃ、構{かま}わんから拾{ひろ}えよ",
              "maleVariant": "",
              "stringId": 1864938740229619712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DE404Z^SPT32グラッド。史{し}上{じょう}最{さい}高{こう}のスナイパーライフルだ",
              "maleVariant": "",
              "stringId": 1887843047505412096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A8404Z^軍{ぐん}にいた時{とき}の記{き}念{ねん}品{ひん}だ。どうして聞{き}く？",
              "maleVariant": "",
              "stringId": 1887843776743882752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3404Z^調{ちょう}子{し}にのるなよ？ 運{うん}がよかっただけだ…",
              "maleVariant": "",
              "stringId": 1887908361693839360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "51404Z^確{たし}かに。ありがとね、バック",
              "maleVariant": "51404Z^確{たし}かに。ありがとな、バック",
              "stringId": 1887936181505638400
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "96404Z^失{う}せろと言{い}っただろ！",
              "maleVariant": "",
              "stringId": 1887936350166990848
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9D44DZ^興{きょう}味{み}があって。限{げん}定{てい}品{ひん}だって聞{き}いた。300丁{ちょう}しか製{せい}造{ぞう}されなかったKGBモデルでしょ",
              "maleVariant": "9D44DZ^興{きょう}味{み}があって。限{げん}定{てい}品{ひん}だって聞{き}いた。300丁{ちょう}しか製{せい}造{ぞう}されなかったKGBモデルだろ",
              "stringId": 1905579341497290752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BE44DZ^でもここは銃{じゅう}の話{はなし}をする場{ば}所{しょ}じゃねえ。骨{ほね}とクロームをかち割{わ}る場{ば}所{しょ}だ。で、やるのか？",
              "maleVariant": "",
              "stringId": 1905581420815110144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9444DZ^そのライフルも入{い}れてよ。負{ま}けるのが怖{こわ}いなら別{べつ}だけど…",
              "maleVariant": "9444DZ^そのライフルも入{い}れてくれ。負{ま}けるのが怖{こわ}いなら別{べつ}だが…",
              "stringId": 1905583150982287360
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2644DZ^はあ？ んなわけねえだろ。いいさ。ライフルも賭{か}けてやる",
              "maleVariant": "",
              "stringId": 1905583780496986112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4344DZ^いいだろう。やるって言{い}うよりやられることになるがな",
              "maleVariant": "",
              "stringId": 1905586980734160896
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ついてこい",
              "maleVariant": "",
              "stringId": 1905588315110035456
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "444DZ^死{し}んでも渡{わた}すか。やれ！",
              "maleVariant": "",
              "stringId": 1905603618380500992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CB44DZ^言{い}いたいように言{い}えばいい。さあ、エディーを渡{わた}して",
              "maleVariant": "CB44DZ^言{い}いたいように言{い}えばいい。さあ、エディーを渡{わた}せ",
              "stringId": 1908129431856402432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B544DZ^受{う}け取{と}ってすぐに失{う}せろ",
              "maleVariant": "",
              "stringId": 1908131149474222080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B744DZ^俺{おれ}を怒{いか}らせようとしてるなら、うまいことやってるぜ",
