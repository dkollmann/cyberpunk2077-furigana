            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのとおりよ",
              "maleVariant": "そのとおりだ",
              "stringId": 1908133206226685956
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DE44DZ^落{お}ち着{つ}いて。ただのスポーツでしょ",
