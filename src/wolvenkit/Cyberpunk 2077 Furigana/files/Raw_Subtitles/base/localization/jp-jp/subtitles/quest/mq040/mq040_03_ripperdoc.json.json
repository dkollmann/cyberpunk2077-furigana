          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "80623Z^「今{いま}にも崩{くず}れそうな建物{たてもの}の中{なか}に姿{すがた}を消{け}す女{おんな}。漂{ただよ}う空{くう}気{き}は、スモッグと背{はい}信{しん}で重{おも}苦{くる}しかった」",
              "maleVariant": "",
              "stringId": 1805535941937475584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4A623Z^気{き}をつけて！ シャツが破{やぶ}れるじゃない！",
              "maleVariant": "",
              "stringId": 1805696370944913408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あらあら",
              "maleVariant": "おやおや",
              "stringId": 1805697185076097024
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F6623Z^ちょっと！ あなた、ここで何{なに}してるの！？",
              "maleVariant": "",
              "stringId": 1805706982399815680
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "81623Z^シンシア！ 誰{だれ}にも尾{び}行{こう}されないようにと言{い}ったろ！",
              "maleVariant": "",
              "stringId": 1805710631159083008
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
