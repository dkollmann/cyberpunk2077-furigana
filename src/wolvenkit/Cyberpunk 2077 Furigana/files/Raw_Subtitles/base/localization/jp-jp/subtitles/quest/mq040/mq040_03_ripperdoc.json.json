            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "79623004^リパードクと会{あ}っていた",
              "maleVariant": "",
              "stringId": 1806661635568447492
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "63623Z^あとは彼{かの}女{じょ}から直{ちょく}接{せつ}聞{き}いて",
