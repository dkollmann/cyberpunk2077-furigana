          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C049DZ^まだこいつと一{いっ}緒{しょ}にいたのか",
              "maleVariant": "",
              "stringId": 1735739653548003328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3A49DZ^黙{だま}れ、ハロルド。呼{よ}んだ理{り}由{ゆう}はわかってるだろう",
              "maleVariant": "",
              "stringId": 1740123369800060928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8949DZ^隠蔽{いんぺい}工{こう}作{さく}をした理{り}由{ゆう}は聞{き}かない。そもそもどうして関{かか}わったのかもな。俺{おれ}が知{し}りたいのは、誰{だれ}に指{し}示{じ}されたかってことだけだ",
              "maleVariant": "",
              "stringId": 1740152551133270016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A549DZ^聞{き}いてどうする、リバー？ 手{て}を汚{よご}していないお前{まえ}には関{かん}係{けい}ないだろう",
              "maleVariant": "",
              "stringId": 1740220064193957888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C549DZ^その役{やく}目{め}は、俺{おれ}が買{か}って出{で}てやった",
              "maleVariant": "",
              "stringId": 1740220558652067840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "349DZ^お前{まえ}は、この街{まち}の仕{し}組{く}みをわかってない。理{り}想{そう}ばっか追{お}い求{もと}めて、現{げん}実{じつ}を見{み}ることを拒{きょ}否{ひ}してるだけなんだよ。おめでたい奴{やつ}だ",
              "maleVariant": "",
              "stringId": 1740220976304082944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5149DZ^そんな目{め}で見{み}るなよ、傭{よう}兵{へい}。俺{おれ}が正{ただ}しいのは知{し}ってるだろ",
              "maleVariant": "",
              "stringId": 1740222373477076992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "149DZ^あたしを巻{ま}き込{こ}まないで",
              "maleVariant": "149DZ^俺{おれ}を巻{ま}き込{こ}むな",
              "stringId": 1740225881123180544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4049DZ^ええ。生{う}まれも育{そだ}ちもナイトシティだから",
              "maleVariant": "4049DZ^ああ。生{う}まれも育{そだ}ちもナイトシティだからな",
              "stringId": 1740226496360468480
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9749DZ^この街{まち}の仕{し}組{く}みなら心{こころ}得{え}てる",
              "maleVariant": "",
              "stringId": 1740226781287927808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3249DZ^さあ、あたしはここの出身{しゅっしん}じゃない",
              "maleVariant": "3249DZ^さあ、俺{おれ}はここの出身{しゅっしん}じゃない",
              "stringId": 1740227028701532160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8349DZ^この街{まち}は知{し}り尽{つ}くしてる",
              "maleVariant": "",
              "stringId": 1740235800383705088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D249DZ^ナイトシティで生{い}きていくためのルールも、暗{あん}黙{もく}の了{りょう}解{かい}もすべて把{は}握{あく}してる",
              "maleVariant": "",
              "stringId": 1740236080881979392
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D449DZ^さあ、都{と}会{かい}のことはよく知{し}らない",
              "maleVariant": "D449DZ^さあな、都{と}会{かい}のことはよく知{し}らない",
              "stringId": 1740236317138735104
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F249DZ^巻{ま}き込{こ}まないでよ。政{せい}治{じ}も警{けい}察{さつ}の内{うち}輪{わ}もめも、あたしには関{かん}係{けい}ない",
              "maleVariant": "F249DZ^巻{ま}き込{こ}まないでくれ。政{せい}治{じ}も警{けい}察{さつ}の内{うち}輪{わ}もめも、俺{おれ}には関{かん}係{けい}ない",
              "stringId": 1740236566750154752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7649DZ^じゃあ仕{し}組{く}みもわかるだろう。市{し}庁{ちょう}舎{しゃ}もストリートも、ルールは一{いっ}緒{しょ}だ。ラインもそれを知{し}っていた",
              "maleVariant": "",
              "stringId": 1740237178850103296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4E49DZ^なら、時{とき}には長{なが}いものに巻{ま}かれる必{ひつ}要{よう}があることもわかってるだろう",
              "maleVariant": "",
              "stringId": 1740238990655213568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほど。ノーマッドか",
              "maleVariant": "",
              "stringId": 1740240855140782080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5549DZ^NCPDはいわば俺{おれ}のファミリー、俺{おれ}のクランだ",
              "maleVariant": "",
              "stringId": 1740241546278195200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D049DZ^じゃあ、なぜここにいる？ どうせ金{きん}のためなんだろ？ いかにも傭{よう}兵{へい}らしいな… とはいえ、別{べつ}にお前{まえ}を批{ひ}判{はん}するつもりはない",
              "maleVariant": "",
              "stringId": 1740242428810088448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2949DZ^誰{だれ}もが皆{みな}、この街{まち}で生{い}きる道{みち}と居{い}場{ば}所{しょ}を探{さが}してる",
              "maleVariant": "",
              "stringId": 1740243113203060736
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F449D004^確{たし}かにファミリーが第{だい}一{いち}よ",
