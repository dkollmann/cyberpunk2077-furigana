          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EA3C5Z^ちくしょう、まただ… なあ、手{て}を貸{か}してくれ",
              "maleVariant": "",
              "stringId": "1811292423955697664"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F3C5004^[開{ひら}ける]",
              "maleVariant": "",
              "stringId": "1811296530565189636"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E33C5Z^そこを押{お}せ。少{すこ}し左{ひだり}に… ああ、そうだ",
              "maleVariant": "",
              "stringId": "1811305132646486016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ちょっとミッチ… どういうこと！？",
              "maleVariant": "おいミッチ… どういうことだ！？",
              "stringId": "1811373862323769344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7F3C5Z^落{お}ち着{つ}けよ。これがスコーピオンの望{のぞ}みなんだ",
              "maleVariant": "",
              "stringId": "1811385601476022272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B83C5Z^スペアタイヤみたいにトランクに詰{つ}め込{こ}まれることが？",
              "maleVariant": "B83C5Z^スペアタイヤみたいにトランクに詰{つ}め込{こ}まれることがか？",
              "stringId": "1811385744166244352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E83C5004^それがスコーピオンの望{のぞ}みだって言{い}うなら…",
              "maleVariant": "",
              "stringId": "1811386006964555780"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DB3C5Z^ソウルが怒{いか}るんじゃない？",
              "maleVariant": "DB3C5Z^ソウルが怒{いか}るんじゃないか？",
              "stringId": "1811386633811677184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ED3C5Z^車{くるま}をダメにしたら、きっとソウルが怒{いか}るでしょ。もったいないだろって",
              "maleVariant": "ED3C5Z^車{くるま}をダメにしたら、きっとソウルが怒{いか}るだろ。もったいないだろって",
              "stringId": "1811386964826148864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "463C5Z^事{じ}故{こ}だったって伝{つた}えりゃいい",
              "maleVariant": "",
              "stringId": "1811387447355658240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4F3C5Z^まあ、スコーピオンがそれを望{のぞ}んだって言{い}うなら…",
              "maleVariant": "",
              "stringId": "1811387679434887168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "883C5Z^ああ、それが遺{ゆい}言{ごん}だ。何{なん}度{ど}も聞{き}いたよ",
              "maleVariant": "",
              "stringId": "1811388212967133184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "593C5004^[タンクを拾{ひろ}う]",
              "maleVariant": "",
              "stringId": "1811389513553694724"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "はい",
              "maleVariant": "ほらよ",
              "stringId": "1811398952448970752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4B3C5Z^よし。それじゃあ助{じょ}手{しゅ}席{せき}に置{お}いてくれ",
              "maleVariant": "",
              "stringId": "1811399237124771840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EE3C5Z^[タンクを置{お}く]",
              "maleVariant": "",
              "stringId": "1811399600636710912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CF3C5Z^運{うん}転{てん}手{しゅ}は… 準{じゅん}備{び}できてるみたい",
              "maleVariant": "CF3C5Z^運{うん}転{てん}手{しゅ}は… 準{じゅん}備{び}できてるみたいだ",
              "stringId": "1811414052681568256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FC3C5Z^急{いそ}いでやるぞ。こいつの体{からだ}はあと1、2時{じ}間{かん}で風{ふう}船{せん}みたいに膨{ふく}らんじまう",
              "maleVariant": "",
              "stringId": "1811416248164831232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やろう",
              "maleVariant": "",
              "stringId": "1811418292787367936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "273C5Z^本{ほん}当{とう}にいいの？",
              "maleVariant": "273C5Z^本{ほん}当{とう}にいいのか？",
              "stringId": "1811419470111723520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "383C5Z^ねえ、本{ほん}当{とう}にこんなことしていいの？",
              "maleVariant": "383C5Z^なあ、本{ほん}当{とう}にこんなことしていいのか？",
              "stringId": "1811421128254312448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1D3C5Z^さあな、V。俺{おれ}にもわからん",
              "maleVariant": "",
              "stringId": "1811422682579488768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やろう",
              "maleVariant": "",
              "stringId": "1811422853220552704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1D3C5Z^そうこなくっちゃ。仕{し}掛{か}けてある布{ぬの}きれに火{ひ}をつけてくれ。俺{おれ}はエンジンをかける",
              "maleVariant": "",
              "stringId": "1811423155025891328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "23C5004^[火{ひ}をつける]",
              "maleVariant": "",
              "stringId": "1811423554004865028"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3F3C5Z^火{ひ}がついた！ 急{いそ}がないと爆{ばく}発{はつ}するよ！",
              "maleVariant": "3F3C5Z^火{ひ}がついた！ 急{いそ}がないと爆{ばく}発{はつ}するぞ！",
              "stringId": "1811423894330691584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1C3C5Z^上手{うま}くいったな。これで約{やく}束{そく}は果{は}たせた",
              "maleVariant": "",
              "stringId": "1811424662542635008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "903C5Z^残{のこ}りし灰{はい}よ、安{やす}らかなれ",
              "maleVariant": "",
              "stringId": "1811425287259049984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BE3C5Z^ええ。しっかり見{み}送{おく}れたことだし",
              "maleVariant": "BE3C5Z^ああ。しっかり見{み}送{おく}れたことだしな",
              "stringId": "1811425434059689984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいね",
              "maleVariant": "いいな",
              "stringId": "1811425723818987520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さて… ビールでもどうだ？",
              "maleVariant": "",
              "stringId": "1811722638246825984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "253C5Z^もう行{い}かないと",
              "maleVariant": "",
              "stringId": "1811735446527168512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F43C5Z^ごめん、そういう気{き}分{ぶん}じゃない",
              "maleVariant": "F43C5Z^すまない、そういう気{き}分{ぶん}じゃない",
              "stringId": "1811735716288024576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D13C5Z^よし、決{き}まりだ",
              "maleVariant": "",
              "stringId": "1811741647050657792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C43C5Z^はあ… 冷{ひ}えたビールは最{さい}高{こう}だ",
              "maleVariant": "",
              "stringId": "1811744593180119040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まったくね",
              "maleVariant": "まったくだ",
              "stringId": "1811747013125754880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B63C5004^[受{う}け取{と}る]",
              "maleVariant": "",
              "stringId": "1845779842239057924"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C83C5Z^何{なに}これ？",
              "maleVariant": "C83C5Z^何{なん}だこれ？",
              "stringId": "1845780327872352256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D73C5Z^スコーピオンはいつも肌{はだ}身{み}離{はな}さずそれを持{も}ってた。理{り}由{ゆう}は知{し}らん",
              "maleVariant": "",
              "stringId": "1845792706219757568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "283C5Z^なあ… 俺{おれ}はもうしばらくここに居{い}るよ。飲{の}み足{た}りねえ",
              "maleVariant": "",
              "stringId": "1845792948096880640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9244DZ^違{ちが}う。あいつはド派{は}手{で}に弔{とぶら}って欲{ほっ}しがってたんだ",
              "maleVariant": "",
              "stringId": "1863237586613751808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1444DZ^あいつを運{うん}転{てん}席{せき}に座{すわ}らせたら、ガソリンをセットし、それから火{ひ}をつけて崖{がけ}下{した}に落{お}とす",
              "maleVariant": "",
              "stringId": "1863238860105109504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7444DZ^頭{あたま}がおかしいんじゃないの！？",
              "maleVariant": "7444DZ^頭{あたま}がおかしいんじゃないのか！？",
              "stringId": "1863240077191467008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ミッチ。こんなのどうかしてる",
              "maleVariant": "",
              "stringId": "1863241269229441024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9544DZ^文{もん}句{く}は受{う}け付{つ}けねえよ",
              "maleVariant": "",
              "stringId": "1863243586733395968"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FC44DZ^タンクを取{と}ってくれるか？",
              "maleVariant": "",
              "stringId": "1863245705880326144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F644DZ^それじゃあ… ショーを始{はじ}めるとするか",
              "maleVariant": "",
              "stringId": "1863249863308005376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C444DZ^でももしこれがあいつの最{さい}後{ご}の望{のぞ}みだとしたら、俺{おれ}はそれを叶{かな}えてやる。たとえお前{まえ}が協{きょう}力{りょく}してくれなくてもな",
              "maleVariant": "",
              "stringId": "1863252538233769984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ならいいさ",
              "maleVariant": "",
              "stringId": "1863258104175841280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2844DZ^おい、V！ 助{たす}かったよ。ありがとうな",
              "maleVariant": "",
              "stringId": "1863258316642504704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2244DZ^ねえ、ミッチ… どうしてわざわざあたしに声{こえ}をかけたの？",
              "maleVariant": "2244DZ^なあ、ミッチ… どうしてわざわざ俺{おれ}に声{こえ}をかけたんだ？",
              "stringId": "1863262761832992768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4C44D004^どうしてあたしに声{こえ}をかけたの？",
              "maleVariant": "4C44D004^どうして俺{おれ}に声{こえ}をかけたんだ？",
              "stringId": "1863262994465869828"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C644DZ^“スコーピオン”の由{ゆ}来{らい}は？",
              "maleVariant": "",
              "stringId": "1863263606398046208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5A44DZ^ねえ… 彼{かれ}はどうして“スコーピオン”って愛{あい}称{しょう}で呼{よ}ばれてたの？",
              "maleVariant": "5A44DZ^なあ… あいつはどうして“スコーピオン”って愛{あい}称{しょう}で呼{よ}ばれてたんだ？",
              "stringId": "1863264231651332096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5D44DZ^まあ、それがな… 俺{おれ}もまったく知{し}らねえんだ。聞{き}く機{き}会{かい}がなくてよ",
              "maleVariant": "",
              "stringId": "1863264970436038656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7544DZ^さそり座{ざ}だから、とか？",
              "maleVariant": "",
              "stringId": "1863265301551173632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2744DZ^へっ… デカい毒針{どくばり}がついてたのかもな！ ハハハ！",
              "maleVariant": "",
              "stringId": "1863266236545421312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F544DZ^ったく、スコーピオンの野{や}郎{ろう}… 寂{さび}しくなるぜ",
              "maleVariant": "",
              "stringId": "1863266849886883840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "544DZ^いい奴{やつ}そうだからさ",
              "maleVariant": "",
              "stringId": "1863267756393418752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FD44DZ^真面目{まじめ}に答{こた}えて",
              "maleVariant": "FD44DZ^真面目{まじめ}に答{こた}えろ",
              "stringId": "1863268095561617408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9C44DZ^さあ… どうだろうな… でもこうして、あいつをちゃんと見{み}送{おく}ることができたのは… お前{まえ}のおかげだ",
              "maleVariant": "",
              "stringId": "1863268252848017408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2544DZ^偶{ぐう}然{ぜん}のめぐり合{あ}わせさ。少{すこ}しの間{かん}、俺{おれ}たちの道{みち}は交{まじ}わった… それだけだ",
              "maleVariant": "",
              "stringId": "1863269066895314944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "644DZ^V、いいか… これは幸{こう}運{うん}のお守{まも}りだ。スコーピオンが持{も}ってた。お前{まえ}に預{あず}ける。貰{もら}ってやってくれ",
              "maleVariant": "",
              "stringId": "1863270157296914432"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FD44DZ^わかった。あたしはもう行{い}く",
              "maleVariant": "FD44DZ^わかった。俺{おれ}はもう行{い}く",
              "stringId": "1863272334694338560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B644DZ^それと… ありがとうな、V。今日{きょう}のことも… あのとき助{たす}けてくれたこともよ",
              "maleVariant": "",
              "stringId": "1863272509596815360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5044DZ^お前{まえ}はいい奴{やつ}だ。感{かん}謝{しゃ}してる",
              "maleVariant": "",
              "stringId": "1863273061936320512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5344DZ^遠{えん}慮{りょ}しとく",
              "maleVariant": "",
              "stringId": "1873522370078363648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3244DZ^遠{えん}慮{りょ}しとく。今{いま}は酔{よ}いたくないの",
              "maleVariant": "3244DZ^遠{えん}慮{りょ}しとく。今{いま}は酔{よ}いたくないんだ",
              "stringId": "1873523224223207424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3D44DZ^都{と}会{かい}っ子{こ}は違{ちが}うな、チューマ。まあ、弔{ちょう}い方{かた}は人{にん}それぞれだ",
              "maleVariant": "",
              "stringId": "1873524512897945600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "コーヒーだって",
              "maleVariant": "",
              "stringId": "1873528054668906496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1544DZ^ああ。そうだ… もしアシがないなら、俺{おれ}の車{くるま}でキャンプに戻{もど}ってもいいぞ",
              "maleVariant": "",
              "stringId": "1908015766788427776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A83C5Z^道{どう}中{ちゅう}、気{き}をつけて逝{い}ってこいよ",
              "maleVariant": "",
              "stringId": "1934467678261235712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EA3C5004^ちょっと、何{なん}のつもり？",
              "maleVariant": "EA3C5004^おい、何{なん}のつもりだ？",
              "stringId": "2121969965643616260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "273BCZ^どうした、V？ 容{よう}器{き}だよ。早{はや}くしてくれ",
              "maleVariant": "",
              "stringId": "2238669820146728960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BB3BCZ^さあ、スコーピオンを送{おく}ってやろう",
