        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C62EF004^ワトソンについて教{おし}えて",
              "maleVariant": "C62EF004^ワトソンについて教{おし}えてくれ",
              "stringId": "1956220983743279108"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "72EFZ^レジーナ、ワトソンを一{ひと}言{こと}で表{あらわ}すとしたら？",
              "maleVariant": "",
              "stringId": "1956221285481508864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BA2EFZ^あんた… 前{まえ}はメディアにいた？",
              "maleVariant": "BA2EFZ^あんた… 前{まえ}はメディアにいたか？",
              "stringId": "1956221292779597824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BC2EFZ^ロイスという男{おとこ}について、何{なに}か知{し}ってる？",
              "maleVariant": "BC2EFZ^ロイスという男{おとこ}について、何{なに}か知{し}ってるか？",
              "stringId": "1956225175463587840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A32EF004^とある人{じん}物{ぶつ}について情{じょう}報{ほう}が欲{ほ}しいんだけど",
              "maleVariant": "A32EF004^とある人{じん}物{ぶつ}について情{じょう}報{ほう}が欲{ほ}しいんだが",
              "stringId": "1956225321073045508"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D32EFZ^ふぅん、名{な}前{まえ}は？",
              "maleVariant": "",
              "stringId": "1956225704130441216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A42EFZ^サイモン・ランドール。通{つう}称{しょう}ロイスよ",
              "maleVariant": "A42EFZ^サイモン・ランドール。通{つう}称{しょう}ロイスだ",
              "stringId": "1956225767766421504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "132EFZ^メイルストロームね。デクスが彼{かれ}に興{きょう}味{み}を示{しめ}してるって噂{うわさ}を聞{き}いた",
              "maleVariant": "",
              "stringId": "1956225825463267328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6D2EFZ^他{ほか}のフィクサーと同{おな}じことを言{い}うか確{たし}かめてるの？ 賢{かしこ}いけど、賢{けん}明{めい}じゃないわね",
              "maleVariant": "",
              "stringId": "1956225934347399168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EB2EFZ^元{もと}記{き}者{しゃ}なのよね",
              "maleVariant": "EB2EFZ^元{もと}記{き}者{しゃ}なんだよな",
              "stringId": "1956226567821520896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3F2EFZ^隠{かく}してるつもりもないけど、察{さ}しがいいってことにしといてあげる",
              "maleVariant": "",
              "stringId": "1956228055289491456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてやめたの？",
              "maleVariant": "どうしてやめたんだ？",
              "stringId": "1956228106443223040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C02EFZ^やめてはいないわ。情{じょう}報{ほう}は武{ぶ}器{き}よ、V。私{わたし}はただもっと大{おお}きな銃{じゅう}に持{も}ち替{か}えただけ",
              "maleVariant": "",
              "stringId": "1956228632979369984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあまた",
              "maleVariant": "",
              "stringId": "1956232568377372672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "692EFZ^じゃあね、メディアの嘘{うそ}には気{き}をつけるのよ",
              "maleVariant": "",
              "stringId": "1956232900717244416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8C2EFZ^カブキは日{に}本{ほん}人{じん}が築{きづ}いたけど、今{いま}住{す}んでるのはほとんど中国人{ちゅうごくじん}。ノースサイドは今{いま}や廃{すた}れきってる",
              "maleVariant": "",
              "stringId": "1956368441882374144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "…ワトソンよ",
              "maleVariant": "",
              "stringId": "1956368568181256192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5D2EFZ^あと、コーポの連{れん}中{ちゅう}がたくさん詰{つ}まった水{すい}槽{そう}がある。例{たと}えじゃなく文字通{もじどお}りにね",
              "maleVariant": "",
              "stringId": "1956378255094116352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "52EFZ^一{ひと}言{こと}で、って言{い}ったんだけど",
              "maleVariant": "52EFZ^一{ひと}言{こと}で、って言{い}ったんだが",
              "stringId": "1956378545675497472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "182EFZ^V。レジーナ・ジョーンズよ。ワトソンの仕{し}事{ごと}なら、私{わたし}に任{まか}せて",
              "maleVariant": "",
              "stringId": "1959212455610478592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "842EFZ^どうやってあたしの連{れん}絡{らく}先{さき}を？ それに名{な}前{まえ}も",
              "maleVariant": "842EFZ^どうやって俺{おれ}の連{れん}絡{らく}先{さき}を？ それに名{な}前{まえ}も",
              "stringId": "1959213887646527488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1A2EFZ^情{じょう}報{ほう}集{あつ}めは得{とく}意{い}な方{ほう}なのよ。じゃあね、V",
              "maleVariant": "",
              "stringId": "1959214839350882304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2E559Z^マックスとは仲{なか}良{よ}くしてる？",
              "maleVariant": "2E559Z^マックスとは仲{なか}良{よ}くしてるか？",
              "stringId": "1995381535222894592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マックスとはどう？",
              "maleVariant": "マックスとはどうだ？",
              "stringId": "1995382056776208384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9F559Z^難{むずか}しいところね",
              "maleVariant": "",
              "stringId": "1995383525688578048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7F559Z^想{そう}像{ぞう}はつく。今{いま}のあんたを認{みと}めてないんでしょ？",
              "maleVariant": "7F559Z^想{そう}像{ぞう}はつく。今{いま}のあんたを認{みと}めてないんだろ？",
              "stringId": "1995383619640987648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9F559Z^私{わたし}にだって嫌{きら}いなものはある。でも、それを我{が}慢{まん}することを覚{おぼ}えたのよ",
              "maleVariant": "",
              "stringId": "1995384526415958016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AD559Z^彼{かれ}はどうするつもりなの？",
              "maleVariant": "AD559Z^彼{かれ}はどうするつもりなんだ？",
              "stringId": "1995384964737503232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DD559Z^本{ほん}人{にん}もわかってないでしょうね",
              "maleVariant": "",
              "stringId": "1995385055737122816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3E559Z^マックスの話{はなし}を聞{き}いた",
              "maleVariant": "",
              "stringId": "1995385431026667520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "マックスのことだけど――",
              "maleVariant": "マックスのことだが――",
              "stringId": "1995386048679874560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "V",
              "maleVariant": "",
              "stringId": "1995386662038114304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なに？",
              "maleVariant": "なんだ？",
              "stringId": "1995386707152048128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3D559Z^話{わ}題{だい}を変{か}えましょ",
              "maleVariant": "",
              "stringId": "1995386758154784768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8F62AZ^サイバーサイコシスの研{けん}究{きゅう}はどうなってる？",
              "maleVariant": "",
              "stringId": "2353281384965120000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A462AZ^V、あなたは多{おお}くのサイバーサイコにやりなおすチャンスを与{あた}えてくれた",
              "maleVariant": "",
              "stringId": "2353282682397564928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C062AZ^サイバーサイコの脅{きょう}威{い}は去{さ}ったと言{い}ってもいいわ。Vにはお礼{れい}を言{い}わなきゃね",
              "maleVariant": "",
              "stringId": "2353284692912021504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B162AZ^V、あなたは一{ひと}人{り}も犠{ぎ}牲{せい}者{しゃ}を出{だ}すことなく、サイバーサイコに対{たい}処{しょ}してくれた",
              "maleVariant": "",
              "stringId": "2353285594603495424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8662AZ^とにかく、報{ほう}酬{しゅう}はあなたのものよ",
              "maleVariant": "",
              "stringId": "2353286366976188416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5B62AZ^V、デリケートな案{あん}件{けん}があってね。だからあなたに連{れん}絡{らく}したの",
              "maleVariant": "",
              "stringId": "2354617741692018688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9D62AZ^街{まち}でサイバーサイコによる襲{しゅう}撃{げき}事{じ}件{けん}が増{ふ}えてる。よくある話{はなし}だと思{おも}うかもしれないけど、私{わたし}にとっては重{じゅう}要{よう}な問{もん}題{だい}なの。色々{いろいろ}とあってね",
              "maleVariant": "",
              "stringId": "2354619319052312576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5162AZ^実{じつ}は、サイバーサイコシスを治{なお}せると言{い}ってる人{にん}たちがいるのよ",
              "maleVariant": "",
              "stringId": "2354619536820576256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9162AZ^V、もう最{さい}近{きん}はサイバーサイコの事{じ}件{けん}をすっかり聞{き}かなくなった。私{わたし}たちの役{やく}目{め}も終{お}わったようね",
              "maleVariant": "",
              "stringId": "2358007747399819264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B262AZ^今{こん}度{ど}遊{あそ}びに来{き}て。直{ちょく}接{せつ}話{はな}しましょ",
              "maleVariant": "",
              "stringId": "2358009586199470080"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4B62AZ^サイバーサイコシスの仕{し}事{ごと}？ まだないわよ",
              "maleVariant": "",
              "stringId": "2358012066962513920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7562AZ^もっとデータが必{ひつ}要{よう}なの。実{じっ}験{けん}療{りょう}法{ほう}をやってる団{だん}体{たい}は… 正{しょう}直{じき}、私{わたし}も信{しん}頼{らい}していいかわからない",
              "maleVariant": "",
              "stringId": "2358015254532890624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D92B6Z^バカげた話{はなし}に聞{き}こえるのはわかってる。でも命{いのち}を奪{うば}うより、その実{じっ}験{けん}療{りょう}法{ほう}を試{ため}すべきだと思{おも}うの",
              "maleVariant": "",
              "stringId": "2364822319809650688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C12B6Z^今{こん}後{ご}、サイバーサイコ襲{しゅう}撃{げき}の情{じょう}報{ほう}が入{いっ}った時{とき}は連{れん}絡{らく}する。マックス・タックが到{とう}着{ちゃく}する前{まえ}に、あなたに調{ちょう}査{さ}してほしい",
              "maleVariant": "",
              "stringId": "2364828564289445888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7E2B6Z^でも、忘{わす}れないで。あなたを行{い}かせるのは、処{しょ}刑{けい}させるためじゃない。襲{しゅう}撃{げき}者{もの}の動{うご}きを封{ふう}じてくれたら、回{かい}収{しゅう}の車{くるま}を送{おく}るから",
              "maleVariant": "",
              "stringId": "2364831616887119872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EF2B6Z^頼{たの}んだわよ、V",
              "maleVariant": "",
              "stringId": "2364837434168664064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "402B6Z^命{いのち}を救{すく}うより、奪{うば}うことのほうが簡{かん}単{たん}だったはず。あなたは本{ほん}当{とう}に立派{りっぱ}よ",
              "maleVariant": "",
              "stringId": "2364923180254715904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BC2B6Z^簡{かん}単{たん}にはできないことよ",
              "maleVariant": "",
              "stringId": "2364939915527675904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F02B6018^発{はっ}症{しょう}者{もの}の治{ち}療{りょう}も始{はじ}まってる。たとえ完{かん}治{ち}できなくても、病{びょう}気{き}について多{おお}くのことがわかるはずよ",
              "maleVariant": "",
              "stringId": "2364946895721947160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F32B6Z^私{わたし}はサイバーウェアが“暴{ぼう}走{そう}”しただけなんて話{はなし}は一{いっ}切{さい}信{しん}じてない",
              "maleVariant": "",
              "stringId": "2364946895772278784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "312B6Z^いつか、このことを記{き}事{じ}にできるかもしれないわね…",
              "maleVariant": "",
              "stringId": "2364966194100527104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D02B6Z^でもまずは、被{ひ}験{けん}体{からだ}を集{あつ}めないことには始{はじ}まらないわ",
              "maleVariant": "",
              "stringId": "2364977114574970880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E12B6Z^でも正{しょう}直{じき}に言{い}うと、もっと多{おお}くの人{にん}を救{すく}えたはず…",
              "maleVariant": "",
              "stringId": "2365003941225914368"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "602B6Z^サイバーサイコシスの件{けん}？ 特{とく}に進{しん}展{てん}はないわ。でも協{きょう}力{りょく}してくれてありがとう",
              "maleVariant": "",
              "stringId": "2365015432394137600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2B6Z^状{じょう}況{きょう}を教{おし}えてくれる？",
              "maleVariant": "2B6Z^状{じょう}況{きょう}を教{おし}えてくれるか？",
              "stringId": "2369211545112240128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあまたね",
              "maleVariant": "じゃあまたな",
              "stringId": "2421308452675690496"
            }
          ]
        }
      }
    },
