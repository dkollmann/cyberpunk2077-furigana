    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしたの？",
              "maleVariant": "どうしたんだ？",
              "stringId": "1783919372995026944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "314E6Z^ジェイクの様{よう}子{す}はどうだ？",
              "maleVariant": "",
              "stringId": "1783921470449016832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A54E6Z^あまりよくない。出血{しゅっけつ}は止{や}められたけど、あのインプラントがないと…",
              "maleVariant": "",
              "stringId": "1783922821014904832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしたの？",
              "maleVariant": "どうしたんだ？",
              "stringId": "1783924323917914112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7C4E6Z^ジェイクが撃{う}たれて、ひどい傷{きず}でな。替{か}えの腎{じん}臓{ぞう}がいる",
              "maleVariant": "",
              "stringId": "1783925023645261824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ED4E6Z^問{もん}題{だい}は… 鎌{かま}状{じょう}赤血球{せっけっきゅう}症{しょう}ってのにかかってて、特{とく}殊{しゅ}な腎{じん}臓{ぞう}インプラントが必{ひつ}要{よう}なんだ",
              "maleVariant": "",
              "stringId": "1783925429268013056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "314E6Z^街{まち}にいけばあるはずよ",
              "maleVariant": "314E6Z^街{まち}にいけばあるはずだ",
              "stringId": "1783925606502522880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EF4E6Z^ああ。ダコタが病{びょう}院{いん}にいる知{し}り合{あ}いに聞{き}いて… どうにか用{よう}意{い}できるとは言{い}われてる…",
              "maleVariant": "",
              "stringId": "1783929174513049600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やったのはラフェン・シヴ？",
              "maleVariant": "やったのはラフェン・シヴか？",
              "stringId": "1783929713363673088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C14E6004^ああ。今{こん}回{かい}はどうにかしのげたけどな…",
              "maleVariant": "",
              "stringId": "1783929891000836100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "584E6Z^そういうインプラントはナイトシティの闇{やみ}市{いち}場{ば}で見{み}つかるはず",
              "maleVariant": "584E6Z^そういうインプラントはナイトシティの闇{やみ}市{いち}場{ば}で見{み}つかるはずだ",
              "stringId": "1783932844176728064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A04E6Z^ラフェン・シヴ―― 連{れん}中{ちゅう}の仕{し}業{わざ}なの？",
              "maleVariant": "A04E6Z^ラフェン・シヴ―― 連{れん}中{ちゅう}の仕{し}業{わざ}か？",
              "stringId": "1783933141737431040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "でも？",
              "maleVariant": "",
              "stringId": "1783935348696309760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E4E6004^任{まか}せておいて",
              "maleVariant": "E4E6004^任{まか}せてくれ",
              "stringId": "1783935819347550212"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "684E6Z^悪{わる}いけど、手{て}は貸{か}せない",
              "maleVariant": "684E6Z^悪{わる}いが、手{て}は貸{か}せない",
              "stringId": "1783936044195799040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6C4E6004^あたしに任{まか}せて",
              "maleVariant": "6C4E6004^俺{おれ}に任{まか}せろ",
              "stringId": "1783936675623100420"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6C4E600C^ダコタの知{し}り合{あ}いがリトルチャイナの病{びょう}院{いん}で働{はたら}いてる。インプラントを受{う}け取{と}るだけでいい。金{きん}は安{あん}全{ぜん}なルートで送{おく}るから",
              "maleVariant": "",
              "stringId": "1783936675623100428"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D84E6004^悪{わる}いけど、今{いま}すぐには手{て}は貸{か}せない。ごめん",
              "maleVariant": "D84E6004^悪{わる}いが、今{いま}すぐには手{て}は貸{か}せない。すまない",
              "stringId": "1783937304500264964"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D94E6Z^わかった。まだ少{すこ}し時{じ}間{かん}はあるから、手{て}が空{あ}くのを待{ま}ってるぞ",
              "maleVariant": "",
              "stringId": "1783937304517042176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "254E6004^ジェイクを助{たす}ける",
              "maleVariant": "",
              "stringId": "1795066824481464324"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1C4E6Z^でも、街{まち}に行{い}くとキャンプの人{ひと}手{で}が足{た}りなくなるんだ。ラフェン・シヴがいつ戻{もど}ってきてもおかしくない… リスクが高{たか}すぎる",
              "maleVariant": "",
              "stringId": "1851614608562741248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6A44DZ^任{まか}せてくれ。方{ほう}法{ほう}は言{い}えないが、大{だい}丈{じょう}夫{ぶ}。約{やく}束{そく}する",
              "maleVariant": "",
              "stringId": "1853117736755187712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8244DZ^わかると思{おも}うが、そういうものは、ここバッドランズじゃなかなか手{て}に入{い}らない",
              "maleVariant": "",
              "stringId": "1853122581880950784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CB44DZ^防{ぼう}弾{だん}チョッキのすぐ下{した}に、弾{だん}の破{は}片{へん}が当{あ}たったんだ",
              "maleVariant": "",
              "stringId": "1853131946134392832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4444DZ^それは… 運{うん}が悪{わる}かったね",
              "maleVariant": "4444DZ^それは… 運{うん}が悪{わる}かったな",
              "stringId": "1853133030496194560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5344DZ^まだ命{いのち}は救{すく}えるけど… 急{いそ}がないと",
              "maleVariant": "",
              "stringId": "1853133752302358528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AF44DZ^ごまかしても仕{し}方{かた}ないから、はっきり言{い}う。なあ、V… 俺{おれ}たちを助{たす}けてくれないか？",
              "maleVariant": "",
              "stringId": "1853139556346679296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A044DZ^助{たす}かるよ",
              "maleVariant": "",
              "stringId": "1853142751550689280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1F44DZ^わかった。じゃあまた後{あと}で",
              "maleVariant": "",
              "stringId": "1853144299869622272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BE44DZ^この前{まえ}の件{けん}だけど――",
              "maleVariant": "BE44DZ^この前{まえ}の件{けん}だが――",
              "stringId": "1853148528785018880"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
}