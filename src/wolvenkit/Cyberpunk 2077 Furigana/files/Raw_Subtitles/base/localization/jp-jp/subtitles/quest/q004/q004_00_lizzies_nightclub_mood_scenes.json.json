    "RootChunk": {
      "$type": "JsonResource",
      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6C2E3Z^あなたの好{この}み次{し}第{だい}よ。何{なん}でもしてあげる… 一杯{いっぱい}おごってくれたらね",
              "maleVariant": "",
              "stringId": "1487994350681862144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "おい！ こっちにおかわり！",
              "maleVariant": "",
              "stringId": "1487994350681862160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "732E3Z^いやいやいや、バーチャルって言{い}っても、感{かん}触{しょく}はマジでリアルなんだ。ムチで打{う}たれた感{かん}じがまだ残{のこ}ってるし、女{おんな}の子{こ}もすげえからな",
              "maleVariant": "",
              "stringId": "1487994350799302656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "732E3010^そうか？ 俺{おれ}も何{なん}度{ど}か試{ため}したが、頭{あたま}が痛{いた}くなっただけだ",
              "maleVariant": "",
              "stringId": "1487994350799302672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "742E300C^ここのは違{ちが}うんだって。きっとカスタムされてんだ。たった数{すう}秒{びょう}で、すぐに現{げん}実{じつ}と区{く}別{べつ}がつかなくなる。リアルと変{か}わらねえんだよ",
              "maleVariant": "",
              "stringId": "1487994350816079884"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "742E301C^見{み}ろよ。ヴィッキーとバックでAFだ。いいケツだろ？ 場{ば}所{しょ}も選{えら}べるぞ。ワイハの海{うみ}小{こ}屋{や}に、ウエストブルックのペントハウスだって…",
              "maleVariant": "",
              "stringId": "1487994350816079900"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "742E302C^俺{おれ}はもっと普{ふ}通{つう}のヤツでいい。こういうのとか",
              "maleVariant": "",
              "stringId": "1487994350816079916"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "742E303C^え、“ムカデ”か？ まあ… 好{す}きにしな。ヴィッキーは俺{おれ}がもらうぜ",
              "maleVariant": "",
              "stringId": "1487994350816079932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "752E3008^ほら、景{けい}気{き}づけだ。飲{の}んだらボックスに直行{ちょっこう}するぞ",
              "maleVariant": "",
              "stringId": "1487994350832857096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "792E3Z^昨日{きのう}、朝{あさ}4時{とき}にジーナとバーにいたんだけど",
              "maleVariant": "",
              "stringId": "1487994350899965952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7A2E300C^あの子{こ}、また銀{ぎん}行{こう}の話{はなし}をするわけ。ベラベラ、延々{えんえん}と",
              "maleVariant": "",
              "stringId": "1487994350916743180"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7A2E301C^暴{あば}れ出{だ}したの？",
              "maleVariant": "",
              "stringId": "1487994350916743196"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7A2E302C^それがさ… バーチャルじゃない生{なま}身{み}の相{あい}手{て}とヤラせろ、とか言{い}うの。禁{きん}止{し}なのはわかってるけど3倍{ばい}払{はら}うから、って",
              "maleVariant": "",
              "stringId": "1487994350916743212"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7A2E303C^そしたらジーナが… どうしたと思{おも}う？",
              "maleVariant": "",
              "stringId": "1487994350916743228"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7B2E300C^そいつをボックスにブチ込{こ}んだの。でもハイになってるからBD用{よう}の電{でん}極{きょく}をオモチャと勘{かん}違{ちが}いしちゃって。そのまま、最{さい}後{ご}のとこでイッちゃったままループするヤツをロードしてやったの",
              "maleVariant": "",
              "stringId": "1487994350933520396"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウソ… バレなかったの？",
              "maleVariant": "",
              "stringId": "1487994350950297600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7C2E3010^当{とう}然{ぜん}よ。シラフの奴{やつ}だって、あのブッカケを本{ほん}物{もの}と勘{かん}違{ちが}いするんだから",
              "maleVariant": "",
              "stringId": "1487994350950297616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "872E3Z^モール、ちょっと相{そう}談{だん}したいことがあるんだけど…",
              "maleVariant": "",
              "stringId": "1487994351134846976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "882E3004^客{きゃく}とのトラブルかい？ 金{きん}を払{はら}わず逃{に}げたのなら、入口{いりぐち}の女{おんな}たちに言{い}いな",
              "maleVariant": "",
              "stringId": "1487994351151624196"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "882E3014^そうじゃなくて… 私{わたし}の友達{ともだち}がさ、八{はち}番{ばん}街{まち}で働{はたら}いてるの",
              "maleVariant": "",
              "stringId": "1487994351151624212"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "882E3024^うちらのシマじゃない。管{かん}轄{かつ}外{そと}だ",
              "maleVariant": "",
              "stringId": "1487994351151624228"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "892E300C^それはわかってるんだけど、彼{かの}女{じょ}、そこを離{はな}れてこっちで働{はたら}きたいって。フルタイムで",
              "maleVariant": "",
              "stringId": "1487994351168401420"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8A2E3Z^でもパパが許{ゆる}さなくて。逃{に}げようとしたら、売{う}りモンだろうと容{よう}赦{しゃ}しないって言{い}われたみたい",
              "maleVariant": "",
              "stringId": "1487994351185178624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8A2E3010^パパの名{な}前{まえ}は？",
              "maleVariant": "",
              "stringId": "1487994351185178640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8A2E3020^本{ほん}名{みょう}はわかんない。でも誰{だれ}かが“ラテックス・フレッド”って呼{よ}んでた。イタいよね",
              "maleVariant": "",
              "stringId": "1487994351185178656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8B2E3004^わかった。その友達{ともだち}に、ラテックス・フレッドのことはもう心{しん}配{ぱい}しなくていいって言{い}っときな",
              "maleVariant": "",
              "stringId": "1487994351201955844"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8F2E3Z^それで… 今{こん}日{にち}はどんなプレイをしにきたの？",
              "maleVariant": "",
              "stringId": "1487994351269064704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8F2E3010^ちょっと激{はげ}しめのノーマル？ もっと変{へん}態{たい}っぽいヤツならワックスに電{でん}流{りゅう}とか？ それともパワーインプラント？",
              "maleVariant": "",
              "stringId": "1487994351269064720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8F2E3020^いや… そうだな、君{くん}の友達{ともだち}にも来{き}てもらおう。ヒールに目{め}隠{かく}し、猿{さる}ぐつわをつけてね",
              "maleVariant": "",
              "stringId": "1487994351269064736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8F2E3030^ええ、いいわよ。購{こう}入{にゅう}？ それともレンタル？",
              "maleVariant": "",
              "stringId": "1487994351269064752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "レンタルで。いくらだ？",
              "maleVariant": "",
              "stringId": "1487994351285841920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "902E3010^大{だい}丈{じょう}夫{ぶ}、つけておくから。あなたにぴったりのヤツがあるの",
              "maleVariant": "",
              "stringId": "1487994351285841936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "932E3Z^私{わたし}とキャンディスのよ。カラダの隅々{すみずみ}まで私{わたし}たち二{ふた}人{り}が満足{まんぞく}させてあげる",
              "maleVariant": "",
              "stringId": "1487994351336173568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "932E3010^それじゃ、向{む}こうで会{あ}いましょ",
              "maleVariant": "",
              "stringId": "1487994351336173584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8429FZ^お前{まえ}のちんけな妄{もう}想{そう}なんかよりずっと凄{すご}いのが体{たい}験{けん}できるぜ。ほら、このカタログ――",
              "maleVariant": "",
              "stringId": "1631443522876469248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C229FZ^そしたらそこに、変{へん}な男{おとこ}がフラフラ入{いっ}ってきたの。ドアのところでモールとぶつかりながら",
              "maleVariant": "",
              "stringId": "1631451954937458688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1129FZ^でもさ、保{ほ}育{いく}園{えん}に娘{むすめ}を迎{むか}えに行{い}くのに、午{ご}後{ご}のシフト代{か}わってほしかったから… 言{い}い出{だ}すタイミング見{み}つつ、まあ我{が}慢{まん}してたわけ",
              "maleVariant": "",
              "stringId": "1631704311361302528"
            }
          ]
        }
      }
    },
    "EmbeddedFiles": []
  }
