            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E53BCZ^失{しつ}礼{れい}、癖{くせ}で",
              "maleVariant": "",
              "stringId": 1877751644003483648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
