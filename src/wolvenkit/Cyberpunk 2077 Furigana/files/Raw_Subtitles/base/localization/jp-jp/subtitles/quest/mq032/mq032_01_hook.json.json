        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A6404Z^この腕{うで}も… 何{なに}もかも… もはや私{わたし}ではない！ 冷{つめ}たく、命{いのち}なき金{きん}属{ぞく}の塊{かい}だ！",
              "maleVariant": "",
              "stringId": 1770666438831390720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "91404Z^助{たす}けてくれ！ 誰{だれ}か助{たす}けてくれ！ お願{ねが}いだ！",
              "maleVariant": "",
              "stringId": 1770667555170566144
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしたの？",
              "maleVariant": "どうした？",
              "stringId": 1770667932674703364
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B8404Z^何{なに}があったの？ そのインプラント、未{み}完{かん}成{せい}じゃ…",
              "maleVariant": "B8404Z^何{なに}があった？ そのインプラント、未{み}完{かん}成{せい}じゃ…",
              "stringId": 1770668414818336768
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B3404Z^…未{み}完{かん}成{せい}かどうかは、問{もん}題{だい}ではないのだ！",
              "maleVariant": "",
              "stringId": 1770671627369988096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "82404Z^比丘{びく}はサイバーウェアによって体{からだ}を穢{けが}してはならない。波{なみ}羅{ら}提{てい}木{き}叉{さ}の誓{ちか}いに反{はん}するからだ！",
              "maleVariant": "",
              "stringId": 1770672013094961152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "81404004^弟{おとうと}は今{いま}どこに？",
              "maleVariant": "",
              "stringId": 1770672253596352516
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうしてそんなことに？",
              "maleVariant": "",
              "stringId": 1770672759513300992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "63404Z^意{い}味{み}がわからない。一{いっ}体{たい}どこの誰{だれ}が無{む}理{り}やりそんなことを？",
              "maleVariant": "",
              "stringId": 1770673292475121664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "メイルストロームね…",
              "maleVariant": "メイルストロームか…",
              "stringId": 1770674503437795328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CB404Z^あんたの弟{おとうと}は、まだそいつらと？",
              "maleVariant": "",
              "stringId": 1770674733033996288
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "47404Z^ああ。まず拙{せつ}僧{そう}を済{す}ませた後{あと}で、弟{おとうと}をどう… 改{かい}造{ぞう}すべきか話{はな}し合{あ}っていた",
              "maleVariant": "",
              "stringId": 1770675018582212608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "66404Z^手{て}を貸{か}すよ",
              "maleVariant": "",
              "stringId": 1770675508728578048
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "77404Z^その見{み}返{かえ}りは？",
              "maleVariant": "",
              "stringId": 1770675904150781952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B1404Z^見{み}返{かえ}りは？ タダで仕{し}事{ごと}はできない",
              "maleVariant": "",
              "stringId": 1770676197181636608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A3404Z^拙{せつ}僧{そう}に差{さ}し出{だ}せるは感{かん}謝{しゃ}の心{こころ}のみ。どうか、今{こん}回{かい}だけは例{れい}外{がい}としてくれまいか",
              "maleVariant": "",
              "stringId": 1770677021580476416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CE404Z^きっと天{てん}は見{み}てくださる",
              "maleVariant": "",
              "stringId": 1770677541992939520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "55404Z^関{かか}わりたくない",
              "maleVariant": "",
              "stringId": 1770678188502958080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A7404Z^悪{わる}いけど、メイルストロームに関{かか}わるのはご免{めん}よ",
              "maleVariant": "A7404Z^悪{わる}いが、メイルストロームに関{かか}わるのはご免{めん}だ",
              "stringId": 1770679856326000640
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BC404Z^ああ、無{む}理{り}を言{い}ってすまなかった。弟{おとうと}は人{にん}を傷{きず}つけることのない善{ぜん}人{にん}だ。きっと誰{だれ}かが救{すく}ってくれるだろう",
              "maleVariant": "",
              "stringId": 1770680238930411520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7A404004^わかったよ、手{て}を貸{か}す",
              "maleVariant": "",
              "stringId": 1770681337334743044
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "29404Z^わかったよ。人{にん}の心{こころ}に訴{うった}えかけるのが上{じょう}手{ず}ね。弟{おとうと}を助{たす}けてあげる",
              "maleVariant": "29404Z^わかったよ。人{にん}の心{こころ}に訴{うった}えかけるのが上手{うま}いな。弟{おとうと}を助{たす}けてやる",
              "stringId": 1770681735407747072
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "36404Z^あたしが何{なん}とかする。メイルストロームは危{き}険{けん}な連{れん}中{ちゅう}だから、穏{おん}便{びん}に済{す}ませられる保{ほ}証{しょう}はないよ",
              "maleVariant": "36404Z^俺{おれ}が何{なん}とかする。メイルストロームは危{き}険{けん}な連{れん}中{ちゅう}だから、穏{おん}便{びん}に済{す}ませられる保{ほ}証{しょう}はないが",
              "stringId": 1770682195187351552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5E404Z^感{かん}謝{しゃ}の言葉{ことば}もないよ。弟{おとうと}が囚{とら}われている場{ば}所{しょ}はここから北{きた}だ",
              "maleVariant": "",
              "stringId": 1770682883053207552
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C1404Z^波{は}止{と}場{ば}沿{そ}いの道{みち}付{ふ}近{きん}を探{さが}してくれ",
              "maleVariant": "",
              "stringId": 1770683984225779712
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "943BCZ^奴{やつ}らは… 拙{せつ}僧{そう}らが乞{こ}食{じき}をしているところに現{あらわ}れたんだ",
              "maleVariant": "",
              "stringId": 1825577848811536384
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "343BCZ^拙{せつ}僧{そう}らを笑{わら}い、インプラントの施{ほどこ}しも受{う}け取{と}るかと聞{き}いてきた",
              "maleVariant": "",
              "stringId": 1825583795730628608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C73BCZ^人{にん}というより… 機{き}械{かい}のような風{ふう}貌{ぼう}だったよ。拙{せつ}僧{そう}らも“改{かい}造{ぞう}”してやると言{い}ってな… 道{どう}具{ぐ}のように",
              "maleVariant": "",
              "stringId": 1825586100299350016
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2A3BCZ^あの手{て}の連{れん}中{ちゅう}と渡{わた}り合{あ}える方{ほう}とお見{み}受{う}けする。どうか、弟{おとうと}を救{すく}ってはくれないか！",
              "maleVariant": "",
              "stringId": 1825598467171139584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D13BCZ^まだ間{ま}に合{あ}うことを祈{いの}るばかりだ。弟{おとうと}には… 拙{せつ}僧{そう}のような目{め}に遭{あ}ってほしくない",
              "maleVariant": "",
              "stringId": 1825598633181691904
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3F3BCZ^誰{だれ}か！ どうか、救{すく}いの手{て}を！",
              "maleVariant": "",
              "stringId": 1825702311242743808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C23BCZ^ど、どうして私{わたし}がこんな目{め}に！ ヒンサの犠{ぎ}牲{せい}になるなんて！",
              "maleVariant": "",
              "stringId": 1834265666479964160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9B3BCZ^この、このかゆみは何{なん}だ？ なぜ感{かん}覚{かく}がある？ 私{わたし}は何{なに}を感{かん}じてるんだ？ 金{きん}属{ぞく}に感{かん}覚{かく}などないはずだろう！",
              "maleVariant": "",
              "stringId": 1834265790379704320
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "623BCZ^お、落{お}ち着{つ}け… 自{じ}分{ぶん}の魂{たましい}など構{かま}うな。あいつの魂{たましい}を救{すく}うんだ",
              "maleVariant": "",
              "stringId": 1834265978401964032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "373BCZ^ええと… 大{だい}丈{じょう}夫{ぶ}？",
              "maleVariant": "373BCZ^ええと… 大{だい}丈{じょう}夫{ぶ}か？",
              "stringId": 1834279627086610432
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "663BCZ^インプラントを着{つ}けた僧{そう}侶{りょ}？",
              "maleVariant": "",
              "stringId": 1834279709479518208
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E93BCZ^待{ま}って… 無{む}理{り}やりインプラントを？",
              "maleVariant": "E93BCZ^待{ま}て… 無{む}理{り}やりインプラントを？",
              "stringId": 1834309196627820544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "143BCZ^インプラントは… 信{しん}仰{こう}上{うえ}ダメじゃなかった？",
              "maleVariant": "143BCZ^インプラントは… 信{しん}仰{こう}上{うえ}ダメなんじゃ？",
              "stringId": 1834331093092515840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DD3BCZ^当{とう}然{ぜん}だ！ 自{みずか}らこんな姿{すがた}になったと思{おも}うのか！？",
              "maleVariant": "",
              "stringId": 1834331203838918656
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F53BCZ^ああ、お辞{じ}儀{ぎ}もできずに申{もう}し訳{わけ}ない。まだ… この状{じょう}況{きょう}に困{こん}惑{わく}していてな",
              "maleVariant": "",
              "stringId": 1834332123364573184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BE3BCZ^拙{せつ}僧{そう}だけではない… 弟{おとうと}もだ。奴{やつ}らは我{われ}らに薬{くすり}を盛{も}り… 拙{せつ}僧{そう}の体{からだ}を穢{けが}した",
              "maleVariant": "",
              "stringId": 1834332358665027584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7D3BCZ^誰{だれ}かー！",
              "maleVariant": "",
              "stringId": 1834400540180332544
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "103BCZ^連{れん}中{ちゅう}はあざ笑{わら}っていたよ… 同{おな}じ人{にん}間{げん}にこんな真似{まね}をして、どうして笑{わら}っていられるんだ？",
              "maleVariant": "",
              "stringId": 1837457554338988032
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6F3BCZ^奴{やつ}ら… それをわかっていながら！ もう取{と}り返{かえ}しがつかない…",
              "maleVariant": "",
              "stringId": 1853384054875602944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DD3BCZ^クズどもを皆{みな}殺{ころ}しにしてやる",
              "maleVariant": "",
              "stringId": 1853404389096275968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "393BCZ^クローム中{ちゅう}毒{どく}のイカれた連{れん}中{ちゅう}め… メイルストロームが相{あい}手{て}じゃ、穏{おん}便{びん}にはいかないよ",
              "maleVariant": "393BCZ^クローム中{ちゅう}毒{どく}のイカれた連{れん}中{ちゅう}め… メイルストロームが相{あい}手{て}じゃ、穏{おん}便{びん}にはいかないな",
              "stringId": 1853404626862981120
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "663BCZ^絶{ぜっ}対{たい}許{ゆる}さない…",
              "maleVariant": "663BCZ^絶{ぜっ}対{たい}許{ゆる}さねえ…",
              "stringId": 1853406581828075520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DE3BCZ^嘆{なげ}きの僧{そう}侶{りょ}なんてずいぶんとドラマチックだな。お前{まえ}の武{ぶ}勇{ゆう}伝{でん}に持{も}ってこいだ",
              "maleVariant": "",
              "stringId": 1896638530641969152
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "773BCZ^座{ざ}禅{ぜん}でもしながら連{れん}中{ちゅう}が死{し}に絶{た}えるのをじっと待{ま}ってみるか？",
              "maleVariant": "",
              "stringId": 1896639082964697088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7E3BCZ^いい加{か}減{げん}にして…",
              "maleVariant": "7E3BCZ^いい加{か}減{げん}にしろ…",
              "stringId": 1896639323600306176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7F3BCZ^他{ほか}に方{ほう}法{ほう}があるはずだ。助{たす}けてほしいが、頼{たの}む、殺生{せっしょう}だけは避{さ}けてくれ",
              "maleVariant": "",
              "stringId": 1915385687633018880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AE3BCZ^努{ど}力{りょく}はする… 何{なに}にせよ弟{おとうと}は助{たす}け出{だ}してみせるから",
              "maleVariant": "",
              "stringId": 1915394338485682176
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "353BCZ^力{ちから}を貸{か}してくれ… だが殺生{せっしょう}だけは、どうか避{さ}けてほしい",
              "maleVariant": "",
              "stringId": 1915396797471899648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "96404Z^[ジョニーに中{なか}指{ゆび}を立{た}てる]",
              "maleVariant": "",
              "stringId": 2121595052244353024
            }
          ]
        }
      }
