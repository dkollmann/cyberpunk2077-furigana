        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C95F9Z^常{つね}に鬼{おに}がいると、安{やす}らぎを見{み}いだすのは容{よう}易{い}ではない",
              "maleVariant": "",
              "stringId": "1815330641850372096"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "515F9Z^そなたは薄{うす}れていく。この世{せ}界{かい}に強{つよ}く根{ね}を下{くだ}ろしてはいない",
              "maleVariant": "",
              "stringId": "1815384163719548928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった、やってみる",
              "maleVariant": "",
              "stringId": "1815388204881121280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9A5F9Z^何{なん}だか知{し}らないけど、信{しん}用{よう}できない",
              "maleVariant": "9A5F9Z^何{なん}だか知{し}らないが、信{しん}用{よう}できない",
              "stringId": "1815389280250335232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6A5F9Z^何{なん}だか知{し}らないけど、信{しん}用{よう}できない",
              "maleVariant": "6A5F9Z^何{なん}だか知{し}らんが、信{しん}用{よう}できない",
              "stringId": "1815390271582474240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D45F9Z^仕{し}方{かた}ない。気{き}が変{か}わったら戻{もど}ってくるんだな",
              "maleVariant": "",
              "stringId": "1815396007142199296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "755F9Z^意{い}識{しき}を解{かい}放{ほう}してくれる、瞑{めい}想{そう}のブレインダンスをお渡{わた}ししよう",
              "maleVariant": "",
              "stringId": "1815396409275289600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "15F9Z^いくら払{はら}うかはそなた次{し}第{だい}だ",
              "maleVariant": "",
              "stringId": "1815406796855021568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "タダでもらう",
              "maleVariant": "",
              "stringId": "1815413424744534020"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F45F9Z^[支{し}払{はら}う] わかった",
              "maleVariant": "",
              "stringId": "1815413810318512128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "575F9Z^それなら、タダで頂{いただ}く",
              "maleVariant": "575F9Z^それなら、タダで頂{いただ}くよ",
              "stringId": "1815414374620172288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "これでどう？",
              "maleVariant": "これでどうだ",
              "stringId": "1815414465200361472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いいだろう",
              "maleVariant": "",
              "stringId": "1815416624193179648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A85F9Z^瞑{めい}想{そう}術{じゅつ}というものは、経{けい}験{けん}者{しゃ}の監{かん}督{とく}下{か}で学{まな}ぶのが肝{かん}要{よう}だ",
              "maleVariant": "",
              "stringId": "1815420380343406592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E25F9Z^そなたのことを待{ま}っていた。瞑{めい}想{そう}術{じゅつ}を身{み}に付{つ}けたいか？",
              "maleVariant": "",
              "stringId": "1815428503099641856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D15F9Z^[座{すわ}る] 気{き}が変{か}わった。やってみる",
              "maleVariant": "",
              "stringId": "1815429374692790272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、あんまり",
              "maleVariant": "",
              "stringId": "1815429621552746496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "145F9Z^そうね、気{き}が変{か}わった",
              "maleVariant": "145F9Z^そうだな、気{き}が変{か}わった",
              "stringId": "1815429839673331712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "いや、あんまり",
              "maleVariant": "",
              "stringId": "1815429908745129984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あの… え？",
              "maleVariant": "",
              "stringId": "1815434311388598272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A55F9Z^自{じ}身{しん}を浄{じょう}化{か}し、平{へい}静{せい}を取{と}り戻{もど}すのだ。私{わたし}が手{て}伝{つだ}ってやろう",
              "maleVariant": "",
              "stringId": "1815569402773344256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "595F9004^[座{すわ}る] 続{つづ}けて",
              "maleVariant": "595F9004^[座{すわ}る] 続{つづ}けてくれ",
              "stringId": "1815569921189318660"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C15F9008^この街{まち}で、そなたは自{し}然{ぜん}から切{き}り離{はな}されている",
              "maleVariant": "",
              "stringId": "1815570515639635976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C15F9010^たしかに禅寺{ぜんでら}はないね",
              "maleVariant": "C15F9010^たしかに禅寺{ぜんでら}はないな",
              "stringId": "1815570515639635984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C15F9018^静{しず}けさを取{と}り戻{もど}し、自{じ}身{しん}との絆{きずな}を回{かい}復{ふく}するのだ",
              "maleVariant": "",
              "stringId": "1815570515639635992"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "955F9Z^どうぞ… 続{つづ}けて",
              "maleVariant": "955F9Z^…続{つづ}けてくれ",
              "stringId": "1815678494674227200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5F9004^[立{た}つ] 姿{すがた}を消{け}す手品{てじな}？",
              "maleVariant": "5F9004^[立{た}つ] 姿{すがた}を消{け}す手品{てじな}か？",
              "stringId": "1815690273269714948"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A35F9Z^なんかの… 手品{てじな}？",
              "maleVariant": "A35F9Z^なんかの… 手品{てじな}か？",
              "stringId": "1815692633941446656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3A5F9Z^瞑{めい}想{そう}は、台{たい}風{ふう}の目{め}のような静{しず}けさをもたらす",
              "maleVariant": "",
              "stringId": "1816786727853854720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C15F9Z^さあ始{はじ}めるぞ。導{みちび}いてやろう",
              "maleVariant": "",
              "stringId": "1816811542144847872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだいたの？",
              "maleVariant": "まだいたのか？",
              "stringId": "1821684636852391940"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "まだいたの？",
              "maleVariant": "まだいたのか？",
              "stringId": "1821686137188487168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "642C5Z^座{すわ}ってくれ",
              "maleVariant": "",
              "stringId": "1918523800738877440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6E2C5Z^何{なに}かのドッキリ？ カメラ回{まわ}ってる？",
              "maleVariant": "6E2C5Z^何{なに}かのドッキリか？ カメラ回{まわ}ってる？",
              "stringId": "1918555450520653824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D22C5Z^何{なに}かのドッキリ？ どうせカメラが回{まわ}ってるんでしょ？",
              "maleVariant": "D22C5Z^何{なに}かのドッキリか？ どうせカメラが回{まわ}ってるんだろ？",
              "stringId": "1918556560299937792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BD2C5Z^自{じ}分{ぶん}自{じ}身{しん}から離{はな}れていく",
              "maleVariant": "",
              "stringId": "1918559158402830336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3B2C5Z^普{ふ}段{だん}は新{あたら}しいことに前{まえ}向{む}きなのに、ここにきて怖{こわ}くなったのか？",
              "maleVariant": "",
              "stringId": "1918564683844702208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7B2C5Z^…で、これは何{なに}？",
              "maleVariant": "7B2C5Z^…で、これは何{なに}なんだ？",
              "stringId": "1918565578271641600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "632C5Z^瞑{めい}想{そう}だ",
              "maleVariant": "",
              "stringId": "1918568326648057856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "632C5Z^つまり、座{ざ}禅{ぜん}みたいな？",
              "maleVariant": "",
              "stringId": "1918568562871259136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのとおり",
              "maleVariant": "",
              "stringId": "1918568884826034176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "522C5Z^金{きん}属{ぞく}、コンクリート、ネオン… 汚{よご}れた世{せ}界{かい}に囲{かこ}まれ、息{いき}が詰{つ}まっているのだ",
              "maleVariant": "",
              "stringId": "1918600942344491008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "912C5Z^そなたは根{こん}源{げん}。そこから不{ふ}要{よう}なものを抜{ぬ}き取{と}るだけでいい",
              "maleVariant": "",
              "stringId": "1918613218417987584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "282C5Z^準{じゅん}備{び}はいいか？",
              "maleVariant": "",
              "stringId": "1918621875310448640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D62C5Z^詳{くわ}しく説{せつ}明{めい}して。これに何{なん}の得{え}があるの？",
              "maleVariant": "D62C5Z^詳{くわ}しく説{せつ}明{めい}してくれ。これに何{なん}の得{え}がある？",
              "stringId": "1918626568333971456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "832C5Z^これに、何{なん}の得{え}があるっていうの？",
              "maleVariant": "832C5Z^これに、何{なん}の得{え}があるっていうんだ？",
              "stringId": "1918628113129689088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7E2C5Z^自{じ}己{こ}を満{み}たせるのだ。そうすれば、自{じ}分{ぶん}の声{こえ}だけが聞{き}こえるようになる",
              "maleVariant": "",
              "stringId": "1918635637828505600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "192C5Z^他{ほか}のすべてが沈{ちん}黙{もく}と化{か}すのだ",
              "maleVariant": "",
              "stringId": "1918645205321142272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "922C5Z^具{ぐ}体{たい}的{てき}に言{い}ってくれない？",
              "maleVariant": "922C5Z^具{ぐ}体{たい}的{てき}に言{い}ってくれないか？",
              "stringId": "1918649382059397120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D02C5Z^自{じ}分{ぶん}の人{じん}生{せい}から自{じ}分{ぶん}自{じ}身{しん}を追{お}い出{だ}すことはできまい",
              "maleVariant": "",
              "stringId": "1918649606437883904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やってみるとしましょ",
              "maleVariant": "やってみるとするか",
              "stringId": "1918654403815690240"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F02C5Z^「渡{わた}す」？",
              "maleVariant": "",
              "stringId": "1918655061583220736"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "322C5Z^金{かね}儲{もう}けでやってるわけじゃないんでしょ？",
              "maleVariant": "322C5Z^金{かね}儲{もう}けでやってるわけじゃないんだろ？",
              "stringId": "1918668905370112000"
            }
          ]
        }
      }
