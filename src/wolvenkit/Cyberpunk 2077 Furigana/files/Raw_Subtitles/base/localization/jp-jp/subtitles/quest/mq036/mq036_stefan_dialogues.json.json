            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BE2E3Z^優{やさ}しそう？",
              "maleVariant": "",
              "stringId": "1812839534861103104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "22E3Z^1時{じ}間{かん}やるから、ナイトシティを出{で}て二{に}度{ど}と戻{もど}ってこないで",
