            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A42E3004^[ブレインダンスを再{さい}生{せい}]",
              "maleVariant": "",
              "stringId": "1811460089713668100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AA2E3Z^金{きん}を返{かえ}して",
              "maleVariant": "AA2E3Z^金{きん}を返{かえ}せ",
              "stringId": "1812826847192166400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D92E3Z^おい、あんた… 大{だい}丈{じょう}夫{ぶ}か？ あのバーチャを見{み}始{はじ}めたら、いきなりグリッチしだしてさ…",
              "maleVariant": "",
              "stringId": "1812827560945266688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ED2E3Z^下手{へた}な芝{しば}居{い}はやめて。わかってて見{み}せたんでしょ。騙{だま}したのね",
              "maleVariant": "ED2E3Z^下手{へた}な芝{しば}居{い}はやめろ。わかってて見{み}せたんだろ。騙{だま}したんだな",
              "stringId": "1812828261360480256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FA2E3Z^おい待{ま}ってくれ… とりあえず落{お}ち着{つ}こう！ な？",
              "maleVariant": "",
              "stringId": "1812829326730473472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FD2E3004^今{こん}回{かい}だけは見{み}逃{のが}してやる",
              "maleVariant": "",
              "stringId": "1812836649700044804"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2E3Z^ナイトシティを出{で}て、二{に}度{ど}と戻{もど}ってこないで",
