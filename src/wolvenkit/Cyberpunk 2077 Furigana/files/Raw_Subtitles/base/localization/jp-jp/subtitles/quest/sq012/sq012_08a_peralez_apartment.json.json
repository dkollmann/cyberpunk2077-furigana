            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "542B6Z^その情{じょう}報{ほう}で十{じゅう}分{ぶん}よ。ありがとう",
              "maleVariant": "",
              "stringId": "1941844188692439040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AB2B6Z^よかったわ。これで… 一{ひと}息{いき}つける",
              "maleVariant": "",
              "stringId": "1941846015513157632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D22B6Z^一{ひと}つだけ言{い}っておく",
              "maleVariant": "",
              "stringId": "1944687764098998272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8C2B6004^何{なに}？",
              "maleVariant": "8C2B6004^何{なん}だ？",
              "stringId": "1944698410148519940"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C12B6Z^一{ひと}つ以{い}上{じょう}になりそうな気{き}がする",
              "maleVariant": "C12B6Z^一{ひと}つ以{い}上{じょう}になりそうな気{き}がするな",
              "stringId": "1944698956498558976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "412B6Z^一{ひと}つで済{す}む気{き}がしないけど、どうぞ",
              "maleVariant": "412B6Z^一{ひと}つで済{す}む気{き}がしないが、どうぞ",
