          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C949DZ^V。どうぞ、入{いっ}って",
              "maleVariant": "",
              "stringId": "1699884261689643008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CC49DZ^よく来{き}てくれた",
              "maleVariant": "",
              "stringId": "1699885739208724480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1049D004^何{なに}があったのか、だいたいわかった",
              "maleVariant": "",
              "stringId": "1699885852018724868"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F049DZ^いかんせん謎{なぞ}の多{おお}い事{じ}件{けん}だった",
              "maleVariant": "",
              "stringId": "1699885967445970944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうでしょうね。それで――？",
              "maleVariant": "",
              "stringId": "1699891785969029120"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E549DZ^ラインの死{し}は事{じ}故{こ}よ――それも恥{は}ずかしいヤツ。セックスクラブで死{し}んだの。張{は}り切{き}りすぎてね",
              "maleVariant": "E549DZ^ラインの死{し}は事{じ}故{こ}だ――それも恥{は}ずかしいヤツ。セックスクラブで死{し}んだんだ。張{は}り切{き}りすぎてな",
              "stringId": "1750453383196168192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BD49DZ^つまり――彼{かれ}が死{し}んで得{え}する者{もの}が関{かん}与{よ}した可{か}能{のう}性{せい}はないと？",
              "maleVariant": "",
              "stringId": "1750454344597753856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8749DZ^ご苦{く}労{ろう}だった、V",
              "maleVariant": "",
              "stringId": "1750454902037532672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FE49DZ^とにかく複{ふく}雑{ざつ}な事{じ}件{けん}で、不{ふ}可{か}解{かい}な点{てん}が多{おお}いの。まだ結{けつ}論{ろん}は出{だ}したくない",
              "maleVariant": "FE49DZ^とにかく複{ふく}雑{ざつ}な事{じ}件{けん}で、不{ふ}可{か}解{かい}な点{てん}が多{おお}いんだ。まだ結{けつ}論{ろん}は出{だ}したくない",
              "stringId": "1750455526804279296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6E49DZ^別{べつ}に推{すい}測{そく}でも構{かま}わないさ",
              "maleVariant": "",
              "stringId": "1750456241647898624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1249DZ^ラインの死{し}にはウェルドン・ホルトが関{かん}与{よ}してた",
              "maleVariant": "",
              "stringId": "1750456772680339456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E649DZ^ある程{てい}度{ど}はわかった",
              "maleVariant": "",
              "stringId": "1750457338877825024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CC49DZ^ラインはセックスクラブで殺{ころ}された。ウイルス入{い}りBDを渡{わた}されてね",
              "maleVariant": "CC49DZ^ラインはセックスクラブで殺{ころ}された。ウイルス入{い}りBDを渡{わた}されてな",
              "stringId": "1750459893947158528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A649DZ^確{たし}かなのか？",
              "maleVariant": "",
              "stringId": "1750460447360405504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4A2B6Z^どうぞ、座{すわ}って",
              "maleVariant": "",
              "stringId": "1941764551241064448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C72B6Z^報{ほう}告{こく}があるようだな。ぜひ聞{き}かせてくれ",
              "maleVariant": "",
              "stringId": "1941767194743103488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1B2B6004^ラインの死{し}は事{じ}故{こ}で、NCPDがもみ消{け}したの",
              "maleVariant": "1B2B6004^ラインの死{し}は事{じ}故{こ}で、NCPDがもみ消{け}したんだ",
              "stringId": "1941780506256039940"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "322B6Z^ホルトが関{かん}与{よ}してた",
              "maleVariant": "",
              "stringId": "1941781073577598976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "292B6Z^ラインはBDによって殺{ころ}された",
              "maleVariant": "",
              "stringId": "1941785909559779328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C82B6Z^あたしの見{けん}解{かい}ではね",
              "maleVariant": "C82B6Z^俺{おれ}の見{けん}解{かい}ではな",
              "stringId": "1941811175224991744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9C2B6Z^よかった。一{ひと}安{あん}心{しん}ね…",
              "maleVariant": "",
              "stringId": "1941811552443916288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D22B6Z^だとしてらウソをつく必{ひつ}要{よう}なんて…？ いや、まあ冷{れい}静{せい}に考{かんが}えれば当{とう}然{ぜん}か",
              "maleVariant": "",
              "stringId": "1941811789573087232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D62B6Z^明{めい}確{かく}な結{けつ}論{ろん}は出{だ}せない",
              "maleVariant": "",
              "stringId": "1941814770347499520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B2B6Z^ジェフ、真{しん}意{い}を汲{く}んであげて",
              "maleVariant": "",
              "stringId": "1941817073339162624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5F2B6Z^「複{ふく}雑{ざつ}な事{じ}件{けん}」ってVは言{い}ったけど、心{しん}不{ふ}全{ぜん}で亡{な}くなるなんて、ちっとも“複{ふく}雑{ざつ}”じゃないわ",
              "maleVariant": "",
              "stringId": "1941817731878445056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ではやはり…？",
              "maleVariant": "",
              "stringId": "1941818565974515712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "292B6Z^ええ。この情{じょう}報{ほう}だけで十{じゅう}分{ぶん}よ。ジェフ、これからはさらに注{ちゅう}意{い}していかないと",
              "maleVariant": "",
              "stringId": "1941818783239462912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1C2B6Z^Vはそれを伝{つた}えようとしたのよ",
              "maleVariant": "",
              "stringId": "1941819401496649728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "52B6Z^妻{つま}の言{い}うとおり、これで安{あん}心{しん}して眠{ねむ}れるな",
              "maleVariant": "",
              "stringId": "1941820337413644288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D2B6Z^報{ほう}酬{しゅう}だ",
              "maleVariant": "",
              "stringId": "1941830039878983680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E12B6Z^ええ。あたしも同{おな}じBDにやられそうになったから",
              "maleVariant": "E12B6Z^ああ。俺{おれ}も同{おな}じBDにやられそうになったからな",
              "stringId": "1941831168717185024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7E2B6Z^犯{はん}人{にん}の正{しょう}体{たい}は？",
              "maleVariant": "",
              "stringId": "1941831691042250752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ウェルドン・ホルト",
              "maleVariant": "",
              "stringId": "1941832675680280580"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わからない",
              "maleVariant": "",
              "stringId": "1941833239075971072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "572B6Z^警{けい}察{さつ}が調{しら}べてる",
              "maleVariant": "",
              "stringId": "1941833391194988544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5D2B6Z^確{たし}かなことは言{い}えないけど、ウェルドン・ホルトが怪{あや}しい",
              "maleVariant": "5D2B6Z^確{たし}かなことは言{い}えないが… ウェルドン・ホルトが怪{あや}しい",
              "stringId": "1941833855152119808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それはわからなかった",
              "maleVariant": "",
              "stringId": "1941833873338621952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C32B6Z^警{けい}察{さつ}が調{しら}べてる。ラインの死{し}に関{かん}する捜{そう}査{さ}を再{さい}開{かい}するみたいよ",
              "maleVariant": "C32B6Z^警{けい}察{さつ}が調{しら}べてる。ラインの死{し}に関{かん}する捜{そう}査{さ}を再{さい}開{かい}するみたいだ",
              "stringId": "1941833891223134208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9B2B6010^やはりな！ 証{しょう}拠{こ}は？",
              "maleVariant": "",
              "stringId": "1941839358045413392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9C2B6Z^ないけど… そうとしか思{おも}えないの",
