            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "212B6Z^油{ゆ}断{だん}なんかしない",
              "maleVariant": "",
              "stringId": "1944719969089183744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B92B6Z^報{ほう}告{こく}しないと、報{ほう}酬{しゅう}をもらえない",
              "maleVariant": "",
              "stringId": "1944720005999058944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E42B6Z^ふん、好{す}きにしやがれ。どうなっても知{し}らねえからな",
              "maleVariant": "",
