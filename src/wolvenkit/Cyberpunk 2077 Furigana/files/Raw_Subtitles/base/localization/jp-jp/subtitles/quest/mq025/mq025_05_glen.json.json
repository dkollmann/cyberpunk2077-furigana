      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F22D7004^仕{し}事{ごと}を探{さが}してるって言{い}ってたじゃない",
              "maleVariant": "",
              "stringId": 1853325216020525060
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F42D7Z^戦{たたか}いに来{き}た",
              "maleVariant": "",
              "stringId": 1853325216054079488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FA2D7Z^シーザーね？ 戦{たたか}いに来{き}た",
              "maleVariant": "FA2D7Z^シーザーだな？ 戦{たたか}いに来{き}た",
              "stringId": 1853325216154742784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FF2D7004^いかにも。グレン最{さい}強{きょう}の男{おとこ}だ",
              "maleVariant": "",
              "stringId": 1853325216238628868
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FF2D700C^ついでにナイトシティ最{さい}悪{あく}の父{ちち}親{おや}ね",
              "maleVariant": "",
              "stringId": 1853325216238628876
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "12D7Z^わかった、同{どう}額{がく}を出{だ}す",
              "maleVariant": "",
              "stringId": 1853325216272183296
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "12D7020^そんな額{ひたい}は賭{か}けられない",
              "maleVariant": "",
              "stringId": 1853325216272183328
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "22D7008^どんな車{くるま}？",
              "maleVariant": "22D7008^どんな車{くるま}だ？",
              "stringId": 1853325216288960520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "42D7004^そんな額{ひたい}、今{いま}すぐは無{む}理{り}ね",
              "maleVariant": "42D7004^そんな額{ひたい}、今{いま}すぐは無{む}理{り}だ",
              "stringId": 1853325216322514948
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "42D700C^ああ、そうか… まあ、気{き}が変{か}わったら言{い}ってくれよな",
              "maleVariant": "",
              "stringId": 1853325216322514956
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "82D7004^同{どう}額{がく}を出{だ}す。それでいい？",
              "maleVariant": "82D7004^同{どう}額{がく}を出{だ}す。それでいいか？",
              "stringId": 1853325216389623812
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D2D7Z^素{す}手{で}だけだ。いいな？ モノワイヤーやマンティスブレードはナシだぞ？",
              "maleVariant": "",
              "stringId": 1853325216473509888
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "102D7004^気{き}をつけろ。手{て}加{か}減{げん}しないぞ",
              "maleVariant": "",
              "stringId": 1853325216523841540
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "192D7004^やめて！ もうやめてよ！ 十{じゅう}分{ぶん}でしょ！？",
              "maleVariant": "",
              "stringId": 1853325216674836484
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1C2D7Z^ミカエラ… ごめん… 俺{おれ}――",
              "maleVariant": "",
              "stringId": 1853325216725168128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1C2D7008^もう聞{き}きたくない、このバカ。キーを渡{わた}したら家{いえ}に帰{かえ}るよ",
              "maleVariant": "",
              "stringId": 1853325216725168136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1E2D7004^この辺{へん}でやめとくか？",
              "maleVariant": "",
              "stringId": 1853325216758722564
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1E2D700C^よし… 再{さい}戦{せん}ならいつでも受{う}けてやるぜ。エディーはいくらあっても、困{こま}らないからな",
              "maleVariant": "",
              "stringId": 1853325216758722572
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "282D7Z^[試{し}合{あい}を開{かい}始{し}する] いいよ",
              "maleVariant": "282D7Z^[試{し}合{あい}を開{かい}始{し}する] いいぞ",
              "stringId": 1853325216926494720
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2A2D7004^ええ、試{し}合{あい}開{かい}始{し}ね",
              "maleVariant": "2A2D7004^ああ、試{し}合{あい}開{かい}始{し}だな",
              "stringId": 1853325216960049156
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2C2D7004^先{さき}に教{おし}えて。どんな車{くるま}なの？",
              "maleVariant": "2C2D7004^先{さき}に教{おし}えてくれ。どんな車{くるま}だ？",
              "stringId": 1853325216993603588
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2C2D700C^フルカスタムの2056年{ねん}モデルだ。なのにまだ2万{まん}マイルしか走{はし}ってない。へこみも継{つ}ぎはぎもないぞ",
              "maleVariant": "",
              "stringId": 1853325216993603596
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2D2D7004^へえ。盗{とう}難{なん}車{しゃ}？",
              "maleVariant": "2D2D7004^へえ。盗{とう}難{なん}車{しゃ}か？",
              "stringId": 1853325217010380804
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし。やろうか",
              "maleVariant": "",
              "stringId": 1853325217027158020
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "142D7Z^見{み}つけただろ！",
              "maleVariant": "",
              "stringId": 1853337697765912576
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "912D7Z^こんなの仕{し}事{ごと}じゃない。あなたが死{し}んだら、お金{きん}が全{ぜん}部{ぶ}なくなったら、私{わたし}たちがどうなるか考{かんが}えたことある？",
              "maleVariant": "",
              "stringId": 1853337966151036928
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シーザー、もうちょっとちゃんとしてよ…",
              "maleVariant": "",
              "stringId": 1853338688292745216
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CF2D7Z^今{いま}すぐ大{たい}金{きん}が必{ひつ}要{よう}なんだ。だから大{おお}きな賭{か}けしかするつもりはない",
              "maleVariant": "",
              "stringId": 1853342056000090112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2D7Z^ミカエラ、頼{たの}むよ… 大{だい}丈{じょう}夫{ぶ}だから――",
              "maleVariant": "",
              "stringId": 1853342756901842944
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "892D7Z^あなたとは話{はな}してない。クロ－ムに大{たい}金{きん}つぎこんだかと思{おも}えば、次{つぎ}は… ああもう、やってらんないわ",
              "maleVariant": "",
              "stringId": 1853342789265092608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "322D7Z^こっちは車{くるま}を賭{か}ける。それに見{み}合{あ}う金{きん}を出{だ}すつもりがないなら、忘{わす}れてくれ",
              "maleVariant": "",
              "stringId": 1853343621029130240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4D2D7Z^いやいや、まさか。別{べつ}にやましい車{くるま}じゃないって",
              "maleVariant": "",
              "stringId": 1853355857843941376
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CE2D7004^いい車{くるま}をありがとう",
              "maleVariant": "",
              "stringId": 1853392040812703748
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9F2D7Z^エディーはもらうけど、車{くるま}は取{と}っておいて",
              "maleVariant": "9F2D7Z^エディーはもらうが、車{くるま}は取{と}っておけ",
              "stringId": 1853392460930969600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C62D7Z^どうも。乗{の}り回{まわ}すのが楽{たの}しみよ",
              "maleVariant": "C62D7Z^どうも。乗{の}り回{まわ}すのが楽{たの}しみだ",
              "stringId": 1853392981276323840
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3C2D7Z^ミエルダ… いい車{くるま}だったのに…",
              "maleVariant": "",
              "stringId": 1853393262428909568
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E2D7Z^エディーはもらうけど、車{くるま}は取{と}っておいて",
              "maleVariant": "E2D7Z^エディーはもらう。だが車{くるま}は取{と}っておけ",
              "stringId": 1853394404118458368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "902D7Z^え… 本{ほん}気{き}なのか？",
              "maleVariant": "",
              "stringId": 1853395192278511616
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BD2D7Z^どんな名{な}前{まえ}にしよう？",
              "maleVariant": "",
              "stringId": 1853403160197820416
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "742D7Z^ねえ、白{はく}馬{ば}の王子様{おうじさま}… どうやって家{いえ}に帰{かえ}るわけ？ 歩{ある}くっての！？",
              "maleVariant": "",
              "stringId": 1853404198355169280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "90404Z^エディーも車{くるま}も取{と}っておいて",
              "maleVariant": "90404Z^エディーも車{くるま}も取{と}っておけ",
              "stringId": 1862161572718198784
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "30404Z^全{ぜん}部{ぶ}取{と}っておきなよ。こっちは何{なに}もいらないから",
              "maleVariant": "30404Z^全{ぜん}部{ぶ}取{と}っておけって。こっちは何{なに}もいらないから",
              "stringId": 1862162730748755968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2644DZ^ミカエラ！ ミカエラ、待{ま}ってくれよ！",
              "maleVariant": "",
              "stringId": 1908167778851147776
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E044DZ^なあ… この条{じょう}件{けん}が気{き}に入{い}らないなら断{た}ってくれ。こちとら、時{じ}間{かん}も金{きん}も余{よ}裕{ゆう}がないんだ",
              "maleVariant": "",
              "stringId": 1908174881552650240
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E344DZ^うっ… ほら。大{だい}事{じ}に乗{の}れよ",
              "maleVariant": "",
              "stringId": 1908188269016043520
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ええ… そうね",
              "maleVariant": "ああ… そうだな",
              "stringId": 1908189732324823040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1B44DZ^こんな冗{じょう}談{だん}は言{い}わない",
              "maleVariant": "",
              "stringId": 1908198852754984960
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AE44DZ^さあ、気{き}が変{か}わる前{まえ}にエディーは渡{わた}してよ",
              "maleVariant": "AE44DZ^さあ、気{き}が変{か}わる前{まえ}にエディーは渡{わた}してくれ",
              "stringId": 1908200371344691200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8F44DZ^あのさ… その… なんてお礼{れい}言{い}っていいのか…",
              "maleVariant": "",
              "stringId": 1908200482493747200
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A244DZ^別{べつ}にお礼{れい}はいらない",
              "maleVariant": "",
              "stringId": 1908201560849305600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5F44DZ^残{のこ}ったものを大{たい}切{せつ}にするのね",
              "maleVariant": "5F44DZ^残{のこ}ったものを大{たい}切{せつ}にするんだな",
              "stringId": 1908201718639022080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8B44DZ^気{き}にしないで",
              "maleVariant": "8B44DZ^気{き}にするな",
              "stringId": 1908203471723876352
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DC44DZ^とにかく残{のこ}ったものを大{たい}切{せつ}にして",
              "maleVariant": "DC44DZ^とにかく残{のこ}ったものを大{たい}切{せつ}にしろ",
              "stringId": 1908203576162045952
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふう… ラッキーだったな",
              "maleVariant": "",
              "stringId": 1908203741283405824
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのつもりだ",
              "maleVariant": "",
              "stringId": 1908205221637836800
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EA44DZ^愚{おろ}か者{もの}は運{うん}だけはいいのよね",
              "maleVariant": "",
              "stringId": 1908206784737497088
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BD44DZ^うーん… “V”が付{つ}く名{な}前{まえ}は？",
              "maleVariant": "",
              "stringId": 1908208811207086080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "544DZ^何{なに}か考{かんが}えるから。とにかく落{お}ち着{つ}けよ！",
              "maleVariant": "",
              "stringId": 1908210066545496064
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F244DZ^この人{にん}にわからせてやってくれる？ 負{ま}けないと理{り}解{かい}できないのよ",
              "maleVariant": "",
              "stringId": 1932970634292547584
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかった",
              "maleVariant": "",
              "stringId": 1932975713745686528
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A944DZ^前{まえ}に戦{たたか}った奴{やつ}のロングスパーズには驚{おどろ}かされたからな。とにかく… 準{じゅん}備{び}はいいか？",
              "maleVariant": "",
              "stringId": 1932976908015030272
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "93404Z^エディーはいらない。車{くるま}だけでいい",
              "maleVariant": "",
              "stringId": 2096711922941444096
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FF3BCZ^ありがとう。でも賭{か}けたエディーは別{べつ}にいらない",
              "maleVariant": "FF3BCZ^ありがとう。だが賭{か}けたエディーは別{べつ}にいらない",
              "stringId": 2252011877682626560
            }
          ]
        }
      }
