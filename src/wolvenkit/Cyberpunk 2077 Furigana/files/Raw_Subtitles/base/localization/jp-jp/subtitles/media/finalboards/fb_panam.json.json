          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DA4EA004^もしもし、V。死{し}後{ご}の世{せ}界{かい}がホントにあればいいのにね… なんでかわかる？ ねえ？",
              "maleVariant": "",
              "stringId": "1790650011999969284"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DC4EA004^V、今{こん}夜{や}はキャンプまで帰{かえ}れそうにない。砂{すな}嵐{あらし}が近{ちか}付{づ}いてて、ここでやり過{す}ごすことにしたんだ",
              "maleVariant": "",
              "stringId": "1790650012033523716"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DF4EA004^もしもし、V！ 調{ちょう}子{し}はどう？",
              "maleVariant": "",
              "stringId": "1790650012083855364"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E14EA004^もしもし、V！ 元{げん}気{き}にしてる？",
              "maleVariant": "",
              "stringId": "1790650012117409796"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E34EAZ^もしもし、V？ パナムよ、覚{おぼ}えてる？ ほら、車{くるま}の件{けん}の",
              "maleVariant": "",
              "stringId": "1790650012150964224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "544EAZ^何{なに}なのよ！このままずっと私{わたし}を避{さ}け続{つづ}けるつもり？ この卑{ひ}怯{きょう}者{もの}！",
              "maleVariant": "",
              "stringId": "1790652273199587328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A644DZ^そしたらいつかまた会{あ}えるから。その時{とき}は、あんたを思{おも}いっきりぶん殴{なぐ}ってやる！",
              "maleVariant": "",
              "stringId": "1790824550007885824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E744DZ^いったい何{なに}考{かんが}えてたの？ それとも何{なに}も考{かんが}えてなかった？ どうなのよ！",
              "maleVariant": "",
              "stringId": "1790825019249840128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E544DZ^あんたのためを思{おも}って、色々{いろいろ}やってあげたのに！ このバカ！ その恩{おん}を返{かえ}すどころか、命{いのち}を絶{た}つなんて",
              "maleVariant": "",
              "stringId": "1790825272619356160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1644DZ^最{さい}低{てい}だよ。地{じ}獄{ごく}に落{お}ちて、せいぜい苦{くる}しめばいい。それでも私{わたし}はあんたを許{ゆる}せない。さよなら",
              "maleVariant": "",
              "stringId": "1790825582679085056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6E44DZ^一{いち}応{おう}伝{つた}えとこうと思{おも}って。その… ほら… 前{まえ}も一{いち}度{ど}こっちの都{つ}合{ごう}でドタキャンしたことがあったからさ",
              "maleVariant": "",
              "stringId": "1790827757408931840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3E44DZ^リーダーがこんなに難{むずか}しいとはね。見{み}てるのとやるのとじゃ大{おお}違{ちが}い",
              "maleVariant": "",
              "stringId": "1790828040071467008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "444DZ^V… あんたがいくれて本{ほん}当{とう}によかった。私{わたし}がどれだけ救{すく}われてるか… 言{こと}葉{ば}だけじゃ――",
              "maleVariant": "",
              "stringId": "1790828210897080320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DD44DZ^砂{すな}嵐{あらし}が来{く}るぞ！ 電{でん}気{き}はもってあと30秒{びょう}だ！",
              "maleVariant": "",
              "stringId": "1790828472235773952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1644DZ^行{い}かなきゃ。埋{う}め合{あ}わせは絶{ぜっ}対{たい}するから。約{やく}束{そく}する",
              "maleVariant": "",
              "stringId": "1790828782429720576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DE44DZ^バッドランズは今日{きょう}も、クーラーボックスが溶{と}けるほどいい天{てん}気{き}… 日{ひ}陰{かげ}でもクソ暑{あつ}いし、砂{すな}嵐{あらし}も向{む}かって来{き}てる… あんたもいればよかったのにね",
              "maleVariant": "",
              "stringId": "1790830220304240640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6E44DZ^冗{じょう}談{だん}はさておき… こっちはうまくやってるよ。仲{なか}間{ま}は増{ふ}え続{つづ}ける一方{いっぽう}で、仕{し}事{ごと}は山{やま}積{づ}み――",
              "maleVariant": "",
              "stringId": "1790830433173557248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1544DZ^たまにはゆっくり眠{ねむ}りたいって思{おも}うけど… 何{なに}もかも思{おも}い通{とう}りにはならないもんね。そうでしょ、V？",
              "maleVariant": "",
              "stringId": "1790830719443193856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7C44DZ^都{と}会{かい}に飽{あ}きたら連{れん}絡{らく}して。ながーいドライブに連{つ}れてってあげるからさ",
              "maleVariant": "",
              "stringId": "1790831326761635840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B744DZ^じゃあ… 元{げん}気{き}でね、V",
              "maleVariant": "",
              "stringId": "1790831735773384704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7344DZ^バッドランズは今日{きょう}も、クーラーボックスが溶{と}けるほどいい天{てん}気{き}… 日{ひ}陰{かげ}でもクソ暑{あつ}いし、砂{すな}嵐{あらし}も向{む}かって来{き}てる… あんたもいればよかったのに！",
              "maleVariant": "",
              "stringId": "1790832778309586944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F444DZ^冗{じょう}談{だん}はさておき… こっちはうまくやってるよ。ソウルとはそれなりに… 仲{なか}良{よ}くしてるし。仲{なか}間{ま}は増{ふ}え続{つづ}ける一方{いっぽう}で、仕{し}事{ごと}は山{やま}積{づ}み。でも忙{いそが}しい方{ほう}がいいもんね",
              "maleVariant": "",
              "stringId": "1790833008107114496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "344DZ^あんたがいなかったら、私{わたし}はまだナイトシティでくすぶってた…",
              "maleVariant": "",
              "stringId": "1790833304711516160"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A544DZ^私{わたし}のためにしてくれたこと、すべてに感{かん}謝{しゃ}してる。あんたのおかげで私{わたし}は今{いま}ここにいるんだ",
              "maleVariant": "",
              "stringId": "1790833509292888064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9C44DZ^それじゃ… また近{ちか}いうちにね！ ちゃんと連{れん}絡{らく}してよ",
              "maleVariant": "",
              "stringId": "1790833835559407616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E344DZ^あのさ、仕{し}事{ごと}を紹{しょう}介{かい}してもらうことってできないかな？",
              "maleVariant": "",
              "stringId": "1790835125240778752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E344DZ^金{きん}さえ入{い}るなら、内{ない}容{よう}は選{えら}ばない",
              "maleVariant": "",
              "stringId": "1790838277746774016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C044DZ^仕{し}事{ごと}でも、依{い}頼{らい}人{にん}でも、なんでもいいから… もし情{じょう}報{ほう}があったら連{れん}絡{らく}ちょうだい。ほんと、頼{たの}むね",
              "maleVariant": "",
              "stringId": "1790838577807282176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B044DZ^じゃ、そんな感{かん}じで… よろしく。またそのうちアフターライフで会{あ}えるといいね",
              "maleVariant": "",
              "stringId": "1790838861006688256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6044DZ^借{か}り物{もの}の人{じん}生{せい}をせいぜい謳{おう}歌{か}してな、ジョニー。どうせ長{なが}くは続{つづ}かない。この私{わたし}がいる限{かぎ}りね",
              "maleVariant": "",
              "stringId": "1790839770197577728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1F44DZ^サイコ野{や}郎{ろう}め！ どこに隠{かく}れたって絶{ぜっ}対{たい}見{み}つけ出{だ}してやるから。覚{かく}悟{ご}しな",
              "maleVariant": "",
              "stringId": "1790840190013853696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F44DZ^そんであんたの頭{あたま}からVを引{ひ}き抜{ぬ}いてやる。方{ほう}法{ほう}なんかどうでもいい、やると言{い}ったらやるんだ",
              "maleVariant": "",
              "stringId": "1790840468918292480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1044DZ^あんたなんてただの寄{き}生{せい}虫{ちゅう}よ、ジョニー。私{わたし}が絶{ぜっ}対{たい}に駆{く}除{じょ}してやる！ 聞{き}こえた！？",
              "maleVariant": "",
              "stringId": "1790840687978401792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6F44DZ^少{すく}ない残{のこ}り時{じ}間{かん}を楽{たの}しむんだね",
              "maleVariant": "",
              "stringId": "1790841003104849920"
            }
          ]
        }
      }
