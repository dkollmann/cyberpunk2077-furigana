      "cookingPlatform": "PLATFORM_PC",
      "root": {
        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1D2E3Z^金{きん}を出{だ}せ！ さっさとしねえとブッ殺{ころ}すぞ！",
              "maleVariant": "",
              "stringId": "1177805824054931456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DD2E3Z^ちょっ… 待{ま}ってくれ、落{お}ち着{つ}いて…！",
              "maleVariant": "",
              "stringId": "1177810650524430336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B32E3Z^早{はや}くしろ！ アタマ吹{ふ}っ飛{と}ばされてえのか！",
              "maleVariant": "",
              "stringId": "1177812419346313216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ああ、わかってる",
              "maleVariant": "",
              "stringId": "1203656761373487104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D729FZ^いいか。出{だ}し惜{お}しみはなしだ。アドレナリンが出{で}れば出{で}るほど、BDの値{ね}段{だん}も上{あ}がるからな",
              "maleVariant": "",
              "stringId": "1203657093595918336"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2129FZ^余{よ}計{けい}なことはしなくていい。中{なか}に入{いっ}って現{げん}金{きん}を奪{うば}い、外{そと}に出{で}る。あとはスタジオのサイコ野{や}郎{ろう}どもにこのBDを売{う}るだけだ",
              "maleVariant": "",
              "stringId": "1203772998824882176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2C2E3Z^はいよ、今{こん}日{にち}はカレー味{あじ}とサーフアンドターフ味{あじ}がお買{か}い得{どく}だ。どうだい？",
              "maleVariant": "",
              "stringId": "1451721074460733440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D629FZ^え、賞{しょう}味{み}期{き}限{げん}切{き}れとかじゃないでしょうね？",
              "maleVariant": "",
              "stringId": "1466084306943864832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D75AEZ^記{き}録{ろく}者{しゃ}の感{かん}覚{かく}配{はい}列{れつ}からVを切{き}り離{はな}すよ。そうすれば、場{ば}面{めん}全{ぜん}体{たい}を自{じ}由{ゆう}に観{かん}察{さつ}できるようになる",
              "maleVariant": "",
              "stringId": "1560656617783615488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D75AE018^分{ぶん}析{せき}モード中{なか}はカメラを自{じ}由{ゆう}自{じ}在{ざい}に動{うご}かせる。移{い}動{どう}もズームも、すべてが思{おも}うがまま… 自{じ}分{ぶん}だけのサンドボックスってわけ",
              "maleVariant": "",
              "stringId": "1560656617783615512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D75AE024^じゃ、色々{いろいろ}調{しら}べてみて",
              "maleVariant": "",
              "stringId": "1560656617783615524"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "185AE018^それが… 記{き}録{ろく}者{しゃ}の知{ち}覚{かく}が及{およ}んでいた限{げん}界{かい}ラインってところね。エディターでもそれより先{さき}に行{い}くことはできない。わかる？",
              "maleVariant": "",
              "stringId": "1560658646098698264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "185AE030^よし、じゃあ強{ごう}盗{とう}たちのシーンに戻{もど}るよ",
              "maleVariant": "",
              "stringId": "1560658646098698288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B85AEZ^分{ぶん}析{せき}モードでは再{さい}生{せい}速{そく}度{ど}を調{ちょう}節{せつ}できる。必{ひつ}要{よう}なら一{いち}時{じ}停{てい}止{し}も可{か}能{のう}よ",
              "maleVariant": "",
              "stringId": "1560659640920498176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B95AE008^コンソールを使{つか}って再{さい}開{かい}できるから、やってみて",
              "maleVariant": "",
              "stringId": "1560659640937275400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "175AEZ^すごいでしょ？ でもこれだけじゃない。早{はや}送{おく}りも早{そう}戻{もど}しも、好{す}きなようにできるんだから",
              "maleVariant": "",
              "stringId": "1560663989021237248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "175AE00C^さっそく、最{さい}初{しょ}のとこまで早{そう}戻{もど}ししてみて",
              "maleVariant": "",
              "stringId": "1560663989021237260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "715AEZ^うん、OK。じゃ、今{こん}度{ど}は早{はや}送{おく}りして",
              "maleVariant": "",
              "stringId": "1560664471567523840"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A25AEZ^OK。ちなみにリセットして、一{いち}番{ばん}最{さい}初{しょ}のとこに戻{もど}ることもできるの。やってみて",
              "maleVariant": "",
              "stringId": "1560664983490715648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5A5AEZ^簡{かん}単{たん}でしょ？ じゃあ、準{じゅん}備{び}ができたらリセットして。次{つぎ}に進{すす}もう",
              "maleVariant": "",
              "stringId": "1560666163398762496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7C5AEZ^さ、こっからが本{ほん}番{ばん}。そもそもの目{もく}的{てき}はこれだからね",
              "maleVariant": "",
              "stringId": "1560671309340008448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7C5AE010^分{ぶん}析{せき}モードでは、記{き}録{ろく}者{しゃ}がBDに取{と}り込{こ}んだ環{かん}境{きょう}を探{たん}索{さく}したり、スキャンしたりできるの",
              "maleVariant": "",
              "stringId": "1560671309340008464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7D5AE014^銃{じゅう}にフォーカスを。最{さい}初{しょ}のシーンで相{あい}棒{ぼう}から受{う}け取{と}ったヤツ。それをスキャンして",
              "maleVariant": "",
              "stringId": "1560671309356785684"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "355AEZ^上{じょう}出{で}来{き}よ",
              "maleVariant": "",
              "stringId": "1560671956688887808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "355AE010^続{つづ}けましょ",
              "maleVariant": "",
              "stringId": "1560671956688887824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6A5AEZ^さて… 分{ぶん}析{せき}モードでは、記{き}録{ろく}者{しゃ}の近{ちか}くで発{はっ}生{せい}したノイズや、会{かい}話{わ}を拾{ひろ}うことができるの",
              "maleVariant": "",
              "stringId": "1560674757896757248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6A5AE00C^BDは記{き}録{ろく}者{しゃ}本{ほん}人{にん}が意{い}識{しき}してなかった光{こう}景{けい}や音{おと}まで、あらゆる細{さい}部{ぶ}を記{き}録{ろく}してくれる",
              "maleVariant": "",
              "stringId": "1560674757896757260"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6B5AE00C^エディター内{ない}で音{おと}レイヤーに切{き}り替{か}えると、各{かく}信{しん}号{ごう}の発{はっ}信{しん}元{もと}が視{し}覚{かく}化{か}されるはず。さっそくやってみて",
              "maleVariant": "",
              "stringId": "1560674757913534476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C65AEZ^OK。店{てん}内{ない}にいくつかの音{おん}声{せい}パターンが表{ひょう}示{じ}されてるでしょ。一{ひと}つを選{えら}んでフォーカスしてみて",
              "maleVariant": "",
              "stringId": "1560675927671365632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "875AEZ^ご感{かん}想{そう}は？",
              "maleVariant": "",
              "stringId": "1560678452055171072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "885AE008^信{しん}じられない。本{ほん}当{とう}にすぐ隣{となり}で起{お}きてるみたい",
              "maleVariant": "885AE008^信{しん}じられない。本{ほん}当{とう}にすぐ隣{となり}で起{お}きてるみたいだ",
              "stringId": "1560678452071948296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "885AE014^これがBDレコーダーインプラントの力{ちから}よ。あらゆる背{はい}景{けい}情{じょう}報{ほう}を記{き}録{ろく}し、エディターの機{き}能{のう}で細{さい}部{ぶ}を引{ひ}き立{た}たせてやるの",
              "maleVariant": "",
              "stringId": "1560678452071948308"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "885AE020^そのまま音{おと}の分{ぶん}析{せき}を続{つづ}けてみて。飽{あ}きたら次{つぎ}に行{い}くよ",
              "maleVariant": "",
              "stringId": "1560678452071948320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8B5AEZ^じゃ、男{おとこ}がカウンターの客{きゃく}を殴{なぐ}るまで待{ま}って",
              "maleVariant": "",
              "stringId": "1560679289640902656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "875AEZ^スキャンは人{にん}間{げん}にも使{つか}える。そこのケガしてる子{こ}に近{ちか}づいて、スキャンしてみて",
              "maleVariant": "",
              "stringId": "1560680178632024064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "125AE010^それじゃ、次{つぎ}。強{ごう}盗{とう}が痛{いた}い目{め}に遭{あ}う場{ば}面{めん}まで移{い}動{どう}して",
              "maleVariant": "",
              "stringId": "1560680919698432016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "95AEZ^わかる？ 彼{かれ}はこの時{とき}、自{じ}分{ぶん}がどう撃{う}たれたか見{み}てない",
              "maleVariant": "",
              "stringId": "1560681443533447168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A5AE004^でもあなたは見{み}れちゃうの。どう？ ワクワクするでしょ",
              "maleVariant": "",
              "stringId": "1560681443550224388"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B35AEZ^入口{いりぐち}の上{うえ}の方{ほう}で点{てん}滅{めつ}してるのが見{み}える？ 監{かん}視{し}カメラよ",
              "maleVariant": "",
              "stringId": "1560684169394839552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B45AE008^映{えい}像{ぞう}は店{てん}員{いん}の後{うし}ろの画{が}面{めん}に送{おく}られてる",
              "maleVariant": "",
              "stringId": "1560684169411616776"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DE5AEZ^OK、これでエディターが不{ふ}足{そく}データを補{ほ}完{かん}して、発砲{はっぽう}した人{じん}物{ぶつ}を特{とく}定{てい}できる。再{さい}開{かい}して",
              "maleVariant": "",
              "stringId": "1560685578865532928"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "695AEZ^撃{う}ったのは彼{か}の相{あい}棒{ぼう}。きっと最{さい}初{しょ}からそのつもりだったのね。この手{て}のBDは、ブラックマーケットで高{たか}く売{う}れるから",
              "maleVariant": "",
              "stringId": "1560697181904232448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "695AE010^リアルな死{し}の体{たい}験{けん}は、マニアが飛{と}びつくのよ",
              "maleVariant": "",
              "stringId": "1560697181904232464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "695AE01C^さて… もう十{じゅう}分{ぶん}なら、いつでも終{しゅう}了{りょう}して",
              "maleVariant": "",
              "stringId": "1560697181904232476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B62E3Z^煙草{たばこ}とブローセフのパック、あとザッパー二{ふた}つ",
              "maleVariant": "",
              "stringId": "1660754145775071232"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "162E3Z^クソ！ 何{なに}なんだよ…",
              "maleVariant": "",
              "stringId": "1661875906944053248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E22E3Z^何{なに}が「補{ほ}充{じゅう}に行{い}け。レジは俺{おれ}がやる」だよ。ったく、ふざけやがって…",
              "maleVariant": "",
              "stringId": "1661884246898126848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C02E3Z^生{なま}データには、記{き}録{ろく}者{しゃ}のサイバーウェアが取{しゅ}得{とく}した別{べつ}のレイヤーが含{ふく}まれてることもある",
              "maleVariant": "",
              "stringId": "1662182858223923200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "というと？",
              "maleVariant": "",
              "stringId": "1662189239622578176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "212E3Z^キロシの赤{せき}外{がい}線{せん}検{けん}知{ち}ウェアを着{つ}けてるエヴリンの記{き}録{ろく}なら、熱{ねつ}パターンの分{ぶん}析{せき}ができるってこと",
              "maleVariant": "",
              "stringId": "1662189504165720064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "なるほどね",
              "maleVariant": "なるほどな",
              "stringId": "1663182014853541888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "272E3Z^全{ぜん}員{いん}伏{ふ}せろ！ さっさとしやがれ！ 床{とこ}にキスするんだよ！",
              "maleVariant": "",
              "stringId": "1793872359748153344"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8629FZ^何{なに}見{み}てんだ、クソアマ！",
              "maleVariant": "",
              "stringId": "1945715303684632576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1A29FZ^撃{う}った奴{やつ}を捉{とら}えてるはず、確{たし}かめてみて",
              "maleVariant": "",
              "stringId": "1968861151213776896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よし、ここだね",
              "maleVariant": "",
              "stringId": "1970527589012221952"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D529FZ^強{ごう}盗{とう}の視{し}界{かい}に画{が}面{めん}が入{い}るところまで戻{もど}して、スキャンしてみて",
              "maleVariant": "",
              "stringId": "1979152407093440512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "242FZ^よし、行{い}け",
              "maleVariant": "",
              "stringId": "2231624522677809152"
            }
          ]
        }
      }
    },
