        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8C386Z^防{ぼう}衛{えい}網{もう}を突破{とっぱ}した",
              "maleVariant": "",
              "stringId": "1511333766146580480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョニー…",
              "maleVariant": "",
              "stringId": "1511334303017492480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BC386Z^コアへの道{みち}は開{ひら}けたが、時{じ}間{かん}があまりない",
              "maleVariant": "",
              "stringId": "1511339406243946496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "17386Z^この先{さき}どうなるかはわからない。でもね、ここまで来{こ}れたのはあんたのおかげ",
              "maleVariant": "17386Z^この先{さき}どうなるかはわからない。だが、ここまで来{こ}れたのはお前{まえ}のおかげだ",
              "stringId": "1511360496060096512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "27386Z^聞{き}こえてるかわからないけど…",
              "maleVariant": "27386Z^聞{き}こえてるかわからないが…",
              "stringId": "1511361608725061632"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "73386Z^むしろその方{ほう}がいいか。今{こん}回{かい}ばかりは途{と}中{ちゅう}で口{くち}を挟{はさ}まれたくないから",
              "maleVariant": "73386Z^むしろその方{ほう}がいいか。今{こん}回{かい}ばかりは途{と}中{ちゅう}で口{くち}を挟{はさ}まれたくないからな",
              "stringId": "1512980104001118208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D1386Z^ムカつく皮{ひ}肉{にく}野{や}郎{ろう}のね…",
              "maleVariant": "D1386Z^ムカつく皮{ひ}肉{にく}野{や}郎{ろう}のな…",
              "stringId": "1512980814247780352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2386Z^ありがとう。感{かん}謝{しゃ}してるよ",
              "maleVariant": "2386Z^ありがとう。感{かん}謝{しゃ}してる",
              "stringId": "1512981485084762112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "パナム？",
              "maleVariant": "",
              "stringId": "1792153176280932352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "アダム・スマッシャー",
              "maleVariant": "",
              "stringId": "1792153903908790272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグ…",
              "maleVariant": "",
              "stringId": "1792154242909216768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5F502Z^いいか… このクソボーグ野{や}郎{ろう}",
              "maleVariant": "",
              "stringId": "1792174083376422912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "62386Z^いつも喧{けん}嘩{か}ばっかだったけど… さみしくなるよ、ソウル",
              "maleVariant": "",
              "stringId": "1792421297447788544"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "97386Z^覚{おぼ}えてるよ。シルヴァーハンドを殺{ころ}すと誓{ちか}った奴{やつ}ね",
              "maleVariant": "97386Z^覚{おぼ}えてるぞ。シルヴァーハンドを殺{ころ}すと誓{ちか}った奴{やつ}だな",
              "stringId": "1792425430095519744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3B386004^シルヴァーハンドがよろしく言{い}ってた",
              "maleVariant": "",
              "stringId": "1792427163718803460"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1A386Z^ソウルは立派{りっぱ}な男{おとこ}だった",
              "maleVariant": "",
              "stringId": "1792427721510903808"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "48386004^お前{まえ}を殺{ころ}すべきか考{かんが}えてる",
              "maleVariant": "",
              "stringId": "1792427975685726212"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ローグはやっとケリをつけられた",
              "maleVariant": "",
              "stringId": "1792428076382576640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1E386Z^いいこと教{おし}えてあげる",
              "maleVariant": "1E386Z^いいことを教{おし}えてやる",
              "stringId": "1792428571981537280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "83386Z^あんたが殺{ころ}した男{おとこ}は、立派{りっぱ}な奴{やつ}だったのよ",
              "maleVariant": "83386Z^あんたが殺{ころ}した男{おとこ}は、立派{りっぱ}な奴{やつ}だった",
              "stringId": "1792428659575382016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "75386Z^50年{ねん}前{まえ}は、美{うつく}しい世{せ}界{かい}を守{まも}るために手{て}を汚{よご}した",
              "maleVariant": "",
              "stringId": "1792428801074421760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F0386Z^ローグはお前{まえ}を憎{にく}んでた… そしてようやくケリをつけた",
              "maleVariant": "",
              "stringId": "1792428871857496064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AB386Z^自{じ}分{ぶん}で望{のぞ}んだわけでもないのに、ここまで来{き}て、命{いのち}を捧{ささ}げたのよ。仲{なか}間{ま}のため、ファミリーのために",
              "maleVariant": "AB386Z^自{じ}分{ぶん}で望{のぞ}んだわけでもないのに、ここまで来{き}て、命{いのち}を捧{ささ}げたんだ。仲{なか}間{ま}のため、ファミリーのために",
              "stringId": "1792429553599668224"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "あんたにはわかりっこない",
              "maleVariant": "AB386Z^お前{まえ}にはわからないさ",
              "stringId": "1792430803435151360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BE386Z^今{いま}ここに、ジョニーがいるの。話{はな}すことはできないけど、あたしの中{なか}に存{そん}在{ざい}してる",
              "maleVariant": "BE386Z^今{いま}ここに、ジョニーがいるんだ。話{はな}すことはできないが、俺{おれ}の中{なか}に存{そん}在{ざい}してる",
              "stringId": "1792435485268271104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F386Z^最{さい}後{ご}に笑{わら}うのは、あんたじゃなく、シルヴァーハンドだってこと",
              "maleVariant": "F386Z^最{さい}後{ご}に笑{わら}うのは、お前{まえ}じゃなく、シルヴァーハンドだってことさ",
              "stringId": "1792436981275844608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7E386Z^ローグは、俺{おれ}やお前{まえ}なんかとはまるで違{ちが}う。生{い}きなきゃならねえ奴{やつ}だったんだ",
              "maleVariant": "",
              "stringId": "1792442369027104768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DF386Z^お前{まえ}にはわかりっこない",
              "maleVariant": "",
              "stringId": "1792443702094356480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DF386Z^今{いま}俺{おれ}が殺{ころ}すのは、その美{うつく}しい思{おも}い出{で}を守{まも}るためだけだ",
              "maleVariant": "",
              "stringId": "1792445712139051008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "クソ、クソ、クソ！",
              "maleVariant": "",
              "stringId": "1841442353290174464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "よくもあいつらを！ このクソボーグ！",
              "maleVariant": "",
              "stringId": "1841443441846280192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CD386Z^覚{かく}悟{ご}の上{うえ}で来{き}たのだろ",
              "maleVariant": "",
              "stringId": "1841447423213854720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2B386Z^お前{まえ}の宿{しゅく}敵{てき}、シルヴァーハンドを覚{おぼ}えてるか？ 殺{ころ}したくてたまらなかったんだよな。この俺{おれ}がそうだ",
              "maleVariant": "",
              "stringId": "1841448309554176000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "わかってねえな、スマッシャー。あいつなりのけじめをつけたんだよ",
              "maleVariant": "",
              "stringId": "1841449782476300288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B9386Z^いったい、何{なに}を言{い}ってるんだ？",
              "maleVariant": "",
              "stringId": "1995048870861103104"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C1386Z^無{む}謀{ぼう}だとわかってたはずだ",
              "maleVariant": "",
              "stringId": "1995076448980328448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6F386Z^お前{まえ}は変{か}わらず、腰{こし}抜{ぬ}けだ",
              "maleVariant": "",
              "stringId": "1995167848803622912"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E1386Z^くだらん。あの老{お}いぼれはもう死{し}んだんだ",
              "maleVariant": "",
              "stringId": "1995168576565698560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "シルヴァーハンド？ そんなことがあってたまるか",
              "maleVariant": "",
              "stringId": "1995324830177058816"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B3386Z^運{うん}命{めい}の再{さい}会{かい}だな。ただ、あの頃{ごろ}の俺{おれ}なら迷{まよ}わずとどめを刺{さ}してたが…",
              "maleVariant": "",
              "stringId": "1995329907650486272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "41386Z^わかってねえな。俺{おれ}たちはみんな変{か}わったんだよ。過{か}去{こ}の決{けつ}意{い}なんて今{いま}さら何{なん}の意{い}味{み}もない",
              "maleVariant": "",
              "stringId": "1995333152733159424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AF386Z^許{ゆる}さねえ、よくもローグを",
              "maleVariant": "",
              "stringId": "1995412551344087040"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CB386004^スマッシャー、覚{おぼ}えてるよ",
              "maleVariant": "CB386004^スマッシャー、覚{おぼ}えてるぞ",
              "stringId": "1995413380742537220"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3E386Z^大{だい}丈{じょう}夫{ぶ}？",
              "maleVariant": "3E386Z^大{だい}丈{じょう}夫{ぶ}か？",
              "stringId": "1995643429710225408"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "73386004^[拾{ひろ}う] じゃあな、ローグ",
              "maleVariant": "",
              "stringId": "1995644293887844356"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FB386Z^スマッシャー、このクソ野{や}郎{ろう}…",
              "maleVariant": "",
              "stringId": "1995645722098688000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "92386Z^いつも… 一{いち}番{ばん}かっこよかったぜ",
              "maleVariant": "",
              "stringId": "2020243204199702528"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7757Z^わからずやで… 家{か}族{ぞく}のためなら自{みずか}らの身{み}も顧{かえり}みない",
              "maleVariant": "",
              "stringId": "2228711302282702848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "87757Z^最{さい}後{ご}まであんたらしかった",
              "maleVariant": "",
              "stringId": "2228715324519575552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B9757Z^親父{おやじ}はいつも言{い}ってた。無{む}人{じん}島{しま}にひとつだけ持{も}っていけるものがあるとすれば、ローグにするってな",
              "maleVariant": "",
              "stringId": "2229942650917974016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "22757Z^この世{せ}代{だい}の連{れん}中{ちゅう}には… 逆{さか}立{だ}ちしても敵{かな}わねえ",
              "maleVariant": "",
              "stringId": "2230130368520220672"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FE757Z^これで終{お}わりだと思{おも}うな！ この決{けつ}断{だん}を後{こう}悔{かい}させてやる！",
              "maleVariant": "",
              "stringId": "2262373572170575872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "どうだかね",
              "maleVariant": "どうだかな",
              "stringId": "2262373785761312768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7F757Z^所{しょ}詮{せん}人{にん}間{げん}か",
              "maleVariant": "",
              "stringId": "2262439708241260544"
            }
          ]
        }
      }
