            "Properties": {
              "entries": [
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "15405Z^ずっと弾{ひ}いてると指{ゆび}が痛{いた}くなる。痛{いた}くない弾{ひ}き方{かた}ってある？",
                    "maleVariant": null,
                    "stringId": 1264623330770571264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "どこへだ！？",
                    "maleVariant": null,
                    "stringId": 1264632307822645248
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "72405Z^おい、そんなムキになるなよ、チューマ。スティーブにはちょっと仕{し}事{ごと}を頼{たの}んでるだけだ。2時{じ}間{かん}もかからない",
                    "maleVariant": null,
                    "stringId": 1264658413623726080
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "92405Z^ないな。指{ゆび}で鉄線{てっせん}を絞{しぼ}って音{おと}を出{だ}してんだ。痛{いた}みばっかりはどうしようもない",
                    "maleVariant": null,
                    "stringId": 1264669263247986688
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "そっか…",
                    "maleVariant": null,
                    "stringId": 1264669306315100160
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "3B405Z^こいつがどこで何{なに}するかは、俺{おれ}の決{き}めることだ。お前{まえ}じゃねえ",
                    "maleVariant": null,
                    "stringId": 1264680617681899520
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "562B6Z^タダ働{はたら}きさせるわけじゃない",
                    "maleVariant": null,
                    "stringId": 1315354781185433600
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "9E2B6Z^あー、ちょっと… その辺{へん}だよ。すぐ戻{もど}る",
                    "maleVariant": null,
                    "stringId": 1315402121522929664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "132B6Z^うちの中{なか}に戻{もど}れ！ 今{いま}すぐだ！ 隙{げき}ありゃフラフラ遊{あそ}びに行{い}きやがって！",
                    "maleVariant": null,
                    "stringId": 1315402527212789760
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "572B6Z^また車{くるま}の鍵{かぎ}を持{も}ち出{だ}したな？",
                    "maleVariant": null,
                    "stringId": 1315403791074025472
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "752B6Z^バカ野{や}郎{ろう}！ うちの中{なか}に戻{もど}れ！",
                    "maleVariant": null,
                    "stringId": 1315404697815441408
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "482B6Z^車{くるま}が要{い}るならてめえで買{か}え！ 俺{おれ}の息子{むすこ}には近{ちか}寄{よ}るな！",
                    "maleVariant": null,
                    "stringId": 1315405818046930944
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "742B6Z^おい、お前{まえ}に言{い}ってんだよ！",
                    "maleVariant": null,
                    "stringId": 1315408705003151360
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "462B6Z^なにジロジロ見{み}てる？ 文{もん}句{く}でもあんのか？",
                    "maleVariant": null,
                    "stringId": 1315409662009106432
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "C42B6Z^うちの中{なか}に戻{もど}れっつってんだよ！ 聞{き}こえねえのか！？",
                    "maleVariant": null,
                    "stringId": 1315409694187806720
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "F72B6Z^心{しん}配{ぱい}すんな。ずっと続{つづ}けてりゃ、そのうち指{ゆび}の皮{かわ}が分{ふん}厚{あつ}くなって、いくら弾{ひ}いても痛{いた}まなくなる",
                    "maleVariant": null,
                    "stringId": 1315428532770004992
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "8A2B6Z^コーティング弦{げん}を買{か}うつもりでさ",
                    "maleVariant": null,
                    "stringId": 1315434007024590848
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "1B2B6Z^クズの分際{ぶんざい}で、そいつに手{て}をだすな",
                    "maleVariant": null,
                    "stringId": 1316813986744459264
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "BF2B6Z^何{なん}の用{よう}だ？",
                    "maleVariant": null,
                    "stringId": 1319638694997221376
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "2C2B6Z^お金{きん}を貯{た}めてたんだけど、母{かあ}さんが家{いえ}のことで困{こま}ってたから渡{わた}しちゃった",
                    "maleVariant": null,
                    "stringId": 1319750816947200000
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "762B6Z^この野{や}郎{ろう}、人{にん}ん家{いえ}の前{まえ}でエラそうにしやがって！ お前{まえ}みたいなクズにびびるとでも思{おも}ってんのか！？",
                    "maleVariant": null,
                    "stringId": 1695406297327558656
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "432B6Z^2時{じ}間{かん}だぞ。帰{かえ}ってきたらとっとと俺{おれ}に金{きん}を寄{より}こせよ、スティーブ",
                    "maleVariant": null,
                    "stringId": 1695414508449390592
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "わかったよ",
                    "maleVariant": null,
                    "stringId": 1695416261450358784
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "C6610Z^知{し}るかよ。どうせホームレスかなんかだろ",
                    "maleVariant": null,
                    "stringId": 1793855822500003840
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "スパイかもな",
                    "maleVariant": null,
                    "stringId": 1793896368266346496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "42610Z^じゃあ捕{つか}まえてみろよ、親父{おやじ}。できないくせに",
                    "maleVariant": null,
                    "stringId": 1793969344860979200
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "E7610Z^立{た}ってるのもやっとなんだから",
                    "maleVariant": null,
                    "stringId": 1793973015531290624
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "38610Z^酔{よ}っ払{はら}いと一緒{いっしょ}のゴミ捨{す}て場{ば}じゃ、スティーブが家{いえ}に居{い}たがらないのも無{む}理{り}ないな",
                    "maleVariant": null,
                    "stringId": 1793997365059846144
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "15610Z^はあ？ お前{まえ}んちはマシだってのか？",
                    "maleVariant": null,
                    "stringId": 1794001126863994880
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "2D610Z^息子{むすこ}を引{ひ}き留{と}めたきゃ、まともな家{か}庭{てい}を築{きづ}け",
                    "maleVariant": null,
                    "stringId": 1794132823848845312
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "39610Z^そいつに手{て}をだすな",
                    "maleVariant": null,
                    "stringId": 1794133700223500288
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "48610Z^落{お}ち着{つ}け。金{きん}なら払{はら}う",
                    "maleVariant": null,
                    "stringId": 1794134164331626496
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "5C610Z^おい、いったい何{なに}してんだ？",
                    "maleVariant": null,
                    "stringId": 1795642290893553664
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "99610Z^出{で}かけんだ",
