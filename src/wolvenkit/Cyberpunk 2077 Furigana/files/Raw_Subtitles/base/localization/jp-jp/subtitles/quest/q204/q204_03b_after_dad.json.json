        "entries": [
          {
            "stringId": 1264746220086120448,
            "femaleVariant": "親父{おやじ}の奴{やつ}、あんたに完{カン}全{ゼン}にビビってた…"
          },
          {
            "stringId": 1264753264050589696,
            "femaleVariant": "悪{わる}いことをしたな"
          },
          {
            "stringId": 1308595607224471552,
            "femaleVariant": "乗{の}って"
          },
          {
            "stringId": 1315470759160406016,
            "femaleVariant": "え？ 逆{ぎゃく}だよ！"
          },
          {
            "stringId": 1316854673573634048,
            "femaleVariant": "俺{おれ}は… 親父{おやじ}が痛{いた}い目{め}に遭{あ}えばいいって思{おも}ってる"
          },
          {
            "stringId": 1677951527939981312,
            "femaleVariant": "お金{きん}なんていらないよ"
          },
          {
            "stringId": 1695447352097857536,
            "femaleVariant": "わかってる。だがお前{まえ}の親父{おやじ}にゃ必要{ひつよう}だろ"
          },
          {
            "stringId": 1794042101942517764,
            "femaleVariant": "心配{しんぱい}するな。向{む}こうも忘{わす}れてる"
          },
          {
            "stringId": 1794049458768510980,
            "femaleVariant": "こんな調子{ちょうし}じゃ息子{むすこ}を失{うしな}うぞ"
          },
          {
            "stringId": 1794054569813147648,
            "femaleVariant": "惨{さん}めな奴{やつ}め"
          },
          {
            "stringId": 1794060208417341440,
            "femaleVariant": "なんて惨{さん}めな野{ヤ}郎{ロウ}だ"
          },
          {
            "stringId": 1794061687161159680,
            "femaleVariant": "あいつは俺{おれ}の子{こ}だ。お前{まえ}にとやかく言{い}われる覚{おぼ}えはねえんだよ！"
          },
          {
            "stringId": 1794063439038054400,
            "femaleVariant": "うるせえ！ てめえだって似{に}たようなもんだろ！"
          },
          {
            "stringId": 1794064221409968128,
            "femaleVariant": "心配{しんぱい}するな。あんな様子{ようす}じゃ、帰{かえ}って来{く}る頃{ごろ}には全{ゼン}部{ブ}すっかり忘{わす}れてるさ"
          },
          {
            "stringId": 1794065261412810752,
            "femaleVariant": "口答{こうとう}えしたのは失敗{しっぱい}だったな"
          },
          {
            "stringId": 1794068307836469248,
            "femaleVariant": "今{いま}はあんまり口答{こうとう}えせずに、黙{だま}っていうとおり従{したが}っとけ。親{おや}の力{ちから}は必要{ひつよう}だ"
          },
          {
            "stringId": 1794076104527511552,
            "femaleVariant": "ふう… だといいけどね"
          },
          {
            "stringId": 1794076676026597376,
            "femaleVariant": "言{い}うのは簡{カン}単{タン}さ"
          },
          {
            "stringId": 1794080857143705600,
            "femaleVariant": "親父{おやじ}に大口{おおぐち}叩{たた}いたけど――"
          },
          {
            "stringId": 1795385583214854144,
            "femaleVariant": "うちはゴミ捨{す}て場{ば}なんかじゃない。俺{おれ}が掃除{そうじ}してるし"
          },
          {
            "stringId": 1795392326263177216,
            "femaleVariant": "言葉{ことば}のアヤってやつだ、本{ホン}気{キ}にすんじゃねえ"
          },
          {
            "stringId": 1806762269081296896,
            "femaleVariant": "いいじゃないか、スティーブもそのうち出{で}ていくんだ… そうすりゃ好{す}きなだけ飲{の}める。その調子{ちょうし}で続{つづ}けりゃいい"
          },
          {
            "stringId": 1806762680156643328,
            "femaleVariant": "今{いま}に心{こころ}の底{そこ}から嫌{きら}われるぞ。そしたら二{ニ}度{ド}と息子{むすこ}には会{あ}えなくなる"
          },
          {
            "stringId": 1806763542253887488,
            "femaleVariant": "はあ… もう嫌{きら}われてるさ"
          },
          {
            "stringId": 1806765141743329280,
            "femaleVariant": "いずれ息子{むすこ}がいたらと悲{かな}しむ日{にち}がきっとくる。その時{とき}には手遅{ておく}れだがな"
          },
          {
            "stringId": 1806789170978054144,
            "femaleVariant": "きっと、また後{あと}で叱{しか}られる"
          },
          {
            "stringId": 1806817998244896768,
            "femaleVariant": "もう少{すこ}し大人{おとな}になるまで待{ま}てばいい"
          },
          {
            "stringId": 1806822453317165056,
            "femaleVariant": "だとしても、俺{おれ}から言{い}えるのはそれだけだ"
          },
          {
            "stringId": 1806835620512489472,
            "femaleVariant": "だろうね。でも俺{おれ}は自{ジ}分{ブン}で稼{かせ}げてるんだ"
          },
          {
            "stringId": 2237622305589780480,
            "femaleVariant": "負け犬{まけいぬ}が偉{えら}そうに"
          },
          {
            "stringId": 2237623692360900608,
            "femaleVariant": "苦{ク}労{ロウ}して育{そだ}ててやったってのに、得体{えたい}の知{し}れねえクズと遊{あそ}び歩{ある}きやがって。恩知{おんぢ}らずが"
          },
          {
            "stringId": 2237624672049651712,
            "femaleVariant": "きつく叱{しか}ってやらねえとな。優{やさ}しくしすぎたんだ"
          },
          {
            "stringId": 2237626464594518016,
            "femaleVariant": "クソ… 酒{さけ}がいる。あいつのせいで肝{カン}臓{ゾウ}がやられちまうよ"
          },
          {
            "stringId": 2512618004766851076,
            "femaleVariant": "[乗{の}る]"
          }
        ]
