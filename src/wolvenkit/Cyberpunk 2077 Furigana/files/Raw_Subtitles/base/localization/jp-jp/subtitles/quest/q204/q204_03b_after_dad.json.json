        "HandleId": "0",
        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7B405Z^親父{おやじ}の奴{やつ}、あんたに完{かん}全{ぜん}にビビってた…",
              "maleVariant": "",
              "stringId": "1264746220086120448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "88405Z^悪{わる}いことをしたな",
              "maleVariant": "",
              "stringId": "1264753264050589696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "83405Z^乗{の}って",
              "maleVariant": "",
              "stringId": "1308595607224471552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "902B6Z^え？ 逆{ぎゃく}だよ！",
              "maleVariant": "",
              "stringId": "1315470759160406016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3F2B6Z^俺{おれ}は… 親父{おやじ}が痛{いた}い目{め}に遭{あ}えばいいって思{おも}ってる",
              "maleVariant": "",
              "stringId": "1316854673573634048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "452FCZ^お金{きん}なんていらないよ",
              "maleVariant": "",
              "stringId": "1677951527939981312"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "452B6Z^わかってる。だがお前{まえ}の親父{おやじ}にゃ必{ひつ}要{よう}だろ",
              "maleVariant": "",
              "stringId": "1695447352097857536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "56610004^心{しん}配{ぱい}するな。向{む}こうも忘{わす}れてる",
              "maleVariant": "",
              "stringId": "1794042101942517764"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3B610004^こんな調{ちょう}子{し}じゃ息子{むすこ}を失{うしな}うぞ",
              "maleVariant": "",
              "stringId": "1794049458768510980"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3D610Z^惨{さん}めな奴{やつ}め",
              "maleVariant": "",
              "stringId": "1794054569813147648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "14610Z^なんて惨{さん}めな野{や}郎{ろう}だ",
              "maleVariant": "",
              "stringId": "1794060208417341440"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "60610Z^あいつは俺{おれ}の子{こ}だ。お前{まえ}にとやかく言{い}われる覚{おぼ}えはねえんだよ！",
              "maleVariant": "",
              "stringId": "1794061687161159680"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "44610Z^うるせえ！ てめえだって似{に}たようなもんだろ！",
              "maleVariant": "",
              "stringId": "1794063439038054400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6D610Z^心{しん}配{ぱい}するな。あんな様{よう}子{す}じゃ、帰{かえ}って来{く}る頃{ごろ}には全{ぜん}部{ぶ}すっかり忘{わす}れてるさ",
              "maleVariant": "",
              "stringId": "1794064221409968128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "92610Z^口{こう}答{とう}えしたのは失敗{しっぱい}だったな",
              "maleVariant": "",
              "stringId": "1794065261412810752"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DF610Z^今{いま}はあんまり口{こう}答{とう}えせずに、黙{だま}っていうとおり従{したが}っとけ。親{おや}の力{ちから}は必{ひつ}要{よう}だ",
              "maleVariant": "",
              "stringId": "1794068307836469248"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ふう… だといいけどね",
              "maleVariant": "",
              "stringId": "1794076104527511552"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3E610Z^言{い}うのは簡{かん}単{たん}さ",
              "maleVariant": "",
              "stringId": "1794076676026597376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BC610Z^親父{おやじ}に大口{おおぐち}叩{たた}いたけど――",
              "maleVariant": "",
              "stringId": "1794080857143705600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F2610Z^うちはゴミ捨{す}て場{ば}なんかじゃない。俺{おれ}が掃{そう}除{じ}してるし",
              "maleVariant": "",
              "stringId": "1795385583214854144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EF610Z^言{こと}葉{ば}のアヤってやつだ、本{ほん}気{き}にすんじゃねえ",
              "maleVariant": "",
              "stringId": "1795392326263177216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A2B6Z^いいじゃないか、スティーブもそのうち出{で}ていくんだ… そうすりゃ好{す}きなだけ飲{の}める。その調{ちょう}子{し}で続{つづ}けりゃいい",
              "maleVariant": "",
              "stringId": "1806762269081296896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C02B6Z^今{いま}に心{こころ}の底{そこ}から嫌{きら}われるぞ。そしたら二{に}度{ど}と息子{むすこ}には会{あ}えなくなる",
              "maleVariant": "",
              "stringId": "1806762680156643328"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "792B6Z^はあ… もう嫌{きら}われてるさ",
              "maleVariant": "",
              "stringId": "1806763542253887488"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E22B6Z^いずれ息子{むすこ}がいたらと悲{かな}しむ日{にち}がきっとくる。その時{とき}には手{て}遅{おく}れだがな",
              "maleVariant": "",
              "stringId": "1806765141743329280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A02B6Z^きっと、また後{あと}で叱{しか}られる",
              "maleVariant": "",
              "stringId": "1806789170978054144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7F2B6Z^もう少{すこ}し大人{おとな}になるまで待{ま}てばいい",
              "maleVariant": "",
              "stringId": "1806817998244896768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C62B6Z^だとしても、俺{おれ}から言{い}えるのはそれだけだ",
              "maleVariant": "",
              "stringId": "1806822453317165056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "802B6Z^だろうね。でも俺{おれ}は自{じ}分{ぶん}で稼{かせ}げてるんだ",
              "maleVariant": "",
              "stringId": "1806835620512489472"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "AA2B6Z^負{ま}け犬{いぬ}が偉{えら}そうに",
              "maleVariant": "",
              "stringId": "2237622305589780480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8C2B6Z^苦{く}労{ろう}して育{そだ}ててやったってのに、得{え}体{たい}の知{し}れねえクズと遊{あそ}び歩{ある}きやがって。恩知{おんぢ}らずが",
              "maleVariant": "",
              "stringId": "2237623692360900608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A62B6Z^きつく叱{しか}ってやらねえとな。優{やさ}しくしすぎたんだ",
              "maleVariant": "",
              "stringId": "2237624672049651712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "22B6Z^クソ… 酒{さけ}がいる。あいつのせいで肝{かん}臓{ぞう}がやられちまうよ",
              "maleVariant": "",
              "stringId": "2237626464594518016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4E610004^[乗{の}る]",
              "maleVariant": "",
              "stringId": "2512618004766851076"
            }
          ]
        }
      }
    },
