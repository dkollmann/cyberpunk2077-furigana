                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
                    "femaleVariant": "453BCZ^そういう人{にん}がいるといいよね",
                    "maleVariant": null,
                    "stringId": 1841856078480654336
                  }
                },
                {
                  "Type": "localizationPersistenceSubtitleEntry",
                  "Properties": {
