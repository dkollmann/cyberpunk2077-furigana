        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B5627004^アンドリューのお墓{はか}に行{い}った",
              "maleVariant": "",
              "stringId": 1775014363328049156
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "83627Z^ああ、お前{ぜん}か。バリーと話{はな}したか？ 引{ひ}き籠{こも}るのをやめてくれりゃいいんだが",
              "maleVariant": "",
              "stringId": 1793827762354679808
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B6627Z^バリーはふさぎ込{こ}んでる",
              "maleVariant": "",
              "stringId": 1793849555874377728
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "993BCZ^死{し}んだ友達{ともだち}について、バリーは具{ぐ}体{たい}的{てき}に何{なん}て言{い}ってたの？",
              "maleVariant": "993BCZ^死{し}んだ友達{ともだち}について、バリーは具{ぐ}体{たい}的{てき}に何{なん}て言{い}ってた？",
              "stringId": 1841843375376678912
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "753BCZ^バリーは死{し}んだ友達{ともだち}について何{なん}て？",
              "maleVariant": "",
              "stringId": 1841843452082110464
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "63BCZ^詳{くわ}しいことは何{なに}も。彼{か}の話{はなし}はほとんどしてなかった。唯{ゆい}一{いつ}理{り}解{かい}してくれる相{あい}手{て}とだけ",
              "maleVariant": "",
              "stringId": 1841855909919965184
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "453BCZ^そういう人{にん}がいるといいよね",
              "maleVariant": "",
              "stringId": 1841856078480654336
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E93BCZ^いい加{か}減{げん}にしてよ、メンデス！",
              "maleVariant": "",
              "stringId": 1895457101287505920
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EC3BCZ^少{すこ}し話{はなし}をした。彼{かれ}、相{そう}当{とう}思{おも}い詰{つ}めてるみたいよ。メンデスに対{たい}して不{ふ}満{まん}があったみたい",
              "maleVariant": "EC3BCZ^少{すこ}し話{はなし}をした。あいつ、相{そう}当{とう}思{おも}い詰{つ}めてるみたいだぞ。メンデスに対{たい}して不{ふ}満{まん}があったらしい",
              "stringId": 1896484015133409280
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "553BCZ^さあね。自{じ}分{ぶん}で聞{き}きなよ",
              "maleVariant": "553BCZ^さあな。自{じ}分{ぶん}で聞{き}けよ",
              "stringId": 1896484102794362880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "さあね",
              "maleVariant": "さあな",
              "stringId": 1896487074458746884
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D13BCZ^彼{か}の問{もん}題{だい}に無{む}関{かん}心{しん}だったでしょ",
              "maleVariant": "D13BCZ^あいつの問{もん}題{だい}に無{む}関{かん}心{しん}だっただろ",
              "stringId": 1896487124236746752
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "223BCZ^バリーは仕{し}事{ごと}に耐{た}えられなくて、話{はなし}をできる相{あい}手{て}もいなかった",
              "maleVariant": "",
              "stringId": 1896487404768575488
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "943BCZ^はぁ…？ じゃあどうして俺{おれ}に言{い}わないんだ！？",
              "maleVariant": "",
              "stringId": 1896489395251036160
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C3BCZ^ああ、聞{き}くさ！ お前{まえ}と話{はな}したんなら、もう駄々{だだ}をこねるのはやめたんだろうし",
              "maleVariant": "",
              "stringId": 1896499997243588608
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "833BCZ^ガツンと一発{いっぱつ}食{く}らわせてやる。酒{さけ}と拳{こぶし}、どっちにするか",
              "maleVariant": "",
              "stringId": 1896500213988442112
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "543BCZ^手{て}を貸{か}してくれてありがとう。これをどうぞ",
              "maleVariant": "",
              "stringId": 1896500346343899136
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E93BCZ^理{り}解{かい}しようともしなかったんだってね",
              "maleVariant": "E93BCZ^理{り}解{かい}しようともしなかったんだってな",
              "stringId": 1896513332529840128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "403BCZ^ほらね？ 言{い}ったでしょ？",
              "maleVariant": "",
              "stringId": 1896513540147888128
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "593BCZ^ちょっと待{ま}て。バリーは大{おお}げさに騒{さわ}ぎ立{た}ててるだけだ。ほとぼりが冷{さ}めて話{はなし}をすりゃ大{だい}丈{じょう}夫{ぶ}だって",
              "maleVariant": "",
              "stringId": 1896514249236922368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "393BCZ^どうかな… 後{あと}で様{よう}子{す}を見{み}にいかないと",
              "maleVariant": "",
              "stringId": 1896514356074233856
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4F3BCZ^バリーは人{にん}を信{しん}じられなくなってる。あんたたちよりカメと話{はな}すことを選{えら}んだくらいにね",
              "maleVariant": "4F3BCZ^バリーは人{にん}を信{しん}じられなくなってる。あんたたちよりカメと話{はな}すことを選{えら}んだくらいにな",
              "stringId": 1896658787603562496
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "513BCZ^冗{じょう}談{だん}のつもりか？",
              "maleVariant": "",
              "stringId": 1896808866679341056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "813BCZ^いいえ。残{ざん}念{ねん}ながら本{ほん}当{とう}の話{はなし}よ",
              "maleVariant": "813BCZ^いいや。残{ざん}念{ねん}ながら本{ほん}当{とう}の話{はなし}だ",
              "stringId": 1896809022103470080
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "93BCZ^そしてあんたたち以{い}外{がい}には… おばあさんのことを思{おも}い出{だ}させてくれる、親{しん}友{ゆう}のカメしかいなかった。それも死{し}んじゃったけど",
              "maleVariant": "93BCZ^そしてあんたら以{い}外{がい}には… おばあさんのことを思{おも}い出{だ}させてくれる、親{しん}友{ゆう}のカメしかいなかった。それも死{し}んじまったけどな",
              "stringId": 1896809763119546368
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "423BCZ^嘘{うそ}だろ… そんなくだらない話{はなし}、信{しん}じられるか！",
              "maleVariant": "",
              "stringId": 1896809888629899264
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4F3BC004^自{じ}分{ぶん}で見{み}に行{い}きなよ",
              "maleVariant": "4F3BC004^なら自{じ}分{ぶん}で見{み}に行{い}け",
              "stringId": 1896811439331196932
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "103BCZ^警{けい}官{かん}をからかってる暇{ひま}なんてない",
              "maleVariant": "",
              "stringId": 1896811502698741760
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "393BCZ^証{しょう}拠{こ}が欲{ほっ}しかったら、納{のう}骨{こつ}堂{どう}にあるから見{み}に行{い}けばいい",
              "maleVariant": "",
              "stringId": 1896811799739351040
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "173BCZ^警{けい}官{かん}をからかうのが趣{しゅ}味{み}の暇{ひま}人{じん}に見{み}える？",
              "maleVariant": "173BCZ^警{けい}官{かん}をからかうのが趣{しゅ}味{み}の暇{ひま}人{じん}に見{み}えるか？",
              "stringId": 1896811958082715648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3D3BCZ^どうして彼{かれ}は私{わたし}たちにウソを？ そのカメのこと、普{ふ}通{つう}の人{にん}間{げん}みたいに話{はな}してたのよ！",
              "maleVariant": "",
              "stringId": 1896825827169648640
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "143BCZ^真{しん}実{じつ}を話{はな}したら、メンデスがどんな反{はん}応{のう}したと思{おも}う？",
              "maleVariant": "",
              "stringId": 1896859868392570880
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "253BCZ^バリーの奴{やつ}… 大{おお}げさに騒{さわ}ぎ立{た}ててるだけだと思{おも}ってたが、こりゃ話{はなし}をしにいかないといけないな",
              "maleVariant": "",
              "stringId": 1896860616002093056
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうね",
              "maleVariant": "",
              "stringId": 1897940019079528448
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "883BCZ^バリーは警{けい}官{かん}として苦{くる}しんでたのに、それを茶{ちゃ}化{か}されたから、殻{から}に閉{と}じこもっちゃったのよ",
              "maleVariant": "883BCZ^バリーは警{けい}官{かん}として苦{くる}しんでたのに、それを茶{ちゃ}化{か}されたから、殻{から}に閉{と}じこもっちまったんだよ",
              "stringId": 1967956602883129344
            }
          ]
        }
      }
