        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5852CZ^古{ふる}いトロフィー、2042年{ねん}、学校{がっこう}対{たい}抗{こう}のレスリングトーナメントで優{ゆう}勝{しょう}",
              "maleVariant": "",
              "stringId": "1354559229814554624"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EE52CZ^どういうことだ、アンソニー？ これは報{ほう}告{こく}しないといけない。わかってるな？",
              "maleVariant": "",
              "stringId": "1560404525063520256"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BB5F9Z^待{ま}て、どの学校{がっこう}が優{ゆう}勝{しょう}したか調{しら}べてみる…",
              "maleVariant": "",
              "stringId": "1679272597438566400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "96521Z^カメは病{びょう}気{き}だったから… 食{しょく}欲{よく}はないし、甲{こう}羅{ら}にこもって… 助{たす}けてやりたかったんだ",
              "maleVariant": "",
              "stringId": "1775204307735547904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CD521Z^リアムのカメを殺{ころ}すなんて――アンソニー、なぜこんなことをやったんだね",
              "maleVariant": "",
              "stringId": "1785030042780307456"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "74521Z^ヒト成{せい}長{ちょう}ホルモンを注{ちゅう}射{しゃ}してか？ 何{なに}を考{かんが}えてる？ こんなこと、お父{とう}さんに教{おそ}わったのか？",
              "maleVariant": "",
              "stringId": "1785040636971454464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "54521Z^ラグーナ・ベンド。50年{ねん}代{だい}に洪水{こうずい}で沈{しず}んだ町{まち}か",
              "maleVariant": "",
              "stringId": "1785055621575479296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "EB521Z^いい人{にん}たちに引{ひ}き取{と}ってもらったのに、これが君{くん}の恩返{おんがえ}しの仕{し}方{かた}かね？",
              "maleVariant": "",
              "stringId": "1838903295546101760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "82521Z^友{とも}達{だち}が死{し}んで辛{つら}いのはわかるが… 言{い}い訳{わけ}にはならない",
              "maleVariant": "",
              "stringId": "1840351874584285184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C2521Z^死{し}ねよ",
              "maleVariant": "",
              "stringId": "1840353692429193216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B6521Z^今{いま}、何{なん}と言{い}った？",
              "maleVariant": "",
              "stringId": "1840360976492400640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "33521Z^よし、よくわかった。校{こう}長{ちょう}に報{ほう}告{こく}する。里{さと}親{おや}のお二{ふた}人{り}にもだ",
              "maleVariant": "",
              "stringId": "1840361541230268416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3C521Z^蛙{かえる}の子{こ}は蛙{かえる}だな",
              "maleVariant": "",
              "stringId": "1840362048187404288"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DB521Z^父{とう}さんのことを口{くち}にするな…",
              "maleVariant": "",
              "stringId": "1840362845423931392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6C521Z^ふん、全{ぜん}部{ぶ}知{し}ってるんだからな、ワルガキが。農{のう}場{じょう}が破{は}綻{たん}した後{のち}、頭{あたま}を撃{う}って死{し}んだんだろ",
              "maleVariant": "",
              "stringId": "1840363526461460480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CC521Z^黙{だま}れ、黙{だま}れ、黙{だま}れ！！ 父{とう}さんの話{はなし}をするな、クソ野{や}郎{ろう}！",
              "maleVariant": "",
              "stringId": "1840364786497490944"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B3521Z^違{ちが}う… 違{ちが}う… 病{びょう}気{き}で弱{よわ}ってたんだ… 助{たす}けようとしただけだ",
              "maleVariant": "",
              "stringId": "1840372731767558144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C7521Z^これでハリスが住{す}んでたおおよその場{ば}所{しょ}はわかった",
              "maleVariant": "",
              "stringId": "1927255638418788352"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "825B6Z^これは、ハリスの子{こ}供{ども}時{じ}代{だい}か…",
              "maleVariant": "",
              "stringId": "1995084720032669696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "97521Z^この事{じ}件{けん}… アカデミーのケーススタディに使{つか}われてたヤツだ",
              "maleVariant": "",
              "stringId": "1997001401079828480"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "67521Z^どの学校{がっこう}だったか調{しら}べてみる…",
              "maleVariant": "",
              "stringId": "1997004874903064576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "615F9004^[調{しら}べる]",
              "maleVariant": "",
              "stringId": "2101252810638069764"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "575F9004^[調{しら}べる]",
              "maleVariant": "",
              "stringId": "2101265162796240900"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E5F9Z^これで全{ぜん}部{ぶ}のようね…",
              "maleVariant": "E5F9Z^これで全{ぜん}部{ぶ}のようだな…",
              "stringId": "2243147507819319296"
            }
          ]
