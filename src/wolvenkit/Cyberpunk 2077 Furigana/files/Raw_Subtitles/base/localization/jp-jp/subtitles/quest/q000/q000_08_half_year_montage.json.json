        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ハロー、ナイトシティ！",
              "maleVariant": "",
              "stringId": 1923976551582420992
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "682EFZ^毎{まい}度{ど}おなじみスタンだ。さあ今日{きょう}も夢{ゆめ}の街{まち}で新{あら}たな一日{ついたち}が始{はじ}まるぜ",
              "maleVariant": "",
              "stringId": 1957459904137457664
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4E2EFZ^俺{おれ}はこの街{まち}が大{だい}好{す}きだ。どれくらい好{す}きかっていうと、たまに孤{こ}児{じ}院{いん}に顔{かお}を出{だ}してはタバコをせがんでくる母{はは}親{おや}と同{おな}じくらいにな",
              "maleVariant": "",
              "stringId": 1957460015370399744
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "312EFZ^今日{きょう}もまた何{なん}百{びゃく}人{にん}と新参者{しんざんもの}がここにやってくる。だが、一{いち}年{ねん}持{も}つのはせいぜい半{はん}分{ぶん}だ。それも、当{あ}たりの年{ねん}ならな",
              "maleVariant": "",
              "stringId": 1957461363503591424
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E02EFZ^連{れん}中{ちゅう}は何{なに}をしにナイトシティに来{く}るのかって？ そりゃ次{つぎ}なるモーガン・ブラックハンドやウェイランド・ボア・ボアみたいなレジェンドになるためにさ。ほら、リスクがデカけりゃデカいほど、リターンもデカい… って言{い}うしな",
              "maleVariant": "",
              "stringId": 1957462358577049600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7A2EFZ^だが、メジャーリーガーの選{せん}手{しゅ}生{せい}命{めい}は短{みじか}い。 炎{ほのお}ってのは勢{いきお}いがいいほど、燃{も}え尽{ことごと}きるのも早{はや}いもんだ。その前{まえ}に鉛{なまり}玉{だま}を食{く}らう可{か}能{のう}性{せい}だってある",
              "maleVariant": "",
              "stringId": 1957462807837339648
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "512EFZ^この街{まち}の伝{でん}説{せつ}が集{あつ}まる場{ば}所{しょ}を知{し}ってるか？",
              "maleVariant": "",
              "stringId": 1957463760632213504
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9B2EFZ^墓{ぼ}地{ち}だよ",
              "maleVariant": "",
              "stringId": 1957463847773073408
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2EFZ^とはいえ――伝{でん}説{せつ}になるには、出身{しゅっしん}も身{み}分{ぶん}も関{かん}係{けい}ない",
              "maleVariant": "",
              "stringId": 1957464304734105600
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D82EFZ^お前{まえ}がホンモノなら、チャンスはあるさ",
              "maleVariant": "",
              "stringId": 1957464724969811968
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ナイトシティは――",
              "maleVariant": "",
              "stringId": 1957465366916427776
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2A2EFZ^そう、夢{ゆめ}の街{まち}だからな",
              "maleVariant": "",
              "stringId": 1957465422130245632
            }
          ]
        }
      }
    },
