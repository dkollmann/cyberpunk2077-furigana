              "femaleVariant": "FC4B6Z^銃{じゅう}を取{と}れ",
              "maleVariant": "",
              "stringId": "1779431313179893760"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E94B6Z^…そいつにも教{おし}えてやろうか",
              "maleVariant": "",
              "stringId": "1779438021600043008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "864B6Z^この野{や}郎{ろう}、助{たす}かるためにウソついてやがるな",
              "maleVariant": "",
              "stringId": "1779459709523943424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D54B6Z^よし。クズが一{ひと}人{り}減{へ}った",
