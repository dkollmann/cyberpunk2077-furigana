              "stringId": "1729069070970216448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "スマッシャーはどこ？",
              "maleVariant": "",
              "stringId": "1756735917089579016"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "774B6004^話{はな}したほうが身{み}のためよ",
