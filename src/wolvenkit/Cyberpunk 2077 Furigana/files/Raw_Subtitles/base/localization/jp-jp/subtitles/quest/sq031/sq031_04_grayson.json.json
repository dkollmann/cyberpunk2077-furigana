            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3A6E4Z^開{ひら}けりゃわかる。ほら、アクセスカードだ",
              "maleVariant": "",
              "stringId": "1879795933901635584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
