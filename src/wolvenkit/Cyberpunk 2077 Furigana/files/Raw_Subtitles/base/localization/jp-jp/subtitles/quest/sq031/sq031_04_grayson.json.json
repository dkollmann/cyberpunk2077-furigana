              "maleVariant": "",
              "stringId": "1879802279128612864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E16E4Z^頭{あたま}をかち割{さ}ってやってもいいのよ",
