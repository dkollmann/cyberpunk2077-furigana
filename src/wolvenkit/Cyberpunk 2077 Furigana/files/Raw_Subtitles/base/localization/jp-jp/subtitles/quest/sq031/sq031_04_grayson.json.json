              "stringId": "1822737913892638720"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "それで？",
              "maleVariant": "",
              "stringId": "1822738390785003520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D0351Z^結{けっ}局{きょく}行{い}かず仕{し}舞{ま}いだ。よくあったことさ",
