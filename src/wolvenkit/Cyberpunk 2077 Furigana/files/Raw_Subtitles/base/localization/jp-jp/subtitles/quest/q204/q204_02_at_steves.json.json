        "Data": {
          "$type": "localizationPersistenceSubtitleEntries",
          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "77405Z^運{うん}転{てん}手{しゅ}が要{い}る",
              "maleVariant": "",
              "stringId": "1264586597559455744"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そのアザ、どうした？",
              "maleVariant": "",
              "stringId": "1264590844644577280"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2E405Z^練{れん}習{しゅう}してるか？",
              "maleVariant": "",
              "stringId": "1264590964316459008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "92405Z^そのアザ、どうした？ 転{ころ}んだわけでもないだろ？",
              "maleVariant": "",
              "stringId": "1264591528634896384"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "48405Z^ちゃんとコードの練{れん}習{しゅう}してんのか？",
              "maleVariant": "",
              "stringId": "1264591651947433984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D7405Z^車{くるま}と運{うん}転{てん}手{しゅ}を探{さが}してるんだ。今{こん}日{にち}は街{まち}でいろいろと野{や}暮{ぼ}用{よう}があってな",
              "maleVariant": "",
              "stringId": "1264592423145721856"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "やあ！",
              "maleVariant": "",
              "stringId": "1264594654750330880"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "えーっと…",
              "maleVariant": "",
              "stringId": "1264598509919686656"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E2405Z^いいよ、任{まか}せといて",
              "maleVariant": "",
              "stringId": "1264604419173928960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "53405Z^出{で}かける時{とき}は声{こえ}をかけろって言{い}うんだけど",
              "maleVariant": "",
              "stringId": "1264605653725368320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3D405Z^行{い}こう",
              "maleVariant": "",
              "stringId": "1264606516644696064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "442B6Z^うん。でも指{ゆび}が追{お}い付{つ}かなくて",
              "maleVariant": "",
              "stringId": "1314083384664481792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B42B6Z^全{ぜん}然{ぜん}上{じょう}達{たつ}しないんだ",
              "maleVariant": "",
              "stringId": "1314085190429794304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "282B6Z^そのうち上手{うま}くなる",
              "maleVariant": "",
              "stringId": "1314087623327440896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C72B6Z^技{ぎ}術{じゅつ}より心{こころ}だ",
              "maleVariant": "",
              "stringId": "1314091938142183424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6C2B6Z^とにかく練{れん}習{しゅう}しろ。そのうち上手{うま}くなる",
              "maleVariant": "",
              "stringId": "1314092658169962496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "792B6Z^なにもリンダ・オズを目指{めざ}せって言{い}ってるわけじゃない",
              "maleVariant": "",
              "stringId": "1314092770057216000"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E72B6Z^親{おや}の隙{げき}を見{み}て練{れん}習{しゅう}してるけど… あんまりチャンスがなくてさ",
              "maleVariant": "",
              "stringId": "1314094322385903616"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F52B6Z^自{じ}分{ぶん}でも下手{へた}だってわかってる",
              "maleVariant": "",
              "stringId": "1314094893851435008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2B6Z^頑{がん}張{ば}ってるけど…",
              "maleVariant": "",
              "stringId": "1314101950667251712"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F62B6Z^親父{おやじ}か？",
              "maleVariant": "",
              "stringId": "1314134841359032320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "372B6Z^いずれ何{なに}も考{かんが}えずに弾{ひ}けるようになるさ。コツさえ掴{つか}めばな",
              "maleVariant": "",
              "stringId": "1314139910510960640"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F72B6Z^ああ。ある程{てい}度{ど}は譜{ふ}面{めん}通{とう}りに弾{ひ}かなきゃならんが、それより心{こころ}を込{こ}めろ",
              "maleVariant": "",
              "stringId": "1314143242331840512"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "922B6Z^よう、元{げん}気{き}か",
              "maleVariant": "",
              "stringId": "1314147183417319424"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "962B6Z^今{こん}日{にち}はどうしたの？",
              "maleVariant": "",
              "stringId": "1314151985257865216"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "632B6Z^〈Curse These Metal Hands〉の新{しん}盤{ばん}を買{か}ったんだけどさ、最{さい}初{しょ}の四{し}曲{きょく}がもう最{さい}高{こう}だよ",
              "maleVariant": "",
              "stringId": "1314152753201373184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "642B6Z^初{はじ}めて聞{き}いた時{とき}は鳥{とり}肌{はだ}立{た}った。でも… 後{こう}半{はん}はイマイチなんだよね",
              "maleVariant": "",
              "stringId": "1314152753218150400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "662B6Z^で？ 何{なに}か用{よう}があるんだろ？",
              "maleVariant": "",
              "stringId": "1314152753251704832"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E02B6Z^その… 起{お}こすの忘{わす}れて、仕{し}事{ごと}に遅{おく}れさせちゃってさ。俺{おれ}が悪{わる}いんだ…",
              "maleVariant": "",
              "stringId": "1314214705906802688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "642B6030^きっと酔{よ}いが残{のこ}ってて、虫{むし}の居{い}所{どころ}が悪{わる}かったんだ",
              "maleVariant": "",
              "stringId": "1316758917898526768"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "FE2B6Z^お前{まえ}のせいじゃない",
              "maleVariant": "",
              "stringId": "1316761269829328896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "882B6Z^たかが朝{あさ}寝{ね}坊{ぼう}をしたくらいで、手{て}を出{だ}すとはな",
              "maleVariant": "",
              "stringId": "1316766731048017920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うん…",
              "maleVariant": "",
              "stringId": "1316768043865169920"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DC2B6Z^後{のち}から謝{あやま}ってくれたしね",
              "maleVariant": "",
              "stringId": "1316772680987009024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "952B6Z^で、何{なに}しに来{き}たの？",
              "maleVariant": "",
              "stringId": "1316791474572713984"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "762B6Z^もう一{いっ}曲{きょく}練{れん}習{しゅう}してたんだ… 聞{き}く？",
              "maleVariant": "",
              "stringId": "1719850304828825600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BC2B6Z^今{いま}は急{いそ}いでる。また今{こん}度{ど}な",
              "maleVariant": "",
              "stringId": "1719932576101785600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "832B6Z^いいか、観{かん}客{きゃく}が求{もと}めてるのは魂{たましい}のこもった音{おん}楽{がく}だ。技{ぎ}術{じゅつ}なんておざなりでいい",
              "maleVariant": "",
              "stringId": "1760583057622654976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そうなの？",
              "maleVariant": "",
              "stringId": "1760583256030011392"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A12B6Z^どうせ母{かあ}さんは何{なに}も聞{き}いちゃいないから",
              "maleVariant": "",
              "stringId": "1760615184481345536"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "25610Z^誰{だれ}？ 何{なに}か用{よう}？",
              "maleVariant": "",
              "stringId": "1802751272594505728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8B610004^俺{おれ}だ",
              "maleVariant": "",
              "stringId": "1802752227788521476"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "リチャード・ナイトだ",
              "maleVariant": "",
              "stringId": "1802752509628973056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "88610Z^落{お}ち着{つ}け。俺{おれ}だ、スティーブ",
              "maleVariant": "",
              "stringId": "1802753005127270400"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "CE610Z^リチャード・ナイトだ。この街{まち}を作{つく}った奴{やつ}だぞ、さっさと開{ひら}けろ",
              "maleVariant": "",
              "stringId": "1802753087906054144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B42B6Z^目覚{めざ}まし時計{とけい}をやるよ",
              "maleVariant": "",
              "stringId": "1805495973840642048"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4D2B6Z^うちの目覚{めざ}まし時計{とけい}を持{も}ってけ。鍵{かぎ}は開{ひら}いてる",
              "maleVariant": "",
              "stringId": "1805498175430811648"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E72B6Z^使{つか}わないの？",
              "maleVariant": "",
              "stringId": "1805548132779122688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8C2B6Z^アラームは好{す}きじゃないんだよ",
              "maleVariant": "",
              "stringId": "1805556128481501184"
            }
          ]
        }
      }
