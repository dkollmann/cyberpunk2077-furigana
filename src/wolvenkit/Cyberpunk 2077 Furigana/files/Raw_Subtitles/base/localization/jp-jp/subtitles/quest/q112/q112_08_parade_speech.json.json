              "stringId": "1724187621863591936"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "184E1Z^殺{ころ}す必{ひつ}要{よう}はなかっただろ！",
              "maleVariant": "",
              "stringId": "1724188104258883584"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6C4E1004^逆{ぎゃく}の立場{たちば}なら必{かなら}ず殺{ころ}されてた",
              "maleVariant": "",
              "stringId": "1724189454287900676"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B94E1Z^不{ふ}安{あん}の種{たね}を残{のこ}しておけない",
              "maleVariant": "",
              "stringId": "1724192698280054784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "454E1Z^逆{ぎゃく}の立場{たちば}なら殺{ころ}されてた",
              "maleVariant": "",
              "stringId": "1724194637659115520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C94E1Z^仕{し}方{かた}なかった。不{ふ}安{あん}の種{たね}は残{のこ}しておけない",
