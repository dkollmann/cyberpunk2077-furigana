            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E1514Z^警{けい}告{こく}したはずだ",
              "maleVariant": "",
              "stringId": "2004115740769206272"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "1145AZ^一{いっ}瞬{しゅん}で終{お}わらせてやろう、女{おんな}",
              "maleVariant": "1145AZ^一{いっ}瞬{しゅん}で終{お}わらせてやろう、クズめ",
              "stringId": "2230175985079525376"
