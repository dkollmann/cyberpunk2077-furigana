            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "うう…",
              "maleVariant": "",
              "stringId": "1724200725154500608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4F4E1Z^V、俺{おれ}こそお前{まえ}に礼{れい}を言{い}う",
              "maleVariant": "",
              "stringId": "1724201522659463168"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D04E1Z^今{いま}の… ところはね…",
              "maleVariant": "D04E1Z^今{いま}の… ところはな…",
              "stringId": "1724202997997506560"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "オダ！",
              "maleVariant": "",
              "stringId": "1725692919384248320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "792D7Z^ハナコ様{さま}が見{み}える。お話{はな}しになられるぞ",
              "maleVariant": "",
              "stringId": "1740315746735321088"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "634E1Z^人{じん}類{るい}史{し}上{じょう}最{さい}大{だい}かつ最{さい}強{きょう}の軍{ぐん}需{じゅ}企{き}業{ぎょう}の創{そう}業{ぎょう}者{しゃ}にして、最{さい}高{こう}経{けい}営{えい}責{せき}任{にん}者{しゃ}",
              "maleVariant": "",
              "stringId": "1773226942591275008"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E34E1Z^兵{へい}士{し}、実{じつ}業{ぎょう}家{か}、そして――",
              "maleVariant": "",
              "stringId": "1773227138012286976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B44E1Z^私{わたし}の父{ちち}",
              "maleVariant": "",
              "stringId": "1773227278957678592"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "244E1Z^先{さき}ほどの収容所{しゅうようじょ}での言{こと}葉{ば}は、父{ちち}の友{ゆう}人{じん}や家{か}族{ぞく}、同{どう}輩{はい}に向{む}けられたものでは決{けっ}してありません",
              "maleVariant": "",
              "stringId": "1773228423298027520"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "744E1Z^敵{てき}に向{む}けられた言{こと}葉{ば}なのです",
              "maleVariant": "",
              "stringId": "1773228678043275264"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E34E1Z^私{わたし}がここで、父{ちち}の言{い}を引{いん}用{よう}したのは偶{ぐう}然{ぜん}ではありません",
              "maleVariant": "",
              "stringId": "1773228825934434304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F44E1Z^私{わたし}たちの敵{てき}が暴{ぼう}挙{きょ}に走{はし}ったのは50年{ねん}前{まえ}、ここナイトシティでのことでした",
              "maleVariant": "",
              "stringId": "1773229002313306112"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D24E1Z^無{む}差{さ}別{べつ}テロという卑{ひ}劣{れつ}極{きわ}まりない行{こう}為{い}が、勤{きん}勉{べん}なるアラサカ社{しゃ}従{じゅう}業{ぎょう}員{いん}、無{む}垢{く}なるナイトシティ市{し}民{みん}、合{あ}わせて4000人{にん}もの尊{とうと}い命{いのち}を無{む}慈{じ}悲{ひ}にも奪{うば}い去{さ}ったのです",
              "maleVariant": "",
              "stringId": "1773229207901310976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "424E1Z^アラサカ・タワーは崩{ほう}壊{かい}しました",
              "maleVariant": "",
              "stringId": "1773229364399181824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ですが…",
              "maleVariant": "",
              "stringId": "1773229528446799872"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "44E1Z^ですが、その尖{せん}塔{とう}は今{いま}もなお摩{ま}天{てん}楼{ろう}にそびえ立{た}っています",
              "maleVariant": "",
              "stringId": "1773229586697293824"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E34E1Z^その姿{すがた}は、いまだかつてないほどに力強{ちからづよ}く、尊{とうと}いものです",
              "maleVariant": "",
              "stringId": "1773229822366846976"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A34E1Z^私{わたし}の父{ちち}は、テロと同{おな}じくらい卑{ひ}劣{れつ}な、許{ゆる}されざる暴{ぼう}力{りょく}の犠{ぎ}牲{せい}者{しゃ}となりました",
              "maleVariant": "",
              "stringId": "1773229988796829696"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "634E1Z^これは私{わたし}たちが価{か}値{ち}観{かん}を分{わ}かち合{あ}う、この世{せ}界{かい}に対{たい}する冒{ぼう}涜{とく}にほかなりません",
              "maleVariant": "",
              "stringId": "1773230168111714304"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B34E1Z^ナイトシティの市{し}民{みん}が見{み}守{まも}る中{なか}、アラサカ社{しゃ}は50年{ねん}前{まえ}と同{どう}様{よう}、これまで以{い}上{じょう}に全{ぜん}社{しゃ}が一{いち}丸{がん}となり、この試{し}練{れん}を乗{の}り越{こ}えることをここに誓{ちか}います",
              "maleVariant": "",
              "stringId": "1773230362727419904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "634E1Z^私{わたし}たちは今日{きょう}、この場{ば}所{しょ}から強{つよ}くなるのです",
              "maleVariant": "",
              "stringId": "1773230601903411200"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C34E1Z^私{わたし}たちの声{こえ}を、その耳{みみ}でしかと聞{き}き届{とど}けなさい",
              "maleVariant": "",
              "stringId": "1773231479687352320"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C54E1Z^沈{ちん}黙{もく}は決{けっ}して金{きん}ならず",
