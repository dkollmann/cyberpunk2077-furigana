          "entries": [
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7B5F9Z^一{ひと}人{り}で大{たい}変{へん}でしょう",
              "maleVariant": "7B5F9Z^一{ひと}人{り}で大{たい}変{へん}だろ",
              "stringId": "1921436089602510848"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7B5F9010^リバーとは仲{なか}直{なお}りした？",
              "maleVariant": "7B5F9010^リバーとは仲{なか}直{なお}りしたか？",
              "stringId": "1921436089602510864"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7C5F9004^子{こ}供{ども}三{さん}人{にん}を一{ひと}人{り}で育{そだ}てるなんて大{たい}変{へん}でしょ",
              "maleVariant": "7C5F9004^子{こ}供{ども}三{さん}人{にん}を一{ひと}人{り}で育{そだ}てるなんて大{たい}変{へん}だろ",
              "stringId": "1921436089619288068"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7C5F9014^夫{おっと}がいた時{とき}だって大{たい}差{さ}なかった",
              "maleVariant": "",
              "stringId": "1921436089619288084"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7C5F9024^いない日{にち}のほうが平{へい}和{わ}だったくらい",
              "maleVariant": "",
              "stringId": "1921436089619288100"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7C5F9034^そばにいてほしくなくて、しばらく消{き}えてほしいと思{おも}ったこともあったのよ",
              "maleVariant": "",
              "stringId": "1921436089619288116"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7E5F9004^リバーとの関{かん}係{けい}は問{もん}題{だい}ない？",
              "maleVariant": "7E5F9004^リバーとの関{かん}係{けい}は問{もん}題{だい}ないか？",
              "stringId": "1921436089652842500"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7E5F9014^ええ。今{いま}さらだけど、やっとお互{たが}い理{り}解{かい}し合{あ}えるようになった",
              "maleVariant": "",
              "stringId": "1921436089652842516"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7E5F9024^よかった。それならこっちも安{あん}心{しん}だから…",
              "maleVariant": "7E5F9024^よかった。それならこっちも安{あん}心{しん}だよ…",
              "stringId": "1921436089652842532"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7F5F9004^ランディには明{あか}るい未{み}来{らい}が待{ま}ってるといいね",
              "maleVariant": "7F5F9004^ランディには明{あか}るい未{み}来{らい}が待{ま}ってるといいな",
              "stringId": "1921436089669619716"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "7F5F9014^だといいんだけど… 旦{だん}那{な}が亡{な}くなって、いろいろ変{か}わったから",
              "maleVariant": "",
              "stringId": "1921436089669619732"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "805F9004^父{ちち}親{おや}を失{うしな}うのはつらい",
              "maleVariant": "805F9004^父{ちち}親{おや}を失{うしな}うのはつらいな",
              "stringId": "1921436089686396932"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "805F9014^でもあの殺{さつ}人{じん}鬼{き}の元{もと}から生{い}きて帰{かえ}れたのなら… あとはよくなる一方{いっぽう}なはず",
              "maleVariant": "",
              "stringId": "1921436089686396948"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "805F9024^間{ま}違{ちが}いない",
              "maleVariant": "",
              "stringId": "1921436089686396964"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "8521004^そんなにひどい旦{だん}那{な}だったの？",
              "maleVariant": "8521004^そんなにひどい旦{だん}那{な}だったのか？",
              "stringId": "1937433101388091396"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "そんなにひどかったの？",
              "maleVariant": "そんなにひどかったのか？",
              "stringId": "1937433238692827136"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "6F521Z^じゃあ、旦{だん}那{な}がいなくなって良{よ}かったんじゃ？",
              "maleVariant": "",
              "stringId": "1937433257734967296"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "86521Z^旦{だん}那{な}さんに何{なに}が？",
              "maleVariant": "",
              "stringId": "1937433275300712448"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "88521Z^えっと、話{わ}題{だい}を変{か}えましょ",
              "maleVariant": "88521Z^えっと、話{わ}題{だい}を変{か}えよう",
              "stringId": "1937433292514136064"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "85521Z^じゃあ旦{だん}那{な}が亡{な}くなってせいせいしたと？",
              "maleVariant": "",
              "stringId": "1937433429902757888"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "24521Z^どうして亡{な}くなったの？",
              "maleVariant": "24521Z^どうして亡{な}くなったんだ？",
              "stringId": "1937433466930073600"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A8521Z^話{わ}題{だい}を変{か}えましょ",
              "maleVariant": "A8521Z^話{わ}題{だい}を変{か}えよう",
              "stringId": "1937433499209437184"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "19521Z^これでランディは安{あん}全{ぜん}ね",
              "maleVariant": "19521Z^これでランディは安{あん}全{ぜん}だな",
              "stringId": "1937457110540488704"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "D9521Z^バイクで高{こう}速{そく}を走{はし}るのが生{い}きがいの男{おとこ}だった。でも家{いえ}じゃ… ひとつ話{はなし}をしてあげる",
              "maleVariant": "",
              "stringId": "1937487895792324608"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "3C521Z^ある時{とき}、彼{かれ}はドリアンを街{まち}に連{つ}れてったの。何{なに}か用{よう}事{じ}があって、二{ふた}人{り}でね",
              "maleVariant": "",
              "stringId": "1937488756446728192"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4A521Z^夜{よる}に戻{もど}った彼{かれ}は酔{よ}っ払{はら}って、よくも警{けい}察{さつ}を呼{よ}んだなって怒{ど}鳴{な}り散{ち}らしたの",
              "maleVariant": "",
              "stringId": "1937489534070689792"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "A1521Z^数{すう}週{しゅう}間{かん}、泣{な}き続{つづ}けたわ",
              "maleVariant": "",
              "stringId": "1937496987298566144"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2B521Z^夫{おっと}が、子{こ}供{ども}たちの父{ちち}親{おや}が殺{ころ}されたんだから",
              "maleVariant": "",
              "stringId": "1937498132075122688"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ごめん…",
              "maleVariant": "すまない…",
              "stringId": "1937498589472362496"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "71521Z^ランディはそれから引{ひ}きこもるようになった。もちろん下{した}の子{こ}たちもいる",
              "maleVariant": "",
              "stringId": "1937498863393968128"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4B521Z^彼{かれ}がいてもいなくても、人{じん}生{せい}が大{たい}変{へん}であることに変{か}わりはないわ",
              "maleVariant": "",
              "stringId": "1937500056757342208"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "14521Z^アルデカルドスには、立派{りっぱ}な最{さい}期{ご}を遂{と}げたって言{い}われた。どういう意{い}味{み}か知{し}らないけど",
              "maleVariant": "",
              "stringId": "1937504728759013376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "4C521Z^顔{かお}のない遺{い}体{たい}を見{み}せられた時{とき}、それが立派{りっぱ}な死{し}にざまになんか見{み}えなかった",
              "maleVariant": "",
              "stringId": "1937506112678006784"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5C521Z^一{いち}時{じ}はどうなることかと思{おも}ったけど…",
              "maleVariant": "",
              "stringId": "1937558700526014464"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "5B521Z^終{お}わりよければ全{すべ}てよし、よ。ランディと… 兄{にい}さんを取{と}り戻{もど}した",
              "maleVariant": "",
              "stringId": "1937559117121064960"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあね、ジョス",
              "maleVariant": "じゃあな、ジョス",
              "stringId": "1937577636801941504"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "じゃあね、ジョス",
              "maleVariant": "じゃあな、ジョス",
              "stringId": "1937577789340389376"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "17521Z^呼{よ}んだの？",
              "maleVariant": "17521Z^呼{よ}んだのか？",
              "stringId": "1963542117637820416"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "9D521Z^リバーがたまり場{ば}で二{ふた}人{り}を見{み}つけて連{つ}れ帰{かえ}ったのよ",
              "maleVariant": "",
              "stringId": "1963542326044397568"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "B1521Z^リバーからご両{りょう}親{しん}のことを聞{き}いた",
              "maleVariant": "",
              "stringId": "1963568882162733056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C5521Z^リバーからご両{りょう}親{しん}のことを聞{き}いた…",
              "maleVariant": "",
              "stringId": "1963569226095661056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E8521Z^信{しん}頼{らい}されてるのね。親{おや}のことは話{はな}さないのに",
              "maleVariant": "",
              "stringId": "1963573508765257728"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "C5521Z^ご両{りょう}親{しん}のあんな最{さい}期{ご}を目{め}の当{あ}たりにするなんて… 何{なに}が心{こころ}の支{ささ}えに？",
              "maleVariant": "",
              "stringId": "1963576553309868032"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "E6521Z^孤{こ}児{じ}院{いん}で少{すこ}し気{き}持{も}ちの整{せい}理{り}ができるようになったけど、一{いち}番{ばん}助{たす}けになったのはお互{たが}いの存{そん}在{ざい}ね",
              "maleVariant": "",
              "stringId": "1963577292597891072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "BB521Z^でも、やがて兄{あに}は強{きょう}迫{はく}観{かん}念{ねん}に駆{か}られるようになった。あらゆる殺{さつ}人{じん}犯{はん}を自{じ}分{ぶん}で捕{つか}まえようとするくらいにね",
              "maleVariant": "",
              "stringId": "1963581037087952896"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "76521Z^いいことじゃない。いまどきそんな警{けい}官{かん}はいないよ",
              "maleVariant": "76521Z^いいことじゃないか。いまどきそんな警{けい}官{かん}はいない",
              "stringId": "1963585992322584576"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "28521Z^かもね。でも私{わたし}は今{いま}の兄{にい}さんがいい。やっと自{じ}分{ぶん}らしい生{い}き方{かた}を受{う}け入{い}れたみたい",
              "maleVariant": "",
              "stringId": "1963586493525135360"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DA521Z^ジョス、何{なん}て言{い}ったらいいか…",
              "maleVariant": "",
              "stringId": "2005702499139457024"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "DC521Z^その… 何{なん}て言{い}ったらいいか…",
              "maleVariant": "",
              "stringId": "2005702799820722176"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "2521Z^よくも… よくも二{ふた}人{り}を見{み}捨{す}てたわね… 約{やく}束{そく}したのに…",
              "maleVariant": "",
              "stringId": "2005704389596155904"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "ジョス、あたしは…",
              "maleVariant": "72521Z^ジョス、俺{おれ}は…",
              "stringId": "2005704971295789056"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "F9521Z^兄{にい}さんとあの子{こ}は死{し}んだ。死{し}んだのよ！ あんたのせいで！",
              "maleVariant": "",
              "stringId": "2005705450302083072"
            },
            {
              "$type": "localizationPersistenceSubtitleEntry",
              "femaleVariant": "41521Z^もう二{に}度{ど}と顔{かお}を見{み}せないで… 二{に}度{ど}とね",
